                {
                    "sense_id": "2d710e77-1188-3988-b5b2-3808345a4637",
                    "sense_definition": "Een kokervormige sjaal van elastische stof die om de nek of het hoofd gedragen wordt ter bescherming tegen kou, wind of stof.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het hardlopen in de winter draag ik altijd een <w>buff</w> om mijn nek warm te houden.",
                            "target_lang_translations": {
                                "en": "When running in the winter, I always wear a <w>neck gaiter</w> to keep my neck warm.",
                                "ru": "Во время бега зимой я всегда ношу <w>бафф</w>, чтобы держать шею в тепле."
                            }
                        },
                        {
                            "text": "Hij trok zijn <w>buff</w> omhoog tot over zijn neus om zich tegen de snijdende wind te beschermen.",
                            "target_lang_translations": {
                                "en": "He pulled his <w>neck gaiter</w> up over his nose to protect himself against the cutting wind.",
                                "ru": "Он натянул свой <w>бафф</w> до самого носа, чтобы защититься от пронизывающего ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "colsjaal",
                        "nekwarmer"
                    ],
                    "common_phrases": [
                        "een warme buff",
                        "buff dragen"
                    ],
                    "target_lang_definitions": {
                        "en": "A tubular scarf made of elastic fabric worn around the neck or head for protection against cold, wind, or dust.",
                        "ru": "Трубчатый шарф из эластичной ткани, который носят на шее или голове для защиты от холода, ветра или пыли."
                    },

                {
                    "sense_id": "059cf1a7-84a4-3c96-bb7a-f9eabb8fe5f4",
                    "sense_definition": "Met een grote afstand van de oppervlakte tot de bodem; ver naar beneden reikend.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water in dit zwembad is erg <w>diep</w>, dus je kunt hier niet staan.",
                            "target_lang_translations": {
                                "en": "The water in this pool is very <w>deep</w>, so you cannot stand here.",
                                "ru": "Вода в этом бассейне очень <w>глубокая</w>, поэтому здесь нельзя стоять."
                            }
                        },
                        {
                            "text": "Ze groeven een <w>diep</w> gat in het zand om de schat te verstoppen.",
                            "target_lang_translations": {
                                "en": "They dug a <w>deep</w> hole in the sand to hide the treasure.",
                                "ru": "Они вырыли <w>глубокую</w> яму в песке, чтобы спрятать сокровище."
                            }
                        }
                    ],
                    "antonyms": [
                        "ondiep"
                    ],
                    "common_phrases": [
                        "in het diepe springen",
                        "diep water"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a large distance from the surface to the bottom; reaching far down.",
                        "ru": "Имеющий большое расстояние от поверхности до дна; простирающийся далеко вниз."
                    },
                    "translations": {

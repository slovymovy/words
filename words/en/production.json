{
    "word_family": [
        "produce",
        "producer",
        "product",
        "productive",
        "productivity",
        "reproduce",
        "reproduction",
        "counterproductive"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db307e3f-67f9-3d45-8cbc-f75c00d34399",
                    "sense_definition": "The process of making, manufacturing, or growing goods and materials, especially in large quantities.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manufacturing_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory halted <w>production</w> due to the unexpected strike.",
                            "target_lang_translations": {
                                "ru": "Завод остановил <w>производство</w> из-за неожиданной забастовки."
                            }
                        },
                        {
                            "text": "Mass <w>production</w> has lowered the cost of many household items.",
                            "target_lang_translations": {
                                "ru": "Массовое <w>производство</w> снизило стоимость многих бытовых товаров."
                            }
                        }
                    ],
                    "synonyms": [
                        "manufacturing",
                        "fabrication",
                        "creation",
                        "construction"
                    ],
                    "antonyms": [
                        "destruction",
                        "consumption"
                    ],
                    "common_phrases": [
                        "mass production",
                        "industrial production",
                        "food production",
                        "production line"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс изготовления, обработки или выращивания товаров и материалов, особенно в больших количествах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производство",
                                "target_lang_sense_clarification": "общий процесс создания товаров"
                            },
                            {
                                "target_lang_word": "изготовление",
                                "target_lang_sense_clarification": "создание конкретных изделий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21097a70-3c88-313c-9c98-01f0197ca164",
                    "sense_definition": "A film, play, television program, or musical recording; the process of organizing and presenting such a work.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "arts_entertainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She starred in a new <w>production</w> of Romeo and Juliet at the local theater.",
                            "target_lang_translations": {
                                "ru": "Она сыграла главную роль в новой <w>постановке</w> «Ромео и Джульетты» в местном театре."
                            }
                        },
                        {
                            "text": "The cost of the film <w>production</w> exceeded the initial budget.",
                            "target_lang_translations": {
                                "ru": "Стоимость <w>производства</w> фильма превысила первоначальный бюджет."
                            }
                        }
                    ],
                    "synonyms": [
                        "performance",
                        "show",
                        "staging",
                        "presentation"
                    ],
                    "common_phrases": [
                        "film production",
                        "stage production",
                        "theater production",
                        "television production"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фильм, спектакль, телепрограмма или музыкальная запись; процесс организации и представления такой работы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постановка",
                                "target_lang_sense_clarification": "театральный спектакль или шоу"
                            },
                            {
                                "target_lang_word": "производство",
                                "target_lang_sense_clarification": "процесс создания фильма или программы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66ade1bb-dbf6-3a41-badb-ae237cd29388",
                    "sense_definition": "The total amount of something that is made or grown.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "manufacturing_output",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wheat <w>production</w> has fallen significantly this year due to the drought.",
                            "target_lang_translations": {
                                "ru": "<w>Производство</w> пшеницы в этом году значительно сократилось из-за засухи."
                            }
                        },
                        {
                            "text": "The company aims to increase <w>production</w> to meet the rising demand.",
                            "target_lang_translations": {
                                "ru": "Компания стремится увеличить <w>объём производства</w>, чтобы удовлетворить растущий спрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "output",
                        "yield"
                    ],
                    "common_phrases": [
                        "increase production",
                        "production levels",
                        "annual production",
                        "domestic production"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее количество чего-либо произведенного или выращенного"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объём производства",
                                "target_lang_sense_clarification": "количество выпущенной продукции"
                            },
                            {
                                "target_lang_word": "добыча",
                                "target_lang_sense_clarification": "объём извлеченных природных ресурсов"
                            },
                            {
                                "target_lang_word": "выработка",
                                "target_lang_sense_clarification": "количество продукции на единицу времени или труда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "303f4c08-9961-33c2-9fe9-bc8eb34d16f5",
                    "sense_definition": "The live environment where finished software or code is used by actual users, as opposed to a testing or development environment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing_env",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We discovered a critical bug in the <w>production</w> database.",
                            "target_lang_translations": {
                                "ru": "Мы обнаружили критическую ошибку в <w>рабочей</w> базе данных."
                            }
                        },
                        {
                            "text": "The team is preparing to deploy the new features to <w>production</w> tonight.",
                            "target_lang_translations": {
                                "ru": "Команда готовится развернуть новые функции в <w>рабочей среде</w> сегодня вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "live environment"
                    ],
                    "antonyms": [
                        "development",
                        "staging",
                        "testing"
                    ],
                    "common_phrases": [
                        "production environment",
                        "in production",
                        "production server"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рабочая среда, где готовое программное обеспечение используется реальными пользователями"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рабочая среда",
                                "target_lang_sense_clarification": "официальный термин в IT"
                            },
                            {
                                "target_lang_word": "продакшн",
                                "target_lang_sense_clarification": "профессиональный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "937ed755-6821-3eaf-8657-6169d21db11d",
                    "sense_definition": "The act of showing something officially, such as a document, ticket, or evidence, for inspection.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_showing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Entry to the secure facility is permitted only upon <w>production</w> of a valid ID card.",
                            "target_lang_translations": {
                                "ru": "Вход на охраняемый объект разрешен только по <w>предъявлении</w> действительного удостоверения личности."
                            }
                        },
                        {
                            "text": "The <w>production</w> of these documents is required for the audit to proceed.",
                            "target_lang_translations": {
                                "ru": "Для проведения аудита требуется <w>предъявление</w> этих документов."
                            }
                        }
                    ],
                    "synonyms": [
                        "presentation",
                        "display",
                        "showing"
                    ],
                    "antonyms": [
                        "concealment",
                        "hiding"
                    ],
                    "common_phrases": [
                        "on production of",
                        "production of evidence",
                        "production of documents"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по официальному показу чего-либо (документа, билета) для проверки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предъявление",
                                "target_lang_sense_clarification": "показ документов или билетов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aec821bb-df35-3b56-b8a1-0c0f3a73b16a",
                    "sense_definition": "An occasion where someone makes a simple task or situation seem much more difficult, important, or complicated than it needs to be.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic_fuss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made a huge <w>production</w> out of choosing a tie for the party.",
                            "target_lang_translations": {
                                "ru": "Он устроил целый <w>спектакль</w> из выбора галстука для вечеринки."
                            }
                        },
                        {
                            "text": "Don't make a <w>production</w> of it; just sign the paper and leave.",
                            "target_lang_translations": {
                                "ru": "Не устраивай из этого <w>сцену</w>; просто подпиши бумагу и уходи."
                            }
                        }
                    ],
                    "synonyms": [
                        "fuss",
                        "hassle",
                        "scene"
                    ],
                    "common_phrases": [
                        "make a production",
                        "big production"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'make a production out of'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда кто-то необоснованно усложняет простую задачу или придает ей излишнюю важность"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спектакль",
                                "target_lang_sense_clarification": "перен. разыгрывание сцены, притворство"
                            },
                            {
                                "target_lang_word": "церемония",
                                "target_lang_sense_clarification": "излишние сложности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ecb06bf-5752-38a3-9a2e-42071f584df8",
                    "sense_definition": "The legal process of providing written documents or evidence to support a case or defense.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_discovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge ordered the immediate <w>production</w> of all emails related to the contract.",
                            "target_lang_translations": {
                                "ru": "Судья приказал немедленно <w>представить</w> все электронные письма, связанные с контрактом."
                            }
                        },
                        {
                            "text": "Delays in the <w>production</w> of evidence caused the trial to be postponed.",
                            "target_lang_translations": {
                                "ru": "Задержки в <w>представлении</w> доказательств привели к отсрочке судебного разбирательства."
                            }
                        }
                    ],
                    "synonyms": [
                        "disclosure",
                        "discovery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридический процесс предоставления письменных документов или доказательств для поддержки дела"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "подача доказательств суду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54df34de-4ac3-3c50-9790-8a935b23e90d",
                    "sense_definition": "Emitować lub wydzielać światło, być źródłem jasności",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce mocno <w>świeciło</w> przez cały dzień, ogrzewając ziemię.",
                            "target_lang_translations": {
                                "ru": "Солнце ярко <w>светило</w> весь день, согревая землю."
                            }
                        },
                        {
                            "text": "W oddali <w>świeciły</w> światła wielkiego miasta, wskazując nam drogę.",
                            "target_lang_translations": {
                                "ru": "Вдали <w>светились</w> огни большого города, указывая нам путь."
                            }
                        }
                    ],
                    "synonyms": [
                        "jaśnieć",
                        "palić się"
                    ],
                    "common_phrases": [
                        "słońce świeci",
                        "księżyc świeci",
                        "lampa świeci",
                        "gwiazdy świecą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излучать или испускать свет, быть источником света"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светить",
                                "target_lang_sense_clarification": "излучать свет"
                            },
                            {
                                "target_lang_word": "сиять",
                                "target_lang_sense_clarification": "ярко светить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fb4ad35-148d-307a-8d04-3cb08f298b0f",
                    "sense_definition": "Odbijać światło, lśnić lub błyszczeć (zwykle używane z zaimkiem „się”)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "light_reflection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej oczy <w>świeciły</w> się ze szczęścia, gdy usłyszała dobrą nowinę.",
                            "target_lang_translations": {
                                "ru": "Её глаза <w>светились</w> от счастья, когда она услышала хорошую новость."
                            }
                        },
                        {
                            "text": "Wypolerowane buty żołnierzy <w>świeciły</w> się w słońcu.",
                            "target_lang_translations": {
                                "ru": "Начищенные сапоги солдат <w>блестели</w> на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "jaśnieć"
                    ],
                    "common_phrases": [
                        "oczy się świecą",
                        "świecić się jak psu jajca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отражать свет, блестеть или сиять (обычно используется с возвратной частицей)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светиться",
                                "target_lang_sense_clarification": "излучать свет или радость"
                            },
                            {
                                "target_lang_word": "блестеть",
                                "target_lang_sense_clarification": "отражать свет"
                            },
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "ярко блестеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8ba964c-1a73-3b5c-ae10-83a35a049d4d",
                    "sense_definition": "Kierować strumień światła na określony obiekt lub w określonym kierunku za pomocą urządzenia",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "light_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mi <w>świecić</w> latarką pod nogi, bo jest tu bardzo ciemno.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>светить</w> мне фонариком под ноги, потому что здесь очень темно."
                            }
                        },
                        {
                            "text": "Lekarz <w>świecił</w> pacjentowi w gardło małą lampką diagnostyczną.",
                            "target_lang_translations": {
                                "ru": "Врач <w>светил</w> пациенту в горло маленькой диагностической лампочкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świecić latarką",
                        "świecić w oczy",
                        "świecić po oczach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять луч света на объект или в определенном направлении"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светить",
                                "target_lang_sense_clarification": "направлять свет"
                            },
                            {
                                "target_lang_word": "освещать",
                                "target_lang_sense_clarification": "делать светлым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36c9c574-3565-3383-8f4f-98060cbc1ddb",
                    "sense_definition": "Ostentacyjnie pokazywać lub odsłaniać coś, zwłaszcza części ciała lub nagość; przenośnie także wstydzić się za kogoś",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exposure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wypada tak <w>świecić</w> gołym brzuchem w miejscu publicznym.",
                            "target_lang_translations": {
                                "ru": "Не подобает так <w>светить</w> голым животом в общественном месте."
                            }
                        },
                        {
                            "text": "Musiałem za ciebie <w>świecić</w> oczami przed szefem z powodu twojego spóźnienia.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось за тебя <w>краснеть</w> перед начальником из-за твоего опоздания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świecić oczami",
                        "świecić przykładem",
                        "świecić golizną"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выставлять напоказ, демонстрировать (особенно части тела); перен. стыдиться за кого-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светить",
                                "target_lang_sense_clarification": "показывать (разг.)"
                            },
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "мелькать, оголять"
                            },
                            {
                                "target_lang_word": "краснеть",
                                "target_lang_sense_clarification": "стыдиться (в идиомах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4894bf4b-e2d1-365d-b997-9a14d0006f2c",
                    "sense_definition": "Mówić nieprawdę, zmyślać lub opowiadać bzdury",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wierz w ani jedno jego słowo, on znowu <w>świeci</w>.",
                            "target_lang_translations": {
                                "ru": "Не верь ни единому его слову, он снова <w>заливает</w>."
                            }
                        },
                        {
                            "text": "Przestań mi tu <w>świecić</w> i powiedz wreszcie, jak było naprawdę.",
                            "target_lang_translations": {
                                "ru": "Перестань мне тут <w>заливать</w> и скажи наконец, как было на самом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "trajlować"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить неправду, выдумывать или рассказывать небылицы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заливать",
                                "target_lang_sense_clarification": "врать (слэнг)"
                            },
                            {
                                "target_lang_word": "врать",
                                "target_lang_sense_clarification": "говорить неправду"
                            }
                        ]
                    }
                }
            ]
        }
    ]

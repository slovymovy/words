    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "40ee3350-12bb-362b-9921-ffbc695ad7a4",
                    "sense_definition": "One's parents.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "parents_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to ask my <w>olds</w> if I can borrow the car for the weekend.",
                            "target_lang_translations": {
                                "ru": "Мне нужно спросить у <w>предков</w>, можно ли взять машину на выходные."
                            }
                        },
                        {
                            "text": "His <w>olds</w> are coming to visit him at university next week.",
                            "target_lang_translations": {
                                "ru": "Его <w>старики</w> приезжают навестить его в университете на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "parents",
                        "folks"
                    ],
                    "antonyms": [
                        "kids",
                        "children"
                    ],
                    "common_phrases": [
                        "my olds",
                        "the olds"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in British or Australian English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Родители (разговорное или сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предки",
                                "target_lang_sense_clarification": "родители (молодежный сленг)"
                            },
                            {
                                "target_lang_word": "старики",
                                "target_lang_sense_clarification": "родители (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5e9ff9e-1cca-335c-a98b-4164b1e52d8f",
                    "sense_definition": "People who are not considered young, often middle-aged or older adults.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>olds</w> just don't understand how social media works these days.",
                            "target_lang_translations": {
                                "ru": "<w>Старики</w> просто не понимают, как сейчас работают соцсети."
                            }
                        },
                        {
                            "text": "Don't worry about what the <w>olds</w> think; just do what makes you happy.",
                            "target_lang_translations": {
                                "ru": "Не парься о том, что думают <w>старики</w>; просто делай то, что делает тебя счастливым."
                            }
                        }
                    ],
                    "synonyms": [
                        "oldsters",
                        "seniors"
                    ],
                    "antonyms": [
                        "youths",
                        "youngsters"
                    ],
                    "common_phrases": [
                        "us olds"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used dismissively or humorously by younger people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, которые не считаются молодыми; часто люди среднего или пожилого возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старики",
                                "target_lang_sense_clarification": "пожилые люди (часто пренебрежительно)"
                            },
                            {
                                "target_lang_word": "старшее поколение",
                                "target_lang_sense_clarification": "люди старшего возраста"
                            }
                        ]
                    }
                },
                {

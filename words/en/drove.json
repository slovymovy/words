    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "866f608c-1521-322d-aff9-d8f76f485786",
                    "sense_definition": "Simple past of <w>drive</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>drove</w> the car carefully down the icy road.",
                            "target_lang_translations": {
                                "ru": "Она осторожно <w>вела</w> машину по обледенелой дороге."
                            }
                        },
                        {
                            "text": "We <w>drove</w> to the beach to watch the sunset.",
                            "target_lang_translations": {
                                "ru": "Мы <w>поехали</w> на пляж, чтобы посмотреть на закат."
                            }
                        },
                        {
                            "text": "The loud noise nearly <w>drove</w> me crazy.",
                            "target_lang_translations": {
                                "ru": "Громкий шум чуть не <w>свел</w> меня с ума."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drove home",
                        "drove away",
                        "drove off",
                        "drove crazy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время от глагола <w>drive</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вел",
                                "target_lang_sense_clarification": "управлял машиной"
                            },
                            {
                                "target_lang_word": "ехал",
                                "target_lang_sense_clarification": "передвигался на транспорте"
                            },
                            {
                                "target_lang_word": "свел",
                                "target_lang_sense_clarification": "в выражении 'свести с ума'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ef1caef-ed6e-3789-980e-c1c408d023ab",
                    "sense_definition": "A large number of people or animals moving or acting together.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tourists arrived in <w>droves</w> to see the famous monument.",
                            "target_lang_translations": {
                                "ru": "Туристы прибывали <w>толпами</w>, чтобы увидеть знаменитый памятник."
                            }
                        },
                        {
                            "text": "People are leaving the countryside in <w>droves</w> to find work in the city.",
                            "target_lang_translations": {
                                "ru": "Люди <w>массово</w> покидают сельскую местность, чтобы найти работу в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "drive",
                        "drive"
                    ],
                    "common_phrases": [
                        "in droves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество людей или животных, движущихся или действующих вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толпа",
                                "target_lang_sense_clarification": "большое скопление людей"
                            },
                            {
                                "target_lang_word": "множество",
                                "target_lang_sense_clarification": "большое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cb9b866-b591-31d6-9cb4-393543b14feb",
                    "sense_definition": "A herd of animals, especially cattle, being driven in a group.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer moved the <w>drove</w> of sheep to the upper pasture.",
                            "target_lang_translations": {
                                "ru": "Фермер перегнал <w>отару</w> овец на верхнее пастбище."
                            }
                        },
                        {
                            "text": "We encountered a large <w>drove</w> of cattle blocking the country lane.",
                            "target_lang_translations": {
                                "ru": "Мы столкнулись с большим <w>гуртом</w> скота, перегородившим проселочную дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стадо животных, особенно крупного рогатого скота, перегоняемое группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гурт",
                                "target_lang_sense_clarification": "стадо скота"
                            },
                            {
                                "target_lang_word": "стадо",
                                "target_lang_sense_clarification": "группа животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecb622d3-5c38-3dc0-a8dd-28c96db420ca",
                    "sense_definition": "A road or track along which cattle are habitually driven.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They walked along the old grassy <w>drove</w> that crossed the marsh.",
                            "target_lang_translations": {
                                "ru": "Они шли по старому, заросшему травой <w>скотопрогону</w>, пересекавшему болото."
                            }
                        },
                        {
                            "text": "The ancient <w>drove</w> is now used mostly by hikers and cyclists.",
                            "target_lang_translations": {
                                "ru": "Древняя <w>дорога для перегона скота</w> теперь используется в основном туристами и велосипедистами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорога или тропа, по которой обычно перегоняют скот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скотопрогон",
                                "target_lang_sense_clarification": "путь для перегона скота"
                            },
                            {
                                "target_lang_word": "дорога для перегона",
                                "target_lang_sense_clarification": "специальная дорога"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99a7f11c-ff77-34bd-b09b-7cd4577c4ba6",
                    "sense_definition": "A broad chisel used by stonemasons to smooth stone surfaces.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mason used a <w>drove</w> to finish the face of the block.",
                            "target_lang_translations": {
                                "ru": "Каменщик использовал <w>скарпель</w>, чтобы отделать лицевую сторону блока."
                            }
                        },
                        {
                            "text": "Marks left by the <w>drove</w> were visible on the limestone wall.",
                            "target_lang_translations": {
                                "ru": "На известняковой стене были видны следы от <w>скарпели</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкое долото, используемое каменщиками для выравнивания каменных поверхностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скарпель",
                                "target_lang_sense_clarification": "инструмент каменщика"
                            },
                            {
                                "target_lang_word": "долото",
                                "target_lang_sense_clarification": "инструмент для обработки камня"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d0a42da-bf92-3348-a0a8-394990d35219",
                    "sense_definition": "To herd animals, particularly over a long distance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent his youth <w>droving</w> cattle across the Australian outback.",
                            "target_lang_translations": {
                                "ru": "Он провел молодость, <w>перегоняя</w> скот через австралийский аутбек."
                            }
                        },
                        {
                            "text": "They <w>droved</w> the sheep from the mountains down to the valley for winter.",
                            "target_lang_translations": {
                                "ru": "Они <w>перегнали</w> овец с гор в долину на зиму."
                            }
                        }
                    ],
                    "synonyms": [
                        "drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сгонять животных, особенно для перегона на большие расстояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перегонять",
                                "target_lang_sense_clarification": "гнать скот"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        {
                            "text": "He worked as an undercover <w>operative</w> for the government for ten years.",
                            "target_lang_translations": {
                                "ru": "Он десять лет работал тайным <w>агентом</w> правительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "agent",
                        "spy",
                        "mole"

                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "распродавать остатки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bdb28c1-7d94-3b81-9f5b-126e8d585113",

                        "ru": [
                            {
                                "target_lang_word": "нарисованный",
                                "target_lang_sense_clarification": "созданный от руки"
                            },
                            {
                                "target_lang_word": "рисованный",
                                "target_lang_sense_clarification": "относящийся к графике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef80da43-59d2-35c2-bf5b-1f4b2a324680",
                    "sense_definition": "Zichtbaar beschadigd of verouderd door zorgen, ziekte of zware ervaringen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De door het leven <w>getekende</w> vrouw ging toch moedig door.",
                            "target_lang_translations": {
                                "en": "The woman, <w>marked</w> by life, nevertheless continued courageously.",
                                "ru": "Женщина, <w>отмеченная</w> тяжелой жизнью, всё же мужественно продолжала идти вперёд."
                            }
                        },
                        {
                            "text": "Zijn gezicht was <w>getekend</w> door jaren van zwaar werk in de mijn.",
                            "target_lang_translations": {
                                "en": "His face was <w>lined</w> by years of hard work in the mine.",
                                "ru": "Его лицо было <w>изборождено</w> годами тяжелой работы в шахте."
                            }
                        }
                    ],
                    "synonyms": [

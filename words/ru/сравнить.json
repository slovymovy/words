{
    "word_family": [
        "сравнение",
        "сравнительный",
        "сравнимый",
        "несравнимый"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5df55c19-ba22-3b6a-ae17-9bdc569cf30c",
                    "sense_definition": "Сопоставить кого-либо или что-либо для установления сходства или различия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <сравнить> цены в разных магазинах перед покупкой.",
                            "target_lang_translations": {
                                "en": "We decided to <compare> prices in different stores before buying."
                            }
                        },
                        {
                            "text": "Учитель попросил учеников <сравнить> два текста и найти ошибки.",
                            "target_lang_translations": {
                                "en": "The teacher asked the students to <compare> the two texts and find errors."
                            }
                        }
                    ],
                    "synonyms": [
                        "сопоставить"
                    ],
                    "common_phrases": [
                        "сравнить цены",
                        "сравнить результаты",
                        "сравнить с оригиналом",
                        "сравнить два варианта"
                    ],
                    "target_lang_definitions": {
                        "en": "To examine two or more things in order to note similarities and differences."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "compare",
                                "target_lang_sense_clarification": "to examine for similarities and differences"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8adb53c-f59e-3f33-a32f-6366717bb4ec",
                    "sense_definition": "Признать равным или очень похожим; уподобить кому-либо или чему-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его талант нельзя <сравнить> ни с чьим другим.",
                            "target_lang_translations": {
                                "en": "His talent cannot be <compared> with anyone else's."
                            }
                        },
                        {
                            "text": "Поэт в своём стихотворении решил <сравнить> её глаза со звёздами.",
                            "target_lang_translations": {
                                "en": "The poet decided to <compare> her eyes to the stars in his poem."
                            }
                        }
                    ],
                    "synonyms": [
                        "приравнять",
                        "уподобить"
                    ],
                    "common_phrases": [
                        "ничто не сравнить",
                        "нельзя сравнить",
                        "сравнить с землёй"
                    ],
                    "target_lang_definitions": {
                        "en": "To consider something as similar or equal to something else; to liken."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "compare",
                                "target_lang_sense_clarification": "to liken or equate"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
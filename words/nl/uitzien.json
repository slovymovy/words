{
    "word_family": [
        "uitzicht"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b16b04cd-c010-3d53-b47d-339c3741097a",
                    "sense_definition": "Een bepaald uiterlijk hebben of een bepaalde indruk wekken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jij <w>ziet</w> er vandaag prachtig uit.",
                            "target_lang_translations": {
                                "en": "You <w>look</w> beautiful today.",
                                "ru": "Ты сегодня прекрасно <w>выглядишь</w>."
                            }
                        },
                        {
                            "text": "Het oude huis <w>zag</w> er verlaten en vervallen uit.",
                            "target_lang_translations": {
                                "en": "The old house <w>looked</w> abandoned and dilapidated.",
                                "ru": "Старый дом <w>выглядел</w> заброшенным и ветхим."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er goed uitzien",
                        "eruitzien als"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "meestal gebruikt in de combinatie 'eruitzien'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To have a particular appearance or give a certain impression.",
                        "ru": "Иметь определенную внешность или производить определенное впечатление."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "to have a certain appearance"
                            },
                            {
                                "target_lang_word": "look like",
                                "target_lang_sense_clarification": "to resemble or appear as"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "иметь определенный внешний вид"
                            },
                            {
                                "target_lang_word": "смотреться",
                                "target_lang_sense_clarification": "выглядеть определенным образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78d37004-6776-36c2-8aaf-38736105c545",
                    "sense_definition": "Met verlangen wachten op iets dat gaat gebeuren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anticipation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zie</w> ernaar uit om samen aan de slag te gaan.",
                            "target_lang_translations": {
                                "en": "I <w>look forward to</w> getting started together.",
                                "ru": "Я <w>с нетерпением жду</w> начала нашей совместной работы."
                            }
                        },
                        {
                            "text": "Wij <w>zien</w> erg uit naar onze vakantie in Frankrijk.",
                            "target_lang_translations": {
                                "en": "We really <w>look forward to</w> our holiday in France.",
                                "ru": "Мы очень <w>ждем</w> нашего отпуска во Франции."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitkijken",
                        "verheugen"
                    ],
                    "common_phrases": [
                        "uitzien naar",
                        "ernaar uitzien"
                    ],
                    "target_lang_definitions": {
                        "en": "To wait with pleasure or eagerness for something that is going to happen.",
                        "ru": "С нетерпением или радостью ожидать чего-то, что должно произойти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look forward to",
                                "target_lang_sense_clarification": "to anticipate with pleasure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ждать с нетерпением",
                                "target_lang_sense_clarification": "ожидать чего-то приятного"
                            },
                            {
                                "target_lang_word": "предвкушать",
                                "target_lang_sense_clarification": "заранее радоваться чему-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8074ce58-2555-3d1a-88a4-c7f0e71b5f22",
                    "sense_definition": "De indruk wekken dat iets zal gebeuren; lijken alsof.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>ziet</w> ernaar uit dat het vandaag gaat regenen.",
                            "target_lang_translations": {
                                "en": "It <w>looks</w> like it is going to rain today.",
                                "ru": "<w>Похоже</w>, что сегодня пойдет дождь."
                            }
                        },
                        {
                            "text": "Het <w>ziet</w> ernaar uit dat we de wedstrijd gaan winnen.",
                            "target_lang_translations": {
                                "en": "It <w>looks</w> like we are going to win the match.",
                                "ru": "<w>Похоже</w>, что мы выиграем матч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het ziet ernaar uit"
                    ],
                    "target_lang_definitions": {
                        "en": "To give the impression that something is likely to happen; to seem as if.",
                        "ru": "Создавать впечатление, что что-то вероятно произойдет; казаться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look like",
                                "target_lang_sense_clarification": "to seem probable"
                            },
                            {
                                "target_lang_word": "look as if",
                                "target_lang_sense_clarification": "to give the impression that"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "похоже",
                                "target_lang_sense_clarification": "создается впечатление (вводное слово)"
                            },
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
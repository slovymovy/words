            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f048bfa0-2b9b-3318-99dc-5c1877a5acb2",
                    "sense_definition": "Sprawić, że ktoś lub coś przestaje się poruszać lub nie może wykonywać ruchów; wyłączyć urządzenie lub mechanizm z działania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "immobilization_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz musiał natychmiast <unieruchomić> złamaną nogę pacjenta, zakładając gips.",
                            "target_lang_translations": {
                                "ru": "Врач должен был немедленно <зафиксировать> сломанную ногу пациента, наложив гипс."
                            }
                        },
                        {
                            "text": "Awaria elektroniki potrafi całkowicie <unieruchomić> nowoczesny samochód na środku drogi.",
                            "target_lang_translations": {
                                "ru": "Сбой электроники может полностью <обездвижить> современный автомобиль посреди дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "zablokować",
                        "zatrzymać",
                        "obezwładnić"
                    ],
                    "antonyms": [
                        "uruchomić"
                    ],
                    "common_phrases": [
                        "unieruchomić kończynę",
                        "unieruchomić pojazd",
                        "trwale unieruchomić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать кого-либо или что-либо неподвижным; лишить возможности двигаться или вывести механизм из строя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обездвижить",
                                "target_lang_sense_clarification": "лишить возможности двигаться"
                            },
                            {
                                "target_lang_word": "зафиксировать",
                                "target_lang_sense_clarification": "закрепить в неподвижном положении (часто в медицине)"
                            },
                            {
                                "target_lang_word": "остановить",
                                "target_lang_sense_clarification": "прекратить движение или работу механизма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe5797b6-51e3-3e42-ad91-90f8342f5ff6",
                    "sense_definition": "Zastygnąć w bezruchu; przestać się poruszać (używane z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "immobilization_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestraszone zwierzę <unieruchomiło> się w trawie, próbując uniknąć wzroku drapieżnika.",
                            "target_lang_translations": {
                                "ru": "Испуганное животное <застыло> в траве, пытаясь избежать взгляда хищника."
                            }
                        },
                        {
                            "text": "Mechanizm zamka <unieruchomił> się z powodu rdzy i nie można go otworzyć.",
                            "target_lang_translations": {
                                "ru": "Механизм замka <заклинил> из-за ржавчины, и его невозможно открыть."
                            }
                        }
                    ],
                    "synonyms": [
                        "zastygnąć",
                        "zatrzymać się"
                    ],
                    "common_phrases": [
                        "nagle się unieruchomić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застыть в неподвижности; перестать двигаться (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застыть",
                                "target_lang_sense_clarification": "стать неподвижным (о живых существах)"
                            },
                            {
                                "target_lang_word": "заклинить",
                                "target_lang_sense_clarification": "перестать работать, застрять (о механизмах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c4d104cd-77a1-3b85-80bd-8824536a0ce9",
                    "sense_definition": "Een deels open constructie die dient om een gebied af te scheiden van de omgeving.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen klommen over het <w>hek</w> om hun bal uit de tuin te halen.",
                            "target_lang_translations": {
                                "en": "The children climbed over the <w>fence</w> to get their ball out of the garden."
                            }
                        },
                        {
                            "text": "Er staat een hoog ijzeren <w>hek</w> om het schoolplein heen.",
                            "target_lang_translations": {
                                "en": "There is a high iron <w>fence</w> surrounding the schoolyard."
                            }
                        }
                    ],
                    "synonyms": [
                        "omheining",
                        "afscheiding"
                    ],
                    "common_phrases": [
                        "over het hek",
                        "achter het hek",
                        "het hek sluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "A barrier, typically of wood or wire, enclosing an area to mark a boundary or control access."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fence",
                                "target_lang_sense_clarification": "barrier enclosing an area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fb83eac-8915-307a-ab62-2ba1fe30bfc1",
                    "sense_definition": "Een draaibaar deel van een omheining dat als toegang wordt gebruikt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeet niet het <w>hek</w> achter je dicht te doen als je weggaat.",
                            "target_lang_translations": {
                                "en": "Don't forget to close the <w>gate</w> behind you when you leave."
                            }
                        },
                        {
                            "text": "Hij opende het <w>hek</w> zodat de auto de oprit op kon rijden.",
                            "target_lang_translations": {
                                "en": "He opened the <w>gate</w> so the car could drive onto the driveway."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het hek openen",
                        "door het hek"
                    ],
                    "target_lang_definitions": {
                        "en": "A movable barrier, usually on hinges, closing an opening in a fence, wall, or other enclosure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gate",
                                "target_lang_sense_clarification": "movable entrance in a fence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25322d61-1d9d-37dd-b3ea-b7f94c7379c5",
                    "sense_definition": "De achterzijde van een schip.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De naam van het schip stond in grote letters op het <w>hek</w> geschilderd.",
                            "target_lang_translations": {
                                "en": "The ship's name was painted in large letters on the <w>stern</w>."
                            }
                        },
                        {
                            "text": "Vanaf het <w>hek</w> keken de passagiers naar het kielzog in het water.",
                            "target_lang_translations": {
                                "en": "From the <w>stern</w>, the passengers looked at the wake in the water."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The rear part of a ship or boat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stern",
                                "target_lang_sense_clarification": "rear part of a ship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57084deb-aa6f-3a6c-93fd-0c25f7807d0d",
                    "sense_definition": "Het raamwerk van latten aan de wiek van een molen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De molenaar moest het beschadigde <w>hek</w> repareren na de zware storm.",
                            "target_lang_translations": {
                                "en": "The miller had to repair the damaged <w>frame</w> after the severe storm."
                            }
                        },
                        {
                            "text": "Zonder het <w>hek</w> kan de wiek van de molen niet genoeg wind vangen.",
                            "target_lang_translations": {
                                "en": "Without the <w>frame</w>, the windmill sail cannot catch enough wind."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The latticework or framework on the sail of a windmill that supports the canvas."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "frame",
                                "target_lang_sense_clarification": "structure of a windmill sail"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "3993bb0e-fbbe-33ea-84e5-39a334952bd5",
                    "sense_definition": "A room or area used for cooking and preparing food.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is baking a chocolate cake in the <w>kitchen</w>.",
                            "target_lang_translations": {
                                "ru": "Она печет шоколадный торт на <w>кухне</w>."
                            }
                        },
                        {
                            "text": "Please put your dirty dishes in the <w>kitchen</w> sink.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сложи грязную посуду в раковину на <w>кухне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kitchenette",
                        "galley",
                        "scullery"
                    ],
                    "common_phrases": [
                        "kitchen sink",
                        "soup kitchen",
                        "kitchen table",
                        "kitchen cabinet",
                        "hell's kitchen"

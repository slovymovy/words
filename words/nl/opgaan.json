{
    "word_family": [
        "opgang",
        "opgaand"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "879c3a40-abf6-33f2-8796-c25fc666317a",
                    "sense_definition": "Zichtbaar worden boven de horizon (van hemellichamen).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer <gaat> de zon heel vroeg <op>.",
                            "target_lang_translations": {
                                "en": "In the summer, the sun <rises> very early.",
                                "ru": "Летом солнце <всходит> очень рано."
                            }
                        },
                        {
                            "text": "We keken samen hoe de volle maan <opging> boven de zee.",
                            "target_lang_translations": {
                                "en": "We watched together as the full moon <rose> above the sea.",
                                "ru": "Мы вместе смотрели, как полная луна <всходила> над морем."
                            }
                        }
                    ],
                    "synonyms": [
                        "opkomen",
                        "rijzen"
                    ],
                    "antonyms": [
                        "ondergaan"
                    ],
                    "common_phrases": [
                        "de zon gaat op"
                    ],
                    "target_lang_definitions": {
                        "en": "To become visible above the horizon (of celestial bodies).",
                        "ru": "Становиться видимым над горизонтом (о солнце, луне и звездах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "of the sun, moon, or stars"
                            },
                            {
                                "target_lang_word": "come up",
                                "target_lang_sense_clarification": "appear above the horizon"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "всходить",
                                "target_lang_sense_clarification": "о небесных телах"
                            },
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "над горизонтом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99fc1a2e-acf9-394d-b4e0-23a395d26af5",
                    "sense_definition": "Deelnemen aan een examen of toets om een diploma te behalen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer <gaat> volgende week <op> voor zijn rijexamen.",
                            "target_lang_translations": {
                                "en": "My brother is <taking> his driving test next week.",
                                "ru": "Мой брат на следующей неделе <идет сдавать> экзамен по вождению."
                            }
                        },
                        {
                            "text": "Zij is al twee keer <opgegaan> voor haar diploma, maar helaas gezakt.",
                            "target_lang_translations": {
                                "en": "She has already <sat for> her diploma twice, but unfortunately failed.",
                                "ru": "Она уже дважды <пыталась сдать> на диплом, но, к сожалению, провалилась."
                            }
                        }
                    ],
                    "synonyms": [
                        "examen doen"
                    ],
                    "common_phrases": [
                        "opgaan voor een examen"
                    ],
                    "target_lang_definitions": {
                        "en": "To participate in an exam or test to obtain a qualification.",
                        "ru": "Сдавать экзамен или проходить испытание для получения квалификации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "an exam or test"
                            },
                            {
                                "target_lang_word": "sit for",
                                "target_lang_sense_clarification": "an exam"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "экзамен"
                            },
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "на экзамен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea8e75a9-a1fc-3fa1-9e72-86fa3a4a4db9",
                    "sense_definition": "Geldig zijn, kloppen of van toepassing zijn in een bepaalde situatie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat excuus <gaat> deze keer echt niet <op>.",
                            "target_lang_translations": {
                                "en": "That excuse really doesn't <hold> this time.",
                                "ru": "Это оправдание на этот раз точно не <пройдет>."
                            }
                        },
                        {
                            "text": "Die regel geldt voor de meeste mensen, maar <gaat> hier niet <op>.",
                            "target_lang_translations": {
                                "en": "That rule applies to most people, but does not <apply> here.",
                                "ru": "Это правило действует для большинства людей, но здесь оно не <применимо>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelden",
                        "kloppen",
                        "van toepassing zijn"
                    ],
                    "common_phrases": [
                        "dat gaat niet op"
                    ],
                    "target_lang_definitions": {
                        "en": "To be valid, correct, or applicable in a certain situation.",
                        "ru": "Быть действительным, верным или применимым в конкретной ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold true",
                                "target_lang_sense_clarification": "be valid or correct"
                            },
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "be relevant to a situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "об оправданиях или аргументах"
                            },
                            {
                                "target_lang_word": "быть применимым",
                                "target_lang_sense_clarification": "о правилах"
                            },
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "иметь силу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4172c575-b481-3908-9681-2a35fa8837fb",
                    "sense_definition": "Volledig in beslag genomen worden door een bezigheid en de omgeving vergeten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "absorption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen <gingen> tijdens het spelen helemaal <op> in hun fantasiewereld.",
                            "target_lang_translations": {
                                "en": "The children were completely <absorbed> in their fantasy world while playing.",
                                "ru": "Дети во время игры полностью <погрузились> в свой мир фантазий."
                            }
                        },
                        {
                            "text": "Hij kan urenlang <opgaan> in zijn werk zonder pauze te nemen.",
                            "target_lang_translations": {
                                "en": "He can <lose himself> in his work for hours without taking a break.",
                                "ru": "Он может часами <уходить с головой> в свою работу, не делая перерывов."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdiept zijn"
                    ],
                    "common_phrases": [
                        "opgaan in je werk",
                        "opgaan in het spel"
                    ],
                    "target_lang_definitions": {
                        "en": "To be completely occupied by an activity and forget one's surroundings.",
                        "ru": "Быть полностью поглощенным каким-либо занятием, забывая об окружающем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be absorbed",
                                "target_lang_sense_clarification": "in an activity or thought"
                            },
                            {
                                "target_lang_word": "lose oneself",
                                "target_lang_sense_clarification": "become fully immersed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "погружаться",
                                "target_lang_sense_clarification": "в занятие или мысли"
                            },
                            {
                                "target_lang_word": "уходить с головой",
                                "target_lang_sense_clarification": "полностью отдаваться делу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69c26a2d-4a3e-3165-ac0d-c832c3d8cd52",
                    "sense_definition": "Deel gaan uitmaken van een groter geheel of de achtergrond, waardoor het niet meer opvalt.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "absorption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dankzij zijn schutkleur <gaat> het dier volledig <op> in de omgeving.",
                            "target_lang_translations": {
                                "en": "Thanks to its camouflage, the animal completely <blends in> with the environment.",
                                "ru": "Благодаря своей защитной окраске животное полностью <сливается> с окружающей средой."
                            }
                        },
                        {
                            "text": "Het is de bedoeling dat dit kleine bedrijf zal <opgaan> in de grote multinational.",
                            "target_lang_translations": {
                                "en": "The intention is for this small company to <merge> into the large multinational.",
                                "ru": "Планируется, что эта небольшая компания <вольется> в крупную транснациональную корпорацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "versmelten",
                        "verdwijnen"
                    ],
                    "common_phrases": [
                        "opgaan in de menigte",
                        "opgaan in de omgeving"
                    ],
                    "target_lang_definitions": {
                        "en": "To become part of a larger whole or background, making it less noticeable.",
                        "ru": "Становиться частью чего-то большего или сливаться с фоном."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blend in",
                                "target_lang_sense_clarification": "match the surroundings"
                            },
                            {
                                "target_lang_word": "merge",
                                "target_lang_sense_clarification": "combine into a larger entity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сливаться",
                                "target_lang_sense_clarification": "с окружением"
                            },
                            {
                                "target_lang_word": "вливаться",
                                "target_lang_sense_clarification": "в состав организации"
                            },
                            {
                                "target_lang_word": "растворяться",
                                "target_lang_sense_clarification": "исчезать в чем-то большем"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2cb11ccb-ef5a-38ae-b022-1be23300e53e",
                    "sense_definition": "Сделанный более лёгким по весу, имеющий уменьшенную массу по сравнению с обычным.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Благодаря использованию пластика, новый корпус прибора стал <w>облегчённым</w> и удобным.",
                            "target_lang_translations": {
                                "en": "Thanks to the use of plastic, the new device body became <w>lightweight</w> and convenient."
                            }
                        },
                        {
                            "text": "Туристы купили специальный <w>облегчённый</w> рюкзак для длительных походов в горы.",
                            "target_lang_translations": {
                                "en": "The tourists bought a special <w>lightweight</w> backpack for long mountain hikes."
                            }
                        }
                    ],
                    "antonyms": [
                        "утяжелённый"
                    ],
                    "common_phrases": [
                        "облегчённая конструкция",
                        "облегчённый вариант",
                        "облегчённый вес"
                    ],
                    "target_lang_definitions": {
                        "en": "Made lighter in weight, having reduced mass compared to normal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lightweight",
                                "target_lang_sense_clarification": "regarding physical weight"
                            },
                            {
                                "target_lang_word": "lighter",
                                "target_lang_sense_clarification": "comparative weight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d6794a5-4700-303b-b24c-8a6e1775fcce",
                    "sense_definition": "Упрощённый, менее строгий или сложный; содержащий меньше трудностей.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "complexity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач рекомендовал пациенту <w>облегчённый</w> режим работы на время восстановления.",
                            "target_lang_translations": {
                                "en": "The doctor recommended a <w>light</w> work schedule for the patient during recovery."
                            }
                        },
                        {
                            "text": "Для начинающих студентов была разработана <w>облегчённая</w> программа обучения.",
                            "target_lang_translations": {
                                "en": "A <w>simplified</w> study program was developed for beginner students."
                            }
                        }
                    ],
                    "synonyms": [
                        "упрощённый"
                    ],
                    "antonyms": [
                        "усложнённый",
                        "затруднённый"
                    ],
                    "common_phrases": [
                        "облегчённый режим",
                        "облегчённая программа",
                        "облегчённая версия"
                    ],
                    "target_lang_definitions": {
                        "en": "Simplified, less strict or complex; containing fewer difficulties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "simplified",
                                "target_lang_sense_clarification": "made less complex"
                            },
                            {
                                "target_lang_word": "light",
                                "target_lang_sense_clarification": "version or regime"
                            },
                            {
                                "target_lang_word": "reduced",
                                "target_lang_sense_clarification": "lessened in strictness or amount"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54457c47-e01c-31df-bc79-f30e56e32f4c",
                    "sense_definition": "Выражающий чувство успокоения и снятия напряжения (о вздохе, жесте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда опасность миновала, из груди вырвался глубокий <w>облегчённый</w> вздох.",
                            "target_lang_translations": {
                                "en": "When the danger passed, a deep <w>relieved</w> sigh escaped his chest."
                            }
                        },
                        {
                            "text": "Мы заметили его <w>облегчённый</w> взгляд, когда он узнал хорошие новости.",
                            "target_lang_translations": {
                                "en": "We noticed his <w>relieved</w> look when he found out the good news."
                            }
                        }
                    ],
                    "common_phrases": [
                        "облегчённый вздох"
                    ],
                    "target_lang_definitions": {
                        "en": "Expressing a feeling of calm and release of tension (referring to a sigh, gesture)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relieved",
                                "target_lang_sense_clarification": "expressing relief"
                            }
                        ]
                    }
                }
            ]
        },
        {

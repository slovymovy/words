    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f41bf442-9a99-3644-ac83-0111d2b4ca03",
                    "sense_definition": "De voormalige munteenheid van Nederland (tot 2002) en de huidige munteenheid van Curaçao en Sint Maarten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "currency_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Nederland wordt niet meer met de <w>gulden</w> betaald.",
                            "target_lang_translations": {
                                "en": "In the Netherlands, people no longer pay with the <w>guilder</w>.",
                                "ru": "В Нидерландах больше не платят <w>гульденами</w>."
                            }
                        },
                        {
                            "text": "De toegangsprijs voor het museum was destijds vijf <w>gulden</w>.",
                            "target_lang_translations": {
                                "en": "The entrance fee for the museum was five <w>guilders</w> at the time.",
                                "ru": "В то время плата за вход в музей составляла пять <w>гульденов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "florijn",
                        "fl",
                        "ƒ"
                    ],
                    "common_phrases": [
                        "op de gulden af"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "historical in Netherlands, current in parts of Caribbean"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The former currency of the Netherlands (until 2002) and the current currency of Curaçao and Sint Maarten.",
                        "ru": "Бывшая валюта Нидерландов (до 2002 года) и текущая валюта Кюрасао и Синт-Мартена."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guilder",
                                "target_lang_sense_clarification": "currency unit"
                            },
                            {
                                "target_lang_word": "florin",
                                "target_lang_sense_clarification": "historical synonym"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гульден",
                                "target_lang_sense_clarification": "денежная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9757ef47-0bfd-3913-ae29-abd2535272a0",
                    "sense_definition": "Een muntstuk of bankbiljet met de waarde van één eenheid van deze valuta.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "currency_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gaf de straatmuzikant een <w>gulden</w> voor zijn optreden.",
                            "target_lang_translations": {
                                "en": "He gave the street musician a <w>guilder</w> for his performance.",
                                "ru": "Он дал уличному музыканту <w>гульден</w> за его выступление."
                            }
                        },
                        {
                            "text": "Heb jij nog ergens een oude <w>gulden</w> liggen?",
                            "target_lang_translations": {
                                "en": "Do you still have an old <w>guilder</w> lying around somewhere?",
                                "ru": "У тебя где-нибудь завалялся старый <w>гульден</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "piek"
                    ],
                    "target_lang_definitions": {
                        "en": "A coin or banknote worth one unit of this currency.",
                        "ru": "Монета или банкнота достоинством в одну единицу этой валюты."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guilder",
                                "target_lang_sense_clarification": "coin or note"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гульден",
                                "target_lang_sense_clarification": "монета или банкнота"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8febe18c-10ee-3f93-8a35-dbeb56288eae",
                    "sense_definition": "Gemaakt van goud, goudkleurig of figuurlijk zeer waardevol; tegenwoordig voornamelijk gebruikt in vaste uitdrukkingen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "golden_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij kozen uiteindelijk voor de <w>gulden</w> middenweg om iedereen tevreden te stellen.",
                            "target_lang_translations": {
                                "en": "They eventually chose the <w>golden</w> mean to satisfy everyone.",
                                "ru": "В итоге они выбрали <w>золотую</w> середину, чтобы все остались довольны."
                            }
                        },
                        {
                            "text": "Die man had een <w>gulden</w> hart en hielp iedereen.",
                            "target_lang_translations": {
                                "en": "That man had a <w>golden</w> heart and helped everyone.",
                                "ru": "У этого человека было <w>золотое</w> сердце, и он помогал всем."
                            }
                        }
                    ],
                    "synonyms": [
                        "gouden"
                    ],
                    "common_phrases": [
                        "de gulden middenweg",
                        "de gulden snede",
                        "de gulden regel"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "in literal sense, replaced by 'gouden'"
                        },
                        {
                            "trait_type": "form",
                            "comment": "fixed expressions"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Made of gold, gold-colored, or figuratively very valuable; nowadays mainly used in fixed expressions.",
                        "ru": "Сделанный из золота, золотистого цвета или, в переносном смысле, очень ценный; в настоящее время используется преимущественно в устойчивых выражениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "golden",
                                "target_lang_sense_clarification": "archaic or figurative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "золотой",
                                "target_lang_sense_clarification": "архаичное или в устойчивых выражениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
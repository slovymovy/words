            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a2c7b08-e2af-3958-9b02-41a7612ee053",
                    "sense_definition": "Een vaardigheid, taal of onderwerp grondig kennen en goed kunnen toepassen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "skill_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn collega <w>beheerst</w> de Franse taal uitstekend.",
                            "target_lang_translations": {
                                "ru": "Мой коллега отлично <w>владеет</w> французским языком."
                            }
                        },
                        {
                            "text": "Na deze cursus zul je de nieuwe software volledig <w>beheersen</w>.",
                            "target_lang_translations": {
                                "ru": "После этого курса вы будете полностью <w>владеть</w> новым программным обеспечением."
                            }
                        }
                    ],
                    "synonyms": [
                        "kunnen",
                        "kennen",
                        "verstaan"
                    ],
                    "common_phrases": [
                        "een taal beheersen",
                        "de stof beheersen",
                        "de techniek beheersen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубоко знать навык, язык или предмет и уметь применять его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "владеть",
                                "target_lang_sense_clarification": "навыком, языком"
                            },
                            {
                                "target_lang_word": "знать в совершенстве",
                                "target_lang_sense_clarification": "предмет или тему"
                            },
                            {
                                "target_lang_word": "освоить",
                                "target_lang_sense_clarification": "выучить, овладеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a653d40-78e7-3d6a-a5b7-8fc8638a913f",
                    "sense_definition": "Gevoelens, zenuwen of impulsen onder controle houden en kalm blijven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "self_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was erg boos, maar hij wist zich goed te <w>beheersen</w>.",
                            "target_lang_translations": {
                                "ru": "Он был очень зол, но сумел <w>сдержаться</w>."
                            }
                        },
                        {
                            "text": "Het is belangrijk om je zenuwen te <w>beheersen</w> tijdens het examen.",
                            "target_lang_translations": {
                                "ru": "Важно <w>контролировать</w> свои нервы во время экзамена."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedwingen",
                        "inhouden",
                        "onderdrukken"
                    ],
                    "antonyms": [
                        "laten gaan",
                        "uitbarsten"
                    ],
                    "common_phrases": [

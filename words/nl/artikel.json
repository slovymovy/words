            "senses": [
                {
                    "sense_id": "a05cdbf8-6eab-3d1e-ad5d-50e23ff29c04",
                    "sense_definition": "Een geschreven tekst over een bepaald onderwerp in een krant, tijdschrift of encyclopedie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De journalist schreef een kritisch <w>artikel</w> over de politiek.",
                            "target_lang_translations": {
                                "ru": "Журналист написал критическую <w>статью</w> о политике."
                            }
                        },
                        {
                            "text": "Ik heb gisteren een interessant <w>artikel</w> in de krant gelezen.",
                            "target_lang_translations": {
                                "ru": "Вчера я прочитал в газете интересную <w>статью</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een interessant artikel",
                        "een artikel schrijven",
                        "een artikel lezen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Написанный текст на определенную тему в газете, журнале или энциклопедии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статья",
                                "target_lang_sense_clarification": "публикация в СМИ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ff3381e-ef36-3e36-8239-1fc7a24d9aec",
                    "sense_definition": "Een voorwerp dat te koop wordt aangeboden in een winkel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze winkel zijn alle <w>artikelen</w> in de uitverkoop.",
                            "target_lang_translations": {
                                "ru": "В этом магазине все <w>товары</w> на распродаже."
                            }
                        },
                        {
                            "text": "Je kunt deze afgeprijsde <w>artikelen</w> niet meer ruilen.",
                            "target_lang_translations": {
                                "ru": "Вы больше не можете обменять эти уцененные <w>товары</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "afgeprijsde artikelen",
                        "nieuwe artikelen",
                        "luxe artikelen"
                    ],
                    "target_lang_definitions": {

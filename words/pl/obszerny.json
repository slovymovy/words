{
    "word_family": [
        "obszerność",
        "obszernie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "163799c3-234c-3047-9eb1-1657ce6719db",
                    "sense_definition": "Zajmujący dużo miejsca, mający duże rozmiary, przestronny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>obszerny</w> dom bez problemu pomieści całą naszą wielopokoleniową rodzinę.",
                            "target_lang_translations": {
                                "ru": "Этот <w>просторный</w> дом без труда вместит всю нашу большую семью."
                            }
                        },
                        {
                            "text": "W hotelu czekał na nas <w>obszerny</w> apartament z pięknym widokiem na morze.",
                            "target_lang_translations": {
                                "ru": "В отеле нас ждал <w>просторный</w> номер с прекрасным видом на море."
                            }
                        }
                    ],
                    "synonyms": [
                        "przestronny",
                        "rozległy",
                        "duży",
                        "pojemny",
                        "wielki"
                    ],
                    "antonyms": [
                        "ciasny",
                        "wąski",
                        "nieprzestrzenny",
                        "nieobszerny"
                    ],
                    "common_phrases": [
                        "obszerny pokój",
                        "obszerny teren",
                        "obszerny dom",
                        "obszerny ogród"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимающий много места, имеющий большие размеры, просторный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просторный",
                                "target_lang_sense_clarification": "о помещении, доме"
                            },
                            {
                                "target_lang_word": "обширный",
                                "target_lang_sense_clarification": "о территории, пространстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a346c92-7b58-3225-9b80-26451f0630b7",
                    "sense_definition": "Zawierający wiele szczegółów, informacji lub obejmujący szeroki zakres zagadnień.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel zadał nam do przeczytania bardzo <w>obszerny</w> rozdział z podręcznika historii.",
                            "target_lang_translations": {
                                "ru": "Учитель задал нам прочитать очень <w>объёмную</w> главу из учебника истории."
                            }
                        },
                        {
                            "text": "Jej <w>obszerna</w> wiedza na temat biologii pozwoliła jej bez trudu wygrać ten konkurs.",
                            "target_lang_translations": {
                                "ru": "Её <w>обширные</w> знания в области биологии позволили ей без труда выиграть этот конкурс."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczegółowy",
                        "wyczerpujący",
                        "rozbudowany",
                        "wszechstronny",
                        "szeroki"
                    ],
                    "antonyms": [
                        "zwięzły",
                        "lakoniczny",
                        "ogólnikowy",
                        "krótki",
                        "skromny"
                    ],
                    "common_phrases": [
                        "obszerny artykuł",
                        "obszerna wiedza",
                        "obszerny raport",
                        "obszerny opis",
                        "obszerny materiał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий много подробностей, информации или охватывающий широкий круг вопросов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обширный",
                                "target_lang_sense_clarification": "о знаниях, докладе"
                            },
                            {
                                "target_lang_word": "объёмный",
                                "target_lang_sense_clarification": "о тексте, книге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92efa297-8a85-35cb-bf75-3a07e1fde276",
                    "sense_definition": "O ubraniu: luźny, szeroki, nieprzylegający ściśle do ciała.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starzec otulił się szczelnie w swój <w>obszerny</w> wełniany płaszcz, aby uchronić się przed wiatrem.",
                            "target_lang_translations": {
                                "ru": "Старик плотно закутался в свой <w>просторный</w> шерстяной плащ, чтобы защититься от ветра."
                            }
                        },
                        {
                            "text": "Kupiła sobie <w>obszerną</w> sukienkę, aby czuć się swobodnie i wygodnie w upalne dni.",
                            "target_lang_translations": {
                                "ru": "Она купила себе <w>просторное</w> платье, чтобы чувствовать себя свободно и комфортно в жаркие дни."
                            }
                        }
                    ],
                    "synonyms": [
                        "luźny",
                        "szeroki"
                    ],
                    "antonyms": [
                        "(o ubraniu) obcisły",
                        "ciasny"
                    ],
                    "common_phrases": [
                        "obszerny płaszcz",
                        "obszerna szata",
                        "obszerne spodnie",
                        "obszerna bluza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об одежде: свободный, широкий, не облегающий тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просторный",
                                "target_lang_sense_clarification": "свободного покроя"
                            },
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "не тесный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
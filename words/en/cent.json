{
    "word_family": [
        "percent",
        "percentage",
        "century",
        "centennial",
        "centimeter",
        "centipede"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e1c286a-1938-3bfb-a7fc-8431c5b230b4",
                    "sense_definition": "A subunit of currency equal to one-hundredth of the main unit of currency in many countries, such as the dollar or euro.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This candy bar costs fifty <w>cents</w>.",
                            "target_lang_translations": {
                                "ru": "Этот батончик стоит пятьдесят <w>центов</w>."
                            }
                        },
                        {
                            "text": "There are one hundred <w>cents</w> in one dollar.",
                            "target_lang_translations": {
                                "ru": "В одном долларе сто <w>центов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dollarcent",
                        "Eurocent"
                    ],
                    "common_phrases": [
                        "fifty cents",
                        "ten cents"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная единица, равная одной сотой части основной валюты во многих странах (например, доллара или евро)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цент",
                                "target_lang_sense_clarification": "денежная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17bef9b7-b4bf-362a-bedc-bff4f75432ed",
                    "sense_definition": "A coin having a face value of one cent.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I found a shiny <w>cent</w> on the sidewalk.",
                            "target_lang_translations": {
                                "ru": "Я нашел на тротуаре блестящий <w>цент</w>."
                            }
                        },
                        {
                            "text": "He dropped a <w>cent</w> into the donation jar.",
                            "target_lang_translations": {
                                "ru": "Он бросил <w>цент</w> в банку для пожертвований."
                            }
                        }
                    ],
                    "synonyms": [
                        "penny"
                    ],
                    "common_phrases": [
                        "copper cent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монета номиналом в один цент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цент",
                                "target_lang_sense_clarification": "монета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17b89512-fe62-332c-b436-ae7f99dfffa6",
                    "sense_definition": "A very small sum of money.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "money_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He did not have a single <w>cent</w> to his name.",
                            "target_lang_translations": {
                                "ru": "У него не было ни <w>цента</w> за душой."
                            }
                        },
                        {
                            "text": "She refused to pay a <w>cent</w> more than the agreed price.",
                            "target_lang_translations": {
                                "ru": "Она отказалась платить ни <w>центом</w> больше оговоренной цены."
                            }
                        }
                    ],
                    "common_phrases": [
                        "every last cent",
                        "not a red cent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленькая сумма денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цент",
                                "target_lang_sense_clarification": "ни гроша"
                            },
                            {
                                "target_lang_word": "копейка",
                                "target_lang_sense_clarification": "аналог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42fb5b21-ecde-33b2-ae6c-758e41daefe9",
                    "sense_definition": "A unit of pitch interval equal to one hundredth of a semitone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The piano was tuned to within a few <w>cents</w> of the standard pitch.",
                            "target_lang_translations": {
                                "ru": "Фортепиано было настроено с точностью до нескольких <w>центов</w> от эталона."
                            }
                        },
                        {
                            "text": "A difference of five <w>cents</w> is barely audible to the untrained ear.",
                            "target_lang_translations": {
                                "ru": "Разница в пять <w>центов</w> едва различима для нетренированного уха."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица музыкального интервала, равная одной сотой полутона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цент",
                                "target_lang_sense_clarification": "музыка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29699c8a-f950-332e-8462-025d50077754",
                    "sense_definition": "A unit of nuclear reactivity equal to one hundredth of a dollar.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physics_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The reactivity of the core increased by fifty <w>cents</w>.",
                            "target_lang_translations": {
                                "ru": "Реактивность активной зоны увеличилась на пятьдесят <w>центов</w>."
                            }
                        },
                        {
                            "text": "Engineers measure the reactor period in <w>cents</w> for precision.",
                            "target_lang_translations": {
                                "ru": "Инженеры измеряют период реактора в <w>центах</w> для точности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения реактивности ядерного реактора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цент",
                                "target_lang_sense_clarification": "физика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "e606068e-28be-3af8-b46e-e31cd9591e4c",
                    "sense_definition": "Een heel kort moment geleden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein naar Amsterdam is <w>juist</w> vertrokken.",
                            "target_lang_translations": {
                                "en": "The train to Amsterdam has <w>just</w> left.",
                                "ru": "Поезд в Амстердам <w>только что</w> отправился."
                            }
                        },
                        {
                            "text": "Ik kwam <w>juist</w> thuis toen het begon te regenen.",
                            "target_lang_translations": {
                                "en": "I had <w>just</w> come home when it started raining.",
                                "ru": "Я <w>только что</w> пришел домой, когда начался дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "net"
                    ],
                    "common_phrases": [
                        "kom juist aan",
                        "is juist weg"
                    ],
                    "target_lang_definitions": {
                        "en": "A very short moment ago.",
                        "ru": "Только что; совсем недавно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "a very short time ago"
                            }

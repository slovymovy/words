            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce5a8059-a417-3de3-9203-95707b5e8ba4",
                    "sense_definition": "Dawać komuś coś fizycznego, wręczać przedmiot innej osobie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kurier codziennie <przekazuje> paczki bezpośrednio do rąk klientów.",
                            "target_lang_translations": {
                                "ru": "Курьер ежедневно <передает> посылки прямо в руки клиентам."
                            }
                        },
                        {
                            "text": "Rodzice często <przekazują> dzieciom cenne pamiątki rodzinne.",
                            "target_lang_translations": {
                                "ru": "Родители часто <передают> детям ценные семейные реликвии."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawać",
                        "wręczać",
                        "oddawać"
                    ],
                    "antonyms": [
                        "odbierać",
                        "przyjmować",
                        "brać"
                    ],
                    "common_phrases": [
                        "przekazywać paczkę",
                        "przekazywać klucze",
                        "przekazywać dokumenty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-то что-то физическое, вручать предмет другому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "вручать предмет"
                            },
                            {
                                "target_lang_word": "вручать",
                                "target_lang_sense_clarification": "торжественно или лично отдавать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fde68dc-b99d-3326-8a66-f55a45435575",
                    "sense_definition": "Komunikować komuś wiadomość, informację lub wiedzę; powtarzać to, co się usłyszało.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sekretarka na bieżąco <przekazuje> szefowi wszystkie ważne wiadomości.",
                            "target_lang_translations": {
                                "ru": "Секретарь своевременно <передает> начальнику все важные сообщения."
                            }
                        },
                        {
                            "text": "Nauczyciel <przekazuje> uczniom wiedzę w bardzo przystępny sposób.",
                            "target_lang_translations": {
                                "ru": "Учитель <передает> ученикам знания очень доступным способом."
                            }
                        }
                    ],
                    "synonyms": [
                        "podawać",
                        "ogłaszać"
                    ],
                    "common_phrases": [

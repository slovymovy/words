        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "001b4981-602a-32db-bcec-eedd0158892b",
                    "sense_definition": "Initialism of <w>never</w> <w>mind</w>; used to tell someone to disregard a previous statement or that something does not matter.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I thought I lost my keys, but <w>nm</w>, they were in my pocket.",
                            "target_lang_translations": {
                                "ru": "Я думал, что потерял ключи, но <w>забей</w>, они были у меня в кармане."
                            }
                        },
                        {
                            "text": "Did you call me earlier? Oh, <w>nm</w>, it was someone else.",
                            "target_lang_translations": {
                                "ru": "Ты звонил мне раньше? А, <w>неважно</w>, это был кто-то другой."
                            }
                        }
                    ],
                    "synonyms": [
                        "nvm"
                    ],
                    "common_phrases": [
                        "oh nm",
                        "but nm"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"never mind\"; используется, чтобы сказать кому-то не обращать внимания на предыдущее утверждение или что это не имеет значения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забей",
                                "target_lang_sense_clarification": "сленговое выражение"
                            },
                            {
                                "target_lang_word": "неважно",
                                "target_lang_sense_clarification": "нейтральное выражение"
                            },
                            {
                                "target_lang_word": "проехали",
                                "target_lang_sense_clarification": "разговорное выражение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "f4772d5a-9918-3f5e-85e9-07c3607c60c7",
                    "sense_definition": "Initialism of <w>nothing</w> <w>much</w>; often used as a response to a greeting like \"what is up\".",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A: \"What are you doing right now?\" B: \"<w>nm</w>, just watching TV.\"",
                            "target_lang_translations": {
                                "ru": "А: \"Что делаешь сейчас?\" Б: \"<w>Ничего особенного</w>, просто смотрю телевизор.\""
                            }
                        },
                        {
                            "text": "There is <w>nm</w> happening here at the moment.",
                            "target_lang_translations": {
                                "ru": "Здесь сейчас <w>ничего особенного</w> не происходит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nm here",
                        "nm u"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"nothing much\"; часто используется как ответ на приветствие типа \"как дела\" (what is up)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничего особенного",
                                "target_lang_sense_clarification": "стандартный ответ"
                            },
                            {
                                "target_lang_word": "ничего нового",
                                "target_lang_sense_clarification": "отсутствие новостей"
                            }
                        ]
                    }
                }
            ]
        },

            "senses": [
                {
                    "sense_id": "4e6a5eba-e7d2-3cff-b4be-3f3dd1b3b890",
                    "sense_definition": "Zepsuć komuś opinię, mówiąc o nim złe lub nieprawdziwe rzeczy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "defamation_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziennikarz próbował <w>zniesławić</w> znanego polityka, publikując sfałszowane dokumenty.",
                            "target_lang_translations": {
                                "ru": "Журналист пытался <w>оклеветать</w> известного политика, опубликовав сфальсифицированные документы."
                            }
                        },
                        {
                            "text": "Nie pozwolę nikomu bezkarnie <w>zniesławić</w> mojego dobrego imienia w towarzystwie.",
                            "target_lang_translations": {
                                "ru": "Я никому не позволю безнаказанно <w>опорочить</w> мое доброе имя в обществе."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczernić",
                        "spotwarzyć",
                        "obsmarować",
                        "znieważyć"
                    ],
                    "antonyms": [
                        "zrehabilitować",
                        "oczyścić",
                        "pochwalić"
                    ],
                    "common_phrases": [
                        "zniesławić dobre imię",
                        "publicznie zniesławić",
                        "niesłusznie zniesławić",
                        "próbować kogoś zniesławić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испортить чью-то репутацию, говоря о нем плохие или неправдивые вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оклеветать",
                                "target_lang_sense_clarification": "распространить заведомо ложные обвинения"
                            },
                            {
                                "target_lang_word": "опорочить",
                                "target_lang_sense_clarification": "запятнать честь или доброе имя"
                            },
                            {
                                "target_lang_word": "очернить",
                                "target_lang_sense_clarification": "представить кого-то в дурном свете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "194a04f4-62be-3e31-b707-d8142d0adbe6",
                    "sense_definition": "Zepsuć własną reputację poprzez niegodne lub wstydliwe zachowanie.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "defamation_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swoim nieuczciwym postępowaniem dyrektor <w>zniesławił się</w> w oczach wszystkich pracowników.",
                            "target_lang_translations": {
                                "ru": "Своим нечестным поведением директор <w>опозорился</w> в глазах всех сотрудников."
                            }
                        },
                        {
                            "text": "Rycerz wolał zginąć w walce, niż uciec i <w>zniesławić się</w> tchórzostwem.",
                            "target_lang_translations": {
                                "ru": "Рыцарь предпочел погибнуть в бою, чем сбежать и <w>обесславить себя</w> трусостью."
                            }
                        }
                    ],
                    "synonyms": [
                        "skompromitować się",
                        "zhańbić się"
                    ],
                    "antonyms": [
                        "wsławić się"
                    ],
                    "common_phrases": [
                        "zniesławić się czynem",
                        "zniesławić się kłamstwem",
                        "zniesławić się w oczach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испортить собственную репутацию недостойным или постыдным поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опозориться",
                                "target_lang_sense_clarification": "совершить постыдный поступок, потерять уважение"
                            },
                            {
                                "target_lang_word": "обесславить себя",
                                "target_lang_sense_clarification": "лишить себя чести, покрыть себя позором"
                            },
                            {
                                "target_lang_word": "сскомпрометировать себя",
                                "target_lang_sense_clarification": "подорвать доверие к себе своим поведением"
                            }
                        ]
                    }
                }
            ]
        }
    ]

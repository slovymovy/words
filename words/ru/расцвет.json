                        "закат",
                        "кризис"
                    ],
                    "common_phrases": [
                        "в полном расцвете сил",
                        "расцвет культуры",
                        "экономический расцвет",
                        "достичь расцвета"
                    ],
                    "target_lang_definitions": {

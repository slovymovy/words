{
    "word_family": [
        "dimensionaal",
        "driedimensionaal",
        "tweedimensionaal",
        "multidimensionaal",
        "dimensioneren"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4647f5ab-ff89-32a6-a5fe-ff129e194d00",
                    "sense_definition": "Een afmeting in een bepaalde richting, zoals lengte, breedte of hoogte.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je de <w>dimensies</w> van dat raam wel goed opgemeten?",
                            "target_lang_translations": {
                                "ru": "Ты правильно измерил <w>размеры</w> этого окна?"
                            }
                        },
                        {
                            "text": "De <w>dimensies</w> van de nieuwe kast zijn te groot voor deze kamer.",
                            "target_lang_translations": {
                                "ru": "<w>Габариты</w> нового шкафа слишком велики для этой комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "afmeting"
                    ],
                    "common_phrases": [
                        "enorme dimensies",
                        "juiste dimensies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величина предмета в определенном направлении, например, длина, ширина или высота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "величина предмета"
                            },
                            {
                                "target_lang_word": "габариты",
                                "target_lang_sense_clarification": "внешние очертания предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "900d8627-7150-3c5b-b1ca-d5e5d4e2d994",
                    "sense_definition": "Een van de richtingen of parameters die nodig zijn om een ruimte te beschrijven, zoals hoogte, breedte en diepte.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een ruimte die met drie coördinaten te beschrijven is, heeft drie <w>dimensies</w>.",
                            "target_lang_translations": {
                                "ru": "Пространство, которое можно описать тремя координатами, имеет три <w>измерения</w>."
                            }
                        },
                        {
                            "text": "Tijd wordt in de natuurkunde vaak gezien als de vierde <w>dimensie</w>.",
                            "target_lang_translations": {
                                "ru": "Время в физике часто рассматривается как четвертое <w>измерение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drie dimensies",
                        "vierde dimensie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из направлений или параметров, необходимых для описания пространства (высота, ширина, глубина)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерение",
                                "target_lang_sense_clarification": "пространственная координата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8d3e812-c220-3d12-be29-d3df6649af73",
                    "sense_definition": "Een specifiek aspect of onderdeel van een situatie, probleem of toestand.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De muziek geeft een extra <w>dimensie</w> aan de sfeer in de film.",
                            "target_lang_translations": {
                                "ru": "Музыка придает дополнительное <w>измерение</w> атмосфере фильма."
                            }
                        },
                        {
                            "text": "We moeten ook rekening houden met de sociale <w>dimensie</w> van dit besluit.",
                            "target_lang_translations": {
                                "ru": "Мы также должны учитывать социальный <w>аспект</w> этого решения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "extra dimensie",
                        "nieuwe dimensie",
                        "sociale dimensie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный аспект или часть ситуации, проблемы или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аспект",
                                "target_lang_sense_clarification": "сторона вопроса"
                            },
                            {
                                "target_lang_word": "измерение",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccc8532b-2398-34af-8d06-3c75756f1240",
                    "sense_definition": "De categorie waartoe een natuurkundige grootheid behoort, onafhankelijk van de eenheid waarin deze wordt gemeten.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>dimensie</w> lengte heeft in het SI-stelsel de eenheid meter.",
                            "target_lang_translations": {
                                "ru": "<w>Размерность</w> длины в системе СИ имеет единицу измерения метр."
                            }
                        },
                        {
                            "text": "Om de formule te controleren, moet je kijken of de <w>dimensie</w> aan beide kanten gelijk is.",
                            "target_lang_translations": {
                                "ru": "Чтобы проверить формулу, нужно посмотреть, одинакова ли <w>размерность</w> с обеих сторон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dimensie tijd",
                        "dimensie massa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Категория физической величины независимо от единиц измерения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размерность",
                                "target_lang_sense_clarification": "физическая величина"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
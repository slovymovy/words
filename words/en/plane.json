    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83f29d19-7d21-31e1-8aff-783968590167",
                    "sense_definition": "An airplane; a powered flying vehicle with fixed wings and a weight greater than that of the air it displaces.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "aircraft_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We boarded the <w>plane</w> just before the doors closed.",
                            "target_lang_translations": {
                                "ru": "Мы сели в <w>самолёт</w> прямо перед тем, как закрылись двери."
                            }
                        },
                        {
                            "text": "The <w>plane</w> flew high above the clouds.",
                            "target_lang_translations": {
                                "ru": "<w>Самолёт</w> летел высоко над облаками."
                            }
                        }
                    ],
                    "synonyms": [
                        "aeroplane"
                    ],
                    "common_phrases": [
                        "by plane",
                        "catch a plane",
                        "board a plane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самолёт; летательный аппарат с двигателем и неподвижным крылом, который тяжелее воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самолёт",
                                "target_lang_sense_clarification": "летательный аппарат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5fac893-4484-3e2b-86b7-013abae2d870",
                    "sense_definition": "A flat or level surface, often used in geometry to describe a two-dimensional area extending infinitely.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "flat_surface_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In geometry, three points define a single <w>plane</w>.",
                            "target_lang_translations": {
                                "ru": "В геометрии три точки определяют одну <w>плоскость</w>."
                            }
                        },
                        {
                            "text": "The floor of the room forms a horizontal <w>plane</w>.",
                            "target_lang_translations": {
                                "ru": "Пол комнаты образует горизонтальную <w>плоскость</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "horizontal plane",
                        "vertical plane",
                        "inclined plane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская или ровная поверхность; в геометрии — двумерная область, простирающаяся бесконечно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плоскость",
                                "target_lang_sense_clarification": "геометрическое понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a83f853-b888-3c65-9952-4f6fd3b8c1e3",
                    "sense_definition": "A level of existence, thought, or development.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_level_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The debate moved to a more intellectual <w>plane</w>.",
                            "target_lang_translations": {
                                "ru": "Дебаты перешли в более интеллектуальную <w>плоскость</w>."
                            }
                        },
                        {
                            "text": "Meditation can help you reach a higher spiritual <w>plane</w>.",
                            "target_lang_translations": {
                                "ru": "Медитация может помочь вам достичь более высокого духовного <w>уровня</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "higher plane",
                        "spiritual plane",
                        "astral plane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень существования, мышления или развития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "степень развития"
                            },
                            {
                                "target_lang_word": "плоскость",
                                "target_lang_sense_clarification": "сфера отношений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3990befc-9943-3381-aa44-78a94fe841b9",
                    "sense_definition": "A tool used for smoothing or shaping wood by removing thin layers from the surface.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter used a <w>plane</w> to smooth the rough edge of the door.",
                            "target_lang_translations": {
                                "ru": "Плотник использовал <w>рубанок</w>, чтобы сгладить неровный край двери."
                            }
                        },
                        {
                            "text": "He sharpened the blade of his hand <w>plane</w> before starting the work.",
                            "target_lang_translations": {
                                "ru": "Он заточил лезвие своего ручного <w>рубанка</w> перед началом работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hand plane",
                        "block plane",
                        "wood plane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент для сглаживания или придания формы дереву путем снятия тонких слоев с поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубанок",
                                "target_lang_sense_clarification": "столярный инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fb9ee8e-5afa-3267-a834-3c8280c9d986",
                    "sense_definition": "A deciduous tree of the genus Platanus, often found in cities, known for its peeling bark.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tree_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We sat in the shade of a large <w>plane</w> tree in the park.",
                            "target_lang_translations": {
                                "ru": "Мы сидели в тени большого <w>платана</w> в парке."
                            }
                        },
                        {
                            "text": "The avenue was lined with old <w>plane</w> trees.",
                            "target_lang_translations": {
                                "ru": "Аллея была обсажена старыми <w>платанами</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plane tree",
                        "London plane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лиственное дерево рода Platanus, часто встречающееся в городах, известное своей отслаивающейся корой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платан",
                                "target_lang_sense_clarification": "ботаническое название"
                            },
                            {
                                "target_lang_word": "чинара",
                                "target_lang_sense_clarification": "восточное название дерева"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8b329f65-bda5-3f6c-8cc1-dc90c9d367f3",
                    "sense_definition": "To smooth or shape wood using a plane.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had to <w>plane</w> the door down so it would close properly.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось <w>подстрогать</w> дверь, чтобы она нормально закрывалась."
                            }
                        },
                        {
                            "text": "The carpenter <w>planed</w> the surface until it was perfectly smooth.",
                            "target_lang_translations": {
                                "ru": "Плотник <w>строгал</w> поверхность, пока она не стала идеально гладкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plane down",
                        "plane smooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сглаживать или придавать форму дереву с помощью рубанка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строгать",
                                "target_lang_sense_clarification": "обрабатывать рубанком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6701cc0e-fac3-3225-b5da-6855300d3486",
                    "sense_definition": "To glide or soar; to move over a surface (like water) with the main body lifted out of it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "glide_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The speedboat began to <w>plane</w> across the water as it gained speed.",
                            "target_lang_translations": {
                                "ru": "Быстроходный катер начал <w>глиссировать</w> по воде, набирая скорость."
                            }
                        },
                        {
                            "text": "The bird <w>planed</w> effortlessly on the wind currents.",
                            "target_lang_translations": {
                                "ru": "Птица без усилий <w>парила</w> на потоках ветра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plane over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скользить или парить; двигаться над поверхностью (например, воды), приподнявшись над ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парить",
                                "target_lang_sense_clarification": "держаться в воздухе"
                            },
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "плавно снижаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "79a2ace5-3591-34d0-812f-ceded7635463",
                    "sense_definition": "Of a surface: flat or level.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "flat_surface_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mirror has a perfectly <w>plane</w> surface.",
                            "target_lang_translations": {
                                "ru": "Зеркало имеет идеально <w>плоскую</w> поверхность."
                            }
                        },
                        {
                            "text": "In mathematics, <w>plane</w> geometry deals with flat shapes like triangles and circles.",
                            "target_lang_translations": {
                                "ru": "В математике <w>плоская</w> геометрия изучает плоские фигуры, такие как треугольники и круги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plane surface",
                        "plane geometry"
                    ],
                    "target_lang_definitions": {
                        "ru": "О поверхности: плоский или ровный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плоский",
                                "target_lang_sense_clarification": "не имеющий углублений"
                            },
                            {
                                "target_lang_word": "ровный",
                                "target_lang_sense_clarification": "гладкий"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "eea3597a-cf8a-3dd1-8c5b-8fa69e080dfa",
                    "sense_definition": "Waarbij een zeer groot aantal mensen of dingen betrokken is; op zeer grote schaal.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er was een <w>massaal</w> protest tegen de nieuwe wet.",
                            "target_lang_translations": {
                                "ru": "Прошел <w>массовый</w> протест против нового закона."
                            }
                        },
                        {
                            "text": "Door het politieoptreden is de demonstratie alleen maar <w>massaler</w> geworden.",
                            "target_lang_translations": {
                                "ru": "Из-за действий полиции демонстрация стала только более <w>массовой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "grootschalig",
                        "omvangrijk",
                        "groot"
                    ],
                    "antonyms": [
                        "kleinschalig",
                        "individueel",
                        "beperkt"
                    ],
                    "common_phrases": [
                        "een massaal protest",
                        "massale werkloosheid",
                        "massale steun",
                        "massale belangstelling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охватывающий очень большое количество людей или предметов; происходящий в крупных масштабах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массовый",
                                "target_lang_sense_clarification": "охватывающий множество людей"
                            },
                            {
                                "target_lang_word": "масштабный",
                                "target_lang_sense_clarification": "имеющий большой размах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "353d77bd-8538-3d94-859b-9abb5d115e10",
                    "sense_definition": "Met heel veel mensen of exemplaren tegelijk; in groten getale.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mensen liepen <w>massaal</w> de straat op om te protesteren.",
                            "target_lang_translations": {
                                "ru": "Люди <w>массово</w> вышли на улицы, чтобы выразить протест."
                            }
                        },
                        {
                            "text": "De supporters reisden <w>massaal</w> af naar de finale in het buitenland.",
                            "target_lang_translations": {
                                "ru": "Болельщики <w>массово</w> отправились на финал за границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "en masse",
                        "gezamenlijk",
                        "in groten getale"
                    ],
                    "antonyms": [
                        "individueel"
                    ],
                    "common_phrases": [
                        "massaal opkomen",
                        "massaal stemmen",
                        "massaal gehoor geven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершаемый большим количеством людей одновременно; в большом числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массово",
                                "target_lang_sense_clarification": "большим количеством участников"
                            },
                            {
                                "target_lang_word": "в массовом порядке",
                                "target_lang_sense_clarification": "организованно и в большом количестве"

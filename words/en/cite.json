    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d048de78-0e7e-3353-938f-f5745de69468",
                    "sense_definition": "To mention something as a reason, example, or proof to support an argument or explanation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mention_refer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mayor <w>cited</w> budget cuts as the main reason for the delay."
                        },
                        {
                            "text": "She <w>cited</w> several examples of success to prove her point."
                        }
                    ],
                    "synonyms": [
                        "mention",
                        "refer to",
                        "adduce",
                        "point to"
                    ],
                    "common_phrases": [
                        "cite a reason",
                        "cite an example",
                        "cite evidence",
                        "cite a case"
                    ]
                },
                {
                    "sense_id": "a1aeb6f4-23df-367c-a02a-a276a27dc95e",
                    "sense_definition": "To list the sources from which information or ideas were taken, usually in an academic work.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mention_refer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>cite</w> all the sources you used in your essay."
                        },
                        {
                            "text": "The student failed to <w>cite</w> the original article in his paper."
                        }
                    ],
                    "synonyms": [
                        "reference",
                        "credit",
                        "acknowledge"
                    ],
                    "common_phrases": [
                        "cite sources",
                        "cite the author",
                        "properly cite"
                    ]
                },
                {
                    "sense_id": "2010a2c4-144e-390d-81f4-63a70a2582f7",
                    "sense_definition": "To quote a passage from a book or the words of another person.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mention_refer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He likes to <w>cite</w> Shakespeare when discussing human nature."
                        },
                        {
                            "text": "The lawyer <w>cited</w> a previous case to support the defense."
                        }
                    ],
                    "synonyms": [
                        "quote",
                        "reproduce"
                    ],
                    "common_phrases": [
                        "cite a passage",
                        "cite a poem"
                    ]
                },
                {

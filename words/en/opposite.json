            "senses": [
                {
                    "sense_id": "52b22707-6ebf-3130-8b63-aecd4f7d02ae",
                    "sense_definition": "In a position facing someone or something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was on my seat and she stood <w>opposite</w>.",
                            "target_lang_translations": {
                                "ru": "Я сидел на своем месте, а она стояла <w>напротив</w>."
                            }
                        },
                        {
                            "text": "The bank is just <w>opposite</w>, across the street.",
                            "target_lang_translations": {
                                "ru": "Банк находится прямо <w>напротив</w>, через дорогу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sit opposite",
                        "stand opposite"
                    ],
                    "target_lang_definitions": {
                        "ru": "В положении лицом к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напро́тив",
                                "target_lang_sense_clarification": "наречие места"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
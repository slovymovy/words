                {
                    "sense_id": "170804c9-0004-37be-977e-60791f0e7d8c",
                    "sense_definition": "Gemakkelijk vast te houden of te bedienen vanwege een handig formaat of gewicht.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit lichte gereedschap is zeer <w>hanteerbaar</w> voor mensen met minder kracht.",
                            "target_lang_translations": {
                                "ru": "Этот легкий инструмент очень <w>удобен в обращении</w> для людей с меньшей физической силой."
                            }
                        },
                        {
                            "text": "De koffer heeft een <w>hanteerbaar</w> formaat en past precies in de kofferbak.",
                            "target_lang_translations": {
                                "ru": "У чемодана <w>удобный</w> размер, и он точно помещается в багажник."
                            }
                        }
                    ],
                    "synonyms": [
                        "handzaam",
                        "handelbaar"
                    ],
                    "antonyms": [

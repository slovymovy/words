{
    "word_family": [
        "сиюминутный"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "86059bce-2e55-3441-b5a5-fb4ef1f90738",
                    "sense_definition": "В данный момент, в то время, когда происходит речь.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_present",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>сейчас</w> очень занят и не могу говорить."
                        },
                        {
                            "text": "Где ты <w>сейчас</w> находишься?"
                        }
                    ],
                    "synonyms": [
                        "тепе́рь",
                        "в настоя́щее вре́мя"
                    ],
                    "antonyms": [
                        "тогда"
                    ],
                    "common_phrases": [
                        "прямо сейчас",
                        "именно сейчас",
                        "а сейчас"
                    ]
                },
                {
                    "sense_id": "f9f329a5-2ae0-38a2-a61f-233502af134e",
                    "sense_definition": "Очень скоро, через минуту, в ближайшем будущем.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подожди немного, я <w>сейчас</w> приду."
                        },
                        {
                            "text": "Мы <w>сейчас</w> всё исправим, не волнуйтесь."
                        }
                    ],
                    "common_phrases": [
                        "сейчас вернусь",
                        "сейчас приду",
                        "сейчас сделаю"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies immediate future action"
                        }
                    ]
                },
                {
                    "sense_id": "a112c481-fef0-3771-83a3-012afc4b4b32",
                    "sense_definition": "В настоящее время, в наши дни (о длительном периоде времени).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_present",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раньше здесь был лес, а <w>сейчас</w> построили парк."
                        },
                        {
                            "text": "<w>Сейчас</w> многие люди работают из дома."
                        }
                    ],
                    "synonyms": [
                        "тепе́рь",
                        "в настоя́щее вре́мя"
                    ],
                    "antonyms": [
                        "давно"
                    ],
                    "common_phrases": [
                        "до сейчас",
                        "как сейчас"
                    ]
                },
                {
                    "sense_id": "df7569a1-356a-32f2-b538-dcd467ea7f15",
                    "sense_definition": "Только что, совсем недавно (о прошлом).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>сейчас</w> встретил твоего брата на улице."
                        },
                        {
                            "text": "Ты <w>сейчас</w> сказал что-то очень важное."
                        }
                    ],
                    "synonyms": [
                        "то́лько что"
                    ],
                    "antonyms": [
                        "давно"
                    ],
                    "common_phrases": [
                        "только сейчас"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "062a9289-3bd5-3601-a19c-867746238c73",
                    "sense_definition": "Сразу, с первого взгляда.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По его лицу <w>сейчас</w> видно, что он говорит правду."
                        },
                        {
                            "text": "Это <w>сейчас</w> понятно, но тогда мы ничего не знали."
                        }
                    ],
                    "synonyms": [
                        "сра́зу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "3975fda8-0e9e-3232-b610-c22d6748d3db",
                    "sense_definition": "Короткий ответ, означающий просьбу подождать или обещание сделать что-то немедленно.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interjection_wait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама, иди сюда! — <w>Сейчас</w>, иду!"
                        },
                        {
                            "text": "Ты готов выходить? — <w>Сейчас</w>, ещё одну минуту."
                        }
                    ],
                    "synonyms": [
                        "погоди́",
                        "подожди́"
                    ],
                    "common_phrases": [
                        "да сейчас",
                        "ну сейчас"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                }
            ]
        }
    ]

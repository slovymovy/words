    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6df33d9e-3e00-35e2-9bec-1fcab39c266f",
                    "sense_definition": "Słowo występujące współcześnie niemal wyłącznie w wyrażeniu oznaczającym stan wielkiego zaangażowania emocjonalnego, robienie czegoś bez opamiętania lub bardzo mocno.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "idiom_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek zakochał się w nowej koleżance <w>na zabój</w>.",
                            "target_lang_translations": {
                                "ru": "Марек влюбился в новую подругу <w>до безумия</w>."
                            }
                        },
                        {
                            "text": "Oni kochają się <w>na zabój</w> i nie widzą świata poza sobą.",
                            "target_lang_translations": {
                                "ru": "Они любят друг друга <w>до смерти</w> и не видят никого вокруг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na zabój",
                        "zakochany na zabój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, встречающееся сегодня почти исключительно в выражении, означающем состояние сильной эмоциональной вовлеченности, действия без оглядки или очень интенсивно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насмерть",
                                "target_lang_sense_clarification": "в выражении 'na zabój' (очень сильно, до крайности)"
                            },
                            {
                                "target_lang_word": "до смерти",
                                "target_lang_sense_clarification": "в переносном смысле: безумно, очень сильно"
                            },
                            {
                                "target_lang_word": "до безумия",
                                "target_lang_sense_clarification": "в контексте чувств (например, влюбиться)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f89f9cc9-db2d-3d9e-840a-5c96b2881eaf",
                    "sense_definition": "Przestarzałe lub rzadkie określenie na czyn polegający na pozbawieniu kogoś życia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "killing_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych kronikach opisano ten okrutny <w>zabój</w>.",

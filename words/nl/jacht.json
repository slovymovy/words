                    "common_phrases": [
                        "op jacht gaan",
                        "jacht maken op"
                    ],
                    "target_lang_definitions": {
                        "en": "The tracking and pursuing of wild animals with the aim of killing or catching them.",
                        "ru": "Выслеживание и преследование диких животных с целью их убийства или поимки."
                    },

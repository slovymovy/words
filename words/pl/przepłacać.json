    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "886f302f-67bd-3d45-93ad-a65a05dfa14d",
                    "sense_definition": "Płacić za coś więcej, niż jest to warte lub niż wynosi cena rynkowa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "money_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klienci często nieświadomie <w>przepłacają</w> za produkty znanych marek.",
                            "target_lang_translations": {
                                "ru": "Клиенты часто неосознанно <w>переплачивают</w> за продукты известных брендов."
                            }
                        },
                        {
                            "text": "Nie warto <w>przepłacać</w> za hotel, skoro w okolicy są tańsze pensjonaty.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>переплачивать</w> за отель, раз в округе есть более дешевые пансионаты."
                            }
                        }
                    ],
                    "antonyms": [
                        "oszczędzać"
                    ],
                    "common_phrases": [
                        "słono przepłacać",
                        "nie warto przepłacać",
                        "przepłacać za jakość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Платить за что-то больше, чем оно того стоит, или выше рыночной цены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переплачивать",
                                "target_lang_sense_clarification": "платить слишком много"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cceffaac-804e-3c50-bce0-d600f18b024a",
                    "sense_definition": "Wręczać komuś korzyść majątkową w zamian za odstąpienie od obowiązków lub załatwienie sprawy; dawać łapówkę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "corruption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oskarżono go o to, że próbował <w>przepłacać</w> urzędników państwowych.",
                            "target_lang_translations": {
                                "ru": "Его обвинили в том, что он пытался <w>подкупать</w> государственных чиновников."
                            }
                        },
                        {
                            "text": "Gangsterzy regularnie <w>przepłacali</w> strażników, aby przemycać towary.",
                            "target_lang_translations": {
                                "ru": "Гангстеры регулярно <w>подкупали</w> охранников, чтобы провозить контрабанду."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekupywać",
                        "korumpować"
                    ],
                    "common_phrases": [
                        "przepłacać świadków",
                        "przepłacać urzędnika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-либо материальную выгоду в обмен на нарушение обязанностей; давать взятку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкупать",
                                "target_lang_sense_clarification": "давать взятку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

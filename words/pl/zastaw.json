            "senses": [
                {
                    "sense_id": "b606149b-02c3-3605-a533-821942b2e505",
                    "sense_definition": "Rzecz, pieniądze lub prawo majątkowe stanowiące zabezpieczenie spłaty długu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem oddać swój zegarek w <w>zastaw</w>, aby otrzymać szybką pożyczkę.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось отдать свои часы в <w>залог</w>, чтобы получить быстрый заем."
                            }
                        },
                        {
                            "text": "Bank wymagał wpisu do hipoteki jako <w>zastawu</w> pod kredyt na mieszkanie.",
                            "target_lang_translations": {
                                "ru": "Банк требовал ипотечной записи в качестве <w>залога</w> под кредит на квартиру."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabezpieczenie",
                        "gwarancja",
                        "fant"
                    ],
                    "common_phrases": [
                        "dać w zastaw",
                        "wziąć w zastaw",
                        "pod zastaw",
                        "zastaw hipoteczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, деньги или имущественное право, служащие обеспечением погашения долга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залог",
                                "target_lang_sense_clarification": "обеспечение долга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b49c5211-170a-38ab-9b78-935b92b3ad97",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>zastawa</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie z antykami było wiele pięknych, porcelanowych <w>zastaw</w> stołowych.",
                            "target_lang_translations": {
                                "ru": "В антикварном магазине было много красивых фарфоровых столовых <w>сервизов</w>."
                            }
                        },
                        {
                            "text": "Konserwacja <w>zastaw</w> na rzece jest konieczna, aby zapobiec powodziom.",
                            "target_lang_translations": {
                                "ru": "Техническое обслуживание <w>запруд</w> на реке необходимо для предотвращения наводнений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного <w>zastawa</w> (сервиз, запруда)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сервизов",
                                "target_lang_sense_clarification": "посуда"
                            },
                            {
                                "target_lang_word": "запруд",
                                "target_lang_sense_clarification": "гидротехническое сооружение"
                            }
                        ]
                    }
                }
            ]
        },

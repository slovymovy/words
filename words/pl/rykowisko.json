    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79de2b06-cff3-30af-b794-06d252695c1c",
                    "sense_definition": "Czas godów u jeleni, podczas którego samce wydają charakterystyczne, głośne odgłosy w celu zwabienia samic i odstraszenia rywali.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lasach gorąco, <w>rykowisko</w> wkracza w kulminacyjną fazę.",
                            "target_lang_translations": {
                                "ru": "В лесах жарко, <w>гон</w> вступает в кульминационную фазу."
                            }
                        },
                        {
                            "text": "Każdego roku we wrześniu myśliwi obserwują intensywne <w>rykowisko</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый год в сентябре охотники наблюдают интенсивный <w>гон</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gody"
                    ],
                    "common_phrases": [
                        "jesienne rykowisko",
                        "czas rykowiska",
                        "odgłosy rykowiska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период спаривания у оленей, во время которого самцы издают характерные громкие звуки для привлечения самок и отпугивания соперников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гон",
                                "target_lang_sense_clarification": "период спаривания"
                            },
                            {
                                "target_lang_word": "рёв",
                                "target_lang_sense_clarification": "охотничий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d0fbade-176d-3b9a-82b5-ec871d5f4a38",
                    "sense_definition": "Miejsce w lesie, gdzie jelenie gromadzą się i walczą ze sobą w okresie godowym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leśniczy pokazał nam stare <w>rykowisko</w> na skraju puszczy.",
                            "target_lang_translations": {
                                "ru": "Лесничий показал нам старое <w>место гона</w> на краю пущи."
                            }
                        },
                        {
                            "text": "Zwierzęta często wracają na to samo <w>rykowisko</w> przez wiele lat.",
                            "target_lang_translations": {
                                "ru": "Животные часто возвращаются на одно и то же <w>место гона</w> в течение многих лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znane rykowisko",
                        "pobliskie rykowisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место в лесу, где олени собираются и сражаются друг с другом в брачный период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "место гона",
                                "target_lang_sense_clarification": "территория"
                            },
                            {
                                "target_lang_word": "место рёва",
                                "target_lang_sense_clarification": "территория"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_definition": "Gebiedende wijs van <w>touren</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Tour</w> voorzichtig en geniet van de mooie omgeving.",
                            "target_lang_translations": {
                                "ru": "<w>Путешествуй</w> осторожно и наслаждайся красивыми окрестностями."
                            }

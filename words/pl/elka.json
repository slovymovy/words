            "senses": [
                {
                    "sense_id": "1974279f-38e1-39a3-9f9e-5e9ecbfbf992",
                    "sense_definition": "Samochód do nauki jazdy, charakterystycznie oznakowany literą L na dachu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sorry, że się spóźniłem, ale przede mną wlekła się <w>elka</w>.",
                            "target_lang_translations": {
                                "ru": "Извини, что опоздал, но передо мной плелась <w>учебная машина</w>."
                            }
                        },
                        {
                            "text": "Na skrzyżowaniu zgasła mi <w>elka</w> i wszyscy zaczęli trąbić.",
                            "target_lang_translations": {
                                "ru": "На перекрёстке у меня заглохла <w>учебная машина</w>, и все начали сигналить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jechać elką",
                        "kurs elką",
                        "widzieć elkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобиль для обучения вождению, обычно обозначенный буквой L на крыше."

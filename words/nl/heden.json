            "senses": [
                {
                    "sense_id": "d2ff8128-3e5b-3433-b432-6dd5dbda61a4",
                    "sense_definition": "In de huidige tijd; op dit moment.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tot op <w>heden</w> hebben wij nog geen reactie van hen ontvangen.",

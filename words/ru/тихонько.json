            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "adf98adb-44b4-3779-9537-e47bab1bd1d0",
                    "sense_definition": "Очень тихо, не производя шума (обычно с оттенком мягкости или ласки).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Обе мои собаки <w>тихонько</w> подошли к двери и стали ждать.",
                            "target_lang_translations": {
                                "en": "Both my dogs <w>quietly</w> approached the door and began to wait."
                            }
                        },
                        {
                            "text": "Старик засмеялся <w>тихонько</w>, чтобы не разбудить спящих внуков.",
                            "target_lang_translations": {
                                "en": "The old man laughed <w>softly</w> so as not to wake his sleeping grandchildren."
                            }
                        }
                    ],
                    "synonyms": [
                        "тихо",
                        "потихоньку"
                    ],
                    "antonyms": [
                        "громко",
                        "что есть мочи"
                    ],
                    "common_phrases": [
                        "сидеть тихонько",
                        "говорить тихонько",
                        "спросить тихонько",
                        "тихонько напевать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very quietly, without making noise (often with a nuance of softness or affection)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quietly",
                                "target_lang_sense_clarification": "with low volume"
                            },
                            {
                                "target_lang_word": "softly",
                                "target_lang_sense_clarification": "gentle sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a9a6d8c-41c5-3463-98c6-23e808bcc62f",
                    "sense_definition": "Осторожно, слегка или медленно (о действии, движении или прикосновении).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ветер подхватывал сухие листья и <w>тихонько</w> катил их по дороге.",
                            "target_lang_translations": {
                                "en": "The wind picked up dry leaves and <w>gently</w> rolled them down the road."
                            }
                        },
                        {
                            "text": "Мама <w>тихонько</w> гладит ребёнка по голове, успокаивая его перед сном.",
                            "target_lang_translations": {
                                "en": "Mom <w>gently</w> strokes the child's head, soothing him before sleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "потихоньку"
                    ],
                    "antonyms": [
                        "во весь опор",
                        "изо всех сил",
                        "со всей мочи"
                    ],
                    "common_phrases": [
                        "тихонько тронуть",
                        "ехать тихонько",
                        "тихонько толкнуть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Cautiously, slightly, or slowly (regarding action, movement, or touch)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gently",
                                "target_lang_sense_clarification": "carefully or tenderly"
                            },
                            {
                                "target_lang_word": "lightly",
                                "target_lang_sense_clarification": "with little force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eac1c0e2-d009-3417-93e0-32460db554fb",
                    "sense_definition": "Скрытно от других, чтобы никто не заметил; тайком.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабке показалось, что кто-то <w>тихонько</w> лезет через забор за яблоками.",
                            "target_lang_translations": {
                                "en": "It seemed to the old woman that someone was <w>stealthily</w> climbing over the fence for apples."
                            }
                        },
                        {
                            "text": "Мы решили <w>тихонько</w> уйти с собрания, пока никто не видел.",
                            "target_lang_translations": {
                                "en": "We decided to <w>quietly</w> leave the meeting while no one was looking."
                            }
                        }
                    ],
                    "synonyms": [
                        "тайком",
                        "втихомолку",
                        "потихоньку"
                    ],
                    "antonyms": [
                        "открыто",
                        "в открытую"
                    ],
                    "common_phrases": [
                        "уйти тихонько",
                        "сделать тихонько",
                        "тихонько сбежать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Secretly, so no one notices; stealthily."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secretly",
                                "target_lang_sense_clarification": "hidden from others"
                            },
                            {
                                "target_lang_word": "stealthily",
                                "target_lang_sense_clarification": "moving unnoticed"
                            },
                            {
                                "target_lang_word": "quietly",
                                "target_lang_sense_clarification": "without drawing attention"
                            }
                        ]
                    }
                }

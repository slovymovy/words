            "senses": [
                {
                    "sense_id": "e348e881-e3ae-3c5c-b2f1-963715a94e91",
                    "sense_definition": "Het binnenbrengen van goederen uit het buitenland voor handel of gebruik.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trade_import",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>invoer</w> van buitenlandse producten is dit jaar gestegen.",
                            "target_lang_translations": {
                                "en": "The <w>import</w> of foreign products has increased this year.",
                                "ru": "<w>Импорт</w> иностранных товаров в этом году вырос."
                            }
                        },
                        {
                            "text": "Er gelden strenge regels voor de <w>invoer</w> van medicijnen.",
                            "target_lang_translations": {
                                "en": "Strict rules apply to the <w>import</w> of medicines.",
                                "ru": "Действуют строгие правила на <w>ввоз</w> лекарств."
                            }
                        }
                    ],
                    "synonyms": [
                        "import"
                    ],
                    "antonyms": [
                        "uitvoer"
                    ],
                    "common_phrases": [
                        "invoer van goederen",
                        "verbod op invoer",
                        "invoer en uitvoer"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of bringing goods into a country from abroad for trade or use.",
                        "ru": "Ввоз товаров из-за границы для торговли или использования."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "import",
                                "target_lang_sense_clarification": "goods/trade"
                            },
                            {
                                "target_lang_word": "importation",
                                "target_lang_sense_clarification": "act of importing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "импорт",
                                "target_lang_sense_clarification": "экономика/торговля"
                            },
                            {
                                "target_lang_word": "ввоз",
                                "target_lang_sense_clarification": "действие по перемещению товаров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25f81de1-760d-32a2-8138-c3d0c9c552b1",
                    "sense_definition": "Gegevens, signalen of energie die in een computer of systeem worden ingevoerd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "data_input",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De computer wacht op <w>invoer</w> van de gebruiker om verder te gaan.",

                {
                    "sense_id": "bf0092c5-d0fe-334d-89fd-47f49a8ccaca",
                    "sense_definition": "Zmiana kierunku ruchu lub pozycji pojazdu, statku lub ciała, wymagająca zazwyczaj ostrożności i precyzji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca wykonał niebezpieczny <w>manewr</w> wyprzedzania na wąskiej drodze.",
                            "target_lang_translations": {
                                "ru": "Водитель совершил опасный <w>манёвр</w> обгона на узкой дороге."
                            }
                        },
                        {
                            "text": "Ten skomplikowany <w>manewr</w> parkowania wymagał od niego dużego skupienia.",
                            "target_lang_translations": {
                                "ru": "Этот сложный <w>манёвр</w> парковки потребовал от него большой сосредоточенности."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruch"
                    ],
                    "common_phrases": [
                        "manewr wyprzedzania",
                        "manewr parkowania",
                        "manewr cofania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменение направления движения или позиции транспортного средства, судна или тела, обычно требующее осторожности и точности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манёвр",
                                "target_lang_sense_clarification": "движение транспортного средства"

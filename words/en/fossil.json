                {
                    "sense_id": "3828994e-1a92-3820-b37b-86d5b07abaf9",
                    "sense_definition": "The remains or impression of a prehistoric organism, such as a plant or animal, preserved in rock.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displays a rare dinosaur <w>fossil</w> found in the desert.",
                            "target_lang_translations": {
                                "ru": "В музее выставлена редкая <w>окаменелость</w> динозавра, найденная в пустыне."
                            }
                        },
                        {
                            "text": "Burning a <w>fossil</w> fuel releases carbon dioxide into the atmosphere.",
                            "target_lang_translations": {
                                "ru": "Сжигание <w>ископаемого</w> топлива приводит к выбросу углекислого газа в атмосферу."
                            }
                        }
                    ],
                    "synonyms": [
                        "remains",
                        "relic"
                    ],
                    "common_phrases": [
                        "fossil fuel",
                        "fossil record",
                        "dinosaur fossil",
                        "trace fossil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Останки или отпечаток доисторического организма, такого как растение или животное, сохранившиеся в камне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ископаемое",
                                "target_lang_sense_clarification": "общий термин для древних организмов"
                            },
                            {
                                "target_lang_word": "окаменелость",
                                "target_lang_sense_clarification": "превратившиеся в камень останки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04bab56a-d0b3-3dbb-a9dd-a305aa346e16",
                    "sense_definition": "A person or thing that is considered extremely old, outdated, or resistant to change.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My laptop is such a <w>fossil</w> that it takes ten minutes to start up.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "376c43d4-fe75-3429-96cd-bd26b5b0e7f7",
                    "sense_definition": "Znak interpunkcyjny służący do oddzielania mniejszych części wypowiedzi, zdań składowych w zdaniu złożonym lub elementów wyliczenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zawsze przed słowem „że” stawiamy <w>przecinek</w>.",
                            "target_lang_translations": {
                                "ru": "Мы не всегда ставим <w>запятую</w> перед словом «что»."
                            }
                        },
                        {
                            "text": "W tym długim zdaniu brakuje jednego <w>przecinka</w>.",
                            "target_lang_translations": {
                                "ru": "В этом длинном предложении не хватает одной <w>запятой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        ","
                    ],
                    "common_phrases": [
                        "postawić przecinek",
                        "przed przecinkiem",
                        "oddzielić przecinkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак препинания, служащий для разделения частей предложения или элементов перечисления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запятая",
                                "target_lang_sense_clarification": "знак препинания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1c731be-3a34-3e5e-9a3c-ba1b1ce2d7a8",
                    "sense_definition": "Znak graficzny oddzielający część całkowitą liczby od części ułamkowej w zapisie dziesiętnym.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dane były zebrane z dokładnością dwóch miejsc po <w>przecinku</w>.",
                            "target_lang_translations": {
                                "ru": "Данные были собраны с точностью до двух знаков после <w>запятой</w>."
                            }
                        },
                        {
                            "text": "Liczba pi ma nieskończenie wiele cyfr po <w>przecinku</w>.",
                            "target_lang_translations": {
                                "ru": "Число пи имеет бесконечно много цифр после <w>запятой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "miejsca po przecinku",
                        "dwa po przecinku",
                        "ułamek dziesiętny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак, отделяющий целую часть числа от дробной в десятичной записи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запятая",
                                "target_lang_sense_clarification": "в математике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6baed4f0-922b-3ea9-8806-1abae0ba17c7",
                    "sense_definition": "Wulgarne słowo używane nadmiernie jako przerywnik w mowie potocznej, często bez konkretnego znaczenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wypowiedź była chaotyczna i pełna niepotrzebnych <w>przecinków</w>.",
                            "target_lang_translations": {
                                "ru": "Его речь была хаотичной и полной ненужных <w>ругательств</w>."
                            }
                        },
                        {
                            "text": "Przestań rzucać <w>przecinkami</w>, gdy rozmawiasz z rodzicami.",
                            "target_lang_translations": {
                                "ru": "Перестань сыпать <w>матом</w>, когда разговариваешь с родителями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzucać przecinkami",
                        "same przecinki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "slang",
                            "comment": "eufemizm na wulgaryzm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное слово, чрезмерно используемое как слово-паразит в разговорной речи (эвфемизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ругательство",
                                "target_lang_sense_clarification": "бранное слово"
                            },
                            {
                                "target_lang_word": "мат",
                                "target_lang_sense_clarification": "ненормативная лексика"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "5c905ee1-b3f6-3dbf-87ed-44c00831d27b",
                    "sense_definition": "Имеющий окраску одного из основных цветов спектра, соответствующий цвету крови, огня или спелой земляники.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне очень нравится этот яркий <w>красный</w> шарф.",
                            "target_lang_translations": {
                                "en": "I really like this bright <w>red</w> scarf."
                            }
                        },
                        {
                            "text": "Туристы обязательно посещают место, где находится <w>Красная площадь</w>.",
                            "target_lang_translations": {
                                "en": "Tourists always visit the place where <w>Red Square</w> is located."
                            }
                        },
                        {
                            "text": "Осенью в лесу можно найти <w>красные</w> и жёлтые листья.",
                            "target_lang_translations": {
                                "en": "In autumn, one can find <w>red</w> and yellow leaves in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "а́лый"
                    ],
                    "antonyms": [
                        "бе́лый",
                        "зелёный"
                    ],
                    "common_phrases": [
                        "Красная площадь",
                        "красное вино",
                        "Красная книга",
                        "красный свет"
                    ],
                    "target_lang_definitions": {
                        "en": "Having the color of one of the primary colors of the spectrum, corresponding to the color of blood, fire, or ripe strawberries."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "red",
                                "target_lang_sense_clarification": "color"
                            }
                        ]
                    }
                },
                {

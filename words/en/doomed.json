{
    "word_family": [
        "doom",
        "doomsday"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fc68b093-ee21-3fcc-898b-9007af5a192b",
                    "sense_definition": "Assured to suffer death, failure, or a similarly negative outcome.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "fate_outcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dinosaurs were <w>doomed</w> to extinction due to the catastrophic event.",
                            "target_lang_translations": {
                                "ru": "Динозавры были <w>обречены</w> на вымирание из-за катастрофического события."
                            }
                        },
                        {
                            "text": "Without a proper engine, the ship was <w>doomed</w> to sink in the storm.",
                            "target_lang_translations": {
                                "ru": "Без исправного двигателя корабль был <w>обречён</w> затонуть во время шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "condemned",
                        "ill-fated",
                        "cursed",
                        "finished",
                        "wrecked"
                    ],
                    "antonyms": [
                        "blessed",
                        "protected"
                    ],
                    "common_phrases": [
                        "doomed to failure",
                        "doomed to die",
                        "doomed from the start"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обречённый на смерть, провал или иной негативный исход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обречённый",
                                "target_lang_sense_clarification": "неминуемо ожидающий гибели или неудачи"
                            },
                            {
                                "target_lang_word": "приговорённый",
                                "target_lang_sense_clarification": "осуждённый на что-либо негативное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46ae3e1e-1c77-3075-8bc8-07437bfcf4ec",
                    "sense_definition": "Assured of any outcome, whether positive or negative; fated.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fate_outcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hero felt he was <w>doomed</w> to wander the earth until he found the truth.",
                            "target_lang_translations": {
                                "ru": "Герой чувствовал, что <w>обречён</w> скитаться по земле, пока не найдет истину."
                            }
                        },
                        {
                            "text": "She was <w>doomed</w> to meet him again, as if the stars had aligned.",
                            "target_lang_translations": {
                                "ru": "Ей было <w>суждено</w> встретить его снова, словно так сошлись звезды."
                            }
                        }
                    ],
                    "synonyms": [
                        "destined",
                        "fated",
                        "predestined",
                        "foreordained"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предопределённый судьбой к какому-либо исходу; фатальный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предначертанный",
                                "target_lang_sense_clarification": "заранее определённый судьбой"
                            },
                            {
                                "target_lang_word": "сужденный",
                                "target_lang_sense_clarification": "предопределённый свыше"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9bb4f8ec-14ff-34e6-8e16-ae1a68ce43ff",
                    "sense_definition": "Simple past and past participle of <w>doom</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal <w>doomed</w> his political career before it even began.",
                            "target_lang_translations": {
                                "ru": "Скандал <w>погубил</w> его политическую карьеру еще до того, как она началась."
                            }
                        },
                        {
                            "text": "Poor planning <w>doomed</w> the project to failure.",
                            "target_lang_translations": {
                                "ru": "Плохое планирование <w>обрекло</w> проект на провал."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие от глагола 'to doom' (обрекать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрёк",
                                "target_lang_sense_clarification": "сделал неудачу неизбежной"
                            },
                            {
                                "target_lang_word": "погубил",
                                "target_lang_sense_clarification": "привел к краху или гибели"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
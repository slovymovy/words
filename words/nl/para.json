                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "verkorting"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A soldier trained to jump from an aircraft using a parachute; short for paratrooper."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "paratrooper",
                                "target_lang_sense_clarification": "military soldier"
                            },
                            {

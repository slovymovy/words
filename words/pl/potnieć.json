    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54208a9b-d3e9-3330-b32c-d3e75bc42328",
                    "sense_definition": "Wydzielać pot przez pory skóry; stawać się wilgotnym od potu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physiological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze zdenerwowania zaczęły mi gwałtownie <potnieć> dłonie.",
                            "target_lang_translations": {
                                "ru": "От волнения у меня начали сильно <потеть> ладони."
                            }
                        },
                        {
                            "text": "Podczas intensywnego wysiłku fizycznego całe ciało zaczyna <potnieć>.",
                            "target_lang_translations": {
                                "ru": "Во время интенсивной физической нагрузки всё тело начинает <потеть>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pocić się"
                    ],
                    "common_phrases": [
                        "dłonie potnieją",
                        "ciało potnieje",
                        "czoło potnieje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделять пот через поры кожи; становиться влажным от пота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потеть",
                                "target_lang_sense_clarification": "физиологический процесс"
                            },
                            {
                                "target_lang_word": "покрываться потом",
                                "target_lang_sense_clarification": "становиться мокрым от пота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "408b8aa0-8266-3003-a6f8-aafb4dab5520",
                    "sense_definition": "Pokrywać się osadem z pary wodnej; stawać się wilgotnym na powierzchni (o przedmiotach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą szyby w samochodzie często <potnieją> od środka z powodu różnicy temperatur.",
                            "target_lang_translations": {
                                "ru": "Зимой стёкла в машине часто <запотевают> изнутри из-за разницы температур."
                            }
                        },
                        {
                            "text": "Kiedy wchodzisz z mrozu do ciepłego pomieszczenia, okulary natychmiast zaczynają <potnieć>.",
                            "target_lang_translations": {
                                "ru": "Когда входишь с мороза в тёплое помещение, очки мгновенно начинают <запотевать>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaparowywać"
                    ],
                    "common_phrases": [
                        "szyby potnieją",
                        "okulary potnieją",
                        "lustro potnieje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрываться налетом влаги (конденсатом); становиться влажным на поверхности (о предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запотевать",
                                "target_lang_sense_clarification": "покрываться конденсатом (о стекле, зеркалах)"
                            },
                            {
                                "target_lang_word": "потеть",
                                "target_lang_sense_clarification": "разговорное; о стеклах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3de3eb6e-d945-302a-ad40-3a1045ef8629",
                    "sense_definition": "zdrobnienie od: <w>polana</w>; mała, niezalesiona przestrzeń wśród drzew w lesie",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nature_clearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długim marszu przez gęsty las naszym oczom ukazała się słoneczna <polanka>.",
                            "target_lang_translations": {
                                "ru": "После долгого марша через густой лес нашим глазам открылась солнечная <w>полянка</w>."
                            }
                        },
                        {
                            "text": "Ta urocza leśna <polanka> to doskonałe miejsce na krótki odpoczynek.",
                            "target_lang_translations": {
                                "ru": "Эта очаровательная лесная <w>полянка</w> — отличное место для короткого отдыха."
                            }
                        }
                    ],
                    "synonyms": [
                        "łączka"
                    ],
                    "common_phrases": [
                        "leśna polanka",
                        "słoneczna polanka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьшительная форма от <w>polana</w>; небольшое, незалесённое пространство среди деревьев в лесу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полянка",
                                "target_lang_sense_clarification": "уменьшительное от «поляна»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e38f463-8ccc-3108-aa44-0d0f0e7956e4",
                    "sense_definition": "forma fleksyjna (dopełniacz liczby pojedynczej) rzeczownika <w>polanko</w>",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "polanko_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciel tego małego <polanka> postanowił sprzedać ziemię sąsiadowi.",
                            "target_lang_translations": {
                                "ru": "Владелец этого маленького <w>поля</w> решил продать землю соседу."
                            }
                        },
                        {
                            "text": "Wokół tego <polanka> nie ma jeszcze żadnego ogrodzenia.",
                            "target_lang_translations": {
                                "ru": "Вокруг этого <w>участка</w> ещё нет никакого забора."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "флективная форма (родительный падеж единственного числа) существительного <w>polanko</w>"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маленькое поле",
                                "target_lang_sense_clarification": "небольшой участок земли"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "земля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f993bd6-7326-3191-ad15-5ad6a9334e07",
                    "sense_definition": "forma fleksyjna (mianownik, biernik lub wołacz liczby mnogiej) rzeczownika <w>polanko</w>",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "polanko_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te dwa uprawne <polanka> przynoszą rolnikowi niewielkie zbiory.",
                            "target_lang_translations": {
                                "ru": "Эти два возделываемых <w>поля</w> приносят фермеру небольшие урожаи."
                            }
                        },
                        {
                            "text": "Przez okno pociągu widziałem liczne <polanka> obsiane zbożem.",
                            "target_lang_translations": {
                                "ru": "Из окна поезда я видел многочисленные <w>поля</w>, засеянные зерном."
                            }
                        }
                    ],
                    "traits": [

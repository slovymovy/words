    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "77b75ac2-ac66-34bb-a88d-6287d9a712ee",
                    "sense_definition": "Designed to respond to the actions, commands, or input of a user, allowing a two-way flow of information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tech_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum features an <w>interactive</w> map that visitors can touch to learn more.",
                            "target_lang_translations": {
                                "ru": "В музее представлена <w>интерактивная</w> карта, которой посетители могут коснуться, чтобы узнать больше."
                            }
                        },
                        {
                            "text": "Children learn faster when they play an <w>interactive</w> game rather than just reading.",
                            "target_lang_translations": {
                                "ru": "Дети учатся быстрее, когда играют в <w>интерактивную</w> игру, а не просто читают."
                            }
                        }
                    ],
                    "synonyms": [
                        "responsive"
                    ],
                    "antonyms": [
                        "static",
                        "passive"
                    ],
                    "common_phrases": [
                        "interactive map",
                        "interactive game",
                        "interactive website",
                        "interactive whiteboard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разработанный для реагирования на действия, команды или ввод пользователя, обеспечивающий двусторонний обмен информацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интерактивный",
                                "target_lang_sense_clarification": "о программах и устройствах"
                            },
                            {
                                "target_lang_word": "диалоговый",
                                "target_lang_sense_clarification": "режим работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f68a531-e551-3e70-9c73-ed09bb81312c",
                    "sense_definition": "Involving communication or action between people or things where they influence one another.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workshop was highly <w>interactive</w>, encouraging everyone to share their ideas.",
                            "target_lang_translations": {
                                "ru": "Семинар был очень <w>интерактивным</w>, побуждая всех делиться своими идеями."
                            }
                        },
                        {
                            "text": "These two chemicals have an <w>interactive</w> effect when they are mixed together.",
                            "target_lang_translations": {
                                "ru": "Эти два химиката оказывают <w>взаимное</w> воздействие при смешивании."
                            }
                        }
                    ],
                    "synonyms": [
                        "collaborative",
                        "reciprocal"
                    ],
                    "antonyms": [

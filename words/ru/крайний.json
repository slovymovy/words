    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "48c6b3d7-db3c-3830-8472-3c025f830dd0",
                    "sense_definition": "Находящийся с краю, в начале или в конце ряда, линии или очереди.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто здесь <крайний> в очереди к врачу?",
                            "target_lang_translations": {
                                "en": "Who is <last> in line to see the doctor?"
                            }
                        },
                        {
                            "text": "Мы купили билеты на <крайний> ряд балкона.",
                            "target_lang_translations": {
                                "en": "We bought tickets for the <last> row of the balcony."
                            }
                        }
                    ],
                    "common_phrases": [
                        "крайний дом",
                        "крайний ряд",
                        "кто крайний",
                        "крайний срок"
                    ],
                    "target_lang_definitions": {
                        "en": "Located at the edge, at the beginning or end of a row, line, or queue."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "in a sequence or queue"
                            },
                            {
                                "target_lang_word": "end",
                                "target_lang_sense_clarification": "position in a row or structure"
                            },
                            {
                                "target_lang_word": "outermost",
                                "target_lang_sense_clarification": "furthest from the center"
                            }

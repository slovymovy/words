                    "sense_id": "a22fa5ed-cffb-3214-a570-83f34cadd733",
                    "sense_definition": "Iets wat veel energie kost en waar je moe van wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange en <w>vermoeiende</w> dag ging hij direct naar bed.",
                            "target_lang_translations": {
                                "en": "After a long and <w>tiring</w> day, he went straight to bed.",
                                "ru": "После долгого и <w>утомительного</w> дня он сразу лег спать."
                            }
                        },
                        {
                            "text": "Het reizen met het openbaar vervoer kan soms erg <w>vermoeiend</w> zijn.",
                            "target_lang_translations": {
                                "en": "Traveling by public transport can sometimes be very <w>tiring</w>.",
                                "ru": "Поездки на общественном транспорте иногда могут быть очень <w>утомительными</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitputtend",
                        "zwaar",
                        "afmattend"
                    ],
                    "antonyms": [
                        "ontspannend",
                        "rustgevend"
                    ],
                    "common_phrases": [
                        "een vermoeiende dag",
                        "erg vermoeiend",
                        "een vermoeiende reis"
                    ],
                    "target_lang_definitions": {

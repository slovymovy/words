    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94429677-f83d-3b6b-a653-7f4f72e16363",
                    "sense_definition": "Сильно, учащённо или громко биться (обычно о сердце или пульсе) из-за волнения, страха или физической нагрузки.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "heartbeat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После быстрого бега моё сердце бешено <колотилось> в груди.",
                            "target_lang_translations": {
                                "en": "After a fast run, my heart was <pounding> furiously in my chest."
                            }
                        },
                        {
                            "text": "От страха сердце <колотится> так сильно, что его слышно.",
                            "target_lang_translations": {
                                "en": "From fear, my heart <beats> so hard that it can be heard."
                            }
                        }
                    ],
                    "synonyms": [
                        "биться"
                    ],
                    "common_phrases": [
                        "сердце колотится",
                        "бешено колотиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To beat strongly, rapidly, or loudly (usually of the heart or pulse) due to excitement, fear, or physical exertion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pound",
                                "target_lang_sense_clarification": "to beat loudly or heavily (of the heart)"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to move with pulsation or throbbing"
                            },
                            {
                                "target_lang_word": "throb",
                                "target_lang_sense_clarification": "to beat with a strong, regular rhythm"
                            },
                            {
                                "target_lang_word": "race",
                                "target_lang_sense_clarification": "to beat very fast due to emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3ef9aeb-f891-34c2-8416-5759ac4e1c83",
                    "sense_definition": "Ударяться телом или частью тела обо что-либо твёрдое; совершать резкие, беспорядочные движения, натыкаясь на препятствия.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Испуганная птица <колотилась> в закрытое окно, пытаясь вылететь.",
                            "target_lang_translations": {
                                "en": "The frightened bird <thrashed> against the closed window, trying to fly out."
                            }
                        },
                        {
                            "text": "В отчаянии он <колотился> головой о стену.",
                            "target_lang_translations": {
                                "en": "In despair, he <banged> his head against the wall."
                            }
                        }
                    ],
                    "synonyms": [
                        "биться",
                        "ударяться"
                    ],
                    "common_phrases": [
                        "колотиться головой",
                        "колотиться об стену",
                        "колотиться в истерике"
                    ],
                    "target_lang_definitions": {
                        "en": "To hit one's body or a part of it against something hard; to make sharp, disorderly movements, bumping into obstacles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thrash",
                                "target_lang_sense_clarification": "to move violently and irregularly against obstacles"
                            },
                            {
                                "target_lang_word": "bang",
                                "target_lang_sense_clarification": "to hit a body part forcefully against something"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to strike repeatedly against something"
                            },
                            {
                                "target_lang_word": "knock",
                                "target_lang_sense_clarification": "to bump or impact against something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "417c9938-db68-31da-9218-d13ffb0d5b12",
                    "sense_definition": "Громко и настойчиво стучать в дверь, ворота или окно, требуя, чтобы впустили.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто-то громко <колотился> в железную дверь, но никто не открывал.",
                            "target_lang_translations": {
                                "en": "Someone was loudly <pounding> on the iron door, but no one opened."
                            }
                        },
                        {
                            "text": "Мы долго <колотились> в ворота, пока нас не услышал сторож.",
                            "target_lang_translations": {
                                "en": "We <banged> on the gate for a long time until the watchman heard us."
                            }
                        }
                    ],
                    "synonyms": [
                        "биться",
                        "ударяться"
                    ],
                    "common_phrases": [
                        "колотиться в дверь",
                        "колотиться в окно"
                    ],
                    "target_lang_definitions": {
                        "en": "To knock loudly and persistently on a door, gate, or window, demanding to be let in."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pound",
                                "target_lang_sense_clarification": "to knock heavily and continuously"
                            },
                            {
                                "target_lang_word": "bang",
                                "target_lang_sense_clarification": "to hit noisily to demand attention"
                            },
                            {
                                "target_lang_word": "hammer",
                                "target_lang_sense_clarification": "to beat or strike repeatedly with force"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to strike repeatedly for admittance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58cb3815-562b-3ba0-9cca-283f8a73e9d2",
                    "sense_definition": "Разбиваться, ломаться на мелкие части (обычно о посуде или стекле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом доме постоянно <колотится> посуда из-за неосторожности.",
                            "target_lang_translations": {
                                "en": "Dishes constantly <break> in this house due to carelessness."
                            }
                        },
                        {
                            "text": "При переезде много хрупких вещей <колотилось> в коробках.",
                            "target_lang_translations": {
                                "en": "During the move, many fragile things <broke> in the boxes."
                            }
                        }
                    ],
                    "synonyms": [
                        "разбиваться",
                        "биться"
                    ],
                    "common_phrases": [
                        "посуда колотится"
                    ],
                    "target_lang_definitions": {
                        "en": "To break or shatter into small pieces (usually regarding dishes or glass)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "to separate into pieces as a result of a blow or shock"
                            },
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "to break suddenly and violently into many pieces"
                            },
                            {
                                "target_lang_word": "smash",
                                "target_lang_sense_clarification": "to break noisily into pieces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ae24e0f-7c08-38f5-9033-4ac806637abe",
                    "sense_definition": "Тяжело работать, суетиться, прилагать чрезмерные усилия для решения жизненных проблем или достижения цели.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "struggle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она одна <колотится> с тремя детьми, пытаясь их прокормить.",
                            "target_lang_translations": {
                                "en": "She <struggles> alone with three children, trying to feed them."
                            }
                        },
                        {
                            "text": "Мы целый месяц <колотились> над этим сложным проектом.",
                            "target_lang_translations": {
                                "en": "We <toiled> for a whole month over this complex project."
                            }
                        }
                    ],
                    "synonyms": [
                        "биться"
                    ],
                    "common_phrases": [
                        "колотиться над задачей",
                        "колотиться на работе"
                    ],
                    "target_lang_definitions": {
                        "en": "To work hard, fuss, or exert excessive effort to solve life problems or achieve a goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "struggle",
                                "target_lang_sense_clarification": "to make a strenuous effort in the face of difficulties"
                            },
                            {
                                "target_lang_word": "toil",
                                "target_lang_sense_clarification": "to work extremely hard or incessantly"
                            },
                            {
                                "target_lang_word": "bust one's hump",
                                "target_lang_sense_clarification": "slang for working very hard"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "be3a538a-c219-3d96-8e83-53945aacf51b",
                    "sense_definition": "Iets op een bepaalde manier uitleggen, opvatten of begrijpen; betekenis toekennen aan gegevens of signalen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "begrip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De partij <w>interpreteerde</w> de positieve stembusuitslag als rechtvaardiging van haar conservatieve beleid.",
                            "target_lang_translations": {
                                "ru": "Партия <w>интерпретировала</w> положительные результаты выборов как оправдание своей консервативной политики."
                            }
                        },
                        {
                            "text": "Het kan soms lastig zijn om de stilte van iemand anders correct te <w>interpreteren</w>.",
                            "target_lang_translations": {
                                "ru": "Иногда бывает сложно правильно <w>истолковать</w> чье-то молчание."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitleggen",
                        "verklaren",
                        "duiden"
                    ],
                    "common_phrases": [
                        "verkeerd interpreteren",
                        "ruim interpreteren",
                        "vrij interpreteren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объяснять, воспринимать или понимать что-либо определенным образом; придавать значение данным или сигналам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интерпретировать",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "толковать",
                                "target_lang_sense_clarification": "объяснять смысл"
                            },
                            {
                                "target_lang_word": "истолковывать",
                                "target_lang_sense_clarification": "понимать определенным образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a276279-059b-378b-b324-b9c93baec951",
                    "sense_definition": "Een artistiek werk, zoals muziek of een toneelrol, op een persoonlijke manier uitvoeren of uitbeelden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kunst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jonge pianist <w>interpreteerde</w> het bekende stuk van Chopin op een verrassende manier.",
                            "target_lang_translations": {
                                "ru": "Молодой пианист <w>интерпретировал</w> известное произведение Шопена неожиданным образом."
                            }
                        },
                        {
                            "text": "De actrice wist de complexe rol van de koningin overtuigend te <w>interpreteren</w>.",
                            "target_lang_translations": {
                                "ru": "Актриса смогла убедительно <w>интерпретировать</w> сложную роль королевы."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertolken"
                    ],
                    "common_phrases": [
                        "een rol interpreteren",
                        "een muziekstuk interpreteren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполнять или изображать художественное произведение (музыку, роль) в индивидуальной манере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интерпретировать",
                                "target_lang_sense_clarification": "в искусстве"
                            },
                            {
                                "target_lang_word": "трактовать",
                                "target_lang_sense_clarification": "давать трактовку образу или произведению"
                            }
                        ]
                    }
                }
            ]
        }
    ]

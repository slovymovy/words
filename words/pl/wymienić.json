                    "sense_id": "cfddb045-2893-34c2-9b8e-72f6af52871a",
                    "sense_definition": "Zastąpić jedną rzecz inną, zazwyczaj nową, lepszą lub sprawną.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_replace",
                    "name_type": "no",

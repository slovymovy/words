    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f9c70eec-4737-301a-8834-be7a6daf13a6",
                    "sense_definition": "Resistant to control or stubborn, especially describing an animal that refuses to move forward or obey commands.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stubbornness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer struggled to lead the <w>resty</w> horse out of the stable.",
                            "target_lang_translations": {
                                "ru": "Фермер с трудом пытался вывести <w>норовистую</w> лошадь из конюшни."
                            }
                        },
                        {
                            "text": "She found the mule to be <w>resty</w> and unwilling to carry the heavy load.",
                            "target_lang_translations": {
                                "ru": "Она нашла мула <w>упрямым</w> и не желающим нести тяжелый груз."
                            }
                        }
                    ],
                    "synonyms": [
                        "restive",
                        "stubborn",
                        "obstinate",
                        "unmanageable"
                    ],
                    "antonyms": [
                        "docile",
                        "obedient",
                        "manageable"
                    ],
                    "common_phrases": [
                        "resty horse"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Commonly used in older texts to describe horses."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Упрямый, отказывающийся двигаться вперед или подчиняться (особенно о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "норовистый",
                                "target_lang_sense_clarification": "о лошадях или других животных"
                            },
                            {
                                "target_lang_word": "упрямый",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "непокорный",
                                "target_lang_sense_clarification": "трудно управляемый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "676245ad-5d44-37ef-acbf-a5df6e26522e",
                    "sense_definition": "Disposed to rest; inactive, sluggish, or lazy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "inactivity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>resty</w> feeling overcame him after sitting by the warm fire for so long.",
                            "target_lang_translations": {
                                "ru": "<w>Вялое</w> чувство овладело им после того, как он так долго сидел у теплого огня."
                            }
                        },
                        {
                            "text": "He lived a <w>resty</w> life, avoiding hard work whenever possible.",
                            "target_lang_translations": {
                                "ru": "Он вел <w>праздную</w> жизнь, избегая тяжелой работы при любой возможности."
                            }
                        }
                    ],
                    "synonyms": [
                        "lazy",
                        "sluggish",
                        "inactive",
                        "slothful"
                    ],
                    "antonyms": [
                        "active",
                        "energetic"
                    ],
                    "common_phrases": [
                        "resty sloth",
                        "resty life"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Found in literary or historical contexts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к отдыху; бездеятельный, вялый или ленивый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вялый",
                                "target_lang_sense_clarification": "лишенный энергии"
                            },
                            {
                                "target_lang_word": "ленивый",
                                "target_lang_sense_clarification": "избегающий работы"
                            },
                            {
                                "target_lang_word": "праздный",
                                "target_lang_sense_clarification": "проводящий время без дела"
                            }
                        ]
                    }
                }
            ]
        }
    ]

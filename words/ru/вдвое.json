    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ba08ff35-d078-3829-b790-1c7a02c35262",
                    "sense_definition": "В два раза (по количеству, размеру, силе или степени).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом году урожай яблок был <w>вдвое</w> больше обычного.",
                            "target_lang_translations": {
                                "en": "This year the apple harvest was <w>twice</w> as big as usual."
                            }
                        },
                        {
                            "text": "Путь через лес короче <w>вдвое</w>, но он намного опаснее.",
                            "target_lang_translations": {
                                "en": "The path through the forest is <w>half</w> as long, but it is much more dangerous."
                            }
                        }
                    ],
                    "synonyms": [
                        "в два раза",
                        "вдвойне"
                    ],
                    "common_phrases": [
                        "вдвое больше",
                        "вдвое меньше",
                        "вдвое дороже",
                        "вдвое быстрее"
                    ],
                    "target_lang_definitions": {
                        "en": "Twice; two times as much (in quantity, size, strength, or degree)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "twice",
                                "target_lang_sense_clarification": "Two times; by a factor of two"
                            },
                            {
                                "target_lang_word": "double",
                                "target_lang_sense_clarification": "To twice the amount or extent"
                            },
                            {
                                "target_lang_word": "twofold",
                                "target_lang_sense_clarification": "Formal: multiplied by two"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aac41e89-40a4-31e5-8bdc-8106c15c5e58",
                    "sense_definition": "На две равные части (обычно о сгибании или складывании чего-либо).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "folding_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сложите лист бумаги <w>вдвое</w> и напишите своё имя.",
                            "target_lang_translations": {
                                "en": "Fold the sheet of paper <w>in half</w> and write your name."
                            }
                        },
                        {
                            "text": "Веревку пришлось сложить <w>вдвое</w>, чтобы она выдержала груз.",
                            "target_lang_translations": {
                                "en": "The rope had to be folded <w>double</w> to withstand the load."
                            }
                        }
                    ],
                    "synonyms": [
                        "пополам",
                        "напополам"
                    ],
                    "common_phrases": [
                        "сложить вдвое",
                        "согнуть вдвое",
                        "свернуть вдвое"
                    ],
                    "target_lang_definitions": {
                        "en": "In half; into two equal parts (usually regarding folding or bending)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in half",
                                "target_lang_sense_clarification": "Into two equal parts"
                            },
                            {
                                "target_lang_word": "in two",
                                "target_lang_sense_clarification": "Into two parts"
                            },
                            {
                                "target_lang_word": "double",
                                "target_lang_sense_clarification": "Folded over to create two layers"
                            }
                        ]
                    }
                }
            ]
        }
    ]

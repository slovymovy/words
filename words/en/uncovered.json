                    "antonyms": [
                        "insured",
                        "covered"
                    ],
                    "common_phrases": [
                        "uncovered risk",
                        "uncovered loan",
                        "uncovered position"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий финансового обеспечения или страхового покрытия."
                    },
                    "translations": {

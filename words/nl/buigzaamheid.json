                    "sense_id": "99a21f27-2071-360f-87fd-5e7c6920c37f",
                    "sense_definition": "Het vermogen om gemakkelijk te buigen zonder te breken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>buigzaamheid</w> van bamboe zorgt ervoor dat het niet breekt in de wind.",
                            "target_lang_translations": {
                                "en": "The <w>flexibility</w> of bamboo ensures that it does not break in the wind.",
                                "ru": "<w>Гибкость</w> бамбука позволяет ему не ломаться на ветру."
                            }
                        },
                        {
                            "text": "Door dagelijks te rekken verbeter je de <w>buigzaamheid</w> van je spieren.",
                            "target_lang_translations": {
                                "en": "By stretching daily, you improve the <w>flexibility</w> of your muscles.",
                                "ru": "Ежедневная растяжка улучшает <w>гибкость</w> ваших мышц."
                            }
                        }
                    ],
                    "synonyms": [
                        "flexibiliteit",
                        "soepelheid",
                        "lenigheid"
                    ],
                    "antonyms": [
                        "onbuigzaamheid",
                        "starheid"
                    ],
                    "common_phrases": [
                        "grote buigzaamheid",
                        "fysieke buigzaamheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to bend easily without breaking.",
                        "ru": "Способность легко гнуться, не ломаясь."
                    },

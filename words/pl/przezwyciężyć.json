                {
                    "sense_id": "dfab48c0-f109-3f22-8ed8-d764ec3b1419",
                    "sense_definition": "Pokonać trudności, przeszkody lub silne, negatywne uczucia, aby osiągnąć zamierzony cel.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sportowiec musiał <w>przezwyciężyć</w> ogromny ból, aby dobiec do mety i zdobyć medal.",
                            "target_lang_translations": {
                                "ru": "Спортсмену пришлось <w>преодолеть</w> огромную боль, чтобы добежать до финиша и завоевать медаль."
                            }
                        },
                        {
                            "text": "Wspólnymi siłami udało nam się <w>przezwyciężyć</w> kryzys finansowy w naszej firmie.",
                            "target_lang_translations": {
                                "ru": "Общими усилиями нам удалось <w>преодолеть</w> финансовый кризис в нашей фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokonać",
                        "zwalczyć",
                        "przełamać"
                    ],
                    "antonyms": [
                        "ulec",
                        "poddać się"
                    ],
                    "common_phrases": [
                        "przezwyciężyć strach",
                        "przezwyciężyć kryzys",
                        "przezwyciężyć trudności",
                        "przezwyciężyć barierę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преодолеть трудности, препятствия или сильные негативные чувства, чтобы достичь намеченной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преодолеть",
                                "target_lang_sense_clarification": "трудности, препятствия, кризис"
                            },
                            {
                                "target_lang_word": "перебороть",
                                "target_lang_sense_clarification": "страх, боль, чувства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d5bb198-a25e-39f8-9dc6-03958ddf6b71",
                    "sense_definition": "Opanować swoje słabości, niechęć lub lęk, zmuszając się do wykonania jakiejś czynności (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem się <w>przezwyciężyć</w>, żeby wstać wcześnie rano i pójść biegać w deszczu.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <w>пересилить</w> себя, чтобы встать рано утром и пойти бегать под дождем."
                            }
                        },
                        {
                            "text": "Długo nie mogła się <w>przezwyciężyć</w>, by zadzwonić do niego z przeprosinami po kłótni.",
                            "target_lang_translations": {
                                "ru": "Она долго не могла <w>пересилить</w> себя, чтобы позвонить ему с извинениями после ссоры."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemóc"
                    ],
                    "common_phrases": [
                        "przezwyciężyć się wewnętrznie",
                        "przezwyciężyć się do działania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справиться со своими слабостями, нежеланием или страхом, заставляя себя выполнить какое-либо действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересилить себя",
                                "target_lang_sense_clarification": "заставить себя сделать что-то"
                            },
                            {
                                "target_lang_word": "преодолеть себя",
                                "target_lang_sense_clarification": "справиться с внутренним сопротивлением"
                            }
                        ]
                    }
                }
            ]
        }

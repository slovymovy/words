                    "frequency": "High",
                    "semantic_group_id": "future_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>will</w> call you as soon as I arrive at the station."
                        },
                        {
                            "text": "The weather forecast says it <w>will</w> rain tomorrow morning."
                        }
                    ],
                    "common_phrases": [

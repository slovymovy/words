{
    "word_family": [
        "kieszonka",
        "kieszeniowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "389df89c-4cca-3a20-9800-1c46e4eb9875",
                    "sense_definition": "rodzaj małego schowka, np. w ubraniu lub torbie, służącego do przechowywania drobnych przedmiotów",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Włożyłem zmarznięte ręce do <w>kieszeni</w> płaszcza.",
                            "target_lang_translations": {
                                "ru": "Я сунул замёрзшие руки в <w>карманы</w> пальто."
                            }
                        },
                        {
                            "text": "Schowaj ten bilet do <w>kieszeni</w>, żeby go nie zgubić.",
                            "target_lang_translations": {
                                "ru": "Спрячь этот билет в <w>карман</w>, чтобы не потерять его."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Poznań) kiejda",
                        "gw. (Górny Śląsk) kabza",
                        "gw. (Śląsk Cieszyński) kapsa"
                    ],
                    "common_phrases": [
                        "kieszeń spodni",
                        "wewnętrzna kieszeń",
                        "ręce w kieszeniach",
                        "głęboka kieszeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "вид небольшого отделения, например, в одежде или сумке, служащего для хранения мелких предметов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карман",
                                "target_lang_sense_clarification": "часть одежды или сумки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6b5094b-a853-3c45-b595-7e10a6a56e0c",
                    "sense_definition": "dostępne środki finansowe, fundusze, którymi ktoś dysponuje",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finances",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Własne mieszkanie w centrum nie jest na moją <w>kieszeń</w>.",
                            "target_lang_translations": {
                                "ru": "Собственная квартира в центре мне не по <w>карману</w>."
                            }
                        },
                        {
                            "text": "Niespodziewany remont samochodu mocno uderzył nas po <w>kieszeni</w>.",
                            "target_lang_translations": {
                                "ru": "Неожиданный ремонт машины сильно ударил нас по <w>карману</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "portfel",
                        "kabza",
                        "trzos",
                        "sakiewka"
                    ],
                    "common_phrases": [
                        "na każdą kieszeń",
                        "uderzyć po kieszeni",
                        "z własnej kieszeni",
                        "mieć węża w kieszeni"
                    ],
                    "target_lang_definitions": {
                        "ru": "доступные финансовые средства, деньги, которыми кто-либо располагает"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карман",
                                "target_lang_sense_clarification": "финансы, бюджет"
                            },
                            {
                                "target_lang_word": "кошелёк",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cfa2e1f-1499-3090-99ed-cda090d3c80f",
                    "sense_definition": "część urządzenia w formie wnęki, do której wsuwa się wymienny element, np. dysk twardy lub kasetę",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym starym napędzie DVD często zacina się <w>kieszeń</w>.",
                            "target_lang_translations": {
                                "ru": "В этом старом DVD-приводе часто заедает <w>лоток</w>."
                            }
                        },
                        {
                            "text": "Musisz dokupić zewnętrzną <w>kieszeń</w> na dodatkowy dysk.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно докупить внешний <w>карман</w> для дополнительного диска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kieszeń na dysk",
                        "kieszeń montażowa",
                        "zewnętrzna kieszeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "часть устройства в виде ниши, в которую вставляется сменный элемент, например, жесткий диск или кассета"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карман",
                                "target_lang_sense_clarification": "для жесткого диска"
                            },
                            {
                                "target_lang_word": "отсек",
                                "target_lang_sense_clarification": "ниша в корпусе"
                            },
                            {
                                "target_lang_word": "лоток",
                                "target_lang_sense_clarification": "выдвижная часть привода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b47792f2-8103-3378-8b64-b3c85e45c3ec",
                    "sense_definition": "wnęka lub pomieszczenie przy scenie teatralnej służące do przechowywania dekoracji",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "theater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktorzy czekali w bocznej <w>kieszeni</w> na zmianę scenografii.",
                            "target_lang_translations": {
                                "ru": "Актёры ждали в боковом <w>кармане</w> смены декораций."
                            }
                        },
                        {
                            "text": "Wszystkie duże dekoracje zostały przesunięte do lewej <w>kieszeni</w>.",
                            "target_lang_translations": {
                                "ru": "Все крупные декорации были перемещены в левый <w>карман</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kieszeń sceniczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "ниша или помещение при театральной сцене, служащее для хранения декораций"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карман",

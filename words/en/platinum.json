                    "common_phrases": [
                        "platinum blonde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беловато-серого цвета, как металл платина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платиновый",
                                "target_lang_sense_clarification": "о цвете"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cdd80ae6-1fb9-3d2f-a993-c131167d7d03",
                    "sense_definition": "To attain all of the achievements or trophies in a video game.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I managed to <w>platinum</w> the game after playing for two hundred hours.",
                            "target_lang_translations": {
                                "ru": "Мне удалось <w>выбить платину</w> в игре после двухсот часов прохождения."
                            }
                        },
                        {
                            "text": "He tries to <w>platinum</w> every new release in the series.",
                            "target_lang_translations": {
                                "ru": "Он пытается <w>выбить платину</w> в каждом новом выпуске серии."
                            }

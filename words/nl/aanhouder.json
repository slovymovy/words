{
    "word_family": [
        "aanhouden",
        "aanhouding",
        "aanhoudend"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23478e43-676a-353b-83b9-14690e6b8610",
                    "sense_definition": "Iemand die vasthoudend is en niet snel opgeeft om een doel te bereiken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "persister",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>aanhouder</w> wint uiteindelijk, dus je moet blijven proberen.",
                            "target_lang_translations": {
                                "en": "The <w>one who perseveres</w> wins in the end, so you must keep trying.",
                                "ru": "<w>Настойчивый</w> в конце концов побеждает, так что нужно продолжать пытаться."
                            }
                        },
                        {
                            "text": "Hij is een echte <w>aanhouder</w> en laat zich niet door tegenslagen stoppen.",
                            "target_lang_translations": {
                                "en": "He is a real <w>perseverer</w> and doesn't let setbacks stop him.",
                                "ru": "Он очень <w>настойчивый человек</w> и не позволяет неудачам остановить себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "volhouder",
                        "doorzetter"
                    ],
                    "antonyms": [
                        "opgever"
                    ],
                    "common_phrases": [
                        "de aanhouder wint"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is persistent and does not give up easily to achieve a goal.",
                        "ru": "Человек, который проявляет упорство и не сдается на пути к достижению цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perseverer",
                                "target_lang_sense_clarification": "someone who persists in a course of action"
                            },
                            {
                                "target_lang_word": "sticker",
                                "target_lang_sense_clarification": "someone who remains persistent (informal/dated)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "настойчивый человек",
                                "target_lang_sense_clarification": "тот, кто проявляет настойчивость"
                            },
                            {
                                "target_lang_word": "упорный человек",
                                "target_lang_sense_clarification": "тот, кто не отступает перед трудностями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ea4d43c-43ba-3d3d-875f-14802de927d7",
                    "sense_definition": "Iemand die een ander arresteert of staande houdt, meestal een politieagent.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "arrester",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>aanhouder</w> vroeg de verdachte om zijn legitimatiebewijs.",
                            "target_lang_translations": {
                                "en": "The <w>arresting officer</w> asked the suspect for his ID.",
                                "ru": "<w>Задерживающий</w> попросил у подозреваемого удостоверение личности."
                            }
                        },
                        {
                            "text": "In het rapport stond de naam van de <w>aanhouder</w> vermeld.",
                            "target_lang_translations": {
                                "en": "The name of the <w>arrester</w> was mentioned in the report.",
                                "ru": "В отчете было указано имя <w>задерживающего</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "arrestant"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who arrests or stops another person, usually a police officer.",
                        "ru": "Лицо, производящее задержание или арест (обычно сотрудник полиции)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arresting officer",
                                "target_lang_sense_clarification": "police officer making an arrest"
                            },
                            {
                                "target_lang_word": "arrester",
                                "target_lang_sense_clarification": "person who performs an arrest"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "задерживающий",
                                "target_lang_sense_clarification": "лицо, осуществляющее задержание"
                            },
                            {
                                "target_lang_word": "производящий арест",
                                "target_lang_sense_clarification": "официальное лицо, выполняющее арест"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c72c96f0-d903-3fef-8616-500382bb4888",
                    "sense_definition": "Een minnaar of geliefde, vaak in een buitenechtelijke relatie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "lover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niemand wist dat zij een geheime <w>aanhouder</w> had.",
                            "target_lang_translations": {
                                "en": "No one knew she had a secret <w>paramour</w>.",
                                "ru": "Никто не знал, что у нее был тайный <w>любовник</w>."
                            }
                        },
                        {
                            "text": "De vrouw en haar <w>aanhouder</w> ontmoetten elkaar stiekem in het park.",
                            "target_lang_translations": {
                                "en": "The woman and her <w>lover</w> met secretly in the park.",
                                "ru": "Женщина и её <w>любовник</w> тайно встречались в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "minnaar"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A lover or beloved, often in an extramarital relationship.",
                        "ru": "Любовник или ухажер (устаревшее значение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "paramour",
                                "target_lang_sense_clarification": "an illicit lover (dated)"
                            },
                            {
                                "target_lang_word": "lover",
                                "target_lang_sense_clarification": "romantic partner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "любовник",
                                "target_lang_sense_clarification": "мужчина, с которым женщина состоит в любовной связи"
                            },
                            {
                                "target_lang_word": "ухажёр",
                                "target_lang_sense_clarification": "поклонник"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
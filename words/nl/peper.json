            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cf5b331-0cff-359f-909d-58f516fd12a9",
                    "sense_definition": "Een specerij met een scherpe smaak, gemaakt van de gemalen, gedroogde bessen van de peperplant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik de <w>peper</w> en het zout even?",
                            "target_lang_translations": {
                                "ru": "Можно мне <w>перец</w> и соль?"
                            }
                        },
                        {
                            "text": "Hij doet altijd veel te veel <w>peper</w> op zijn eten.",
                            "target_lang_translations": {
                                "ru": "Он всегда кладет слишком много <w>перца</w> в еду."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwarte peper"
                    ],
                    "common_phrases": [
                        "zout en peper",
                        "gemalen peper",
                        "witte peper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специя с острым вкусом, приготовленная из молотых сушеных ягод перечного растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перец",
                                "target_lang_sense_clarification": "специя (черный или белый)"
                            },
                            {
                                "target_lang_word": "черный перец",
                                "target_lang_sense_clarification": "конкретный вид специи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0501eef7-93b5-3016-90f6-7462b80b6481",
                    "sense_definition": "Een vrucht met een hete smaak, vaak rood of groen, die als groente of specerij wordt gebruikt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vegetable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Snijd de <w>peper</w> in kleine stukjes voor de saus.",
                            "target_lang_translations": {
                                "ru": "Нарежьте <w>перец</w> мелкими кусочками для соуса."
                            }
                        },
                        {
                            "text": "Pas op, want deze rode <w>peper</w> is erg heet.",
                            "target_lang_translations": {
                                "ru": "Осторожно, этот красный <w>перец</w> очень острый."
                            }
                        }
                    ],
                    "synonyms": [
                        "chilipeper",
                        "Spaanse peper"
                    ],
                    "common_phrases": [
                        "rode peper",
                        "hete peper",
                        "verse peper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плод с острым вкусом, часто красный или зеленый, используемый как овощ или специя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перец",
                                "target_lang_sense_clarification": "овощ/стручок"
                            },
                            {

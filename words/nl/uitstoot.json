    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d2b0a3c-9258-366e-bef4-adc6542b0330",
                    "sense_definition": "Het vrijkomen of de lozing van gassen, deeltjes of straling in de lucht of het milieu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid wil de <w>uitstoot</w> van broeikasgassen drastisch verminderen.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет радикально сократить <w>выбросы</w> парниковых газов."
                            }
                        },
                        {
                            "text": "Oude auto's veroorzaken vaak meer <w>uitstoot</w> dan nieuwe modellen.",
                            "target_lang_translations": {
                                "ru": "Старые автомобили часто производят больше <w>выбросов</w>, чем новые модели."
                            }
                        }
                    ],
                    "synonyms": [
                        "emissie"
                    ],
                    "common_phrases": [
                        "CO2-uitstoot",
                        "uitstoot verminderen",
                        "uitstoot van stikstof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выброс или выделение газов, частиц или излучения в воздух или окружающую среду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выброс",
                                "target_lang_sense_clarification": "газов, загрязняющих веществ"
                            },
                            {
                                "target_lang_word": "эмиссия",
                                "target_lang_sense_clarification": "технический или финансовый термин"
                            },
                            {
                                "target_lang_word": "выделение",
                                "target_lang_sense_clarification": "веществ в окружающую среду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1551504d-0a54-379b-ad32-567d5579f9f8",
                    "sense_definition": "Het vertrek van werknemers uit een bedrijf of organisatie, vaak zonder dat deze vervangen worden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "personnel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door natuurlijke <w>uitstoot</w> is het personeelsbestand dit jaar gekrompen.",
                            "target_lang_translations": {
                                "ru": "Из-за естественной <w>убыли</w> штат сотрудников в этом году сократился."
                            }
                        },
                        {
                            "text": "Het bedrijf hoopt gedwongen ontslagen te voorkomen door in te zetten op <w>uitstoot</w> via pensionering.",
                            "target_lang_translations": {
                                "ru": "Компания надеется избежать принудительных увольнений, делая ставку на <w>уход</w> сотрудников на пенсию."
                            }
                        }
                    ],
                    "synonyms": [
                        "emissie"
                    ],
                    "common_phrases": [
                        "natuurlijke uitstoot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уход сотрудников из компании или организации, часто без их замены (естественная убыль)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убыль",
                                "target_lang_sense_clarification": "персонала, естественная"
                            },
                            {
                                "target_lang_word": "отток",
                                "target_lang_sense_clarification": "кадров"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3479b29e-40a9-38de-928a-61b063ee31d4",
                    "sense_definition": "Vervoegde vorm van <w>uitstoten</w>: eerste, tweede en derde persoon enkelvoud in een bijzin.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is belangrijk dat de fabriek minder rook <w>uitstoot</w>.",
                            "target_lang_translations": {
                                "ru": "Важно, чтобы завод <w>выбрасывал</w> меньше дыма."
                            }
                        },
                        {
                            "text": "Ik wacht tot de vulkaan weer as <w>uitstoot</w>.",
                            "target_lang_translations": {
                                "ru": "Я жду, когда вулкан снова <w>извергнет</w> пепел."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dependent clause singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спрягаемая форма глагола uitstoten (выбрасывать, извергать): первое, второе и третье лицо единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбрасывать",
                                "target_lang_sense_clarification": "дым, газ, мусор"
                            },
                            {
                                "target_lang_word": "извергать",
                                "target_lang_sense_clarification": "о вулкане, лаве"
                            },
                            {

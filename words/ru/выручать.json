                        "оставлять"
                    ],
                    "common_phrases": [
                        "выручать из плена",
                        "выручать вещи"
                    ],
                    "target_lang_definitions": {
                        "en": "To get back something that was lost, captured, or pawned."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "redeem",

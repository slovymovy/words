{
    "word_family": [
        "hels",
        "helling",
        "hellen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c85a6d57-e735-39ef-a362-fbbad18075bf",
                    "sense_definition": "De plaats of toestand van eeuwige straf en afwezigheid van God, waar de zielen van zondaars zich na de dood bevinden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "afterlife_punishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In veel religies wordt de <w>hel</w> beschreven als een plek van vuur.",
                            "target_lang_translations": {
                                "ru": "Во многих религиях <w>ад</w> описывается как место огня."
                            }
                        },
                        {
                            "text": "Ik hoop dat je voor je daden zult branden in de <w>hel</w>.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, ты будешь гореть в <w>аду</w> за свои поступки."
                            }
                        }
                    ],
                    "antonyms": [
                        "hemel"
                    ],
                    "common_phrases": [
                        "naar de hel",
                        "loop naar de hel",
                        "branden in de hel"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in curses or expressions of anger"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или состояние вечного наказания и отсутствия Бога, где после смерти находятся души грешников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ад",
                                "target_lang_sense_clarification": "религиозное понятие"
                            },
                            {
                                "target_lang_word": "преисподняя",
                                "target_lang_sense_clarification": "место мучений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0b8114b-096b-3b03-a935-731e7542913f",
                    "sense_definition": "Een zeer onaangename, pijnlijke of verschrikkelijke situatie, plaats of ervaring.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "figurative_suffering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij maakte haar leven tot een <w>hel</w> door zijn gedrag.",
                            "target_lang_translations": {
                                "ru": "Своим поведением он превратил её жизнь в <w>ад</w>."
                            }
                        },
                        {
                            "text": "De reis naar huis door de sneeuwstorm was een ware <w>hel</w>.",
                            "target_lang_translations": {
                                "ru": "Поездка домой сквозь метель была настоящим <w>адом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een hel op aarde",
                        "een hel van een"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень неприятная, болезненная или ужасная ситуация, место или опыт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ад",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "кошмар",
                                "target_lang_sense_clarification": "ужасная ситуация"
                            },
                            {
                                "target_lang_word": "мука",
                                "target_lang_sense_clarification": "страдание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd1f6e89-bdf5-35c1-8474-e99c7c027f69",
                    "sense_definition": "Het dodenrijk of de onderwereld waar de zielen van overledenen verblijven (vaak in mythologische of theologische context).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "underworld",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de oude mythen daalde de held af in de <w>hel</w> om een ziel te redden.",
                            "target_lang_translations": {
                                "ru": "В древних мифах герой спускался в <w>преисподнюю</w>, чтобы спасти душу."
                            }
                        },
                        {
                            "text": "Hij is nedergedaald ter <w>helle</w> en de derde dag verrezen.",
                            "target_lang_translations": {
                                "ru": "Он сошел в <w>ад</w> и на третий день воскрес."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodenrijk",
                        "sjeool",
                        "Hades"
                    ],
                    "common_phrases": [
                        "ter helle"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often refers to Sheol or Hades in older texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Царство мертвых или подземный мир, где обитают души умерших (часто в мифологическом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преисподняя",
                                "target_lang_sense_clarification": "мифология/архаичное"
                            },
                            {
                                "target_lang_word": "царство мертвых",
                                "target_lang_sense_clarification": "загробный мир"
                            },
                            {
                                "target_lang_word": "ад",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7ff76b93-86be-39e7-b3b9-be938f0f7e83",
                    "sense_definition": "(van licht of kleur) Zeer fel, intens en zuiver; veel licht uitstralend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bright_sensory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>helle</w> licht van de zon deed pijn aan mijn ogen.",
                            "target_lang_translations": {
                                "ru": "<w>Яркий</w> солнечный свет причинял боль моим глазам."
                            }
                        },
                        {
                            "text": "Ze droeg een <w>hel</w> rode jurk die iedereen opviel.",
                            "target_lang_translations": {
                                "ru": "На ней было <w>ярко</w>-красное платье, которое бросалось всем в глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "helder"
                    ],
                    "common_phrases": [
                        "hel verlicht",
                        "helblauw",
                        "helwit"
                    ],
                    "target_lang_definitions": {
                        "ru": "(о свете или цвете) Очень яркий, интенсивный и чистый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яркий",
                                "target_lang_sense_clarification": "о свете или цвете"
                            },
                            {
                                "target_lang_word": "светлый",
                                "target_lang_sense_clarification": "о цвете"
                            },
                            {
                                "target_lang_word": "ослепительный",
                                "target_lang_sense_clarification": "очень яркий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9fb1358-0721-33f1-9c3e-a59af499a2ad",
                    "sense_definition": "(van geluid) Hoog, scherp en doordringend van toon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_sensory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Plotseling klonk er een <w>helle</w> kreet uit het bos.",
                            "target_lang_translations": {
                                "ru": "Внезапно из леса раздался <w>пронзительный</w> крик."
                            }
                        },
                        {
                            "text": "De klokken maakten een <w>hel</w> geluid dat ver te horen was.",
                            "target_lang_translations": {
                                "ru": "Колокола издавали <w>звонкий</w> звук, который был слышен издалека."
                            }
                        }
                    ],
                    "synonyms": [
                        "helder"
                    ],
                    "target_lang_definitions": {
                        "ru": "(о звуке) Высокий, резкий и пронзительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонкий",
                                "target_lang_sense_clarification": "о голосе или звуке"
                            },
                            {
                                "target_lang_word": "пронзительный",
                                "target_lang_sense_clarification": "резкий звук"
                            },
                            {
                                "target_lang_word": "резкий",
                                "target_lang_sense_clarification": "о звуке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd21a48e-d821-3c55-810e-504e167a8e81",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>hellen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hel</w> iets naar voren om het beter te kunnen zien.",
                            "target_lang_translations": {
                                "ru": "Я немного <w>наклоняюсь</w> вперед, чтобы лучше видеть."
                            }
                        },
                        {
                            "text": "Als ik te ver over de rand <w>hel</w>, verlies ik mijn evenwicht.",
                            "target_lang_translations": {
                                "ru": "Если я слишком сильно <w>перегнусь</w> через край, я потеряю равновесие."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "1-е лицо единственного числа настоящего времени от глагола 'hellen' (наклоняться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наклоняюсь",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "склоняюсь",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcf89ce9-6d5d-3345-bfc0-c51c270f0034",
                    "sense_definition": "Gebiedende wijs van <w>hellen</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hel</w> niet te ver over de reling, dat is gevaarlijk!",
                            "target_lang_translations": {
                                "ru": "Не <w>перегибайся</w> слишком сильно через перила, это опасно!"
                            }
                        },
                        {
                            "text": "<w>Hel</w> een beetje naar links zodat de foto recht is.",
                            "target_lang_translations": {
                                "ru": "<w>Наклонись</w> немного влево, чтобы фотография получилась ровной."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение от глагола 'hellen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наклонись",
                                "target_lang_sense_clarification": "приказ/просьба"
                            },
                            {
                                "target_lang_word": "не перегибайся",
                                "target_lang_sense_clarification": "в отрицании (не падай)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
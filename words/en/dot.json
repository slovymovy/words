                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mark_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tiny cottages <w>dot</w> the green landscape.",
                            "target_lang_translations": {
                                "ru": "Крошечные коттеджи <w>усеивают</w> зеленый пейзаж."
                            }

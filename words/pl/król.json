    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ba9adf6-0c54-34db-9aaf-85fe6810ff2c",
                    "sense_definition": "Mężczyzna sprawujący najwyższą władzę w państwie o ustroju monarchicznym; władca koronowany.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "monarch_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach <król> decydował o losach całego narodu.",
                            "target_lang_translations": {
                                "ru": "В давние времена <король> решал судьбу всего народа."
                            }
                        },
                        {
                            "text": "Nasz <król> panował sprawiedliwie przez wiele długich lat.",
                            "target_lang_translations": {
                                "ru": "Наш <король> правил справедливо долгие годы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wałek królewski",
                        "potentat"
                    ],
                    "common_phrases": [
                        "król Polski",
                        "Trzech Króli",
                        "żyć jak król",
                        "dwór króla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, осуществляющий верховную власть в монархическом государстве; коронованный правитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "король",
                                "target_lang_sense_clarification": "титул монарха"
                            },
                            {
                                "target_lang_word": "монарх",
                                "target_lang_sense_clarification": "правитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61a48b7b-2383-3dc4-ae7c-63cb4d76a667",
                    "sense_definition": "Karta do gry z wizerunkiem monarchy, w hierarchii starsza od damy, a młodsza od asa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "games_pieces",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam w ręku mocne karty, w tym jednego <króla>.",
                            "target_lang_translations": {
                                "ru": "У меня на руках сильные карты, в том числе один <король>."
                            }
                        },
                        {
                            "text": "<Król> trefl przebija damę tego samego koloru.",
                            "target_lang_translations": {
                                "ru": "<Король> треф бьёт даму той же масти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "król pik",
                        "król kier",
                        "król trefl",
                        "król karo",
                        "cztery króle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игральная карта с изображением монарха, по старшинству выше дамы, но ниже туза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "король",
                                "target_lang_sense_clarification": "игральная карта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5336c0d-1c31-319f-a440-7321331b3360",
                    "sense_definition": "Najważniejsza figura w szachach, której zamatowanie oznacza koniec partii.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "games_pieces",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz chronić swojego <króla>, aby nie przegrać partii.",
                            "target_lang_translations": {
                                "ru": "Ты должен защищать своего <короля>, чтобы не проиграть партию."
                            }
                        },
                        {
                            "text": "Biały <król> przesunął się o jedno pole w lewo.",
                            "target_lang_translations": {
                                "ru": "Белый <король> сдвинулся на одно поле влево."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szach i mat",
                        "ruch królem",
                        "szach królowi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая важная фигура в шахматах, мат которой означает конец партии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "король",
                                "target_lang_sense_clarification": "шахматная фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "859bb4e5-f51b-3ea0-afaf-be590157ff4d",
                    "sense_definition": "Osoba, która jest najlepsza, najbardziej wpływowa lub najbogatsza w jakiejś dziedzinie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "monarch_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elvis Presley jest powszechnie nazywany <królem> rock and rolla.",
                            "target_lang_translations": {
                                "ru": "Элвиса Пресли повсеместно называют <королём> рок-н-ролла."
                            }
                        },
                        {
                            "text": "Ten piłkarz został <królem> strzelców w ostatnich mistrzostwach.",
                            "target_lang_translations": {
                                "ru": "Этот футболист стал <лучшим бомбардиром> на прошлом чемпионате."
                            }
                        }
                    ],
                    "synonyms": [
                        "potentat"
                    ],
                    "common_phrases": [
                        "król strzelców",
                        "król życia",
                        "król parkietu",
                        "król zwierząt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который является лучшим, самым влиятельным или богатым в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "король",
                                "target_lang_sense_clarification": "лучший в своём деле"
                            },
                            {
                                "target_lang_word": "царь",

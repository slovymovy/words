                {
                    "sense_id": "13aa7acb-4083-39d6-8718-db8f10b0d0cd",
                    "sense_definition": "Een financiële uitkering van de overheid voor mensen die geen of onvoldoende inkomen hebben om van te leven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_welfare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moet elke maand van de <w>bijstand</w> zien rond te komen.",
                            "target_lang_translations": {
                                "en": "He has to make ends meet on <w>welfare</w> every month."
                            }
                        },
                        {
                            "text": "Als je je baan verliest en geen recht meer hebt op WW, kom je in de <w>bijstand</w>.",
                            "target_lang_translations": {
                                "en": "If you lose your job and are no longer entitled to unemployment benefits, you end up on <w>social assistance</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitkering",
                        "steun"
                    ],
                    "common_phrases": [
                        "in de bijstand",
                        "bijstand aanvragen",
                        "bijstand ontvangen"
                    ],
                    "target_lang_definitions": {
                        "en": "A financial benefit from the government for people who have no or insufficient income to live on."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "welfare",
                                "target_lang_sense_clarification": "government financial support"
                            },
                            {
                                "target_lang_word": "social assistance",
                                "target_lang_sense_clarification": "formal term for government benefits"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eabc2bce-8dfc-3cca-ab8c-f99ae44c433d",
                    "sense_definition": "Hulp of ondersteuning die aan iemand wordt geboden, vaak in een officiële, juridische of medische context.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_help",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met <w>bijstand</w> van het buurland wist men de vijand het hoofd te bieden.",
                            "target_lang_translations": {
                                "en": "With the <w>aid</w> of the neighboring country, they managed to withstand the enemy."
                            }
                        },
                        {
                            "text": "De verdachte heeft tijdens het verhoor recht op juridische <w>bijstand</w>.",
                            "target_lang_translations": {
                                "en": "The suspect has the right to legal <w>assistance</w> during the interrogation."
                            }
                        }
                    ],

{
    "word_family": [
        "pozwalać",
        "pozwolenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "22d14dca-0dc7-3063-9c9c-d733fba3ebbb",
                    "sense_definition": "Wyrazić zgodę na to, by ktoś coś zrobił, lub nie czynić przeszkód w czyimś działaniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice w końcu <w>pozwolili</w> mi pójść na ten koncert z przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Родители наконец <w>разрешили</w> мне пойти на этот концерт с друзьями."
                            }
                        },
                        {
                            "text": "Nauczyciel nie <w>pozwolił</w> uczniom korzystać z telefonów podczas egzaminu.",
                            "target_lang_translations": {
                                "ru": "Учитель не <w>разрешил</w> ученикам пользоваться телефонами во время экзамена."
                            }
                        }
                    ],
                    "synonyms": [
                        "zezwolić",
                        "dozwolić"
                    ],
                    "antonyms": [
                        "zabronić",
                        "zakazać"
                    ],
                    "common_phrases": [
                        "pozwolić na wyjazd",
                        "pozwolić dzieciom",
                        "pozwolić odejść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать согласие на совершение чего-либо или не препятствовать действиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрешить",
                                "target_lang_sense_clarification": "дать согласие"
                            },
                            {
                                "target_lang_word": "позволить",
                                "target_lang_sense_clarification": "не препятствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbb00074-e977-3552-9750-ad12ca50c9b9",
                    "sense_definition": "Sprawić, że coś staje się możliwe do wykonania; stworzyć warunki do zaistnienia czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe okna <w>pozwoliły</w> znacznie ograniczyć hałas dobiegający z ruchliwej ulicy.",
                            "target_lang_translations": {
                                "ru": "Новые окна <w>позволили</w> значительно снизить шум с оживленной улицы."
                            }
                        },
                        {
                            "text": "Ta wygrana <w>pozwoli</w> naszej drużynie awansować do wyższej ligi rozgrywek.",
                            "target_lang_translations": {
                                "ru": "Эта победа <w>позволит</w> нашей команде выйти в высшую лигу."
                            }
                        }
                    ],
                    "synonyms": [
                        "umożliwić"
                    ],
                    "common_phrases": [
                        "pozwolić na rozwój",
                        "pozwolić zrozumieć",
                        "pozwolić uniknąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать возможным выполнение чего-либо; создать условия для чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволить",
                                "target_lang_sense_clarification": "сделать возможным"
                            },
                            {
                                "target_lang_word": "дать возможность",
                                "target_lang_sense_clarification": "предоставить условия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44c44bc4-5474-36aa-b64f-1e8f1de09581",
                    "sense_definition": "Zdecydować się na zrobienie czegoś przyjemnego, kosztownego lub ryzykownego (używane z zaimkiem „sobie”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ten weekend wreszcie <w>pozwolę</w> sobie na odrobinę luksusu i odpoczynku w spa.",
                            "target_lang_translations": {
                                "ru": "В эти выходные я наконец <w>позволю</w> себе немного роскоши и отдыха в спа."
                            }
                        },
                        {
                            "text": "Niestety nie możemy <w>pozwolić</w> sobie na tak duży wydatek w tym miesiącu.",
                            "target_lang_translations": {
                                "ru": "К сожалению, мы не можем <w>позволить</w> себе такие большие расходы в этом месяце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pozwolić sobie na coś",
                        "nie móc sobie pozwolić",
                        "pozwolić sobie na żart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решиться на что-то приятное или затратное (обычно с возвратным местоимением «себе»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволить себе",
                                "target_lang_sense_clarification": "иметь финансовую или иную возможность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ade4e236-332d-355f-a284-af1a10e8f1f7",
                    "sense_definition": "Używane jako zwrot grzecznościowy przy proponowaniu czegoś, wchodzeniu gdzieś lub rozpoczynaniu wypowiedzi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Pozwoli</w> pan, że przedstawię mojego nowego asystenta, który będzie prowadził spotkanie.",
                            "target_lang_translations": {
                                "ru": "<w>Позвольте</w> представить моего нового ассистента, который будет вести встречу."
                            }
                        },
                        {
                            "text": "Czy <w>pozwolą</w> państwo, że zajmę to wolne miejsce przy stole?",
                            "target_lang_translations": {
                                "ru": "<w>Позволите</w> ли вы мне занять это свободное место за столом?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "pozwoli pan",
                        "pozwolą państwo",
                        "pozwoli pani"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zwrot grzecznościowy (polite)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как вежливый оборот при предложении чего-либо или начале речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволить",
                                "target_lang_sense_clarification": "вежливая форма"
                            },
                            {
                                "target_lang_word": "разрешить",
                                "target_lang_sense_clarification": "вежливая форма"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
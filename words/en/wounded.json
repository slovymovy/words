    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "50453389-b833-3dab-a5ae-06d2500f85db",
                    "sense_definition": "Suffering from a physical injury, especially one caused by a weapon or accident.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>wounded</w> soldier was carried off the battlefield by his comrades.",
                            "target_lang_translations": {
                                "ru": "<w>Раненого</w> солдата товарищи вынесли с поля боя."
                            }
                        },
                        {
                            "text": "Doctors worked through the night to treat the <w>wounded</w> civilians.",
                            "target_lang_translations": {
                                "ru": "Врачи работали всю ночь, чтобы оказать помощь <w>раненым</w> мирным жителям."
                            }
                        }
                    ],
                    "synonyms": [
                        "injured",
                        "hurt"
                    ],
                    "antonyms": [
                        "uninjured",
                        "healthy",
                        "intact",
                        "unwounded"
                    ],
                    "common_phrases": [
                        "wounded soldier",
                        "seriously wounded",
                        "walking wounded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдающий от физической травмы, особенно полученной от оружия или в результате несчастного случая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раненый",
                                "target_lang_sense_clarification": "физически пострадавший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b791fd6-1664-3e46-abf6-cb3ada4a7803",
                    "sense_definition": "Suffering from emotional pain or offended feelings.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat quietly in the corner, nursing his <w>wounded</w> pride.",
                            "target_lang_translations": {
                                "ru": "Он тихо сидел в углу, лелея свое <w>уязвлённое</w> самолюбие."
                            }
                        },
                        {
                            "text": "She gave him a <w>wounded</w> look after he criticized her work.",
                            "target_lang_translations": {
                                "ru": "Она бросила на него <w>обиженный</w> взгляд после того, как он раскритиковал ее работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurt",
                        "damaged",
                        "traumatised"
                    ],
                    "common_phrases": [
                        "wounded pride",
                        "wounded feelings",
                        "deeply wounded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий душевную боль или чувство обиды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уязвлённый",
                                "target_lang_sense_clarification": "о гордости или самолюбии"
                            },
                            {
                                "target_lang_word": "обиженный",
                                "target_lang_sense_clarification": "о чувствах или взгляде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c111cc06-17fd-3473-aad5-3052c70d2e48",
                    "sense_definition": "Of a particle: having undergone an inelastic collision.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The physicist analyzed the trajectory of the <w>wounded</w> nucleon.",
                            "target_lang_translations": {
                                "ru": "Физик проанализировал траекторию <w>раненого</w> нуклона."
                            }
                        },
                        {
                            "text": "Energy loss was observed in the <w>wounded</w> particle system.",
                            "target_lang_translations": {
                                "ru": "Потеря энергии наблюдалась в системе <w>раненых</w> частиц."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О частице: претерпевшая неупругое столкновение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раненый",
                                "target_lang_sense_clarification": "термин физики частиц"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c417f0dc-4f76-37f0-a4ab-b650f14bbca0",

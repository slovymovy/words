    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "266de497-97f7-302e-b1fd-3a6354e27b7b",
                    "sense_definition": "Оценка ситуации как нормальной, приемлемой или выражение согласия (в разговорной речи); иногда используется как требование прекратить что-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как у тебя дела на новой работе? — <Норма>, справляюсь.",
                            "target_lang_translations": {
                                "en": "How are things at your new job? — <Fine>, I'm managing."
                            }
                        },
                        {
                            "text": "Всё, <норма>! Больше не буду так делать.",
                            "target_lang_translations": {
                                "en": "It's all <fine>! I won't do it again."
                            }
                        }
                    ],
                    "synonyms": [
                        "хватит",
                        "довольно"
                    ],
                    "common_phrases": [
                        "всё норма"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Assessment of a situation as normal, acceptable, or an expression of agreement (in spoken language); sometimes used as a demand to stop something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "satisfactory state or agreement"
                            },
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "acceptable state"
                            },
                            {
                                "target_lang_word": "alright",
                                "target_lang_sense_clarification": "colloquial agreement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5da554a8-2259-3340-8d50-bd8aa745e491",
                    "sense_definition": "Установленное правило, образец или общепринятый порядок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом обществе действуют строгие моральные <нормы>.",
                            "target_lang_translations": {
                                "en": "Strict moral <norms> operate in this society."
                            }
                        },
                        {
                            "text": "Студенты должны соблюдать <нормы> литературного языка.",
                            "target_lang_translations": {
                                "en": "Students must observe the <norms> of the literary language."
                            }
                        }
                    ],
                    "synonyms": [
                        "правило"
                    ],
                    "antonyms": [
                        "нарушение"
                    ],
                    "common_phrases": [
                        "социальные нормы",
                        "нормы права",
                        "нормы поведения",
                        "в пределах нормы"
                    ],
                    "target_lang_definitions": {
                        "en": "An established rule, pattern, or generally accepted order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "norm",
                                "target_lang_sense_clarification": "social or moral rule"
                            },
                            {
                                "target_lang_word": "standard",
                                "target_lang_sense_clarification": "accepted level of quality or behavior"
                            },
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "regulation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53f5977f-73e8-3428-b0bc-23ca15466465",
                    "sense_definition": "Обычное, естественное состояние или положение вещей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для него опоздания на работу стали <нормой>.",
                            "target_lang_translations": {
                                "en": "For him, being late to work has become the <norm>."
                            }
                        },
                        {
                            "text": "Температура тридцать градусов — это <норма> для июля в этом регионе.",
                            "target_lang_translations": {
                                "en": "A temperature of thirty degrees is the <norm> for July in this region."
                            }
                        }
                    ],
                    "antonyms": [
                        "экстраординарность",
                        "эксцесс"
                    ],
                    "common_phrases": [
                        "войти в норму",
                        "прийти в норму",
                        "считать нормой"
                    ],
                    "target_lang_definitions": {
                        "en": "A usual, natural state or state of things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "norm",
                                "target_lang_sense_clarification": "typical situation"
                            },
                            {
                                "target_lang_word": "normal",
                                "target_lang_sense_clarification": "state of being usual"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c28fbe05-c9a9-3612-80f8-00b8a7e0ffe3",
                    "sense_definition": "Установленная мера, средняя величина или обязательное количество чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочий выполнил свою дневную <норму> уже к обеду.",
                            "target_lang_translations": {
                                "en": "The worker fulfilled his daily <quota> by lunchtime."
                            }
                        },
                        {
                            "text": "Врачи рекомендуют соблюдать суточную <норму> потребления воды.",
                            "target_lang_translations": {
                                "en": "Doctors recommend observing the daily <intake> of water."
                            }
                        }
                    ],
                    "synonyms": [
                        "квота"
                    ],
                    "common_phrases": [
                        "дневная норма",
                        "выполнять норму",
                        "сверх нормы"
                    ],
                    "target_lang_definitions": {
                        "en": "An established measure, average value, or mandatory quantity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quota",
                                "target_lang_sense_clarification": "production target or fixed share"
                            },
                            {
                                "target_lang_word": "allowance",
                                "target_lang_sense_clarification": "permitted amount"
                            },
                            {
                                "target_lang_word": "norm",
                                "target_lang_sense_clarification": "standard amount or rate"
                            }
                        ]
                    }
                }
            ]
        },

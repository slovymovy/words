{
    "word_family": [
        "knieval"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed5da940-438e-3682-b70e-abf4302cd2b2",
                    "sense_definition": "Het op de knieën vallen als teken van onderwerping of diep respect, vaak figuurlijk gebruikt voor het toegeven aan eisen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "submission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister maakte een onverwachte <w>voetval</w> voor de oppositie om zijn plannen te redden.",
                            "target_lang_translations": {
                                "ru": "Министр неожиданно <w>пошел на поклон</w> к оппозиции, чтобы спасти свои планы."
                            }
                        },
                        {
                            "text": "In vroegere tijden was een diepe <w>voetval</w> verplicht bij het benaderen van de vorst.",
                            "target_lang_translations": {
                                "ru": "В прежние времена глубокий <w>земной поклон</w> был обязателен при приближении к монарху."
                            }
                        }
                    ],
                    "synonyms": [
                        "knieval",
                        "onderwerping"
                    ],
                    "common_phrases": [
                        "een voetval doen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Падение на колени в знак покорности или глубокого уважения, часто используется в переносном смысле как уступка требованиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земной поклон",
                                "target_lang_sense_clarification": "физическое действие"
                            },
                            {
                                "target_lang_word": "уступка",
                                "target_lang_sense_clarification": "переносное значение, компромисс"
                            },
                            {
                                "target_lang_word": "коленопреклонение",
                                "target_lang_sense_clarification": "акт выражения почтения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ca8d0b2-abf2-3a4e-8962-b7618425ea85",
                    "sense_definition": "Een klein kapelletje of halteplaats langs een weg, vaak als onderdeel van een kruisweg, waar men kan bidden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de wandeling kwamen we langs een oude <w>voetval</w> die onlangs gerestaureerd is.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки мы прошли мимо старой <w>часовни</w>, которую недавно отреставрировали."
                            }
                        },
                        {
                            "text": "De gelovigen stopten bij elke <w>voetval</w> om een kort gebed uit te spreken.",
                            "target_lang_translations": {
                                "ru": "Верующие останавливались у каждой <w>часовни</w>, чтобы произнести короткую молитву."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapelletje",
                        "kruiswegstatie"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in southern Netherlands and Belgium"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая часовня или место остановки у дороги, часто как часть крестного пути, где можно помолиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часовня",
                                "target_lang_sense_clarification": "маленькая церковь"
                            },
                            {
                                "target_lang_word": "придорожная молельня",
                                "target_lang_sense_clarification": "место для молитвы у дороги"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "ff8651dc-be13-391f-8e80-b3cd3dce3cb9",
                    "sense_definition": "tranen vergietend als uiting van verdriet, pijn of onmacht",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "huilend_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>huilend</w> kind verstoorde het concert.",
                            "target_lang_translations": {
                                "en": "A <w>crying</w> child disturbed the concert.",
                                "ru": "<w>Плачущий</w> ребенок помешал концерту."
                            }
                        },
                        {
                            "text": "Het <w>huilende</w> meisje werd door haar moeder getroost.",
                            "target_lang_translations": {
                                "en": "The <w>crying</w> girl was comforted by her mother.",
                                "ru": "Мама утешала <w>плачущую</w> девочку."
                            }
                        }
                    ],
                    "synonyms": [
                        "wenend"
                    ],
                    "antonyms": [
                        "lachend"
                    ],
                    "common_phrases": [
                        "een huilend kind",
                        "huilend wakker worden"
                    ],
                    "target_lang_definitions": {
                        "en": "shedding tears as an expression of sadness, pain, or helplessness",
                        "ru": "Проливающий слезы как выражение горя, боли или бессилия"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crying",
                                "target_lang_sense_clarification": "shedding tears"
                            },
                            {
                                "target_lang_word": "weeping",
                                "target_lang_sense_clarification": "shedding tears, often more intensely or formally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "плачущий",
                                "target_lang_sense_clarification": "прилагательное/причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7bc6216e-839e-3497-a347-223bb25b19d8",
                    "sense_definition": "A small change made to something in order to correct or improve it.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We made a slight <w>adjustment</w> to the plan to save time.",
                            "target_lang_translations": {
                                "ru": "Мы внесли небольшую <w>корректировку</w> в план, чтобы сэкономить время."
                            }
                        },
                        {
                            "text": "The mechanic performed a minor <w>adjustment</w> on the brakes.",
                            "target_lang_translations": {
                                "ru": "Механик выполнил небольшую <w>регулировку</w> тормозов."
                            }
                        }
                    ],
                    "synonyms": [
                        "modification",
                        "change",
                        "correction"
                    ],
                    "common_phrases": [
                        "make an adjustment",
                        "minor adjustment",
                        "slight adjustment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое изменение, вносимое для исправления или улучшения чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регулировка",
                                "target_lang_sense_clarification": "механическая или техническая"
                            },
                            {
                                "target_lang_word": "корректировка",
                                "target_lang_sense_clarification": "планов, документов или абстрактных понятий"
                            },
                            {
                                "target_lang_word": "настройка",
                                "target_lang_sense_clarification": "параметров или оборудования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72b96724-6bfa-3fd7-94ed-2f7bc2ab3bb9",
                    "sense_definition": "The process of adapting or becoming used to a new situation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moving to a different country requires a significant cultural <w>adjustment</w>.",
                            "target_lang_translations": {
                                "ru": "Переезд в другую страну требует значительной культурной <w>адаптации</w>."
                            }
                        },
                        {
                            "text": "It was a difficult <w>adjustment</w> for the students to switch to online learning.",
                            "target_lang_translations": {
                                "ru": "Студентам было сложно пройти <w>адаптацию</w> при переходе на онлайн-обучение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "period of adjustment",
                        "adjustment to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс адаптации или привыкания к новой ситуации"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адаптация",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "привыкание",
                                "target_lang_sense_clarification": "психологический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "378c90ed-38f2-3677-a043-1cfd1592fdc2",
                    "sense_definition": "The settling of a financial account or the determination of an insurance claim amount.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bank made an <w>adjustment</w> to my account to refund the fee.",
                            "target_lang_translations": {
                                "ru": "Банк сделал <w>корректировку</w> моего счета, чтобы вернуть комиссию."
                            }
                        },
                        {
                            "text": "The insurance company completed the <w>adjustment</w> of the loss quickly.",
                            "target_lang_translations": {
                                "ru": "Страховая компания быстро завершила <w>урегулирование</w> убытка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "loss adjustment",
                        "account adjustment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Урегулирование финансовых вопросов или определение суммы страховой выплаты"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "урегулирование",
                                "target_lang_sense_clarification": "претензий или убытков"
                            },
                            {
                                "target_lang_word": "корректировка",
                                "target_lang_sense_clarification": "суммы на счете"
                            }
                        ]
                    }
                }
            ]
        }
    ]

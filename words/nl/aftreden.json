            "pos": "verb",
            "senses": [
                {
                    "sense_id": "271092be-58ee-34b5-8ac5-b4cda18956f4",
                    "sense_definition": "Een officiële functie of ambt neerleggen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "resign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister moest gedwongen <aftreden> na het grote schandaal.",
                            "target_lang_translations": {
                                "en": "The minister was forced to <resign> after the major scandal.",
                                "ru": "Министр был вынужден <уйти в отставку> после крупного скандала."
                            }
                        },
                        {
                            "text": "Hij is gisteren onverwacht <afgetreden> als voorzitter van de vereniging.",
                            "target_lang_translations": {
                                "en": "He unexpectedly <stepped down> as chairman of the association yesterday.",
                                "ru": "Вчера он неожиданно <ушел с поста> председателя ассоциации."
                            }
                        }
                    ],
                    "synonyms": [
                        "opstappen",
                        "terugtreden",
                        "ontslag nemen",
                        "abdiceren"
                    ],
                    "antonyms": [
                        "aantreden",
                        "in dienst treden"
                    ],
                    "common_phrases": [
                        "per direct aftreden",
                        "gedwongen aftreden",
                        "vrijwillig aftreden"
                    ],
                    "target_lang_definitions": {
                        "en": "To give up an official position or office.",
                        "ru": "Оставить официальную должность или пост."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resign",
                                "target_lang_sense_clarification": "to leave a job or position voluntarily or involuntarily"
                            },
                            {
                                "target_lang_word": "step down",
                                "target_lang_sense_clarification": "to resign from an important position"
                            },
                            {
                                "target_lang_word": "abdicate",
                                "target_lang_sense_clarification": "specifically for a monarch giving up the throne"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уйти в отставку",
                                "target_lang_sense_clarification": "официально покинуть должность"
                            },
                            {
                                "target_lang_word": "сложить полномочия",
                                "target_lang_sense_clarification": "формальный отказ от должности"
                            },
                            {
                                "target_lang_word": "отречься",
                                "target_lang_sense_clarification": "от престола (для монархов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c63d71f6-0a47-3352-9cdd-27607be07d4f",
                    "sense_definition": "Naar beneden stappen van een verhoging, zoals een podium of trap.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "step_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zijn toespraak <trad> de spreker voorzichtig <af> van het podium.",
                            "target_lang_translations": {
                                "en": "After his speech, the speaker carefully <stepped down> from the podium.",
                                "ru": "После выступления оратор осторожно <сошел> с трибуны."
                            }
                        },
                        {
                            "text": "De koning <trad> <af> van de troon om de gasten te begroeten.",
                            "target_lang_translations": {
                                "en": "The king <stepped down> from the throne to greet the guests.",
                                "ru": "Король <сошел> с трона, чтобы поприветствовать гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "afstappen",
                        "afdalen"
                    ],
                    "antonyms": [
                        "opstappen",
                        "bestijgen"
                    ],
                    "common_phrases": [
                        "van het podium aftreden"
                    ],
                    "target_lang_definitions": {
                        "en": "To walk down from a raised surface, such as a podium or stairs.",
                        "ru": "Спуститься вниз с возвышения, например, с подиума или ступеней."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "step down",
                                "target_lang_sense_clarification": "to move down from a higher level"
                            },
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "to move from a higher to a lower place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сойти",
                                "target_lang_sense_clarification": "спуститься с чего-либо (например, со сцены)"
                            },
                            {
                                "target_lang_word": "спуститься",
                                "target_lang_sense_clarification": "переместиться вниз"
                            }
                        ]
                    }
                }
            ]
        }
    ]

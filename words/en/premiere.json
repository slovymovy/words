{
    "word_family": [
        "premier"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "952a06d5-6576-3997-83b0-c77f907c7e9a",
                    "sense_definition": "The first public performance or showing of a film, play, or other form of entertainment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_first_showing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actors walked the red carpet at the movie <w>premiere</w> in London.",
                            "target_lang_translations": {
                                "ru": "Актеры прошли по красной дорожке на <w>премьере</w> фильма в Лондоне."
                            }
                        },
                        {
                            "text": "We managed to get tickets for the world <w>premiere</w> of the new play.",
                            "target_lang_translations": {
                                "ru": "Нам удалось достать билеты на мировую <w>премьеру</w> новой пьесы."
                            }
                        }
                    ],
                    "synonyms": [
                        "debut",
                        "opening"
                    ],
                    "antonyms": [
                        "finale"
                    ],
                    "common_phrases": [
                        "world premiere",
                        "movie premiere",
                        "gala premiere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое публичное представление или показ фильма, спектакля или другого развлекательного мероприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "премьера",
                                "target_lang_sense_clarification": "первый показ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3f4b031-0667-3589-8380-ab399bd77796",
                    "sense_definition": "The first episode of a television show or a particular season of that show.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_first_showing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Millions of viewers tuned in to watch the season <w>premiere</w> last night.",
                            "target_lang_translations": {
                                "ru": "Миллионы зрителей включили телевизоры, чтобы посмотреть <w>премьеру</w> сезона вчера вечером."
                            }
                        },
                        {
                            "text": "The series <w>premiere</w> introduced the main characters and the central mystery.",
                            "target_lang_translations": {
                                "ru": "<w>Премьера</w> сериала познакомила зрителей с главными героями и главной загадкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "opener",
                        "pilot"
                    ],
                    "antonyms": [
                        "finale"
                    ],
                    "common_phrases": [
                        "season premiere",
                        "series premiere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первая серия телевизионного шоу или конкретного сезона этого шоу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "премьера",
                                "target_lang_sense_clarification": "сезона или сериала"
                            },
                            {
                                "target_lang_word": "первая серия",
                                "target_lang_sense_clarification": "эпизод"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2822fd5e-c6a1-37b8-b918-894380ec18ae",
                    "sense_definition": "To be shown, performed, or broadcast for the first time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_show_first",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The highly anticipated documentary <w>premieres</w> on television this weekend.",
                            "target_lang_translations": {
                                "ru": "Долгожданный документальный фильм <w>выходит</w> на телевидении в эти выходные."
                            }
                        },
                        {
                            "text": "Her new ballet <w>premiered</w> to a standing ovation from the audience.",
                            "target_lang_translations": {
                                "ru": "Ее новый балет <w>был представлен</w> под бурные овации зрителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "debut",
                        "open"
                    ],
                    "common_phrases": [
                        "premiere exclusively",
                        "premiere globally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть показанным, исполненным или транслируемым впервые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "о фильме, передаче"
                            },
                            {
                                "target_lang_word": "состояться",
                                "target_lang_sense_clarification": "о премьере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "171ea187-9311-3340-b1da-b23a81995e3b",
                    "sense_definition": "To present or introduce a creative work to the public for the first time.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_show_first",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The studio decided to <w>premiere</w> the trailer online before the main event.",
                            "target_lang_translations": {
                                "ru": "Студия решила <w>представить</w> трейлер онлайн до главного события."
                            }
                        },
                        {
                            "text": "They will <w>premiere</w> their new collection at the fashion show in Paris.",
                            "target_lang_translations": {
                                "ru": "Они <w>представят</w> свою новую коллекцию на показе мод в Париже."
                            }
                        }
                    ],
                    "synonyms": [
                        "launch",
                        "unveil",
                        "introduce"
                    ],
                    "common_phrases": [
                        "premiere a film",
                        "premiere a song"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представить или показать творческую работу публике впервые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "публике"
                            },
                            {
                                "target_lang_word": "показать впервые",
                                "target_lang_sense_clarification": "демонстрировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
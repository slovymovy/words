                    "sense_id": "3503f998-e574-3926-aca1-515a575f8661",
                    "sense_definition": "At a later time; following something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "time_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The book was published in 1990 and <w>subsequently</w> translated into ten languages.",
                            "target_lang_translations": {
                                "ru": "Книга была опубликована в 1990 году и <w>впоследствии</w> переведена на десять языков."
                            }
                        },
                        {
                            "text": "He was arrested by the police and <w>subsequently</w> charged with theft.",
                            "target_lang_translations": {
                                "ru": "Он был арестован полицией и <w>впоследствии</w> обвинен в краже."
                            }
                        }
                    ],
                    "synonyms": [
                        "afterwards",
                        "later",
                        "then",
                        "afterward"
                    ],
                    "antonyms": [
                        "formerly"
                    ],
                    "common_phrases": [

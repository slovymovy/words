    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f7becd17-3941-3426-896c-b4e3bef00bbf",
                    "sense_definition": "A regular beat felt in the arteries, such as at the wrist or neck, caused by the heart pumping blood.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "heartbeat_rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse checked the patient's <w>pulse</w> to ensure his heart rate was normal.",
                            "target_lang_translations": {
                                "ru": "Медсестра проверила <w>пульс</w> пациента, чтобы убедиться, что его сердцебиение в норме."
                            }
                        },
                        {
                            "text": "His <w>pulse</w> began to race as he waited nervously for the results.",
                            "target_lang_translations": {
                                "ru": "Его <w>пульс</w> начал учащаться, пока он нервно ждал результатов."
                            }
                        }
                    ],
                    "synonyms": [
                        "undulate",
                        "autosoliton",
                        "undulate"
                    ],
                    "common_phrases": [
                        "check the pulse",
                        "weak pulse",
                        "racing pulse",
                        "take a pulse"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмические толчки, ощущаемые в артериях (например, на запястье или шее), вызванные работой сердца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульс",
                                "target_lang_sense_clarification": "биение сердца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5c9dfc2-69cf-3873-bf26-0447cee10868",
                    "sense_definition": "A strong, regular beat or rhythm, especially in music or sound.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "heartbeat_rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The steady <w>pulse</w> of the drums kept the dancers moving in time.",
                            "target_lang_translations": {
                                "ru": "Ровная <w>пульсация</w> барабанов заставляла танцоров двигаться в такт."
                            }
                        },
                        {
                            "text": "You could feel the rhythmic <w>pulse</w> of the bass vibrating through the floor.",
                            "target_lang_translations": {
                                "ru": "Можно было почувствовать ритмичную <w>пульсацию</w> баса, вибрирующую через пол."
                            }
                        }
                    ],
                    "synonyms": [
                        "undulate"
                    ],
                    "common_phrases": [
                        "rhythmic pulse",
                        "steady pulse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный, регулярный ритм или такт, особенно в музыке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ритм",
                                "target_lang_sense_clarification": "музыкальный такт"
                            },
                            {
                                "target_lang_word": "пульсация",
                                "target_lang_sense_clarification": "равномерное биение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8e032f4-f85d-38cf-957e-34d354632c04",
                    "sense_definition": "The edible seeds of certain plants, such as beans, lentils, and peas; also, the plants themselves.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vegetarians often rely on <w>pulses</w> like lentils and chickpeas for protein.",
                            "target_lang_translations": {
                                "ru": "Вегетарианцы часто полагаются на <w>бобовые</w>, такие как чечевица и нут, для получения белка."
                            }
                        },
                        {
                            "text": "The soup was made from a mixture of dried <w>pulses</w> and vegetables.",
                            "target_lang_translations": {
                                "ru": "Суп был приготовлен из смеси сушеных <w>бобовых</w> и овощей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dried pulses",
                        "grains and pulses"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "botany"
                        },
                        {
                            "trait_type": "form",
                            "comment": "food"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобные семена некоторых растений, таких как фасоль, чечевица и горох; бобовые культуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бобовые",
                                "target_lang_sense_clarification": "растения и семена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6eb504dc-7042-3f5d-bd94-313390ac4e1e",
                    "sense_definition": "The central point of energy, activity, or feeling in a place or group.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She always keeps her finger on the <w>pulse</w> of the latest fashion trends.",
                            "target_lang_translations": {
                                "ru": "Она всегда держит руку на <w>пульсе</w> последних модных тенденций."
                            }
                        },
                        {
                            "text": "The market district is the vibrant <w>pulse</w> of the city.",
                            "target_lang_translations": {
                                "ru": "Рыночный район — это живой <w>пульс</w> города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "finger on the pulse",
                        "pulse of the city"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центр энергии, активности или настроения в каком-либо месте или группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульс",
                                "target_lang_sense_clarification": "переносное значение: ритм жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2173fbe5-ffb4-3c53-ba9b-258ab6066d03",
                    "sense_definition": "A short burst of energy, such as light, sound, or electricity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physics_burst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The radar system sends out a <w>pulse</w> of radio waves to detect distant objects.",
                            "target_lang_translations": {
                                "ru": "Радарная система посылает <w>импульс</w> радиоволн для обнаружения удаленных объектов."
                            }
                        },
                        {
                            "text": "A sudden electromagnetic <w>pulse</w> disrupted the electronic equipment.",
                            "target_lang_translations": {
                                "ru": "Внезапный электромагнитный <w>импульс</w> нарушил работу электронного оборудования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "electromagnetic pulse",
                        "pulse of light"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "physics"
                        },
                        {
                            "trait_type": "form",
                            "comment": "technology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий всплеск энергии, например света, звука или электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импульс",
                                "target_lang_sense_clarification": "физика: всплеск энергии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03cf7ff1-182c-3701-978f-4cda2668aec5",
                    "sense_definition": "A setting on a machine, such as a food processor, that produces short bursts of power.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "appliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Use the <w>pulse</w> setting to chop the nuts without turning them into powder.",
                            "target_lang_translations": {
                                "ru": "Используйте <w>импульсный режим</w>, чтобы измельчить орехи, не превращая их в порошок."
                            }
                        },
                        {
                            "text": "A few quick presses of the <w>pulse</w> button will mix the dough perfectly.",
                            "target_lang_translations": {
                                "ru": "Несколько быстрых нажатий кнопки <w>импульсного режима</w> идеально смешают тесто."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pulse setting",
                        "pulse button"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "culinary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Режим работы прибора (например, кухонного комбайна), создающий короткие всплески мощности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импульсный режим",
                                "target_lang_sense_clarification": "настройка прибора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a30027c9-7bb7-3410-a11f-b4b4589309bb",
                    "sense_definition": "To expand and contract rhythmically; to beat, throb, or vibrate strongly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "heartbeat_rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could feel the blood <w>pulsing</w> through my veins after the run.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал, как кровь <w>пульсирует</w> в моих венах после пробежки."
                            }
                        },
                        {
                            "text": "The music <w>pulsed</w> through the speakers, filling the room with sound.",
                            "target_lang_translations": {
                                "ru": "Музыка <w>пульсировала</w> из колонок, наполняя комнату звуком."
                            }
                        }
                    ],
                    "synonyms": [
                        "undulate"
                    ],
                    "common_phrases": [
                        "pulse with excitement",
                        "blood pulsed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмично расширяться и сжиматься; сильно биться или вибрировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульсировать",
                                "target_lang_sense_clarification": "ритмично биться"
                            },
                            {
                                "target_lang_word": "биться",
                                "target_lang_sense_clarification": "о сердце или крови"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52bc54f2-a0db-3134-a987-4ef14d38ea90",
                    "sense_definition": "To be filled with strong, rhythmic energy, activity, or emotion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The streets <w>pulsed</w> with life during the annual festival.",
                            "target_lang_translations": {
                                "ru": "Во время ежегодного фестиваля улицы <w>пульсировали</w> жизнью."
                            }
                        },
                        {
                            "text": "Anger <w>pulsed</w> through him when he heard the unfair news.",
                            "target_lang_translations": {
                                "ru": "Гнев <w>пульсировал</w> в нем, когда он услышал несправедливые новости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pulse with life",
                        "pulse with energy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть наполненным сильной ритмичной энергией, активностью или эмоциями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульсировать",
                                "target_lang_sense_clarification": "быть наполненным энергией"
                            },
                            {
                                "target_lang_word": "бурлить",
                                "target_lang_sense_clarification": "о жизни или эмоциях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "495cdeab-c0aa-3126-8933-6c177f060329",
                    "sense_definition": "To operate a machine, such as a food processor, in short bursts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "appliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Briefly <w>pulse</w> the ingredients until they are coarsely chopped.",
                            "target_lang_translations": {
                                "ru": "Коротко <w>прокрутите</w> ингредиенты, пока они не будут крупно нарезаны."
                            }
                        },
                        {
                            "text": "You should <w>pulse</w> the blender to avoid over-processing the sauce.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>включать</w> блендер <w>импульсами</w>, чтобы не перевзбить соус."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pulse the mixture"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "culinary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Включать прибор (например, блендер) короткими рывками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокручивать",
                                "target_lang_sense_clarification": "включать кратковременно"
                            },
                            {
                                "target_lang_word": "включать импульсами",
                                "target_lang_sense_clarification": "режим работы техники"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65873c61-6327-385d-8a68-05d2de26f858",
                    "sense_definition": "To emit or send out energy in short bursts or waves.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physics_burst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The distant star <w>pulses</w> with light at regular intervals.",
                            "target_lang_translations": {
                                "ru": "Далекая звезда <w>пульсирует</w> светом через равные промежутки времени."
                            }
                        },
                        {
                            "text": "The device <w>pulsed</w> a signal to the receiver every few seconds.",
                            "target_lang_translations": {
                                "ru": "Устройство <w>посылало импульсный</w> сигнал на приемник каждые несколько секунд."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Излучать или посылать энергию короткими вспышками или волнами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульсировать",
                                "target_lang_sense_clarification": "излучать свет/сигнал"
                            },
                            {
                                "target_lang_word": "посылать импульсы",
                                "target_lang_sense_clarification": "о сигнале"
                            }
                        ]
                    }
                }
            ]
        }
    ]

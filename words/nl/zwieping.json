                {
                    "sense_id": "acede3f9-977e-3de7-8027-6d092075d7af",
                    "sense_definition": "Een afwijking of verbuiging die ontstaat door doorbuigen of zwiepen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het frame is extra stevig gemaakt om elke <w>zwieping</w> te voorkomen.",
                            "target_lang_translations": {
                                "en": "The frame is made extra sturdy to prevent any <w>whipping</w>.",
                                "ru": "Рама сделана особо прочной, чтобы предотвратить любое <w>раскачивание</w>."
                            }
                        },
                        {
                            "text": "Door de harde wind ontstond er een gevaarlijke <w>zwieping</w> in de mast.",
                            "target_lang_translations": {
                                "en": "The strong wind caused a dangerous <w>sway</w> in the mast.",
                                "ru": "Из-за сильного ветра возникло опасное <w>колебание</w> мачты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zwieping voorkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "A deviation, bending, or oscillation caused by a whipping or swaying motion.",
                        "ru": "Отклонение, изгиб или колебание, вызванное резким движением, прогибом или раскачиванием."
                    },
                    "translations": {

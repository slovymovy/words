                {
                    "sense_id": "d947a264-e5a1-35f8-aaf5-d492b206367f",
                    "sense_definition": "Een rond voorwerp dat gebruikt wordt bij diverse sporten en spellen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "round_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen spelen met een rode <w>bal</w> in de tuin.",
                            "target_lang_translations": {
                                "ru": "Дети играют с красным <w>мячом</w> в саду."
                            }
                        },
                        {
                            "text": "De scheidsrechter legde de <w>bal</w> op de middenstip.",
                            "target_lang_translations": {
                                "ru": "Судья положил <w>мяч</w> на центральную отметку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een balletje trappen",
                        "de bal is rond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглый предмет, используемый в различных видах спорта и играх."

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fde83734-8278-3b73-831e-7818c32480df",
                    "sense_definition": "Taki sam pod względem wielkości, ilości, wartości, znaczenia lub praw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "equality_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy ludzie powinni mieć <w>równe</w> prawa.",
                            "target_lang_translations": {
                                "ru": "Все люди должны иметь <w>равные</w> права."
                            }
                        },
                        {
                            "text": "Wynik tego dodawania jest <w>równy</w> dziesięć.",
                            "target_lang_translations": {
                                "ru": "Результат этого сложения <w>равен</w> десяти."
                            }
                        }
                    ],
                    "synonyms": [
                        "taki sam",
                        "jednakowy"
                    ],
                    "antonyms": [
                        "różny",
                        "inny"
                    ],
                    "common_phrases": [
                        "równe prawa",
                        "być sobie równym",
                        "równy podział",
                        "znak równości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой же по величине, количеству, ценности, значению или правам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равный",
                                "target_lang_sense_clarification": "о правах, возможностях или математических величинах"
                            },
                            {
                                "target_lang_word": "одинаковый",
                                "target_lang_sense_clarification": "идентичный, такой же"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d7058f7-bbc3-3aea-829f-11de3109e547",
                    "sense_definition": "Mający gładką powierzchnię, pozbawiony wgłębień, dziur i wzniesień.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stół musi stać na <w>równym</w> podłożu, żeby się nie chwiał.",
                            "target_lang_translations": {
                                "ru": "Стол должен стоять на <w>ровной</w> поверхности, чтобы не шатался."
                            }
                        },
                        {
                            "text": "Droga do miasta jest szeroka i <w>równa</w>.",
                            "target_lang_translations": {
                                "ru": "Дорога в город широкая и <w>ровная</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "płaski",
                        "gładki",
                        "prosty"
                    ],
                    "antonyms": [
                        "krzywy",
                        "nierówny"
                    ],
                    "common_phrases": [
                        "równa droga",
                        "równy teren",
                        "równa powierzchnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий гладкую поверхность, без углублений, ям и возвышений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровный",
                                "target_lang_sense_clarification": "о поверхности, дороге"
                            },
                            {
                                "target_lang_word": "гладкий",
                                "target_lang_sense_clarification": "без шероховатостей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9385989-d639-397c-b47f-f62b26a7879b",
                    "sense_definition": "Przebiegający bez zmian, w stałym tempie; powtarzający się regularnie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "consistency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biegacz utrzymywał <w>równe</w> tempo przez cały wyścig.",
                            "target_lang_translations": {
                                "ru": "Бегун поддерживал <w>ровный</w> темп на протяжении всей гонки."
                            }
                        },
                        {
                            "text": "Pacjent ma spokojny i <w>równy</w> oddech.",
                            "target_lang_translations": {
                                "ru": "У пациента спокойное и <w>ровное</w> дыхание."
                            }
                        }
                    ],
                    "synonyms": [
                        "miarowy",
                        "rytmiczny"
                    ],
                    "antonyms": [
                        "nierówny"
                    ],
                    "common_phrases": [
                        "równe tempo",
                        "równy oddech",
                        "równy krok",
                        "równy puls"
                    ],
                    "target_lang_definitions": {
                        "ru": "Протекающий без изменений, в постоянном темпе; повторяющийся регулярно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровный",
                                "target_lang_sense_clarification": "о темпе, дыхании, пульсе"
                            },
                            {
                                "target_lang_word": "размеренный",
                                "target_lang_sense_clarification": "ритмичный, спокойный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3717a2b0-dabb-368c-9ff4-71766bf4d4e4",
                    "sense_definition": "W gramatyce: podstawowa forma przymiotnika lub przysłówka, nie wyrażająca wyższego ani najwyższego natężenia cechy.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słowo \"duży\" to stopień <w>równy</w> przymiotnika.",
                            "target_lang_translations": {
                                "ru": "Слово «большой» — это <w>положительная</w> степень прилагательного."
                            }
                        },
                        {
                            "text": "Na lekcji języka polskiego omawialiśmy stopień <w>równy</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке польского языка мы обсуждали <w>положительную</w> степень."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stopień równy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В грамматике: основная форма прилагательного или наречия, не выражающая ни высшей, ни превосходной степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительный",
                                "target_lang_sense_clarification": "о степени сравнения прилагательных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "689f2fc8-b483-3124-a243-e9a203c779c6",
                    "sense_definition": "O człowieku: przyjacielski, lojalny, taki, na którym można polegać.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad to <w>równy</w> gość, zawsze mi pomaga.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <w>надежный</w> мужик, всегда мне помогает."
                            }
                        },
                        {
                            "text": "To jest <w>równa</w> dziewczyna, na pewno się dogadacie.",
                            "target_lang_translations": {
                                "ru": "Это <w>классная</w> девчонка, вы точно поладите."
                            }
                        }
                    ],
                    "common_phrases": [
                        "równy gość",
                        "równy chłopak",
                        "równa babka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: дружелюбный, верный, такой, на которого можно положиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надежный",
                                "target_lang_sense_clarification": "верный, на которого можно положиться"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "разг. простой в общении, близкий по духу"
                            },
                            {
                                "target_lang_word": "классный",
                                "target_lang_sense_clarification": "разг. хороший, отличный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
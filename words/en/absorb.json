{
    "word_family": [
        "absorption",
        "absorbent",
        "absorbency"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abe2bdec-6cbc-345e-bef6-77a3cf80caa2",
                    "sense_definition": "To take in a liquid, gas, or other substance from the surface or space around.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dry soil will quickly <w>absorb</w> the rain.",
                            "target_lang_translations": {
                                "ru": "Сухая почва быстро <w>впитает</w> дождь."
                            }
                        },
                        {
                            "text": "You can use a paper towel to <w>absorb</w> the spilled milk.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать бумажное полотенце, чтобы <w>впитать</w> пролитое молоко."
                            }
                        }
                    ],
                    "synonyms": [
                        "soak up",
                        "suck up",
                        "take in",
                        "drink in",
                        "sop up"
                    ],
                    "common_phrases": [
                        "absorb water",
                        "absorb moisture",
                        "absorb liquid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вбирать в себя жидкость, газ или другое вещество с поверхности или из окружающего пространства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "впитывать",
                                "target_lang_sense_clarification": "вбирать влагу или жидкость"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "интенсивно вбирать вещество"
                            },
                            {
                                "target_lang_word": "вбирать",
                                "target_lang_sense_clarification": "втягивать внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03236b69-d43c-3511-aa92-b409d99a580a",
                    "sense_definition": "To understand and remember facts, ideas, or information completely.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It takes time to <w>absorb</w> all the details of the new policy.",
                            "target_lang_translations": {
                                "ru": "Требуется время, чтобы <w>усвоить</w> все детали новой политики."
                            }
                        },
                        {
                            "text": "Children <w>absorb</w> languages much faster than adults do.",
                            "target_lang_translations": {
                                "ru": "Дети <w>впитывают</w> языки гораздо быстрее, чем взрослые."
                            }
                        }
                    ],
                    "synonyms": [
                        "take in",
                        "assimilate",
                        "incorporate",
                        "imbibe"
                    ],
                    "common_phrases": [
                        "absorb information",
                        "absorb knowledge",
                        "absorb lessons"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать, усваивать и полностью запоминать факты, идеи или информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усваивать",
                                "target_lang_sense_clarification": "понимать и запоминать информацию"
                            },
                            {
                                "target_lang_word": "впитывать",
                                "target_lang_sense_clarification": "жадно воспринимать (знания, культуру)"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "получать информацию в большом количестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99b551d0-ee43-353f-96e1-cf7942ed23d6",
                    "sense_definition": "To hold someone's attention or interest completely.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was so <w>absorbed</w> in her work that she forgot to eat lunch.",
                            "target_lang_translations": {
                                "ru": "Она была настолько <w>поглощена</w> своей работой, что забыла пообедать."
                            }
                        },
                        {
                            "text": "The video game <w>absorbed</w> the boys for the entire afternoon.",
                            "target_lang_translations": {
                                "ru": "Видеоигра <w>увлекла</w> мальчиков на весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "engross",
                        "engage",
                        "occupy",
                        "immerse",
                        "monopolize"
                    ],
                    "common_phrases": [
                        "be absorbed in",
                        "totally absorbed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью завладевать чьим-либо вниманием или интересом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "всецело захватывать внимание"
                            },
                            {
                                "target_lang_word": "увлекать",
                                "target_lang_sense_clarification": "вызывать сильный интерес"
                            },
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "овладевать вниманием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94e0e0e0-cfa3-3c50-8499-36456fb49eae",
                    "sense_definition": "To take in and reduce the effect of energy, heat, light, sound, or impact.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dark colors tend to <w>absorb</w> heat from the sun.",
                            "target_lang_translations": {
                                "ru": "Темные цвета, как правило, <w>поглощают</w> солнечное тепло."
                            }
                        },
                        {
                            "text": "The walls of the recording studio are designed to <w>absorb</w> sound.",
                            "target_lang_translations": {
                                "ru": "Стены студии звукозаписи спроектированы так, чтобы <w>поглощать</w> звук."
                            }
                        }
                    ],
                    "synonyms": [
                        "take in"
                    ],
                    "antonyms": [
                        "emit"
                    ],
                    "common_phrases": [
                        "absorb heat",
                        "absorb energy",
                        "absorb shock",
                        "absorb light"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать в себя и уменьшать воздействие энергии, тепла, света, звука или удара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "физическое явление (свет, звук, тепло)"
                            },
                            {
                                "target_lang_word": "амортизировать",
                                "target_lang_sense_clarification": "смягчать удар или толчок"
                            },
                            {
                                "target_lang_word": "абсорбировать",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b7f2372-21f0-3a5c-9619-d4bae9ad3dea",
                    "sense_definition": "To take something in and make it part of a larger group, organization, or system.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_systemic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small bank was <w>absorbed</w> by a large financial institution.",
                            "target_lang_translations": {
                                "ru": "Маленький банк был <w>поглощен</w> крупным финансовым учреждением."
                            }
                        },
                        {
                            "text": "The city continues to <w>absorb</w> new residents from the countryside.",
                            "target_lang_translations": {
                                "ru": "Город продолжает <w>принимать</w> новых жителей из сельской местности."
                            }
                        }
                    ],
                    "synonyms": [
                        "assimilate",
                        "incorporate",
                        "swallow up",
                        "engulf"
                    ],
                    "common_phrases": [
                        "absorb into",
                        "be absorbed by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включать что-либо в состав более крупной группы, организации или системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "слияние компаний или групп"
                            },
                            {
                                "target_lang_word": "вбирать",
                                "target_lang_sense_clarification": "включать в свой состав"
                            },
                            {
                                "target_lang_word": "присоединять",
                                "target_lang_sense_clarification": "объединять с собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f91b3931-d8ca-304d-af32-9c36fbcad637",
                    "sense_definition": "To accept or deal with costs, expenses, or losses without passing them on to others.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_systemic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company agreed to <w>absorb</w> the shipping costs for the customer.",
                            "target_lang_translations": {
                                "ru": "Компания согласилась <w>взять на себя</w> расходы по доставке для клиента."
                            }
                        },
                        {
                            "text": "Small businesses often struggle to <w>absorb</w> unexpected financial losses.",
                            "target_lang_translations": {
                                "ru": "Малому бизнесу часто трудно <w>покрывать</w> неожиданные финансовые убытки."
                            }
                        }
                    ],
                    "synonyms": [
                        "bear",
                        "pay for",
                        "assume"
                    ],
                    "common_phrases": [
                        "absorb costs",
                        "absorb losses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать на себя расходы или убытки, не перекладывая их на других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брать на себя",
                                "target_lang_sense_clarification": "принимать ответственность за расходы"
                            },
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "оплачивать издержки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
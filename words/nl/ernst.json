        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e99063cb-a3d5-36f6-b0a6-d2e2400be329",
                    "sense_definition": "De mate waarin een situatie belangrijk, zorgwekkend of gevaarlijk is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gravity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>ernst</w> van de situatie drong langzaam tot iedereen door.",
                            "target_lang_translations": {
                                "ru": "<w>Серьёзность</w> ситуации медленно доходила до каждого.",
                                "en": "The <w>gravity</w> of the situation slowly dawned on everyone."
                            }
                        },
                        {
                            "text": "De dokter legde de patiënt de <w>ernst</w> van zijn aandoening uit.",
                            "target_lang_translations": {
                                "ru": "Врач объяснил пациенту <w>тяжесть</w> его заболевания.",
                                "en": "The doctor explained the <w>severity</w> of his condition to the patient."
                            }
                        }
                    ],
                    "synonyms": [
                        "serieusheid"
                    ],
                    "common_phrases": [
                        "de ernst van de situatie",
                        "de ernst inzien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень важности, тревожности или опасности ситуации.",
                        "en": "The degree to which a situation is important, worrying, or dangerous."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серьёзность",
                                "target_lang_sense_clarification": "ситуации или положения"
                            },
                            {
                                "target_lang_word": "тяжесть",
                                "target_lang_sense_clarification": "заболевания или преступления"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "seriousness",
                                "target_lang_sense_clarification": "general quality"
                            },
                            {
                                "target_lang_word": "gravity",
                                "target_lang_sense_clarification": "of a situation"
                            },
                            {
                                "target_lang_word": "severity",
                                "target_lang_sense_clarification": "of a condition or illness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "561f9d82-27de-32c8-9321-bf33fa77ea5d",
                    "sense_definition": "Een serieuze en oprechte gemoedstoestand zonder spot of humor.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "demeanor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze sprak met grote <w>ernst</w> over de gevolgen van haar beslissing.",
                            "target_lang_translations": {
                                "ru": "Она с большой <w>серьёзностью</w> говорила о последствиях своего решения.",
                                "en": "She spoke with great <w>seriousness</w> about the consequences of her decision."
                            }
                        },
                        {
                            "text": "Meen je dat nu in alle <w>ernst</w> of maak je een grapje?",
                            "target_lang_translations": {
                                "ru": "Ты говоришь это со всей <w>серьёзностью</w> или шутишь?",
                                "en": "Do you mean that in all <w>seriousness</w> or are you joking?"
                            }
                        }
                    ],
                    "synonyms": [
                        "serieusheid",
                        "seriositeit"
                    ],
                    "common_phrases": [
                        "in alle ernst",
                        "met grote ernst",
                        "bittere ernst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серьёзное и искреннее душевное состояние, без насмешки или юмора.",
                        "en": "A serious and sincere state of mind without mockery or humor."
                    },
                    "translations": {

                },
                {
                    "sense_id": "0ba18d68-bf42-3265-9731-494967935c92",
                    "sense_definition": "A submunicipal administrative unit of a city in North or South Korea.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "admin_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My friend lives in a quiet <w>dong</w> on the outskirts of Seoul.",
                            "target_lang_translations": {

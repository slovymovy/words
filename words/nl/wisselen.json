                {
                    "sense_id": "0d708d4a-9cfe-3311-8a2f-2f81fb0c9bc0",
                    "sense_definition": "Geld omruilen voor een andere valuta of groot geld omruilen voor kleingeld.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "exchange_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag honderd euro <w>wisselen</w> in kleinere biljetten.",

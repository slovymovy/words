                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "serious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een <w>erge</w> fout gemaakt op zijn werk.",
                            "target_lang_translations": {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d7cb9132-050c-3adb-a816-d1e804b698ff",
                    "sense_definition": "Iemand die voortdurend negatieve kritiek levert op anderen, vaak uit jaloezie of onvrede, vooral op het internet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "critic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trek je niets aan van wat die <w>hater</w> in de reacties schrijft.",
                            "target_lang_translations": {
                                "en": "Don't let what that <w>hater</w> writes in the comments bother you.",
                                "ru": "Не обращай внимания на то, что пишет этот <w>хейтер</w> в комментариях."
                            }
                        },
                        {
                            "text": "Als je succesvol bent, zul je altijd wel een <w>hater</w> tegenkomen.",
                            "target_lang_translations": {
                                "en": "If you are successful, you will always encounter a <w>hater</w>.",
                                "ru": "Если ты успешен, тебе всегда встретится какой-нибудь <w>хейтер</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "criticaster",
                        "azijnpisser"
                    ],
                    "antonyms": [
                        "fan",
                        "bewonderaar"
                    ],
                    "common_phrases": [
                        "jaloerse-hater",
                        "online-hater"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who constantly criticizes others, often out of jealousy, especially online",
                        "ru": "Тот, кто постоянно критикует других, часто из зависти или недовольства, особенно в интернете"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hater",
                                "target_lang_sense_clarification": "person who criticizes others (slang)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хейтер",
                                "target_lang_sense_clarification": "интернет-сленг: недоброжелатель, пишущий злобные комментарии"
                            },
                            {
                                "target_lang_word": "злопыхатель",
                                "target_lang_sense_clarification": "человек, который злобно критикует"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "163966c1-ab21-3f3c-a459-48ae98222994",
                    "sense_definition": "Iemand die een diepe afkeer of vijandschap koestert jegens een persoon, groep of zaak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "enemy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een bekende <w>hater</w> van onrecht en corruptie.",
                            "target_lang_translations": {
                                "en": "He is a known <w>hater</w> of injustice and corruption.",
                                "ru": "Он известный <w>ненавистник</w> несправедливости и коррупции."
                            }
                        },
                        {
                            "text": "De <w>hater</w> van de koning werd door de politie in de gaten gehouden.",
                            "target_lang_translations": {
                                "en": "The <w>hater</w> of the king was being watched by the police.",
                                "ru": "За <w>ненавистником</w> короля следила полиция."
                            }
                        }
                    ],
                    "synonyms": [
                        "vijand"
                    ],
                    "antonyms": [
                        "liefhebber",
                        "vriend"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who feels deep dislike or enmity towards a person, group, or thing",
                        "ru": "Человек, испытывающий глубокую неприязнь или вражду к кому-либо или чему-либо"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hater",
                                "target_lang_sense_clarification": "one who hates something specific"
                            },
                            {
                                "target_lang_word": "loather",
                                "target_lang_sense_clarification": "one who feels intense disgust"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ненавистник",
                                "target_lang_sense_clarification": "тот, кто ненавидит кого-то или что-то"
                            }
                        ]
                    }
                }
            ]
        }

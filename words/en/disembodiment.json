    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "173f083b-d756-3208-b8f2-a4ab121bbcc8",
                    "sense_definition": "The state or feeling of being separated from the body or physical existence.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deep meditation can sometimes induce a profound sense of <w>disembodiment</w>.",
                            "target_lang_translations": {
                                "ru": "Глубокая медитация иногда может вызвать сильное чувство <w>бестелесности</w>."
                            }
                        },
                        {
                            "text": "The virtual reality experience was so immersive that it caused a temporary feeling of <w>disembodiment</w>.",
                            "target_lang_translations": {
                                "ru": "Виртуальная реальность была настолько захватывающей, что вызвала временное ощущение <w>развоплощения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "incorporeality",
                        "detachment"
                    ],
                    "antonyms": [
                        "embodiment",
                        "incarnation"
                    ],
                    "common_phrases": [
                        "sense of disembodiment",
                        "feeling of disembodiment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или ощущение отделенности от тела или физического существования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бестелесность",
                                "target_lang_sense_clarification": "состояние отсутствия физического тела"
                            },
                            {
                                "target_lang_word": "развоплощение",
                                "target_lang_sense_clarification": "процесс или результат утраты телесной оболочки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e55de8e4-4698-3d96-836f-68431ca5b1a9",
                    "sense_definition": "A spirit, soul, or consciousness that exists without a physical form.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The medium claimed to communicate with a <w>disembodiment</w> that lingered in the room.",
                            "target_lang_translations": {
                                "ru": "Медиум утверждал, что общается с <w>бесплотным духом</w>, который находился в комнате."
                            }
                        },
                        {
                            "text": "In the story, the <w>disembodiment</w> wandered the castle halls searching for its lost life.",
                            "target_lang_translations": {
                                "ru": "В рассказе <w>бесплотная сущность</w> бродила по залам замка в поисках своей утраченной жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "spirit",
                        "phantom",
                        "ghost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дух, душа или сознание, существующие без физической формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесплотный дух",
                                "target_lang_sense_clarification": "сущность, не имеющая тела"
                            },
                            {
                                "target_lang_word": "призрак",
                                "target_lang_sense_clarification": "душа умершего"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "98c198cf-065e-3fba-830f-3c97e2a9a60a",
                    "sense_definition": "Качество пищи или напитка (сладкое, горькое, кислое, солёное), которое ощущается во рту во время еды.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот суп имеет очень приятный и нежный <w>вкус</w>.",
                            "target_lang_translations": {
                                "en": "This soup has a very pleasant and delicate <w>taste</w>."
                            }
                        },
                        {
                            "text": "Попробуй яблоко на <w>вкус</w>, оно должно быть сладким.",
                            "target_lang_translations": {
                                "en": "Check the <w>taste</w> of the apple, it should be sweet."
                            }
                        }
                    ],
                    "common_phrases": [
                        "приятный вкус",
                        "на вкус",
                        "горький вкус",
                        "сладкий вкус"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of food or drink (sweet, bitter, sour, salty) felt in the mouth while eating."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "taste",
                                "target_lang_sense_clarification": "sensation of flavor"
                            },
                            {

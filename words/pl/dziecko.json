            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7a43a854-94f4-3fc7-98b3-ae4552e4edf0",
                    "sense_definition": "Istota ludzka w okresie wzrostu i dojrzewania, która nie osiągnęła jeszcze dorosłości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe <w>dziecko</w> bawi się w piaskownicy swoimi zabawkami.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>ребёнок</w> играет в песочнице своими игрушками."
                            }
                        },
                        {
                            "text": "Każde <w>dziecko</w> powinno chodzić do szkoły i się uczyć.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>ребёнок</w> должен ходить в школу и учиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "dzieciak",
                        "maluch",
                        "brzdąc",
                        "szkrab",
                        "smyk",
                        "małolat"
                    ],
                    "antonyms": [
                        "dorosły",
                        "starzec"
                    ],
                    "common_phrases": [
                        "małe dziecko",
                        "grzeczne dziecko",
                        "płaczące dziecko",
                        "bawić się jak dziecko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек в период роста и развития, еще не достигший взрослости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ребёнок",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "дитя",
                                "target_lang_sense_clarification": "поэтическое или устаревшее"
                            },
                            {
                                "target_lang_word": "малыш",
                                "target_lang_sense_clarification": "маленький ребенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cc9ec22-9797-34f1-a891-a8907d88da2f",
                    "sense_definition": "Syn lub córka w relacji do swoich rodziców, niezależnie od wieku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona jest jedynym <w>dzieckiem</w> swoich rodziców i bardzo o nich dba.",
                            "target_lang_translations": {
                                "ru": "Она — единственный <w>ребёнок</w> у своих родителей и очень о них заботится."
                            }
                        },
                        {
                            "text": "Sąsiedzi spodziewają się drugiego <w>dziecka</w> w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Соседи ждут второго <w>ребёнка</w> в следующем месяце."
                            }
                        }
                    ],
                    "synonyms": [
                        "potomek",
                        "latorośl",
                        "pociecha"
                    ],
                    "antonyms": [
                        "antenat"
                    ],
                    "common_phrases": [
                        "jedyne dziecko",
                        "adoptowane dziecko",
                        "nieślubne dziecko",
                        "spodziewać się dziecka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сын или дочь по отношению к своим родителям, независимо от возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ребёнок",
                                "target_lang_sense_clarification": "сын или дочь"
                            },
                            {
                                "target_lang_word": "дитя",
                                "target_lang_sense_clarification": "в значении потомства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a94ecdee-ba29-3fbd-90cc-bd868b9b1b11",
                    "sense_definition": "Forma zwrotu do osoby młodszej lub bliskiej, wyrażająca troskę lub sympatię.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje drogie <w>dziecko</w>, musisz jeszcze wiele zrozumieć o świecie.",
                            "target_lang_translations": {
                                "ru": "Моё дорогое <w>дитя</w>, тебе ещё многое нужно понять о мире."
                            }
                        },
                        {
                            "text": "Uważaj na siebie, <w>dziecko</w>, bo na zewnątrz jest bardzo ślisko.",
                            "target_lang_translations": {
                                "ru": "Береги себя, <w>детка</w>, потому что на улице очень скользко."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziecina",
                        "dziecinka"
                    ],
                    "common_phrases": [
                        "moje drogie dziecko",
                        "biedne dziecko"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane przez osoby starsze wobec młodszych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма обращения к младшему или близкому человеку, выражающая заботу или симпатию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дитя",
                                "target_lang_sense_clarification": "ласковое обращение"
                            },
                            {
                                "target_lang_word": "детка",
                                "target_lang_sense_clarification": "разговорное обращение"
                            },
                            {
                                "target_lang_word": "деточка",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e68b9ab-f4a7-38dd-bd1c-92afcca90f23",
                    "sense_definition": "Osoba dorosła, która zachowuje się niedojrzale, naiwnie lub niepoważnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zachowujesz się jak małe <w>dziecko</w>, bądź wreszcie poważny.",
                            "target_lang_translations": {
                                "ru": "Ты ведёшь себя как маленький <w>ребёнок</w>, будь наконец серьёзным."
                            }
                        },
                        {
                            "text": "To duże <w>dziecko</w> wciąż wydaje pieniądze na gry komputerowe.",
                            "target_lang_translations": {
                                "ru": "Этот большой <w>ребёнок</w> всё ещё тратит деньги на компьютерные игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "dzieciuch",
                        "naiwniak"
                    ],
                    "antonyms": [
                        "dorosły"
                    ],
                    "common_phrases": [
                        "duże dziecko",
                        "nie bądź dzieckiem",
                        "zachowywać się jak dziecko"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane z dezaprobatą lub żartobliwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый человек, который ведет себя незрело, наивно или несерьезно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ребёнок",
                                "target_lang_sense_clarification": "наивный человек"
                            },
                            {
                                "target_lang_word": "дитя",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4aebda35-a5b4-3420-ab6d-e7ef1c530de4",
                    "sense_definition": "Wytwór czyjejś pracy, idea lub dzieło, z którym twórca jest silnie związany emocjonalnie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta nowa powieść to moje najukochańsze <w>dziecko</w>, nad którym pracowałem lata.",
                            "target_lang_translations": {
                                "ru": "Этот новый роман — моё самое любимое <w>детище</w>, над которым я работал годами."
                            }
                        },
                        {
                            "text": "Ten innowacyjny projekt jest <w>dzieckiem</w> całego naszego zespołu badawczego.",
                            "target_lang_translations": {
                                "ru": "Этот инновационный проект является <w>детищем</w> всей нашей исследовательской группы."
                            }
                        }
                    ],
                    "synonyms": [

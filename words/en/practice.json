                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судопроизводство",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "практика",
                                "target_lang_sense_clarification": "юридическая"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c07af44b-6a97-3cc1-8bd2-e2bff89830dc",
                    "sense_definition": "To perform an activity repeatedly in order to improve one's proficiency.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "skill_learning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>practice</w> every day if you want to learn the piano.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>заниматься</w> каждый день, если хочешь научиться играть на пианино."
                            }
                        },
                        {
                            "text": "She <w>practices</w> her French by talking to native speakers online.",
                            "target_lang_translations": {
                                "ru": "Она <w>практикует</w> свой французский, разговаривая с носителями языка онлайн."
                            }
                        }
                    ],
                    "synonyms": [
                        "drill",
                        "exercise"
                    ],
                    "common_phrases": [
                        "practice hard",
                        "practice daily"
                    ],

{
    "word_family": [
        "receive",
        "reception",
        "receiver",
        "receptive",
        "receivable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7330c394-04eb-3230-85d9-beabed25de8a",
                    "sense_definition": "A written or printed statement acknowledging that a specific article or sum of money has been received.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please keep your <w>receipt</w> in case you need to return the shirt.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сохраняйте <w>чек</w> на случай, если вам нужно будет вернуть рубашку."
                            }
                        },
                        {
                            "text": "The cashier handed me a <w>receipt</w> for the groceries I bought.",
                            "target_lang_translations": {
                                "ru": "Кассир протянул мне <w>чек</w> за продукты, которые я купил."
                            }
                        }
                    ],
                    "synonyms": [
                        "sales slip",
                        "proof of purchase",
                        "acknowledgment"
                    ],
                    "common_phrases": [
                        "keep the receipt",
                        "ask for a receipt",
                        "tax receipt",
                        "return without a receipt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Письменный или печатный документ, подтверждающий получение определенного товара или денежной суммы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чек",
                                "target_lang_sense_clarification": "кассовый, при покупке товаров"
                            },
                            {
                                "target_lang_word": "квитанция",
                                "target_lang_sense_clarification": "официальный документ об оплате услуг"
                            },
                            {
                                "target_lang_word": "расписка",
                                "target_lang_sense_clarification": "письменное подтверждение получения чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64976c7b-2e66-3c92-908a-20213078387d",
                    "sense_definition": "The action of receiving something or the state of having been received.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The full payment is due immediately upon <w>receipt</w> of the invoice.",
                            "target_lang_translations": {
                                "ru": "Полная оплата должна быть произведена сразу после <w>получения</w> счета."
                            }
                        },
                        {
                            "text": "We will ship your order within two days of the <w>receipt</w> of your letter.",
                            "target_lang_translations": {
                                "ru": "Мы отправим ваш заказ в течение двух дней после <w>получения</w> вашего письма."
                            }
                        }
                    ],
                    "synonyms": [
                        "reception",
                        "arrival",
                        "delivery"
                    ],
                    "common_phrases": [
                        "on receipt of",
                        "acknowledge receipt",
                        "upon receipt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по получению чего-либо или состояние полученного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получение",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4234dba6-a516-3faf-88e6-3828d8bcf883",
                    "sense_definition": "A quantity or amount of money received during a specific period, often by a business.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The box office <w>receipts</w> for the new movie were higher than expected.",
                            "target_lang_translations": {
                                "ru": "<w>Кассовые сборы</w> нового фильма оказались выше ожидаемых."
                            }
                        },
                        {
                            "text": "The shop's daily <w>receipts</w> barely covered the cost of operation.",
                            "target_lang_translations": {
                                "ru": "Дневная <w>выручка</w> магазина едва покрывала эксплуатационные расходы."
                            }
                        }
                    ],
                    "synonyms": [
                        "takings",
                        "proceeds",
                        "income",
                        "revenue",
                        "earnings"
                    ],
                    "antonyms": [
                        "expenditure",
                        "outgoings",
                        "expenses"
                    ],
                    "common_phrases": [
                        "box office receipts",
                        "gross receipts",
                        "cash receipts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество или сумма денег, полученных за определенный период, часто в бизнесе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выручка",
                                "target_lang_sense_clarification": "доход от продаж"
                            },
                            {
                                "target_lang_word": "кассовые сборы",
                                "target_lang_sense_clarification": "доход от фильмов или представлений"
                            },
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "поступление денежных средств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e522b3f-179e-3c23-9224-05d637123739",
                    "sense_definition": "Evidence or documentation, such as screenshots or messages, used to prove someone's past actions or statements, especially regarding wrongdoing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "evidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She claimed she never sent that message, but he had the <w>receipts</w> to prove it.",
                            "target_lang_translations": {
                                "ru": "Она утверждала, что никогда не отправляла это сообщение, но у него были <w>пруфы</w>, чтобы доказать это."
                            }
                        },
                        {
                            "text": "Don't make accusations on the internet unless you can show the <w>receipts</w>.",
                            "target_lang_translations": {
                                "ru": "Не бросайся обвинениями в интернете, если не можешь предъявить <w>доказательства</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "proof",
                        "evidence",
                        "documentation"
                    ],
                    "common_phrases": [
                        "show the receipts",
                        "bring the receipts",
                        "got the receipts"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Доказательства или документация (например, скриншоты), используемые для подтверждения чьих-либо прошлых действий или слов, особенно в контексте разоблачения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доказательства",
                                "target_lang_sense_clarification": "подтверждающие факты"
                            },
                            {
                                "target_lang_word": "пруфы",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "003344a6-fd6c-3644-af78-4adc7d300f59",
                    "sense_definition": "A set of instructions for preparing a particular dish, including a list of the ingredients required.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "instruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother passed down an old <w>receipt</w> for making blackberry jam.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка передала мне старинный <w>рецепт</w> приготовления ежевичного варенья."
                            }
                        },
                        {
                            "text": "She followed the <w>receipt</w> precisely to ensure the cake turned out well.",
                            "target_lang_translations": {
                                "ru": "Она в точности следовала <w>рецепту</w>, чтобы торт получился удачным."
                            }
                        }
                    ],
                    "synonyms": [
                        "recipe",
                        "formula"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "now standardly 'recipe'"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "US South"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор инструкций по приготовлению определенного блюда, включая список необходимых ингредиентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рецепт",
                                "target_lang_sense_clarification": "кулинарный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd9aaf61-a6e6-3118-af64-07569995e960",
                    "sense_definition": "To mark a bill or invoice as having been paid, usually by writing on it or stamping it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clerk <w>receipted</w> the bill after I handed him the cash.",
                            "target_lang_translations": {
                                "ru": "Клерк <w>поставил отметку об оплате</w> на счете после того, как я передал ему наличные."
                            }
                        },
                        {
                            "text": "Please ensure the delivery note is <w>receipted</w> before you leave the warehouse.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убедитесь, что на накладной <w>расписались в получении</w>, прежде чем покинуть склад."
                            }
                        }
                    ],
                    "synonyms": [
                        "sign for",
                        "acknowledge"
                    ],
                    "common_phrases": [
                        "receipt a bill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помечать счет или накладную как оплаченные, обычно путем надписи или штампа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расписаться в получении",
                                "target_lang_sense_clarification": "подтвердить подписью"
                            },
                            {
                                "target_lang_word": "поставить отметку об оплате",
                                "target_lang_sense_clarification": "на документе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
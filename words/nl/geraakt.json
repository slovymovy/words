                },
                {
                    "sense_id": "ca89a091-725b-3464-a791-8bb368fb1e9e",
                    "sense_definition": "Voltooid deelwoord van <w>geraken</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "participle_geraken",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hevige sneeuwval zijn we gisteren niet thuis <w>geraakt</w>.",
                            "target_lang_translations": {
                                "en": "Due to the heavy snowfall, we didn't <w>get</w> home yesterday.",
                                "ru": "Из-за сильного снегопада мы вчера не <w>добрались</w> до дома."
                            }
                        },
                        {
                            "text": "Mijn gedachten zijn bij allen die diep <w>geraakt</w> zijn door dit droevige nieuws.",
                            "target_lang_translations": {
                                "en": "My thoughts are with all those who have been deeply <w>touched</w> by this sad news.",
                                "ru": "Мои мысли со всеми, кто был глубоко <w>тронут</w> этой печальной новостью."
                            }
                        }
                    ],
                    "synonyms": [
                        "gekomen",
                        "beland"
                    ],
                    "common_phrases": [

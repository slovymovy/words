                {
                    "sense_id": "feca5f74-a1ce-3f34-8726-b469bcfc4b68",
                    "sense_definition": "Popular trends, especially in clothing, hair, and accessories; the industry that designs and sells these items.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "style_trend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She likes to read magazines about the latest <w>fashion</w>.",
                            "target_lang_translations": {
                                "ru": "Ей нравится читать журналы о последней <w>моде</w>."
                            }
                        },
                        {
                            "text": "He works in the <w>fashion</w> industry designing shoes.",
                            "target_lang_translations": {
                                "ru": "Он работает в индустрии <w>моды</w>, создавая дизайн обуви."
                            }
                        }
                    ],
                    "synonyms": [
                        "style",
                        "couture",
                        "vogue"
                    ],
                    "common_phrases": [

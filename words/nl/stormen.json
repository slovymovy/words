                            },
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "move quickly"
                            },
                            {
                                "target_lang_word": "dash",
                                "target_lang_sense_clarification": "move suddenly"
                            }
                        ]
                    }
                }

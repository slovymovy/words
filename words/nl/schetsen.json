    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "43ab3bbc-f136-35cc-ac62-0e1fd052dec5",
                    "sense_definition": "Een snelle, voorlopige tekening maken met weinig details.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar <w>schetst</w> eerst het landschap voordat hij begint te schilderen.",
                            "target_lang_translations": {
                                "ru": "Художник сначала <w>набрасывает</w> пейзаж, прежде чем начать писать красками."
                            }
                        },
                        {
                            "text": "Kun jij even snel een plattegrondje <w>schetsen</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь быстро <w>набросать</w> план?"
                            }
                        }
                    ],
                    "synonyms": [
                        "tekenen",
                        "ontwerpen"
                    ],
                    "antonyms": [
                        "uitwerken"
                    ],
                    "common_phrases": [
                        "een portret schetsen",
                        "snel even schetsen",
                        "ruw schetsen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать быстрый, предварительный рисунок с небольшим количеством деталей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набрасывать",
                                "target_lang_sense_clarification": "делать набросок"
                            },
                            {
                                "target_lang_word": "делать эскиз",
                                "target_lang_sense_clarification": "рисовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b41fd9f0-3cc4-3a87-9c42-de40a155e9b1",
                    "sense_definition": "Een situatie, idee of gebeurtenis in grote lijnen beschrijven.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur <w>schetst</w> een positief beeld van de toekomst van het bedrijf.",
                            "target_lang_translations": {
                                "ru": "Директор <w>обрисовывает</w> позитивную картину будущего компании."
                            }
                        },
                        {
                            "text": "Tijdens de vergadering <w>schetste</w> zij de problemen waar we tegenaan lopen.",
                            "target_lang_translations": {
                                "ru": "Во время собрания она <w>обрисовала</w> проблемы, с которыми мы сталкиваемся."
                            }
                        }
                    ],
                    "synonyms": [
                        "beschrijven",
                        "uiteenzetten",
                        "schilderen"
                    ],
                    "common_phrases": [
                        "een beeld schetsen",
                        "de situatie schetsen",
                        "kort schetsen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывать ситуацию, идею или событие в общих чертах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрисовывать",
                                "target_lang_sense_clarification": "описывать ситуацию"
                            },
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "излагать суть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b066171d-0636-3939-b649-32ae33d3604b",
                    "sense_definition": "Iemand oplichten of bedriegen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft die toerist <w>geschetst</w> door hem een neppe telefoon te verkopen.",
                            "target_lang_translations": {
                                "ru": "Он <w>кинул</w> того туриста, продав ему поддельный телефон."
                            }
                        },
                        {
                            "text": "Pas op dat ze je niet <w>schetsen</w> als je daar iets koopt.",
                            "target_lang_translations": {
                                "ru": "Осторожно, чтобы тебя не <w>развели</w>, когда будешь там что-то покупать."
                            }
                        }
                    ],
                    "synonyms": [
                        "oplichten",
                        "bedriegen",
                        "flashen"
                    ],
                    "common_phrases": [
                        "iemand vies schetsen"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "voornamelijk gebruikt in straattaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обманывать или надувать кого-то (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кидать",
                                "target_lang_sense_clarification": "обманывать на деньги"
                            },
                            {
                                "target_lang_word": "разводить",
                                "target_lang_sense_clarification": "мошенничать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

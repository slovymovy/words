                },
                {
                    "sense_id": "93526a96-c4dd-3b97-93cc-f40c9cd207df",
                    "sense_definition": "Relating to conflict or division between different religious or ethnic groups within a society.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sectarian",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government is working to prevent further <w>communal</w> violence in the region.",
                            "target_lang_translations": {
                                "ru": "Правительство работает над предотвращением дальнейшего <w>межобщинного</w> насилия в регионе."
                            }
                        },
                        {
                            "text": "History books often discuss the <w>communal</w> riots that occurred during that period.",
                            "target_lang_translations": {

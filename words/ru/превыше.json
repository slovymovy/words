    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "4bd20c45-461c-351f-bf05-d338c21aaf05",
                    "sense_definition": "Выше, значительнее чего-либо по степени, важности или рангу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для настоящего врача жизнь пациента должна быть <w>превыше</w> всего.",
                            "target_lang_translations": {
                                "en": "For a true doctor, a patient's life must be <w>above</w> all else."
                            }
                        },
                        {
                            "text": "В нашей семье честность всегда ставилась <w>превыше</w> богатства.",
                            "target_lang_translations": {
                                "en": "In our family, honesty was always placed <w>above</w> wealth."
                            }
                        }
                    ],
                    "synonyms": [
                        "важнее",
                        "важнее"
                    ],
                    "antonyms": [
                        "ниже"
                    ],
                    "common_phrases": [
                        "превыше всего",
                        "превыше всех"
                    ],
                    "target_lang_definitions": {
                        "en": "Higher or more significant than something in degree, importance, or rank."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "above",
                                "target_lang_sense_clarification": "indicating higher priority or importance"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "89a7402a-030f-3b16-affb-5126749e86a5",
                    "sense_definition": "Намного выше, гораздо сильнее или важнее (часто используется в функции сказуемого).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его мужество и стойкость были <w>превыше</w> всяких похвал.",
                            "target_lang_translations": {
                                "en": "His courage and resilience were <w>beyond</w> all praise."
                            }
                        },
                        {
                            "text": "Иногда обстоятельства оказываются <w>превыше</w> наших желаний и планов.",
                            "target_lang_translations": {
                                "en": "Sometimes circumstances prove to be <w>beyond</w> our desires and plans."
                            }
                        }
                    ],
                    "synonyms": [
                        "важнее"
                    ],
                    "antonyms": [
                        "ниже"
                    ],
                    "common_phrases": [
                        "превыше сил",
                        "превыше понимания",
                        "превыше похвал"
                    ],
                    "target_lang_definitions": {
                        "en": "Far higher, stronger, or more important; exceeding limits or standards."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beyond",
                                "target_lang_sense_clarification": "outside the range, limits, or capacity of"
                            },
                            {
                                "target_lang_word": "above",
                                "target_lang_sense_clarification": "surpassing a level or standard"
                            }
                        ]
                    }
                }
            ]
        }
    ]

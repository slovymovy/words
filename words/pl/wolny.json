                },
                {
                    "sense_id": "c7e125be-5c32-37d4-88d7-1fe0e3c4c146",
                    "sense_definition": "Niezależny, niepodlegający niczyjej władzy; mogący o sobie decydować.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "liberty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek rodzi się <w>wolny</w> i ma równe prawa.",
                            "target_lang_translations": {
                                "ru": "Каждый человек рождается <w>свободным</w> и имеет равные права."
                            }
                        },
                        {
                            "text": "Polska jest teraz krajem <w>wolnym</w> i suwerennym.",
                            "target_lang_translations": {
                                "ru": "Польша теперь является <w>свободной</w> и суверенной страной."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezależny",
                        "niepodległy",
                        "suwerenny",
                        "swobodny"

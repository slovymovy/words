    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4b6c68ea-f595-315e-aa93-f98914cded33",
                    "sense_definition": "Equal to or fulfilling some requirement; sufficient for a specific need.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The instructions provided were not <w>adequate</w> for a beginner to understand.",
                            "target_lang_translations": {
                                "ru": "Предоставленные инструкции не были <w>достаточными</w> для понимания новичком."
                            }
                        },
                        {
                            "text": "She felt that her current salary was <w>adequate</w> to support her family comfortably.",
                            "target_lang_translations": {
                                "ru": "Она считала, что её текущая зарплата <w>достаточна</w>, чтобы с комфортом содержать семью."
                            }
                        }
                    ],
                    "synonyms": [
                        "sufficient",
                        "satisfactory",
                        "acceptable"
                    ],
                    "antonyms": [
                        "inadequate"
                    ],
                    "common_phrases": [
                        "adequate resources",
                        "adequate protection",
                        "adequate supply",
                        "adequate care"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвечающий требованиям или достаточный для определённой цели"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достаточный",
                                "target_lang_sense_clarification": "в нужном количестве или степени"
                            },
                            {
                                "target_lang_word": "адекватный",
                                "target_lang_sense_clarification": "верный, соответствующий обстоятельствам"
                            },
                            {
                                "target_lang_word": "соответствующий",
                                "target_lang_sense_clarification": "подходящий под требования"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "e98c4cfc-5921-3de7-b5ee-f3878984afa8",
                    "sense_definition": "A sufficient amount of; enough.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to ensure <w>adequate</w> time is allowed for the discussion.",
                            "target_lang_translations": {
                                "ru": "Нам нужно убедиться, что на обсуждение отведено <w>достаточное</w> время."
                            }
                        },
                        {
                            "text": "There was <w>adequate</w> evidence to proceed with the trial immediately.",
                            "target_lang_translations": {
                                "ru": "Имелись <w>достаточные</w> доказательства, чтобы немедленно начать судебное разбирательство."
                            }
                        }
                    ],
                    "synonyms": [
                        "sufficient",
                        "satisfactory"
                    ],
                    "antonyms": [
                        "inadequate"
                    ],
                    "common_phrases": [
                        "adequate time",
                        "adequate funding",
                        "adequate information"
                    ],
                    "target_lang_definitions": {

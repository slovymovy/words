    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0fb2a2dd-b21e-32a3-ba72-693fabeec841",
                    "sense_definition": "The equipment used to provide illumination; the illumination so provided.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "illumination_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lighting</w> in the room was too dim for us to read comfortably.",
                            "target_lang_translations": {
                                "ru": "<w>Освещение</w> в комнате было слишком тусклым, чтобы мы могли комфортно читать."
                            }
                        },
                        {
                            "text": "They installed new street <w>lighting</w> to improve safety in the neighborhood.",
                            "target_lang_translations": {
                                "ru": "Они установили новое уличное <w>освещение</w>, чтобы повысить безопасность в районе."
                            }
                        }
                    ],
                    "synonyms": [
                        "illumination",
                        "light"
                    ],
                    "common_phrases": [
                        "street lighting",
                        "stage lighting",
                        "natural lighting",
                        "poor lighting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оборудование, используемое для обеспечения освещенности; само освещение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освещение",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "свет",
                                "target_lang_sense_clarification": "эффект освещения или источник"
                            },
                            {
                                "target_lang_word": "иллюминация",
                                "target_lang_sense_clarification": "праздничное, яркое освещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "832c7590-5484-351d-bf8d-070f8154f2d4",
                    "sense_definition": "The act of activating lights or igniting a flame.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_igniting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lighting</w> of the Olympic torch marks the start of the games.",
                            "target_lang_translations": {
                                "ru": "<w>Зажигание</w> олимпийского огня знаменует начало игр."
                            }
                        },
                        {
                            "text": "Strict safety rules forbid the <w>lighting</w> of matches near the fuel tanks.",
                            "target_lang_translations": {
                                "ru": "Строгие правила безопасности запрещают <w>зажигание</w> спичек рядом с топливными баками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lighting of the fire",
                        "lighting of the candles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по включению света или зажиганию огня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зажигание",
                                "target_lang_sense_clarification": "огня, свечей"
                            },
                            {
                                "target_lang_word": "запаливание",
                                "target_lang_sense_clarification": "фитиля, горелки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "928d577c-5a9d-314a-8a18-377384dbe3c6",
                    "sense_definition": "The process of annealing metals.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metallurgy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The smith completed the <w>lighting</w> of the copper to make it malleable.",
                            "target_lang_translations": {
                                "ru": "Кузнец закончил <w>отжиг</w> меди, чтобы сделать её ковкой."
                            }
                        },
                        {
                            "text": "Proper <w>lighting</w> is essential to prevent the metal from becoming too brittle.",
                            "target_lang_translations": {
                                "ru": "Правильный <w>отжиг</w> необходим, чтобы металл не стал слишком хрупким."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс отжига металлов."

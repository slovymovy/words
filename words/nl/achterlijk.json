                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "rear",
                                "target_lang_sense_clarification": "situated at the back"
                            },
                            {
                                "target_lang_word": "hind",
                                "target_lang_sense_clarification": "back part (archaic)"
                            }
                        ]
                    }
                }
            ]
        },
        {

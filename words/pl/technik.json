    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d1931b1-cbbd-3f8b-8415-6f2f902f1667",
                    "sense_definition": "Pracownik posiadający wykształcenie techniczne średniego stopnia lub specjalista zajmujący się praktyczną obsługą urządzeń i systemów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat pracuje jako <technik> dentystyczny w dużej klinice.",
                            "target_lang_translations": {
                                "ru": "Мой брат работает зубным <техником> в большой клинике."
                            }
                        },
                        {
                            "text": "Musieliśmy wezwać <technika>, ponieważ nasza pralka przestała działać.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось вызвать <техника>, так как наша стиральная машина перестала работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "specjalista",
                        "fachowiec"
                    ],
                    "antonyms": [
                        "laik"
                    ],
                    "common_phrases": [
                        "technik informatyk",
                        "technik dentystyczny",
                        "technik weterynarii",
                        "główny technik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист со средним техническим образованием или работник, занимающийся обслуживанием оборудования и систем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "техник",
                                "target_lang_sense_clarification": "профессия / специалист"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b175182f-df6c-3264-8e98-5954a10f200b",
                    "sense_definition": "*Dopełniacz* liczby mnogiej od słowa <w>technika</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym muzeum można zobaczyć rozwój różnych <technik> malarskich na przestrzeni wieków.",
                            "target_lang_translations": {
                                "ru": "В этом музее можно увидеть развитие различных живописных <техник> на протяжении веков."
                            }
                        },
                        {
                            "text": "Trener nauczył nas kilku skutecznych <technik> relaksacyjnych.",
                            "target_lang_translations": {
                                "ru": "Тренер научил нас нескольким эффективным <техникам> релаксации."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа слова «техника» (методы, приемы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "техник",
                                "target_lang_sense_clarification": "форма слова «техника»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c595db36-3dbf-399f-9586-57e29ab18812",
                    "sense_definition": "Osoba, która osiągnęła mistrzowską biegłość, precyzję i sprawność w wykonywaniu określonej czynności, szczególnie w sporcie lub sztuce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piłkarz to wybitny <technik>, który potrafi precyzyjnie podać piłkę w każdej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Этот футболист — выдающийся <техник>, который может точно отдать пас в любой ситуации."
                            }
                        },
                        {
                            "text": "Jako pianista jest doskonałym <technikiem>, ale jego grze brakuje nieco emocji.",
                            "target_lang_translations": {
                                "ru": "Как пианист он является превосходным <техником>, но его игре немного не хватает эмоций."
                            }
                        }
                    ],
                    "synonyms": [
                        "wirtuoz",
                        "mistrz"
                    ],
                    "antonyms": [
                        "partacz",
                        "amator"
                    ],
                    "common_phrases": [
                        "świetny technik",
                        "doskonały technik",
                        "wspaniały technik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий высоким техническим мастерством и точностью исполнения, особенно в спорте или искусстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "техник",
                                "target_lang_sense_clarification": "мастер своего дела"
                            },
                            {
                                "target_lang_word": "виртуоз",
                                "target_lang_sense_clarification": "очень умелый человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]

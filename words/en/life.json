                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "совокупность живых существ"
                            },
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "живой мир, флора и фауна"
                            }

                    "sense_id": "aa0d85cc-261f-30ca-aec1-b98665d32dc5",
                    "sense_definition": "Een toegangsbewijs voor het openbaar vervoer, een evenement, de bioscoop of het theater.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet nog snel een <w>kaartje</w> kopen voor de trein van acht uur.",
                            "target_lang_translations": {
                                "en": "I still need to quickly buy a <w>ticket</w> for the eight o'clock train.",
                                "ru": "Мне нужно быстро купить <w>билет</w> на восьмичасовой поезд."
                            }
                        },
                        {
                            "text": "Heb jij je <w>kaartje</w> al laten scannen bij de ingang van de bioscoop?",
                            "target_lang_translations": {
                                "en": "Have you already had your <w>ticket</w> scanned at the entrance of the cinema?",
                                "ru": "Ты уже отсканировал свой <w>билет</w> на входе в кинотеатр?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ticket",
                        "toegangsbewijs",
                        "vervoerbewijs"
                    ],
                    "common_phrases": [
                        "een kaartje kopen",
                        "een kaartje knippen"
                    ],
                    "target_lang_definitions": {
                        "en": "An admission ticket for public transport, an event, the cinema, or the theater.",
                        "ru": "Билет на общественный транспорт, мероприятие, в кино или театр."
                    },
                    "translations": {

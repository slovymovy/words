    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2af96fac-acc5-3fb6-98bd-495ab75eca21",
                    "sense_definition": "Относящийся к празднику, торжеству; нарядный, праздничный.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сегодня у нас <торжественный> день, мы отмечаем юбилей фирмы.",
                            "target_lang_translations": {
                                "en": "Today is a <festive> day for us; we are celebrating the company's jubilee."
                            }
                        },
                        {
                            "text": "На площади началось <торжественное> шествие в честь великого праздника.",
                            "target_lang_translations": {
                                "en": "A <ceremonial> procession began in the square in honor of the great holiday."
                            }
                        }
                    ],
                    "synonyms": [
                        "праздничный"
                    ],
                    "antonyms": [
                        "шутливый",
                        "шутливый"
                    ],
                    "common_phrases": [
                        "торжественный день",
                        "торжественный ужин",
                        "торжественное событие",
                        "торжественная встреча"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a holiday or celebration; festive, elegant, or ceremonial."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "festive",
                                "target_lang_sense_clarification": "relating to a holiday or celebration"
                            },
                            {
                                "target_lang_word": "ceremonial",
                                "target_lang_sense_clarification": "formal and ritualistic"
                            },
                            {
                                "target_lang_word": "grand",
                                "target_lang_sense_clarification": "magnificent or imposing in scale"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc868af8-ed46-3422-884a-be77981b41fc",
                    "sense_definition": "Исполненный величия и достоинства; важный, официальный, серьёзный.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор говорил <торжественным> тоном, объявляя победителей школьного конкурса.",
                            "target_lang_translations": {
                                "en": "The director spoke in a <solemn> tone while announcing the winners of the school contest."
                            }
                        },
                        {
                            "text": "В зале царила <торжественная> тишина перед началом симфонического концерта.",
                            "target_lang_translations": {
                                "en": "A <solemn> silence reigned in the hall before the start of the symphony concert."
                            }
                        }
                    ],
                    "synonyms": [
                        "величественный",
                        "официальный",
                        "важный",
                        "серьёзный",
                        "величавый"
                    ],
                    "antonyms": [
                        "шутливый"
                    ],
                    "common_phrases": [
                        "торжественный вид",
                        "торжественный голос",
                        "торжественная обстановка",
                        "торжественная речь"
                    ],
                    "target_lang_definitions": {
                        "en": "Filled with grandeur and dignity; important, official, or serious."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "solemn",
                                "target_lang_sense_clarification": "serious, dignified, and formal"
                            },
                            {
                                "target_lang_word": "stately",
                                "target_lang_sense_clarification": "dignified and unhurried in manner or appearance"
                            },
                            {
                                "target_lang_word": "grand",
                                "target_lang_sense_clarification": "impressive and majestic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "838d8501-c234-3b8f-b305-18e44806b496",
                    "sense_definition": "Нерушимый, священный (обычно об обещании или клятве).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодые врачи дают <торжественную> клятву помогать всем больным.",
                            "target_lang_translations": {
                                "en": "Young doctors take a <solemn> oath to help all patients."
                            }
                        },
                        {
                            "text": "Он дал <торжественное> обещание никогда не нарушать закон.",
                            "target_lang_translations": {
                                "en": "He gave a <solemn> promise never to break the law."
                            }
                        }
                    ],
                    "synonyms": [
                        "нерушимый",
                        "священный"
                    ],
                    "common_phrases": [
                        "торжественная клятва",
                        "торжественное обещание"
                    ],
                    "target_lang_definitions": {
                        "en": "Inviolable and sacred (usually regarding a promise or oath)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "solemn",
                                "target_lang_sense_clarification": "sincere, binding, and sacred"
                            }
                        ]
                    }
                }
            ]
        }
    ]

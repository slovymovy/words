{
    "word_family": [
        "szmatka",
        "szmaciarz",
        "szmatławiec"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5f8cd0c-fd4e-3f37-ac0c-4f5766005035",
                    "sense_definition": "Kawałek starej, zniszczonej tkaniny używany do sprzątania, mycia lub wycierania kurzu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wytarłem rozlaną wodę starą <szmatą>.",
                            "target_lang_translations": {
                                "ru": "Я вытер разлитую воду старой <тряпкой>."
                            }
                        },
                        {
                            "text": "Weź mokrą <szmatę> i umyj podłogę w kuchni.",
                            "target_lang_translations": {
                                "ru": "Возьми мокрую <тряпку> и помой пол на кухне."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścierka",
                        "reg. śl. hadra"
                    ],
                    "common_phrases": [
                        "mokra szmata",
                        "szmata do podłogi",
                        "wycierać szmatą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок старой, изношенной ткани, используемый для уборки, мытья или вытирания пыли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тряпка",
                                "target_lang_sense_clarification": "кусок ткани для уборки"
                            },
                            {
                                "target_lang_word": "ветошь",
                                "target_lang_sense_clarification": "старая ткань, используемая как протирочный материал (техн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d1a421f-c768-34a6-a5d5-75d029289252",
                    "sense_definition": "Stare, zniszczone lub bezwartościowe ubranie (często używane w liczbie mnogiej).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wydała całą wypłatę na nowe, modne <szmaty>.",
                            "target_lang_translations": {
                                "ru": "Она потратила всю зарплату на новые модные <шмотки>."
                            }
                        },
                        {
                            "text": "Nie będę chodzić w tych starych <szmatach> po domu.",
                            "target_lang_translations": {
                                "ru": "Я не буду ходить в этих старых <тряпках> по дому."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciuch"
                    ],
                    "common_phrases": [
                        "markowe szmaty",
                        "chodzić w szmatach",
                        "sterta szmat"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старая, изношенная или (в разговорной речи) любая одежда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шмотки",
                                "target_lang_sense_clarification": "одежда (слэнг)"
                            },
                            {
                                "target_lang_word": "тряпки",
                                "target_lang_sense_clarification": "одежда, наряды (разг., часто пренебр.)"
                            },
                            {
                                "target_lang_word": "старьё",
                                "target_lang_sense_clarification": "старые вещи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "217c8efd-5851-3d2b-a8d0-832e9f1708a5",
                    "sense_definition": "Wulgarne, obraźliwe określenie kobiety uważanej za niemoralną lub rozwiązłą seksualnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie nazywaj jej <szmatą>, to bardzo niekulturalne.",
                            "target_lang_translations": {
                                "ru": "Не называй её <шлюхой>, это очень некультурно."
                            }
                        },
                        {
                            "text": "Po kłótni wyzywał swoją byłą dziewczynę od <szmat>.",
                            "target_lang_translations": {
                                "ru": "После ссоры он обзывал свою бывшую девушку <шалавой>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kurwa",
                        "dziwka",
                        "reg. śl. gónka"
                    ],
                    "common_phrases": [
                        "głupia szmata"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar, offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное, оскорбительное название женщины, считающейся аморальной или распутной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлюха",
                                "target_lang_sense_clarification": "вульгарное оскорбление"
                            },
                            {
                                "target_lang_word": "шалава",
                                "target_lang_sense_clarification": "грубое сленговое оскорбление"
                            },
                            {
                                "target_lang_word": "подстилка",
                                "target_lang_sense_clarification": "уничижительное оскорбление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b740e80d-af0a-31a0-b979-acf4cf64ced8",
                    "sense_definition": "Ocena niedostateczna w szkole (stopień pierwszy).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "school_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znowu dostałem <szmatę> z matematyki, rodzice będą źli.",
                            "target_lang_translations": {
                                "ru": "Я снова получил <кол> по математике, родители будут злиться."
                            }
                        },
                        {
                            "text": "Muszę poprawić tę <szmatę>, żeby zdać do następnej klasy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно исправить этот <кол>, чтобы перейти в следующий класс."
                            }
                        }
                    ],
                    "synonyms": [
                        "pała",
                        "jedynka",
                        "lufa",
                        "laga"
                    ],
                    "common_phrases": [
                        "dostać szmatę",
                        "poprawić szmatę",
                        "zarobić szmatę"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "student slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неудовлетворительная оценка в школе (единица)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кол",
                                "target_lang_sense_clarification": "оценка «единица» (школьный слэнг)"
                            },
                            {
                                "target_lang_word": "пара",
                                "target_lang_sense_clarification": "оценка «двойка»/неуд (школьный слэнг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "affeaf8c-394d-3ced-a877-9a233dd72bd7",
                    "sense_definition": "Pogardliwe określenie człowieka bez charakteru, godności lub zasad moralnych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten polityk to sprzedajna <szmata>, która kłamie dla pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Этот политик — продажная <шкура>, которая лжёт ради денег."
                            }
                        },
                        {
                            "text": "Zachował się jak zwykła <szmata>, zostawiając rodzinę w potrzebie.",
                            "target_lang_translations": {
                                "ru": "Он повёл себя как последняя <тряпка>, бросив семью в беде."
                            }
                        }
                    ],
                    "synonyms": [
                        "szmaciarz",
                        "ścierwo"
                    ],
                    "common_phrases": [
                        "ludzka szmata",
                        "zwykła szmata"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar, offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное название человека без характера, достоинства или моральных принципов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тряпка",
                                "target_lang_sense_clarification": "слабовольный человек"
                            },
                            {
                                "target_lang_word": "ничтожество",
                                "target_lang_sense_clarification": "человек без достоинства"
                            },
                            {
                                "target_lang_word": "продажная шкура",
                                "target_lang_sense_clarification": "беспринципный, продажный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd947517-b900-3962-b735-7318bd68bedd",
                    "sense_definition": "Gazeta lub czasopismo o niskim poziomie merytorycznym, często brukowiec.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie czytaj tej <szmaty>, tam piszą same kłamstwa o celebrytach.",
                            "target_lang_translations": {
                                "ru": "Не читай эту <газетёнку>, там пишут одну ложь о знаменитостях."
                            }
                        },
                        {
                            "text": "To zwykła <szmata>, a nie rzetelne dziennikarstwo.",
                            "target_lang_translations": {
                                "ru": "Это обычная <жёлтая пресса>, а не честная журналистика."
                            }
                        }
                    ],
                    "synonyms": [
                        "szmatławiec"
                    ],
                    "common_phrases": [
                        "brukowa szmata",
                        "czytać szmatę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Газета или журнал низкого качества, часто бульварная пресса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газетёнка",
                                "target_lang_sense_clarification": "пренебрежительно о газете"
                            },
                            {
                                "target_lang_word": "жёлтая пресса",
                                "target_lang_sense_clarification": "бульварная пресса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa7dcbf6-f9a7-3198-ba65-9c9f78533a46",
                    "sense_definition": "W piłce nożnej: lekki, łatwy do obrony strzał, który został przepuszczony przez bramkarza w wyniku błędu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz puścił straszną <szmatę> w ostatniej minucie meczu.",
                            "target_lang_translations": {
                                "ru": "Вратарь пустил ужасную <бабочку> на последней минуте матча."
                            }
                        },
                        {
                            "text": "Kibice gwizdali, gdy padła taka <szmata>.",
                            "target_lang_translations": {
                                "ru": "Болельщики свистели, когда залетела такая <бабочка>."
                            }
                        }
                    ],
                    "synonyms": [
                        "babol",
                        "kartofel"
                    ],
                    "common_phrases": [
                        "puścić szmatę",
                        "wpuścić szmatę"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sports slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В футболе: слабый, легкий для защиты удар, пропущенный вратарем из-за ошибки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабочка",
                                "target_lang_sense_clarification": "лёгкий гол, пропущенный вратарём (спорт. слэнг)"
                            },
                            {
                                "target_lang_word": "пенка",
                                "target_lang_sense_clarification": "грубая ошибка вратаря (спорт. слэнг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "813733a3-da34-3fe1-a33a-af0ace4c42df",
                    "sense_definition": "Żagle na statku lub jachcie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan kazał postawić wszystkie <szmaty>, żeby płynąć szybciej.",
                            "target_lang_translations": {
                                "ru": "Капитан приказал поднять все <паруса>, чтобы плыть быстрее."
                            }
                        },
                        {
                            "text": "Przy tak silnym wietrze musieliśmy zrzucić <szmaty>.",
                            "target_lang_translations": {
                                "ru": "При таком сильном ветре нам пришлось спустить <тряпки>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ożaglowanie"
                    ],
                    "common_phrases": [
                        "stawiać szmaty",
                        "zrzucić szmaty"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "nautical slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Паруса на корабле или яхте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паруса",
                                "target_lang_sense_clarification": "оснастка судна"
                            },
                            {
                                "target_lang_word": "тряпки",
                                "target_lang_sense_clarification": "паруса (морской жаргон)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
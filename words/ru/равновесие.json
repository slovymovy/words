    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f790f820-56fb-3948-b2fa-4c726e500006",
                    "sense_definition": "Способность сохранять устойчивое вертикальное положение тела, не падать и не шататься при стоянии или движении.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок учится ходить и часто теряет <w>равновесие</w>.",
                            "target_lang_translations": {
                                "en": "The child is learning to walk and often loses <w>balance</w>."
                            }
                        },
                        {
                            "text": "Гимнаст сумел удержать <w>равновесие</w> на одной ноге.",
                            "target_lang_translations": {
                                "en": "The gymnast managed to keep his <w>balance</w> on one leg."
                            }
                        }
                    ],
                    "synonyms": [
                        "баланс"
                    ],
                    "common_phrases": [
                        "потерять равновесие",
                        "удержать равновесие",
                        "сохранять равновесие",
                        "чувство равновесия"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to maintain a stable vertical position of the body, not falling or wobbling when standing or moving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "balance",
                                "target_lang_sense_clarification": "physical stability"
                            },
                            {
                                "target_lang_word": "equilibrium",
                                "target_lang_sense_clarification": "state of physical stability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "213a5e3c-4489-3ade-82f9-1dd5bc45634f",
                    "sense_definition": "Состояние душевного покоя, спокойствия и уверенности в себе.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После отпуска она наконец обрела душевное <w>равновесие</w>.",
                            "target_lang_translations": {
                                "en": "After the vacation, she finally found mental <w>balance</w>."
                            }
                        },
                        {
                            "text": "Грубый ответ коллеги вывел меня из <w>равновесия</w> на весь день.",
                            "target_lang_translations": {
                                "en": "The colleague's rude answer threw me off <w>balance</w> for the whole day."
                            }
                        }
                    ],
                    "synonyms": [
                        "спокойствие",
                        "покой",
                        "самообладание",
                        "уравновешенность",
                        "душевный покой"
                    ],
                    "antonyms": [
                        "беспокойство",
                        "неуравновешенность",
                        "расстройство"
                    ],
                    "common_phrases": [
                        "душевное равновесие",
                        "вывести из равновесия",
                        "психическое равновесие",
                        "вернуть равновесие"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of mental peace, calmness, and self-confidence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "balance",
                                "target_lang_sense_clarification": "mental stability"
                            },
                            {
                                "target_lang_word": "equilibrium",
                                "target_lang_sense_clarification": "mental composure"
                            },
                            {
                                "target_lang_word": "composure",
                                "target_lang_sense_clarification": "self-control"
                            },
                            {
                                "target_lang_word": "peace of mind",
                                "target_lang_sense_clarification": "inner calm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f778cfe4-b39f-323c-b621-7f2de777cc3d",
                    "sense_definition": "Состояние физической или химической системы, при котором воздействие противоположных сил или процессов взаимно погашается.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весы пришли в <w>равновесие</w>, когда на обе чаши положили одинаковый груз.",
                            "target_lang_translations": {
                                "en": "The scales came into <w>equilibrium</w> when an identical load was placed on both pans."
                            }
                        },
                        {
                            "text": "В замкнутой системе со временем устанавливается тепловое <w>равновесие</w>.",
                            "target_lang_translations": {
                                "en": "In a closed system, thermal <w>equilibrium</w> is established over time."
                            }
                        }
                    ],
                    "synonyms": [
                        "баланс"
                    ],
                    "antonyms": [
                        "дисбаланс"
                    ],
                    "common_phrases": [
                        "состояние равновесия",
                        "устойчивое равновесие",
                        "термодинамическое равновесие",
                        "химическое равновесие"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of a physical or chemical system in which the action of opposing forces or processes is mutually cancelled out."
                    },
                    "translations": {
                        "en": [

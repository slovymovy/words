                },
                {
                    "sense_id": "f308e442-e9cd-3bd3-b369-7b8b8d4fb1dd",
                    "sense_definition": "Opalać się na słońcu lub przebywać w dużym upale.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "body_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię <w>piec się</w> na plaży przez wiele godzin.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <w>жариться</w> на пляже по многу часов."
                            }
                        },
                        {
                            "text": "Turysci <w>piekli się</w> w pełnym słońcu nad basenem.",
                            "target_lang_translations": {
                                "ru": "Туристы <w>жарились</w> на солнцепеке у бассейна."
                            }
                        }
                    ],
                    "synonyms": [
                        "opalać się",
                        "smażyć się"
                    ],
                    "common_phrases": [

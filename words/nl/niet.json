            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fef10fe3-639b-33c8-9769-f245b7029d8c",
                    "sense_definition": "Wordt gebruikt om een ontkenning uit te drukken; het tegenovergestelde van 'wel'.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet <w>niet</w> hoe laat de trein precies vertrekt.",
                            "target_lang_translations": {
                                "en": "I do <w>not</w> know exactly what time the train leaves.",
                                "ru": "Я <w>не</w> знаю, во сколько именно отправляется поезд."
                            }
                        },
                        {
                            "text": "Dat is helemaal <w>niet</w> waar wat je nu zegt.",
                            "target_lang_translations": {
                                "en": "That is <w>not</w> true at all what you are saying now.",
                                "ru": "То, что ты сейчас говоришь, совсем <w>не</w> правда."
                            }
                        }
                    ],
                    "antonyms": [
                        "wel"
                    ],
                    "common_phrases": [
                        "helemaal niet",
                        "nog niet",
                        "niet waar",
                        "ook niet",
                        "niet meer"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express a negation; the opposite of 'wel'.",
                        "ru": "Используется для выражения отрицания; противоположность 'wel' (утверждению)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "not",
                                "target_lang_sense_clarification": "negation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "отрицательная частица"
                            }
                        ]
                    }
                }
            ]
        },

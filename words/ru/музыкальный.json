    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "63c5b8b7-e11c-365e-ad42-a887e5341b99",
                    "sense_definition": "Связанный с музыкой, относящийся к ней, предназначенный для неё или состоящий из неё.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой младший брат уже три года ходит в <w>музыкальную</w> школу.",
                            "target_lang_translations": {
                                "en": "My younger brother has been going to <w>music</w> school for three years."
                            }
                        },
                        {
                            "text": "На день рождения ей подарили новый <w>музыкальный</w> инструмент.",
                            "target_lang_translations": {
                                "en": "For her birthday, she was given a new <w>musical</w> instrument."
                            }
                        }
                    ],
                    "antonyms": [
                        "антимузыкальный"
                    ],
                    "common_phrases": [
                        "музыкальный инструмент",
                        "музыкальная школа",
                        "музыкальный театр",
                        "музыкальное образование",
                        "музыкальный слух"
                    ],
                    "target_lang_definitions": {
                        "en": "Connected with music, relating to it, intended for it, or consisting of it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "musical",
                                "target_lang_sense_clarification": "relating to music"
                            },
                            {
                                "target_lang_word": "music",
                                "target_lang_sense_clarification": "used attributively (e.g., music school)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c577961-6f45-3f07-8a40-ca65dd4fe6bd",
                    "sense_definition": "Обладающий способностью тонко чувствовать, понимать или исполнять музыку; одарённый.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "qualitative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она очень <w>музыкальная</w> девочка и схватывает мелодии на лету.",
                            "target_lang_translations": {
                                "en": "She is a very <w>musical</w> girl and picks up melodies on the fly."
                            }
                        },
                        {
                            "text": "Вся их семья удивительно <w>музыкальная</w>, они часто поют вместе.",
                            "target_lang_translations": {
                                "en": "Their whole family is surprisingly <w>musical</w>; they often sing together."
                            }
                        }
                    ],
                    "antonyms": [
                        "антимузыкальный"
                    ],
                    "common_phrases": [
                        "музыкальный ребёнок",
                        "музыкальная семья",
                        "музыкальный человек"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Possessing the ability to feel, understand, or perform music well; gifted."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "musical",
                                "target_lang_sense_clarification": "gifted in music"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b87e14a-c58a-3e3e-96aa-3c8be1990ed4",
                    "sense_definition": "Приятный для слуха, напоминающий звучание музыки; мелодичный.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "qualitative_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У неё был очень мягкий и <w>музыкальный</w> голос.",
                            "target_lang_translations": {
                                "en": "She had a very soft and <w>musical</w> voice."
                            }
                        },
                        {
                            "text": "В тишине леса мы услышали <w>музыкальный</w> звон ручья.",
                            "target_lang_translations": {
                                "en": "In the silence of the forest, we heard the <w>musical</w> sound of the stream."
                            }
                        }
                    ],
                    "synonyms": [
                        "мелодичный"
                    ],
                    "antonyms": [
                        "скрипучий",
                        "резкий"
                    ],
                    "common_phrases": [
                        "музыкальный голос",
                        "музыкальный смех",
                        "музыкальный звон"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Pleasing to the ear, resembling the sound of music; melodious."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "musical",

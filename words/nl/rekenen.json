                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "count",
                                "target_lang_sense_clarification": "to include in a group or category"
                            },
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "to regard or view as"
                            },
                            {
                                "target_lang_word": "reckon",
                                "target_lang_sense_clarification": "to consider or regard (often passive)"
                            }
                        ]
                    }

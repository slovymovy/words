                {
                    "sense_id": "29b1a3b4-01e8-3a35-9e16-24a1ddb92bec",
                    "sense_definition": "Doprowadzać czynność do końca; finalizować coś, co zostało zaczęte.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_finish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaraz <kończę> pracę i wracam do domu.",
                            "target_lang_translations": {
                                "ru": "Я сейчас <заканчиваю> работу и возвращаюсь домой."
                            }
                        },
                        {
                            "text": "Musimy <kończyć> ten projekt do jutra rana.",
                            "target_lang_translations": {
                                "ru": "Мы должны <заканчивать> этот проект к завтрашнему утру."
                            }
                        }
                    ],
                    "synonyms": [
                        "finalizować",
                        "dopełniać",
                        "zamykać"
                    ],
                    "antonyms": [
                        "zaczynać",
                        "rozpoczynać",
                        "inicjować"
                    ],
                    "common_phrases": [
                        "kończyć pracę",
                        "kończyć szkołę",
                        "kończyć studia",
                        "kończyć rozmowę"
                    ],
                    "target_lang_definitions": {

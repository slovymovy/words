    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba4cd701-ad64-3a06-b5d1-68406e953f0d",
                    "sense_definition": "Unsolicited bulk electronic messages.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "internet_junk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please check your <w>spam</w> folder if you do not see the email in your inbox.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте папку <w>спам</w>, если вы не видите письма во входящих."
                            }
                        },
                        {
                            "text": "I receive far too much <w>spam</w> advertising products I do not need.",
                            "target_lang_translations": {
                                "ru": "Я получаю слишком много <w>спама</w> с рекламой товаров, которые мне не нужны."
                            }
                        }
                    ],
                    "synonyms": [
                        "junk mail"
                    ],
                    "antonyms": [
                        "ham"
                    ],
                    "common_phrases": [
                        "spam folder",
                        "spam filter",
                        "mark as spam",
                        "email spam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Массовая рассылка нежелательных электронных сообщений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спам",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            },
                            {
                                "target_lang_word": "нежелательная почта",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "667fa9aa-ff95-35b1-97dd-8bd0e91ab8e7",
                    "sense_definition": "A type of tinned meat made mainly from ham.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meat_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During the camping trip, we ate fried <w>spam</w> and eggs for breakfast.",

            "senses": [
                {
                    "sense_id": "4a626354-b8f7-3e2b-84d3-dd8a5901053d",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>echoën</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>echo</w> zijn woorden omdat ik het volledig met hem eens ben.",
                            "target_lang_translations": {
                                "en": "I <w>echo</w> his words because I fully agree with him.",
                                "ru": "Я <w>вторю</w> его словам, потому что полностью с ним согласен."
                            }
                        },
                        {
                            "text": "Als ik in deze lege kamer praat, <w>echo</w> ik door de hele gang.",
                            "target_lang_translations": {
                                "en": "When I talk in this empty room, I <w>echo</w> through the whole hallway.",
                                "ru": "Когда я говорю в этой пустой комнате, мой голос <w>отдается эхом</w> по всему коридору."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'echoën' (to echo).",
                        "ru": "Форма первого лица единственного числа настоящего времени глагола echoën (вторить, отдаваться эхом)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "echo",
                                "target_lang_sense_clarification": "verb"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вторю",

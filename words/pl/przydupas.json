    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "63ed5178-5ff4-3b03-874e-92d3ee2ad54b",
                    "sense_definition": "Pogardliwe lub wulgarne określenie osoby, która gorliwie wysługuje się komuś ważniejszemu, jest mu ślepo posłuszna i schlebia mu dla własnych korzyści.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "servile_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor zawsze przychodzi na spotkania, a za nim podąża jego wierny <w>przydupas</w>.",
                            "target_lang_translations": {
                                "ru": "Директор всегда приходит на собрания, а за ним следует его верный <w>прихвостень</w>."
                            }
                        },
                        {
                            "text": "Nie zamierzam być twoim <w>przydupasem</w> i wykonywać za ciebie brudnej roboty.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь быть твоей <w>шестёркой</w> и делать за тебя грязную работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "sługus"
                    ],
                    "common_phrases": [
                        "wierny przydupas",
                        "partyjny przydupas"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w języku potocznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное или вульгарное название человека, который усердно прислуживает кому-то более важному, слепо подчиняется ему и льстит ради собственной выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жополиз",
                                "target_lang_sense_clarification": "вульгарно; о подхалиме"
                            },
                            {
                                "target_lang_word": "шестёрка",
                                "target_lang_sense_clarification": "жаргон; о человеке на побегушках"
                            },
                            {
                                "target_lang_word": "прихвостень",
                                "target_lang_sense_clarification": "пренебрежительно; о верном слуге или сообщнике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "128f7d85-21b1-3bfb-a2cd-6ec5fe2bf6a1",
                    "sense_definition": "Wulgarne określenie partnera seksualnego lub kochanka, często o wydźwięku lekceważącym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "lover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi plotkują, że ten młody mężczyzna to jej nowy <w>przydupas</w>.",
                            "target_lang_translations": {
                                "ru": "Соседи сплетничают, что этот молодой мужчина — её новый <w>хахаль</w>."
                            }
                        },
                        {
                            "text": "Zostawiła męża i wyjechała za granicę z jakimś bogatym <w>przydupasem</w>.",
                            "target_lang_translations": {
                                "ru": "Она бросила мужа и уехала за границу с каким-то богатым <w>хахалем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kochanek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное название сексуального партнера или любовника, часто с пренебрежительным оттенком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хахаль",
                                "target_lang_sense_clarification": "разговорное; ухажер или любовник"
                            },
                            {
                                "target_lang_word": "трахаль",
                                "target_lang_sense_clarification": "вульгарно; сексуальный партнер"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
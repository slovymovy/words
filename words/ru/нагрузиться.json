            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3869784e-78b4-318b-991b-c3ec9f3f089c",
                    "sense_definition": "Взять на себя груз, тяжесть для переноски или перевозки.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_load",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед походом мы основательно <нагрузились> рюкзаками и палатками.",
                            "target_lang_translations": {
                                "en": "Before the hike, we thoroughly <w>loaded ourselves up</w> with backpacks and tents."
                            }
                        },
                        {
                            "text": "Ослик <нагрузился> мешками так, что едва мог идти.",
                            "target_lang_translations": {
                                "en": "The little donkey <w>loaded itself up</w> with sacks so much that it could barely walk."
                            }
                        }
                    ],
                    "synonyms": [
                        "загрузиться"
                    ],
                    "antonyms": [
                        "разгрузиться"
                    ],
                    "common_phrases": [
                        "нагрузиться вещами",
                        "нагрузиться сумками"
                    ],
                    "target_lang_definitions": {
                        "en": "To take on a heavy load or weight for carrying or transporting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "load oneself up",
                                "target_lang_sense_clarification": "with heavy objects"
                            },
                            {
                                "target_lang_word": "burden oneself",
                                "target_lang_sense_clarification": "with a load"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e280c99-7ac5-36f8-a1ac-e14043bcc52b",
                    "sense_definition": "Выпить много спиртного, прийти в состояние сильного опьянения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intoxication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости быстро <нагрузились> вином и начали громко петь песни.",
                            "target_lang_translations": {
                                "en": "The guests quickly <w>got loaded</w> on wine and started singing songs loudly."
                            }
                        },
                        {
                            "text": "Он не хотел <нагрузиться> на корпоративе, поэтому пил только сок.",
                            "target_lang_translations": {
                                "en": "He didn't want to <w>get wasted</w> at the office party, so he only drank juice."
                            }
                        }
                    ],
                    "synonyms": [
                        "набраться",
                        "наклюкаться"
                    ],
                    "common_phrases": [
                        "сильно нагрузиться",
                        "нагрузиться алкоголем"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "сниженная лексика"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To drink a large amount of alcohol; to become heavily intoxicated."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get loaded",
                                "target_lang_sense_clarification": "slang for getting drunk"
                            },
                            {
                                "target_lang_word": "get wasted",
                                "target_lang_sense_clarification": "slang for becoming very intoxicated"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "voldoen",
        "voldoende"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "18caeb34-4ce2-36de-bcd4-6538a478b8c0",
                    "sense_definition": "Niet genoeg om aan de eisen, verwachtingen of het beoogde doel te voldoen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "insufficient_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is <w>onvoldoende</w> bewijs om de verdachte te kunnen veroordelen.",
                            "target_lang_translations": {
                                "en": "There is <w>insufficient</w> evidence to convict the suspect.",
                                "ru": "Существует <w>недостаточно</w> доказательств, чтобы осудить подозреваемого."
                            }
                        },
                        {
                            "text": "Door <w>onvoldoende</w> voorbereiding liep het project helaas vertraging op.",
                            "target_lang_translations": {
                                "en": "Due to <w>inadequate</w> preparation, the project unfortunately suffered delays.",
                                "ru": "Из-за <w>недостаточной</w> подготовки проект, к сожалению, задержался."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontoereikend"
                    ],
                    "antonyms": [
                        "voldoende"
                    ],
                    "common_phrases": [
                        "onvoldoende saldo",
                        "onvoldoende ruimte",
                        "onvoldoende middelen",
                        "onvoldoende geacht"
                    ],
                    "target_lang_definitions": {
                        "en": "Not enough to meet requirements, expectations, or the intended purpose.",
                        "ru": "Недостаточный для того, чтобы соответствовать требованиям, ожиданиям или намеченной цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insufficient",
                                "target_lang_sense_clarification": "formal, general use regarding quantity or quality"
                            },
                            {
                                "target_lang_word": "inadequate",
                                "target_lang_sense_clarification": "not capable or good enough for a specific purpose"
                            },
                            {
                                "target_lang_word": "not enough",
                                "target_lang_sense_clarification": "general, less formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "недостаточный",
                                "target_lang_sense_clarification": "мало по количеству или качеству"
                            },
                            {
                                "target_lang_word": "неудовлетворительный",
                                "target_lang_sense_clarification": "не отвечающий требованиям"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88e58b05-a58a-3163-bece-424ad9faedd9",
                    "sense_definition": "Een cijfer of beoordeling die aangeeft dat een prestatie niet goed genoeg is (meestal lager dan de norm).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "failing_grade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij baalde enorm toen hij zag dat hij een <w>onvoldoende</w> had gehaald.",
                            "target_lang_translations": {
                                "en": "He was terribly bummed when he saw he had gotten a <w>failing grade</w>.",
                                "ru": "Он очень расстроился, когда увидел, что получил <w>неудовлетворительную оценку</w>."
                            }
                        },
                        {
                            "text": "Met drie <w>onvoldoendes</w> op zijn rapport gaat hij waarschijnlijk niet over.",
                            "target_lang_translations": {
                                "en": "With three <w>fails</w> on his report card, he probably won't pass the year.",
                                "ru": "С тремя <w>неудами</w> в табеле он, вероятно, не перейдет в следующий класс."
                            }
                        }
                    ],
                    "antonyms": [
                        "voldoende"
                    ],
                    "common_phrases": [
                        "een onvoldoende halen",
                        "een onvoldoende staan",
                        "een zware onvoldoende"
                    ],
                    "target_lang_definitions": {
                        "en": "A grade or assessment indicating that a performance is not good enough (usually below the standard).",
                        "ru": "Оценка или балл, указывающие на то, что результат работы ниже установленной нормы (незачет)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "failing grade",
                                "target_lang_sense_clarification": "school mark below passing"
                            },
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "academic result"
                            },
                            {
                                "target_lang_word": "unsatisfactory mark",
                                "target_lang_sense_clarification": "formal assessment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неудовлетворительная оценка",
                                "target_lang_sense_clarification": "официальное название плохой оценки"
                            },
                            {
                                "target_lang_word": "неуд",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            },
                            {
                                "target_lang_word": "двойка",
                                "target_lang_sense_clarification": "культурный эквивалент плохой оценки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

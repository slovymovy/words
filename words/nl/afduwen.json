            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4e037bec-ba56-3a41-84c1-63d4b63ecb90",
                    "sense_definition": "Een vaartuig of voorwerp van de kant wegduwen om te vertrekken.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten de boot nu <w>afduwen</w> om te kunnen vertrekken.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>оттолкнуть</w> лодку сейчас, чтобы отправиться.",
                                "en": "We need to <w>push off</w> the boat now to be able to leave."
                            }
                        },
                        {
                            "text": "Hij hielp ons bij het <w>afduwen</w> van het vlot.",
                            "target_lang_translations": {
                                "ru": "Он помог нам <w>оттолкнуть</w> плот.",
                                "en": "He helped us <w>push off</w> the raft."
                            }
                        }
                    ],
                    "synonyms": [
                        "afzetten"
                    ],
                    "common_phrases": [
                        "de boot afduwen",
                        "van de wal afduwen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отталкивать судно или предмет от берега, чтобы отплыть.",
                        "en": "To push a vessel or object away from the shore in order to depart."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отталкивать",
                                "target_lang_sense_clarification": "от берега"
                            },
                            {
                                "target_lang_word": "отпихивать",
                                "target_lang_sense_clarification": "разг."
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "push off",
                                "target_lang_sense_clarification": "a boat or vessel"
                            },
                            {
                                "target_lang_word": "shove off",
                                "target_lang_sense_clarification": "from shore"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2eeced97-1f44-3d7f-8b3b-3a183acfe8c1",
                    "sense_definition": "Iets of iemand met kracht naar beneden duwen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag niemand van de trap <w>afduwen</w>.",
                            "target_lang_translations": {
                                "ru": "Нельзя никого <w>сталкивать</w> с лестницы.",
                                "en": "You must not <w>push</w> anyone <w>down</w> the stairs."
                            }
                        },
                        {
                            "text": "Ze wilden de zware steen de helling <w>afduwen</w>.",
                            "target_lang_translations": {
                                "ru": "Они хотели <w>столкнуть</w> тяжелый камень со склона.",
                                "en": "They wanted to <w>push</w> the heavy stone <w>down</w> the slope."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de trap afduwen"
                    ],
                    "target_lang_definitions": {
                        "ru": "С силой толкать кого-либо или что-либо вниз.",
                        "en": "To push someone or something down or off a surface with force."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сталкивать",
                                "target_lang_sense_clarification": "вниз"
                            },
                            {
                                "target_lang_word": "спихивать",
                                "target_lang_sense_clarification": "разг."
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "push down",
                                "target_lang_sense_clarification": "from a height"
                            },
                            {
                                "target_lang_word": "push off",
                                "target_lang_sense_clarification": "from an edge"
                            },
                            {
                                "target_lang_word": "shove down",
                                "target_lang_sense_clarification": "forcefully"
                            }
                        ]
                    }
                }
            ]
        }
    ]

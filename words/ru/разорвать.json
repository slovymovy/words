    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "32c1a09b-6227-30a9-8c38-ad927d1fac12",
                    "sense_definition": "Рывком разделить на части или сделать дырявым (бумагу, ткань, предмет).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_tearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В порыве гнева он схватил письмо и <w>разорвал</w> его на мелкие кусочки.",
                            "target_lang_translations": {
                                "en": "In a fit of rage, he grabbed the letter and <w>tore</w> it into small pieces."
                            }
                        },
                        {
                            "text": "Я случайно зацепился рукавом за гвоздь и <w>разорвал</w> свою новую куртку.",
                            "target_lang_translations": {
                                "en": "I accidentally caught my sleeve on a nail and <w>ripped</w> my new jacket."
                            }
                        }
                    ],
                    "synonyms": [
                        "порвать",
                        "изодрать"
                    ],
                    "antonyms": [
                        "склеить",
                        "сшить",
                        "соединить"
                    ],
                    "common_phrases": [
                        "разорвать на куски",
                        "разорвать письмо",
                        "разорвать одежду",
                        "разорвать в клочья"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull apart into pieces or make a hole in (paper, fabric, object)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "general meaning, to pull apart"
                            },
                            {
                                "target_lang_word": "rip",
                                "target_lang_sense_clarification": "often implies speed, force, or fabric"
                            },
                            {
                                "target_lang_word": "tear up",
                                "target_lang_sense_clarification": "to tear completely into pieces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4665a242-dd46-3190-b822-a5bdd8872368",
                    "sense_definition": "Резко прекратить личные отношения, связи или действие официальной договоренности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой ссоры они решили окончательно <w>разорвать</w> отношения.",
                            "target_lang_translations": {
                                "en": "After a long argument, they decided to finally <w>break off</w> their relationship."
                            }
                        },
                        {
                            "text": "Компания планирует <w>разорвать</w> контракт с ненадежным поставщиком в следующем месяце.",
                            "target_lang_translations": {
                                "en": "The company plans to <w>terminate</w> the contract with the unreliable supplier next month."
                            }
                        }
                    ],
                    "synonyms": [
                        "прекратить",
                        "прервать"
                    ],
                    "antonyms": [
                        "восстановить",
                        "наладить",
                        "заключить"
                    ],
                    "common_phrases": [
                        "разорвать отношения",
                        "разорвать контракт",
                        "разорвать дипломатические связи",
                        "разорвать брак"
                    ],
                    "target_lang_definitions": {
                        "en": "To abruptly end personal relationships, connections, or official agreements."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break off",
                                "target_lang_sense_clarification": "relationships or engagements"
                            },
                            {
                                "target_lang_word": "sever",
                                "target_lang_sense_clarification": "formal, ties or diplomatic relations"
                            },
                            {
                                "target_lang_word": "terminate",
                                "target_lang_sense_clarification": "contracts or official agreements"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "618a332b-b6a7-36e6-9b02-062db46d5263",
                    "sense_definition": "Загрызть, нанести тяжелые раны или убить (о хищных животных).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_tearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стая голодных волков может <w>разорвать</w> отбившуюся от стада овцу.",
                            "target_lang_translations": {
                                "en": "A pack of hungry wolves can <w>tear apart</w> a sheep that has strayed from the flock."
                            }
                        },
                        {
                            "text": "Охотник боялся, что разъяренный медведь <w>разорвёт</w> его собак.",
                            "target_lang_translations": {
                                "en": "The hunter was afraid that the enraged bear would <w>maul</w> his dogs."
                            }
                        }
                    ],
                    "synonyms": [
                        "растерзать",
                        "загрызть"
                    ],
                    "common_phrases": [
                        "волки разорвали",
                        "разорвать добычу",
                        "собаки разорвали"
                    ],
                    "target_lang_definitions": {
                        "en": "To maul, inflict severe wounds, or kill (referring to predatory animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear apart",
                                "target_lang_sense_clarification": "to kill or destroy violently"
                            },
                            {
                                "target_lang_word": "maul",
                                "target_lang_sense_clarification": "to wound seriously (by an animal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3883b393-a73a-3987-bf10-c445c44a0366",
                    "sense_definition": "Разрушить или разнести на части силой взрыва или внутреннего давления.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если не слить воду на зиму, лед может <w>разорвать</w> водопроводные трубы.",
                            "target_lang_translations": {
                                "en": "If you don't drain the water for the winter, ice can <w>burst</w> the water pipes."
                            }
                        },
                        {
                            "text": "Прямое попадание артиллерийского снаряда может <w>разорвать</w> автомобиль на части.",
                            "target_lang_translations": {
                                "en": "A direct hit from an artillery shell can <w>blow</w> the car apart."
                            }
                        }
                    ],
                    "synonyms": [
                        "взорвать",
                        "разнести"
                    ],
                    "common_phrases": [
                        "разорвать на части",
                        "разорвать взрывом",
                        "трубу разорвало"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy or blow to pieces by the force of an explosion or internal pressure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burst",
                                "target_lang_sense_clarification": "due to internal pressure (pipes, containers)"
                            },
                            {
                                "target_lang_word": "blow apart",
                                "target_lang_sense_clarification": "by explosion"
                            },
                            {
                                "target_lang_word": "rupture",
                                "target_lang_sense_clarification": "technical context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a26c314-af83-3dac-bf1f-5535a2aa62a7",
                    "sense_definition": "Внезапно и резко нарушить (тишину, мрак и т.п.).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Громкий телефонный звонок неожиданно <w>разорвал</w> ночную тишину.",
                            "target_lang_translations": {
                                "en": "A loud phone ring unexpectedly <w>shattered</w> the night's silence."
                            }
                        },
                        {
                            "text": "Яркий луч прожектора <w>разорвал</w> густой туман над рекой.",
                            "target_lang_translations": {
                                "en": "A bright searchlight beam <w>pierced</w> the thick fog over the river."
                            }
                        }
                    ],
                    "synonyms": [
                        "нарушить",
                        "пронзить"
                    ],
                    "common_phrases": [
                        "разорвать тишину",
                        "разорвать мрак",
                        "крик разорвал тишину"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly and sharply interrupt (silence, darkness, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "silence or peace"
                            },
                            {
                                "target_lang_word": "pierce",
                                "target_lang_sense_clarification": "darkness, fog, or silence (by sound/light)"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "silence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "ru": [
                            {
                                "target_lang_word": "уж",
                                "target_lang_sense_clarification": "констатация факта (nou eenmaal)"
                            },
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "смягчение или очевидность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "47e82ec9-3c14-3f65-978e-dee1ffd9762d",
                    "sense_definition": "Wordt gebruikt om een zin te beginnen, iemand aan te sporen of een gesprek af te ronden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "24268ac7-5a92-39de-b9fe-d5f1281c8894",
                    "sense_definition": "Вдвоём, без посторонних свидетелей; с глазу на глаз.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "privacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу поговорить с тобой <наедине>, чтобы никто нам не мешал.",
                            "target_lang_translations": {
                                "en": "I want to talk to you <in private> so that no one disturbs us."
                            }
                        },
                        {
                            "text": "Они остались <наедине> в комнате и долго обсуждали планы.",
                            "target_lang_translations": {
                                "en": "They remained <alone> in the room and discussed plans for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "тет-а-тет",
                        "с глазу на глаз"
                    ],
                    "common_phrases": [
                        "остаться наедине",
                        "поговорить наедине",
                        "встретиться наедине"
                    ],
                    "target_lang_definitions": {
                        "en": "In private, without witnesses; just the two of you."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in private",
                                "target_lang_sense_clarification": "without others present"
                            },
                            {
                                "target_lang_word": "alone",
                                "target_lang_sense_clarification": "just the two of you"
                            },
                            {
                                "target_lang_word": "privately",
                                "target_lang_sense_clarification": "confidentially"
                            },
                            {
                                "target_lang_word": "tête-à-tête",
                                "target_lang_sense_clarification": "face-to-face conversation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43c9f901-fb9b-3b85-b2ee-a8ee955e1818",
                    "sense_definition": "В полном одиночестве, изолированно от других людей (часто в сочетании с «с собой», «с мыслями» и т. п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "solitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Иногда каждому человеку полезно побыть <наедине> с собой.",
                            "target_lang_translations": {
                                "en": "Sometimes it is good for everyone to be <alone> with themselves."
                            }
                        },
                        {
                            "text": "Писатель любил работать по ночам, оставаясь <наедине> со своими героями.",
                            "target_lang_translations": {
                                "en": "The writer loved working at night, remaining <alone> with his characters."
                            }
                        }
                    ],
                    "antonyms": [
                        "вместе"
                    ],
                    "common_phrases": [
                        "наедине с собой",
                        "наедине с мыслями",
                        "наедине с природой"
                    ],
                    "target_lang_definitions": {
                        "en": "In complete solitude, isolated from others (often used with 'with oneself', 'with thoughts', etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "alone",
                                "target_lang_sense_clarification": "in solitude"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bc12422-81d5-31c1-b9a8-9190eaa5afb9",
                    "sense_definition": "То, что заготовлено и собрано для использования в будущем (продукты, материалы, деньги).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы сделали большой <w>запас</w> продуктов на всю зиму.",
                            "target_lang_translations": {
                                "en": "We laid in a large <w>stock</w> of food for the whole winter."
                            }
                        },
                        {
                            "text": "У этой машины хороший <w>запас</w> хода без дозаправки.",
                            "target_lang_translations": {
                                "en": "This car has a good <w>range</w> without refueling."
                            }
                        }
                    ],
                    "synonyms": [
                        "резерв"
                    ],
                    "common_phrases": [
                        "про запас",
                        "с запасом",
                        "запас прочности",
                        "золотой запас"
                    ],
                    "target_lang_definitions": {
                        "en": "Something prepared and collected for future use (food, materials, money)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stock",
                                "target_lang_sense_clarification": "available quantity of goods"
                            },
                            {
                                "target_lang_word": "supply",
                                "target_lang_sense_clarification": "amount available for use"
                            },
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "amount kept for future use or emergency"
                            },
                            {
                                "target_lang_word": "stockpile",
                                "target_lang_sense_clarification": "large accumulated supply"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30ee5fee-5d73-3ac1-b018-9b1ed769ed37",
                    "sense_definition": "Состояние военнообязанных граждан, которые прошли службу и могут быть призваны в армию в случае необходимости.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После окончания службы солдата уволили в <w>запас</w>.",
                            "target_lang_translations": {
                                "en": "After finishing service, the soldier was discharged to the <w>reserve</w>."
                            }
                        },
                        {
                            "text": "Мой отец — офицер <w>запаса</w>.",
                            "target_lang_translations": {
                                "en": "My father is a <w>reserve</w> officer."
                            }
                        }
                    ],
                    "synonyms": [
                        "резерв"
                    ],
                    "common_phrases": [
                        "уволить в запас",
                        "офицер запаса",
                        "в запасе"
                    ],
                    "target_lang_definitions": {
                        "en": "The status of citizens liable for military service who have served and can be called up if needed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "military force not currently on active duty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83eaa1c6-b198-3b2f-ae99-d8dcd263478b",
                    "sense_definition": "Излишек ткани у шва, оставленный портным для возможного увеличения размера одежды.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Портной оставил большой <w>запас</w> в талии брюк.",
                            "target_lang_translations": {
                                "en": "The tailor left a large <w>allowance</w> in the waist of the trousers."
                            }
                        },
                        {
                            "text": "Этот <w>запас</w> позволит удлинить рукава пальто.",
                            "target_lang_translations": {
                                "en": "This <w>allowance</w> will allow the coat sleeves to be lengthened."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Excess fabric at a seam left by a tailor for possible size increase; a margin of safety."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allowance",
                                "target_lang_sense_clarification": "extra material for alteration"
                            },
                            {
                                "target_lang_word": "margin",
                                "target_lang_sense_clarification": "extra space or amount"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e7636d89-522e-3543-aee8-5528fbcb60e5",
                    "sense_definition": "Прошедшее время, мужской род, единственное число глагола <w>запасти</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он заранее <w>запас</w> дрова для камина.",
                            "target_lang_translations": {
                                "en": "He <w>stocked up</w> on firewood for the fireplace in advance."
                            }
                        },
                        {
                            "text": "Фермер <w>запас</w> достаточно сена для своих коров.",
                            "target_lang_translations": {
                                "en": "The farmer <w>stored</w> enough hay for his cows."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past tense, masculine, singular of the verb 'to stock up' or 'to store'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stocked up",
                                "target_lang_sense_clarification": "accumulated for future use"
                            },
                            {
                                "target_lang_word": "stored",
                                "target_lang_sense_clarification": "kept for future use"
                            }
                        ]
                    }
                }
            ]
        }
    ]

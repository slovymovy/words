    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3a498ff3-76db-3dcf-897d-d058d4f8f758",
                    "sense_definition": "Вне помещения, на улице, на открытом воздухе.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В доме было тепло, хотя <w>снаружи</w> шёл снег.",
                            "target_lang_translations": {
                                "en": "It was warm in the house, although it was snowing <w>outside</w>."
                            }
                        },
                        {
                            "text": "<w>Снаружи</w> было очень холодно, поэтому мы быстро вернулись домой.",
                            "target_lang_translations": {
                                "en": "It was very cold <w>outside</w>, so we quickly returned home."
                            }
                        }
                    ],
                    "synonyms": [
                        "на улице",
                        "под открытым небом"
                    ],
                    "antonyms": [
                        "внутри"
                    ],
                    "common_phrases": [
                        "ждать снаружи",
                        "стоять снаружи"
                    ],
                    "target_lang_definitions": {
                        "en": "Outside a building, outdoors, in the open air."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outside",
                                "target_lang_sense_clarification": "not in a building"
                            },
                            {
                                "target_lang_word": "outdoors",
                                "target_lang_sense_clarification": "in the open air"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "629d9bd7-9cfb-361f-a3d3-54a84712bf8a",
                    "sense_definition": "На внешней стороне или поверхности предмета (в противоположность внутренней).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Снаружи</w> кошелёк выглядел новым, но внутри был порван.",
                            "target_lang_translations": {
                                "en": "<w>On the outside</w> the wallet looked new, but inside it was torn."
                            }
                        },
                        {
                            "text": "Мы покрасили стены только <w>снаружи</w>.",
                            "target_lang_translations": {
                                "en": "We painted the walls only <w>on the outside</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "вовне"
                    ],
                    "antonyms": [
                        "внутри"
                    ],
                    "common_phrases": [
                        "вид снаружи",
                        "красивый снаружи"
                    ],
                    "target_lang_definitions": {
                        "en": "On the outer side or surface of an object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on the outside",
                                "target_lang_sense_clarification": "on the surface"
                            },
                            {
                                "target_lang_word": "externally",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04d9e9f8-defd-3d1d-b3dd-aea5acd9eeb3",
                    "sense_definition": "Со стороны внешнего пространства; по направлению от внешнего к внутреннему.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дверь была заперта <w>снаружи</w>, и мы не могли выйти.",
                            "target_lang_translations": {
                                "en": "The door was locked <w>from the outside</w>, and we couldn't get out."
                            }
                        },
                        {
                            "text": "Окно на первом этаже легко открывается <w>снаружи</w>.",
                            "target_lang_translations": {
                                "en": "The window on the ground floor opens easily <w>from the outside</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "извне"
                    ],
                    "antonyms": [
                        "изнутри"
                    ],
                    "common_phrases": [
                        "закрыть снаружи",
                        "открыть снаружи",
                        "доноситься снаружи"
                    ],
                    "target_lang_definitions": {
                        "en": "From the external side; directed from the outside inwards."

    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "d718dbf3-7346-3a1f-a3ec-117b642d5cf6",
                    "sense_definition": "Zich bevindend aan de binnenzijde of binnen de grenzen van een ruimte, plaats of object.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locatie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schone servies staat <w>in</w> de kast.",
                            "target_lang_translations": {
                                "en": "The clean crockery is <w>in</w> the cupboard."
                            }
                        },
                        {
                            "text": "Ik woon al meer dan twintig jaar <w>in</w> Utrecht.",
                            "target_lang_translations": {
                                "en": "I have lived <w>in</w> Utrecht for more than twenty years."
                            }
                        }
                    ],
                    "synonyms": [
                        "binnen"
                    ],
                    "antonyms": [
                        "buiten",
                        "uit"
                    ],
                    "common_phrases": [
                        "in huis",
                        "in de stad",
                        "in de buurt"
                    ],
                    "target_lang_definitions": {
                        "en": "Located inside or within the limits of a space, place, or object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "location/inclusion"
                            },
                            {
                                "target_lang_word": "inside",
                                "target_lang_sense_clarification": "contained by"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caa47007-2ed8-36eb-86f9-11041fbf6844",
                    "sense_definition": "Gedurende een tijdsbestek of op een specifiek moment binnen een periode.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tijd",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>In</w> de zomer dragen veel mensen een zonnebril.",
                            "target_lang_translations": {
                                "en": "<w>In</w> the summer, many people wear sunglasses."
                            }
                        },
                        {
                            "text": "Mijn broer werd geboren <w>in</w> 1985.",
                            "target_lang_translations": {
                                "en": "My brother was born <w>in</w> 1985."
                            }
                        }
                    ],
                    "synonyms": [
                        "tijdens",
                        "gedurende"
                    ],
                    "common_phrases": [
                        "in de ochtend",
                        "in het weekend",
                        "in de toekomst"
                    ],
                    "target_lang_definitions": {
                        "en": "During a period of time or at a specific moment within a period."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "during a period"
                            },
                            {
                                "target_lang_word": "during",
                                "target_lang_sense_clarification": "throughout the course of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6785d654-8974-339c-af26-7da88b6ad012",
                    "sense_definition": "Een beweging naar de binnenkant van iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "richting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de gewassen borden even <w>in</w> de kast.",
                            "target_lang_translations": {
                                "en": "Put the washed plates <w>in</w> the cupboard for a moment."
                            }
                        },
                        {
                            "text": "Hij stopte de brief snel <w>in</w> de envelop.",
                            "target_lang_translations": {
                                "en": "He quickly put the letter <w>in</w> the envelope."
                            }
                        }
                    ],
                    "antonyms": [
                        "uit"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ca822c8-6d4b-3970-b3ab-ec90e9eb4ddf",
                    "sense_definition": "A three-dimensional work of art, usually representing a person or animal, usually created by sculpting, carving, molding, or casting.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city decided to erect a <w>statue</w> in the main square.",
                            "target_lang_translations": {
                                "ru": "Город решил воздвигнуть <w>статую</w> на главной площади."
                            }
                        },
                        {
                            "text": "Tourists gathered to take photos of the famous bronze <w>statue</w>.",
                            "target_lang_translations": {
                                "ru": "Туристы собрались, чтобы сфотографировать знаменитую бронзовую <w>статую</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sculpture",
                        "figure"
                    ],
                    "common_phrases": [
                        "bronze statue",
                        "marble statue",
                        "erect a statue",
                        "statue of liberty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трёхмерное произведение искусства, обычно изображающее человека или животное, созданное путём лепки, резьбы или отливки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статуя",
                                "target_lang_sense_clarification": "скульптурное изображение фигуры"
                            },
                            {
                                "target_lang_word": "скульптура",
                                "target_lang_sense_clarification": "произведение искусства"
                            },
                            {
                                "target_lang_word": "изваяние",
                                "target_lang_sense_clarification": "книжное, поэтическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfce379c-781d-30ef-af34-71171a051ce2",
                    "sense_definition": "A portrait.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient text referred to the painting as a <w>statue</w> of the queen.",
                            "target_lang_translations": {
                                "ru": "Древний текст называл эту картину <w>портретом</w> королевы."
                            }
                        },
                        {
                            "text": "He described her image as a living <w>statue</w> of grace.",
                            "target_lang_translations": {
                                "ru": "Он описал её образ как живое <w>воплощение</w> грации."
                            }
                        }
                    ],
                    "synonyms": [
                        "portrait",
                        "likeness"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This sense is largely obsolete in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Портрет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портрет",
                                "target_lang_sense_clarification": "устаревшее значение"
                            },
                            {
                                "target_lang_word": "изображение",
                                "target_lang_sense_clarification": "подобие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a0108fb9-9771-379f-9bbe-fcb4cfb05d75",
                    "sense_definition": "To form a statue of; to make into a statue.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shock of the news <w>statued</w> him into silence.",
                            "target_lang_translations": {
                                "ru": "Шок от новости <w>окаменил</w> его, погрузив в молчание."
                            }
                        },
                        {
                            "text": "The artist <w>statued</w> the hero in solid marble.",
                            "target_lang_translations": {
                                "ru": "Художник <w>изваял</w> героя в цельном мраморе."
                            }
                        }
                    ],
                    "synonyms": [
                        "sculpt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать статую кого-либо; превращать в статую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изваять",
                                "target_lang_sense_clarification": "создать скульптуру"
                            },
                            {
                                "target_lang_word": "превращать в статую",
                                "target_lang_sense_clarification": "делать неподвижным"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "703fb9db-9d03-3ea1-926d-711fdcd785c9",
                    "sense_definition": "Ogół prac twórczych, artystycznych lub naukowych stworzonych przez daną osobę w ciągu jej życia lub okresu aktywności.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_output",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesor ma imponujący <w>dorobek</w> naukowy, obejmujący setki publikacji.",
                            "target_lang_translations": {
                                "ru": "У профессора внушительные научные <w>труды</w>, включающие сотни публикаций."
                            }
                        },
                        {
                            "text": "Charakteryzując sztukę nowoczesną, nie możemy zapominać o <w>dorobku</w> minionych pokoleń artystów.",
                            "target_lang_translations": {
                                "ru": "Характеризуя современное искусство, мы не можем забывать о <w>наследии</w> прошлых поколений художников."
                            }
                        }
                    ],
                    "synonyms": [
                        "dobytek"
                    ],
                    "common_phrases": [
                        "dorobek artystyczny",
                        "dorobek naukowy",
                        "dorobek literacki",
                        "bogaty dorobek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность творческих, художественных или научных работ, созданных человеком в течение жизни или периода деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследие",
                                "target_lang_sense_clarification": "совокупность работ, оставленных после себя"
                            },
                            {
                                "target_lang_word": "творчество",
                                "target_lang_sense_clarification": "в контексте искусства"
                            },
                            {
                                "target_lang_word": "труды",
                                "target_lang_sense_clarification": "в контексте науки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c8d2882-c14c-3864-adc4-55757bfee4fc",
                    "sense_definition": "Suma osiągnięć i wyników uzyskanych w jakiejś dziedzinie, na przykład w sporcie lub pracy zawodowej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_output",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drużyna powiększyła swój <w>dorobek</w> punktowy o kolejne zwycięstwo.",
                            "target_lang_translations": {
                                "ru": "Команда увеличила свой очковый <w>актив</w> благодаря очередной победе."
                            }
                        },
                        {
                            "text": "To medalowe miejsce jest najważniejszym sukcesem w dotychczasowym <w>dorobku</w> tego zawodnika.",
                            "target_lang_translations": {
                                "ru": "Это призовое место является самым важным успехом в нынешнем <w>послужном списке</w> этого спортсмена."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dorobek punktowy",
                        "dorobek medalowy",
                        "dorobek zawodowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма достижений и результатов, полученных в какой-либо области, например, в спорте или профессиональной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "актив",
                                "target_lang_sense_clarification": "совокупность достижений"
                            },
                            {
                                "target_lang_word": "послужной список",
                                "target_lang_sense_clarification": "история профессиональных достижений"
                            },
                            {
                                "target_lang_word": "копилка",
                                "target_lang_sense_clarification": "переносное значение, часто в спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "091c7560-f9b6-3755-b3cb-78d555e1bfac",
                    "sense_definition": "Majątek i dobra materialne zgromadzone przez kogoś dzięki własnej pracy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material_wealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pożar strawił cały <w>dorobek</w> ich życia w ciągu kilku godzin.",
                            "target_lang_translations": {
                                "ru": "Пожар за несколько часов уничтожил всё <w>нажитое</w> ими за всю жизнь."
                            }
                        },
                        {
                            "text": "Małżonkowie postanowili sądownie podzielić swój wspólny <w>dorobek</w>.",
                            "target_lang_translations": {
                                "ru": "Супруги решили в судебном порядке разделить своё общее <w>имущество</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dobytek"
                    ],
                    "common_phrases": [
                        "dorobek życia",
                        "cały dorobek",
                        "wspólny dorobek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имущество и материальные блага, накопленные кем-либо благодаря собственному труду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "имущество",
                                "target_lang_sense_clarification": "материальные блага"
                            },
                            {
                                "target_lang_word": "нажитое",
                                "target_lang_sense_clarification": "разговорное, всё заработанное трудом"
                            },
                            {
                                "target_lang_word": "состояние",
                                "target_lang_sense_clarification": "накопленное богатство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

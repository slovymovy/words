    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3745101c-113a-3cf6-ba9e-6ffe19e88dc3",
                    "sense_definition": "Iemand die oneerlijk handelt, regels omzeilt of fraudeert, vaak op een slinkse of kleinschalige manier.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "fraudster",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De belastingdienst probeert <w>sjoemelaars</w> op te sporen die hun inkomsten verzwijgen.",
                            "target_lang_translations": {
                                "ru": "Налоговая служба пытается выследить <w>махинаторов</w>, скрывающих свои доходы."
                            }
                        },
                        {
                            "text": "Hij staat bekend als een <w>sjoemelaar</w> die het niet zo nauw neemt met de regels.",
                            "target_lang_translations": {
                                "ru": "Он известен как <w>жулик</w>, который не слишком строго соблюдает правила."
                            }
                        }
                    ],
                    "synonyms": [
                        "oplichter",
                        "bedrieger",
                        "charlatan"
                    ],
                    "common_phrases": [
                        "sjoemelaars aanpakken",
                        "kleine sjoemelaar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто действует нечестно, обходит правила или совершает махинации, часто хитрым способом или в мелких масштабах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жулик",
                                "target_lang_sense_clarification": "общий термин для хитрого и нечестного человека"
                            },
                            {
                                "target_lang_word": "махинатор",
                                "target_lang_sense_clarification": "тот, кто проворачивает аферы или обходит правила"
                            },
                            {
                                "target_lang_word": "мошенник",
                                "target_lang_sense_clarification": "более формальный термин, подразумевающий обман"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3b611c2-3007-35fc-99d9-d34dd72643ff",
                    "sense_definition": "Iemand die vals speelt tijdens een kaartspel of ander spel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cheater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niemand wil nog met hem kaarten omdat hij een <w>sjoemelaar</w> is.",
                            "target_lang_translations": {
                                "ru": "Никто больше не хочет играть с ним в карты, потому что он <w>шулер</w>."
                            }
                        },
                        {
                            "text": "De <w>sjoemelaar</w> had stiekem een extra kaart in zijn mouw verstopt.",
                            "target_lang_translations": {
                                "ru": "<w>Шулер</w> тайком спрятал лишнюю карту в рукаве."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто играет нечестно в карты или другие игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шулер",
                                "target_lang_sense_clarification": "особенно в карточных играх"
                            },
                            {
                                "target_lang_word": "плут",
                                "target_lang_sense_clarification": "хитрец, обманщик в игре"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
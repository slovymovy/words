    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e22499fd-78b4-3d3c-9958-3bf2bd85be98",
                    "sense_definition": "The quality of being trusted and believed in.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal caused the government to lose <w>credibility</w> with the voters.",
                            "target_lang_translations": {
                                "ru": "Из-за скандала правительство потеряло <w>доверие</w> избирателей."
                            }
                        },
                        {
                            "text": "She has built a reputation for honesty and <w>credibility</w> over the years.",
                            "target_lang_translations": {
                                "ru": "За эти годы она заработала репутацию честности и <w>надежности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "believability",
                        "personal capital",
                        "personal capital"
                    ],
                    "antonyms": [
                        "unreliability",
                        "dishonesty"
                    ],
                    "common_phrases": [
                        "lose credibility",
                        "gain credibility",
                        "lack credibility",
                        "credibility gap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность вызывать доверие; качество человека или организации, на которых можно положиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доверие",
                                "target_lang_sense_clarification": "отношение, основанное на уверенности в чьей-либо честности и добросовестности"
                            },
                            {
                                "target_lang_word": "авторитет",
                                "target_lang_sense_clarification": "общепризнанное значение, влияние или уважение"
                            },
                            {
                                "target_lang_word": "надежность",
                                "target_lang_sense_clarification": "свойство оправдывать доверие"
                            },
                            {
                                "target_lang_word": "кредит доверия",
                                "target_lang_sense_clarification": "репутация, позволяющая действовать с поддержкой окружающих (часто в политике)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6041b0c-5678-3995-8a47-649ed634ce01",
                    "sense_definition": "The likelihood that a statement or witness is telling the truth, often judged by probability.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer questioned the <w>credibility</w> of the witness during the trial.",
                            "target_lang_translations": {
                                "ru": "Во время суда адвокат поставил под сомнение <w>правдивость</w> свидетеля."
                            }
                        },
                        {
                            "text": "His story was so strange that it really strained <w>credibility</w>.",
                            "target_lang_translations": {
                                "ru": "Его история была настолько странной, что в её <w>правдоподобие</w> верилось с трудом."
                            }
                        }
                    ],
                    "synonyms": [
                        "believability",
                        "personal capital"
                    ],
                    "antonyms": [
                        "implausibility"
                    ],
                    "common_phrases": [
                        "witness credibility",
                        "strain credibility",
                        "assess credibility"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство информации, заявления или показаний быть правдивыми и соответствовать истине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правдоподобие",
                                "target_lang_sense_clarification": "свойство выглядеть правдивым, похожим на правду"
                            },
                            {
                                "target_lang_word": "достоверность",
                                "target_lang_sense_clarification": "подлинность, соответствие фактам и истине"
                            },
                            {
                                "target_lang_word": "убедительность",
                                "target_lang_sense_clarification": "способность заставить поверить в истинность чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "венчать",
        "венчание",
        "увенчать",
        "венценосный",
        "венчик"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "faa622b9-7d3f-3b6c-9a08-8e881b33ac73",
                    "sense_definition": "Драгоценный головной убор, символ власти (корона) или сплетённые в кольцо листья и цветы (венок); в переносном смысле часто используется для обозначения церковного брачного обряда.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "headgear_symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Жених и невеста торжественно встали под <венец> в старинном соборе.",
                            "target_lang_translations": {
                                "en": "The bride and groom solemnly stood at the <altar> in the ancient cathedral."
                            }
                        },
                        {
                            "text": "На голове статуи был золотой <венец>, украшенный драгоценными камнями.",
                            "target_lang_translations": {
                                "en": "On the statue's head was a golden <crown> decorated with precious stones."
                            }
                        }
                    ],
                    "synonyms": [
                        "венок",
                        "обрамление"
                    ],
                    "common_phrases": [
                        "идти под венец",
                        "терновый венец",
                        "брачный венец"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "в значении обычной короны или венка"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "в значении бракосочетания (идти под венец)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A precious headpiece symbolizing power (crown) or a ring of woven leaves and flowers (wreath); figuratively refers to a church wedding ceremony."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crown",
                                "target_lang_sense_clarification": "headgear symbolizing power or royalty"
                            },
                            {
                                "target_lang_word": "wreath",
                                "target_lang_sense_clarification": "woven ring of flowers or leaves"
                            },
                            {
                                "target_lang_word": "wedding",
                                "target_lang_sense_clarification": "figurative reference to the marriage ceremony"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fb2dca7-a22c-3415-85f0-3f27a9866106",
                    "sense_definition": "Высшая ступень, лучшее и самое совершенное достижение чего-либо; успешное завершение дела.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "culmination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Человек традиционно считается <венцом> творения природы.",
                            "target_lang_translations": {
                                "en": "Man is traditionally considered the <crown> of nature's creation."
                            }
                        },
                        {
                            "text": "Этот великолепный роман стал <венцом> его литературной карьеры.",
                            "target_lang_translations": {
                                "en": "This magnificent novel became the <pinnacle> of his literary career."
                            }
                        }
                    ],
                    "synonyms": [
                        "вершина",
                        "апогей",
                        "зенит"
                    ],
                    "common_phrases": [
                        "венец творения",
                        "конец — делу венец"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "высокий стиль"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The highest stage, the best and most perfect achievement of something; successful completion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crown",
                                "target_lang_sense_clarification": "metaphorical highest point (e.g., crown of creation)"
                            },
                            {
                                "target_lang_word": "pinnacle",
                                "target_lang_sense_clarification": "highest level of achievement"
                            },
                            {
                                "target_lang_word": "culmination",
                                "target_lang_sense_clarification": "successful conclusion or climax"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ca947b1-5bfd-3d35-8330-4ff2c8284331",
                    "sense_definition": "Светлое кольцо или сияние вокруг небесного тела (Солнца, Луны) или источника света; нимб.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "light_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вокруг полной луны светился туманный <венец>.",
                            "target_lang_translations": {
                                "en": "A misty <halo> glowed around the full moon."
                            }
                        },
                        {
                            "text": "На иконе святой изображён с золотым <венцом> над головой.",
                            "target_lang_translations": {
                                "en": "In the icon, the saint is depicted with a golden <halo> over his head."
                            }
                        }
                    ],
                    "synonyms": [
                        "обрамление"
                    ],
                    "common_phrases": [
                        "солнечный венец",
                        "венец вокруг луны"
                    ],
                    "target_lang_definitions": {
                        "en": "A luminous ring or radiance around a celestial body (Sun, Moon) or light source; a nimbus."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "halo",
                                "target_lang_sense_clarification": "ring of light around the moon or a holy figure"
                            },
                            {
                                "target_lang_word": "corona",
                                "target_lang_sense_clarification": "astronomical ring of light"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdedd54d-4457-32c7-87fd-c49e49686701",
                    "sense_definition": "Один горизонтальный ряд брёвен в срубе деревянного дома.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители заменили нижний <венец> дома, так как он начал гнить.",
                            "target_lang_translations": {
                                "en": "The builders replaced the bottom <crown> of the house because it had started to rot."
                            }
                        },
                        {
                            "text": "Сруб бани состоит из двенадцати сосновых <венцов>.",
                            "target_lang_translations": {
                                "en": "The log frame of the bathhouse consists of twelve pine <tiers>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ярус"
                    ],
                    "common_phrases": [
                        "нижний венец",
                        "укладывать венцы"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "архитектурный термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A single horizontal row of logs in the framework of a wooden house."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crown",
                                "target_lang_sense_clarification": "architectural term for a row of logs"
                            },
                            {
                                "target_lang_word": "course",
                                "target_lang_sense_clarification": "horizontal layer in building construction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3b406a5-2174-3d75-8d2c-f94cf15fb256",
                    "sense_definition": "Деталь машины или механизма в виде кольца (обычно с зубьями).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Механик проверил зубчатый <венец> маховика на наличие повреждений.",
                            "target_lang_translations": {
                                "en": "The mechanic checked the flywheel's <ring gear> for damage."
                            }
                        },
                        {
                            "text": "Для ремонта двигателя потребовалось заменить стальной <венец>.",
                            "target_lang_translations": {
                                "en": "Repairing the engine required replacing the steel <rim>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "зубчатый венец"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "технический термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A machine part or mechanism in the form of a ring (usually with teeth)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ring gear",
                                "target_lang_sense_clarification": "toothed ring in mechanics"
                            },
                            {
                                "target_lang_word": "rim",
                                "target_lang_sense_clarification": "outer circular part of a wheel or gear"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ca370f9-0b42-359d-ab48-86392e95a1a1",
                    "sense_definition": "Житель или уроженец города Вены (столицы Австрии).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "demonym",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый коренной <ве́нец> любит проводить время в кофейнях.",
                            "target_lang_translations": {
                                "en": "Every native <Viennese> loves spending time in coffee houses."
                            }
                        },
                        {
                            "text": "Мой знакомый <ве́нец> показал нам самые красивые улицы города.",
                            "target_lang_translations": {
                                "en": "My <Viennese> acquaintance showed us the most beautiful streets of the city."
                            }
                        }
                    ],
                    "common_phrases": [
                        "коренной ве́нец"
                    ],
                    "target_lang_definitions": {
                        "en": "A resident or native of the city of Vienna (capital of Austria)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Viennese",
                                "target_lang_sense_clarification": "native or inhabitant of Vienna"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6067e5f1-6f4e-3b01-99c1-343c472670ca",
                    "sense_definition": "Название населённого пункта (посёлка) в России.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "toponym",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Мы проезжали посёлок <Венец> во время путешествия по стране.",
                            "target_lang_translations": {
                                "en": "We passed the settlement of <Venets> while traveling around the country."
                            }
                        },
                        {
                            "text": "На старой карте была отмечена деревня <Венец>.",
                            "target_lang_translations": {
                                "en": "The village of <Venets> was marked on the old map."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The name of a specific settlement or village in Russia."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Venets",
                                "target_lang_sense_clarification": "proper noun, place name"
                            }
                        ]
                    }
                }
            ]
        }
    ]

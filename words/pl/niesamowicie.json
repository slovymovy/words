                {
                    "sense_id": "8846a348-14ef-3e6d-8238-0ce898089bf5",
                    "sense_definition": "Bardzo, w stopniu wyjątkowym, trudnym do uwierzenia; słowo używane do wzmocnienia znaczenia przymiotnika lub innego przysłówka.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film był <w>niesamowicie</w> ciekawy i trzymał w napięciu do samego końca.",
                            "target_lang_translations": {
                                "ru": "Этот фильм был <w>невероятно</w> интересным и держал в напряжении до самого конца."
                            }
                        },
                        {
                            "text": "Jestem <w>niesamowicie</w> wdzięczny za twoją pomoc w tej trudnej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Я <w>безумно</w> благодарен за твою помощь в этой трудной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezwykle",
                        "niesłychanie"
                    ],
                    "common_phrases": [
                        "niesamowicie szybko",
                        "niesamowicie ważny",
                        "niesamowicie trudne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень, в исключительной степени, невероятно; слово используется для усиления значения прилагательного или другого наречия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невероятно",
                                "target_lang_sense_clarification": "в значении очень сильно, неправдоподобно"
                            },
                            {
                                "target_lang_word": "потрясающе",
                                "target_lang_sense_clarification": "в значении восхитительно"
                            },
                            {
                                "target_lang_word": "безумно",
                                "target_lang_sense_clarification": "разговорное усиление (очень)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84e28d89-4eb1-3375-b9cd-01ade2053fab",
                    "sense_definition": "W sposób tajemniczy, dziwny, budzący niepokój, lęk lub zdumienie swoją niezwykłością.",

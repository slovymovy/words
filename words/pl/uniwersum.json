    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "294367a3-9a3c-3134-bbb8-3ba672c9db5e",
                    "sense_definition": "Ogół postaci, miejsc, zdarzeń i historii powiązanych tematycznie w ramach serii książek, filmów, gier lub komiksów.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "fictional_world",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To filmowe <w>uniwersum</w> przyciąga miliony fanów na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Эта кинематографическая <w>вселенная</w> привлекает миллионы фанатов по всему миру."
                            }
                        },
                        {
                            "text": "Autor stworzył niezwykle bogate <w>uniwersum</w>, pełne magii i tajemniczych stworzeń.",
                            "target_lang_translations": {
                                "ru": "Автор создал невероятно богатую <w>вселенную</w>, полную магии и таинственных существ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uniwersum Marvela",
                        "rozszerzone uniwersum",
                        "filmowe uniwersum",
                        "spójne uniwersum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность персонажей, мест, событий и историй, тематически связанных в рамках серии книг, фильмов, игр или комиксов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вселенная",
                                "target_lang_sense_clarification": "вымышленный мир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e24d3408-7666-3445-86b9-18fe72ed5c88",
                    "sense_definition": "Wszechświat, kosmos; cała otaczająca nas przestrzeń fizyczna wraz z materią i energią.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "cosmos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naukowcy wciąż spierają się o to, jak powstało nasze <w>uniwersum</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые до сих пор спорят о том, как возникла наша <w>вселенная</w>."
                            }
                        },
                        {
                            "text": "Wydaje się, że <w>uniwersum</w> nie ma granic i ciągle się rozszerza.",
                            "target_lang_translations": {
                                "ru": "Кажется, что <w>вселенная</w> не имеет границ и постоянно расширяется."
                            }
                        }
                    ],
                    "synonyms": [
                        "wszechświat",
                        "kosmos"
                    ],
                    "common_phrases": [
                        "powstanie uniwersum",
                        "granice uniwersum",
                        "alternatywne uniwersum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вселенная, космос; все окружающее нас физическое пространство вместе с материей и энергией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вселенная",
                                "target_lang_sense_clarification": "космос"
                            },
                            {
                                "target_lang_word": "мироздание",
                                "target_lang_sense_clarification": "все сущее"
                            },
                            {
                                "target_lang_word": "универсум",
                                "target_lang_sense_clarification": "философский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a25800cd-1e35-39f2-b962-2367831b1bea",
                    "sense_definition": "Całokształt pewnych zjawisk, wartości, spraw lub dziedzina życia traktowana jako zamknięta całość.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "totality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego powieści tworzą specyficzne <w>uniwersum</w> moralne, w którym dobro zawsze zwycięża.",
                            "target_lang_translations": {
                                "ru": "Его романы создают специфический моральный <w>универсум</w>, в котором добро всегда побеждает."
                            }
                        },
                        {
                            "text": "Każdy człowiek nosi w sobie własne <w>uniwersum</w> przeżyć i wspomnień.",
                            "target_lang_translations": {
                                "ru": "Каждый человек носит в себе собственную <w>вселенную</w> переживаний и воспоминаний."
                            }
                        }
                    ],
                    "synonyms": [
                        "całokształt"
                    ],
                    "common_phrases": [
                        "uniwersum wartości",
                        "uniwersum symboliczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность определенных явлений, ценностей или сфера жизни, рассматриваемая как замкнутое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "сфера жизни или ценностей"
                            },
                            {
                                "target_lang_word": "универсум",
                                "target_lang_sense_clarification": "целостная система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "914af289-bc21-3e71-b813-a58f5976c6f8",
                    "sense_definition": "Zbiór wszystkich elementów rozważanych w danym kontekście matematycznym lub logicznym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "math_logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy najpierw zdefiniować <w>uniwersum</w>, aby poprawnie rozwiązać to zadanie.",
                            "target_lang_translations": {
                                "ru": "Сначала мы должны определить <w>универсум</w>, чтобы правильно решить эту задачу."
                            }
                        },
                        {
                            "text": "W tym przypadku <w>uniwersum</w> stanowi zbiór liczb naturalnych.",
                            "target_lang_translations": {
                                "ru": "В данном случае <w>универсум</w> составляет множество натуральных чисел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uniwersum dyskursu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Множество всех элементов, рассматриваемых в данном математическом или логическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "универсум",
                                "target_lang_sense_clarification": "предметная область"
                            }
                        ]
                    }
                }
            ]
        }

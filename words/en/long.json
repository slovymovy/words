    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "47f66b0e-11fa-3937-9b5d-ad1dbbedfba3",
                    "sense_definition": "Having a large distance from one end to the other in space.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "space_distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has beautiful <w>long</w> hair that reaches her waist.",
                            "target_lang_translations": {
                                "ru": "У нее красивые <w>длинные</w> волосы, доходящие до талии."
                            }
                        },
                        {
                            "text": "We walked down a <w>long</w> corridor to get to the office.",
                            "target_lang_translations": {
                                "ru": "Мы прошли по <w>длинному</w> коридору, чтобы добраться до офиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "lengthy",
                        "extended"
                    ],
                    "antonyms": [
                        "short"
                    ],
                    "common_phrases": [
                        "long way",
                        "long distance",
                        "long hair",
                        "long legs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большую протяженность в пространстве от одного конца до другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длинный",
                                "target_lang_sense_clarification": "о расстоянии или размере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0195f38a-3446-30db-8663-b21d0b3f8350",
                    "sense_definition": "Lasting for a large amount of time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The meeting was very <w>long</w> and everyone felt tired.",
                            "target_lang_translations": {
                                "ru": "Собрание было очень <w>долгим</w>, и все чувствовали усталость."
                            }
                        },
                        {
                            "text": "They have been good friends for a <w>long</w> time.",
                            "target_lang_translations": {
                                "ru": "Они хорошие друзья уже <w>долгое</w> время."
                            }
                        }
                    ],
                    "synonyms": [
                        "prolonged",
                        "extended",
                        "lengthy"
                    ],
                    "antonyms": [
                        "short",
                        "brief"
                    ],
                    "common_phrases": [
                        "long time",
                        "long term",
                        "long life",
                        "long history"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающийся в течение большого промежутка времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долгий",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "длительный",
                                "target_lang_sense_clarification": "продолжительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dd8110d-9d08-3b36-9fbe-680f82fc51ef",
                    "sense_definition": "Seeming to pass slowly or last too much time because of being boring or difficult.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It has been a <w>long</w> day at work and I just want to sleep.",
                            "target_lang_translations": {
                                "ru": "Это был <w>долгий</w> рабочий день, и я просто хочу спать."
                            }
                        },
                        {
                            "text": "The lecture was so <w>long</w> that many students stopped listening.",
                            "target_lang_translations": {
                                "ru": "Лекция была такой <w>долгой</w>, что многие студенты перестали слушать."
                            }
                        }
                    ],
                    "synonyms": [
                        "slow",
                        "boring",
                        "time-consuming",
                        "lengthy"
                    ],
                    "antonyms": [
                        "brief"
                    ],
                    "common_phrases": [
                        "long day",
                        "long wait",
                        "long journey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кажущийся медленным или слишком долгим из-за скуки или сложности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долгий",
                                "target_lang_sense_clarification": "утомительный"
                            },
                            {
                                "target_lang_word": "затянутый",
                                "target_lang_sense_clarification": "скучный, медленный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c14543c1-55e6-3d97-bf3b-15cfc27693aa",
                    "sense_definition": "Owning financial assets like stocks or bonds with the expectation that their value will rise.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "finance_pos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investor is <w>long</w> in technology stocks this year.",
                            "target_lang_translations": {
                                "ru": "В этом году инвестор держит <w>длинную</w> позицию по акциям технологических компаний."
                            }
                        },
                        {
                            "text": "He decided to maintain a <w>long</w> position despite the market volatility.",
                            "target_lang_translations": {
                                "ru": "Он решил сохранить <w>длинную</w> позицию, несмотря на волатильность рынка."
                            }
                        }
                    ],
                    "antonyms": [
                        "short"
                    ],
                    "common_phrases": [
                        "long position"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Владеющий финансовыми активами в ожидании роста их стоимости (обычно в составе фразы 'длинная позиция')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длинный",
                                "target_lang_sense_clarification": "финансы (длинная позиция)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "34d9b6e1-6c6d-3bbd-86da-4e9d8993bdb6",
                    "sense_definition": "For a large amount of time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This tradition started <w>long</w> ago in a small village.",
                            "target_lang_translations": {
                                "ru": "Эта традиция зародилась <w>давно</w> в маленькой деревне."
                            }
                        },
                        {
                            "text": "She had left the party <w>long</w> before we arrived.",
                            "target_lang_translations": {
                                "ru": "Она ушла с вечеринки <w>задолго</w> до того, как мы приехали."
                            }
                        }
                    ],
                    "antonyms": [
                        "brief"
                    ],
                    "common_phrases": [
                        "long ago",
                        "long before",
                        "long after",
                        "all day long"

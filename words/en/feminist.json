    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a31b5bf4-ce7c-367b-a68d-0654aaefdc3d",
                    "sense_definition": "An advocate of feminism; a person who supports equal rights and opportunities for women.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "person_advocate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a dedicated <w>feminist</w>, she campaigned tirelessly for equal pay in the workplace.",
                            "target_lang_translations": {
                                "ru": "Будучи убежденной <w>феминисткой</w>, она неустанно боролась за равную оплату труда на рабочем месте."
                            }
                        },
                        {
                            "text": "He considers himself a <w>feminist</w> because he believes men and women should have the same rights.",
                            "target_lang_translations": {
                                "ru": "Он считает себя <w>феминистом</w>, потому что верит, что мужчины и женщины должны иметь равные права."
                            }
                        }
                    ],
                    "synonyms": [
                        "women's rights activist"
                    ],
                    "antonyms": [
                        "misogynist",
                        "anti-feminist",
                        "sexist"
                    ],
                    "common_phrases": [
                        "radical feminist",
                        "liberal feminist",
                        "proud feminist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сторонник или сторонница феминизма; человек, выступающий за равные права и возможности для женщин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фемини́стка",
                                "target_lang_sense_clarification": "женщина"
                            },
                            {
                                "target_lang_word": "фемини́ст",
                                "target_lang_sense_clarification": "мужчина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c93f0138-3b4c-3f26-878e-000292c8c5f0",
                    "sense_definition": "Relating to or in accordance with the belief in the social, political, and economic equality of the sexes.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "related_to_theory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author analyzes the novel from a <w>feminist</w> perspective to highlight gender roles.",
                            "target_lang_translations": {
                                "ru": "Автор анализирует роман с <w>феминистской</w> точки зрения, чтобы подчеркнуть гендерные роли."
                            }
                        },
                        {
                            "text": "Many <w>feminist</w> organizations work globally to improve education for girls.",
                            "target_lang_translations": {
                                "ru": "Многие <w>феминистские</w> организации работают по всему миру, чтобы улучшить образование для девочек."
                            }
                        }
                    ],
                    "antonyms": [
                        "anti-feminist",
                        "patriarchal"
                    ],
                    "common_phrases": [
                        "feminist movement",
                        "feminist theory",
                        "feminist literature",
                        "feminist perspective"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к феминизму или соответствующий убеждениям о социальном, политическом и экономическом равенстве полов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фемини́стский",
                                "target_lang_sense_clarification": "относящийся к феминизму"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
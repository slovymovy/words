                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king has been <w>ruling</w> the kingdom with wisdom for many years.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "edb0ab6b-954f-37a9-8b22-9658bda3314d",
                    "sense_definition": "Iemands gevoelens pijn doen door iets onaardigs te zeggen of te doen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotioneel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij voelde zich erg <w>gekwetst</w> door die gemene opmerking.",
                            "target_lang_translations": {
                                "en": "He felt very <w>hurt</w> by that mean remark."
                            }
                        },
                        {
                            "text": "Het was echt niet mijn bedoeling om je te <w>kwetsen</w>.",
                            "target_lang_translations": {
                                "en": "It was really not my intention to <w>hurt</w> you."
                            }
                        }
                    ],
                    "synonyms": [
                        "beledigen",
                        "krenken",
                        "schofferen"
                    ],
                    "antonyms": [
                        "complimenteren",
                        "prijzen"
                    ],
                    "common_phrases": [
                        "iemands gevoelens kwetsen",
                        "zich gekwetst voelen",
                        "diep gekwetst"
                    ],
                    "target_lang_definitions": {
                        "en": "To hurt someone's feelings by saying or doing something unkind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hurt",
                                "target_lang_sense_clarification": "to cause emotional pain or distress"
                            },
                            {
                                "target_lang_word": "offend",
                                "target_lang_sense_clarification": "to cause someone to feel upset or insulted"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43bb9db0-3e72-30e8-91ed-0c321637de2a",
                    "sense_definition": "Lichamelijk letsel toebrengen of iets beschadigen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fysiek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De speler <w>kwetste</w> zijn enkel tijdens de belangrijke wedstrijd.",
                            "target_lang_translations": {
                                "en": "The player <w>injured</w> his ankle during the important match."
                            }
                        },
                        {
                            "text": "Als je het fruit te hard in de mand gooit, <w>kwets</w> je het.",
                            "target_lang_translations": {
                                "en": "If you throw the fruit too hard into the basket, you will <w>bruise</w> it."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwonden",
                        "beschadigen",
                        "bezeren"
                    ],
                    "antonyms": [
                        "genezen",
                        "helen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause physical injury or damage something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "injure",
                                "target_lang_sense_clarification": "to cause physical harm to a person or animal"
                            },
                            {
                                "target_lang_word": "hurt",
                                "target_lang_sense_clarification": "to cause physical pain"
                            },
                            {
                                "target_lang_word": "bruise",
                                "target_lang_sense_clarification": "to damage the skin of fruit or a person"
                            }
                        ]
                    }
                }
            ]
        },

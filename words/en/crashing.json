                    ],
                    "synonyms": [
                        "complete",
                        "total",
                        "absolute"
                    ],
                    "antonyms": [
                        "minor",
                        "slight"
                    ],
                    "common_phrases": [
                        "crashing bore",
                        "crashing failure",
                        "crashing halt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный, абсолютный или сокрушительный; часто используется для усиления негативного результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полный",
                                "target_lang_sense_clarification": "абсолютный (о провале, неудаче)"
                            },
                            {
                                "target_lang_word": "сокрушительный",
                                "target_lang_sense_clarification": "разрушительный, сильный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac18ceb0-9776-3884-a0bc-2e7182a300a5",
                    "sense_definition": "The sound or action of something that crashes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could hear the loud <w>crashings</w> of the thunder throughout the night.",
                            "target_lang_translations": {
                                "ru": "Всю ночь мы слышали громкие <w>раскаты</w> грома."
                            }
                        },
                        {
                            "text": "The continuous <w>crashings</w> of the machinery made it hard to concentrate.",
                            "target_lang_translations": {
                                "ru": "Непрерывный <w>грохот</w> оборудования мешал сосредоточиться."
                            }
                        }

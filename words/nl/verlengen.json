{
    "word_family": [
        "verlenging",
        "lengte",
        "lang"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "948a5c83-37b0-3f28-8e2e-34f105b2d943",
                    "sense_definition": "De geldigheidsduur of tijdsduur van iets langer maken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga mijn abonnement <w>verlengen</w> omdat ik erg tevreden ben met de service.",
                            "target_lang_translations": {
                                "en": "I am going to <w>renew</w> my subscription because I am very satisfied with the service.",
                                "ru": "Я собираюсь <w>продлить</w> свой абонемент, так как очень доволен обслуживанием."
                            }
                        },
                        {
                            "text": "De club wil het contract van de speler graag met twee jaar <w>verlengen</w>.",
                            "target_lang_translations": {
                                "en": "The club would like to <w>extend</w> the player's contract by two years.",
                                "ru": "Клуб хочет <w>продлить</w> контракт игрока на два года."
                            }
                        }
                    ],
                    "synonyms": [
                        "prolongeren"
                    ],
                    "antonyms": [
                        "verkorten"
                    ],
                    "common_phrases": [
                        "het contract verlengen",
                        "een abonnement verlengen",
                        "de deadline verlengen"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the duration or validity period of something.",
                        "ru": "Увеличить срок действия или продолжительность чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "to cause to last for a longer period"
                            },
                            {
                                "target_lang_word": "renew",
                                "target_lang_sense_clarification": "to re-establish the validity of a contract or subscription"
                            },
                            {
                                "target_lang_word": "prolong",
                                "target_lang_sense_clarification": "to make something last longer"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продлить",
                                "target_lang_sense_clarification": "срок действия, время"
                            },
                            {
                                "target_lang_word": "пролонгировать",
                                "target_lang_sense_clarification": "официально (о договорах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0930221d-c55c-3dc9-84be-980bb5dc3735",
                    "sense_definition": "Iets groter maken in lengte of afmeting.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleermaker kan de mouwen van je jas eenvoudig <w>verlengen</w>.",
                            "target_lang_translations": {
                                "en": "The tailor can easily <w>lengthen</w> the sleeves of your jacket.",
                                "ru": "Портной может легко <w>удлинить</w> рукава твоего пальто."
                            }
                        },
                        {
                            "text": "We moeten de tafel <w>verlengen</w> als er meer gasten komen eten.",
                            "target_lang_translations": {
                                "en": "We need to <w>extend</w> the table if more guests are coming to eat.",
                                "ru": "Нам нужно <w>удлинить</w> стол, если придет больше гостей."
                            }
                        }
                    ],
                    "antonyms": [
                        "inkorten",
                        "verkorten"
                    ],
                    "common_phrases": [
                        "een broek verlengen",
                        "een kabel verlengen",
                        "het snoer verlengen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something greater in length or dimension.",
                        "ru": "Сделать что-либо больше в длину или увеличить размер."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lengthen",
                                "target_lang_sense_clarification": "to make physically longer"
                            },
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "to increase in physical extent or reach"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "удлинить",
                                "target_lang_sense_clarification": "сделать длиннее физически"
                            },
                            {
                                "target_lang_word": "надставить",
                                "target_lang_sense_clarification": "увеличить, добавив часть"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
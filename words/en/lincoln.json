    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c85b434e-1b59-3ed7-92c5-197381ded016",
                    "sense_definition": "Abraham Lincoln, the 16th president of the United States, who led the country during the American Civil War and abolished slavery.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_lincoln",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Many historians consider <w>Lincoln</w> to be one of the greatest American leaders.",
                            "target_lang_translations": {
                                "ru": "Многие историки считают <w>Линкольна</w> одним из величайших американских лидеров."
                            }
                        },
                        {
                            "text": "The Gettysburg Address was a famous speech delivered by President <w>Lincoln</w>.",
                            "target_lang_translations": {
                                "ru": "Геттисбергская речь была знаменитым выступлением президента <w>Линкольна</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Abe",
                        "Abe Lincoln"
                    ],
                    "common_phrases": [
                        "President Lincoln",
                        "Lincoln Memorial",
                        "Abraham Lincoln"
                    ],
                    "target_lang_definitions": {
                        "ru": "Авраам Линкольн, 16-й президент США, возглавлявший страну во время Гражданской войны и отменивший рабство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Линкольн",
                                "target_lang_sense_clarification": "фамилия / историческая личность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "382a46dd-99cf-3ba5-b671-808fdfe615d8",
                    "sense_definition": "A historic city and local government district in Lincolnshire, England, known for its medieval cathedral.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_lincoln_uk",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We visited the ancient castle in <w>Lincoln</w> during our trip to England.",
                            "target_lang_translations": {
                                "ru": "Мы посетили старинный замок в <w>Линкольне</w> во время нашей поездки в Англию."
                            }
                        },
                        {
                            "text": "The train to <w>Lincoln</w> departs from the main station every hour.",
                            "target_lang_translations": {
                                "ru": "Поезд до <w>Линкольна</w> отправляется с главного вокзала каждый час."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Lincoln Cathedral",
                        "city of Lincoln"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исторический город и административный район в Линкольншире, Англия, известный своим средневековым собором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Линкольн",
                                "target_lang_sense_clarification": "город в Англии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7635578-5205-30e3-b77e-a11265569c86",
                    "sense_definition": "A male given name, often given in honor of Abraham Lincoln.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Lincoln</w> prefers to be called Linc by his friends.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Линкольн</w> предпочитает, чтобы друзья называли его Линк."
                            }
                        },
                        {
                            "text": "They decided to name their newborn son <w>Lincoln</w> after the president.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Линкольном</w> в честь президента."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, которое часто дают в честь Авраама Линкольна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Линкольн",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]

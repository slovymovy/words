    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8134c94-bbdb-384d-92cd-0acef7b113b9",
                    "sense_definition": "Twee sneetjes brood met beleg ertussen, vaak diagonaal doorgesneden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik bestel meestal een <w>sandwich</w> met kaas en tomaat voor de lunch.",
                            "target_lang_translations": {
                                "ru": "Я обычно заказываю <w>сэндвич</w> с сыром и помидорами на обед."
                            }
                        },
                        {
                            "text": "Zij eet haar <w>sandwich</w> snel op voordat de trein vertrekt.",
                            "target_lang_translations": {
                                "ru": "Она быстро съедает свой <w>сэндвич</w> до отправления поезда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een sandwich maken",
                        "sandwich met kaas",
                        "sandwich met ham"
                    ],
                    "target_lang_definitions": {
                        "ru": "Два ломтика хлеба с начинкой между ними, часто разрезанные по диагонали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сэндвич",
                                "target_lang_sense_clarification": "закрытый бутерброд"
                            },
                            {
                                "target_lang_word": "бутерброд",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "806a7f0b-63cf-3134-a4e0-17e44aa0bb83",
                    "sense_definition": "Een zacht, langwerpig en licht zoet broodje.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij het ontbijt kregen we een verse <w>sandwich</w> met boter en jam.",
                            "target_lang_translations": {
                                "ru": "На завтрак нам дали свежую <w>сдобную булочку</w> с маслом и джемом."
                            }
                        },
                        {
                            "text": "De bakker verkoopt elke ochtend warme <w>sandwiches</w>.",
                            "target_lang_translations": {
                                "ru": "Пекарь продает теплые <w>сдобные булочки</w> каждое утро."
                            }
                        }
                    ],
                    "synonyms": [
                        "puntje"
                    ],
                    "common_phrases": [
                        "een zachte sandwich",
                        "belegde sandwich"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкая, продолговатая и слегка сладкая булочка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдобная булочка",
                                "target_lang_sense_clarification": "мягкий хлеб"
                            },
                            {
                                "target_lang_word": "булочка",
                                "target_lang_sense_clarification": "выпечка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d14839a4-1336-3f73-a79e-53dacba66127",
                    "sense_definition": "Een situatie waarbij men klem zit tussen twee andere zaken of personen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleine auto raakte in een <w>sandwich</w> tussen twee grote vrachtwagens.",
                            "target_lang_translations": {
                                "ru": "Маленькая машина попала в <w>сэндвич</w> между двумя большими грузовиками."
                            }
                        },
                        {
                            "text": "Door de drukte bij de uitgang zat ik in de <w>sandwich</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за толкучки у выхода я оказался <w>зажат</w> со всех сторон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de sandwich",
                        "in een sandwich zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда кто-то или что-то зажато между двумя другими объектами или людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зажатое положение",
                                "target_lang_sense_clarification": "ситуация"
                            },
                            {
                                "target_lang_word": "сэндвич",
                                "target_lang_sense_clarification": "метафора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa2dd49b-4b01-375e-bfe9-68ca1bab8d3f",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>sandwichen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>sandwich</w> de foto voorzichtig tussen twee stukken karton.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>помещаю</w> фотографию между двумя кусками картона."
                            }
                        },
                        {
                            "text": "Soms <w>sandwich</w> ik mijn afspraken zodat ik tussendoor geen pauze heb.",
                            "target_lang_translations": {
                                "ru": "Иногда я <w>ставлю вплотную</w> свои встречи, так что у меня нет перерывов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола \"sandwichen\" (помещать между чем-либо)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помещаю между",
                                "target_lang_sense_clarification": "вставлять"
                            },
                            {
                                "target_lang_word": "зажимаю",
                                "target_lang_sense_clarification": "сдавливать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "76eec911-8766-323e-9c36-6966c0b81dfb",
                    "sense_definition": "Świetna zabawa, śmiech lub sytuacja budząca rozbawienie, często kosztem kogoś innego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "amusement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy mieli niezłą <bekę>, gdy opowiedział ten żart.",
                            "target_lang_translations": {
                                "ru": "У всех была дикая <w>ржака</w>, когда он рассказал эту шутку."
                            }
                        },
                        {
                            "text": "Zrobiliśmy to tylko dla <beki>, nie bierz tego na poważnie.",
                            "target_lang_translations": {
                                "ru": "Мы сделали это только ради <w>прикола</w>, не воспринимай всерьез."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubaw",
                        "heca",
                        "zabawa"
                    ],
                    "antonyms": [
                        "nuda",
                        "powaga"
                    ],
                    "common_phrases": [
                        "mieć bekę",
                        "dla beki",
                        "kręcić bekę",
                        "niezła beka"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "slang młodzieżowy"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "potoczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Веселье, смех или ситуация, вызывающая развлечение, часто за счет кого-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ржака",
                                "target_lang_sense_clarification": "сленг: что-то очень смешное"
                            },
                            {
                                "target_lang_word": "прикол",
                                "target_lang_sense_clarification": "шутка, забава"
                            },
                            {
                                "target_lang_word": "умора",
                                "target_lang_sense_clarification": "разг.: очень смешная ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6835735-68d9-3a16-bea3-d61950c26f18",
                    "sense_definition": "Duża <beczka>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W piwnicy stała stara, dębowa <beka> pełna wina.",
                            "target_lang_translations": {
                                "ru": "В подвале стояла старая дубовая <w>бочка</w>, полная вина."
                            }
                        },
                        {
                            "text": "Toczenie tej ciężkiej <beki> wymagało siły dwóch mężczyzn.",
                            "target_lang_translations": {
                                "ru": "Чтобы катить эту тяжелую <w>бочку</w>, требовалась сила двух мужчин."
                            }
                        }
                    ],
                    "synonyms": [
                        "beczka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zgrubienie"
                        }
                    ],
                    "target_lang_definitions": {

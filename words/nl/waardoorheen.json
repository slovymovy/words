            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "08885900-a52e-3431-9f1a-69253ffd5643",
                    "sense_definition": "Vragend woord dat gebruikt wordt om te vragen door welk object of welke ruimte iets zich verplaatst.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun jij zien <w>waardoorheen</w> de kabel naar buiten loopt?",
                            "target_lang_translations": {
                                "ru": "Ты видишь, <w>через что</w> кабель выходит наружу?"
                            }
                        },
                        {
                            "text": "<w>Waardoorheen</w> moeten we kruipen om aan de overkant te komen?",
                            "target_lang_translations": {
                                "ru": "<w>Через что</w> нам нужно проползти, чтобы добраться до другой стороны?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопросительное слово, используемое, чтобы узнать, через какой объект или пространство происходит движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "через что",
                                "target_lang_sense_clarification": "вопросительная форма"
                            },
                            {
                                "target_lang_word": "сквозь что",
                                "target_lang_sense_clarification": "вопросительная форма, акцент на проникновение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a68bbe5a-78b7-35bf-9369-717965b35a30",
                    "sense_definition": "Verwijst terug naar een eerder genoemd object of plaats waar een beweging dwars doorheen gaat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de tunnel <w>waardoorheen</w> de trein elke dag rijdt.",
                            "target_lang_translations": {
                                "ru": "Это туннель, <w>через который</w> поезд проезжает каждый день."
                            }
                        },
                        {
                            "text": "Het filter <w>waardoorheen</w> het water stroomt, moet schoongemaakt worden.",
                            "target_lang_translations": {
                                "ru": "Фильтр, <w>через который</w> течет вода, нужно почистить."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на ранее упомянутый объект или место, через которые проходит движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "через который",
                                "target_lang_sense_clarification": "относительное местоимение"
                            },
                            {
                                "target_lang_word": "сквозь который",
                                "target_lang_sense_clarification": "относительное местоимение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

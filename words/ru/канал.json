                    "frequency": "Middle",
                    "semantic_group_id": "figurative_means",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания ищет новые <каналы> сбыта своей продукции.",
                            "target_lang_translations": {
                                "en": "The company is looking for new sales <channels> for its products."

                },
                {
                    "sense_id": "a9e66d77-d45e-36d5-bd48-5018cb6c76d5",
                    "sense_definition": "(wederkerend) Belangstelling tonen voor of aandacht besteden aan een bepaald onderwerp.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "show_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <interesseer> me erg voor computers en nieuwe technologie.",
                            "target_lang_translations": {

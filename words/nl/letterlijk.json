    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1b0438d9-aed0-3a62-9225-624935db5981",
                    "sense_definition": "Precies zoals het er staat of gezegd wordt, zonder overdrachtelijke of figuurlijke betekenis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>letterlijke</w> betekenis van dit spreekwoord is mij onbekend.",
                            "target_lang_translations": {
                                "en": "The <w>literal</w> meaning of this proverb is unknown to me."
                            }
                        },
                        {
                            "text": "Je moet zijn opmerking niet te <w>letterlijk</w> nemen, het was maar een grapje.",
                            "target_lang_translations": {
                                "en": "You shouldn't take his remark too <w>literally</w>, it was just a joke."
                            }
                        }
                    ],
                    "synonyms": [
                        "woordelijk",
                        "exact"
                    ],
                    "antonyms": [
                        "figuurlijk"
                    ],
                    "common_phrases": [
                        "letterlijke betekenis",
                        "letterlijke vertaling",
                        "niet te letterlijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Exactly as written or spoken, without metaphorical or figurative meaning."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "literal",
                                "target_lang_sense_clarification": "adhering strictly to the basic meaning of words"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "666a9c9f-6e61-3b32-89ba-805b598ab174",
                    "sense_definition": "Op een manier die precies overeenkomt met de woorden; woord voor woord.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft de tekst <w>letterlijk</w> uit het boek overgeschreven.",
                            "target_lang_translations": {
                                "en": "He copied the text <w>word for word</w> from the book."
                            }
                        },
                        {
                            "text": "Na de lange wandeling was ik <w>letterlijk</w> en figuurlijk de weg kwijt.",
                            "target_lang_translations": {
                                "en": "After the long walk, I was <w>literally</w> and figuratively lost."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a65c3e9-1600-3057-98c3-575815727928",
                    "sense_definition": "Ручной инструмент, состоящий из металлической головки на рукоятке, используемый для забивания гвоздей или ударов по предметам.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец взял тяжёлый <молоток>, чтобы починить сломанный забор на даче.",
                            "target_lang_translations": {
                                "en": "Father took a heavy <w>hammer</w> to fix the broken fence at the summer house."
                            }
                        },
                        {
                            "text": "Рабочий включил отбойный <молоток> и начал громко долбить старый асфальт.",
                            "target_lang_translations": {
                                "en": "The worker turned on the jack<w>hammer</w> and started loudly breaking up the old asphalt."
                            }
                        }
                    ],
                    "synonyms": [
                        "молот"
                    ],
                    "common_phrases": [
                        "забить гвоздь молотком",
                        "отбойный молоток",
                        "стучать молотком",
                        "работать молотком"
                    ],
                    "target_lang_definitions": {
                        "en": "A hand tool consisting of a metal head on a handle, used for driving nails or striking objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hammer",
                                "target_lang_sense_clarification": "tool"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef8bb39a-bffa-3ef0-b94d-89adafb327ef",
                    "sense_definition": "Человек, который отлично справился с делом или поступил правильно; молодец.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "praise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты сдал этот сложный экзамен на отлично? Ну ты просто <молоток>!",
                            "target_lang_translations": {
                                "en": "Did you pass this difficult exam with flying colors? You're a real <w>champ</w>!"
                            }
                        },
                        {
                            "text": "Наш вратарь сегодня <молоток>, он спас команду от верного поражения.",
                            "target_lang_translations": {
                                "en": "Our goalie is a <w>star</w> today; he saved the team from certain defeat."
                            }
                        }
                    ],
                    "synonyms": [
                        "молодец"
                    ],
                    "common_phrases": [
                        "ты молоток",
                        "настоящий молоток",
                        "ну ты молоток"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "сленг"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who has done an excellent job or acted correctly; a clever or capable person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "champ",
                                "target_lang_sense_clarification": "praise for a person"
                            },
                            {
                                "target_lang_word": "star",
                                "target_lang_sense_clarification": "praise for a person"
                            },
                            {
                                "target_lang_word": "legend",
                                "target_lang_sense_clarification": "colloquial praise"
                            }
                        ]
                    }
                }
            ]
        },
        {

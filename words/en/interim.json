    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8dafe3b2-43e4-397d-aafb-3b0fac9b2012",
                    "sense_definition": "Intended to last for a short time until something permanent is established.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company appointed an <w>interim</w> manager while looking for a new CEO.",
                            "target_lang_translations": {
                                "ru": "Компания назначила <w>временного</w> управляющего, пока искала нового генерального директора."
                            }
                        },
                        {
                            "text": "We have put an <w>interim</w> solution in place until the software is fully updated.",
                            "target_lang_translations": {
                                "ru": "Мы внедрили <w>временное</w> решение, пока программное обеспечение не будет полностью обновлено."
                            }
                        }
                    ],
                    "synonyms": [
                        "provisional",
                        "caretaker"
                    ],
                    "antonyms": [
                        "permanent"
                    ],
                    "common_phrases": [
                        "interim government",
                        "interim report",
                        "interim measure",
                        "interim period"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для использования в течение короткого времени, пока не будет установлено что-то постоянное"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "временный",
                                "target_lang_sense_clarification": "существующий или действующий лишь некоторое время"
                            },
                            {
                                "target_lang_word": "переходный",
                                "target_lang_sense_clarification": "относящийся к периоду изменений или смены состояний"
                            },
                            {
                                "target_lang_word": "промежуточный",
                                "target_lang_sense_clarification": "находящийся между двумя стадиями или событиями"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "14ed7667-95b0-372e-93be-f2548e3e5bbd",
                    "sense_definition": "A period of time between two events.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new contract starts in June, but we need an agreement for the <w>interim</w>.",
                            "target_lang_translations": {
                                "ru": "Новый контракт начинается в июне, но нам нужно соглашение на <w>переходный период</w>."
                            }
                        },
                        {
                            "text": "His car is being repaired, so he is using a rental in the <w>interim</w>.",
                            "target_lang_translations": {
                                "ru": "Его машину ремонтируют, поэтому в <w>промежутке</w> он пользуется арендованной."
                            }
                        }
                    ],
                    "synonyms": [
                        "meantime",
                        "meanwhile"
                    ],
                    "common_phrases": [

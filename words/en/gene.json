    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ccac9d8f-1f7e-3614-b1d2-845442886de7",
                    "sense_definition": "A unit of heredity transferred from a parent to offspring that determines some characteristic of the offspring.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biological_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He seems to have inherited the <w>gene</w> for tallness from his father.",
                            "target_lang_translations": {
                                "ru": "Похоже, он унаследовал <w>ген</w> высокого роста от своего отца."
                            }
                        },
                        {
                            "text": "Scientists are worried that the <w>gene</w> pool of this species is becoming too small.",
                            "target_lang_translations": {
                                "ru": "Ученые обеспокоены тем, что <w>генофонд</w> этого вида становится слишком маленьким."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gene pool",
                        "dominant gene",
                        "recessive gene",
                        "selfish gene"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица наследственности, передаваемая от родителей потомству и определяющая какой-либо признак организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ген",
                                "target_lang_sense_clarification": "единица наследственности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1b49d64-dd0a-33aa-a0ad-6f50ac326d77",
                    "sense_definition": "A specific segment of DNA or RNA that contains the instructions for making a protein.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "biological_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Researchers identified a specific <w>gene</w> that contributes to the disease.",
                            "target_lang_translations": {
                                "ru": "Исследователи выявили конкретный <w>ген</w>, способствующий развитию заболевания."
                            }
                        },
                        {
                            "text": "New medical techniques allow doctors to use <w>gene</w> therapy to treat patients.",
                            "target_lang_translations": {
                                "ru": "Новые медицинские методы позволяют врачам использовать <w>генную</w> терапию для лечения пациентов."
                            }
                        }
                    ],

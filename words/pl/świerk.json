    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "168d5f60-dc09-30d4-a4aa-4b133a47efaf",
                    "sense_definition": "Drzewo iglaste z rodziny sosnowatych o stożkowatej koronie, ciemnozielonych igłach i zwisających szyszkach, popularne w strefie klimatu umiarkowanego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tree_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie rośnie wysoki <w>świerk</w>, który daje dużo cienia w upalne dni.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растёт высокая <w>ель</w>, которая даёт много тени в жаркие дни."
                            }
                        },
                        {
                            "text": "Na święta Bożego Narodzenia zawsze dekorujemy żywy <w>świerk</w> kolorowymi bombkami.",
                            "target_lang_translations": {
                                "ru": "На Рождество мы всегда украшаем живую <w>ёлку</w> разноцветными шарами."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Podhale) smreczyna, smrek",
                        "gw. (Śląsk Cieszyński) smyrek"
                    ],
                    "common_phrases": [
                        "las świerkowy",
                        "szyszka świerka",
                        "sadzić świerki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хвойное дерево семейства сосновых с конусовидной кроной, темно-зеленой хвоей и свисающими шишками, распространенное в умеренном климате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ель",
                                "target_lang_sense_clarification": "дерево"
                            },
                            {
                                "target_lang_word": "ёлка",
                                "target_lang_sense_clarification": "разговорное; новогоднее дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ada75926-32bb-36b5-a7b1-02872cb6a473",
                    "sense_definition": "Drewno pozyskane z drzewa świerkowego, wykorzystywane w budownictwie i stolarstwie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material_wood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten solidny stół kuchenny wykonany jest z jasnego <w>świerku</w>.",
                            "target_lang_translations": {
                                "ru": "Этот прочный кухонный стол сделан из светлой <w>ели</w>."
                            }
                        },
                        {
                            "text": "Podłoga w salonie została ułożona z desek ze <w>świerku</w>.",
                            "target_lang_translations": {
                                "ru": "Пол в гостиной был выложен из досок из <w>ели</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "świerczyna"
                    ],
                    "common_phrases": [
                        "stół ze świerku",
                        "drewno ze świerku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древесина ели, используемая в строительстве и столярном деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ель",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "еловая древесина",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56657799-e2b2-3478-9041-63e471d82deb",
                    "sense_definition": "Dźwięk wydawany przez ptaki; szybki i wysoki śpiew.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O poranku obudził mnie radosny <w>świerk</w> wróbli za oknem.",
                            "target_lang_translations": {
                                "ru": "Утром меня разбудило радостное <w>чириканье</w> воробьёв за окном."
                            }
                        },
                        {
                            "text": "W lesie słychać było tylko szum wiatru i cichy <w>świerk</w> ptaków.",
                            "target_lang_translations": {
                                "ru": "В лесу был слышен только шум ветра и тихий <w>щебет</w> птиц."
                            }
                        }
                    ],
                    "synonyms": [
                        "świergot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, издаваемый птицами; быстрое и высокое пение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щебет",
                                "target_lang_sense_clarification": "пение птиц"
                            },
                            {
                                "target_lang_word": "чириканье",
                                "target_lang_sense_clarification": "звуки воробьев"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "047b62be-acc0-3327-b6ca-1b7118624f3e",
                    "sense_definition": "Nazwisko polskie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Świerk</w> jest znanym lokalnym działaczem społecznym.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Сверк</w> — известный местный общественный деятель."
                            }
                        },
                        {
                            "text": "Wczoraj spotkałem panią <w>Świerk</w> w urzędzie miasta.",
                            "target_lang_translations": {
                                "ru": "Вчера я встретил госпожу <w>Сверк</w> в городской администрации."
                            }
                        }
                    ],
                    "traits": [

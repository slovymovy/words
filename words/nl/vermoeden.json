                        "en": [
                            {
                                "target_lang_word": "suspect",
                                "target_lang_sense_clarification": "to imagine something to be true without proof"
                            },
                            {
                                "target_lang_word": "suppose",
                                "target_lang_sense_clarification": "to assume or conclude based on probability"

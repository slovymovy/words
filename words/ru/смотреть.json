    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "73674c38-ee22-3c6a-9be7-1c1053c1b939",
                    "sense_definition": "Направлять взгляд на кого-либо или что-либо, чтобы увидеть.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я люблю <смотреть> на звёзды в ночном небе.",
                            "target_lang_translations": {
                                "en": "I love to <look> at the stars in the night sky."
                            }
                        },
                        {
                            "text": "Пожалуйста, <смотри> мне в глаза, когда говоришь.",
                            "target_lang_translations": {
                                "en": "Please <look> me in the eye when you speak."
                            }
                        }
                    ],
                    "synonyms": [
                        "глядеть",
                        "взирать"
                    ],
                    "common_phrases": [
                        "смотреть в окно",
                        "смотреть на часы",
                        "смотреть в глаза",
                        "смотреть по сторонам"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct one's gaze at someone or something in order to see."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "to direct one's eyes"
                            },
                            {
                                "target_lang_word": "gaze",
                                "target_lang_sense_clarification": "to look steadily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f88c58f-3d50-3e98-8062-e20806aad19b",
                    "sense_definition": "Быть зрителем, воспринимать содержание фильма, спектакля или телепередачи.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера вечером мы всей семьёй <смотрели> комедию.",
                            "target_lang_translations": {
                                "en": "Last night our whole family <watched> a comedy."
                            }
                        },
                        {
                            "text": "Дети любят <смотреть> мультфильмы по выходным.",
                            "target_lang_translations": {
                                "en": "Children love to <watch> cartoons on weekends."
                            }
                        }
                    ],
                    "common_phrases": [
                        "смотреть телевизор",
                        "смотреть фильм",
                        "смотреть новости",
                        "смотреть кино"
                    ],
                    "target_lang_definitions": {
                        "en": "To view or observe a movie, play, or broadcast."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "to observe for a period of time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4cbabde-d2f3-313f-82c5-b93272b551f8",
                    "sense_definition": "Заботиться о ком-либо, следить за безопасностью или порядком.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто будет <смотреть> за твоей собакой, пока ты в отпуске?",
                            "target_lang_translations": {
                                "en": "Who will <look after> your dog while you are on vacation?"
                            }
                        },
                        {
                            "text": "Старшая сестра должна <смотреть> за маленьким братом.",
                            "target_lang_translations": {
                                "en": "The older sister has to <watch> her little brother."
                            }
                        }
                    ],
                    "synonyms": [
                        "следить"
                    ],
                    "common_phrases": [
                        "смотреть за детьми",
                        "смотреть за домом",
                        "смотреть за вещами",
                        "смотреть за порядком"
                    ],
                    "target_lang_definitions": {
                        "en": "To take care of someone or supervise safety and order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "to guard or supervise"
                            },
                            {
                                "target_lang_word": "look after",
                                "target_lang_sense_clarification": "to take care of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d350516f-10ff-37cd-8c6e-6ae7144def7d",
                    "sense_definition": "Быть обращённым в определённую сторону (об окнах, зданиях, частях предметов).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Окна моей спальни <смотрят> прямо в сад.",
                            "target_lang_translations": {
                                "en": "My bedroom windows <look> right into the garden."
                            }
                        },
                        {
                            "text": "Балкон нашего номера <смотрит> на море.",
                            "target_lang_translations": {
                                "en": "The balcony of our room <looks> out onto the sea."
                            }
                        }
                    ],
                    "common_phrases": [
                        "окна смотрят во двор",
                        "смотреть на юг",
                        "смотреть на улицу"
                    ],
                    "target_lang_definitions": {
                        "en": "To face or be oriented in a specific direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "to face a direction"
                            },
                            {
                                "target_lang_word": "face",
                                "target_lang_sense_clarification": "to be opposite to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38c57266-dd05-37b4-aa58-522850adb6c1",
                    "sense_definition": "Относиться к чему-либо определённым образом, иметь мнение или точку зрения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы по-разному <смотрим> на эту сложную проблему.",
                            "target_lang_translations": {
                                "en": "We <look> at this complex problem differently."
                            }
                        },
                        {
                            "text": "Он привык <смотреть> на жизнь с оптимизмом.",
                            "target_lang_translations": {
                                "en": "He is used to <looking> at life with optimism."
                            }
                        }
                    ],
                    "synonyms": [
                        "считать",
                        "полагать"
                    ],
                    "common_phrases": [
                        "смотреть на вещи",
                        "смотреть иначе",
                        "смотреть с надеждой",
                        "как посмотреть"
                    ],
                    "target_lang_definitions": {
                        "en": "To consider or regard something in a particular way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look at",
                                "target_lang_sense_clarification": "to consider or view"
                            },
                            {
                                "target_lang_word": "view",
                                "target_lang_sense_clarification": "to regard mentally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5fa8d9a-15dc-3c9c-8029-cd5864520d1c",
                    "sense_definition": "Внимательно изучать, обследовать или проверять кого-либо или что-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "inspection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач внимательно <смотрел> пациента перед операцией.",
                            "target_lang_translations": {
                                "en": "The doctor carefully <examined> the patient before the surgery."
                            }
                        },
                        {
                            "text": "Таможенник долго <смотрел> мои документы на границе.",
                            "target_lang_translations": {
                                "en": "The customs officer <inspected> my documents for a long time at the border."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассматривать",
                        "просматривать",
                        "глядеть"
                    ],
                    "common_phrases": [
                        "смотреть документы",
                        "смотреть больного",
                        "смотреть машину"
                    ],
                    "target_lang_definitions": {
                        "en": "To examine, inspect, or check someone or something carefully."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "examine",
                                "target_lang_sense_clarification": "to inspect closely"
                            },
                            {
                                "target_lang_word": "check",
                                "target_lang_sense_clarification": "to verify"
                            },
                            {
                                "target_lang_word": "inspect",
                                "target_lang_sense_clarification": "to examine officially"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "681424d1-9139-393d-a420-47a73fd2a987",
                    "sense_definition": "Иметь какой-либо внешний вид, выглядеть определённым образом.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом костюме ты <смотришь> настоящим джентльменом.",
                            "target_lang_translations": {
                                "en": "In this suit you <look> like a real gentleman."
                            }
                        },
                        {
                            "text": "Старый дом <смотрел> угрюмо и заброшенно.",
                            "target_lang_translations": {
                                "en": "The old house <looked> gloomy and abandoned."
                            }
                        }
                    ],
                    "synonyms": [
                        "глядеть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech instead of 'выглядеть'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To have a particular appearance or look."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "to appear or seem"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
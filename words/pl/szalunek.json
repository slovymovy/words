    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ce5d299-0857-369b-8bda-4dcfc19d0228",
                    "sense_definition": "Prowizoryczna konstrukcja, zwykle z desek lub płyt, tworząca formę dla wylewanego betonu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy muszą dokładnie przygotować <w>szalunek</w>, zanim wyleją fundamenty.",
                            "target_lang_translations": {
                                "ru": "Рабочие должны тщательно подготовить <w>опалубку</w>, прежде чем заливать фундамент."
                            }
                        },
                        {
                            "text": "Kiedy beton stwardniał, ekipa budowlana zaczęła demontować drewniany <w>szalunek</w>.",
                            "target_lang_translations": {
                                "ru": "Когда бетон затвердел, строительная бригада начала демонтировать деревянную <w>опалубку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "deskowanie",
                        "szalowanie"
                    ],
                    "common_phrases": [
                        "drewniany szalunek",
                        "montaż szalunku",
                        "zdjąć szalunek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временная конструкция, обычно из досок или плит, служащая формой для заливки бетона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опалубка",
                                "target_lang_sense_clarification": "форма для бетона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4078a36-3741-3788-8c02-dbae90362c02",
                    "sense_definition": "Konstrukcja z desek lub płyt służąca do zabezpieczania ścian wykopów ziemnych przed osunięciem lub do pokrywania ścian budynków.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "construction_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głęboki rów wymagał zabezpieczenia, dlatego zastosowano w nim pełny <w>szalunek</w>.",
                            "target_lang_translations": {
                                "ru": "Глубокая траншея требовала укрепления, поэтому в ней применили сплошное <w>крепление</w>."
                            }
                        },
                        {
                            "text": "Stary <w>szalunek</w> w wykopie chronił pracowników przed osypującą się ziemią.",
                            "target_lang_translations": {
                                "ru": "Старое <w>крепление</w> в котловане защищало рабочих от осыпающейся земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "deskowanie",
                        "szalowanie"
                    ],
                    "common_phrases": [
                        "szalunek wykopu",
                        "pełny szalunek",
                        "szalunek ażurowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конструкция из досок или плит для укрепления стен земляных выемок от обрушения или для обшивки стен зданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крепление",
                                "target_lang_sense_clarification": "траншеи или котлована"
                            },
                            {
                                "target_lang_word": "обшивка",
                                "target_lang_sense_clarification": "стен досками"
                            }
                        ]
                    }
                }
            ]
        }
    ]

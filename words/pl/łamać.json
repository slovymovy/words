                            {
                                "target_lang_word": "ломать",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "разбивать",
                                "target_lang_sense_clarification": "на части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e7a4d25-a62b-3fe3-b592-21d611dc870a",
                    "sense_definition": "Powodować pęknięcie lub uszkodzenie kości w ciele",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Narciarze często <łamią> nogi na trudnych trasach.",
                            "target_lang_translations": {

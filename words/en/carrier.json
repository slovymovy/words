                },
                {
                    "sense_id": "c09114d9-bdeb-3150-b995-bc96ded1ddea",
                    "sense_definition": "A Northern Athabaskan language spoken in Canada.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "language_carrier",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Linguists are studying the dialects of the <w>Carrier</w> language.",
                            "target_lang_translations": {
                                "ru": "Лингвисты изучают диалекты языка <w>кэрриер</w>."
                            }
                        },
                        {
                            "text": "There are efforts to teach <w>Carrier</w> to the younger generation to preserve it.",
                            "target_lang_translations": {
                                "ru": "Предпринимаются усилия по обучению языку <w>кэрриер</w> молодого поколения, чтобы сохранить его."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык северной атабаскской группы, на котором говорят в Канаде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кэрриер",
                                "target_lang_sense_clarification": "язык"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
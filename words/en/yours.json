{
    "word_family": [
        "you",
        "your",
        "yourself",
        "yourselves"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "1913fe4a-1e4a-320b-9fab-55d14b73a336",
                    "sense_definition": "That or those belonging to you.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I think this blue pen is <w>yours</w>.",
                            "target_lang_translations": {
                                "ru": "Думаю, эта синяя ручка — <w>твоя</w>."
                            }
                        },
                        {
                            "text": "The final decision is entirely <w>yours</w>.",
                            "target_lang_translations": {
                                "ru": "Окончательное решение полностью <w>твоё</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "yours and mine",
                        "is yours"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что принадлежит тебе или вам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твой",
                                "target_lang_sense_clarification": "принадлежащий тебе (единственное число)"
                            },
                            {
                                "target_lang_word": "ваш",
                                "target_lang_sense_clarification": "принадлежащий вам (множественное число или вежливо)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e2b16fd-7be1-389d-9ce5-49a9200221e6",
                    "sense_definition": "A formula written at the end of a letter before the signature.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please sign the letter with <w>Yours</w> sincerely.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подпиши письмо фразой «Искренне <w>Ваш</w>»."
                            }
                        },
                        {
                            "text": "I look forward to hearing from you; <w>Yours</w> faithfully, John.",
                            "target_lang_translations": {
                                "ru": "С нетерпением жду ответа; с уважением, <w>Ваш</w> Джон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Yours sincerely",
                        "Yours faithfully",
                        "Sincerely yours"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формула вежливости в конце письма перед подписью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ваш",
                                "target_lang_sense_clarification": "в подписи письма (например: Искренне Ваш)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be106e17-ba01-33a6-8ed0-17a9f581ec6b",
                    "sense_definition": "Your house or home.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Shall we go back to <w>yours</w> after the movie?",
                            "target_lang_translations": {
                                "ru": "Поедем <w>к тебе</w> после фильма?"
                            }
                        },
                        {
                            "text": "I think I left my umbrella at <w>yours</w>.",
                            "target_lang_translations": {
                                "ru": "Кажется, я оставил зонт <w>у тебя</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "back to yours",
                        "at yours"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "referring to one's home"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Твой дом или жилище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к тебе",
                                "target_lang_sense_clarification": "домой к тебе (разговорное)"
                            },
                            {
                                "target_lang_word": "у тебя",
                                "target_lang_sense_clarification": "дома у тебя (разговорное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
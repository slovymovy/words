{
    "word_family": [
        "rol",
        "rollen",
        "oprolling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "96582f1d-7da5-3ac7-9eb3-d5e91c1b9d2d",
                    "sense_definition": "Iets opvouwen of draaien zodat het de vorm van een cilinder krijgt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_rolling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moest zijn slaapzak <w>oprollen</w> voordat hij in de auto stapte.",
                            "target_lang_translations": {
                                "ru": "Ему нужно было <w>свернуть</w> свой спальный мешок, прежде чем сесть в машину."
                            }
                        },
                        {
                            "text": "Omdat het zo warm was, besloot hij zijn mouwen op te <w>rollen</w>.",
                            "target_lang_translations": {
                                "ru": "Так как было очень жарко, он решил <w>закатать</w> рукава."
                            }
                        }
                    ],
                    "synonyms": [
                        "opdraaien"
                    ],
                    "antonyms": [
                        "uiteenrollen",
                        "ontrollen"
                    ],
                    "common_phrases": [
                        "de mouwen oprollen",
                        "een tapijt oprollen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сворачивать или скручивать что-либо, придавая этому форму цилиндра или рулона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сворачивать",
                                "target_lang_sense_clarification": "скручивать в трубку или рулон (ковер, бумагу)"
                            },
                            {
                                "target_lang_word": "засучивать",
                                "target_lang_sense_clarification": "подворачивать кверху (рукава, штанины)"
                            },
                            {
                                "target_lang_word": "скатывать",
                                "target_lang_sense_clarification": "сматывать в клубок или рулон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e847fb74-560e-35c9-ab77-25623d525428",
                    "sense_definition": "Een criminele organisatie of bende opsporen, ontmantelen en de leden arresteren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_arrest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft gisteren een grote drugsbende <w>opgerold</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера полиция <w>ликвидировала</w> крупную банду наркоторговцев."
                            }
                        },
                        {
                            "text": "Het kostte de recherche maanden om het netwerk volledig op te <w>rollen</w>.",
                            "target_lang_translations": {
                                "ru": "Следователям потребовались месяцы, чтобы полностью <w>обезвредить</w> эту сеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontmantelen",
                        "arresteren",
                        "inrekenen"
                    ],
                    "common_phrases": [
                        "een bende oprollen",
                        "een netwerk oprollen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрыть преступную организацию, прекратить её деятельность и арестовать участников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "уничтожать преступную группу"
                            },
                            {
                                "target_lang_word": "обезвредить",
                                "target_lang_sense_clarification": "лишать возможности действовать (банду, сеть)"
                            },
                            {
                                "target_lang_word": "накрыть",
                                "target_lang_sense_clarification": "застать врасплох и арестовать (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fe02951-b721-377c-98f3-34e46f436384",
                    "sense_definition": "Zichzelf klein maken door armen en benen in te trekken, vaak om te slapen of uit angst.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_rolling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De egel <w>rolde</w> zich <w>op</w> toen hij het gevaar zag naderen.",
                            "target_lang_translations": {
                                "ru": "Ёж <w>свернулся</w> в клубок, когда увидел приближающуюся опасность."
                            }
                        },
                        {
                            "text": "Zij wilde zich het liefst onder de warme dekens <w>oprollen</w>.",
                            "target_lang_translations": {
                                "ru": "Ей больше всего хотелось <w>свернуться</w> калачиком под теплыми одеялами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich oprollen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сжиматься в комок, подтягивая руки и ноги, часто для сна или от страха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сворачиваться",
                                "target_lang_sense_clarification": "принимать позу эмбриона или шара"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "główny faworyt",
                        "faworyt bukmacherów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, команда или животное, имеющие наибольшие шансы на победу в соревнованиях или конкурсе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фаворит",
                                "target_lang_sense_clarification": "вероятный победитель"
                            },
                            {
                                "target_lang_word": "претендент",
                                "target_lang_sense_clarification": "на победу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21dbf9eb-0c43-3759-bf28-850a0b31e33a",
                    "sense_definition": "Osoba szczególnie lubiana, wyróżniana przez kogoś i ciesząca się specjalnymi względami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_liked",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako najmłodszy syn był <w>faworytem</w> matki.",

{
    "word_family": [
        "suchlike",
        "nonsuch"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "34c47f84-3dce-3a05-a11e-968b2c74d804",
                    "sense_definition": "Used to emphasize the degree, quality, or extent of something; very or to a great degree.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was <w>such</w> a lovely party that nobody wanted to leave.",
                            "target_lang_translations": {
                                "ru": "Это была <w>такая</w> чудесная вечеринка, что никто не хотел уходить."
                            }
                        },
                        {
                            "text": "You are <w>such</w> a good friend to help me with this.",
                            "target_lang_translations": {
                                "ru": "Ты <w>такой</w> хороший друг, раз помогаешь мне с этим."
                            }
                        },
                        {
                            "text": "Why are you in <w>such</w> a hurry?",
                            "target_lang_translations": {
                                "ru": "Почему ты в <w>такой</w> спешке?"
                            }
                        }
                    ],
                    "synonyms": [
                        "what"
                    ],
                    "common_phrases": [
                        "such a",
                        "such a lot",
                        "such a thing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления степени, качества или масштаба чего-либо; очень или в значительной степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такой",
                                "target_lang_sense_clarification": "усилительная частица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de3cb75e-508d-3d7d-83ba-a29c2f9fd1da",
                    "sense_definition": "Of the type previously mentioned or implied; of this or that kind.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have never seen <w>such</w> beautiful flowers in my life.",
                            "target_lang_translations": {
                                "ru": "Я никогда в жизни не видел <w>таких</w> красивых цветов."
                            }
                        },
                        {
                            "text": "There is no <w>such</w> thing as a free lunch.",
                            "target_lang_translations": {
                                "ru": "Не существует <w>такой</w> вещи, как бесплатный обед."
                            }
                        },
                        {
                            "text": "Animals <w>such</w> as lions and tigers are carnivores.",
                            "target_lang_translations": {
                                "ru": "Животные, <w>такие</w> как львы и тигры, являются плотоядными."
                            }
                        }
                    ],
                    "common_phrases": [
                        "such as",
                        "no such thing",
                        "such and such"
                    ],
                    "target_lang_definitions": {
                        "ru": "Того типа, который упоминался ранее или подразумевался; такого или этакого рода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такой",
                                "target_lang_sense_clarification": "подобный этому"
                            },
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "схожий"
                            },
                            {
                                "target_lang_word": "этакий",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea358b5d-ea38-37b2-9f22-304ecd24b29e",
                    "sense_definition": "Any; whatever kind of; used to refer to a person or thing of a specific kind, often in formal conditions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please send the documents to <w>such</w> address as may be provided later.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте документы на <w>такой</w> адрес, который может быть предоставлен позже."
                            }
                        },
                        {
                            "text": "Any <w>such</w> agreement must be signed by both parties.",
                            "target_lang_translations": {
                                "ru": "Любое <w>подобное</w> соглашение должно быть подписано обеими сторонами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой; какого бы то ни было рода; используется для обозначения лица или предмета определенного вида (часто в официальном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такой",
                                "target_lang_sense_clarification": "упомянутый"
                            },
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "аналогичный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c154b73e-9ce7-30fb-981d-506a86a7605c",
                    "sense_definition": "A certain or particular person or thing, not specified by name.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The text states that we should go into <w>such</w> a city and stay there.",
                            "target_lang_translations": {
                                "ru": "В тексте сказано, что мы должны пойти в <w>такой-то</w> город и остаться там."
                            }
                        },
                        {
                            "text": "He told me that <w>such</w> a knight had arrived at the castle.",
                            "target_lang_translations": {
                                "ru": "Он сказал мне, что <w>такой-то</w> рыцарь прибыл в замок."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное или конкретное лицо или вещь, не названные по имени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такой-то",
                                "target_lang_sense_clarification": "неопределенный, имярек"
                            },
                            {
                                "target_lang_word": "некий",
                                "target_lang_sense_clarification": "какой-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "f5ca972e-b68f-36d1-b078-96d901b5d40c",
                    "sense_definition": "Someone or something of the same kind as that already mentioned.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sells basic goods like milk, bread, and <w>such</w>.",
                            "target_lang_translations": {
                                "ru": "Он продает основные товары, такие как молоко, хлеб и <w>тому подобное</w>."
                            }
                        },
                        {
                            "text": "<w>Such</w> is the nature of the business world.",
                            "target_lang_translations": {
                                "ru": "<w>Такова</w> природа делового мира."
                            }
                        },
                        {
                            "text": "We discussed plans, budgets, and <w>such</w> during the meeting.",
                            "target_lang_translations": {
                                "ru": "Мы обсуждали планы, бюджеты и <w>тому подобное</w> во время встречи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "and such",
                        "such is life",
                        "such as it is"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кто-то или что-то того же рода, что уже упоминалось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тому подобное",
                                "target_lang_sense_clarification": "и прочее"
                            },
                            {
                                "target_lang_word": "таков",
                                "target_lang_sense_clarification": "краткая форма прилагательного"
                            },
                            {
                                "target_lang_word": "таковой",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "53da6bb0-7967-3549-9fac-ce484681761e",
                    "sense_definition": "An item or entity indicated as being similar to another; a specific instance of a quality.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "philosophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The philosopher argued about the distinction between thises and <w>suches</w>.",
                            "target_lang_translations": {
                                "ru": "Философ спорил о различии между «этостями» и «<w>таковостями</w>»."
                            }
                        },
                        {
                            "text": "In this theory, <w>suches</w> represent the qualitative aspects of objects.",
                            "target_lang_translations": {
                                "ru": "В этой теории «<w>таковости</w>» представляют качественные аспекты объектов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical in philosophy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или сущность, обозначенные как подобные другому; конкретный пример качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таковость",
                                "target_lang_sense_clarification": "философский термин"
                            },
                            {
                                "target_lang_word": "подобное",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "1fdbe704-7e36-3ae8-abe2-d2e4ded7431e",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Such</w> was known for his contributions to the local library.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Сач</w> был известен своим вкладом в местную библиотеку."
                            }
                        },
                        {
                            "text": "I met a family named <w>Such</w> while traveling in Europe.",
                            "target_lang_translations": {
                                "ru": "Я встретил семью по фамилии <w>Сач</w> во время путешествия по Европе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сач",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
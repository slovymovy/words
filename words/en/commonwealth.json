                {
                    "sense_id": "c154fd7b-2979-3daa-b3a1-5cf11a9407c0",
                    "sense_definition": "An international association consisting mostly of independent states that were previously part of the British Empire.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "political_entity",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Canada and Australia are active members of the <w>Commonwealth</w>.",
                            "target_lang_translations": {
                                "ru": "Канада и Австралия являются активными членами <w>Содружества</w>."
                            }
                        },
                        {
                            "text": "Leaders from across the <w>Commonwealth</w> gathered for the summit in London.",
                            "target_lang_translations": {
                                "ru": "Лидеры стран <w>Содружества</w> собрались на саммит в Лондоне."
                            }

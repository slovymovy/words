                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола \"roeten\".",
                        "en": "Imperative mood of the verb 'roeten'."

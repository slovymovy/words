            "senses": [
                {
                    "sense_id": "6180b43b-d6ac-3c02-9a16-e7b762e2c9dc",
                    "sense_definition": "Nadludzka istota duchowa będąca przedmiotem kultu religijnego, szczególnie w religiach politeistycznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytni Grecy czcili wiele różnych <w>bóstw</w> zamieszkujących górę Olimp.",
                            "target_lang_translations": {
                                "ru": "Древние греки поклонялись множеству различных <w>божеств</w>, обитающих на горе Олимп."
                            }
                        },
                        {
                            "text": "Miejscowa ludność składała dary swojemu <w>bóstwu</w>, prosząc o deszcz i urodzaj.",
                            "target_lang_translations": {
                                "ru": "Местные жители приносили дары своему <w>божеству</w>, прося дождя и урожая."
                            }
                        }
                    ],
                    "synonyms": [
                        "bóg",
                        "bożek"
                    ],
                    "common_phrases": [
                        "bóstwo opiekuńcze",
                        "czcić bóstwo",
                        "pogańskie bóstwo",
                        "składać ofiary bóstwu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сверхъестественное существо, являющееся объектом религиозного поклонения, особенно в политеистических религиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "божество",
                                "target_lang_sense_clarification": "сверхъестественная сущность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43a6a571-ef83-3a6a-a654-81df8eb8e278",
                    "sense_definition": "Osoba lub przedmiot będący obiektem niezwykłego uwielbienia, podziwu i adoracji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla swoich wiernych fanów piosenkarka była prawdziwym <w>bóstwem</w>.",
                            "target_lang_translations": {
                                "ru": "Для своих преданных фанатов певица была настоящим <w>божеством</w>."
                            }
                        },
                        {
                            "text": "On patrzył na nią jak na <w>bóstwo</w> i spełniał każdą jej zachciankę.",
                            "target_lang_translations": {
                                "ru": "Он смотрел на неё как на <w>божество</w> и исполнял любую её прихоть."
                            }
                        }
                    ],
                    "synonyms": [
                        "bożyszcze",
                        "idol"
                    ],
                    "common_phrases": [
                        "traktować jak bóstwo",
                        "szkolne bóstwo",
                        "wyglądać jak bóstwo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w znaczeniu przenośnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, являющийся объектом особого поклонения, восхищения и обожания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "божество",
                                "target_lang_sense_clarification": "предмет обожания"
                            },
                            {
                                "target_lang_word": "кумир",
                                "target_lang_sense_clarification": "идол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c746c2da-7af9-3a22-8c0d-06af661f1706",
                    "sense_definition": "Natura lub istota bycia bogiem; cecha tego, co boskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teolodzy przez wieki dyskutowali o <w>bóstwie</w> i człowieczeństwie Jezusa.",
                            "target_lang_translations": {
                                "ru": "Теологи веками спорили о <w>божестве</w> и человечности Иисуса."
                            }
                        },
                        {
                            "text": "Cesarz rzymski często przypisywał sobie <w>bóstwo</w>, wymagając czci religijnej od poddanych.",
                            "target_lang_translations": {
                                "ru": "Римский император часто приписывал себе <w>божественность</w>, требуя религиозного поклонения от подданных."
                            }
                        }
                    ],
                    "synonyms": [

{
    "word_family": [
        "выступление",
        "выступ"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6887490f-271c-37f9-a52f-62cc0c98e76f",
                    "sense_definition": "Публично произносить речь, делать доклад или исполнять номер перед аудиторией.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "public_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор школы будет <выступать> с речью на выпускном вечере.",
                            "target_lang_translations": {
                                "en": "The school principal will <speak> at the graduation ceremony."
                            }
                        },
                        {
                            "text": "Известная певица планирует <выступать> на сцене нашего театра в следующем месяце.",
                            "target_lang_translations": {
                                "en": "A famous singer plans to <perform> on the stage of our theater next month."
                            }
                        }
                    ],
                    "antonyms": [
                        "молчать",
                        "наблюдать"
                    ],
                    "common_phrases": [
                        "выступать на сцене",
                        "выступать с докладом",
                        "выступать перед публикой",
                        "выступать с речью"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak publicly, give a report, or perform an act before an audience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perform",
                                "target_lang_sense_clarification": "on stage, artistically"
                            },
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "give a speech"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "publicly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f4b3087-7200-344b-9bf8-6997fb4260d9",
                    "sense_definition": "Участвовать в спортивных соревнованиях, представлять команду или страну.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш спортсмен будет <выступать> на Олимпийских играх под национальным флагом.",
                            "target_lang_translations": {
                                "en": "Our athlete will <compete> in the Olympic Games under the national flag."
                            }
                        },
                        {
                            "text": "Футбольный клуб отказался <выступать> в турнире из-за травм игроков.",
                            "target_lang_translations": {
                                "en": "The football club refused to <play> in the tournament due to player injuries."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выступать за сборную",
                        "выступать на соревнованиях",
                        "выступать за команду"
                    ],
                    "target_lang_definitions": {
                        "en": "To participate in sports competitions, represent a team or country."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "compete",
                                "target_lang_sense_clarification": "in a tournament or event"
                            },

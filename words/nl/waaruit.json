    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2bd56de7-19b6-3d51-b053-9c9f573e4341",
                    "sense_definition": "Uit wat? Wordt gebruikt in een vraag om de oorsprong, inhoud of samenstelling te bepalen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Waaruit</w> bestaat het menu voor vanavond?",
                            "target_lang_translations": {
                                "en": "What does the menu for tonight consist <w>of</w>?",
                                "ru": "<w>Из чего</w> состоит меню на сегодня?"
                            }
                        },
                        {
                            "text": "Kun je me vertellen <w>waaruit</w> dit materiaal is gemaakt?",
                            "target_lang_translations": {
                                "en": "Can you tell me what this material is made <w>of</w>?",
                                "ru": "Можешь сказать мне, <w>из чего</w> сделан этот материал?"
                            }
                        }
                    ],
                    "antonyms": [
                        "waarin"
                    ],
                    "common_phrases": [
                        "waaruit bestaat",
                        "waaruit blijkt"
                    ],
                    "target_lang_definitions": {
                        "en": "From what? Used in a question to determine origin, content, or composition.",
                        "ru": "Из чего? Используется в вопросах для определения происхождения, содержания или состава."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of what",
                                "target_lang_sense_clarification": "regarding composition"
                            },
                            {
                                "target_lang_word": "from what",
                                "target_lang_sense_clarification": "regarding origin or source"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "из чего",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30274aff-d061-3084-bdc3-035cc3bb6a24",
                    "sense_definition": "Uit hetwelk; verwijst terug naar iets waaruit iets komt, gemaakt is of concludeerbaar is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het glas <w>waaruit</w> de gasten hebben gedronken.",
                            "target_lang_translations": {
                                "en": "This is the glass <w>from which</w> the guests drank.",
                                "ru": "Это стакан, <w>из которого</w> пили гости."
                            }
                        },
                        {
                            "text": "Hij toonde de brief <w>waaruit</w> bleek dat hij de waarheid sprak.",
                            "target_lang_translations": {
                                "en": "He showed the letter <w>from which</w> it appeared that he spoke the truth.",
                                "ru": "Он показал письмо, <w>из которого</w> следовало, что он говорил правду."
                            }
                        }
                    ],
                    "antonyms": [
                        "waarin"
                    ],
                    "common_phrases": [
                        "de bron waaruit",
                        "het materiaal waaruit"
                    ],
                    "target_lang_definitions": {
                        "en": "From which; refers back to something from which something originates, is made, or can be concluded.",
                        "ru": "Из которого; отсылает к предмету, из которого что-то происходит, сделано или на основании которого делается вывод."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from which",
                                "target_lang_sense_clarification": "relative reference"
                            },
                            {
                                "target_lang_word": "out of which",
                                "target_lang_sense_clarification": "physical source"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "из которого",
                                "target_lang_sense_clarification": "относительное местоимение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
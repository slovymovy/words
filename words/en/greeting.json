    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1458a61-33ed-3900-97f8-5be8d95341b0",
                    "sense_definition": "A conventional phrase used to start a letter or conversation or otherwise to acknowledge a person's arrival or presence.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is polite to begin a formal letter with a proper <w>greeting</w>.",
                            "target_lang_translations": {
                                "ru": "Вежливо начинать официальное письмо с соответствующего <w>приветствия</w>."
                            }
                        },
                        {
                            "text": "She gave a friendly <w>greeting</w> to everyone in the room.",
                            "target_lang_translations": {
                                "ru": "Она обратилась ко всем в комнате с дружеским <w>приветствием</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "salutation"
                    ],
                    "antonyms": [
                        "farewell"
                    ],
                    "common_phrases": [
                        "warm greeting",
                        "friendly greeting",
                        "exchange greetings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общепринятая фраза или жест, используемые для начала разговора, письма или признания чьего-либо присутствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствие",
                                "target_lang_sense_clarification": "слова или фраза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32919dcd-418a-32d0-80ab-a7d7012bdbaf",
                    "sense_definition": "The action of the verb to <w>greet</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He raised his hand in <w>greeting</w> as he walked past us.",
                            "target_lang_translations": {
                                "ru": "Он поднял руку в знак <w>приветствия</w>, проходя мимо нас."
                            }
                        },
                        {
                            "text": "Her <w>greeting</w> was warm and sincere when we arrived.",
                            "target_lang_translations": {
                                "ru": "Её <w>приветствие</w> было тёплым и искренним, когда мы приехали."
                            }
                        }
                    ],
                    "synonyms": [
                        "salutation"
                    ],
                    "antonyms": [
                        "farewell"
                    ],
                    "common_phrases": [
                        "nod of greeting",
                        "shout a greeting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по глаголу «приветствовать»; знак внимания при встрече."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствие",
                                "target_lang_sense_clarification": "действие или жест"
                            }
                        ]
                    }
                }
            ]
        },

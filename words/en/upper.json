                {
                    "sense_id": "1ddbccd5-0e30-3ed2-a049-ed8806b8f005",
                    "sense_definition": "Located at a higher level, rank, or position.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>upper</w> shelves in the kitchen are too high for the children to reach.",

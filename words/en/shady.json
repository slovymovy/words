    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "283c667b-8584-34af-985d-6b0ad27c93b2",
                    "sense_definition": "Protected from direct sunlight, often by trees or buildings, or providing such protection.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to have our picnic in a cool, <w>shady</w> spot under the oak tree.",
                            "target_lang_translations": {
                                "ru": "Мы решили устроить пикник в прохладном <w>тенистом</w> месте под дубом."
                            }
                        },
                        {
                            "text": "The garden is very <w>shady</w> in the afternoon, making it a perfect place to read.",
                            "target_lang_translations": {
                                "ru": "Днем сад очень <w>тенистый</w>, что делает его идеальным местом для чтения."
                            }
                        }
                    ],
                    "antonyms": [
                        "sunny",
                        "bright"
                    ],
                    "common_phrases": [
                        "shady spot",
                        "shady tree",
                        "shady area",
                        "shady side"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищенный от прямых солнечных лучей, часто деревьями или зданиями, или создающий такую защиту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тенистый",
                                "target_lang_sense_clarification": "обильный тенью (о месте, саде, дереве)"
                            },
                            {
                                "target_lang_word": "находящийся в тени",
                                "target_lang_sense_clarification": "расположенный там, где нет солнца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bc957a5-4bd5-3047-ba40-34ea4d60ab65",
                    "sense_definition": "Seeming dishonest, illegal, or untrustworthy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He refused to sign the contract because it looked like a <w>shady</w> deal.",
                            "target_lang_translations": {
                                "ru": "Он отказался подписывать контракт, так как это выглядело как <w>сомнительная</w> сделка."
                            }
                        },
                        {
                            "text": "The police kept an eye on the <w>shady</w> character standing on the street corner.",
                            "target_lang_translations": {
                                "ru": "Полиция следила за <w>подозрительным</w> типом, стоящим на углу улицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "suspicious",
                        "dodgy",
                        "sketchy",
                        "dubious",
                        "corrupt",
                        "seedy"
                    ],
                    "antonyms": [
                        "trustworthy",
                        "honest",
                        "reputable"
                    ],
                    "common_phrases": [
                        "shady character",
                        "shady deal",
                        "shady business",
                        "shady past"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кажущийся нечестным, незаконным или ненадежным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подозрительный",
                                "target_lang_sense_clarification": "вызывающий недоверие или опасения"
                            },
                            {
                                "target_lang_word": "сомнительный",
                                "target_lang_sense_clarification": "ненадежный, внушающий сомнения (о сделке, бизнесе)"
                            },
                            {
                                "target_lang_word": "тёмный",
                                "target_lang_sense_clarification": "разг. связанный с чем-то незаконным или непонятным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0477df69-255c-310b-bf53-bd82b8ef6191",
                    "sense_definition": "Acting in a mean, cruel, or disrespectful way towards someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be so <w>shady</w>; just tell her the truth nicely instead of mocking her.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>подлым</w>; просто скажи ей правду вежливо, вместо того чтобы насмехаться."
                            }
                        },
                        {
                            "text": "He was being really <w>shady</w> by ignoring her messages while posting online.",
                            "target_lang_translations": {
                                "ru": "Он вёл себя очень <w>мутно</w>, игнорируя её сообщения, но публикуя посты в сети."
                            }

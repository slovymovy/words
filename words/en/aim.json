    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd6ec283-2cf6-3025-8eb5-ff698a38b081",
                    "sense_definition": "A purpose, intention, or desired result that one works towards.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intention_goal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her main <w>aim</w> in life is to become a doctor.",
                            "target_lang_translations": {
                                "ru": "Ее главная <w>цель</w> в жизни — стать врачом."
                            }
                        },
                        {
                            "text": "The organization's primary <w>aim</w> is to help homeless people find shelter.",
                            "target_lang_translations": {
                                "ru": "Основная <w>цель</w> организации — помочь бездомным найти приют."
                            }
                        }
                    ],
                    "synonyms": [
                        "purpose",
                        "intention",
                        "object",
                        "end",
                        "aspiration",
                        "design",
                        "scheme"
                    ],
                    "common_phrases": [
                        "main aim",
                        "primary aim",
                        "with the aim of",
                        "achieve an aim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цель, намерение или желаемый результат, к которому кто-то стремится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цель",
                                "target_lang_sense_clarification": "результат, к которому стремятся"
                            },
                            {
                                "target_lang_word": "намерение",
                                "target_lang_sense_clarification": "замысел, желание сделать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b976a56c-cd49-3a1e-a251-97fded99d513",
                    "sense_definition": "The action of pointing a weapon or object at a target.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_pointing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunter took <w>aim</w> at the deer but decided not to shoot.",
                            "target_lang_translations": {
                                "ru": "Охотник взял оленя на <w>прицел</w>, но решил не стрелять."
                            }
                        },
                        {
                            "text": "Steady your <w>aim</w> before you pull the trigger.",
                            "target_lang_translations": {
                                "ru": "Выровняй <w>прицел</w>, прежде чем нажать на курок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take aim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие наведения оружия или предмета на цель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прицел",
                                "target_lang_sense_clarification": "направление оружия на объект"
                            },
                            {
                                "target_lang_word": "прицеливание",
                                "target_lang_sense_clarification": "процесс наведения на цель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfb6d22e-a135-3c3d-b2cc-bf639a6fbf1e",
                    "sense_definition": "The ability to hit a target accurately.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_pointing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has excellent <w>aim</w> and rarely misses the bullseye.",
                            "target_lang_translations": {
                                "ru": "У нее отличная <w>меткость</w>, и она редко промахивается мимо «яблочка»."
                            }
                        },
                        {
                            "text": "His <w>aim</w> was poor, so the ball went wide of the goal.",
                            "target_lang_translations": {
                                "ru": "Его <w>прицел</w> был неточен, поэтому мяч пролетел мимо ворот."
                            }
                        }
                    ],
                    "common_phrases": [
                        "good aim",
                        "bad aim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность точно поражать цель; меткость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меткость",
                                "target_lang_sense_clarification": "способность точно попадать в цель"
                            },
                            {
                                "target_lang_word": "прицел",
                                "target_lang_sense_clarification": "разг. точность попадания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6b348ca7-d18e-306f-8549-a2ab12452a3a",
                    "sense_definition": "To plan, hope, or intend to achieve a specific purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intention_goal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>aim</w> to finish the project by next Friday.",
                            "target_lang_translations": {
                                "ru": "Мы <w>стремимся</w> завершить проект к следующей пятнице."
                            }
                        },
                        {
                            "text": "The new educational program <w>aims</w> to improve student literacy rates.",
                            "target_lang_translations": {
                                "ru": "Новая образовательная программа <w>нацелена</w> на повышение уровня грамотности студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "tendency",
                        "aspiration",
                        "intention"
                    ],
                    "common_phrases": [
                        "aim to do",
                        "aim for",
                        "aim high"
                    ],
                    "target_lang_definitions": {
                        "ru": "Планировать, надеяться или намереваться достичь определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стремиться",
                                "target_lang_sense_clarification": "настойчиво добиваться цели"
                            },
                            {
                                "target_lang_word": "намереваться",
                                "target_lang_sense_clarification": "иметь желание сделать что-либо"
                            },
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "составлять план действий"
                            },
                            {
                                "target_lang_word": "метить",
                                "target_lang_sense_clarification": "разг. претендовать на что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e9f9bfe-ed59-3d85-b45a-ea2e50e74148",
                    "sense_definition": "To point a weapon or object towards a target with the intent of hitting it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_pointing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>aimed</w> the camera at the sunset to capture the beautiful colors.",
                            "target_lang_translations": {
                                "ru": "Он <w>навел</w> камеру на закат, чтобы запечатлеть красивые цвета."
                            }
                        },
                        {
                            "text": "The archer <w>aimed</w> carefully at the target before releasing the arrow.",
                            "target_lang_translations": {
                                "ru": "Лучник тщательно <w>прицелился</w> в мишень, прежде чем выпустить стрелу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aim at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наводить оружие или предмет на цель с намерением попасть в нее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целиться",
                                "target_lang_sense_clarification": "наводить оружие на цель"
                            },
                            {
                                "target_lang_word": "прицеливаться",
                                "target_lang_sense_clarification": "тщательно наводить оружие"
                            },
                            {
                                "target_lang_word": "наводить",
                                "target_lang_sense_clarification": "направлять предмет в сторону цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6d3d2c2-5d20-3922-8f4c-113c7e14deac",
                    "sense_definition": "To direct information, a product, or a remark towards a specific group of people.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_pointing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The advertising campaign is <w>aimed</w> at young adults.",
                            "target_lang_translations": {
                                "ru": "Рекламная кампания <w>нацелена</w> на молодых людей."
                            }
                        },
                        {
                            "text": "Her criticism was clearly <w>aimed</w> at the management team.",
                            "target_lang_translations": {
                                "ru": "Ее критика была явно <w>направлена</w> на руководство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aimed at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять информацию, продукт или замечание на определенную группу людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нацеливать",
                                "target_lang_sense_clarification": "направлять на кого-либо или что-либо"
                            },
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "адресовать кому-либо"
                            },
                            {
                                "target_lang_word": "предназначать",
                                "target_lang_sense_clarification": "выделять для определенной цели"
                            }
                        ]
                    }
                }
            ]
        },

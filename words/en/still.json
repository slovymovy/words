                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Would you like sparkling or <w>still</w>?",
                            "target_lang_translations": {
                                "ru": "Вам с газом или <w>без газа</w>?"
                            }
                        },
                        {
                            "text": "I will have a glass of <w>still</w>, please.",
                            "target_lang_translations": {
                                "ru": "Мне стакан <w>негазированной</w>, пожалуйста."

            "senses": [
                {
                    "sense_id": "24e24307-d3ae-38d6-b3ac-172f19060c9f",
                    "sense_definition": "The act of formulating a course of action or drawing up plans.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Careful <w>planning</w> is essential for the success of any large project.",
                            "target_lang_translations": {
                                "ru": "Тщательное <w>планирование</w> необходимо для успеха любого крупного проекта."
                            }
                        },
                        {
                            "text": "We are in the early stages of <w>planning</w> for our summer holiday.",
                            "target_lang_translations": {
                                "ru": "Мы находимся на ранних стадиях <w>планирования</w> нашего летнего отпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "preparation",
                        "organization",
                        "arrangement",
                        "design"
                    ],
                    "antonyms": [
                        "improvisation"
                    ],
                    "common_phrases": [
                        "strategic planning",
                        "financial planning",
                        "urban planning",
                        "advance planning",
                        "family planning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс разработки плана действий или составления планов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планирование",
                                "target_lang_sense_clarification": "составление планов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c79cb4d-ad1b-3ddf-a169-c7140493f0b5",
                    "sense_definition": "Official permission from a local authority to build or alter a building.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My neighbours were going to build an extension but they didn't get <w>planning</w>.",
                            "target_lang_translations": {
                                "ru": "Мои соседи собирались сделать пристройку, но не получили <w>разрешение на строительство</w>."
                            }
                        },
                        {
                            "text": "You must apply for <w>planning</w> before you start construction on the new garage.",
                            "target_lang_translations": {
                                "ru": "Вы должны подать заявку на <w>разрешение на строительство</w> до начала постройки нового гаража."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get planning",
                        "apply for planning",
                        "planning permission"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for planning permission"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное разрешение местных властей на строительство или перестройку здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрешение на строительство",
                                "target_lang_sense_clarification": "документ"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "867a29fa-645c-3530-85be-f0d1e9713f36",
                    "sense_definition": "To allow something to fall by releasing one's hold on it.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>drop</w> the glass on the hard floor.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не <w>урони</w> стакан на твердый пол."
                            }
                        },
                        {
                            "text": "He accidentally <w>dropped</w> his keys while walking to the car.",
                            "target_lang_translations": {
                                "ru": "Он случайно <w>уронил</w> ключи, пока шел к машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "release",
                        "let go"
                    ],
                    "antonyms": [
                        "hold",
                        "catch",
                        "grasp"
                    ],
                    "common_phrases": [
                        "drop the ball",
                        "drop a weapon",
                        "drop everything"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволить чему-то упасть, разжав пальцы или выпустив из рук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ронять",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "уронить",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "кидать вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4566d194-4aeb-38a5-a5f4-24fb8afe192d",
                    "sense_definition": "To fall vertically from a higher position to a lower one under the influence of gravity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ripe apple <w>dropped</w> from the tree and rolled onto the grass.",
                            "target_lang_translations": {
                                "ru": "Спелое яблоко <w>упало</w> с дерева и покатилось по траве."
                            }
                        },
                        {
                            "text": "Stones <w>drop</w> faster than feathers because of air resistance.",
                            "target_lang_translations": {
                                "ru": "Камни <w>падают</w> быстрее перьев из-за сопротивления воздуха."
                            }
                        }
                    ],
                    "synonyms": [
                        "fall",
                        "descend",
                        "plummet"
                    ],
                    "antonyms": [
                        "rise",
                        "ascend"
                    ],
                    "common_phrases": [
                        "drop to the ground",
                        "drop from the sky"
                    ],
                    "target_lang_definitions": {
                        "ru": "Падать вертикально с более высокой позиции на более низкую под действием гравитации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "перемещаться вниз"
                            },
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "совершить падение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c29fef1-d6b3-30d5-9097-536b53a1cad7",
                    "sense_definition": "To become lower in level, value, price, amount, or intensity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature is expected to <w>drop</w> significantly tonight.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что температура значительно <w>упадет</w> сегодня ночью."
                            }
                        },
                        {
                            "text": "House prices might <w>drop</w> if the economy slows down.",
                            "target_lang_translations": {
                                "ru": "Цены на жилье могут <w>упасть</w>, если экономика замедлится."
                            }
                        }
                    ],
                    "synonyms": [
                        "decrease",
                        "decline",
                        "diminish",
                        "dip"
                    ],
                    "antonyms": [
                        "increase",
                        "rise",
                        "soar"
                    ],
                    "common_phrases": [
                        "drop in price",
                        "drop sharply",
                        "temperature drops"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться ниже по уровню, стоимости, цене, количеству или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "снижаться (о цене, температуре)"
                            },
                            {
                                "target_lang_word": "снижаться",
                                "target_lang_sense_clarification": "уменьшаться"
                            },
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "становиться меньше"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00051e79-40a1-30c0-aedb-7b0cc324c915",
                    "sense_definition": "To take someone or something to a specific place and leave them there; usually used with 'off'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>drop</w> me at the train station on your way to work?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>высадить</w> меня у вокзала по пути на работу?"
                            }
                        },
                        {
                            "text": "I need to <w>drop</w> these books off at the library before it closes.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>завезти</w> эти книги в библиотеку, пока она не закрылась."
                            }
                        }
                    ],
                    "antonyms": [
                        "pick up"
                    ],
                    "common_phrases": [
                        "drop off",
                        "drop someone at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Довезти кого-то или что-то до определенного места и оставить там; высадить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высаживать",
                                "target_lang_sense_clarification": "пассажира"
                            },
                            {
                                "target_lang_word": "завезти",
                                "target_lang_sense_clarification": "доставить по пути"
                            },
                            {
                                "target_lang_word": "подбросить",
                                "target_lang_sense_clarification": "разг. довезти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a255c406-eedb-3d35-a6fd-9a1c05f3182a",
                    "sense_definition": "To stop doing, discussing, or continuing with something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cessation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's just <w>drop</w> the subject and talk about something happier.",
                            "target_lang_translations": {
                                "ru": "Давай просто <w>оставим</w> эту тему и поговорим о чем-то более веселом."
                            }
                        },
                        {
                            "text": "She decided to <w>drop</w> the history class because it was too difficult.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>бросить</w> курс истории, потому что он был слишком сложным."
                            }
                        }
                    ],
                    "synonyms": [
                        "abandon",
                        "quit",
                        "discontinue"
                    ],
                    "antonyms": [
                        "continue",
                        "pursue"
                    ],
                    "common_phrases": [
                        "drop the subject",
                        "drop a course",
                        "drop the charges"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать делать что-то, обсуждать или продолжать чем-то заниматься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "прекращать занятие"
                            },
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "перестать обсуждать"
                            },
                            {
                                "target_lang_word": "прекратить",
                                "target_lang_sense_clarification": "остановить действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad8f057b-3178-3da2-baec-d398d8e8484c",
                    "sense_definition": "To fall to the ground intentionally or suddenly, often due to exhaustion or a command.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The runners were ready to <w>drop</w> from exhaustion at the finish line.",
                            "target_lang_translations": {
                                "ru": "Бегуны были готовы <w>упасть</w> от изнеможения на финишной прямой."
                            }
                        },
                        {
                            "text": "The soldier was ordered to <w>drop</w> and do twenty push-ups.",
                            "target_lang_translations": {
                                "ru": "Солдату приказали <w>упасть</w> и сделать двадцать отжиманий."
                            }
                        }
                    ],
                    "synonyms": [
                        "collapse",
                        "fall down"
                    ],
                    "antonyms": [
                        "stand up"
                    ],
                    "common_phrases": [
                        "drop dead",
                        "drop to one's knees",
                        "ready to drop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно или внезапно упасть на землю, часто от изнеможения или по команде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "валиться с ног"
                            },
                            {
                                "target_lang_word": "валиться",
                                "target_lang_sense_clarification": "от усталости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adc9ed08-7a74-3572-9803-2d468015a1f2",
                    "sense_definition": "To visit someone informally without an appointment; usually used with 'by' or 'in'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Feel free to <w>drop</w> by my office if you have any questions.",
                            "target_lang_translations": {
                                "ru": "Не стесняйся <w>заглянуть</w> ко мне в офис, если у тебя возникнут вопросы."
                            }
                        },
                        {
                            "text": "We might <w>drop</w> in on your parents later this afternoon.",
                            "target_lang_translations": {
                                "ru": "Мы можем <w>заскочить</w> к твоим родителям сегодня попозже."
                            }
                        }
                    ],
                    "synonyms": [
                        "visit",
                        "stop by"
                    ],
                    "common_phrases": [
                        "drop by",
                        "drop in"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in casual conversation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Навестить кого-то неофициально без предварительной договоренности; заглянуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглянуть",
                                "target_lang_sense_clarification": "зайти ненадолго"
                            },
                            {
                                "target_lang_word": "зайти",
                                "target_lang_sense_clarification": "посетить по пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98d9c022-19af-3bea-a7a8-42a8e3069e1d",
                    "sense_definition": "To mention something or someone casually in conversation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He loves to <w>drop</w> the names of famous people he has met.",
                            "target_lang_translations": {
                                "ru": "Он обожает <w>упоминать</w> имена известных людей, которых встречал."
                            }
                        },
                        {
                            "text": "She managed to <w>drop</w> a hint about what she wanted for her birthday.",
                            "target_lang_translations": {
                                "ru": "Ей удалось <w>намекнуть</w> на то, что она хочет на день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "mention"
                    ],
                    "common_phrases": [
                        "drop a hint",
                        "name drop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упомянуть что-то или кого-то в разговоре как бы невзначай."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упомянуть",
                                "target_lang_sense_clarification": "назвать имя"
                            },
                            {
                                "target_lang_word": "вставить",
                                "target_lang_sense_clarification": "слово или замечание"
                            },
                            {
                                "target_lang_word": "ронять",
                                "target_lang_sense_clarification": "говорить невзначай"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19e46dc9-f067-3958-b3f4-55d04bb32e46",
                    "sense_definition": "To send a short message or letter.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>drop</w> me a line when you arrive safely at the hotel.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>черкни</w> мне пару строк, когда благополучно доберешься до отеля."
                            }
                        },
                        {
                            "text": "I will <w>drop</w> you an email with all the meeting details.",
                            "target_lang_translations": {
                                "ru": "Я <w>скину</w> тебе электронное письмо со всеми деталями встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "send",
                        "write"
                    ],
                    "common_phrases": [
                        "drop a line",
                        "drop a note"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправить короткое сообщение или письмо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черкнуть",
                                "target_lang_sense_clarification": "быстро написать"
                            },
                            {
                                "target_lang_word": "написать",
                                "target_lang_sense_clarification": "отправить сообщение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a97a043d-dab4-3518-88ca-1eef6e2600d4",
                    "sense_definition": "A very small quantity of liquid held together by surface tension, usually round in shape.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A single <w>drop</w> of rain fell on his nose.",
                            "target_lang_translations": {
                                "ru": "Одинокая <w>капля</w> дождя упала ему на нос."
                            }
                        },
                        {
                            "text": "Put three <w>drops</w> of oil into the mixture and stir well.",
                            "target_lang_translations": {
                                "ru": "Добавьте три <w>капли</w> масла в смесь и хорошо перемешайте."
                            }
                        }
                    ],
                    "synonyms": [
                        "bead",
                        "globule",
                        "drip"
                    ],
                    "common_phrases": [
                        "drop of water",
                        "rain drop",
                        "eye drops"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленькое количество жидкости, удерживаемое поверхностным натяжением, обычно круглой формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капля",
                                "target_lang_sense_clarification": "жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03031546-2f9b-39b9-bd53-bd9d7a596826",
                    "sense_definition": "A reduction in amount, level, quality, or degree.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There has been a sharp <w>drop</w> in sales this quarter.",
                            "target_lang_translations": {
                                "ru": "В этом квартале произошло резкое <w>падение</w> продаж."
                            }
                        },
                        {
                            "text": "The sudden <w>drop</w> in temperature made everyone shiver.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>понижение</w> температуры заставило всех дрожать."
                            }
                        }
                    ],
                    "synonyms": [
                        "decline",
                        "decrease",
                        "fall",
                        "reduction",
                        "dip"
                    ],
                    "antonyms": [
                        "rise",
                        "increase",
                        "surge"
                    ],
                    "common_phrases": [
                        "drop in price",
                        "sharp drop",
                        "sudden drop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снижение количества, уровня, качества или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "уменьшение величины"
                            },
                            {
                                "target_lang_word": "спад",
                                "target_lang_sense_clarification": "снижение активности"
                            },
                            {
                                "target_lang_word": "снижение",
                                "target_lang_sense_clarification": "уменьшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81cc1fbf-9102-3852-a5b1-a54e7149ce2c",
                    "sense_definition": "The vertical distance from a higher point to a lower one, or a steep slope.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful near the edge because there is a sheer fifty-foot <w>drop</w>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен у края, там отвесный пятнадцатиметровый <w>обрыв</w>."
                            }
                        },
                        {
                            "text": "The <w>drop</w> from the bridge to the water is quite dangerous.",
                            "target_lang_translations": {
                                "ru": "<w>Падение</w> с моста в воду довольно опасно."
                            }
                        }
                    ],
                    "synonyms": [
                        "descent",
                        "fall",
                        "slope"
                    ],
                    "common_phrases": [
                        "vertical drop",
                        "sheer drop",
                        "steep drop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальное расстояние от высшей точки до низшей или крутой склон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрыв",
                                "target_lang_sense_clarification": "крутой склон"
                            },
                            {
                                "target_lang_word": "перепад",
                                "target_lang_sense_clarification": "высот"
                            },
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "расстояние вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "746d0cb4-2f14-3f5f-a3d0-f9a8257ae9af",
                    "sense_definition": "The act of delivering something, or the location where something is delivered.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "logistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The courier has three more <w>drops</w> to make before lunch.",
                            "target_lang_translations": {
                                "ru": "Курьеру нужно сделать еще три <w>доставки</w> до обеда."
                            }
                        },
                        {
                            "text": "The spies arranged a secret <w>drop</w> for the classified documents.",
                            "target_lang_translations": {
                                "ru": "Шпионы организовали тайный <w>тайник</w> для секретных документов."
                            }
                        }
                    ],
                    "synonyms": [
                        "delivery"
                    ],
                    "antonyms": [
                        "pickup"
                    ],
                    "common_phrases": [
                        "make a drop",
                        "mail drop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт доставки чего-либо или место, куда что-то доставляется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доставка",
                                "target_lang_sense_clarification": "груза"
                            },
                            {
                                "target_lang_word": "тайник",
                                "target_lang_sense_clarification": "место закладки"
                            },
                            {
                                "target_lang_word": "сброс",
                                "target_lang_sense_clarification": "груза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fa88f66-6724-3564-b7d2-866310271b51",
                    "sense_definition": "A very small amount of something, often liquid or alcohol.",
                    "learner_level": "C1",
                    "frequency": "Low",

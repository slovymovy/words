{
    "word_family": [
        "бить",
        "битва",
        "биение",
        "бой",
        "боец",
        "добиться",
        "разбить",
        "убить",
        "забить",
        "выбить",
        "прибить",
        "сбить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "073bade9-f1e2-33d7-976f-c3027c7bc067",
                    "sense_definition": "Ритмично ударять, пульсировать (о сердце или пульсе).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её сердце <билось> так сильно, что она не могла говорить.",
                            "target_lang_translations": {
                                "en": "Her heart was <beating> so hard that she couldn't speak."
                            }
                        },
                        {
                            "text": "У пациента ровно <бьётся> пульс.",
                            "target_lang_translations": {
                                "en": "The patient's pulse is <beating> steadily."
                            }
                        }
                    ],
                    "synonyms": [
                        "колотиться"
                    ],
                    "common_phrases": [
                        "сердце бьётся",
                        "пульс бьётся",
                        "ровно биться",
                        "часто биться"
                    ],
                    "target_lang_definitions": {
                        "en": "To beat rhythmically or pulsate (of a heart or pulse)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to move with pulsation or throbbing"
                            },
                            {
                                "target_lang_word": "pulsate",
                                "target_lang_sense_clarification": "to expand and contract rhythmically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bcfe677-e0b3-35fd-ad17-0929c33261e2",
                    "sense_definition": "Драться, сражаться с врагом или противником.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты храбро <бились> за свою родину.",
                            "target_lang_translations": {
                                "en": "The soldiers <fought> bravely for their homeland."
                            }
                        },
                        {
                            "text": "Мы будем <биться> с врагом до последней капли крови.",
                            "target_lang_translations": {
                                "en": "We will <fight> the enemy to the last drop of blood."
                            }
                        }
                    ],
                    "synonyms": [
                        "драться",
                        "сражаться"
                    ],
                    "common_phrases": [
                        "биться до конца",
                        "биться насмерть",
                        "биться с врагом"
                    ],
                    "target_lang_definitions": {
                        "en": "To fight or battle against an enemy or opponent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fight",
                                "target_lang_sense_clarification": "to engage in physical combat"
                            },
                            {
                                "target_lang_word": "battle",
                                "target_lang_sense_clarification": "to struggle tenaciously against an opponent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28773f7b-2561-3f98-aa2d-d9bb50b71f52",
                    "sense_definition": "Ударяться обо что-либо твердое.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Птица <билась> в окно, пытаясь вылететь наружу.",
                            "target_lang_translations": {
                                "en": "The bird was <beating> against the window, trying to fly out."
                            }
                        },
                        {
                            "text": "Волны с шумом <бились> о скалы во время шторма.",
                            "target_lang_translations": {
                                "en": "The waves were <crashing> noisily against the rocks during the storm."
                            }
                        }
                    ],
                    "synonyms": [
                        "ударяться",
                        "стукаться"
                    ],
                    "common_phrases": [
                        "биться головой",
                        "биться о стену",
                        "биться в дверь"
                    ],
                    "target_lang_definitions": {
                        "en": "To hit or bump against something hard repeatedly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to strike repeatedly against something"
                            },
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "to come into contact forcefully"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to impact forcibly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcd6128d-580a-3636-9706-a5c4c4b7bc79",
                    "sense_definition": "Разбиваться на куски, терять целостность (о посуде, стекле).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта ваза очень хрупкая и легко <бьётся>.",
                            "target_lang_translations": {
                                "en": "This vase is very fragile and <breaks> easily."
                            }
                        },
                        {
                            "text": "Говорят, что посуда <бьётся> к счастью.",
                            "target_lang_translations": {
                                "en": "They say that dishes <breaking> brings good luck."
                            }
                        }
                    ],
                    "synonyms": [
                        "разбиваться"
                    ],
                    "common_phrases": [
                        "посуда бьётся",
                        "легко биться",
                        "стекло бьётся"
                    ],
                    "target_lang_definitions": {
                        "en": "To break into pieces or lose integrity (of dishes, glass)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "to separate into pieces as a result of a blow or shock"
                            },
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "to break suddenly and violently into pieces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18b6a416-b2ff-397f-9007-d9e20c7479cd",
                    "sense_definition": "Прилагать большие усилия, стараясь чего-то добиться или решить трудную задачу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученые уже много лет <бьются> над созданием этого лекарства.",
                            "target_lang_translations": {
                                "en": "Scientists have been <struggling> to create this medicine for many years."
                            }
                        },
                        {
                            "text": "Я целый час <бился> с этим замком, но так и не открыл его.",
                            "target_lang_translations": {
                                "en": "I <struggled> with this lock for a whole hour but never opened it."
                            }
                        }
                    ],
                    "synonyms": [
                        "трудиться",
                        "корпеть"
                    ],
                    "common_phrases": [
                        "биться над задачей",
                        "биться над решением",
                        "долго биться"
                    ],
                    "target_lang_definitions": {
                        "en": "To make great efforts trying to achieve something or solve a difficult task."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "struggle",
                                "target_lang_sense_clarification": "to make strenuous or violent efforts in the face of difficulties"
                            },
                            {
                                "target_lang_word": "strive",
                                "target_lang_sense_clarification": "to devote serious effort or energy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0890469f-0741-3e59-a8f4-619f2de9936c",
                    "sense_definition": "Совершать резкие, беспорядочные движения (например, в лихорадке или припадке).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пойманная рыба <билась> на дне лодки.",
                            "target_lang_translations": {
                                "en": "The caught fish was <thrashing> at the bottom of the boat."
                            }
                        },
                        {
                            "text": "Больной <бился> в лихорадке всю ночь.",
                            "target_lang_translations": {
                                "en": "The patient was <tossing> in a fever all night."
                            }
                        }
                    ],
                    "synonyms": [
                        "трепыхаться"
                    ],
                    "common_phrases": [
                        "биться в истерике",
                        "биться в конвульсиях",
                        "рыба бьётся"
                    ],
                    "target_lang_definitions": {
                        "en": "To make sharp, disorderly movements (e.g., in a fever or fit)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thrash",
                                "target_lang_sense_clarification": "to move about wildly or violently"
                            },
                            {
                                "target_lang_word": "writhe",
                                "target_lang_sense_clarification": "to make twisting, squirming movements"
                            },
                            {
                                "target_lang_word": "toss",
                                "target_lang_sense_clarification": "to move restlessly from side to side"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b9df40d-4561-3707-b927-8845662e3544",
                    "sense_definition": "Совпадать, сходиться (о цифрах, отчетах, данных).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_match",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У бухгалтера итоговые цифры никак не <бились>.",
                            "target_lang_translations": {
                                "en": "The accountant's final figures just wouldn't <tally>."
                            }
                        },
                        {
                            "text": "Твои слова не <бьются> с тем, что мы видели своими глазами.",
                            "target_lang_translations": {
                                "en": "Your words don't <add up> with what we saw with our own eyes."
                            }
                        }
                    ],
                    "synonyms": [
                        "сходиться"
                    ],
                    "common_phrases": [
                        "цифры не бьются",
                        "отчет не бьётся"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в профессиональной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To coincide or tally (of figures, reports, data)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tally",
                                "target_lang_sense_clarification": "to agree or correspond"
                            },
                            {
                                "target_lang_word": "add up",
                                "target_lang_sense_clarification": "to seem reasonable or consistent"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "141f60a7-2d94-3cf8-b74b-bbbebe7401f4",
                    "sense_definition": "Wykonać gwałtowny ruch ręką lub przedmiotem, aby zadać cios lub trafić w coś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_hit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna ze złością <uderzył> pięścią w drewniany stół.",
                            "target_lang_translations": {
                                "ru": "Мужчина со злости <ударил> кулаком по деревянному столу."
                            }
                        },
                        {
                            "text": "Napastnik <uderzył> ofiarę w twarz i uciekł z jej torebką.",
                            "target_lang_translations": {
                                "ru": "Нападавший <ударил> жертву по лицу и убежал с её сумочкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bić",
                        "walnąć",
                        "przyłożyć",
                        "grzmotnąć",
                        "trzepnąć"
                    ],
                    "antonyms": [
                        "głaskać"
                    ],
                    "common_phrases": [
                        "uderzyć pięścią w stół",
                        "uderzyć kogoś w twarz",
                        "mocno uderzyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести удар рукой или предметом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "нанести удар"
                            },
                            {
                                "target_lang_word": "стукнуть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecebd2ec-921c-3abd-b37b-80d10f9e5af4",
                    "sense_definition": "Zrobić sobie krzywdę poprzez gwałtowne zetknięcie własnego ciała z twardym przedmiotem (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_collision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wchodząc do niskiego pomieszczenia, niechcący <uderzył> się w głowę.",
                            "target_lang_translations": {
                                "ru": "Входя в низкое помещение, он случайно <ударился> головой."
                            }
                        },
                        {
                            "text": "Dziecko płakało, ponieważ <uderzyło> się boleśnie w kolano.",
                            "target_lang_translations": {
                                "ru": "Ребёнок плакал, потому что больно <ударился> коленом."
                            }
                        }
                    ],
                    "synonyms": [
                        "potłuc się",
                        "walnąć się",
                        "zranić się"
                    ],
                    "common_phrases": [
                        "uderzyć się w głowę",
                        "uderzyć się w palec",
                        "boleśnie się uderzyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удариться или ушибиться о твердый предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удариться",
                                "target_lang_sense_clarification": "обо что-то"
                            },
                            {
                                "target_lang_word": "ушибиться",
                                "target_lang_sense_clarification": "получить ушиб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "864fe43e-0a0c-37ae-9d9d-b1c19376925c",
                    "sense_definition": "Gwałtownie natrafić na przeszkodę lub zderzyć się z czymś z dużą siłą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_collision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rozpędzony samochód wpadł w poślizg i <uderzył> w przydrożne drzewo.",
                            "target_lang_translations": {
                                "ru": "Разогнавшийся автомобиль занесло, и он <врезался> в придорожное дерево."
                            }
                        },
                        {
                            "text": "Fale morskie z wielkim hukiem <uderzyły> o skalisty brzeg.",
                            "target_lang_translations": {
                                "ru": "Морские волны с сильным грохотом <ударили> о скалистый берег."
                            }

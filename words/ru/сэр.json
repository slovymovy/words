                    ],
                    "antonyms": [
                        "мэм",
                        "леди",
                        "мисс"
                    ],
                    "common_phrases": [
                        "да, сэр",
                        "нет, сэр",
                        "слушаюсь, сэр"
                    ],
                    "target_lang_definitions": {
                        "en": "A polite form of address to a man in English-speaking countries."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sir",
                                "target_lang_sense_clarification": "polite address"
                            }
                        ]

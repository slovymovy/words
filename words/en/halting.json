                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "прекращение движения"
                            },
                            {
                                "target_lang_word": "заминка",
                                "target_lang_sense_clarification": "пауза или колебание"
                            }
                        ]

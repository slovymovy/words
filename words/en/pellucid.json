{
    "word_family": [
        "pellucidity",
        "pellucidly",
        "pellucidness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c89d2fc3-7db2-3380-9469-b34535e34881",
                    "sense_definition": "Allowing light to pass through completely; clear and transparent.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat seemed to float on air as it glided across the <w>pellucid</w> water.",
                            "target_lang_translations": {
                                "ru": "Казалось, что лодка парит в воздухе, скользя по <w>прозрачной</w> воде."
                            }
                        },
                        {
                            "text": "Hikers stopped to drink from the <w>pellucid</w> stream flowing down the mountain.",
                            "target_lang_translations": {
                                "ru": "Туристы остановились, чтобы напиться из <w>кристально чистого</w> ручья, текущего с горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "transparent",
                        "clear",
                        "limpid",
                        "crystalline",
                        "see-through"
                    ],
                    "antonyms": [
                        "turbid"
                    ],
                    "common_phrases": [
                        "pellucid water",
                        "pellucid stream",
                        "pellucid blue sky"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пропускающий свет; совершенно прозрачный и чистый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачный",
                                "target_lang_sense_clarification": "о воде, воздухе или материале"
                            },
                            {
                                "target_lang_word": "кристально чистый",
                                "target_lang_sense_clarification": "совершенно чистый, без примесей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa72cfa2-cd26-3eb2-aa90-4a9f159b29b1",
                    "sense_definition": "Extremely clear in meaning and easy to understand; lucid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author is famous for his <w>pellucid</w> prose, which simplifies difficult concepts.",
                            "target_lang_translations": {
                                "ru": "Автор известен своей <w>ясной</w> прозой, которая упрощает сложные понятия."
                            }
                        },
                        {
                            "text": "She offered a <w>pellucid</w> explanation that cleared up our confusion immediately.",
                            "target_lang_translations": {
                                "ru": "Она дала <w>понятное</w> объяснение, которое сразу же развеяло наше замешательство."
                            }
                        }
                    ],
                    "synonyms": [
                        "lucid",
                        "clear",
                        "comprehensible",
                        "understandable",
                        "perspicuous"
                    ],
                    "common_phrases": [
                        "pellucid prose",
                        "pellucid explanation",
                        "pellucid logic",
                        "pellucid style"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень ясный по смыслу и легкий для понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясный",
                                "target_lang_sense_clarification": "легкий для понимания (о стиле, речи)"
                            },
                            {
                                "target_lang_word": "понятный",
                                "target_lang_sense_clarification": "доступный для осмысления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "930fc340-69eb-3ac0-92bd-80ebec048de1",
                    "sense_definition": "Of sound or music: clear, pure, and pleasant to hear.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "auditory_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bird's <w>pellucid</w> song echoed through the quiet forest at dawn.",
                            "target_lang_translations": {
                                "ru": "<w>Звонкая</w> песня птицы эхом разнеслась по тихому лесу на рассвете."
                            }
                        },
                        {
                            "text": "Her <w>pellucid</w> voice captivated the audience during the opera performance.",
                            "target_lang_translations": {
                                "ru": "Её <w>чистый</w> голос пленил аудиторию во время оперного выступления."
                            }
                        }
                    ],
                    "synonyms": [
                        "clear",
                        "pure"
                    ],
                    "common_phrases": [
                        "pellucid tone",
                        "pellucid notes",
                        "pellucid voice"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке или музыке: чистый, звонкий и приятный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "о звуке или голосе, без искажений"
                            },
                            {
                                "target_lang_word": "звонкий",
                                "target_lang_sense_clarification": "громкий и отчетливый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "en": [
                            {
                                "target_lang_word": "smoke",
                                "target_lang_sense_clarification": "preserve food"
                            },
                            {
                                "target_lang_word": "cure",
                                "target_lang_sense_clarification": "preserve food"
                            }
                        ]
                    }

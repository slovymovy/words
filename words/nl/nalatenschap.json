{
    "word_family": [
        "nalaten",
        "nalatig"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5c957d4-a213-396f-9ff2-fe8adf63f87d",
                    "sense_definition": "Het geheel van bezittingen en schulden dat iemand na overlijden achterlaat voor de erfgenamen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legacy_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen ruzieden jarenlang over de verdeling van de <w>nalatenschap</w>.",
                            "target_lang_translations": {
                                "ru": "Дети годами ссорились из-за раздела <w>наследства</w>."
                            }
                        },
                        {
                            "text": "Zijn <w>nalatenschap</w> bestond voornamelijk uit onroerend goed en enkele waardevolle schilderijen.",
                            "target_lang_translations": {
                                "ru": "Его <w>наследство</w> состояло в основном из недвижимости и нескольких ценных картин."
                            }
                        }
                    ],
                    "synonyms": [
                        "erfenis"
                    ],
                    "common_phrases": [
                        "de nalatenschap verdelen",
                        "de nalatenschap aanvaarden",
                        "de nalatenschap verwerpen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность имущества и долгов, которые человек оставляет наследникам после смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследство",
                                "target_lang_sense_clarification": "имущество, деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f746a081-e9a3-3bd6-b06d-063b82a55867",
                    "sense_definition": "De invloed, het werk of de reputatie die iemand of iets achterlaat voor de toekomst, vaak in figuurlijke zin.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legacy_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver liet een indrukwekkende literaire <w>nalatenschap</w> achter voor volgende generaties.",
                            "target_lang_translations": {
                                "ru": "Писатель оставил впечатляющее литературное <w>наследие</w> для будущих поколений."
                            }
                        },
                        {
                            "text": "We moeten zorgvuldig omgaan met de culturele <w>nalatenschap</w> van onze voorouders.",
                            "target_lang_translations": {
                                "ru": "Мы должны бережно относиться к культурному <w>наследию</w> наших предков."
                            }
                        }
                    ],
                    "synonyms": [
                        "erfgoed"
                    ],
                    "common_phrases": [
                        "culturele nalatenschap",
                        "literaire nalatenschap",
                        "politieke nalatenschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Влияние, труды или репутация, оставленные кем-либо для будущих поколений, часто в переносном смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследие",
                                "target_lang_sense_clarification": "культурное, духовное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
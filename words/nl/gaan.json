    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "16c0c4b5-fdf2-37f5-93b2-f80fd2a4544c",
                    "sense_definition": "Zich verplaatsen van de ene plek naar de andere.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ga</w> morgen met de trein naar Amsterdam.",
                            "target_lang_translations": {
                                "ru": "Я завтра <w>поеду</w> на поезде в Амстердам.",
                                "en": "I am <w>going</w> to Amsterdam by train tomorrow."
                            }
                        },
                        {
                            "text": "Wij <w>gaan</w> vaak naar het strand in de zomer.",
                            "target_lang_translations": {
                                "ru": "Мы часто <w>ездим</w> на пляж летом.",
                                "en": "We often <w>go</w> to the beach in the summer."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrekken",
                        "reizen"
                    ],
                    "antonyms": [
                        "blijven",
                        "komen"
                    ],
                    "common_phrases": [
                        "naar huis gaan",
                        "weg gaan",
                        "naar bed gaan",
                        "te voet gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться из одного места в другое.",
                        "en": "To move from one place to another."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "пешком или общее значение"
                            },
                            {
                                "target_lang_word": "ехать",
                                "target_lang_sense_clarification": "на транспорте"
                            },
                            {
                                "target_lang_word": "отправляться",
                                "target_lang_sense_clarification": "начинать движение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "general movement"
                            },
                            {
                                "target_lang_word": "travel",
                                "target_lang_sense_clarification": "to journey"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "to depart"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24aa4d00-e56b-3b45-aba7-b9a81089b625",
                    "sense_definition": "Hulpwerkwoord dat wordt gebruikt om de toekomende tijd uit te drukken; van plan zijn iets te doen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>gaat</w> vanmiddag waarschijnlijk regenen.",
                            "target_lang_translations": {
                                "ru": "Сегодня днем, вероятно, <w>пойдет</w> дождь.",
                                "en": "It is probably <w>going</w> to rain this afternoon."
                            }
                        },
                        {
                            "text": "Ik <w>ga</w> straks boodschappen doen bij de supermarkt.",
                            "target_lang_translations": {
                                "ru": "Я <w>собираюсь</w> скоро пойти за продуктами в супермаркет.",
                                "en": "I am <w>going</w> to get groceries at the supermarket later."
                            }
                        }
                    ],
                    "synonyms": [
                        "zullen"
                    ],
                    "common_phrases": [
                        "gaan slapen",
                        "gaan eten",
                        "iets gaan doen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "auxiliary verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательный глагол для выражения будущего времени; намереваться что-то сделать.",
                        "en": "Auxiliary verb used to express the future tense; to intend to do something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "планировать сделать что-то"
                            },
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "образует будущее время"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "be going to",
                                "target_lang_sense_clarification": "future plan or intention"
                            },
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "future marker"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12887c06-72cd-386c-b0c3-8744b182c073",
                    "sense_definition": "Op een bepaalde manier verlopen, functioneren of gesteld zijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hoe <w>gaat</w> het vandaag met jou?",
                            "target_lang_translations": {
                                "ru": "Как у тебя сегодня <w>идут</w> дела?",
                                "en": "How is it <w>going</w> with you today?"
                            }
                        },
                        {
                            "text": "Dat <w>gaat</w> helaas niet lukken omdat ik moet werken.",
                            "target_lang_translations": {
                                "ru": "Это, к сожалению, не <w>получится</w>, так как мне нужно работать.",
                                "en": "Unfortunately that is not <w>going</w> to work because I have to work."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlopen",
                        "lukken"
                    ],
                    "common_phrases": [
                        "hoe gaat het",
                        "dat gaat niet",
                        "het gaat wel",
                        "gang van zaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Протекать, функционировать или обстоять определенным образом.",
                        "en": "To proceed, function, or be in a certain state."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о делах или процессе"
                            },
                            {
                                "target_lang_word": "получаться",
                                "target_lang_sense_clarification": "удаваться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "to proceed or unfold"
                            },
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to function or succeed"
                            },
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "state of being"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed5fa480-5ca0-3bab-aa82-5775c0b66e02",
                    "sense_definition": "Leiden naar of zich uitstrekken in een bepaalde richting (van wegen, paden of lijnen).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze lange weg <w>gaat</w> helemaal naar het zuiden van het land.",
                            "target_lang_translations": {
                                "ru": "Эта длинная дорога <w>ведет</w> на самый юг страны.",
                                "en": "This long road <w>goes</w> all the way to the south of the country."
                            }
                        },
                        {
                            "text": "Het smalle pad <w>gaat</w> steil omhoog de berg op.",
                            "target_lang_translations": {
                                "ru": "Узкая тропинка <w>идет</w> круто вверх на гору.",
                                "en": "The narrow path <w>goes</w> steeply up the mountain."
                            }
                        }
                    ],
                    "synonyms": [
                        "leiden",
                        "lopen"
                    ],
                    "common_phrases": [
                        "de weg gaat",
                        "nergens heen gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести куда-либо или простираться в определенном направлении (о дорогах, путях).",
                        "en": "To lead to or extend in a certain direction (of roads, paths, or lines)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "направлять куда-то"
                            },
                            {
                                "target_lang_word": "пролегать",
                                "target_lang_sense_clarification": "располагаться"
                            },
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "простираться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "to direct towards a place"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "to extend along a path"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "to extend or stretch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f8ca624-337e-3f58-8f25-2bdfda56e5e6",
                    "sense_definition": "Goed of slecht reageren op iets; een bepaald effect ervaren van iets.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>gaat</w> altijd heel goed op dit soort muziek.",
                            "target_lang_translations": {
                                "ru": "Он всегда очень хорошо <w>реагирует</w> на такую музыку.",
                                "en": "He always <w>reacts</w> very well to this kind of music."
                            }
                        },
                        {
                            "text": "Ik <w>ga</w> echt slecht op mensen die liegen.",
                            "target_lang_translations": {
                                "ru": "Я действительно плохо <w>переношу</w> людей, которые лгут.",
                                "en": "I really <w>react</w> badly to people who lie."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lekker gaan",
                        "slecht gaan op"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used among youth"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Реагировать на что-то; испытывать определенный эффект от чего-либо.",
                        "en": "To react well or poorly to something; to experience a specific effect from something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реагировать",
                                "target_lang_sense_clarification": "отзываться на что-то"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "выдерживать (обычно с 'плохо')"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "react",
                                "target_lang_sense_clarification": "to respond to a stimulus"
                            },
                            {
                                "target_lang_word": "respond",
                                "target_lang_sense_clarification": "to show a reaction"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "to deal with or experience"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
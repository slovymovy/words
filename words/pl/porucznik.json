{
    "word_family": [
        "podporucznik",
        "porucznikowski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aff38bc7-d74a-3299-a973-9a5cf8c16847",
                    "sense_definition": "Oficer w wojsku lub innych służbach mundurowych posiadający stopień wyższy od podporucznika, a niższy od kapitana.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <w>porucznik</w> zameldował dowódcy wykonanie powierzonego zadania.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>поручик</w> доложил командиру о выполнении порученного задания."
                            }
                        },
                        {
                            "text": "Panie <w>poruczniku</w>, żołnierze są gotowi do wymarszu.",
                            "target_lang_translations": {
                                "ru": "Господин <w>поручик</w>, солдаты готовы к маршу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "panie poruczniku",
                        "porucznik rezerwy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Офицер в армии или других силовых структурах, имеющий звание выше подпоручика и ниже капитана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поручик",
                                "target_lang_sense_clarification": "в польской армии и историческом контексте"
                            },
                            {
                                "target_lang_word": "старший лейтенант",
                                "target_lang_sense_clarification": "аналог звания в армии РФ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e42f758d-f6df-3f7a-83f9-46d21645d17d",
                    "sense_definition": "Stopień wojskowy w korpusie oficerów młodszych, znajdujący się pomiędzy podporucznikiem a kapitanem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ukończeniu szkoły oficerskiej otrzymał awans na <w>porucznika</w>.",
                            "target_lang_translations": {
                                "ru": "После окончания офицерского училища он получил повышение до <w>поручика</w>."
                            }
                        },
                        {
                            "text": "Stopień <w>porucznika</w> wiąże się z dowodzeniem plutonem lub kompanią.",
                            "target_lang_translations": {
                                "ru": "Звание <w>поручика</w> связано с командованием взводом или ротой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "awans na porucznika",
                        "stopień porucznika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воинское звание младшего офицерского состава, находящееся между подпоручиком и капитаном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старший лейтенант",
                                "target_lang_sense_clarification": "воинское звание"
                            },
                            {
                                "target_lang_word": "звание поручика",
                                "target_lang_sense_clarification": "воинское звание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "09041a41-f4d0-31a1-a4bc-d9a9b88eff0b",
                    "sense_definition": "Wąskie pomieszczenie łączące inne pomieszczenia w budynku",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na przerwie uczniowie powinni wyjść z klasy na <w>korytarz</w>.",
                            "target_lang_translations": {
                                "ru": "На перемене ученики должны выйти из класса в <w>коридор</w>."
                            }
                        },
                        {
                            "text": "Zostawiłem mokre buty i płaszcz na <w>korytarzu</w>.",
                            "target_lang_translations": {
                                "ru": "Я оставил мокрые ботинки и пальто в <w>коридоре</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przejście",
                        "hol"
                    ],
                    "common_phrases": [
                        "długi korytarz",
                        "ciemny korytarz",
                        "wyjść na korytarz",
                        "szkolny korytarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкое помещение, соединяющее другие комнаты в здании"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коридор",
                                "target_lang_sense_clarification": "часть здания"
                            },
                            {
                                "target_lang_word": "прихожая",
                                "target_lang_sense_clarification": "входная зона в квартире"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf0b3a3d-18ca-3b78-844c-9785fddea490",
                    "sense_definition": "Długie, wąskie przejście wykopane, wyżłobione w ziemi lub w skale",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krety kopią długie <w>korytarze</w> mieszkalne pod powierzchnią trawnika.",
                            "target_lang_translations": {
                                "ru": "Кроты роют длинные жилые <w>ходы</w> под поверхностью газона."
                            }
                        },
                        {
                            "text": "Górnicy zabezpieczyli stary <w>korytarz</w> w kopalni soli.",
                            "target_lang_translations": {
                                "ru": "Шахтёры укрепили старый <w>проход</w> в соляной шахте."
                            }
                        }
                    ],
                    "synonyms": [
                        "sztolnia",
                        "przejście"
                    ],
                    "common_phrases": [
                        "podziemny korytarz",
                        "drążyć korytarz",
                        "wąski korytarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный узкий проход, вырытый в земле или скале"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "нора, подземный путь"
                            },
                            {
                                "target_lang_word": "туннель",
                                "target_lang_sense_clarification": "подземное сооружение"
                            },
                            {
                                "target_lang_word": "проход",
                                "target_lang_sense_clarification": "в шахте или пещере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a66e8c6-5bca-3a80-b6d3-05d0936d8fc2",
                    "sense_definition": "Wydzielony obszar komunikacyjny, często przebiegający przez terytorium obcego państwa lub przestrzeń powietrzną",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "designated_zone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samoloty pasażerskie latają tylko po ściśle określonych <w>korytarzach</w> powietrznych.",
                            "target_lang_translations": {
                                "ru": "Пассажирские самолёты летают только по строго определённым воздушным <w>коридорам</w>."
                            }
                        },
                        {
                            "text": "Kierowcy na autostradzie utworzyli <w>korytarz</w> życia dla karetki pogotowia.",
                            "target_lang_translations": {
                                "ru": "Водители на автостраде создали спасательный <w>коридор</w> для машины скорой помощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "kanał",
                        "droga"
                    ],
                    "common_phrases": [
                        "korytarz powietrzny",
                        "korytarz humanitarny",
                        "korytarz życia",
                        "korytarz transportowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделенная зона для движения транспорта, часто через чужую территорию или воздушное пространство"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коридор",
                                "target_lang_sense_clarification": "воздушный, гуманитарный, транспортный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13bd860c-5689-3f54-88a2-c6790857156a",
                    "sense_definition": "Wydzielone pasmo telekomunikacyjne",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "designated_zone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na potrzeby akcji ratunkowej udostępniono specjalny <w>korytarz</w> łączności radiowej.",
                            "target_lang_translations": {
                                "ru": "Для спасательной операции был выделен специальный <w>канал</w> радиосвязи."
                            }
                        },
                        {
                            "text": "Operator sieci zapewnił bezpieczny <w>korytarz</w> przesyłu danych dla banku.",
                            "target_lang_translations": {
                                "ru": "Сетевой оператор обеспечил безопасный <w>канал</w> передачи данных для банка."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasmo",

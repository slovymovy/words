                            {
                                "target_lang_word": "banner",
                                "target_lang_sense_clarification": "flag"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "штандарт",
                                "target_lang_sense_clarification": "флаг"
                            }

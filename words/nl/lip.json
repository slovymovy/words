                            }
                        }
                    ],
                    "common_phrases": [
                        "het lipje van een blikje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая деталь предмета, часто предназначенная для его открывания или закрывания.",
                        "en": "A protruding piece of material on an object, often intended for opening or closing it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язычок",
                                "target_lang_sense_clarification": "деталь молнии, банки"
                            },
                            {
                                "target_lang_word": "ушко",
                                "target_lang_sense_clarification": "выступающая деталь с отверстием"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tab",
                                "target_lang_sense_clarification": "small flap or strip"
                            },
                            {
                                "target_lang_word": "pull tab",
                                "target_lang_sense_clarification": "opener on a can"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "624c2c78-a28e-3a50-a65c-1778d28f2337",
                    "sense_definition": "Nauczyć się czegoś bardzo dokładnie na pamięć, zazwyczaj z dużym wysiłkiem i bez głębszego zrozumienia treści.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "learning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>wkuć</w> wszystkie daty bitew na jutrzejszy sprawdzian z historii.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>вызубрить</w> все даты битв к завтрашней контрольной по истории."
                            }
                        },
                        {
                            "text": "Zamiast <w>wkuć</w> ten wiersz na blachę, spróbuj zrozumieć jego przesłanie.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы <w>вызубрить</w> это стихотворение назубок, попробуй понять его смысл."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykuć",
                        "zapamiętać"
                    ],
                    "antonyms": [
                        "zapomnieć"
                    ],
                    "common_phrases": [
                        "wkuć na blachę",
                        "wkuć na pamięć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane potocznie, często w środowisku szkolnym i studenckim"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выучить что-либо очень тщательно наизусть, обычно с большим усилием и без глубокого понимания содержания (зубрить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызубрить",
                                "target_lang_sense_clarification": "выучить наизусть (разг.)"
                            },
                            {
                                "target_lang_word": "зазубрить",
                                "target_lang_sense_clarification": "запомнить механически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d16932a-bbd5-3abe-9fe7-712adfa9b22c",
                    "sense_definition": "Umieścić element trwale w twardym materiale (np. w ścianie, kamieniu) poprzez kucie lub wybijanie otworu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elektryk musiał <w>wkuć</w> przewody w ścianę, aby były niewidoczne.",
                            "target_lang_translations": {
                                "ru": "Электрику пришлось <w>вмонтировать</w> провода в стену, чтобы их не было видно."
                            }
                        },
                        {
                            "text": "Postanowili <w>wkuć</w> metalową tablicę pamiątkową w duży głaz przy drodze.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>вделать</w> металлическую памятную доску в большой валун у дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "osadzić"
                    ],
                    "antonyms": [
                        "wykuć"
                    ],
                    "common_phrases": [
                        "wkuć w ścianę",
                        "wkuć instalację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить элемент в твердый материал (например, в стену, камень), выдалбливая для него отверстие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вделать",
                                "target_lang_sense_clarification": "прочно укрепить внутри чего-либо"
                            },
                            {
                                "target_lang_word": "вмонтировать",
                                "target_lang_sense_clarification": "вставить и закрепить технически"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62c5851c-6154-318f-84a5-cb72f0421124",
                    "sense_definition": "Zjawisko fizyczne, które polega na wnikaniu cząsteczek jednej substancji między cząsteczki drugiej substancji, tworząc jednorodny roztwór.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "chemistry_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybkie <w>rozpuszczanie</w> tabletki w wodzie ułatwia jej przyjęcie przez pacjenta.",
                            "target_lang_translations": {
                                "ru": "Быстрое <w>растворение</w> таблетки в воде облегчает её прием пациентом."
                            }
                        },
                        {
                            "text": "Temperatura cieczy ma kluczowy wpływ na <w>rozpuszczanie</w> się w niej cukru.",
                            "target_lang_translations": {
                                "ru": "Температура жидкости оказывает ключевое влияние на <w>растворение</w> в ней сахара."
                            }
                        }
                    ],
                    "synonyms": [
                        "dyfuzja"
                    ],
                    "antonyms": [
                        "wytrącanie",
                        "krystalizacja"
                    ],
                    "common_phrases": [
                        "rozpuszczanie cukru",
                        "rozpuszczanie soli",
                        "szybkość rozpuszczania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое явление, при котором частицы одного вещества проникают между частицами другого, образуя однородный раствор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растворение",
                                "target_lang_sense_clarification": "процесс создания раствора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a133ca44-23ee-3ba9-ae98-72c748306146",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>rozpuszczać</w>: doprowadzanie substancji stałej do stanu płynnego pod wpływem ciepła.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "phase_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przepis na ciasto zaczyna się od powolnego <w>rozpuszczania</w> masła na patelni.",
                            "target_lang_translations": {
                                "ru": "Рецепт пирога начинается с медленного <w>растапливания</w> масла на сковороде."
                            }
                        },
                        {
                            "text": "<w>Rozpuszczanie</w> czekolady w kąpieli wodnej zapobiega jej przypaleniu.",
                            "target_lang_translations": {
                                "ru": "<w>Растапливание</w> шоколада на водяной бане предотвращает его пригорание."
                            }
                        }
                    ],
                    "synonyms": [
                        "dyfuzja"
                    ],
                    "common_phrases": [
                        "rozpuszczanie masła",
                        "rozpuszczanie czekolady"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technically 'topienie', but commonly used in cooking"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Доведение твердого вещества до жидкого состояния под воздействием тепла (обычно о жирах или шоколаде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растапливание",
                                "target_lang_sense_clarification": "кулинария (масло, шоколад)"
                            },
                            {
                                "target_lang_word": "плавление",
                                "target_lang_sense_clarification": "физический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4d512a0-e85b-3bd6-acca-c812d02e727b",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>rozpuszczać</w>: nadmierne pobłażanie komuś (zwykle dziecku), spełnianie wszystkich zachcianek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągłe <w>rozpuszczanie</w> dzieci przez dziadków często prowadzi do konfliktów z rodzicami.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>избаловывание</w> детей бабушками и дедушками часто приводит к конфликтам с родителями."
                            }
                        },
                        {
                            "text": "Takie bezkrytyczne <w>rozpuszczanie</w> syna nie nauczy go odpowiedzialności.",
                            "target_lang_translations": {
                                "ru": "Такое некритичное <w>избаловывание</w> сына не научит его ответственности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozpuszczanie dzieci",
                        "rozpuszczanie wnuków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерное потакание кому-либо (обычно ребенку), исполнение всех его прихотей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избаловывание",
                                "target_lang_sense_clarification": "процесс воспитания"
                            },
                            {
                                "target_lang_word": "баловство",
                                "target_lang_sense_clarification": "потакание капризам"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
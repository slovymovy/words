                            {
                                "target_lang_word": "Амур",
                                "target_lang_sense_clarification": "бог любви"
                            },
                            {
                                "target_lang_word": "Купидон",
                                "target_lang_sense_clarification": "мифологический персонаж"
                            },
                            {
                                "target_lang_word": "Любовь",
                                "target_lang_sense_clarification": "персонификация"
                            }
                        ]
                    }
                }

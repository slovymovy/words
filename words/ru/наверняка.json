{
    "word_family": [
        "верный",
        "вера",
        "верить",
        "уверенный",
        "наверное"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "33fe8894-3d26-31fc-b551-05ee74d5eccd",
                    "sense_definition": "Несомненно, точно, с полной уверенностью.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не знаю точного адреса, но он <w>наверняка</w> сейчас дома.",
                            "target_lang_translations": {
                                "en": "I don't know the exact address, but he is <w>surely</w> at home right now."
                            }
                        },
                        {
                            "text": "Мы хотим знать <w>наверняка</w>, что этот план сработает.",
                            "target_lang_translations": {
                                "en": "We want to know <w>for sure</w> that this plan will work."
                            }
                        }
                    ],
                    "synonyms": [
                        "несомненно",
                        "точно",
                        "верно",
                        "наверное"
                    ],
                    "common_phrases": [
                        "знать наверняка",
                        "сказать наверняка"
                    ],
                    "target_lang_definitions": {
                        "en": "Undoubtedly, for certain, with complete confidence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for sure",
                                "target_lang_sense_clarification": "definitely; often used in phrases like 'know for sure'"
                            },
                            {
                                "target_lang_word": "surely",
                                "target_lang_sense_clarification": "used to express strong probability or belief"
                            },
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "without doubt"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ce631a4-1942-32dd-b81c-ffe43b2b3411",
                    "sense_definition": "С расчётом на верный успех, без риска ошибиться.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный охотник всегда старается бить <w>наверняка</w>, чтобы не упустить зверя.",
                            "target_lang_translations": {
                                "en": "An experienced hunter always tries to shoot <w>surely</w> so as not to lose the beast."
                            }
                        },
                        {
                            "text": "В такой сложной ситуации лучше действовать <w>наверняка</w>.",
                            "target_lang_translations": {
                                "en": "In such a difficult situation, it is better to act <w>for sure</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "безошибочно"
                    ],
                    "common_phrases": [
                        "бить наверняка",
                        "действовать наверняка"
                    ],
                    "target_lang_definitions": {
                        "en": "With a guarantee of success, without risk of error."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for sure",
                                "target_lang_sense_clarification": "in a way that guarantees a result"
                            },
                            {
                                "target_lang_word": "surely",
                                "target_lang_sense_clarification": "firmly, steadily, or with confidence"
                            },
                            {
                                "target_lang_word": "without fail",
                                "target_lang_sense_clarification": "with absolute certainty of success"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
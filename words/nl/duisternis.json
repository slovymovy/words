            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8fc2d246-fe82-3cdf-93c9-2b85101a6301",
                    "sense_definition": "De toestand waarin er weinig of geen licht is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We konden bijna niets zien in de <w>duisternis</w>.",
                            "target_lang_translations": {
                                "en": "We could see almost nothing in the <w>darkness</w>."
                            }
                        },
                        {
                            "text": "Door de invallende <w>duisternis</w> moesten we stoppen met zoeken.",
                            "target_lang_translations": {
                                "en": "Due to the gathering <w>darkness</w>, we had to stop searching."
                            }
                        }
                    ],
                    "synonyms": [
                        "donkerte"
                    ],
                    "antonyms": [
                        "licht"
                    ],
                    "common_phrases": [
                        "in de duisternis",
                        "totale duisternis",
                        "invallende duisternis"
                    ],
                    "target_lang_definitions": {
                        "en": "The state in which there is little or no light."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "darkness",
                                "target_lang_sense_clarification": "absence of light"
                            },
                            {
                                "target_lang_word": "the dark",
                                "target_lang_sense_clarification": "absence of light (informal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a7925d3-9812-3acf-9f84-f06d1ef32e7d",
                    "sense_definition": "Een toestand van onwetendheid, somberheid of gebrek aan geestelijk inzicht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jaren van <w>duisternis</w> kwam er eindelijk weer hoop in zijn leven.",
                            "target_lang_translations": {
                                "en": "After years of <w>darkness</w>, hope finally returned to his life."
                            }
                        },
                        {
                            "text": "Het boek beschrijft de geestelijke <w>duisternis</w> van die periode.",
                            "target_lang_translations": {
                                "en": "The book describes the spiritual <w>darkness</w> of that period."
                            }
                        }
                    ],
                    "antonyms": [
                        "licht"
                    ],
                    "common_phrases": [
                        "geestelijke duisternis",
                        "eeuwen van duisternis"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of ignorance, gloom, or lack of spiritual insight."
                    },
                    "translations": {

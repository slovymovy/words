                },
                {
                    "sense_id": "f3d69dc0-4010-37b4-93b6-6531db948c04",
                    "sense_definition": "dziecięce lub pieszczotliwe określenie męskiego członka (penisa)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {

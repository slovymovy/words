    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d273657a-4e7b-30a5-bb08-848d17ec88b1",
                    "sense_definition": "Used to express disappointment, regret, or sorrow regarding a statement or event.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Unfortunately</w>, we arrived too late to catch the train.",
                            "target_lang_translations": {
                                "ru": "<w>К сожалению</w>, мы приехали слишком поздно, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "I wanted to help you, but <w>unfortunately</w> I have no money.",
                            "target_lang_translations": {
                                "ru": "Я хотел помочь тебе, но, <w>к сожалению</w>, у меня нет денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "sadly",
                        "regrettably",
                        "alas",
                        "sad to say"
                    ],
                    "antonyms": [
                        "fortunately",
                        "luckily"
                    ],
                    "common_phrases": [
                        "but unfortunately",
                        "unfortunately not",
                        "unfortunately for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения разочарования или сожаления по поводу какого-либо факта или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к сожалению",
                                "target_lang_sense_clarification": "выражение сожаления"
                            },
                            {
                                "target_lang_word": "к несчастью",
                                "target_lang_sense_clarification": "выражение огорчения или неудачи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bf31a51-c605-3352-82e9-c91eac358792",
                    "sense_definition": "In a manner resulting from bad luck or an unfavorable event.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The decision was <w>unfortunately</w> timed right before the holiday.",
                            "target_lang_translations": {
                                "ru": "Решение было <w>неудачно</w> принято прямо перед праздником."
                            }
                        },
                        {
                            "text": "There is an <w>unfortunately</w> large gap in the data we collected.",
                            "target_lang_translations": {
                                "ru": "В собранных нами данных, <w>к сожалению</w>, есть большой пробел."
                            }
                        }

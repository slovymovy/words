                {
                    "sense_id": "57ac46ee-2150-373e-8167-057cad6b2ab2",
                    "sense_definition": "Wettelijke verklaring dat men als bedenker de enige is die het bedachte mag gebruiken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor een perpetuum mobile kun je geen <w>patent</w> krijgen.",
                            "target_lang_translations": {
                                "en": "You cannot get a <w>patent</w> for a perpetual motion machine.",
                                "ru": "На вечный двигатель нельзя получить <w>патент</w>."
                            }
                        },
                        {
                            "text": "Het bedrijf heeft een <w>patent</w> aangevraagd op de nieuwe technologie om concurrentie te voorkomen.",
                            "target_lang_translations": {
                                "en": "The company has applied for a <w>patent</w> on the new technology to prevent competition.",
                                "ru": "Компания подала заявку на <w>патент</w> на новую технологию, чтобы предотвратить конкуренцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "octrooi"
                    ],
                    "common_phrases": [
                        "patent aanvragen",
                        "patent verlenen",
                        "patent hebben op"
                    ],
                    "target_lang_definitions": {
                        "en": "Legal declaration that the inventor is the only one allowed to use the invention.",
                        "ru": "Официальный документ, удостоверяющий исключительное право изобретателя на его изобретение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "patent",
                                "target_lang_sense_clarification": "intellectual property right"
                            }

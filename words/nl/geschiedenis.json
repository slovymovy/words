                    ],
                    "common_phrases": [
                        "leraar geschiedenis",
                        "studeren geschiedenis"
                    ],
                    "target_lang_definitions": {
                        "en": "The science that studies and describes the past, or the school subject dealing with this.",
                        "ru": "Наука, изучающая прошлое, или школьный предмет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "history",
                                "target_lang_sense_clarification": "study or subject"

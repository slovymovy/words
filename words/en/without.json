{
    "word_family": [
        "with",
        "within",
        "withal",
        "withdraw",
        "withhold",
        "withstand"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "275111f6-fddd-3811-aa1e-bda3e5a39e23",
                    "sense_definition": "Not having, containing, or including something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "absence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a mistake to leave my house <w>without</w> a coat.",
                            "target_lang_translations": {
                                "ru": "Было ошибкой выйти из дома <w>без</w> пальто."
                            }
                        },
                        {
                            "text": "I was <w>without</w> formal education but was well read and articulate.",
                            "target_lang_translations": {
                                "ru": "Я был <w>без</w> формального образования, но начитан и красноречив."
                            }
                        },
                        {
                            "text": "One day my dreams were surely dying, just like a flower <w>without</w> rain.",
                            "target_lang_translations": {
                                "ru": "Однажды мои мечты, несомненно, умирали, как цветок <w>без</w> дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "lacking",
                        "with no",
                        "minus",
                        "sans"
                    ],
                    "antonyms": [
                        "with",
                        "having"
                    ],
                    "common_phrases": [
                        "without a doubt",
                        "without question",
                        "without fail",
                        "without exception"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий на отсутствие, неимение чего-либо или кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без",
                                "target_lang_sense_clarification": "указывает на отсутствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "000c11db-2741-3093-924b-35d4e085d5cb",
                    "sense_definition": "Used to indicate that an action is not performed or does not happen; typically followed by a verb ending in -ing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "absence_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He likes to eat everything <w>without</w> sharing.",
                            "target_lang_translations": {
                                "ru": "Он любит съедать всё, <w>не</w> делясь ни с кем."
                            }
                        },
                        {
                            "text": "He shot <w>without</w> warning anyone.",
                            "target_lang_translations": {
                                "ru": "Он выстрелил, никого <w>не</w> предупредив."
                            }
                        },
                        {
                            "text": "She walked past him <w>without</w> noticing his presence.",
                            "target_lang_translations": {
                                "ru": "Она прошла мимо него, <w>не</w> заметив его присутствия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "without thinking",
                        "without knowing",
                        "without realizing",
                        "without saying"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что действие не выполняется; часто переводится деепричастием с частицей «не» или предлогом «без» с существительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "с деепричастием (напр. не зная)"
                            },
                            {
                                "target_lang_word": "без",
                                "target_lang_sense_clarification": "с отглагольным существительным (напр. без предупреждения)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d3be5a1-4d20-3349-99bd-7b7def8e9e41",
                    "sense_definition": "Situated on the outside of or beyond the limits of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "location_outside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The winds blew not more violently <w>without</w> the house than within.",
                            "target_lang_translations": {
                                "ru": "Ветры дули <w>снаружи</w> дома не сильнее, чем внутри."
                            }
                        },
                        {
                            "text": "Eternity, before the world and after, is <w>without</w> our reach.",
                            "target_lang_translations": {
                                "ru": "Вечность, до сотворения мира и после, находится <w>вне</w> нашей досягаемости."
                            }
                        }
                    ],
                    "synonyms": [
                        "outwith"
                    ],
                    "antonyms": [
                        "within"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Scottish (outwith)"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся снаружи или за пределами чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вне",
                                "target_lang_sense_clarification": "за пределами"
                            },
                            {
                                "target_lang_word": "снаружи",
                                "target_lang_sense_clarification": "с внешней стороны"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5146a919-ce4d-3541-a51a-b7500c69478b",
                    "sense_definition": "In the absence of something needed or desired; lacking something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "absence_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We've run out of bread; you'll have to do <w>without</w> until I can get to the shop.",
                            "target_lang_translations": {
                                "ru": "У нас закончился хлеб; тебе придется обойтись <w>без него</w>, пока я не доберусь до магазина."
                            }
                        },
                        {
                            "text": "Being from a large, poor family, he learned to live <w>without</w>.",
                            "target_lang_translations": {
                                "ru": "Будучи из большой бедной семьи, он научился жить <w>без этого</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do without",
                        "go without",
                        "live without"
                    ],
                    "target_lang_definitions": {
                        "ru": "В отсутствие чего-либо необходимого или желаемого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без этого",
                                "target_lang_sense_clarification": "в отсутствие упомянутого предмета"
                            },
                            {
                                "target_lang_word": "не имея",
                                "target_lang_sense_clarification": "деепричастие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d11fba57-5102-3c29-9f9f-11c8ba31e7e7",
                    "sense_definition": "On the outside; externally.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "location_outside_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I knew that someone had entered the house cautiously from <w>without</w>.",
                            "target_lang_translations": {
                                "ru": "Я знал, что кто-то осторожно вошел в дом <w>снаружи</w>."
                            }
                        },
                        {
                            "text": "Strange silence here: <w>without</w>, the sounding street heralds the world's swift passage.",
                            "target_lang_translations": {
                                "ru": "Здесь странная тишина: <w>снаружи</w> шумная улица возвещает о стремительном движении мира."
                            }
                        }
                    ],
                    "antonyms": [
                        "within"
                    ],
                    "common_phrases": [
                        "from without"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снаружи; внешне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снаружи",
                                "target_lang_sense_clarification": "на внешней стороне"
                            },
                            {
                                "target_lang_word": "извне",
                                "target_lang_sense_clarification": "с внешней стороны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "297096a1-5805-3951-b33f-f411038440b3",
                    "sense_definition": "Without using a condom.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He refused to engage in any activity <w>without</w>.",
                            "target_lang_translations": {
                                "ru": "Он отказался заниматься чем-либо <w>без защиты</w>."
                            }
                        },
                        {
                            "text": "They discussed the risks of doing it <w>without</w>.",
                            "target_lang_translations": {
                                "ru": "Они обсуждали риски того, чтобы делать это <w>без презерватива</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Без использования презерватива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без защиты",
                                "target_lang_sense_clarification": "сленг, о сексе"
                            },
                            {
                                "target_lang_word": "без презерватива",
                                "target_lang_sense_clarification": "прямое значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "eb5c34ef-9428-3dd8-ac59-200aecd83ac7",
                    "sense_definition": "Unless; except.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You don't know about me, <w>without</w> you have read a book by the name of The Adventures of Tom Sawyer.",
                            "target_lang_translations": {
                                "ru": "Вы ничего не знаете обо мне, <w>если</w> только <w>не</w> читали книгу под названием «Приключения Тома Сойера»."
                            }
                        },
                        {
                            "text": "I cannot leave <w>without</w> you tell me where to go.",
                            "target_lang_translations": {
                                "ru": "Я не могу уйти, <w>пока</w> ты <w>не</w> скажешь мне, куда идти."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "non-standard"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Если не; за исключением того, что."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "если не",
                                "target_lang_sense_clarification": "условие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "df75a65c-10e8-38f7-85bb-452b2b0cfb15",
                    "sense_definition": "Produced by humans or machinery, usually on a large scale, rather than occurring naturally.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "production_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country relies heavily on the export of <w>manufactured</w> goods.",
                            "target_lang_translations": {
                                "ru": "Страна сильно зависит от экспорта <w>промышленных</w> товаров."
                            }
                        },
                        {
                            "text": "We bought a <w>manufactured</w> home because it was more affordable than a traditional house.",
                            "target_lang_translations": {
                                "ru": "Мы купили <w>сборный</w> дом, потому что он был доступнее традиционного."
                            }
                        }
                    ],
                    "synonyms": [
                        "manmade",
                        "fabricated",
                        "industrial"
                    ],
                    "antonyms": [
                        "natural",
                        "handmade"
                    ],
                    "common_phrases": [
                        "manufactured goods",
                        "manufactured products",
                        "manufactured home"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произведенный людьми или машинами, обычно в больших масштабах; не природный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промышленный",
                                "target_lang_sense_clarification": "о товарах и изделиях"
                            },
                            {
                                "target_lang_word": "фабричный",
                                "target_lang_sense_clarification": "заводского производства"
                            },
                            {
                                "target_lang_word": "сборный",
                                "target_lang_sense_clarification": "о домах и конструкциях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3955a44-09a4-3c04-9df0-b41a4adcc1c6",
                    "sense_definition": "Invented or created artificially to deceive or produce a specific effect; not genuine.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "production_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scandal was largely a <w>manufactured</w> crisis designed to distract the public.",
                            "target_lang_translations": {
                                "ru": "Скандал был по большей части <w>сфабрикованным</w> кризисом, призванным отвлечь общественность."
                            }
                        },
                        {
                            "text": "Critics argued that the band's image was completely <w>manufactured</w> by their management team.",
                            "target_lang_translations": {
                                "ru": "Критики утверждали, что имидж группы был полностью <w>искусственно создан</w> их командой менеджеров."
                            }
                        }
                    ],
                    "synonyms": [
                        "contrived",
                        "artificial",
                        "fake"
                    ],
                    "antonyms": [
                        "genuine",
                        "authentic",
                        "real"
                    ],
                    "common_phrases": [
                        "manufactured crisis",
                        "manufactured consent",
                        "manufactured outrage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственно созданный или выдуманный с целью обмана или создания определенного эффекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сфабрикованный",
                                "target_lang_sense_clarification": "поддельный, ложный"
                            },
                            {
                                "target_lang_word": "надуманный",
                                "target_lang_sense_clarification": "вымышленный"
                            },
                            {
                                "target_lang_word": "искусственный",
                                "target_lang_sense_clarification": "ненатуральный, созданный специально"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "80b3a536-1c63-398a-a7c6-46f4c26be01b",
                    "sense_definition": "Zich in gedachten verplaatsen in de gevoelens, gedachten of situatie van iemand anders.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een acteur moet zich <w>inleven</w> in zijn personage om geloofwaardig over te komen.",
                            "target_lang_translations": {
                                "en": "An actor must <w>identify with</w> his character to come across as credible.",
                                "ru": "Актер должен <w>вжиться</w> в своего персонажа, чтобы выглядеть убедительно."
                            }
                        },
                        {
                            "text": "Hij had zich volledig in die rol <w>ingeleefd</w>.",
                            "target_lang_translations": {
                                "en": "He had completely <w>immersed himself</w> in that role.",
                                "ru": "Он полностью <w>вжился</w> в эту роль."
                            }
                        },
                        {
                            "text": "Ik kan me goed <w>inleven</w> in jouw moeilijke situatie.",
                            "target_lang_translations": {
                                "en": "I can easily <w>empathize</w> with your difficult situation.",
                                "ru": "Я могу хорошо <w>понять</w> твою сложную ситуацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "invoelen",
                        "meeleven"
                    ],
                    "common_phrases": [
                        "zich inleven in",
                        "zich kunnen inleven"
                    ],
                    "target_lang_definitions": {
                        "en": "To mentally place oneself in the feelings, thoughts, or situation of someone else; to empathize.",
                        "ru": "Мысленно поставить себя на место другого человека, проникнуться его чувствами, мыслями или ситуацией."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "empathize",
                                "target_lang_sense_clarification": "with feelings or a situation"
                            },
                            {
                                "target_lang_word": "identify with",
                                "target_lang_sense_clarification": "with a person or character"
                            },
                            {
                                "target_lang_word": "imagine oneself in",
                                "target_lang_sense_clarification": "a situation or position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вживаться",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c24d673-7f01-342a-afac-c5b043bd3a11",
                    "sense_definition": "Duży, reprezentacyjny budynek, będący zwykle siedzibą ważnej instytucji publicznej lub kulturalnej",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na rogu naszej ulicy znajduje się okazały <w>gmach</w> sądu rejonowego.",
                            "target_lang_translations": {
                                "ru": "На углу нашей улицы находится величественное <w>здание</w> районного суда."
                            }
                        },
                        {
                            "text": "Studenci zebrali się przed głównym <w>gmachem</w> uniwersytetu, aby zaprotestować.",
                            "target_lang_translations": {
                                "ru": "Студенты собрались перед главным <w>корпусом</w> университета, чтобы выразить протест."
                            }
                        }
                    ],
                    "synonyms": [
                        "budynek",
                        "budowla"
                    ],
                    "common_phrases": [
                        "gmach sądu",
                        "gmach parlamentu",
                        "gmach muzeum",
                        "okazały gmach",
                        "zabytkowy gmach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое, представительное здание, обычно являющееся резиденцией важного государственного или культурного учреждения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здание",
                                "target_lang_sense_clarification": "крупное сооружение"
                            },
                            {
                                "target_lang_word": "корпус",
                                "target_lang_sense_clarification": "отдельное здание в ряду других (напр. университета)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce7c9be-1ef7-3b44-ba97-f73603b9c42f",
                    "sense_definition": "Przenośnie o czymś, co ma złożoną, wieloelementową strukturę, np. systemie prawnym, teorii naukowej lub organizacji",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeden fałszywy dowód sprawił, że cały <w>gmach</w> oskarżenia runął w gruzach.",
                            "target_lang_translations": {
                                "ru": "Одно ложное доказательство привело к тому, что всё <w>здание</w> обвинения рухнуло."
                            }
                        },
                        {
                            "text": "Filozof przez całe życie budował swój intelektualny <w>gmach</w>, pisząc liczne traktaty.",
                            "target_lang_translations": {
                                "ru": "Философ всю жизнь строил своё интеллектуальное <w>построение</w>, сочиняя многочисленные трактаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "budowla"
                    ],
                    "common_phrases": [
                        "gmach wiedzy",
                        "gmach kłamstw"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переносное значение: сложная, многоэлементная структура, например, правовая система, научная теория или организация"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здание",
                                "target_lang_sense_clarification": "метафорически о сложной структуре"
                            },
                            {
                                "target_lang_word": "построение",
                                "target_lang_sense_clarification": "логическая или теоретическая конструкция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c63f3c6a-65b4-34b4-8381-af78d867c02a",
                    "sense_definition": "Dawne określenie na pomieszczenie mieszkalne, izbę lub komnatę",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "room_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Służba przygotowała dla gości wygodny <w>gmach</w> na piętrze zamku.",
                            "target_lang_translations": {
                                "ru": "Слуги подготовили для гостей удобный <w>покой</w> на этаже замка."
                            }
                        },
                        {
                            "text": "Weszli do ciemnego <w>gmachu</w>, oświetlonego jedynie słabym blaskiem świec.",
                            "target_lang_translations": {
                                "ru": "Они вошли в темный <w>покой</w>, освещенный лишь слабым мерцанием свечей."
                            }
                        }
                    ],
                    "synonyms": [
                        "izba",
                        "komnata",
                        "pokój",
                        "mieszkanie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used historically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название жилого помещения, комнаты или палаты"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "устаревшее: комната, жилое помещение"
                            },
                            {
                                "target_lang_word": "палата",
                                "target_lang_sense_clarification": "богатое жилое помещение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

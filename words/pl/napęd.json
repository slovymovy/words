                    "sense_id": "7daa82b4-2a00-3b8e-b1c2-d32583615077",
                    "sense_definition": "Mechanizm lub system służący do wprawiania w ruch maszyny lub pojazdu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten samochód terenowy posiada <w>napęd</w> na cztery koła.",
                            "target_lang_translations": {
                                "ru": "У этого внедорожника полный <w>привод</w>."
                            }
                        },
                        {
                            "text": "Awaria <w>napędu</w> zmusiła kapitana statku do wezwania pomocy.",
                            "target_lang_translations": {
                                "ru": "Поломка <w>привода</w> вынудила капитана судна вызвать помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "mechanizm napędowy"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "009a1a5e-c667-37d5-9364-47e192f59b1b",
                    "sense_definition": "Другим способом, не так, как раньше или как принято.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили поступить <иначе> и поехали другой дорогой.",
                            "target_lang_translations": {
                                "en": "We decided to act <w>differently</w> and went a different way."
                            }
                        },
                        {
                            "text": "Всё сложилось совсем <иначе>, чем мы планировали вначале.",
                            "target_lang_translations": {
                                "en": "Everything turned out quite <w>differently</w> than we planned at the start."
                            }
                        }
                    ],
                    "synonyms": [
                        "по-другому",
                        "по-иному"
                    ],
                    "antonyms": [
                        "так же",
                        "аналогично"
                    ],
                    "common_phrases": [
                        "так или иначе",
                        "иначе говоря",
                        "выглядеть иначе"
                    ],
                    "target_lang_definitions": {
                        "en": "In a different way; not as before or as accepted."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "differently",
                                "target_lang_sense_clarification": "in a different manner"
                            },
                            {
                                "target_lang_word": "otherwise",
                                "target_lang_sense_clarification": "in another way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ffb0b48-aea4-37f1-953d-aacb08406872",
                    "sense_definition": "В противном случае; если условие не будет выполнено.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Надень тёплую шапку, <иначе> ты можешь простудиться.",
                            "target_lang_translations": {
                                "en": "Put on a warm hat, <w>otherwise</w> you might catch a cold."
                            }
                        },
                        {
                            "text": "Нам нужно поторопиться, <иначе> мы опоздаем на последний автобус.",
                            "target_lang_translations": {
                                "en": "We need to hurry, <w>or else</w> we'll miss the last bus."
                            }
                        }
                    ],
                    "synonyms": [
                        "в противном случае",
                        "а то",
                        "а не то"
                    ],
                    "common_phrases": [
                        "не иначе как"
                    ],
                    "target_lang_definitions": {
                        "en": "In the opposite case; if the condition is not met."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "otherwise",
                                "target_lang_sense_clarification": "if not"
                            },
                            {
                                "target_lang_word": "or else",
                                "target_lang_sense_clarification": "if not"
                            }
                        ]
                    }

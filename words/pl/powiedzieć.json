            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d7fd37e8-9286-372c-b282-2245e96caf1c",
                    "sense_definition": "Przekazać informację, wyrazić myśl lub uczucie za pomocą mowy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication_verbal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę ci <w>powiedzieć</w> coś bardzo ważnego o jutrzejszym spotkaniu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>сказать</w> тебе кое-что очень важное о завтрашней встрече."
                            }
                        },
                        {
                            "text": "Nauczyciel <w>powiedział</w> uczniom, żeby otworzyli książki na stronie dziesiątej.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>сказал</w> ученикам, чтобы они открыли книги на десятой странице."
                            }
                        },
                        {
                            "text": "Szczerze <w>powiedziawszy</w>, nie spodziewałem się takiego wyniku.",
                            "target_lang_translations": {
                                "ru": "Честно <w>сказать</w>, я не ожидал такого результата."
                            }
                        }
                    ],
                    "synonyms": [
                        "oznajmić",
                        "rzec",
                        "stwierdzić",
                        "oświadczyć",
                        "orzec"
                    ],
                    "common_phrases": [
                        "powiedzieć prawdę",
                        "powiedzieć wprost",
                        "trudno powiedzieć",
                        "szczerze powiedziawszy",
                        "krótko mówiąc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать информацию, выразить мысль или чувство с помощью речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "выразить словами"
                            },
                            {
                                "target_lang_word": "сообщить",
                                "target_lang_sense_clarification": "передать сведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30046ff5-1fd8-367c-8313-6c59058f69b1",
                    "sense_definition": "Zwrócić się do kogoś, używając określonej formy gramatycznej, imienia lub tytułu.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "communication_addressing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możemy sobie <w>powiedzieć</w> po imieniu, żeby przełamać lody?",
                            "target_lang_translations": {
                                "ru": "Можем ли мы <w>называть</w> друг друга по имени, чтобы растопить лед?"
                            }
                        },
                        {
                            "text": "Wstydził się <w>powiedzieć</w> do niej mamo przy kolegach.",
                            "target_lang_translations": {
                                "ru": "Он стеснялся <w>назвать</w> её мамой при приятелях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "powiedzieć sobie po imieniu",
                        "powiedzieć na ty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обратиться к кому-либо, используя определенную форму, имя или титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назвать",
                                "target_lang_sense_clarification": "именовать, обратиться по имени или титулу"
                            },
                            {
                                "target_lang_word": "обратиться",
                                "target_lang_sense_clarification": "адресовать речь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f348a26-7205-3b46-b1ab-e8d9fe1d6aff",
                    "sense_definition": "Przekazać treść, znaczenie lub intencję w sposób inny niż dosłowny (np. gestem, zachowaniem) lub sugerować coś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_nonverbal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego smutny wzrok <w>powiedział</w> mi więcej niż tysiąc słów.",
                            "target_lang_translations": {
                                "ru": "Его грустный взгляд <w>сказал</w> мне больше, чем тысяча слов."
                            }
                        },
                        {
                            "text": "Co autor chciał <w>powiedzieć</w> przez tę metaforę w ostatnim wersie?",
                            "target_lang_translations": {
                                "ru": "Что автор хотел <w>сказать</w> этой метафорой в последней строке?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "co chcesz powiedzieć",
                        "że tak powiem",
                        "wiele powiedzieć o"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать смысл или намерение невербально или косвенно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c69fc8fe-72e6-3fd3-8c95-e1918fe36d17",
                    "sense_definition": "Onderdelen passend maken en aan elkaar bevestigen, vooral gebruikt in de installatietechniek voor leidingen en buizen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "assembly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De loodgieter moest de nieuwe waterleidingen nauwkeurig <w>fitten</w>.",
                            "target_lang_translations": {
                                "en": "The plumber had to <w>fit</w> the new water pipes precisely."
                            }
                        },
                        {
                            "text": "Het is een kunst om deze koppelstukken goed op elkaar te <w>fitten</w>.",
                            "target_lang_translations": {
                                "en": "It is an art to <w>fit</w> these couplings together properly."
                            }
                        }
                    ],
                    "synonyms": [
                        "monteren",
                        "assembleren"
                    ],
                    "antonyms": [
                        "demonteren"
                    ],
                    "common_phrases": [
                        "buizen fitten",
                        "leidingen fitten"
                    ],
                    "target_lang_definitions": {
                        "en": "To make parts fit and attach them to each other, especially used in installation technology for pipes and tubes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "to join pipes or parts together"
                            },
                            {
                                "target_lang_word": "install",
                                "target_lang_sense_clarification": "to set up piping or machinery"
                            },
                            {
                                "target_lang_word": "assemble",
                                "target_lang_sense_clarification": "to put components together"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0204fcd-8f0a-3d58-a188-8800a560ca4a",
                    "sense_definition": "Meervoud verleden tijd van <w>fitten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>fitten</w> gisteren de laatste onderdelen van de machine.",
                            "target_lang_translations": {

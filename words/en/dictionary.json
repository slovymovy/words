    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cf579a2a-ad46-375f-ac08-baa96c783dd2",
                    "sense_definition": "A reference work listing words from one or more languages, usually ordered alphabetically, explaining their meanings, pronunciations, and usage.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reference_book",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new English <w>dictionary</w> for my classes.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый <w>словарь</w> английского языка для занятий."
                            }
                        },
                        {
                            "text": "If you do not know the word, look it up in the <w>dictionary</w>.",
                            "target_lang_translations": {
                                "ru": "Если вы не знаете слово, посмотрите его в <w>словаре</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lexicon",
                        "explanatory dictionary"
                    ],
                    "common_phrases": [
                        "English dictionary",
                        "online dictionary",
                        "look up in the dictionary",
                        "bilingual dictionary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справочное издание, содержащее слова одного или нескольких языков, обычно расположенные в алфавитном порядке, с объяснением их значения, произношения и употребления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "словарь",
                                "target_lang_sense_clarification": "книга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "324ab8cb-c78a-3bac-a52d-e8fdc80d388c",
                    "sense_definition": "A reference book or resource on a particular subject, where entries are arranged alphabetically.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reference_book",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The medical <w>dictionary</w> contains definitions for thousands of diseases.",
                            "target_lang_translations": {
                                "ru": "Медицинский <w>словарь</w> содержит определения тысяч болезней."
                            }
                        },
                        {
                            "text": "She consulted a biographical <w>dictionary</w> to find the writer's birth date.",
                            "target_lang_translations": {
                                "ru": "Она обратилась к биографическому <w>словарю</w>, чтобы найти дату рождения писателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "lexicon"
                    ],
                    "common_phrases": [
                        "medical dictionary",
                        "law dictionary",
                        "biographical dictionary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справочник или ресурс по определенной теме, где статьи расположены в алфавитном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "словарь",
                                "target_lang_sense_clarification": "справочник по теме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98cdae9a-6e43-37c0-9484-5a0b1bb99ea0",
                    "sense_definition": "A data structure in computing where values are stored and referenced by unique keys.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program uses a <w>dictionary</w> to store user settings and preferences.",
                            "target_lang_translations": {
                                "ru": "Программа использует <w>словарь</w> для хранения настроек и предпочтений пользователя."
                            }
                        },
                        {
                            "text": "You can retrieve the value from the <w>dictionary</w> by using its specific key.",
                            "target_lang_translations": {
                                "ru": "Вы можете получить значение из <w>словаря</w>, используя его конкретный ключ."
                            }
                        }
                    ],
                    "synonyms": [
                        "dict"
                    ],
                    "common_phrases": [
                        "dictionary keys",
                        "dictionary values",
                        "key-value pair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Структура данных в информатике, где значения хранятся и вызываются по уникальным ключам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "словарь",
                                "target_lang_sense_clarification": "структура данных"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6650d7c8-8a41-31da-8397-21686b5fcc0f",
                    "sense_definition": "To look up a word or information in a dictionary.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had to <w>dictionary</w> the unusual term to understand what the author meant.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <w>посмотреть в словаре</w> этот необычный термин, чтобы понять, что имел в виду автор."
                            }
                        },
                        {
                            "text": "She <w>dictionaried</w> the word quickly on her phone.",
                            "target_lang_translations": {
                                "ru": "Она быстро <w>посмотрела в словаре</w> это слово на своем телефоне."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "rarely used as a verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать слово или информацию в словаре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать в словаре",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e109d0b2-9ebc-3924-ad36-8ab450eed87d",
                    "sense_definition": "To add a word to a dictionary or compile it into one.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many slang words are never <w>dictionaried</w> despite being popular.",
                            "target_lang_translations": {
                                "ru": "Многие сленговые слова так и не <w>вносятся в словарь</w>, несмотря на их популярность."
                            }
                        },
                        {
                            "text": "The lexicographer spent years deciding which new terms should be <w>dictionaried</w>.",
                            "target_lang_translations": {
                                "ru": "Лексикограф потратил годы, решая, какие новые термины следует <w>внести в словарь</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "rarely used as a verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять слово в словарь или составлять его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вносить в словарь",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "305992a6-1cd8-3ef5-b82d-a1f705881343",
                    "sense_definition": "A nickname for a very studious person or someone who uses unnecessarily complicated words.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nickname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "His classmates called him <w>Dictionary</w> because he always knew the answers.",
                            "target_lang_translations": {
                                "ru": "Одноклассники называли его <w>Словарь</w>, потому что он всегда знал ответы."
                            }
                        },
                        {
                            "text": "Don't be such a <w>Dictionary</w>; just explain it simply.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>Словарем</w>, просто объясни это попроще."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a nickname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозвище для очень прилежного человека или того, кто использует излишне сложные слова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Словарь",
                                "target_lang_sense_clarification": "прозвище"
                            }
                        ]
                    }
                }
            ]
        }
    ]

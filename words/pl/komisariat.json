    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a3baca39-cb83-3f7e-b456-0111ee1b789c",
                    "sense_definition": "Budynek lub lokal, w którym znajduje się siedziba jednostki policji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "police_station",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść na <w>komisariat</w>, żeby zgłosić kradzież portfela.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в <w>отделение полиции</w>, чтобы заявить о краже кошелька."
                            }
                        },
                        {
                            "text": "Najbliższy <w>komisariat</w> policji znajduje się w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Ближайший <w>полицейский участок</w> находится в центре города."
                            }
                        }
                    ],
                    "synonyms": [
                        "posterunek"
                    ],
                    "common_phrases": [
                        "komisariat policji",
                        "iść na komisariat",
                        "zgłosić na komisariacie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или помещение, в котором располагается подразделение полиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделение полиции",
                                "target_lang_sense_clarification": "здание полиции"
                            },
                            {
                                "target_lang_word": "полицейский участок",
                                "target_lang_sense_clarification": "место расположения полиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a5764e2-a9d2-3f8c-b448-61a906fa8387",
                    "sense_definition": "Jednostka organizacyjna policji odpowiedzialna za bezpieczeństwo na określonym terenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "police_station",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały <w>komisariat</w> pracuje nad rozwiązaniem tej trudnej zagadki kryminalnej.",
                            "target_lang_translations": {
                                "ru": "Всё <w>отделение</w> работает над решением этой сложной криминальной загадки."
                            }
                        },
                        {
                            "text": "Nowy komendant zarządza pracą, którą wykonuje ten <w>komisariat</w>.",
                            "target_lang_translations": {
                                "ru": "Новый комендант руководит работой, которую выполняет это <w>отделение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "komendant komisariatu",
                        "pracownik komisariatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организационное подразделение полиции, отвечающее за безопасность на определенной территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделение",
                                "target_lang_sense_clarification": "подразделение полиции"
                            },
                            {
                                "target_lang_word": "отдел",
                                "target_lang_sense_clarification": "организационная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4addf438-1f9b-312f-b0a7-cfbbf80bb724",
                    "sense_definition": "Urząd administracyjny, często o charakterze tymczasowym lub specjalnym, powołany do realizacji określonych zadań (znaczenie głównie historyczne lub administracyjne).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W okresie międzywojennym w Gdańsku funkcjonował <w>komisariat</w> generalny.",
                            "target_lang_translations": {
                                "ru": "В межвоенный период в Гданьске функционировал генеральный <w>комиссариат</w>."
                            }
                        },
                        {
                            "text": "Historycy analizują dokumenty wydane przez ówczesny <w>komisariat</w> wojskowy.",
                            "target_lang_translations": {
                                "ru": "Историки анализируют документы, изданные тогдашним военным <w>комиссариатом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "komisariat rządu",
                        "ludowy komisariat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административное учреждение, часто временного или специального характера (историческое или административное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комиссариат",
                                "target_lang_sense_clarification": "административное или историческое учреждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
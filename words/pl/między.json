            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "b7b9cba9-ecb8-3a4f-a7aa-2d5911be2a67",
                    "sense_definition": "Określa położenie w przestrzeni oddzielającej dwa obiekty, osoby lub miejsca.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stół stoi <między> łóżkiem a krzesłem.",
                            "target_lang_translations": {
                                "ru": "Стол стоит <w>между</w> кроватью и стулом."
                            }
                        },
                        {
                            "text": "Samochód zaparkował <między> dwoma autobusami.",
                            "target_lang_translations": {
                                "ru": "Машина припарковалась <w>между</w> двумя автобусами."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomiędzy"
                    ],
                    "common_phrases": [
                        "między nami",
                        "między drzwiami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает положение в пространстве, разделяющем два объекта, человека или места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "между",
                                "target_lang_sense_clarification": "пространственное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bac81109-cf5b-308e-be80-fbaae736b1ef",
                    "sense_definition": "Określa położenie w otoczeniu grupy osób lub rzeczy; wskazuje na bycie częścią zbioru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W parku pospacerowałem sobie <między> drzewami.",
                            "target_lang_translations": {
                                "ru": "В парке я погулял <w>между</w> деревьями."
                            }
                        },
                        {
                            "text": "Czuł się bardzo dobrze <między> przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя очень хорошо <w>среди</w> друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "wśród",
                        "pośród",
                        "pomiędzy"
                    ],
                    "common_phrases": [
                        "między innymi",
                        "między ludźmi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает нахождение в окружении группы лиц или предметов; указывает на принадлежность к множеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среди",
                                "target_lang_sense_clarification": "в окружении, в числе"
                            },
                            {
                                "target_lang_word": "между",
                                "target_lang_sense_clarification": "среди множества объектов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b8c46c9-f3d0-32fb-aa28-ffe2af957467",
                    "sense_definition": "Określa przedział czasowy oddzielający dwa zdarzenia lub punkty w czasie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wpadnę do ciebie <między> piątą a szóstą.",
                            "target_lang_translations": {
                                "ru": "Я заскочу к тебе <w>между</w> пятью и шестью."
                            }
                        },
                        {
                            "text": "Sklep jest zamknięty <między> trzynastą a czternastą.",
                            "target_lang_translations": {
                                "ru": "Магазин закрыт <w>между</w> тринадцатью и четырнадцатью часами."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomiędzy"
                    ],
                    "common_phrases": [
                        "między poniedziałkiem a wtorkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает временной интервал, разделяющий два события или момента времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "между",
                                "target_lang_sense_clarification": "временное значение"

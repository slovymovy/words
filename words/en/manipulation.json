            "senses": [
                {
                    "sense_id": "bc8af5ca-ab1d-31b0-b65d-72c7d94a29f4",
                    "sense_definition": "The usage of psychological influence over a person, event, or situation to gain a desired outcome.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used emotional <w>manipulation</w> to make her feel guilty about the decision.",
                            "target_lang_translations": {
                                "ru": "Он использовал эмоциональную <w>манипуляцию</w>, чтобы заставить её чувствовать вину за это решение."
                            }
                        },
                        {
                            "text": "The politician was accused of media <w>manipulation</w> to win the election.",
                            "target_lang_translations": {
                                "ru": "Политика обвинили в <w>манипулировании</w> СМИ ради победы на выборах."
                            }
                        }
                    ],
                    "synonyms": [
                        "influence",
                        "exploitation",
                        "control"
                    ],
                    "antonyms": [
                        "honesty",
                        "sincerity"
                    ],
                    "common_phrases": [
                        "emotional manipulation",
                        "market manipulation",
                        "political manipulation",
                        "psychological manipulation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использование психологического влияния на человека, событие или ситуацию для получения желаемого результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манипуляция",
                                "target_lang_sense_clarification": "психологическое воздействие"
                            },
                            {
                                "target_lang_word": "манипулирование",
                                "target_lang_sense_clarification": "процесс влияния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fca9472-d36f-343d-bb4f-454402f9dafe",
                    "sense_definition": "The action of handling, controlling, or changing something in a skillful manner.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software allows for easy <w>manipulation</w> of digital images.",
                            "target_lang_translations": {
                                "ru": "Это программное обеспечение позволяет легко выполнять <w>обработку</w> цифровых изображений."
                            }
                        },
                        {
                            "text": "Careful data <w>manipulation</w> is necessary to analyze the complex results.",
                            "target_lang_translations": {
                                "ru": "Для анализа сложных результатов необходима тщательная <w>обработка</w> данных."

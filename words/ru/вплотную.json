                    ],
                    "synonyms": [
                        "всерьёз"
                    ],
                    "antonyms": [
                        "спустя рукава"
                    ],
                    "common_phrases": [
                        "заняться вплотную",
                        "взяться вплотную"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Seriously, thoroughly, with full diligence and attention (usually regarding work or a task)."
                    },
                    "translations": {

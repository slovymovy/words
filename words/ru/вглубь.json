    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2c1d48f7-cc1a-3a11-860e-d62b9018f610",
                    "sense_definition": "В глубину, далеко внутрь или вниз, на большое расстояние от начала или поверхности.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не плавает на поверхности, а сразу ныряет <w>вглубь</w>.",
                            "target_lang_translations": {
                                "en": "He doesn't swim on the surface, but immediately dives <w>deep</w>."
                            }
                        },
                        {
                            "text": "Алина с силой вдавила кнопку переговорного устройства <w>вглубь</w> и вызвала секретаршу.",
                            "target_lang_translations": {
                                "en": "Alina forcefully pressed the intercom button <w>inwards</w> and called the secretary."
                            }
                        }
                    ],
                    "synonyms": [
                        "внутрь",
                        "вниз"
                    ],
                    "antonyms": [
                        "наружу",
                        "вверх",
                        "вширь"
                    ],
                    "common_phrases": [
                        "уйти вглубь",
                        "смотреть вглубь",
                        "проникнуть вглубь"
                    ],
                    "target_lang_definitions": {
                        "en": "To a great depth, far inside or downwards from the surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deep",
                                "target_lang_sense_clarification": "Far down or in"
                            },
                            {
                                "target_lang_word": "inwards",
                                "target_lang_sense_clarification": "Towards the inside"
                            },
                            {
                                "target_lang_word": "into the depths",
                                "target_lang_sense_clarification": "To the deep part"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f5687e17-61c8-3735-a390-547ff6ceec9c",
                    "sense_definition": "Указывает направление внутрь, в толщу чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Расстроенный, он побежал <w>вглубь</w> леса, прислушиваясь ко всем вечерним шорохам.",
                            "target_lang_translations": {
                                "en": "Upset, he ran <w>deep into</w> the forest, listening to all the evening rustles."
                            }
                        },
                        {
                            "text": "Экспедиция отправилась <w>вглубь</w> острова, чтобы изучить местную флору.",
                            "target_lang_translations": {
                                "en": "The expedition went <w>deep into</w> the island to study the local flora."
                            }
                        }
                    ],
                    "synonyms": [
                        "внутрь"
                    ],
                    "antonyms": [
                        "из"
                    ],
                    "common_phrases": [
                        "вглубь леса",
                        "вглубь страны",
                        "вглубь веков"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates direction deep into the interior or thick of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deep into",
                                "target_lang_sense_clarification": "Far inside something"
                            },
                            {
                                "target_lang_word": "into the heart of",
                                "target_lang_sense_clarification": "To the center or interior"
                            }
                        ]
                    }
                }
            ]
        }
    ]

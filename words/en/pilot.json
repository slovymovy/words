                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пилотный",
                                "target_lang_sense_clarification": "проект или исследование"
                            },
                            {
                                "target_lang_word": "пробный",
                                "target_lang_sense_clarification": "тестовый"
                            },
                            {
                                "target_lang_word": "экспериментальный",
                                "target_lang_sense_clarification": "опытный"
                            }
                        ]
                    }
                }
            ]
        }

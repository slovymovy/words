    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12573392-8431-3ec2-bc57-1940f84d8d3d",
                    "sense_definition": "Het verschil zien tussen zaken of personen, of deze als verschillend behandelen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is belangrijk dat kinderen goed van kwaad leren <w>onderscheiden</w>.",
                            "target_lang_translations": {
                                "ru": "Важно, чтобы дети учились <w>отличать</w> добро от зла.",
                                "en": "It is important that children learn to <w>distinguish</w> good from evil."
                            }
                        },
                        {
                            "text": "In de dichte mist kon ik de huizen nauwelijks van elkaar <w>onderscheiden</w>.",
                            "target_lang_translations": {
                                "ru": "В густом тумане я с трудом мог <w>различить</w> дома.",
                                "en": "In the dense fog I could hardly <w>distinguish</w> the houses from each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "uit elkaar houden",
                        "onderkennen",
                        "waarnemen"
                    ],
                    "antonyms": [
                        "ononderscheiden",
                        "ononderscheiden",
                        "ononderscheiden"
                    ],
                    "common_phrases": [
                        "goed van kwaad onderscheiden",
                        "niet te onderscheiden",
                        "onderscheid maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видеть разницу между вещами или людьми или относиться к ним как к разным.",
                        "en": "To see the difference between things or people, or to treat them as different."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "различать",
                                "target_lang_sense_clarification": "видеть разницу"
                            },
                            {
                                "target_lang_word": "отличать",
                                "target_lang_sense_clarification": "находить отличия"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "distinguish",
                                "target_lang_sense_clarification": "recognize a difference"
                            },
                            {
                                "target_lang_word": "discern",
                                "target_lang_sense_clarification": "perceive clearly"
                            },
                            {
                                "target_lang_word": "differentiate",
                                "target_lang_sense_clarification": "identify differences"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74a211ed-9756-3bd6-adb5-9a771cb619fe",
                    "sense_definition": "Iemand een officiële beloning, zoals een medaille of lintje, geven voor een bijzondere prestatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "honor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester werd <w>onderscheiden</w> met een koninklijke medaille.",
                            "target_lang_translations": {
                                "ru": "Мэр был <w>награжден</w> королевской медалью.",
                                "en": "The mayor was <w>decorated</w> with a royal medal."
                            }
                        },
                        {
                            "text": "Hij is postuum <w>onderscheiden</w> voor zijn heldhaftige optreden.",
                            "target_lang_translations": {
                                "ru": "Он был посмертно <w>награжден</w> за свои героические действия.",
                                "en": "He was posthumously <w>decorated</w> for his heroic actions."
                            }
                        }
                    ],
                    "synonyms": [
                        "decoreren",
                        "eren",
                        "lauweren"
                    ],
                    "common_phrases": [
                        "koninklijk onderscheiden",
                        "onderscheiden worden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вручать кому-либо официальную награду, например медаль или орден, за особые достижения.",
                        "en": "To give someone an official reward, such as a medal or ribbon, for a special achievement."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "награждать",
                                "target_lang_sense_clarification": "давать награду"
                            },

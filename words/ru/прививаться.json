{
    "word_family": [
        "прививать",
        "прививка",
        "прививочный",
        "прививание"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "905fd1b0-8216-36c1-8a79-0f81118e3254",
                    "sense_definition": "Делать себе прививку (вводить вакцину) для защиты от инфекционных заболеваний.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи настоятельно рекомендуют <w>прививаться</w> перед началом сезона простуд.",
                            "target_lang_translations": {
                                "en": "Doctors strongly recommend <w>getting vaccinated</w> before the start of the cold season."
                            }
                        },
                        {
                            "text": "Многие люди боятся <w>прививаться</w> из-за возможных побочных эффектов.",
                            "target_lang_translations": {
                                "en": "Many people are afraid to <w>get vaccinated</w> because of possible side effects."
                            }
                        }
                    ],
                    "synonyms": [
                        "вакцинироваться"
                    ],
                    "common_phrases": [
                        "прививаться от гриппа",
                        "прививаться вакциной",
                        "нужно прививаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To get a vaccination (inject a vaccine) for protection against infectious diseases."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get vaccinated",
                                "target_lang_sense_clarification": "medical action"
                            },
                            {
                                "target_lang_word": "be vaccinated",
                                "target_lang_sense_clarification": "state or action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e959d217-5b51-3b94-b8d8-141c05ab1ba0",
                    "sense_definition": "Становиться привычным, усваиваться (о навыках, вкусах, правилах поведения или культуре).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_assimilation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хороший литературный вкус <w>прививается</w> ребёнку через чтение классики.",
                            "target_lang_translations": {
                                "en": "Good literary taste <w>is instilled</w> in a child through reading classics."
                            }
                        },
                        {
                            "text": "Уважение к старшим <w>прививается</w> в семье с раннего детства.",
                            "target_lang_translations": {
                                "en": "Respect for elders <w>is instilled</w> in the family from early childhood."
                            }
                        }
                    ],
                    "synonyms": [
                        "усваиваться",
                        "приживаться",
                        "укореняться"
                    ],
                    "common_phrases": [
                        "вкус прививается",
                        "любовь прививается",
                        "культура прививается"
                    ],
                    "target_lang_definitions": {
                        "en": "To become habitual, to be assimilated (regarding skills, tastes, rules of behavior, or culture)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be instilled",
                                "target_lang_sense_clarification": "of habits or values"
                            },
                            {
                                "target_lang_word": "be cultivated",
                                "target_lang_sense_clarification": "of tastes or skills"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
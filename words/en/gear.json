        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5f820a39-66dd-3b03-8498-4f9fea5c16b6",
                    "sense_definition": "To design, organize, or adjust something to suit a specific purpose, need, or group of people.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new educational program is <w>geared</w> towards helping children with learning difficulties.",
                            "target_lang_translations": {
                                "ru": "Новая образовательная программа <w>ориентирована</w> на помощь детям с трудностями в обучении."
                            }
                        },
                        {
                            "text": "Most of the advertising is <w>geared</w> at a younger audience.",
                            "target_lang_translations": {
                                "ru": "Большая часть рекламы <w>нацелена</w> на более молодую аудиторию."
                            }
                        }
                    ],
                    "synonyms": [
                        "tailor",
                        "adapt",
                        "aim",
                        "direct"
                    ],
                    "common_phrases": [
                        "geared towards",
                        "geared to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрабатывать, организовывать или приспосабливать что-либо для конкретной цели или группы людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентировать",
                                "target_lang_sense_clarification": "направлять на определенную аудиторию"
                            },
                            {
                                "target_lang_word": "нацеливать",
                                "target_lang_sense_clarification": "предназначать для чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bac04794-e9ae-3c86-839a-8a24756ef59f",
                    "sense_definition": "To provide a machine with gears to achieve a desired speed or power ratio.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bicycle is <w>geared</w> for mountain climbing with a special chainset.",
                            "target_lang_translations": {
                                "ru": "Велосипед <w>оснащён передачами</w> для подъема в гору благодаря специальной системе звёзд."
                            }
                        },
                        {
                            "text": "This motor is <w>geared</w> to run at very high speeds.",
                            "target_lang_translations": {
                                "ru": "Этот мотор <w>настроен</w> на работу на очень высоких скоростях."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оснащать механизм зубчатыми передачами для достижения желаемой скорости или мощности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оснащать передачей",
                                "target_lang_sense_clarification": "техническое"
                            },
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "регулировать механизм под задачу"
                            }
                        ]
                    }
                }
            ]
        },
        {

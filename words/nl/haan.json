                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rooster",
                                "target_lang_sense_clarification": "male domestic chicken"
                            },
                            {
                                "target_lang_word": "cock",
                                "target_lang_sense_clarification": "male bird"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7779443-b748-3dbb-aaff-37421beb473a",
                    "sense_definition": "Een man die zich opvallend stoer, dominant of ijdel gedraagt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij loopt erbij als een trotse <w>haan</w> om indruk te maken.",
                            "target_lang_translations": {
                                "en": "He walks around like a proud <w>rooster</w> to make an impression."
                            }
                        },
                        {
                            "text": "Die jongen is echt een <w>haan</w> en wil altijd de baas spelen.",
                            "target_lang_translations": {

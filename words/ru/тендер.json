    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3aa4c489-2f59-3c21-8c20-d1440d937b21",
                    "sense_definition": "Конкурсный отбор предложений на поставку товаров, оказание услуг или выполнение работ по заранее объявленным условиям.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша строительная компания выиграла <w>тендер</w> на ремонт городской больницы.",
                            "target_lang_translations": {
                                "en": "Our construction company won the <w>tender</w> for the renovation of the city hospital."
                            }
                        },
                        {
                            "text": "Государство объявило <w>тендер</w> на закупку новых компьютеров для школ.",
                            "target_lang_translations": {
                                "en": "The state announced a <w>tender</w> for the purchase of new computers for schools."
                            }
                        }
                    ],
                    "synonyms": [
                        "конкурс",
                        "торги"
                    ],
                    "common_phrases": [
                        "выиграть тендер",
                        "объявить тендер",
                        "участие в тендере",
                        "международный тендер"
                    ],
                    "target_lang_definitions": {
                        "en": "Competitive selection of proposals for the supply of goods, provision of services, or execution of works under previously announced conditions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tender",
                                "target_lang_sense_clarification": "business/legal process"
                            },
                            {
                                "target_lang_word": "bid",
                                "target_lang_sense_clarification": "offer or proposal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dc39cc5-1c32-3096-b611-95a1c5eb00e8",
                    "sense_definition": "Специальный вагон, прицепляемый к паровозу для перевозки запаса топлива и воды.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кочегар забрался на <w>тендер</w>, чтобы сбросить уголь ближе к топке.",
                            "target_lang_translations": {
                                "en": "The stoker climbed onto the <w>tender</w> to throw coal closer to the firebox."
                            }
                        },
                        {
                            "text": "За старым паровозом следовал ржавый <w>тендер</w>, наполненный дровами.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df34fade-1ff6-3572-a4d6-64293e30dc5b",
                    "sense_definition": "Cienki, metalowy drucik zagięty w kształt litery U, służący do łączenia kartek papieru za pomocą zszywacza.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym zszywaczu brakuje <w>zszywek</w>, więc nie mogę połączyć dokumentów.",
                            "target_lang_translations": {
                                "ru": "В этом степлере нет <w>скоб</w>, поэтому я не могу скрепить документы."
                            }
                        },
                        {
                            "text": "Muszę kupić nowe pudełko <w>zszywek</w> do biura.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую коробку <w>скоб</w> для офиса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pudełko zszywek",
                        "rozmiar zszywek",
                        "wyciągnąć zszywkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий металлический элемент, изогнутый в форме буквы П, используемый для скрепления листов бумаги с помощью степлера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скоба",
                                "target_lang_sense_clarification": "для степлера"
                            },
                            {
                                "target_lang_word": "скобка",
                                "target_lang_sense_clarification": "канцелярская"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d0819f5-e831-33b8-9a4c-a18a62856c93",
                    "sense_definition": "Zbiór numerów czasopisma lub gazet z określonego okresu, trwale połączonych lub oprawionych w jedną całość.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W bibliotece uniwersyteckiej dostępna jest <w>zszywka</w> gazet z lat przedwojennych.",
                            "target_lang_translations": {
                                "ru": "В университетской библиотеке доступна <w>подшивка</w> газет довоенных лет."
                            }
                        },
                        {
                            "text": "Ta archiwalna <w>zszywka</w> zawiera wszystkie wydania miesięcznika z ubiegłego roku.",
                            "target_lang_translations": {
                                "ru": "Эта архивная <w>подшивка</w> содержит все выпуски ежемесячника за прошлый год."
                            }
                        }
                    ],
                    "synonyms": [
                        "rocznik",
                        "wolumin"
                    ],
                    "common_phrases": [
                        "zszywka gazet",
                        "archiwalna zszywka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект номеров журнала или газет за определенный период, сшитых или переплетенных в одно целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подшивка",
                                "target_lang_sense_clarification": "газет или журналов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
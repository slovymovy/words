    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07f9e451-88a9-3ab9-9650-b79ea1eeb1bb",
                    "sense_definition": "Something necessary; a requisite; something indispensable.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Food, water, and shelter are the basic <necessities> of life.",
                            "target_lang_translations": {
                                "ru": "Еда, вода и жилье — это основные жизненные <потребности>."
                            }
                        },
                        {
                            "text": "A warm coat is a <necessity> when living in such a cold climate.",
                            "target_lang_translations": {
                                "ru": "Теплое пальто — это <необходимость>, когда живешь в таком холодном климате."
                            }
                        }
                    ],
                    "synonyms": [
                        "requisite"
                    ],
                    "antonyms": [
                        "luxury"
                    ],
                    "common_phrases": [
                        "bare necessities",
                        "basic necessities",
                        "daily necessities"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то необходимое; обязательное условие; что-то незаменимое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предмет первой необходимости",
                                "target_lang_sense_clarification": "жизненно важная вещь"
                            },
                            {
                                "target_lang_word": "необходимость",
                                "target_lang_sense_clarification": "вещь, без которой нельзя обойтись"
                            },
                            {
                                "target_lang_word": "потребность",
                                "target_lang_sense_clarification": "надобность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d548133-f02d-3ba7-a608-ae9ee75a2d31",
                    "sense_definition": "The quality or state of being necessary, unavoidable, or absolutely requisite.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to sell the car out of <necessity> rather than choice.",
                            "target_lang_translations": {
                                "ru": "Мы решили продать машину из <необходимости>, а не по собственному выбору."
                            }
                        },
                        {
                            "text": "He understood the <necessity> of finishing the project on time.",
                            "target_lang_translations": {
                                "ru": "Он понимал <необходимость> закончить проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "inevitability",
                        "necessariness",
                        "certainty"
                    ],
                    "antonyms": [
                        "contingency"
                    ],
                    "common_phrases": [
                        "out of necessity",
                        "of necessity",
                        "absolute necessity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство или состояние необходимости, неизбежности или обязательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необходимость",
                                "target_lang_sense_clarification": "обязательность, неизбежность"
                            },
                            {
                                "target_lang_word": "надобность",
                                "target_lang_sense_clarification": "нужда в чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc9c1c4e-cfaa-32a1-96c4-67a36e3d2de8",
                    "sense_definition": "The condition of being needy; desperate need; lack.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family lived in great <necessity> after the father lost his job.",
                            "target_lang_translations": {
                                "ru": "Семья жила в большой <нужде> после того, как отец потерял работу."
                            }
                        },
                        {
                            "text": "Charities were established to help those suffering from hunger and <necessity>.",
                            "target_lang_translations": {
                                "ru": "Благотворительные организации были созданы для помощи тем, кто страдает от голода и <нищеты>."
                            }
                        }
                    ],
                    "synonyms": [
                        "needfulness"
                    ],
                    "common_phrases": [
                        "dire necessity"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние нужды; крайняя потребность; нехватка средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нужда",
                                "target_lang_sense_clarification": "бедность, недостаток"
                            },
                            {
                                "target_lang_word": "нищета",
                                "target_lang_sense_clarification": "крайняя бедность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
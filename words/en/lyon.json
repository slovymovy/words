                    "sense_id": "2fc3d408-01c1-3996-a28a-1e1b548ab1df",
                    "sense_definition": "The third largest city in France and the capital of the Auvergne-Rhône-Alpes region.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We plan to visit <w>Lyon</w> during our trip to France next summer.",
                            "target_lang_translations": {
                                "ru": "Мы планируем посетить <w>Лион</w> во время нашей поездки во Францию следующим летом."
                            }
                        },
                        {
                            "text": "The city of <w>Lyon</w> is famous for its gastronomy and historical architecture.",
                            "target_lang_translations": {
                                "ru": "Город <w>Лион</w> славится своей гастрономией и исторической архитектурой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "city of Lyon",
                        "Old Lyon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Третий по величине город Франции и столица региона Овернь — Рона — Альпы."
                    },
                    "translations": {

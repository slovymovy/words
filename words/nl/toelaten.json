{
    "word_family": [
        "toelating",
        "toelaatbaar",
        "toelaatbaarheid"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f4e0c591-d561-3ea2-8332-1af15554caa6",
                    "sense_definition": "Toestemming geven voor iets of het niet verbieden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn ouders zouden dat gedrag nooit <w>toelaten</w>.",
                            "target_lang_translations": {
                                "en": "My parents would never <w>allow</w> that behavior."
                            }
                        },
                        {
                            "text": "We gaan picknicken als het weer het <w>toelaat</w>.",
                            "target_lang_translations": {
                                "en": "We are going on a picnic if the weather <w>permits</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toestaan",
                        "permitteren",
                        "toestaan"
                    ],
                    "antonyms": [
                        "verbieden",
                        "weigeren"
                    ],
                    "common_phrases": [
                        "als het weer het toelaat",
                        "dat kan ik niet toelaten",
                        "gebruik toelaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To give permission for something or not forbid it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allow",
                                "target_lang_sense_clarification": "general permission"
                            },
                            {
                                "target_lang_word": "permit",
                                "target_lang_sense_clarification": "formal permission"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6447661-6de3-3b58-bcba-5a38d792ab8a",
                    "sense_definition": "Iemand de toegang geven tot een ruimte, gebouw of gebied.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bewaker wilde ons niet <w>toelaten</w> in de club.",
                            "target_lang_translations": {
                                "en": "The bouncer did not want to <w>let</w> us <w>in</w> the club."
                            }
                        },
                        {
                            "text": "Honden worden in dit restaurant niet <w>toegelaten</w>.",
                            "target_lang_translations": {
                                "en": "Dogs are not <w>allowed</w> in this restaurant."
                            }
                        }
                    ],
                    "synonyms": [
                        "permitteren",
                        "toestaan"
                    ],
                    "antonyms": [
                        "buitensluiten",
                        "tegenhouden"
                    ],
                    "common_phrases": [
                        "bezoekers toelaten",
                        "niet toelaten in"
                    ],
                    "target_lang_definitions": {
                        "en": "To give someone access to a space, building, or area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "admit",
                                "target_lang_sense_clarification": "formal entry"
                            },
                            {
                                "target_lang_word": "allow",
                                "target_lang_sense_clarification": "permission to enter"
                            },
                            {
                                "target_lang_word": "let in",
                                "target_lang_sense_clarification": "informal entry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50be0b55-1558-313b-b1d3-28a6686c5b23",
                    "sense_definition": "Iemand accepteren als officieel lid, student of deelnemer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij werd vorig jaar <w>toegelaten</w> tot de universiteit.",
                            "target_lang_translations": {
                                "en": "She was <w>admitted</w> to the university last year."
                            }
                        },
                        {
                            "text": "De vereniging gaat binnenkort nieuwe leden <w>toelaten</w>.",
                            "target_lang_translations": {
                                "en": "The association is going to <w>admit</w> new members soon."
                            }
                        }
                    ],
                    "synonyms": [
                        "toestaan"
                    ],
                    "antonyms": [
                        "afwijzen"
                    ],
                    "common_phrases": [
                        "tot het examen toelaten",
                        "nieuwe studenten toelaten",
                        "als lid toelaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To accept someone as an official member, student, or participant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "admit",
                                "target_lang_sense_clarification": "into an institution or program"
                            },
                            {
                                "target_lang_word": "accept",
                                "target_lang_sense_clarification": "general acceptance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bdb3d9a-d72c-3547-a12c-9de775bcbb3a",
                    "sense_definition": "Iets accepteren of laten gebeuren wat eigenlijk verkeerd of verboden is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur wil geen pestgedrag <w>toelaten</w> op zijn school.",
                            "target_lang_translations": {
                                "en": "The principal does not want to <w>tolerate</w> bullying at his school."
                            }
                        },
                        {
                            "text": "Als samenleving mogen we dit soort geweld nooit <w>toelaten</w>.",
                            "target_lang_translations": {
                                "en": "As a society, we must never <w>tolerate</w> this kind of violence."
                            }
                        }
                    ],
                    "synonyms": [
                        "toestaan"
                    ],
                    "antonyms": [
                        "bestrijden"
                    ],
                    "common_phrases": [
                        "geen wangedrag toelaten",
                        "discriminatie toelaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To accept or let something happen that is actually wrong or forbidden."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "endure without interference"
                            },
                            {
                                "target_lang_word": "condone",
                                "target_lang_sense_clarification": "accept wrong behavior"
                            },
                            {
                                "target_lang_word": "allow",
                                "target_lang_sense_clarification": "let happen"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "7bd7c457-0ca7-3892-8e12-c3833a3c86bc",
                    "sense_definition": "W środku jakiegoś wnętrza, w jakimś pomieszczeniu lub obiekcie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wewnątrz sali jest piętnaście osób.",
                            "target_lang_translations": {
                                "ru": "Внутри зала находится пятнадцать человек."
                            }
                        },
                        {
                            "text": "Dokumenty zostały bezpiecznie ukryte <w>wewnątrz</w> sejfu.",
                            "target_lang_translations": {
                                "ru": "Документы были надежно спрятаны <w>внутри</w> сейфа."

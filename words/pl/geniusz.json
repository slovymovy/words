{
    "word_family": [
        "genialny",
        "genialnie",
        "genialność"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "336fcdc8-fef8-3899-8601-fd0541dbc185",
                    "sense_definition": "Osoba wybitna, o nieprzeciętnych zdolnościach intelektualnych lub artystycznych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Już w szkole podstawowej nauczyciele zauważyli, że jest <w>geniuszem</w>.",
                            "target_lang_translations": {
                                "ru": "Уже в начальной школе учителя заметили, что он <w>гений</w>."
                            }
                        },
                        {
                            "text": "Ten młody <w>geniusz</w> rozwiązał problem, z którym nie radzili sobie dorośli.",
                            "target_lang_translations": {
                                "ru": "Этот юный <w>гений</w> решил проблему, с которой не могли справиться взрослые."
                            }
                        }
                    ],
                    "common_phrases": [
                        "młody geniusz",
                        "uznany geniusz",
                        "geniusz matematyczny",
                        "zostać geniuszem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдающийся человек, обладающий необычайными интеллектуальными или творческими способностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гений",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea029d3d-c859-31aa-b3f8-4bf76a06464c",
                    "sense_definition": "Czyjeś nieprzeciętne zdolności, umiejętności; wybitny talent twórczy lub intelektualny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego muzyczny <w>geniusz</w> został doceniony dopiero po wielu latach.",
                            "target_lang_translations": {
                                "ru": "Его музыкальный <w>гений</w> был оценен по достоинству лишь спустя много лет."
                            }
                        },
                        {
                            "text": "W tym prostym wynalazku widać prawdziwy przebłysk <w>geniuszu</w>.",
                            "target_lang_translations": {
                                "ru": "В этом простом изобретении виден настоящий проблеск <w>гения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wybitny talent",
                        "genialność"
                    ],
                    "common_phrases": [
                        "przebłysk geniuszu",
                        "iskra geniuszu",
                        "granice geniuszu",
                        "podziwiać geniusz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычайные способности, умения; выдающийся творческий или интеллектуальный талант."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гений",
                                "target_lang_sense_clarification": "высшая творческая способность"
                            },
                            {
                                "target_lang_word": "гениальность",
                                "target_lang_sense_clarification": "талант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b38368c9-2a3c-3c0b-8e58-da7e2ef94134",
                    "sense_definition": "Ponadprzeciętność, wybitność czegoś (np. dzieła, pomysłu lub rozwiązania).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Geniusz</w> tego planu polega na jego niezwykłej prostocie.",
                            "target_lang_translations": {
                                "ru": "<w>Гениальность</w> этого плана заключается в его необычайной простоте."
                            }
                        },
                        {
                            "text": "Krytycy filmowi jednogłośnie uznali <w>geniusz</w> scenariusza.",
                            "target_lang_translations": {
                                "ru": "Кинокритики единогласно признали <w>гениальность</w> сценария."
                            }
                        }
                    ],
                    "synonyms": [
                        "genialność"
                    ],
                    "common_phrases": [
                        "geniusz prostoty",
                        "geniusz konstrukcji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исключительность, выдающееся качество чего-либо (например, произведения, идеи или решения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гениальность",
                                "target_lang_sense_clarification": "свойство"
                            },
                            {
                                "target_lang_word": "гений",
                                "target_lang_sense_clarification": "высшее проявление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c96ebfeb-d24d-3f3a-8d5a-05aa440c0442",
                    "sense_definition": "W mitologii rzymskiej: duch sprawujący pieczę nad człowiekiem, społecznością lub miejscem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mythology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytni Rzymianie składali ofiary swojemu opiekuńczemu <w>geniuszowi</w>.",
                            "target_lang_translations": {
                                "ru": "Древние римляне приносили жертвы своему <w>гению</w>-покровителю."
                            }
                        },
                        {
                            "text": "Wierzono, że <w>geniusz</w> tego miejsca chroni mieszkańców przed nieszczęściem.",
                            "target_lang_translations": {
                                "ru": "Считалось, что <w>гений</w> этого места охраняет жителей от несчастий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "geniusz loci",
                        "dobry geniusz"
                    ],
                    "target_lang_definitions": {
                        "ru": "В римской мифологии: дух-покровитель человека, общины или места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гений",
                                "target_lang_sense_clarification": "мифология"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
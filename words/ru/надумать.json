    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "43ba6bf4-ec7c-3ac1-9b6e-ffdbc86abdaf",
                    "sense_definition": "Принять решение сделать что-либо после размышлений.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "decision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если ты всё-таки <w>надумаешь</w> поехать с нами, обязательно позвони заранее.",
                            "target_lang_translations": {
                                "en": "If you do <w>decide</w> to go with us, be sure to call in advance."
                            }
                        },
                        {
                            "text": "Мы долго обсуждали этот вопрос и наконец <w>надумали</w> купить новую квартиру.",
                            "target_lang_translations": {
                                "en": "We discussed this issue for a long time and finally <w>decided</w> to buy a new apartment."
                            }
                        }
                    ],
                    "synonyms": [
                        "решить",
                        "решиться",
                        "задумать"
                    ],
                    "antonyms": [
                        "раздумать",
                        "передумать"
                    ],
                    "common_phrases": [
                        "если надумаешь",
                        "надумать жениться",
                        "надумать уехать"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a decision to do something after thinking about it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decide",
                                "target_lang_sense_clarification": "to choose to do something after consideration"
                            },
                            {
                                "target_lang_word": "make up one's mind",
                                "target_lang_sense_clarification": "to reach a final decision"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15be817a-dac8-3507-b07b-20b8738fe00c",
                    "sense_definition": "Придумать, сочинить или вообразить что-либо, чего часто нет на самом деле.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "invention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит <w>надумать</w> себе лишних страхов и переживать без причины.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>invent</w> unnecessary fears for yourself and worry without reason."
                            }
                        },
                        {
                            "text": "Она <w>надумала</w> целую историю, чтобы оправдать своё опоздание.",
                            "target_lang_translations": {
                                "en": "She <w>made up</w> a whole story to justify her lateness."
                            }
                        }
                    ],
                    "synonyms": [
                        "придумать",
                        "выдумать",
                        "сочинить",
                        "нафантазировать"
                    ],
                    "common_phrases": [
                        "надумать себе",
                        "надумать лишнего",
                        "надумать проблем"
                    ],
                    "target_lang_definitions": {
                        "en": "To invent, compose, or imagine something that often does not actually exist."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invent",
                                "target_lang_sense_clarification": "to create or devise in the mind"
                            },
                            {
                                "target_lang_word": "make up",
                                "target_lang_sense_clarification": "to fabricate or concoct a story or idea"
                            },
                            {
                                "target_lang_word": "imagine",
                                "target_lang_sense_clarification": "to form a mental image of something not present"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
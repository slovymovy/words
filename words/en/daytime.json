    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a8a8bb7-1ae9-340f-91ba-5a991d87979b",
                    "sense_definition": "The time of the day when there is natural light, specifically the period between sunrise and sunset.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is much safer to drive on this road in the <w>daytime</w>.",
                            "target_lang_translations": {
                                "ru": "Ездить по этой дороге <w>днём</w> гораздо безопаснее."
                            }
                        },
                        {
                            "text": "Most animals are active during the <w>daytime</w> and sleep at night.",
                            "target_lang_translations": {
                                "ru": "Большинство животных активны в <w>дневное время</w> и спят ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "day",
                        "daylight"
                    ],
                    "antonyms": [
                        "night",
                        "nighttime"
                    ],
                    "common_phrases": [
                        "in the daytime",
                        "during the daytime"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время суток, когда есть естественное освещение; период между восходом и закатом солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день",
                                "target_lang_sense_clarification": "светлое время суток"
                            },
                            {
                                "target_lang_word": "дневное время",
                                "target_lang_sense_clarification": "период от восхода до заката"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "99db64a9-5a56-3fd8-80d5-757c85173d6e",
                    "sense_definition": "Happening, existing, or active during the day rather than at night.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She enjoys watching <w>daytime</w> television when she has a day off.",
                            "target_lang_translations": {
                                "ru": "Ей нравится смотреть <w>дневное</w> телевидение, когда у неё выходной."
                            }
                        },
                        {
                            "text": "The <w>daytime</w> temperatures in the desert can be extremely high.",
                            "target_lang_translations": {
                                "ru": "<w>Дневные</w> температуры в пустыне могут быть чрезвычайно высокими."
                            }
                        }
                    ],
                    "synonyms": [
                        "day"
                    ],
                    "antonyms": [
                        "night",
                        "nighttime"
                    ],
                    "common_phrases": [
                        "daytime television",
                        "daytime hours",
                        "daytime job"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий, существующий или активный в дневное время, а не ночью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дневной",

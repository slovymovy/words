        "незаконный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1976aa7e-7a2c-3055-995d-781d67e75fa4",
                    "sense_definition": "Придавать чему-либо законную силу, утверждать на законном основании.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый указ позволяет гражданам быстрее <w>узаконивать</w> свои дачные постройки.",
                            "target_lang_translations": {
                                "en": "The new decree allows citizens to <w>legalize</w> their summer houses faster."
                            }
                        },
                        {
                            "text": "Они решили официально <w>узаконивать</w> свои отношения только после рождения ребёнка.",
                            "target_lang_translations": {
                                "en": "They decided to officially <w>legalize</w> their relationship only after the birth of their child."
                            }
                        }
                    ],
                    "synonyms": [
                        "легализова́ть",
                        "узаконять"
                    ],
                    "antonyms": [
                        "запрещать"
                    ],
                    "common_phrases": [
                        "узаконивать отношения",
                        "узаконивать права",
                        "узаконивать перепланировку",
                        "узаконивать постройку"
                    ],
                    "target_lang_definitions": {
                        "en": "To give legal force to something or approve it on a legal basis."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "legalize",
                                "target_lang_sense_clarification": "to make something legal under the law"
                            },
                            {
                                "target_lang_word": "legitimate",
                                "target_lang_sense_clarification": "to make lawful or valid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f29c60e-d62e-3b03-8139-9dbea008c9e2",
                    "sense_definition": "Делать что-либо общепринятым, признанным нормой (обычно о правилах, традициях или поведении).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Словарь фиксирует изменения в языке и тем самым <w>узаконивает</w> новые нормы произношения.",
                            "target_lang_translations": {
                                "en": "The dictionary records changes in the language and thereby <w>legitimizes</w> new pronunciation norms."
                            }
                        },
                        {
                            "text": "Длительная традиция часто <w>узаконивает</w> даже странные обычаи.",
                            "target_lang_translations": {
                                "en": "Long tradition often <w>legitimizes</w> even strange customs."
                            }
                        }
                    ],
                    "synonyms": [
                        "узаконять"
                    ],
                    "antonyms": [
                        "искоренять"
                    ],
                    "common_phrases": [
                        "узаконивать обычай",
                        "узаконивать традицию",
                        "узаконивать ошибку"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something generally accepted or recognized as a norm."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "legitimize",
                                "target_lang_sense_clarification": "to make something seem acceptable, proper, or correct"
                            },
                            {
                                "target_lang_word": "validate",
                                "target_lang_sense_clarification": "to demonstrate or support the validity of something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
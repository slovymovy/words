                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "spelling variant"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять борцовский прием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проводить прием",

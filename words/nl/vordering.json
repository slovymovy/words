{
    "word_family": [
        "vorderen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a0cb92d-61dc-389b-b2c9-c0d1a105d602",
                    "sense_definition": "De ontwikkeling of stap vooruit in een proces of activiteit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "progress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leerling maakt duidelijke <w>vorderingen</w> met het leren van de taal.",
                            "target_lang_translations": {
                                "ru": "Ученик делает заметные <w>успехи</w> в изучении языка."
                            }
                        },
                        {
                            "text": "Ik leunde tegen de rotswand en volgde gespannen zijn <w>vorderingen</w>.",
                            "target_lang_translations": {
                                "ru": "Я прислонился к скале и напряженно следил за его <w>продвижением</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vooruitgang",
                        "voortgang",
                        "progressie"
                    ],
                    "common_phrases": [
                        "vorderingen maken",
                        "gestage vordering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развитие или шаг вперед в каком-либо процессе или деятельности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прогресс",
                                "target_lang_sense_clarification": "развитие, улучшение"
                            },
                            {
                                "target_lang_word": "успехи",
                                "target_lang_sense_clarification": "достижения, положительные результаты"
                            },
                            {
                                "target_lang_word": "продвижение",
                                "target_lang_sense_clarification": "движение вперед"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a30acd8-37a8-3c0c-8e53-9deb3821d9ea",
                    "sense_definition": "Het recht dat iemand heeft om iets, meestal geld, van een ander te ontvangen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_claim",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf heeft nog een openstaande <w>vordering</w> op de klant.",
                            "target_lang_translations": {
                                "ru": "У компании все еще есть непогашенное <w>долговое требование</w> к клиенту."
                            }
                        },
                        {
                            "text": "Na het faillissement bleek de <w>vordering</w> helaas oninbaar te zijn.",
                            "target_lang_translations": {
                                "ru": "После банкротства <w>задолженность</w> оказалась, к сожалению, безнадежной."
                            }
                        }
                    ],
                    "synonyms": [
                        "claim"
                    ],
                    "antonyms": [
                        "schuld"
                    ],
                    "common_phrases": [
                        "vordering innen",
                        "vordering hebben op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право на получение чего-либо (обычно денег) от другого лица; долговое требование"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долговое требование",
                                "target_lang_sense_clarification": "право требовать возврата долга"
                            },
                            {
                                "target_lang_word": "задолженность",
                                "target_lang_sense_clarification": "сумма, причитающаяся к выплате"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98e3e10b-86aa-3400-b9c7-a0bb49c1ea46",
                    "sense_definition": "Een officiële en dwingende eis of bevel, vaak uitgesproken door een autoriteit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "official_demand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het rapport vormt de basis voor de <w>vordering</w> dat het bestuur moet aftreden.",
                            "target_lang_translations": {
                                "ru": "Отчет служит основанием для <w>требования</w>, чтобы правление ушло в отставку."
                            }
                        },
                        {
                            "text": "Op <w>vordering</w> van de politie moest de man zijn auto direct stoppen.",
                            "target_lang_translations": {
                                "ru": "По <w>требованию</w> полиции мужчина должен был немедленно остановить машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "eis"
                    ],
                    "common_phrases": [
                        "op vordering van",
                        "vordering instellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное и обязательное к исполнению требование или приказ, часто исходящие от властей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "требование",
                                "target_lang_sense_clarification": "официальный запрос или приказ"
                            },
                            {
                                "target_lang_word": "предписание",
                                "target_lang_sense_clarification": "письменное распоряжение властей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1d45f55-db56-392b-96ef-a4af1cebb4d3",
                    "sense_definition": "The standard unit of power in the International System of Units (SI), equal to one joule per second.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This old light bulb consumes sixty <w>watts</w> of electricity.",
                            "target_lang_translations": {
                                "ru": "Эта старая лампочка потребляет шестьдесят <w>ватт</w> электроэнергии."
                            }
                        },
                        {
                            "text": "The engine produces a power output of several thousand <w>watts</w>.",
                            "target_lang_translations": {
                                "ru": "Двигатель вырабатывает мощность в несколько тысяч <w>ватт</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hundred watts",
                        "million watts",
                        "watt-hour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартная единица измерения мощности в Международной системе единиц (СИ), равная одному джоулю в секунду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ватт",
                                "target_lang_sense_clarification": "единица измерения мощности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "72b03f30-379f-34eb-b556-936d1967c8d1",
                    "sense_definition": "An English and Scottish surname originating as a patronymic.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "James <w>Watt</w> was a famous Scottish inventor and mechanical engineer.",
                            "target_lang_translations": {
                                "ru": "Джеймс <w>Ватт</w> был известным шотландским изобретателем и инженером-механиком."
                            }
                        },
                        {
                            "text": "We have a meeting scheduled with Ms. <w>Watt</w> tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "У нас запланирована встреча с г-жой <w>Уотт</w> завтра утром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Английская и шотландская фамилия, происходящая от отчества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ватт",
                                "target_lang_sense_clarification": "фамилия (традиционная передача, напр. Джеймс Ватт)"
                            },
                            {
                                "target_lang_word": "Уотт",
                                "target_lang_sense_clarification": "фамилия (современная транслитерация)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0a9465d-addf-3ae5-9258-45ae2a9fc970",
                    "sense_definition": "A medieval diminutive of the male given name <w>Walter</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "In the historical novel, the peasant was simply known as <w>Watt</w>.",
                            "target_lang_translations": {
                                "ru": "В историческом романе крестьянин был известен просто как <w>Уотт</w>."
                            }
                        },
                        {
                            "text": "Old <w>Watt</w> tended to the horses in the village stable.",
                            "target_lang_translations": {
                                "ru": "Старина <w>Уотт</w> ухаживал за лошадьми в деревенской конюшне."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "medieval usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Средневековая уменьшительная форма мужского имени Уолтер."
                    },
                    "translations": {

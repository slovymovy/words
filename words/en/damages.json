    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fda37aed-c0d5-32f1-abb1-3d586f98fa53",
                    "sense_definition": "Third-person singular simple present indicative of <w>damage</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smoking seriously <w>damages</w> your health and can lead to many diseases.",
                            "target_lang_translations": {
                                "ru": "Курение серьезно <w>вредит</w> вашему здоровью и может привести ко многим заболеваниям."
                            }
                        },
                        {
                            "text": "The heavy rain often <w>damages</w> the crops in this valley during the spring.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь часто <w>повреждает</w> посевы в этой долине весной."
                            }
                        }
                    ],
                    "synonyms": [
                        "harms",
                        "hurts",
                        "injures",
                        "impairs",
                        "ruins"
                    ],
                    "antonyms": [
                        "fixes",
                        "repairs",
                        "mends",
                        "improves"
                    ],
                    "common_phrases": [
                        "damages the reputation",
                        "damages the environment",
                        "permanently damages"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола \"damage\" в третьем лице единственного числа настоящего времени (вредит, наносит ущерб)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вредит",
                                "target_lang_sense_clarification": "причинять вред здоровью или благополучию"
                            },
                            {
                                "target_lang_word": "наносит ущерб",
                                "target_lang_sense_clarification": "причинять урон чему-либо"
                            },
                            {
                                "target_lang_word": "повреждает",
                                "target_lang_sense_clarification": "портить физическое состояние объекта"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f729915-e7d3-3193-886e-eec3bc842d02",
                    "sense_definition": "Money that a court orders someone to pay to a person or organization as compensation for loss or injury.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jury awarded the victim one million dollars in <w>damages</w> for the accident.",
                            "target_lang_translations": {
                                "ru": "Присяжные присудили жертве один миллион долларов в качестве <w>возмещения убытков</w> за несчастный случай."
                            }
                        },
                        {
                            "text": "He decided to sue for <w>damages</w> after the newspaper published false information about him.",
                            "target_lang_translations": {
                                "ru": "Он решил подать иск о <w>компенсации за ущерб</w> после того, как газета опубликовала о нем ложную информацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "compensation",
                        "reparation",
                        "indemnity",
                        "restitution"
                    ],
                    "common_phrases": [
                        "sue for damages",
                        "award damages",
                        "punitive damages",
                        "claim damages",
                        "pay damages"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная сумма, которую суд обязывает выплатить в качестве компенсации за понесенный ущерб или травму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возмещение убытков",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "компенсация за ущерб",
                                "target_lang_sense_clarification": "выплата за причиненный вред"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
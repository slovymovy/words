{
    "word_family": [
        "taxation",
        "taxable",
        "taxpayer",
        "overtax",
        "surtax",
        "untaxed"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33e72b2c-08cc-3aac-a4e3-3506d3fae3fe",
                    "sense_definition": "Money paid to the government based on income, property, or goods to fund public services.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_charge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to increase the <w>tax</w> on cigarettes to discourage smoking.",
                            "target_lang_translations": {
                                "ru": "Правительство решило повысить <w>налог</w> на сигареты, чтобы бороться с курением."
                            }
                        },
                        {
                            "text": "You must file a return every year to calculate how much income <w>tax</w> you owe.",
                            "target_lang_translations": {
                                "ru": "Вы должны подавать декларацию каждый год, чтобы рассчитать, какой подоходный <w>налог</w> вы должны заплатить."
                            }
                        }
                    ],
                    "synonyms": [
                        "levy",
                        "duty",
                        "toll",
                        "impost"
                    ],
                    "antonyms": [
                        "subsidy"
                    ],
                    "common_phrases": [
                        "income tax",
                        "sales tax",
                        "property tax",
                        "before tax",
                        "value-added tax"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги, выплачиваемые правительству на основе доходов, имущества или товаров для финансирования государственных услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налог",
                                "target_lang_sense_clarification": "обязательный государственный платеж"
                            },
                            {
                                "target_lang_word": "пошлина",
                                "target_lang_sense_clarification": "плата за таможню или официальные услуги"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "целевой или разовый взнос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfc3323d-418d-3451-990e-64336e9a27ca",
                    "sense_definition": "A heavy demand or burden placed on someone's resources, time, or energy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "burden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The long working hours were a heavy <w>tax</w> on his health and family life.",
                            "target_lang_translations": {
                                "ru": "Долгий рабочий день стал тяжким <w>бременем</w> для его здоровья и семейной жизни."
                            }
                        },
                        {
                            "text": "Looking after three energetic children is a constant <w>tax</w> on her patience.",
                            "target_lang_translations": {
                                "ru": "Присмотр за тремя энергичными детьми — это постоянное <w>испытание</w> для её терпения."
                            }
                        }
                    ],
                    "synonyms": [
                        "demand",
                        "impost",
                        "exaction",
                        "rate"
                    ],
                    "common_phrases": [
                        "tax on patience",
                        "heavy tax"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелое требование или бремя, возлагаемое на чьи-либо ресурсы, время или энергию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бремя",
                                "target_lang_sense_clarification": "тяжелая ноша или обязанность"
                            },
                            {
                                "target_lang_word": "испытание",
                                "target_lang_sense_clarification": "проверка на прочность (терпения, сил)"
                            },
                            {
                                "target_lang_word": "нагрузка",
                                "target_lang_sense_clarification": "давление на ресурсы или здоровье"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8f6fe6b9-e1f2-33d6-9e3c-4a5f56ff9145",
                    "sense_definition": "To require a person or organization to pay a percentage of their money to the government.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_charge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new law aims to <w>tax</w> large corporations at a higher rate than small businesses.",
                            "target_lang_translations": {
                                "ru": "Новый закон нацелен <w>облагать налогом</w> крупные корпорации по более высокой ставке, чем малый бизнес."
                            }
                        },
                        {
                            "text": "Many citizens believe it is unfair to <w>tax</w> essential food items.",
                            "target_lang_translations": {
                                "ru": "Многие граждане считают, что несправедливо <w>облагать налогом</w> товары первой необходимости."
                            }
                        }
                    ],
                    "synonyms": [
                        "levy",
                        "assessment",
                        "exaction"
                    ],
                    "common_phrases": [
                        "tax heavily",
                        "tax the rich",
                        "tax income"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать от человека или организации выплаты процента от их денег правительству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облагать налогом",
                                "target_lang_sense_clarification": "взимать государственный платеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab257309-37fc-3544-b39c-0be7f193e018",
                    "sense_definition": "To make difficult or excessive demands on someone's strength, patience, or resources.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "burden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The difficult exam questions will really <w>tax</w> your knowledge of the subject.",
                            "target_lang_translations": {
                                "ru": "Сложные вопросы экзамена по-настоящему <w>испытают</w> ваши знания предмета."
                            }
                        },
                        {
                            "text": "Please do not <w>tax</w> my patience with any more silly excuses.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>испытывай</w> моё терпение новыми глупыми оправданиями."
                            }
                        }
                    ],
                    "synonyms": [
                        "demand",
                        "exaction"
                    ],
                    "common_phrases": [
                        "tax my patience",
                        "tax the brain",
                        "tax resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предъявлять трудные или чрезмерные требования к чьим-либо силам, терпению или ресурсам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытывать",
                                "target_lang_sense_clarification": "подвергать проверке (терпение, силы)"
                            },
                            {
                                "target_lang_word": "истощать",
                                "target_lang_sense_clarification": "сильно утомлять или расходовать ресурсы"
                            },
                            {
                                "target_lang_word": "напрягать",
                                "target_lang_sense_clarification": "требовать значительных усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee9a8a11-580b-3d65-a3be-d3fe43083d01",
                    "sense_definition": "To accuse or charge someone with a fault or offense.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "accuse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>taxed</w> with negligence after failing to secure the building properly.",
                            "target_lang_translations": {
                                "ru": "Его <w>обвинили</w> в халатности после того, как он не смог должным образом обезопасить здание."
                            }
                        },
                        {
                            "text": "The senator was <w>taxed</w> with inconsistency regarding his voting record.",
                            "target_lang_translations": {
                                "ru": "Сенатора <w>упрекнули</w> в непоследовательности касательно его истории голосований."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tax with"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically prefers 'accuse' or 'charge'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обвинять кого-либо в проступке или правонарушении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обвинять",
                                "target_lang_sense_clarification": "вменять в вину"
                            },
                            {
                                "target_lang_word": "упрекать",
                                "target_lang_sense_clarification": "высказывать претензии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "describe",
        "descriptive",
        "descriptively",
        "indescribable",
        "nondescript"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6f8abb4-b964-3bcd-8748-3b72eb9e6e60",
                    "sense_definition": "A spoken or written statement that gives details about someone or something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please give a brief <description> of the person you saw.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, дайте краткое <описание> человека, которого вы видели."
                            }
                        },
                        {
                            "text": "The job <description> lists all the responsibilities required for the role.",
                            "target_lang_translations": {
                                "ru": "В <описании> вакансии перечислены все обязанности, необходимые для этой роли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "detailed description",
                        "brief description",
                        "job description"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устное или письменное сообщение, содержащее подробности о ком-либо или чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описание",
                                "target_lang_sense_clarification": "изложение деталей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c62c92e-6ca6-3097-87c8-8eee5e2fc23c",
                    "sense_definition": "The act of giving details about a person, object, or event.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The beauty of the landscape defies <description>.",
                            "target_lang_translations": {
                                "ru": "Красота пейзажа не поддается <описанию>."
                            }
                        },
                        {
                            "text": "His <description> of the accident was very confusing to the police.",
                            "target_lang_translations": {
                                "ru": "Его <описание> происшествия было очень запутанным для полиции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "defies description",
                        "beyond description"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по изложению подробностей о человеке, предмете или событии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описание",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af59a6cd-ac34-3cb4-9f98-1a57ee3a5939",
                    "sense_definition": "A specific sort, kind, or variety of people or things.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There were vehicles of every <description> parked in the lot.",
                            "target_lang_translations": {
                                "ru": "На парковке стояли транспортные средства всех <видов>."
                            }
                        },
                        {
                            "text": "We do not sell alcohol of any <description> in this store.",
                            "target_lang_translations": {
                                "ru": "Мы не продаем алкоголь никакого <вида> в этом магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "sort",
                        "kind",
                        "type",
                        "variety"
                    ],
                    "common_phrases": [
                        "of every description",
                        "of any description"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный сорт, вид или разновидность людей или вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "разновидность"
                            },
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "категория"
                            },
                            {
                                "target_lang_word": "сорт",
                                "target_lang_sense_clarification": "тип"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afad1935-88f2-3742-a8bc-8a182cd797d8",
                    "sense_definition": "A formal scientific record of a specific organism used to introduce it to science.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The botanist published a detailed <description> of the new plant species.",
                            "target_lang_translations": {
                                "ru": "Ботаник опубликовал подробное <описание> нового вида растений."
                            }
                        },
                        {
                            "text": "A valid <description> is required before the fungus can be officially named.",
                            "target_lang_translations": {
                                "ru": "Для официального наименования гриба требуется действительное <описание>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальная научная запись о конкретном организме, используемая для введения его в науку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описание",
                                "target_lang_sense_clarification": "научное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b81d8596-ce3e-385d-817a-54a63a8fdfdd",
                    "sense_definition": "The objective analysis and recording of how language is actually used, rather than how it should be used.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Modern linguistics focuses on <description> rather than prescription.",
                            "target_lang_translations": {
                                "ru": "Современная лингвистика фокусируется на <описании>, а не на предписании."
                            }
                        },
                        {
                            "text": "The <description> of the local dialect revealed many interesting grammatical patterns.",
                            "target_lang_translations": {
                                "ru": "<Описание> местного диалекта выявило много интересных грамматических закономерностей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объективный анализ и фиксация того, как язык используется на самом деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описание",
                                "target_lang_sense_clarification": "лингвистическое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
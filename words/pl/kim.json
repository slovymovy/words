            "senses": [
                {
                    "sense_id": "6218308d-4af0-3c7b-bc2b-b74a645ff964",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>kima</w> (oznaczającego drzemkę).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po serii krótkich <w>kim</w> w autobusie czuł się jeszcze bardziej zmęczony.",
                            "target_lang_translations": {
                                "ru": "После серии коротких <w>дремот</w> в автобусе он чувствовал себя еще более уставшим."
                            }
                        },
                        {
                            "text": "Nie lubię takich przerywanych <w>kim</w>, wolę przespać całą noc.",
                            "target_lang_translations": {
                                "ru": "Я не люблю таких прерывистых <w>дремот</w>, предпочитаю проспать всю ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "drzemek"
                    ],
                    "traits": [

        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ac815ad-a7ef-3cf2-aade-6643b3af5c0d",
                    "sense_definition": "Streng afkeuren of kritiek uiten op iemands gedrag of acties",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De commissie <w>laakte</w> het onprofessionele gedrag van de directeur.",
                            "target_lang_translations": {
                                "en": "The committee <w>censured</w> the director's unprofessional behavior.",
                                "ru": "Комиссия <w>осудила</w> непрофессиональное поведение директора."
                            }
                        },
                        {
                            "text": "Het is niet netjes om de inzet van anderen publiekelijk te <w>laken</w>.",
                            "target_lang_translations": {
                                "en": "It is not polite to publicly <w>criticize</w> the efforts of others.",
                                "ru": "Некрасиво публично <w>порицать</w> усилия других."

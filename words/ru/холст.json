    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50510b47-a94e-3bbc-b49a-5331e6717a34",
                    "sense_definition": "Грубая льняная или конопляная ткань полотняного переплетения, используемая для одежды, мешков или как основа для живописи.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fabric_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник тщательно натянул на деревянный подрамник чистый <холст>.",
                            "target_lang_translations": {
                                "en": "The artist carefully stretched a clean <canvas> onto the wooden frame."
                            }
                        },
                        {
                            "text": "В старину крестьяне шили простую одежду из домотканого <холста>.",
                            "target_lang_translations": {
                                "en": "In the old days, peasants sewed simple clothes from homespun <canvas>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ткань",
                        "полотно",
                        "мешковина"
                    ],
                    "common_phrases": [
                        "грубый холст",
                        "льняной холст",
                        "домотканый холст"
                    ],
                    "target_lang_definitions": {
                        "en": "Coarse linen or hemp fabric used for clothing, bags, or as a base for painting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "canvas",
                                "target_lang_sense_clarification": "fabric"
                            },
                            {
                                "target_lang_word": "coarse cloth",
                                "target_lang_sense_clarification": "material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a24cca1-ee3a-39b7-ac0c-4589eaa2e4d6",
                    "sense_definition": "Картина, написанная масляными красками на специальной ткани.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fabric_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее мы долго рассматривали знаменитые <холсты> русских пейзажистов.",
                            "target_lang_translations": {
                                "en": "In the museum, we spent a long time examining the famous <canvases> of Russian landscape painters."
                            }
                        },
                        {
                            "text": "Этот старинный <холст> требует бережного хранения и реставрации.",
                            "target_lang_translations": {
                                "en": "This antique <canvas> requires careful storage and restoration."
                            }
                        }
                    ],
                    "synonyms": [
                        "картина",
                        "полотно"
                    ],
                    "common_phrases": [
                        "холст масло",
                        "грунтованный холст"
                    ],
                    "target_lang_definitions": {
                        "en": "A painting created with oil paints on a fabric base."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "canvas",
                                "target_lang_sense_clarification": "art piece"
                            },
                            {
                                "target_lang_word": "painting",
                                "target_lang_sense_clarification": "artwork"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb24d638-2982-39f2-8baa-f48ff87740bc",
                    "sense_definition": "Слой волокнистого материала, получаемый при обработке на трепальных или чесальных машинах.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прядильная машина формирует из хлопка равномерный и плотный <холст>.",
                            "target_lang_translations": {
                                "en": "The spinning machine forms a uniform and dense <lap> from the cotton."
                            }
                        },
                        {
                            "text": "Технолог проверил качество, с которым <холст> сходил с конвейера.",
                            "target_lang_translations": {
                                "en": "The technologist checked the quality with which the <lap> came off the conveyor."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A layer of fibrous material obtained during processing on scutching or carding machines."
                    },
                    "translations": {

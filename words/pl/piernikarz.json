                        "piernik"
                    ],
                    "common_phrases": [
                        "stary piernikarz"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej, często o zabarwieniu lekceważącym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное выражение для обозначения пожилого, медлительного, неуклюжего или дряхлого человека."
                    },

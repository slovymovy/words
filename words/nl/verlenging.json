    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c658358b-f815-3f10-90cc-39dd4bef3f08",
                    "sense_definition": "Het langer maken van iets in tijd of ruimte, of de periode waarmee iets langer duurt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "act_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na onderhandelingen komt er geen <w>verlenging</w> van de staking.",
                            "target_lang_translations": {
                                "en": "After negotiations, there will be no <w>extension</w> of the strike.",
                                "ru": "После переговоров <w>продления</w> забастовки не будет."
                            }
                        },
                        {
                            "text": "We hebben een <w>verlenging</w> van de deadline aangevraagd.",
                            "target_lang_translations": {
                                "en": "We have requested an <w>extension</w> of the deadline.",
                                "ru": "Мы запросили <w>продление</w> крайнего срока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verlenging van het contract",
                        "verlenging van de termijn"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of making something longer in time or space, or the period by which something lasts longer.",
                        "ru": "Действие по увеличению чего-либо во времени или пространстве, или период, на который что-то продлевается."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extension",
                                "target_lang_sense_clarification": "general act of extending time or space"
                            },
                            {
                                "target_lang_word": "prolongation",
                                "target_lang_sense_clarification": "act of lengthening duration"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продление",
                                "target_lang_sense_clarification": "увеличение срока действия или времени"
                            },
                            {
                                "target_lang_word": "удлинение",
                                "target_lang_sense_clarification": "увеличение физической длины или протяженности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d838df8-bb91-3ef6-9da4-38003afc4901",
                    "sense_definition": "Extra speeltijd die aan een wedstrijd wordt toegevoegd om een winnaar te bepalen bij een gelijke stand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de <w>verlenging</w> scoorde de thuisploeg het winnende doelpunt.",
                            "target_lang_translations": {
                                "en": "During <w>extra time</w>, the home team scored the winning goal.",
                                "ru": "В <w>дополнительное время</w> хозяева поля забили победный гол."
                            }
                        },
                        {
                            "text": "De wedstrijd eindigde in een gelijkspel, dus er volgde een <w>verlenging</w>.",
                            "target_lang_translations": {
                                "en": "The match ended in a draw, so <w>extra time</w> followed.",
                                "ru": "Матч закончился вничью, поэтому последовало <w>дополнительное время</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de verlenging",
                        "na verlenging"
                    ],
                    "target_lang_definitions": {
                        "en": "Extra playing time added to a match to determine a winner in case of a tie.",
                        "ru": "Дополнительное игровое время, добавляемое к матчу для определения победителя при ничейном счете."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extra time",
                                "target_lang_sense_clarification": "sports (UK/International)"
                            },
                            {
                                "target_lang_word": "overtime",
                                "target_lang_sense_clarification": "sports (US)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дополнительное время",
                                "target_lang_sense_clarification": "стандартный спортивный термин"
                            },
                            {
                                "target_lang_word": "овертайм",
                                "target_lang_sense_clarification": "в некоторых видах спорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "132b4e92-fa1b-3959-aced-875c76e3f740",
                    "sense_definition": "Een voorwerp of stuk dat aan iets is vastgemaakt om het langer te maken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verlenging</w> van de trein werd voorzichtig losgekoppeld.",
                            "target_lang_translations": {
                                "en": "The <w>extension</w> of the train was carefully uncoupled.",
                                "ru": "<w>Дополнительная секция</w> поезда была осторожно отцеплена."
                            }
                        },
                        {
                            "text": "Gebruik een <w>verlenging</w> als het snoer niet lang genoeg is.",
                            "target_lang_translations": {
                                "en": "Use an <w>extension</w> if the cord is not long enough.",
                                "ru": "Используйте <w>удлинитель</w>, если шнур недостаточно длинный."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlengstuk"
                    ],
                    "common_phrases": [
                        "verlenging van de kabel"
                    ],
                    "target_lang_definitions": {
                        "en": "An object or piece attached to something to make it longer.",
                        "ru": "Предмет или деталь, прикрепленная к чему-либо для увеличения его длины."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extension",
                                "target_lang_sense_clarification": "physical object or part"
                            },
                            {
                                "target_lang_word": "extension piece",
                                "target_lang_sense_clarification": "specific component"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "удлинитель",
                                "target_lang_sense_clarification": "для проводов или кабелей"
                            },
                            {
                                "target_lang_word": "наставка",
                                "target_lang_sense_clarification": "деталь для удлинения конструкции"
                            }
                        ]
                    }
                }
            ]
        }
    ]

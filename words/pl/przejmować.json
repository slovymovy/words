    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "670e57bc-b1ce-365e-b482-7d9e8c1169fa",
                    "sense_definition": "Zaczynać sprawować nad czymś kontrolę, władzę lub opiekę, często zastępując kogoś innego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy dyrektor oficjalnie <w>przejmuje</w> obowiązki w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Новый директор официально <w>принимает</w> обязанности на следующей неделе."
                            }
                        },
                        {
                            "text": "Wojsko postanowiło <w>przejąć</w> kontrolę nad zniszczonym mostem.",
                            "target_lang_translations": {
                                "ru": "Войска решили <w>взять</w> под контроль разрушенный мост."
                            }
                        }
                    ],
                    "synonyms": [
                        "brać",
                        "odbierać",
                        "zabierać"
                    ],
                    "antonyms": [
                        "oddawać",
                        "zrzekać się"
                    ],
                    "common_phrases": [
                        "przejmować władzę",
                        "przejmować kontrolę",
                        "przejmować obowiązki",
                        "przejmować inicjatywę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать осуществлять контроль, власть или опеку над чем-либо, часто заменяя кого-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "обязанности, командование"
                            },
                            {
                                "target_lang_word": "брать на себя",
                                "target_lang_sense_clarification": "ответственность, инициативу"
                            },
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "власть, контроль (силой)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c866ea06-5269-3be3-b599-dfd4ef30aeba",
                    "sense_definition": "Zatrzymywać lub chwytać coś, co jest w ruchu lub jest przesyłane do kogoś innego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intercept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz zdołał <w>przejąć</w> piłkę w ostatniej chwili.",
                            "target_lang_translations": {
                                "ru": "Вратарю удалось <w>перехватить</w> мяч в последний момент."
                            }
                        },
                        {
                            "text": "Wywiad wojskowy regularnie <w>przejmował</w> tajne wiadomości wroga.",
                            "target_lang_translations": {
                                "ru": "Военная разведка регулярно <w>перехватывала</w> секретные сообщения врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "przechwytywać"
                    ],
                    "common_phrases": [
                        "przejmować piłkę",
                        "przejmować sygnał",
                        "przejmować podanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Останавливать или ловить что-то, что находится в движении или передается кому-то другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перехватывать",
                                "target_lang_sense_clarification": "мяч, сообщение, сигнал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8286d07-c0ab-3eef-bf39-c038147fcfa0",
                    "sense_definition": "Przyswajać sobie czyjeś cechy, zachowania, zwyczaje lub poglądy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adopt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci bardzo szybko <w>przejmują</w> gesty i słowa od swoich rodziców.",
                            "target_lang_translations": {
                                "ru": "Дети очень быстро <w>перенимают</w> жесты и слова от своих родителей."
                            }
                        },
                        {
                            "text": "Młodzi pracownicy często <w>przejmują</w> złe nawyki od starszych kolegów.",
                            "target_lang_translations": {
                                "ru": "Молодые сотрудники часто <w>перенимают</w> плохие привычки у старших коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyswajać",
                        "wzorować się",
                        "zapożyczać",
                        "asymilować"
                    ],
                    "common_phrases": [
                        "przejmować zwyczaje",
                        "przejmować wzorce",
                        "przejmować styl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Усваивать чьи-либо черты, поведение, привычки или взгляды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перенимать",
                                "target_lang_sense_clarification": "привычки, опыт, манеры"
                            },
                            {
                                "target_lang_word": "усваивать",
                                "target_lang_sense_clarification": "взгляды, поведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e7b6329-2bb3-3e87-bcc8-77b307e083d9",
                    "sense_definition": "O uczuciach, strachu lub zimnie: silnie na kogoś oddziaływać, ogarniać kogoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "feelings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy weszła do ciemnego lasu, <w>przejmował</w> ją coraz większy lęk.",
                            "target_lang_translations": {
                                "ru": "Когда она вошла в темный лес, её <w>охватывал</w> всё больший страх."
                            }
                        },
                        {
                            "text": "Na widok tego strasznego wypadku wszystkich świadków <w>przejęła</w> zgroza.",
                            "target_lang_translations": {
                                "ru": "При виде этой страшной аварии всех свидетелей <w>охватил</w> ужас."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogarniać",
                        "opanowywać",
                        "przenikać"
                    ],
                    "common_phrases": [
                        "strach przejmuje",
                        "dreszcz przejmuje",
                        "zgroza przejmuje"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often literary or expressive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О чувствах, страхе или холоде: сильно воздействовать на кого-то, охватывать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "о страхе, ужасе"
                            },
                            {
                                "target_lang_word": "пробирать",
                                "target_lang_sense_clarification": "о холоде, дрожи"
                            },
                            {
                                "target_lang_word": "овладевать",
                                "target_lang_sense_clarification": "о чувствах"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "8aaf5354-945b-3aa4-a187-4f993ea4deaa",
                    "sense_definition": "Having finished an activity or task.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adj_finished",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>done</w> with my breakfast, so we can leave now.",
                            "target_lang_translations": {
                                "ru": "Я <w>закончил</w> завтракать, так что мы можем идти."
                            }
                        },
                        {
                            "text": "Are you <w>done</w> using the computer?",
                            "target_lang_translations": {
                                "ru": "Ты <w>закончил</w> пользоваться компьютером?"
                            }

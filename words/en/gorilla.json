    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0d4a25cc-455e-3cfa-bb32-6332aac0f6bc",
                    "sense_definition": "A large, powerful ape native to the forests of central Africa, known for walking on its knuckles.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The silverback <w>gorilla</w> beat his chest to warn the intruders.",
                            "target_lang_translations": {
                                "ru": "Самец <w>гориллы</w> с серебристой спиной бил себя в грудь, чтобы предупредить чужаков."
                            }
                        },
                        {
                            "text": "We saw a mother <w>gorilla</w> carrying her baby in the zoo enclosure.",
                            "target_lang_translations": {
                                "ru": "Мы видели маму-<w>гориллу</w>, несущую своего детёныша в вольере зоопарка."
                            }
                        }
                    ],
                    "synonyms": [
                        "ape",
                        "great ape"
                    ],
                    "common_phrases": [
                        "mountain gorilla",
                        "silverback gorilla",
                        "western lowland gorilla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная, сильная человекообразная обезьяна, обитающая в лесах Центральной Африки, известная тем, что ходит, опираясь на костяшки пальцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горилла",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6be2c477-f5b7-306a-807a-bea33ebf6da9",
                    "sense_definition": "A large, aggressive, and physically strong man, often acting as a guard or thug.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mob boss sent a <w>gorilla</w> to collect the money from the shopkeeper.",
                            "target_lang_translations": {
                                "ru": "Босс мафии послал <w>громилу</w>, чтобы забрать деньги у лавочника."
                            }
                        },
                        {
                            "text": "Don't mess with that <w>gorilla</w> standing by the door; he looks dangerous.",
                            "target_lang_translations": {
                                "ru": "Не связывайся с тем <w>амбалом</w>, что стоит у двери; он выглядит опасным."
                            }
                        }
                    ],
                    "synonyms": [
                        "thug",
                        "goon",
                        "brute",
                        "ruffian"
                    ],
                    "common_phrases": [
                        "hired gorilla",
                        "big gorilla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный, агрессивный и физически сильный мужчина, часто выполняющий роль охранника или бандита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "громила",
                                "target_lang_sense_clarification": "сильный, опасный человек"
                            },
                            {
                                "target_lang_word": "амбал",
                                "target_lang_sense_clarification": "разг. крупный мужчина"
                            },
                            {
                                "target_lang_word": "горилла",
                                "target_lang_sense_clarification": "перен. грубый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1fc8070-4b18-3023-a05b-a2fd291ab22e",
                    "sense_definition": "A very powerful and dominant person or organization that controls a particular market or field.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software company became the <w>gorilla</w> of the tech industry.",
                            "target_lang_translations": {
                                "ru": "Эта софтверная компания стала <w>гигантом</w> технологической индустрии."
                            }
                        },
                        {
                            "text": "Small businesses often struggle to compete against the 800-pound <w>gorilla</w> in the market.",
                            "target_lang_translations": {
                                "ru": "Малому бизнесу часто трудно конкурировать с огромным <w>монстром</w> на рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "giant",
                        "heavyweight",
                        "behemoth",
                        "titan"
                    ],
                    "antonyms": [
                        "minnow",
                        "underdog"
                    ],
                    "common_phrases": [
                        "800-pound gorilla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень влиятельная и доминирующая личность или организация, контролирующая определенный рынок или сферу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "доминирующая организация"
                            },
                            {
                                "target_lang_word": "монстр",
                                "target_lang_sense_clarification": "перен. влиятельная компания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be4a6f6f-87fc-34d5-a1ac-b41a8ff4d666",
                    "sense_definition": "One thousand pounds or dollars.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He owes me a <w>gorilla</w> for the work I did last month.",
                            "target_lang_translations": {
                                "ru": "Он должен мне <w>штуку</w> за работу, которую я сделал в прошлом месяце."
                            }
                        },
                        {
                            "text": "I am not going to bet a whole <w>gorilla</w> on a single horse race.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь ставить целую <w>штуку</w> на один заезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "grand",
                        "large"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "informal term for money"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тысяча фунтов или долларов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штука",
                                "target_lang_sense_clarification": "сленг: тысяча"
                            },
                            {
                                "target_lang_word": "косарь",
                                "target_lang_sense_clarification": "сленг: тысяча"
                            }
                        ]
                    }
                }
            ]
        }

                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Róg</w> jest naszym nowym sąsiadem z parteru.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Руг</w> — наш новый сосед с первого этажа."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Róg</w> w tej firmie.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Руг</w> в этой фирме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Руг",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

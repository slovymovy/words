{
    "word_family": [
        "szkło",
        "szklany",
        "szklić",
        "szklarnia",
        "szklarz",
        "szklaneczka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34b55de4-80be-3700-b68a-116e54badf7a",
                    "sense_definition": "Szklane naczynie bez ucha, zwykle o pojemności około 250 ml, służące do picia napojów",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naczynie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole w kuchni stoi pusta <w>szklanka</w> po soku.",
                            "target_lang_translations": {
                                "ru": "На кухонном столе стоит пустой <w>стакан</w> из-под сока."
                            }
                        },
                        {
                            "text": "Poproszę o czystą <w>szklankę</w>, bo chcę napić się wody.",
                            "target_lang_translations": {
                                "ru": "Можно мне чистый <w>стакан</w>, я хочу выпить воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szklanka wody",
                        "szklanka do herbaty",
                        "pusta szklanka",
                        "rozbita szklanka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стеклянный сосуд без ручки, обычно объемом около 250 мл, предназначенный для питья"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стакан",
                                "target_lang_sense_clarification": "сосуд для питья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fed980ef-2b00-3a06-a552-15a62ae9ce1a",
                    "sense_definition": "Ilość płynu lub sypkiego produktu, która mieści się w szklance; zwyczajowa miara objętości w przepisach kulinarnych",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "miara",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do upieczenia tego ciasta potrzebna jest jedna <w>szklanka</w> cukru.",
                            "target_lang_translations": {
                                "ru": "Для выпечки этого пирога нужен один <w>стакан</w> сахара."
                            }
                        },
                        {
                            "text": "Codziennie rano dla zdrowia wypijam <w>szklankę</w> ciepłej wody z cytryną.",
                            "target_lang_translations": {
                                "ru": "Каждое утро для здоровья я выпиваю <w>стакан</w> теплой воды с лимоном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szklanka mąki",
                        "szklanka mleka",
                        "szklanka cukru",
                        "pół szklanki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество жидкости или сыпучего продукта, вмещающееся в стакан; традиционная мера объема в кулинарных рецептах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стакан",
                                "target_lang_sense_clarification": "мера объема"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c617cb3-6bcb-32b4-b89a-899d46a5969f",
                    "sense_definition": "Warstwa gładkiego, bardzo śliskiego lodu pokrywająca nawierzchnię drogi lub chodnika",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pogoda",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowcy muszą uważać, ponieważ rano na drogach zrobiła się <w>szklanka</w>.",
                            "target_lang_translations": {
                                "ru": "Водители должны быть осторожны, так как утром на дорогах образовался <w>гололёд</w>."
                            }
                        },
                        {
                            "text": "Nie wychodź z domu bez dobrych butów, bo na chodnikach jest straszna <w>szklanka</w>.",
                            "target_lang_translations": {
                                "ru": "Не выходи из дома без хорошей обуви, потому что на тротуарах страшный <w>гололёд</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gołoledź",
                        "ślizgawica",
                        "czarny lód",
                        "ślizgawka"
                    ],
                    "common_phrases": [
                        "szklanka na drodze",
                        "zrobiła się szklanka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potoczne określenie gołoledzi"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой гладкого, очень скользкого льда, покрывающий поверхность дороги или тротуара"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гололёд",
                                "target_lang_sense_clarification": "погодное явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a01fc194-b8d3-310e-a704-9428f22b9de4",
                    "sense_definition": "Migający sygnalizator świetlny umieszczany na dachu pojazdu uprzywilejowanego, np. radiowozu",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sygnalizator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z daleka widzieliśmy niebieską <w>szklankę</w> karetki pogotowia jadącej na sygnale.",
                            "target_lang_translations": {
                                "ru": "Издалека мы видели синюю <w>мигалку</w> скорой помощи, едущей с сиреной."
                            }
                        },
                        {
                            "text": "Policyjny radiowóz włączył <w>szklankę</w> i ruszył w pościg za piratem drogowym.",
                            "target_lang_translations": {
                                "ru": "Полицейская машина включила <w>мигалку</w> и бросилась в погоню за лихачом."
                            }
                        }
                    ],
                    "synonyms": [
                        "kogut"
                    ],
                    "common_phrases": [
                        "niebieska szklanka"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "środowiskowe lub potoczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мигающий световой сигнал, устанавливаемый на крыше спецтранспорта, например, полицейской машины"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мигалка",
                                "target_lang_sense_clarification": "проблесковый маячок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e859368a-c7a6-3457-828b-ae5136c330a0",
                    "sense_definition": "Odmiana wiśni o jasnoczerwonych owocach i bezbarwnym soku",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botanika",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym sadzie mojego dziadka rośnie rozłożysta <w>szklanka</w>, która co roku obficie owocuje.",
                            "target_lang_translations": {
                                "ru": "В старом саду моего дедушки растет раскидистая <w>аморель</w>, которая каждый год обильно плодоносит."
                            }
                        },
                        {
                            "text": "Owoce <w>szklanki</w> są delikatne w smaku i świetnie nadają się na kompoty.",
                            "target_lang_translations": {
                                "ru": "Плоды <w>аморели</w> имеют нежный вкус и отлично подходят для компотов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сорт вишни со светло-красными плодами и бесцветным соком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аморель",
                                "target_lang_sense_clarification": "сорт вишни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b11b0be-d8ff-3171-bbb3-ebbf1ccfee8e",
                    "sense_definition": "W żeglarstwie: uderzenie w dzwon okrętowy wybijające czas wachty (jedno uderzenie co pół godziny)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "żeglarstwo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wybito cztery <w>szklanki</w>, co dla załogi oznaczało koniec wachty.",
                            "target_lang_translations": {
                                "ru": "Пробили четыре <w>склянки</w>, что для команды означало конец вахты."
                            }
                        },
                        {
                            "text": "Dźwięk ostatniej <w>szklanki</w> rozniósł się echem po pokładzie statku.",
                            "target_lang_translations": {
                                "ru": "Звук последней <w>склянки</w> эхом разнесся по палубе корабля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wybić szklanki"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "terminologia żeglarska, tradycyjna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В мореплавании: удар в корабельный колокол, отбивающий время вахты (один удар каждые полчаса)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склянка",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
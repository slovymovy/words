{
    "word_family": [
        "ons"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "5e578cde-aa2e-362d-8063-f6e75c3bf696",
                    "sense_definition": "De verbogen vorm van <w>ons</w> die bezit of relatie aanduidt; gebruikt vóór de-woorden in het enkelvoud en vóór alle zelfstandige naamwoorden in het meervoud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waar hebben we <w>onze</w> fietsen neergezet?",
                            "target_lang_translations": {
                                "en": "Where did we put <w>our</w> bikes?",
                                "ru": "Куда мы поставили <w>наши</w> велосипеды?"
                            }
                        },
                        {
                            "text": "Dit is <w>onze</w> oudste dochter die in Amsterdam woont.",
                            "target_lang_translations": {
                                "en": "This is <w>our</w> eldest daughter who lives in Amsterdam.",
                                "ru": "Это <w>наша</w> старшая дочь, которая живет в Амстердаме."
                            }
                        },
                        {
                            "text": "We moeten goed voor <w>onze</w> aarde zorgen.",
                            "target_lang_translations": {
                                "en": "We must take good care of <w>our</w> earth.",
                                "ru": "Мы должны хорошо заботиться о <w>нашей</w> Земле."
                            }
                        }
                    ],
                    "antonyms": [
                        "jullie",
                        "hun",
                        "uw"
                    ],
                    "common_phrases": [
                        "onze vader",
                        "onze taal",
                        "onze mensen",
                        "onze lieve heer"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "attributief"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The inflected form of <w>ons</w> indicating possession or relationship; used before 'de'-words in the singular and before all nouns in the plural.",
                        "ru": "Склоняемая форма <w>ons</w>, указывающая на принадлежность или отношение; используется перед существительными рода 'de' в единственном числе и перед всеми существительными во множественном числе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "our",
                                "target_lang_sense_clarification": "possessive determiner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "наша",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "наши",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "37f99182-298f-32b6-85bf-ef573ad10997",
                    "sense_definition": "Dat wat van <w>ons</w> is; zelfstandig gebruikt zonder dat er een zelfstandig naamwoord op volgt (meestal voorafgegaan door 'de' of 'het').",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is dit jullie auto of is het de <w>onze</w>?",
                            "target_lang_translations": {
                                "en": "Is this your car or is it <w>ours</w>?",
                                "ru": "Это ваша машина или <w>наша</w>?"
                            }
                        },
                        {
                            "text": "Hun huis is veel groter dan het <w>onze</w>.",
                            "target_lang_translations": {
                                "en": "Their house is much bigger than <w>ours</w>.",
                                "ru": "Их дом намного больше, чем <w>наш</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "de uwe",
                        "de jouwe",
                        "de hunne"
                    ],
                    "common_phrases": [
                        "het onze",
                        "de onze"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zelfstandig"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "That which belongs to <w>us</w>; used independently without a following noun (usually preceded by 'de' or 'het').",
                        "ru": "То, что принадлежит <w>нам</w>; используется самостоятельно, без последующего существительного (обычно с артиклем 'de' или 'het')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ours",
                                "target_lang_sense_clarification": "possessive pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "самостоятельное употребление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            },
                            {
                                "target_lang_word": "пиарить",
                                "target_lang_sense_clarification": "заниматься пиаром, рекламировать"
                            },
                            {
                                "target_lang_word": "хайпить",
                                "target_lang_sense_clarification": "раздувать шумиху (сленг)"
                            }
                        ]

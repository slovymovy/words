                {
                    "sense_id": "1bee6b61-067a-354b-a20d-1ff9581bda4a",
                    "sense_definition": "Werkend zonder directe menselijke besturing of hulp; zelfwerkend.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mechanical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>automatische</w> deuren gingen open toen we aan kwamen lopen.",
                            "target_lang_translations": {

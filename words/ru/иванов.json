    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "89cfa75d-eccc-3b0a-a981-c1f9597ec9b1",
                    "sense_definition": "Одна из самых распространённых русских фамилий, образованная от имени Иван (ударение на последнем слоге).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "surname_stress_end",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Господин <w>Иванов</w> ожидает вас в приёмной.",
                            "target_lang_translations": {
                                "en": "Mr. <w>Ivanov</w> is waiting for you in the reception area."
                            }
                        },
                        {
                            "text": "В нашем классе учится мальчик по фамилии <w>Иванов</w>.",
                            "target_lang_translations": {
                                "en": "A boy with the surname <w>Ivanov</w> studies in our class."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Иванов, Петров, Сидоров"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "One of the most common Russian surnames, derived from the name Ivan (stress on the last syllable)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Ivanov",
                                "target_lang_sense_clarification": "common Russian surname"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6218ce05-176a-3226-bc2f-b45c0bef404c",
                    "sense_definition": "Вариант фамилии с ударением на втором слоге (часто встречается у болгар или как фамилия известных деятелей культуры).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname_stress_middle",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Знаменитый художник Александр <w>Иванов</w> написал картину «Явление Христа народу».",
                            "target_lang_translations": {
                                "en": "The famous artist Alexander <w>Ivanov</w> painted \"The Appearance of Christ to the People\"."
                            }
                        },
                        {
                            "text": "Моего болгарского коллегу зовут Стоян <w>Иванов</w>.",
                            "target_lang_translations": {
                                "en": "My Bulgarian colleague is named Stoyan <w>Ivanov</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A variant of the surname with stress on the second syllable (often found among Bulgarians or as the surname of famous cultural figures)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Ivanov",
                                "target_lang_sense_clarification": "surname variant"
                            }
                        ]
                    }
                }
            ]
        },

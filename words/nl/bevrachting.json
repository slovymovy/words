    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a7c8181-aaed-328c-bdb4-13c2e04ea782",
                    "sense_definition": "Het proces van het laden van goederen in een vervoermiddel, meestal een schip of vliegtuig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logistics_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bevrachting</w> van het grote containerschip nam de hele dag in beslag.",
                            "target_lang_translations": {
                                "en": "The <w>loading</w> of the large container ship took the entire day.",
                                "ru": "<w>Погрузка</w> большого контейнеровоза заняла весь день."
                            }
                        },
                        {
                            "text": "Tijdens de <w>bevrachting</w> moeten de havenarbeiders strikte veiligheidsregels volgen.",
                            "target_lang_translations": {
                                "en": "During <w>loading</w>, the dock workers must follow strict safety rules.",
                                "ru": "Во время <w>погрузки</w> докеры должны соблюдать строгие правила безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "belading",
                        "lading"
                    ],
                    "antonyms": [
                        "lossing"
                    ],
                    "common_phrases": [
                        "bevrachting van schepen",
                        "tijdens de bevrachting"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of loading goods onto a means of transport, usually a ship or aircraft.",
                        "ru": "Процесс погрузки товаров на транспортное средство, обычно на судно или самолет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loading",
                                "target_lang_sense_clarification": "act of placing cargo on board"
                            },
                            {
                                "target_lang_word": "freighting",
                                "target_lang_sense_clarification": "act of loading goods"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "погрузка",
                                "target_lang_sense_clarification": "процесс размещения груза на борту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d15dfff1-0d42-35e0-b509-52c23a9bc20f",
                    "sense_definition": "Een contract of overeenkomst voor het vervoer van lading per schip of vliegtuig tegen een vastgestelde prijs.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "logistics_contract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rederij en de exporteur hebben gisteren de <w>bevrachting</w> ondertekend.",
                            "target_lang_translations": {
                                "en": "The shipping company and the exporter signed the <w>charter agreement</w> yesterday.",
                                "ru": "Судоходная компания и экспортер вчера подписали <w>договор фрахтования</w>."
                            }
                        },
                        {
                            "text": "In de akte van <w>bevrachting</w> staan alle voorwaarden voor het transport duidelijk omschreven.",
                            "target_lang_translations": {
                                "en": "The deed of <w>affreightment</w> clearly outlines all conditions for the transport.",
                                "ru": "В договоре <w>фрахтования</w> четко описаны все условия перевозки."
                            }
                        }
                    ],
                    "synonyms": [
                        "charter",
                        "vrachtcontract",
                        "vrachtovereenkomst"
                    ],
                    "common_phrases": [
                        "akte van bevrachting",
                        "overeenkomst van bevrachting"
                    ],
                    "target_lang_definitions": {
                        "en": "A contract or agreement for the transport of cargo by ship or aircraft at a set price.",
                        "ru": "Контракт или соглашение о перевозке груза судном или самолетом за установленную плату."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chartering",
                                "target_lang_sense_clarification": "hiring of a vessel"
                            },
                            {
                                "target_lang_word": "affreightment",
                                "target_lang_sense_clarification": "legal contract for carriage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "фрахтование",
                                "target_lang_sense_clarification": "наем судна для перевозки груза"
                            },
                            {
                                "target_lang_word": "чартер",
                                "target_lang_sense_clarification": "договор аренды транспортного средства"
                            }
                        ]
                    }
                }
            ]
        }
    ]

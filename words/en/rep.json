                },
                {
                    "sense_id": "705ad085-4dda-3935-938f-cf5ac07b99df",
                    "sense_definition": "The portrayal of a group or type of person in media; a short form of <w>representation</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie was praised for its positive <w>rep</w> of minority groups.",
                            "target_lang_translations": {
                                "ru": "Фильм хвалили за позитивную <w>репрезентацию</w> меньшинств."
                            }
                        },
                        {
                            "text": "Fans are asking for better queer <w>rep</w> in the new series.",
                            "target_lang_translations": {

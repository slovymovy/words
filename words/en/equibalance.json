                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уравновешивать",
                                "target_lang_sense_clarification": "делать равным по весу или силе"
                            },
                            {
                                "target_lang_word": "балансировать",
                                "target_lang_sense_clarification": "сохранять или создавать равновесие"
                            }
                        ]

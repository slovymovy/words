{
    "word_family": [
        "subscribe",
        "subscription",
        "subscriber"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a1f2df3b-7add-3c30-afae-f2c28c530e6f",
                    "sense_definition": "Not based on or pertaining to a subscription.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company generates significant income through <w>nonsubscription</w> sales of individual products.",
                            "target_lang_translations": {
                                "ru": "Компания получает значительный доход от продаж отдельных товаров <w>не по подписке</w>."
                            }
                        },
                        {
                            "text": "They decided to launch a <w>nonsubscription</w> newsletter that is accessible to everyone for free.",
                            "target_lang_translations": {
                                "ru": "Они решили запустить новостную рассылку <w>без подписки</w>, которая доступна всем бесплатно."
                            }
                        }
                    ],
                    "antonyms": [
                        "subscription-based"
                    ],
                    "common_phrases": [
                        "nonsubscription revenue",
                        "nonsubscription basis",
                        "nonsubscription service"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не основанный на подписке или не относящийся к ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не по подписке",
                                "target_lang_sense_clarification": "о продажах или услугах"
                            },
                            {
                                "target_lang_word": "без подписки",
                                "target_lang_sense_clarification": "не требующий оформления подписки"
                            },
                            {
                                "target_lang_word": "разовый",
                                "target_lang_sense_clarification": "единовременный, не периодический"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f2aa69b-ca8f-3477-9f24-1159dd907058",
                    "sense_definition": "Failure to subscribe to something, such as a doctrine or decision.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>nonsubscription</w> to the party's core ideology eventually led to his resignation.",
                            "target_lang_translations": {
                                "ru": "Его <w>несогласие</w> с основной идеологией партии в конечном итоге привело к его отставке."
                            }
                        },
                        {
                            "text": "The committee noted her <w>nonsubscription</w> to the proposed changes during the meeting.",
                            "target_lang_translations": {
                                "ru": "Комитет отметил ее <w>непринятие</w> предложенных изменений во время заседания."
                            }
                        }
                    ],
                    "synonyms": [
                        "nonadherence",
                        "dissent"
                    ],
                    "antonyms": [
                        "subscription",
                        "adherence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказ принять или поддержать что-либо (например, доктрину или решение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несогласие",
                                "target_lang_sense_clarification": "с мнением или доктриной"
                            },
                            {
                                "target_lang_word": "непринятие",
                                "target_lang_sense_clarification": "отказ поддержать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
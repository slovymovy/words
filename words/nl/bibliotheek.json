    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3834b0e3-ca79-3984-9d81-024dddc9a64d",
                    "sense_definition": "Een instelling die boeken en andere media aan het publiek uitleent of ter inzage biedt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga elke woensdag naar de <w>bibliotheek</w> om nieuwe boeken te halen.",
                            "target_lang_translations": {
                                "ru": "Я хожу в <w>библиотеку</w> каждую среду, чтобы взять новые книги."
                            }
                        },
                        {
                            "text": "In de <w>bibliotheek</w> kun je rustig studeren en de krant lezen.",
                            "target_lang_translations": {
                                "ru": "В <w>библиотеке</w> можно спокойно учиться и читать газету."
                            }
                        }
                    ],
                    "synonyms": [
                        "bieb"
                    ],
                    "common_phrases": [
                        "naar de bibliotheek gaan",
                        "lid worden van de bibliotheek",
                        "boeken lenen bij de bibliotheek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение, которое выдает книги и другие материалы населению или предоставляет их для ознакомления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "библиотека",
                                "target_lang_sense_clarification": "общественное учреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aad414a6-15c4-3078-80a8-b6375a56cb15",
                    "sense_definition": "Een plaats of kamer waar een verzameling boeken bewaard wordt, vaak in een groot huis of gebouw.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude landhuis heeft een prachtige <w>bibliotheek</w> vol antieke boeken.",
                            "target_lang_translations": {
                                "ru": "В старом особняке есть прекрасная <w>библиотека</w>, полная старинных книг."
                            }
                        },
                        {
                            "text": "Hij trok zich terug in de <w>bibliotheek</w> om ongestoord te kunnen werken.",
                            "target_lang_translations": {
                                "ru": "Он уединился в <w>библиотеке</w>, чтобы поработать, не отвлекаясь."
                            }
                        }
                    ],
                    "synonyms": [
                        "librije"
                    ],
                    "common_phrases": [
                        "een uitgebreide bibliotheek hebben",
                        "in de bibliotheek zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или комната, где хранится собрание книг, часто в большом доме или здании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "библиотека",
                                "target_lang_sense_clarification": "комната или частная коллекция"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "common_phrases": [
                        "political slogan",
                        "campaign slogan",
                        "chant a slogan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерная фраза, используемая группой или политической партией для выражения своей позиции или цели."
                    },
                    "translations": {
                        "ru": [
                            {

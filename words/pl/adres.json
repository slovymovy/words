                    "target_lang_definitions": {
                        "ru": "Торжественное письменное заявление или обращение, направленное властям или выдающейся личности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адрес",
                                "target_lang_sense_clarification": "торжественное приветствие"
                            },
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "петиция или заявление"

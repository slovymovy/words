    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "253faec7-081c-31ef-9874-5c62587c9124",
                    "sense_definition": "Iets nieuws ontwerpen en maken, vaak na een periode van onderzoek en voorbereiding.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf wil een nieuwe applicatie <w>ontwikkelen</w> voor de smartphone.",
                            "target_lang_translations": {
                                "en": "The company wants to <w>develop</w> a new application for the smartphone."
                            }
                        },
                        {
                            "text": "Wetenschappers hebben in korte tijd een veilig vaccin <w>ontwikkeld</w>.",
                            "target_lang_translations": {
                                "en": "Scientists have <w>developed</w> a safe vaccine in a short time."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontwerpen",
                        "creëren",
                        "maken"
                    ],
                    "common_phrases": [
                        "software ontwikkelen",
                        "een product ontwikkelen",
                        "een methode ontwikkelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To design and make something new, often after a period of research and preparation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "create software, products, or ideas"
                            },
                            {
                                "target_lang_word": "create",
                                "target_lang_sense_clarification": "general creation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ad9c4ec-a62a-351c-8631-cb509a9b404b",

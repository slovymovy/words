    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9bc8695d-75bd-3736-98d5-7b1c5c227c5c",
                    "sense_definition": "Taki, który wierzy w zabobony, przesądy i zjawiska nadprzyrodzone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek jest bardzo <w>zabobonny</w> i nigdy nie wita się z gośćmi przez próg.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка очень <w>суеверный</w> и никогда не здоровается с гостями через порог."
                            }
                        },
                        {
                            "text": "Nie bądź taki <w>zabobonny</w>, czarny kot przebiegający drogę to żaden pech.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>суеверным</w>, чёрная кошка, перебегающая дорогу, — это не к беде."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesądny"
                    ],
                    "antonyms": [
                        "racjonalny"
                    ],
                    "common_phrases": [
                        "zabobonny człowiek",
                        "być zabobonnym",
                        "zabobonni ludzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто верит в суеверия, предрассудки и сверхъестественные явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суеверный",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8d1e128-7ace-3432-aa24-0db1b70efef4",
                    "sense_definition": "Wynikający z wiary w zabobony lub mający z nimi związek; będący przejawem takiej wiary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy wioski odczuwali <w>zabobonny</w> lęk przed wchodzeniem do starego lasu po zmroku.",
                            "target_lang_translations": {
                                "ru": "Жители деревни испытывали <w>суеверный</w> страх перед тем, чтобы заходить в старый лес после наступления темноты."
                            }
                        },
                        {
                            "text": "To są tylko stare, <w>zabobonne</w> rytuały, które nie mają żadnego wpływu na twoje zdrowie.",
                            "target_lang_translations": {
                                "ru": "Это всего лишь старые <w>суеверные</w> ритуалы, которые не оказывают никакого влияния на твоё здоровье."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesądny"
                    ],
                    "antonyms": [
                        "racjonalny",
                        "naukowy"
                    ],
                    "common_phrases": [
                        "zabobonny lęk",
                        "zabobonny strach",
                        "zabobonne praktyki",
                        "zabobonne zwyczaje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вытекающий из веры в суеверия или связанный с ними; являющийся проявлением такой веры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суеверный",
                                "target_lang_sense_clarification": "о страхе, обычаях, чувствах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7019a85-c5dc-35f6-a1d7-ed0304835c69",
                    "sense_definition": "Obecność kogoś przy innej osobie, wspólne przebywanie ze sobą w jednym miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo lubię przebywać w twoim <w>towarzystwie</w>, ponieważ zawsze potrafisz mnie rozśmieszyć.",
                            "target_lang_translations": {
                                "ru": "Мне очень нравится находиться в твоем <w>обществе</w>, потому что ты всегда умеешь меня рассмешить."
                            }
                        },
                        {
                            "text": "Starsza pani poprosiła sąsiadkę, aby dotrzymała jej <w>towarzystwa</w> podczas popołudniowego spaceru.",
                            "target_lang_translations": {
                                "ru": "Пожилая дама попросила соседку составить ей <w>компанию</w> во время послеобеденной прогулки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dotrzymać towarzystwa",
                        "w towarzystwie",
                        "unikać towarzystwa",
                        "dla towarzystwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присутствие кого-либо рядом с другим человеком, совместное нахождение в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общество",
                                "target_lang_sense_clarification": "пребывание с кем-то"
                            },
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "совместное времяпрепровождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34c1c2a9-74a7-310f-a6fb-aea2dcaaefba",
                    "sense_definition": "Grupa osób wspólnie spędzających czas, bawiących się lub rozmawiających, często znajomych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest prawdziwą duszą <w>towarzystwa</w> i każdy chce go zapraszać na imprezy.",
                            "target_lang_translations": {
                                "ru": "Мой брат — настоящая душа <w>компании</w>, и все хотят приглашать его на вечеринки."
                            }
                        },
                        {
                            "text": "Całe <w>towarzystwo</w> zebrało się w salonie, aby wspólnie oglądać mecz piłki nożnej.",
                            "target_lang_translations": {
                                "ru": "Вся <w>компания</w> собралась в гостиной, чтобы вместе посмотреть футбольный матч."
                            }
                        }
                    ],
                    "synonyms": [
                        "kompania",
                        "ferajna",
                        "paka",
                        "wiara",
                        "banda",
                        "brać"
                    ],
                    "common_phrases": [
                        "dusza towarzystwa",
                        "doborowe towarzystwo",
                        "mieszane towarzystwo",
                        "wesołe towarzystwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, совместно проводящих время, развлекающихся или беседующих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "группа друзей, знакомых"
                            },
                            {
                                "target_lang_word": "общество",
                                "target_lang_sense_clarification": "круг общения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "739fd83f-3dc9-34b6-a085-54abe5eec115",
                    "sense_definition": "Oficjalna organizacja lub instytucja skupiająca osoby o wspólnych celach zawodowych, naukowych, biznesowych lub hobbystycznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "organization",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Lokalne <w>towarzystwo</w> miłośników historii zorganizowało ciekawą wystawę starych fotografii miasta.",
                            "target_lang_translations": {
                                "ru": "Местное <w>общество</w> любителей истории организовало интересную выставку старых фотографий города."
                            }
                        },
                        {
                            "text": "Musisz zgłosić szkodę, dzwoniąc bezpośrednio do swojego <w>towarzystwa</w> ubezpieczeniowego.",
                            "target_lang_translations": {
                                "ru": "Вы должны заявить об ущербе, позвонив непосредственно в свою страховую <w>компанию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stowarzyszenie",
                        "związek",
                        "klub",
                        "spółka"
                    ],
                    "common_phrases": [
                        "towarzystwo ubezpieczeniowe",
                        "towarzystwo naukowe",
                        "towarzystwo akcyjne",
                        "członek towarzystwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная организация или учреждение, объединяющая людей с общими профессиональными, научными или деловыми целями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общество",
                                "target_lang_sense_clarification": "организация, объединение"
                            },
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "коммерческая организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c65daaf8-5556-3474-a855-307c5f5dfe24",
                    "sense_definition": "Środowisko ludzi uprzywilejowanych, uważanych za elitę lub wyższe sfery społeczne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej największym marzeniem było wejść do wielkiego <w>towarzystwa</w> i bywać na wystawnych balach.",
                            "target_lang_translations": {
                                "ru": "Ее самой большой мечтой было войти в высшее <w>общество</w> и бывать на пышных балах."
                            }
                        },
                        {
                            "text": "To zamknięte <w>towarzystwo</w> bardzo niechętnie przyjmuje nowe osoby spoza swojego kręgu.",
                            "target_lang_translations": {
                                "ru": "Это закрытое <w>общество</w> очень неохотно принимает новых людей извне своего круга."
                            }
                        }
                    ],
                    "synonyms": [
                        "elita"
                    ],
                    "common_phrases": [
                        "bywać w towarzystwie",
                        "śmietanka towarzyska",
                        "wejść do towarzystwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Среда привилегированных людей, считающихся элитой или высшим светом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общество",
                                "target_lang_sense_clarification": "высший свет, элита"
                            },
                            {
                                "target_lang_word": "свет",
                                "target_lang_sense_clarification": "высшее общество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98eeb88b-a18a-38fb-a557-fd8eef31b8a1",
                    "sense_definition": "W dawnej Polsce: grupa szlachty służąca w jednej chorągwi kawalerii narodowej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciężkozbrojne <w>towarzystwo</w> ruszyło do ataku, budząc strach w szeregach nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Тяжеловооруженное <w>товарищество</w> двинулось в атаку, вселяя страх в ряды неприятеля."
                            }
                        },
                        {
                            "text": "Szlachcic ten służył przez wiele lat jako <w>towarzystwo</w> w chorągwi pancernej.",
                            "target_lang_translations": {
                                "ru": "Этот шляхтич много лет служил в составе <w>товарищества</w> в панцирной хоругви."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy dawnej wojskowości polskiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В старой Польше: группа шляхты, служащая в одной хоругви национальной кавалерии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "товарищество",
                                "target_lang_sense_clarification": "историческое военное подразделение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

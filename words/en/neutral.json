{
    "word_family": [
        "neutrality",
        "neutralize",
        "neutralization",
        "neutrally"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "25fedc51-1ff0-3d8e-b517-9d69b7176d18",
                    "sense_definition": "Favoring neither the supporting nor opposing viewpoint of a topic of debate; unbiased.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "impartiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist tried to remain <w>neutral</w> while reporting on the election.",
                            "target_lang_translations": {
                                "ru": "Журналист старался оставаться <w>нейтральным</w>, освещая выборы."
                            }
                        },
                        {
                            "text": "We need a <w>neutral</w> observer to ensure the competition is fair.",
                            "target_lang_translations": {
                                "ru": "Нам нужен <w>нейтральный</w> наблюдатель, чтобы гарантировать честность соревнования."
                            }
                        }
                    ],
                    "synonyms": [
                        "unbiased",
                        "impartial",
                        "objective",
                        "disinterested",
                        "dispassionate"
                    ],
                    "common_phrases": [
                        "neutral stance",
                        "neutral point of view",
                        "remain neutral"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не поддерживающий ни одну из сторон в споре или дискуссии; беспристрастный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "не занимающий ничью сторону"
                            },
                            {
                                "target_lang_word": "беспристрастный",
                                "target_lang_sense_clarification": "непредвзятый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5c090b6-9ddc-387d-9df6-23921769b5a9",
                    "sense_definition": "Not taking sides in a conflict such as war; nonaligned.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "impartiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Switzerland is famous for being a <w>neutral</w> country during major wars.",
                            "target_lang_translations": {
                                "ru": "Швейцария известна тем, что была <w>нейтральной</w> страной во время крупных войн."
                            }
                        },
                        {
                            "text": "The two armies met on <w>neutral</w> territory to discuss a peace treaty.",
                            "target_lang_translations": {
                                "ru": "Две армии встретились на <w>нейтральной</w> территории, чтобы обсудить мирный договор."
                            }
                        }
                    ],
                    "synonyms": [
                        "nonaligned",
                        "nonpartisan"
                    ],
                    "common_phrases": [
                        "neutral territory",
                        "neutral country",
                        "neutral ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не принимающий участия в конфликте, например в войне; не присоединившийся к блокам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "о стране или территории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c10397b-c69a-37c2-9e13-d3cf80cd5d53",
                    "sense_definition": "Having no obvious strong colour; usually referring to shades like gray, beige, or white.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She prefers to wear <w>neutral</w> colors because they match everything.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает носить <w>нейтральные</w> цвета, потому что они ко всему подходят."
                            }
                        },
                        {
                            "text": "The walls were painted in a <w>neutral</w> shade to make the room look larger.",
                            "target_lang_translations": {
                                "ru": "Стены были покрашены в <w>нейтральный</w> оттенок, чтобы комната казалась больше."
                            }
                        }
                    ],
                    "synonyms": [
                        "beige"
                    ],
                    "common_phrases": [
                        "neutral colors",
                        "neutral tones"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий ярко выраженного цвета; обычно относится к таким оттенкам, как серый, бежевый или белый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "о цвете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ab9ab04-4e3d-3b7a-8d28-cf689ab4a0b3",
                    "sense_definition": "Possessing no electric charge; having an equivalent amount of positive and negative charge.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Neutrons are <w>neutral</w> particles found inside the nucleus of an atom.",
                            "target_lang_translations": {
                                "ru": "Нейтроны — это <w>нейтральные</w> частицы, находящиеся внутри ядра атома."
                            }
                        },
                        {
                            "text": "The object is electrically <w>neutral</w> because the protons and electrons balance each other out.",
                            "target_lang_translations": {
                                "ru": "Объект электрически <w>нейтрален</w>, так как протоны и электроны уравновешивают друг друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncharged"
                    ],
                    "common_phrases": [
                        "electrically neutral"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обладающий электрическим зарядом; имеющий равное количество положительных и отрицательных зарядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "физика"
                            },
                            {
                                "target_lang_word": "электронейтральный",
                                "target_lang_sense_clarification": "термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4a1fbda-4788-3437-b278-e4dcdfbaebe3",
                    "sense_definition": "Having a pH of or near 7, meaning it is neither acidic nor alkaline.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pure water is <w>neutral</w>, but adding lemon juice makes it acidic.",
                            "target_lang_translations": {
                                "ru": "Чистая вода <w>нейтральна</w>, но добавление лимонного сока делает её кислой."
                            }
                        },
                        {
                            "text": "You can use a litmus test to see if the solution is acidic, alkaline, or <w>neutral</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать лакмусовый тест, чтобы проверить, является ли раствор кислым, щелочным или <w>нейтральным</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "neutral pH",
                        "neutral solution"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chemistry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий pH равный или близкий к 7, то есть не являющийся ни кислотным, ни щелочным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "химия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc33809c-e0d8-382b-8712-fc7356acc4cb",
                    "sense_definition": "The position of gears in a vehicle where the engine is disconnected from the wheels.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He put the car in <w>neutral</w> while waiting for the traffic light to change.",
                            "target_lang_translations": {
                                "ru": "Он переключил машину на <w>нейтраль</w>, пока ждал переключения светофора."
                            }
                        },
                        {
                            "text": "If the engine fails, shift into <w>neutral</w> and try to steer to the side of the road.",
                            "target_lang_translations": {
                                "ru": "Если двигатель заглохнет, переключитесь на <w>нейтраль</w> и попытайтесь вырулить на обочину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in neutral",
                        "shift to neutral"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "automotive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение шестерен в коробке передач, при котором двигатель отсоединен от колес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральная передача",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "нейтраль",
                                "target_lang_sense_clarification": "разговорное/общеупотребительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0bd9483-82b0-3ac4-8c42-d3bb50d388b7",
                    "sense_definition": "A person or country that does not take a side in a dispute or conflict.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During the debate, he acted as a <w>neutral</w>, refusing to support either team.",
                            "target_lang_translations": {
                                "ru": "Во время дебатов он выступал как <w>нейтральная сторона</w>, отказываясь поддерживать любую из команд."
                            }
                        },
                        {
                            "text": "The small nation remained a <w>neutral</w> throughout the entire war.",
                            "target_lang_translations": {
                                "ru": "Маленькая нация оставалась <w>нейтральной</w> на протяжении всей войны."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или страна, не принимающие ничью сторону в споре или конфликте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральная сторона",
                                "target_lang_sense_clarification": "в споре"
                            },
                            {
                                "target_lang_word": "нейтральное государство",
                                "target_lang_sense_clarification": "в политике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "048af6e4-7621-3559-956c-b1277d89c334",
                    "sense_definition": "A color that is not bright or strong, such as beige, gray, or white.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The designer used a palette of <w>neutrals</w> to create a calm atmosphere.",
                            "target_lang_translations": {
                                "ru": "Дизайнер использовал палитру <w>нейтральных тонов</w>, чтобы создать спокойную атмосферу."
                            }
                        },
                        {
                            "text": "She likes to dress in <w>neutrals</w> rather than wearing bright patterns.",
                            "target_lang_translations": {
                                "ru": "Она любит одеваться в <w>нейтральные цвета</w>, а не носить яркие узоры."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цвет, который не является ярким или насыщенным, например бежевый, серый или белый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный цвет",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "нейтральный тон",
                                "target_lang_sense_clarification": "оттенок"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
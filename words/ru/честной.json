                    ],
                    "target_lang_definitions": {
                        "en": "Feminine form of the adjective 'honest' in the genitive, dative, instrumental, or prepositional case."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honest",

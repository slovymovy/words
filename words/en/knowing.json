                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He arrived at the party wearing a very <knowing> hat.",
                            "target_lang_translations": {
                                "ru": "Он пришел на вечеринку в очень <модной> шляпе."
                            }

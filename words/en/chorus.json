{
    "word_family": [
        "choral",
        "chorister"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3771a4f1-2aa4-3cfd-838c-b3816ed16fb6",
                    "sense_definition": "A part of a song that is repeated after each verse.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "song_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone joined in to sing the <w>chorus</w>.",
                            "target_lang_translations": {
                                "ru": "Все присоединились, чтобы спеть <w>припев</w>."
                            }
                        },
                        {
                            "text": "The <w>chorus</w> is the catchiest part of the track.",
                            "target_lang_translations": {
                                "ru": "<w>Припев</w> — самая запоминающаяся часть трека."
                            }
                        }
                    ],
                    "synonyms": [
                        "duet",
                        "duet"
                    ],
                    "antonyms": [
                        "verse"
                    ],
                    "common_phrases": [
                        "join the chorus",
                        "sing the chorus",
                        "catchy chorus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть песни, которая повторяется после каждого куплета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "припев",
                                "target_lang_sense_clarification": "часть песни"
                            },
                            {
                                "target_lang_word": "рефрен",
                                "target_lang_sense_clarification": "повторяющаяся часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00457fd1-62c4-3bd5-ae34-408095024338",
                    "sense_definition": "A large group of singers who perform together, often in a theatre or church.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_performers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sings soprano in the school <w>chorus</w>.",
                            "target_lang_translations": {
                                "ru": "Она поет сопрано в школьном <w>хоре</w>."
                            }
                        },
                        {
                            "text": "The <w>chorus</w> practiced the new hymn for three hours.",
                            "target_lang_translations": {
                                "ru": "<w>Хор</w> репетировал новый гимн три часа."
                            }
                        }
                    ],
                    "synonyms": [
                        "duet",
                        "duet"
                    ],
                    "common_phrases": [
                        "school chorus",
                        "church chorus",
                        "male chorus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая группа певцов, выступающих вместе, часто в театре или церкви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хор",
                                "target_lang_sense_clarification": "музыкальный коллектив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bd712d8-6724-3356-bd00-501a38d8fc93",
                    "sense_definition": "The sound of many people, animals, or things making noise at the same time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "simultaneous_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>chorus</w> of birds began to sing as the sun rose.",
                            "target_lang_translations": {
                                "ru": "С восходом солнца запел <w>хор</w> птиц."
                            }
                        },
                        {
                            "text": "His joke was met with a <w>chorus</w> of laughter from the audience.",
                            "target_lang_translations": {
                                "ru": "Его шутка была встречена <w>хором</w> смеха из зала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chorus of laughter",
                        "chorus of boos",
                        "chorus of cheers",
                        "dawn chorus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук множества людей, животных или предметов, издающих шум одновременно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хор",
                                "target_lang_sense_clarification": "одновременное звучание голосов или звуков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5847217b-374f-3702-af0d-d8bbba99534b",
                    "sense_definition": "A simultaneous expression of the same opinion or criticism by a large number of people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "simultaneous_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a growing <w>chorus</w> of complaints about the new policy.",
                            "target_lang_translations": {
                                "ru": "Растет <w>хор</w> жалоб на новую политику."
                            }
                        },
                        {
                            "text": "Several experts joined the <w>chorus</w> calling for immediate reform.",
                            "target_lang_translations": {
                                "ru": "Несколько экспертов присоединились к <w>хору</w> тех, кто призывает к немедленным реформам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "join the chorus",
                        "growing chorus",
                        "chorus of disapproval"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одновременное выражение одинакового мнения или критики большим количеством людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хор",
                                "target_lang_sense_clarification": "перен. множество высказываний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee9eb706-6df4-37aa-95d1-f9ff276a109b",
                    "sense_definition": "A group of performers in ancient Greek theatre who dance, sing, and comment on the action.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_performers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>chorus</w> enters the stage to explain the hero's tragic fate.",
                            "target_lang_translations": {
                                "ru": "<w>Хор</w> выходит на сцену, чтобы объяснить трагическую судьбу героя."
                            }
                        },
                        {
                            "text": "In this play, the <w>chorus</w> represents the voice of the citizens.",
                            "target_lang_translations": {
                                "ru": "В этой пьесе <w>хор</w> представляет голос граждан."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Greek chorus"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        },
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа исполнителей в древнегреческом театре, которые танцуют, поют и комментируют действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хор",
                                "target_lang_sense_clarification": "в античном театре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ca3175f-df7d-3c9d-b6be-7f6c190a5d2c",
                    "sense_definition": "To sing, speak, or shout something simultaneously with others.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "simultaneous_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children <w>chorused</w> \"Good morning\" to their teacher.",
                            "target_lang_translations": {
                                "ru": "Дети <w>хором поздоровались</w> с учителем."
                            }
                        },
                        {
                            "text": "\"We are ready!\" the soldiers <w>chorused</w> in response.",
                            "target_lang_translations": {
                                "ru": "«Мы готовы!» — <w>хором ответили</w> солдаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "duet"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Петь, говорить или кричать что-либо одновременно с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорить хором",
                                "target_lang_sense_clarification": "произносить одновременно"
                            },
                            {
                                "target_lang_word": "отвечать хором",
                                "target_lang_sense_clarification": "отвечать одновременно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7c72ab5a-cc23-3313-bebe-839abf78c9aa",
                    "sense_definition": "A character in a play, especially in Shakespearean drama, who speaks the prologue and comments on the action.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_performers",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "Enter <w>Chorus</w> to introduce the scene.",
                            "target_lang_translations": {
                                "ru": "Входит <w>Хор</w>, чтобы представить сцену."
                            }
                        },
                        {
                            "text": "The play begins with a speech delivered by <w>Chorus</w>.",
                            "target_lang_translations": {
                                "ru": "Пьеса начинается с речи, которую произносит <w>Хор</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Персонаж в пьесе, особенно в драмах Шекспира, который читает пролог и комментирует действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хор",
                                "target_lang_sense_clarification": "персонаж пьесы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
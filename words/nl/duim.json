                {
                    "sense_id": "ec2c830c-a1f2-3f2f-a94a-6f2683efc9ac",
                    "sense_definition": "De kortste en dikste vinger van de hand, die tegenover de andere vingers geplaatst kan worden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De peuter stopt vaak zijn <w>duim</w> in zijn mond als hij moe is.",
                            "target_lang_translations": {
                                "ru": "Малыш часто засовывает <w>большой палец</w> в рот, когда устает."
                            }
                        },
                        {
                            "text": "Hij stak zijn <w>duim</w> omhoog om te laten zien dat alles goed ging.",
                            "target_lang_translations": {
                                "ru": "Он поднял <w>большой палец</w> вверх, чтобы показать, что все идет хорошо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "duim omhoog",
                        "onder de duim houden",
                        "uit de duim zuigen",
                        "groene duim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый короткий и толстый палец руки, противопоставленный остальным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "большой палец",
                                "target_lang_sense_clarification": "руки"
                            }
                        ]

                            },
                            {
                                "target_lang_word": "резонанс",
                                "target_lang_sense_clarification": "широкая огласка, общественное волнение"
                            },
                            {
                                "target_lang_word": "отголосок",
                                "target_lang_sense_clarification": "следствие или остаточное явление"

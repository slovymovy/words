            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "85ec3313-caa1-34aa-81f8-96bc262b10b5",
                    "sense_definition": "Сознательно, преднамеренно; с предварительным знанием того, что действие является ложным, неправомерным или невыполнимым.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intentional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свидетель дал в суде <w>заведомо</w> ложные показания, чтобы защитить своего друга.",
                            "target_lang_translations": {
                                "en": "The witness gave <w>knowingly</w> false testimony in court to protect his friend."
                            }
                        },
                        {
                            "text": "Мошенники продавали людям <w>заведомо</w> неисправную технику под видом новой.",
                            "target_lang_translations": {
                                "en": "The fraudsters sold people <w>knowingly</w> defective equipment disguised as new."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заведомо ложный",
                        "заведомо ложные показания",
                        "заведомо невыполнимый"
                    ],
                    "target_lang_definitions": {
                        "en": "Consciously, intentionally; with prior knowledge that an action is false, unlawful, or impossible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knowingly",
                                "target_lang_sense_clarification": "done with full awareness, especially regarding something illegal or false"
                            },
                            {
                                "target_lang_word": "deliberately",
                                "target_lang_sense_clarification": "done on purpose; intentionally"
                            },
                            {
                                "target_lang_word": "wittingly",
                                "target_lang_sense_clarification": "done with knowledge of the facts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfc5d9e4-dbe8-34e2-93a6-8d79aedbab11",
                    "sense_definition": "Несомненно, очевидно, бесспорно; так, как было известно или определено заранее.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "obvious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вступать в спор с начальником в такой ситуации — это <w>заведомо</w> проигрышное дело.",
                            "target_lang_translations": {
                                "en": "Getting into an argument with the boss in such a situation is an <w>obviously</w> losing cause."
                            }
                        },
                        {
                            "text": "Результат этого эксперимента был <w>заведомо</w> известен всем участникам научной группы.",
                            "target_lang_translations": {
                                "en": "The result of this experiment was known <w>in advance</w> to all members of the research group."
                            }
                        }
                    ],
                    "synonyms": [
                        "несомненно",
                        "явно"
                    ],
                    "common_phrases": [
                        "заведомо проигрышный",
                        "заведомо известно",
                        "заведомо обречён"
                    ],
                    "target_lang_definitions": {
                        "en": "Undoubtedly, obviously, indisputably; as was known or determined beforehand."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obviously",
                                "target_lang_sense_clarification": "clearly or undeniably"
                            },
                            {
                                "target_lang_word": "in advance",
                                "target_lang_sense_clarification": "known or determined beforehand"
                            },
                            {
                                "target_lang_word": "manifestly",
                                "target_lang_sense_clarification": "clearly true or existing; patent"
                            }
                        ]
                    }
                }

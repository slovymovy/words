                    "common_phrases": [
                        "театральный жест",
                        "театральная пауза",
                        "театральная поза"
                    ],
                    "target_lang_definitions": {
                        "en": "Artificial, unnatural, exaggeratedly expressive, calculated for effect."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "theatrical",

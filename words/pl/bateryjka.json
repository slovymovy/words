            "senses": [
                {
                    "sense_id": "93fea134-ab63-310e-884c-a6b9bd067a9c",
                    "sense_definition": "Zdrobnienie od słowa <w>bateria</w>; małe źródło zasilania używane w drobnych urządzeniach elektronicznych, takich jak zegarki czy zabawki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "power_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę jak najszybciej wymienić <w>bateryjkę</w> w moim starym zegarku.",
                            "target_lang_translations": {
                                "ru": "Мне нужно как можно скорее заменить <w>батарейку</w> в моих старых часах."
                            }
                        },
                        {
                            "text": "Zabawka przestała działać, ponieważ wyczerpała się w niej mała <w>bateryjka</w>.",
                            "target_lang_translations": {
                                "ru": "Игрушка перестала работать, так как в ней села маленькая <w>батарейка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bateria",
                        "ogniwo"
                    ],
                    "common_phrases": [
                        "bateryjka do zegarka",
                        "wymiana bateryjki",
                        "wyczerpana bateryjka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова \"bateria\"; небольшой источник питания, используемый в мелких электронных устройствах, таких как часы или игрушки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "батарейка",
                                "target_lang_sense_clarification": "маленький элемент питания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cec7e193-9806-3bb2-b9c4-1b74ec1a0fb6",
                    "sense_definition": "Mała latarka elektryczna (określenie używane regionalnie lub potocznie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "light_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wziął ze sobą kieszonkową <w>bateryjkę</w>, żeby oświetlić drogę do piwnicy.",
                            "target_lang_translations": {
                                "ru": "Он взял с собой карманный <w>фонарик</w>, чтобы осветить дорогу в подвал."
                            }
                        },
                        {
                            "text": "Ta stara <w>bateryjka</w> daje bardzo słabe i migoczące światło.",
                            "target_lang_translations": {
                                "ru": "Этот старый <w>фонарик</w> дает очень слабый и мерцающий свет."
                            }
                        }

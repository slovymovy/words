                        "property"
                    ],
                    "common_phrases": [
                        "fixed tangibles"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический объект или актив; имущество, имеющее вещественную форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материальные активы",
                                "target_lang_sense_clarification": "имущество компании (здания, оборудование)"
                            },
                            {
                                "target_lang_word": "материальные ценности",
                                "target_lang_sense_clarification": "физические объекты, имеющие стоимость"
                            }
                        ]
                    }
                }
            ]
        }
    ]

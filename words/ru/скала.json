    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b65a815f-2aba-361d-bd2b-524050397de0",
                    "sense_definition": "Каменная глыба или гора с крутыми склонами и острыми выступами.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "nature_rock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы решили подняться на высокую <w>скалу</w>, чтобы увидеть море.",
                            "target_lang_translations": {
                                "en": "The tourists decided to climb a high <w>rock</w> to see the sea."
                            }
                        },
                        {
                            "text": "Корабль разбился о подводные <w>скалы</w> во время шторма.",
                            "target_lang_translations": {
                                "en": "The ship crashed against underwater <w>rocks</w> during the storm."
                            }
                        },
                        {
                            "text": "В середине берегового выступа возвышалась крутая <w>скала</w> с тонкими пальмами.",
                            "target_lang_translations": {
                                "en": "In the middle of the coastal projection rose a steep <w>rock</w> with thin palm trees."
                            }
                        }
                    ],
                    "synonyms": [
                        "шкала"
                    ],
                    "common_phrases": [
                        "отвесная скала",
                        "крутая скала",
                        "вершина скалы",
                        "острая скала"
                    ],
                    "target_lang_definitions": {
                        "en": "A large mass of stone or a mountain with steep slopes and sharp projections."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rock",
                                "target_lang_sense_clarification": "large mass of stone projecting out of the ground or water"
                            },
                            {
                                "target_lang_word": "cliff",
                                "target_lang_sense_clarification": "steep rock face"
                            },
                            {
                                "target_lang_word": "crag",
                                "target_lang_sense_clarification": "steep or rugged rock"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd5fff96-46d4-318b-828c-8cb3323e73c4",
                    "sense_definition": "То же, что шкала; перечень или последовательность величин.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement_scale",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старых документах эта <w>скала</w> использовалась для измерения уровня воды.",
                            "target_lang_translations": {
                                "en": "In old documents, this <w>scale</w> was used to measure the water level."
                            }
                        },
                        {
                            "text": "Музыкальная <w>скала</w> представляет собой ряд звуков, расположенных по высоте.",
                            "target_lang_translations": {
                                "en": "A musical <w>scale</w> represents a series of sounds arranged by pitch."
                            }
                        }
                    ],
                    "synonyms": [
                        "шкала"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'шкала'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An archaic term for a scale; a sequence of values or musical notes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scale",
                                "target_lang_sense_clarification": "ordered sequence for measurement or music"
                            }
                        ]
                    }
                }
            ]
        },
        {

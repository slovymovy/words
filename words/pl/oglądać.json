                    "sense_id": "eed0d5da-ffa6-3a80-9640-06a1f92b87c1",
                    "sense_definition": "Śledzić wzrokiem przebieg jakiegoś wydarzenia, widowiska lub programu (np. w telewizji, kinie lub na żywo).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy piątek cała nasza rodzina siada na kanapie, aby wspólnie <w>oglądać</w> film.",
                            "target_lang_translations": {
                                "ru": "Каждую пятницу вся наша семья садится на диван, чтобы вместе <w>смотреть</w> фильм."
                            }
                        },
                        {
                            "text": "Mój dziadek bardzo lubi <w>oglądać</w> wiadomości wieczorne w telewizji.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка очень любит <w>смотреть</w> вечерние новости по телевизору."
                            }
                        }
                    ],
                    "synonyms": [
                        "patrzeć",
                        "obserwować"
                    ],
                    "common_phrases": [
                        "oglądać telewizję",
                        "oglądać film",
                        "oglądać mecz",
                        "lubić oglądać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следить взглядом за ходом какого-либо события, зрелища или программы (например, по телевизору, в кино или вживую)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "фильм, телевизор, представление"
                            },
                            {
                                "target_lang_word": "наблюдать",
                                "target_lang_sense_clarification": "следить за происходящим"
                            }
                        ]
                    }
                },
                {

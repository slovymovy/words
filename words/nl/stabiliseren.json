    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a2924adf-b222-36d3-8ccb-efd7a224c134",
                    "sense_definition": "Iets in evenwicht brengen of bestendig maken, zodat de toestand niet verslechtert of verandert.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "make_stable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De artsen hebben hard gewerkt om de gewonde man te <w>stabiliseren</w>.",
                            "target_lang_translations": {
                                "en": "The doctors worked hard to <w>stabilize</w> the injured man."
                            }
                        },
                        {
                            "text": "De regering neemt maatregelen om de economie te <w>stabiliseren</w>.",
                            "target_lang_translations": {
                                "en": "The government is taking measures to <w>stabilize</w> the economy."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestendigen",
                        "consolideren"
                    ],
                    "antonyms": [
                        "destabiliseren"
                    ],
                    "common_phrases": [
                        "de economie stabiliseren",
                        "een patiënt stabiliseren",
                        "de prijzen stabiliseren"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring into balance or make steady, preventing deterioration or change."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stabilize",
                                "target_lang_sense_clarification": "make stable"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b4f09ba-6f00-3051-b54f-4e491a02bc94",
                    "sense_definition": "In een vaste toestand komen; niet meer veranderen of schommelen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "become_stable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een onrustige week <w>stabiliseerde</w> het weer zich eindelijk.",
                            "target_lang_translations": {
                                "en": "After a restless week, the weather finally <w>stabilized</w>."
                            }
                        },
                        {
                            "text": "De toestand van de patiënt <w>stabiliseerde</w> de volgende dag.",
                            "target_lang_translations": {
                                "en": "The patient's condition <w>stabilized</w> the next day."
                            }
                        }
                    ],
                    "synonyms": [
                        "tot rust komen",
                        "gelijkblijven"
                    ],
                    "antonyms": [
                        "destabiliseren",
                        "destabiliseren"
                    ],
                    "common_phrases": [
                        "zich stabiliseren",
                        "snel stabiliseren",
                        "rond een niveau stabiliseren"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a fixed state; to stop changing or fluctuating."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stabilize",
                                "target_lang_sense_clarification": "become stable"
                            },
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "calm down or stop moving"
                            }
                        ]
                    }
                }
            ]
        }
    ]

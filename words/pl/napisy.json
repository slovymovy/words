                {
                    "sense_id": "24ad36af-7d9a-3eea-9ae1-af18ac7bb152",
                    "sense_definition": "tekst wyświetlany na dole ekranu podczas filmu lub programu, tłumaczący dialogi lub ułatwiający odbiór osobom niesłyszącym",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na płycie niestety nie ma polskich <w>napisów</w> do tego filmu.",

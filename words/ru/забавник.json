            "senses": [
                {
                    "sense_id": "680c82a9-42ac-3424-8ec7-3cba9b43f6f0",
                    "sense_definition": "Человек, который любит и умеет веселить других; шутник, весёлый человек.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "entertainer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг — настоящий <w>забавник</w>, с ним никогда не бывает скучно в компании.",
                            "target_lang_translations": {
                                "en": "My friend is a real <w>joker</w>; it is never boring with him in a group."
                            }
                        },
                        {
                            "text": "Дети обожали этого <w>забавника</w> за его бесконечные фокусы и смешные истории.",
                            "target_lang_translations": {
                                "en": "The children adored this <w>entertainer</w> for his endless tricks and funny stories."
                            }
                        }
                    ],
                    "synonyms": [
                        "шутник",
                        "весельчак",
                        "балагур",
                        "потешник"
                    ],
                    "antonyms": [
                        "зануда"
                    ],
                    "common_phrases": [
                        "большой забавник",
                        "известный забавник"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who loves to and is good at amusing others; a joker, a cheerful person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "joker",
                                "target_lang_sense_clarification": "someone who likes to make jokes and amuse people"
                            },
                            {
                                "target_lang_word": "entertainer",
                                "target_lang_sense_clarification": "someone who amuses others, often informally"
                            },
                            {
                                "target_lang_word": "funny fellow",
                                "target_lang_sense_clarification": "a cheerful, amusing person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70b42966-4476-3123-9572-635e22a36cc6",
                    "sense_definition": "Шут при царском дворе или в доме богатого человека (в прошлом).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "entertainer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При дворе короля жил старый <w>забавник</w>, которому позволялось говорить правду в шутливой форме.",
                            "target_lang_translations": {
                                "en": "An old <w>jester</w> lived at the king's court, who was allowed to speak the truth in a joking manner."
                            }
                        },
                        {
                            "text": "Богатый помещик держал <w>забавников</w> для развлечения гостей во время пиров.",
                            "target_lang_translations": {
                                "en": "The rich landowner kept <w>jesters</w> to entertain guests during feasts."
                            }
                        }
                    ],
                    "synonyms": [
                        "шут",
                        "скоморох"
                    ],
                    "common_phrases": [
                        "придворный забавник"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "историческое значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A jester at a royal court or in a wealthy household (historical)."
                    },
                    "translations": {

                },
                {
                    "sense_id": "ec832b18-84c9-303e-aa67-7088d4e56469",
                    "sense_definition": "Een hoeveelheid van 1.000.000 eenheden of exemplaren, vaak gebruikt om geldbedragen aan te duiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft in de loterij een <w>miljoen</w> gewonnen.",
                            "target_lang_translations": {

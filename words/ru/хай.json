                            {
                                "target_lang_word": "criticize",
                                "target_lang_sense_clarification": "to express disapproval"
                            },
                            {
                                "target_lang_word": "badmouth",
                                "target_lang_sense_clarification": "to speak negatively about"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc36efa8-ef45-3e34-b03a-578d1f0c78d7",
                    "sense_definition": "Астрономическое явление, при котором одно небесное тело или его тень временно закрывает другое небесное тело от наблюдателя.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В следующем году мы сможем наблюдать полное солнечное <w>затмение</w>.",
                            "target_lang_translations": {
                                "en": "Next year we will be able to observe a total solar <w>eclipse</w>."
                            }
                        },
                        {
                            "text": "Лунное <w>затмение</w> происходит, когда Земля оказывается между Солнцем и Луной.",
                            "target_lang_translations": {
                                "en": "A lunar <w>eclipse</w> occurs when the Earth ends up between the Sun and the Moon."
                            }
                        }
                    ],
                    "synonyms": [
                        "затменье"
                    ],
                    "common_phrases": [
                        "солнечное затмение",
                        "лунное затмение",
                        "полное затмение",
                        "кольцеобразное затмение"
                    ],
                    "target_lang_definitions": {
                        "en": "An astronomical phenomenon in which one celestial body or its shadow temporarily obscures another celestial body from an observer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eclipse",
                                "target_lang_sense_clarification": "astronomical phenomenon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b805db5-0b57-3c4b-914c-d22ac2c3eac0",
                    "sense_definition": "Временное помутнение рассудка, утрата ясности сознания или способности здраво мыслить.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На него нашло какое-то <w>затмение</w>, и он совершил ужасную ошибку.",
                            "target_lang_translations": {
                                "en": "Some kind of <w>blackout</w> came over him, and he made a terrible mistake."
                            }
                        },
                        {
                            "text": "В минуту гнева у человека может случиться временное <w>затмение</w> разума.",
                            "target_lang_translations": {
                                "en": "In a moment of anger, a person can experience a temporary <w>clouding</w> of reason."
                            }
                        }
                    ],
                    "synonyms": [
                        "затменье"
                    ],
                    "common_phrases": [
                        "затмение нашло",
                        "затмение разума",
                        "затмение ума",
                        "временное затмение"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A temporary clouding of reason, loss of clarity of consciousness, or ability to think sanely."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clouding of the mind",
                                "target_lang_sense_clarification": "mental state"
                            },
                            {
                                "target_lang_word": "blackout",
                                "target_lang_sense_clarification": "temporary loss of memory or consciousness"
                            },
                            {
                                "target_lang_word": "lapse of reason",
                                "target_lang_sense_clarification": "momentary loss of judgment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e70624c5-2f5f-3106-9b78-8cbf1b23b852",
                    "sense_definition": "Потеря зрения, слепота.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый поэт описывал в стихах своё внезапное <w>затмение</w>.",
                            "target_lang_translations": {
                                "en": "The old poet described his sudden <w>blindness</w> in verse."
                            }
                        },
                        {
                            "text": "Болезнь прогрессировала, и ему грозило полное <w>затмение</w> глаз.",
                            "target_lang_translations": {
                                "en": "The disease was progressing, and he was threatened with total <w>loss of sight</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "затменье"
                    ],

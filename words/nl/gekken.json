            "senses": [
                {
                    "sense_id": "fe00376b-ba30-334a-bc68-f961ba86854d",
                    "sense_definition": "Grapjes maken, plagen of niet serieus doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen waren aan het <w>gekken</w> en maakten veel lawaai in de tuin.",
                            "target_lang_translations": {
                                "en": "The children were <w>fooling around</w> and making a lot of noise in the garden.",
                                "ru": "Дети <w>дурачились</w> и сильно шумели в саду."
                            }
                        },
                        {
                            "text": "Ik ben niet aan het <w>gekken</w>, dit is echt een serieuze zaak.",
                            "target_lang_translations": {
                                "en": "I am not <w>kidding</w>, this is really a serious matter.",
                                "ru": "Я не <w>шучу</w>, это действительно серьезное дело."

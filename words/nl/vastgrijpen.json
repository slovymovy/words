    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "92fc7e96-aa86-3c61-94b6-fb358f9ec072",
                    "sense_definition": "Iets of iemand plotseling en met kracht in de handen nemen en vasthouden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_grab",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieagent moest de verdachte stevig <w>vastgrijpen</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейскому пришлось крепко <w>схватить</w> подозреваемого.",
                                "en": "The police officer had to firmly <w>grab</w> the suspect."
                            }
                        },
                        {
                            "text": "Ze wilde mijn arm <w>vastgrijpen</w> toen ik bijna struikelde.",
                            "target_lang_translations": {
                                "ru": "Она хотела <w>схватить</w> меня за руку, когда я чуть не споткнулся.",
                                "en": "She wanted to <w>grab</w> my arm when I almost stumbled."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastpakken",
                        "grijpen",
                        "omklemmen",
                        "bij de lurven pakken"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "iemand bij de arm vastgrijpen",
                        "de leuning vastgrijpen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять что-то или кого-то внезапно и с силой в руки и удерживать.",
                        "en": "To take something or someone suddenly and forcefully into one's hands and hold them."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "схватить",
                                "target_lang_sense_clarification": "резко взять"
                            },
                            {
                                "target_lang_word": "ухватить",
                                "target_lang_sense_clarification": "крепко взять"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "take suddenly"
                            },
                            {
                                "target_lang_word": "seize",
                                "target_lang_sense_clarification": "take by force"
                            },
                            {
                                "target_lang_word": "take hold of",
                                "target_lang_sense_clarification": "grasp firmly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9033cc8-4728-3bbf-879f-15f779b5e5e5",
                    "sense_definition": "Zichzelf ergens aan vasthouden om steun te vinden of niet te vallen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action_hold_on",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kon zich nog net aan een uitstekende tak <w>vastgrijpen</w>.",
                            "target_lang_translations": {
                                "ru": "Он едва успел <w>ухватиться</w> за торчащую ветку.",
                                "en": "He was just able to <w>grab hold of</w> a protruding branch."
                            }
                        },
                        {
                            "text": "Tijdens de zware storm moest ik me aan een lantaarnpaal <w>vastgrijpen</w>.",
                            "target_lang_translations": {
                                "ru": "Во время сильного шторма мне пришлось <w>ухватиться</w> за фонарный столб.",
                                "en": "During the heavy storm, I had to <w>hold on to</w> a lamppost."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich vastklampen",
                        "zich vasthouden"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "zich ergens aan vastgrijpen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держаться за что-то, чтобы найти опору или не упасть.",
                        "en": "To hold onto something to find support or prevent falling."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухватиться",
                                "target_lang_sense_clarification": "найти опору"
                            },
                            {
                                "target_lang_word": "схватиться",
                                "target_lang_sense_clarification": "держаться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "hold on to",
                                "target_lang_sense_clarification": "find support"
                            },
                            {
                                "target_lang_word": "grab hold of",
                                "target_lang_sense_clarification": "seize for support"
                            },
                            {
                                "target_lang_word": "cling to",
                                "target_lang_sense_clarification": "hold tightly"
                            }
                        ]
                    }
                }
            ]
        }
    ]

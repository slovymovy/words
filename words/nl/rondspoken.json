    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4cb1edaf-40a6-3424-afdc-b1520e9195ea",
                    "sense_definition": "Zich zonder duidelijk doel ergens bewegen, vaak gezegd van geesten die een plek onveilig maken.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de oude legende blijven de geesten van de overleden zeelieden hier eeuwig <w>rondspoken</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно старой легенде, призраки погибших моряков продолжают вечно здесь <w>бродить</w>."
                            }
                        },
                        {
                            "text": "Ik hoorde gisteravond vreemde geluiden op zolder alsof er iemand aan het <w>rondspoken</w> was.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером я слышал странные звуки на чердаке, как будто там кто-то <w>бродил</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rondwaren"
                    ],
                    "common_phrases": [
                        "in een huis rondspoken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бродить без определенной цели; часто говорится о призраках, обитающих в каком-либо месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродить",
                                "target_lang_sense_clarification": "о призраках или людях"
                            },
                            {
                                "target_lang_word": "обитать",
                                "target_lang_sense_clarification": "о духах в определенном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54a033f2-0e41-35f8-8818-b3204184aaff",
                    "sense_definition": "Voortdurend in iemands gedachten aanwezig blijven, vaak op een hinderlijke of zorgelijke manier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die vervelende vraag blijft maar door mijn hoofd <w>rondspoken</w> zonder dat ik een antwoord vind.",
                            "target_lang_translations": {
                                "ru": "Этот назойливый вопрос продолжает <w>вертеться</w> у меня в голове, а ответа я так и не нахожу."
                            }
                        },
                        {
                            "text": "De angst voor een nieuwe economische crisis blijft bij veel investeerders <w>rondspoken</w>.",
                            "target_lang_translations": {
                                "ru": "Страх перед новым экономическим кризисом продолжает <w>преследовать</w> многих инвесторов."
                            }
                        }
                    ],
                    "synonyms": [
                        "malen"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "door het hoofd rondspoken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно присутствовать в мыслях, не давать покоя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вертеться",
                                "target_lang_sense_clarification": "в голове или мыслях"
                            },
                            {
                                "target_lang_word": "крутиться",
                                "target_lang_sense_clarification": "навязчиво в мыслях"
                            },
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "о страхах или мыслях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
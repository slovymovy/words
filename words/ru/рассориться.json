{
    "word_family": [
        "ссора",
        "ссориться",
        "поссориться",
        "перессориться"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b864edd8-cc1c-3e1e-a29e-d34dcaa306f3",
                    "sense_definition": "Сильно поссориться, испортить отношения с кем-либо в результате конфликта.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quarrel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они <рассорились> из-за пустяка и теперь не разговаривают друг с другом.",
                            "target_lang_translations": {
                                "en": "They <fell out> over a trifle and now do not speak to each other."
                            }
                        },
                        {
                            "text": "Я боюсь <рассориться> с лучшим другом из-за этого глупого спора.",
                            "target_lang_translations": {
                                "en": "I am afraid to <fall out> with my best friend over this stupid argument."
                            }
                        }
                    ],
                    "synonyms": [
                        "поссориться",
                        "разругаться"
                    ],
                    "antonyms": [
                        "помириться"
                    ],
                    "common_phrases": [
                        "рассориться с друзьями",
                        "рассориться с семьёй",
                        "рассориться окончательно",
                        "рассориться в пух и прах"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To quarrel badly, to spoil relations with someone as a result of a conflict."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall out",
                                "target_lang_sense_clarification": "to stop being friendly with someone because of a disagreement"
                            },
                            {
                                "target_lang_word": "quarrel",
                                "target_lang_sense_clarification": "to have an angry argument or disagreement"
                            },
                            {
                                "target_lang_word": "break off relations",
                                "target_lang_sense_clarification": "to end a relationship or friendship due to conflict"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6765d62-85f1-3934-8333-5475370261eb",
                    "sense_definition": "Рассыпаться, упасть и разлететься в разные стороны (о мелких предметах или сыпучих веществах).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scatter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мешок порвался, и зерно <рассорилось> по всей дороге.",
                            "target_lang_translations": {
                                "en": "The sack tore, and the grain <scattered> all over the road."
                            }
                        },
                        {
                            "text": "Вся мелочь из кошелька случайно <рассорилась> по полу.",
                            "target_lang_translations": {
                                "en": "All the small change from the wallet accidentally <spilled> on the floor."
                            }
                        }
                    ],
                    "synonyms": [
                        "разругаться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To scatter, fall, and fly apart in different directions (referring to small objects or loose substances)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scatter",
                                "target_lang_sense_clarification": "to separate and move in different directions"
                            },
                            {
                                "target_lang_word": "spill",
                                "target_lang_sense_clarification": "to fall or flow out of a container and spread"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "76e87908-fd86-381a-81c6-6c649b9aec63",
                    "sense_definition": "Gerelateerd aan of afkomstig uit het continent Afrika.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "continent_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er waren geen uitgestrekte zandvlaktes met golvende duinen zoals in de <w>Afrikaanse</w> Sahara.",
                            "target_lang_translations": {
                                "ru": "Там не было обширных песчаных равнин с волнистыми дюнами, как в <w>африканской</w> Сахаре."
                            }
                        },
                        {
                            "text": "De <w>Afrikaanse</w> olifant is groter dan zijn Aziatische soortgenoot.",
                            "target_lang_translations": {
                                "ru": "<w>Африканский</w> слон крупнее своего азиатского сородича."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Afrikaanse landen",
                        "Afrikaanse cultuur",
                        "Afrikaanse wilde hond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к континенту Африка или происходящий из него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "африканский",
                                "target_lang_sense_clarification": "относящийся к континенту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3382382f-1658-3020-a614-1ee40b9b5014",
                    "sense_definition": "Gerelateerd aan de taal Afrikaans.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "language_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij leest graag <w>Afrikaanse</w> gedichten en romans.",
                            "target_lang_translations": {
                                "ru": "Он любит читать стихи и романы на <w>африкаанс</w>."
                            }
                        },
                        {
                            "text": "De <w>Afrikaanse</w> grammatica is in sommige opzichten eenvoudiger dan de Nederlandse.",
                            "target_lang_translations": {
                                "ru": "Грамматика <w>африкаанс</w> в некоторых отношениях проще нидерландской."
                            }
                        }
                    ],
                    "synonyms": [
                        "Zuid-Afrikaans"
                    ],
                    "common_phrases": [
                        "Afrikaanse literatuur",
                        "Afrikaanse woorden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к языку африкаанс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "африкаанс",
                                "target_lang_sense_clarification": "относящийся к языку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d40f3964-58bb-3488-be6a-def360747158",
                    "sense_definition": "Een West-Germaanse taal die voornamelijk in Zuid-Afrika en Namibië wordt gesproken en afstamt van het Nederlands.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "language_name",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Hij spreekt vloeiend <w>Afrikaans</w> met zijn familie.",
                            "target_lang_translations": {
                                "ru": "Он свободно говорит на <w>африкаанс</w> со своей семьей."
                            }
                        },
                        {
                            "text": "Het <w>Afrikaans</w> is een van de officiële talen van Zuid-Afrika.",
                            "target_lang_translations": {
                                "ru": "<w>Африкаанс</w> является одним из официальных языков Южной Африки."
                            }
                        }
                    ],
                    "synonyms": [
                        "Zuid-Afrikaans"
                    ],
                    "common_phrases": [
                        "in het Afrikaans",
                        "Zuid-Afrikaans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Западногерманский язык, происходящий от нидерландского, на котором говорят в основном в Южной Африке и Намибии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "африкаанс",
                                "target_lang_sense_clarification": "язык"
                            }
                        ]
                    }
                }
            ]
        }
    ]

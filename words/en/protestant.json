            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c247fc72-1926-322e-935f-d89c6102eeae",
                    "sense_definition": "A member of any of several Christian denominations that separated from the Roman Catholic Church during the Reformation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "religious_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many <w>Protestants</w> attend services at their local church every Sunday.",
                            "target_lang_translations": {
                                "ru": "Многие <w>протестанты</w> посещают службы в своей местной церкви каждое воскресенье."
                            }
                        },
                        {
                            "text": "He was raised as a <w>Protestant</w> but has friends of many different faiths.",
                            "target_lang_translations": {
                                "ru": "Он был воспитан как <w>протестант</w>, но у него есть друзья самых разных вероисповеданий."
                            }
                        }
                    ],
                    "antonyms": [
                        "Catholic"
                    ],
                    "common_phrases": [
                        "Protestant church",
                        "Protestant reformation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член одной из нескольких христианских конфессий, отделившихся от Римско-католической церкви во время Реформации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протестант",
                                "target_lang_sense_clarification": "последователь протестантизма"
                            },
                            {
                                "target_lang_word": "протестантка",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64d7ef97-af30-38fa-9eb9-df9bf08797ec",
                    "sense_definition": "A person who protests or dissents; a protester.",

                    "common_phrases": [
                        "мощный дизель",
                        "ремонт дизеля",
                        "судовой дизель"
                    ],
                    "target_lang_definitions": {
                        "en": "An internal combustion engine in which heat produced by the compression of air in the cylinder is used to ignite the fuel."
                    },
                    "translations": {

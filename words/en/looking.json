    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "958f3f69-5ab1-3af0-a4c8-a16bbdb925bf",
                    "sense_definition": "Present participle and gerund of <w>look</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>looking</w> for my keys, but I cannot find them anywhere.",
                            "target_lang_translations": {
                                "ru": "Я <w>ищу</w> свои ключи, но нигде не могу их найти."
                            }
                        },
                        {
                            "text": "She is <w>looking</w> at the beautiful painting on the wall.",
                            "target_lang_translations": {
                                "ru": "Она <w>смотрит</w> на красивую картину на стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "watching",
                        "viewing",
                        "observing",
                        "gazing"
                    ],
                    "antonyms": [
                        "ignoring"
                    ],
                    "common_phrases": [
                        "looking for",
                        "looking at",
                        "looking after",
                        "looking forward to",
                        "looking back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола look."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "направлять взгляд"
                            },
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "пытаться найти (в сочетании looking for)"
                            },
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "иметь вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "960ca0e3-aee7-3f5a-9c4b-7ea4f5f07cde",
                    "sense_definition": "Relating to or having the appearance of; used in compound adjectives.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a very good-<w>looking</w> man who works as a model.",
                            "target_lang_translations": {
                                "ru": "Он очень хорошо <w>выглядящий</w> мужчина, который работает моделью."
                            }
                        },
                        {
                            "text": "That is a strange-<w>looking</w> animal that I have never seen before.",
                            "target_lang_translations": {
                                "ru": "Это странно <w>выглядящее</w> животное, которого я никогда раньше не видел."
                            }
                        }
                    ],
                    "synonyms": [
                        "appearing",
                        "seeming"
                    ],
                    "common_phrases": [
                        "good-looking",
                        "nice-looking",
                        "strange-looking",
                        "funny-looking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий определенный вид; используется в составных прилагательных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выглядящий",
                                "target_lang_sense_clarification": "в составе сложных прилагательных"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "04cdb087-9858-3bd6-be48-0cbf4a36eb43",
                    "sense_definition": "The act of one who looks; a glance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His intense <w>looking</w> made everyone in the room feel uncomfortable.",
                            "target_lang_translations": {
                                "ru": "Его пристальное <w>разглядывание</w> заставило всех в комнате чувствовать себя неловко."
                            }
                        },
                        {
                            "text": "After much <w>looking</w> and searching, we finally found the right path.",
                            "target_lang_translations": {
                                "ru": "После долгих <w>поисков</w> и блужданий мы наконец нашли правильный путь."
                            }
                        }
                    ],
                    "synonyms": [
                        "glance",
                        "gaze",
                        "observation",
                        "inspection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие смотрения или поиска; взгляд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разглядывание",

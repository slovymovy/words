                },
                {
                    "sense_id": "8f2eddc1-1c18-3757-9550-ed5ae9223ac2",
                    "sense_definition": "Lacking in quantity, fullness, or richness; scanty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "poverty_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>penurious</w> soil could not support the crops needed to feed the village.",

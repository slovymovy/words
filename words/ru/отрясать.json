    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "baae91af-9412-3b0f-acdc-8207e07e6f6c",
                    "sense_definition": "Удалять что-либо (пыль, снег, капли) с какой-либо поверхности резким движением, встряхиванием.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путник остановился у порога и долго <отрясал> дорожную пыль с плаща.",
                            "target_lang_translations": {
                                "en": "The traveler stopped at the threshold and <shook off> the road dust from his cloak for a long time."
                            }
                        },
                        {
                            "text": "Зимой мы часто <отрясаем> снег с обуви перед тем, как войти в дом.",
                            "target_lang_translations": {
                                "en": "In winter, we often <shake off> the snow from our shoes before entering the house."
                            }
                        }
                    ],
                    "synonyms": [
                        "стряхивать",
                        "сбивать",
                        "отряхивать"
                    ],
                    "common_phrases": [
                        "отрясать прах",
                        "отрясать снег",
                        "отрясать руки"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove something (dust, snow, drops) from a surface by a sharp movement or shaking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shake off",
                                "target_lang_sense_clarification": "to remove a substance by shaking"
                            },
                            {
                                "target_lang_word": "brush off",
                                "target_lang_sense_clarification": "to remove dust or dirt, often with a hand or brush"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f091f433-a795-3a3f-9993-1d379b13e1d0",
                    "sense_definition": "Освобождаться или избавляться от какого-либо гнетущего чувства, состояния или влияния.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Человек постепенно <отрясал> с себя остатки тяжёлого сна и возвращался к реальности.",
                            "target_lang_translations": {
                                "en": "The man gradually <shook off> the remnants of a heavy sleep and returned to reality."
                            }
                        },
                        {
                            "text": "Народ медленно, но верно <отрясал> вековое иго рабства и страха.",
                            "target_lang_translations": {
                                "en": "The people slowly but surely <cast off> the centuries-old yoke of slavery and fear."
                            }
                        }
                    ],
                    "synonyms": [
                        "сбрасывать",
                        "избавляться",
                        "отвергать"
                    ],
                    "common_phrases": [
                        "отрясать сон",
                        "отрясать иго",
                        "отрясать сомнения"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To free oneself or get rid of an oppressive feeling, state, or influence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shake off",
                                "target_lang_sense_clarification": "figurative; to get rid of a feeling or state"
                            },
                            {
                                "target_lang_word": "cast off",
                                "target_lang_sense_clarification": "literary; to discard a burden or yoke"
                            }
                        ]
                    }
                }
            ]
        }
    ]

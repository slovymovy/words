    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "959a1246-9945-3e47-adf8-9345e40c6f2d",
                    "sense_definition": "Een stuk grasland voor begrazing door vee.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er staan in die <w>wei</w> een paar geiten.",
                            "target_lang_translations": {
                                "en": "There are a few goats in that <w>pasture</w>.",
                                "ru": "На том <w>лугу</w> стоит несколько коз."
                            }
                        },
                        {
                            "text": "De koeien lopen de hele dag buiten in de <w>wei</w>.",
                            "target_lang_translations": {
                                "en": "The cows are out in the <w>pasture</w> all day.",
                                "ru": "Коровы весь день гуляют на <w>пастбище</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "weide"
                    ],
                    "common_phrases": [
                        "in de wei"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of grassland used for grazing livestock.",
                        "ru": "Участок земли, покрытый травой, используемый для выпаса скота."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pasture",
                                "target_lang_sense_clarification": "Land covered with grass and other low plants suitable for grazing animals"
                            },
                            {
                                "target_lang_word": "meadow",
                                "target_lang_sense_clarification": "A field of grass, often used for grazing or making hay"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пастбище",
                                "target_lang_sense_clarification": "место для выпаса скота"
                            },
                            {
                                "target_lang_word": "луг",
                                "target_lang_sense_clarification": "поле с травой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1b9bf14-fac3-3494-9d0b-6f8702d7fc76",
                    "sense_definition": "Een vloeistof die ontstaat als restproduct bij het kaasmaken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een van de nevenproducten van melk is <w>wei</w>.",
                            "target_lang_translations": {
                                "en": "One of the by-products of milk is <w>whey</w>.",
                                "ru": "Одним из побочных продуктов молока является <w>сыворотка</w>."
                            }
                        },
                        {
                            "text": "Zit er nog <w>wei</w> in de emmer?",
                            "target_lang_translations": {
                                "en": "Is there still <w>whey</w> in the bucket?",
                                "ru": "В ведре еще осталась <w>сыворотка</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "hui",
                        "melkwei"
                    ],
                    "target_lang_definitions": {
                        "en": "A liquid that remains as a by-product during cheese making.",
                        "ru": "Жидкость, остающаяся после свертывания молока при производстве сыра."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whey",
                                "target_lang_sense_clarification": "The watery part of milk that remains after the formation of curds"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сыворотка",
                                "target_lang_sense_clarification": "молочный продукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8e91a3c-9708-31b5-8545-ada62f856c5a",
                    "sense_definition": "Vloeibaar deel van het bloed dat overblijft na verwijdering van de bloedcellen en de stolstof.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De centrifuge scheidt het stolsel van de <w>wei</w>.",
                            "target_lang_translations": {
                                "en": "The centrifuge separates the clot from the <w>serum</w>.",
                                "ru": "Центрифуга отделяет сгусток от <w>сыворотки</w>."
                            }
                        },
                        {
                            "text": "In het laboratorium werd de <w>wei</w> onderzocht op antistoffen.",
                            "target_lang_translations": {
                                "en": "In the laboratory, the <w>serum</w> was examined for antibodies.",
                                "ru": "В лаборатории <w>сыворотку</w> исследовали на наличие антител."
                            }
                        }
                    ],
                    "synonyms": [
                        "serum",
                        "bloedserum"
                    ],
                    "target_lang_definitions": {
                        "en": "The liquid part of blood that remains after clotting.",
                        "ru": "Жидкая часть крови, остающаяся после удаления клеток крови и свертывающих веществ."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serum",
                                "target_lang_sense_clarification": "The clear liquid that can be separated from clotted blood"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сыворотка",
                                "target_lang_sense_clarification": "компонент крови"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "1591a469-d22e-3435-98e5-0888f5ae1399",
                    "sense_definition": "Wskazuje na miejsce znajdujące się wyżej od czegoś lub ruch w kierunku takiego miejsca.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "loc_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem słońce świeci wysoko <w>ponad</w> górami.",
                            "target_lang_translations": {
                                "ru": "Летом солнце светит высоко <w>над</w> горами."
                            }
                        },
                        {
                            "text": "Jaskółki latały nisko <w>ponad</w> dachami domów.",
                            "target_lang_translations": {
                                "ru": "Ласточки летали низко <w>над</w> крышами домов."
                            }
                        }
                    ],
                    "synonyms": [
                        "nad",
                        "powyżej"
                    ],
                    "antonyms": [
                        "pod",
                        "popod"
                    ],
                    "common_phrases": [
                        "ponad górami",
                        "ponad chmurami",
                        "ponad ziemią",
                        "ponad głowami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на место, находящееся выше чего-либо, или движение в направлении такого места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "над",
                                "target_lang_sense_clarification": "предлог; указывает на нахождение поверх или выше предмета"
                            },
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "на более высоком уровне, чем что-либо"
                            },
                            {
                                "target_lang_word": "поверх",
                                "target_lang_sense_clarification": "на поверхность чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88d74c2f-9c28-3e87-9ce0-42bba2dd9c55",
                    "sense_definition": "Oznacza ilość, liczbę lub czas większy niż wymieniony.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracuję w tym biurze <w>ponad</w> dwie dekady.",
                            "target_lang_translations": {
                                "ru": "Я работаю в этом офисе <w>более</w> двух десятилетий."
                            }
                        },
                        {
                            "text": "Dorosły wieloryb waży zazwyczaj <w>ponad</w> dwie tony.",
                            "target_lang_translations": {
                                "ru": "Взрослый кит обычно весит <w>свыше</w> двух тонн."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeszło",
                        "powyżej"
                    ],
                    "common_phrases": [
                        "ponad rok",
                        "ponad połowa",
                        "ponad sto",
                        "ponad dwie godziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Означает количество, число или время, превышающее указанное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "более",
                                "target_lang_sense_clarification": "употребляется при сравнении количества или времени"
                            },
                            {
                                "target_lang_word": "свыше",
                                "target_lang_sense_clarification": "больше какого-либо количества, нормы"
                            },
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "разговорный вариант для обозначения превышения количества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2927a103-ad2c-3fea-a5d3-25232957af80",
                    "sense_definition": "Wskazuje na przekroczenie pewnej miary, zakresu, możliwości lub stopnia intensywności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To zadanie jest zdecydowanie <w>ponad</w> moje umiejętności.",
                            "target_lang_translations": {
                                "ru": "Эта задача определенно <w>выше</w> моих способностей."
                            }
                        },
                        {
                            "text": "Ona kocha swoje dzieci <w>ponad</w> wszystko.",
                            "target_lang_translations": {
                                "ru": "Она любит своих детей <w>больше</w> всего."
                            }
                        }
                    ],
                    "synonyms": [
                        "powyżej",
                        "nad"
                    ],
                    "common_phrases": [
                        "ponad siły",
                        "ponad wszystko",
                        "ponad miarę",
                        "ponad podziały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на превышение определенной меры, диапазона, возможностей или степени интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "превосходя уровень, возможности или понимание"
                            },
                            {
                                "target_lang_word": "сверх",
                                "target_lang_sense_clarification": "более того, что ожидалось или требуется"
                            },
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "в значении степени (напр. любить больше всего)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
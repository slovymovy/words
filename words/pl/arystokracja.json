    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "82877510-73ed-3a60-a526-63bc79c08f22",
                    "sense_definition": "Najwyższa warstwa społeczna, do której przynależy się zazwyczaj ze względu na urodzenie i posiadany majątek.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "grupa_ludzi",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawna <w>arystokracja</w> mieszkała w pięknych pałacach i dworach.",
                            "target_lang_translations": {
                                "ru": "Былая <w>аристократия</w> жила в прекрасных дворцах и усадьбах."
                            }
                        },
                        {
                            "text": "Polska <w>arystokracja</w> rodowa dbała o zachowanie narodowych tradycji.",
                            "target_lang_translations": {
                                "ru": "Польская родовая <w>аристократия</w> заботилась о сохранении национальных традиций."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnateria",
                        "możnowładztwo",
                        "jaśniepaństwo"
                    ],
                    "common_phrases": [
                        "arystokracja rodowa",
                        "polska arystokracja",
                        "salonowa arystokracja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высший общественный слой, принадлежность к которому обычно определяется происхождением и богатством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аристократия",
                                "target_lang_sense_clarification": "высшее сословие"
                            },
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "родовитое дворянство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56ac97ca-fe8d-3b0a-98dc-fcce23d1d523",
                    "sense_definition": "Elitarna grupa ludzi wyróżniająca się w społeczeństwie wybitnymi cechami, talentem lub pozycją w danej dziedzinie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "grupa_ludzi",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To stowarzyszenie naukowe to prawdziwa <w>arystokracja</w> ducha i intelektu.",
                            "target_lang_translations": {
                                "ru": "Это научное общество — настоящая <w>аристократия</w> духа и интеллекта."
                            }
                        },
                        {
                            "text": "Wśród pracowników fabryki wyłoniła się tak zwana <w>arystokracja</w> robotnicza.",
                            "target_lang_translations": {
                                "ru": "Среди работников фабрики выделилась так называемая рабочая <w>аристократия</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arystokracja ducha",
                        "arystokracja pieniądza",
                        "arystokracja robotnicza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элитная группа людей, выделяющаяся в обществе выдающимися качествами, талантом или положением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аристократия",
                                "target_lang_sense_clarification": "элита, избранные"
                            },
                            {
                                "target_lang_word": "элита",
                                "target_lang_sense_clarification": "лучшие представители"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce1f54eb-58f1-3679-aa44-2e41ac6f63ea",
                    "sense_definition": "Ustrój polityczny, w którym władzę sprawuje uprzywilejowana mniejszość (elita).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ustroj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytna <w>arystokracja</w> była uważana przez filozofów za rządy najlepszych obywateli.",
                            "target_lang_translations": {
                                "ru": "Древняя <w>аристократия</w> считалась философами правлением лучших граждан."
                            }
                        },
                        {
                            "text": "W tym państwie przez wiele lat panowała <w>arystokracja</w>, zanim wprowadzono demokrację.",
                            "target_lang_translations": {
                                "ru": "В этом государстве много лет господствовала <w>аристократия</w>, прежде чем была введена демократия."
                            }
                        }
                    ],
                    "antonyms": [
                        "oligarchia"
                    ],
                    "common_phrases": [
                        "rządy arystokracji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма государственного правления, при которой власть принадлежит привилегированному меньшинству (элите)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аристократия",
                                "target_lang_sense_clarification": "форма правления"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "widzieć",
        "widok",
        "widzenie",
        "widoczny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c9a0f6d2-6da7-3792-a69f-72315195be59",
                    "sense_definition": "Dostrzec kogoś lub coś za pomocą wzroku; zarejestrować obraz oczami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <zobaczyć> ten piękny krajobraz na własne oczy.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <увидеть> этот красивый пейзаж своими глазами."
                            }
                        },
                        {
                            "text": "Czy możesz <zobaczyć>, kto stoi przy bramie?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <посмотреть>, кто стоит у ворот?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ujrzeć",
                        "dostrzec",
                        "dostrzec"
                    ],
                    "common_phrases": [
                        "zobaczyć na własne oczy",
                        "zobaczyć świat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увидеть кого-то или что-то с помощью зрения; зафиксировать образ глазами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увидеть",
                                "target_lang_sense_clarification": "воспринять зрительно"
                            },
                            {
                                "target_lang_word": "заметить",
                                "target_lang_sense_clarification": "обратить внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75fc5505-50f6-3bc7-ab10-789602a22f72",
                    "sense_definition": "Obejrzeć coś, na przykład film, wystawę lub przedstawienie teatralne.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "watching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W weekend planujemy <zobaczyć> nowy film w kinie.",
                            "target_lang_translations": {
                                "ru": "В выходные мы планируем <посмотреть> новый фильм в кинотеатре."
                            }
                        },
                        {
                            "text": "Musisz koniecznie <zobaczyć> tę wystawę w muzeum.",
                            "target_lang_translations": {
                                "ru": "Ты обязательно должен <посмотреть> эту выставку в музее."
                            }
                        }
                    ],
                    "synonyms": [
                        "obejrzeć"
                    ],
                    "common_phrases": [
                        "zobaczyć film",
                        "zobaczyć wystawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посмотреть что-то, например фильм, выставку или театральное представление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "фильм, спектакль и т.д."
                            },
                            {
                                "target_lang_word": "увидеть",
                                "target_lang_sense_clarification": "ознакомиться зрительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "650faab9-2fc0-3681-a6ae-d42e7eb7038b",
                    "sense_definition": "Spotkać się z kimś, spędzić z kimś czas (używane w formie zwrotnej 'zobaczyć się').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że wkrótce się <zobaczymy>.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, что скоро мы <увидимся>."
                            }
                        },
                        {
                            "text": "Muszę się z tobą <zobaczyć>, żeby omówić szczegóły.",
                            "target_lang_translations": {
                                "ru": "Мне нужно с тобой <увидеться>, чтобы обсудить детали."
                            }
                        }
                    ],
                    "synonyms": [
                        "spotkać się"
                    ],
                    "common_phrases": [
                        "do zobaczenia",
                        "miło cię zobaczyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встретиться с кем-то, провести с кем-то время (используется в возвратной форме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увидеться",
                                "target_lang_sense_clarification": "встретиться лично"
                            },
                            {
                                "target_lang_word": "встретиться",
                                "target_lang_sense_clarification": "собраться вместе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "896b1323-d7c1-32b7-9126-2ef8040f7231",
                    "sense_definition": "Sprawdzić stan czegoś, przekonać się o czymś lub dowiedzieć się, jak wygląda sytuacja.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "checking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pójdę <zobaczyć>, czy dzieci już śpią.",
                            "target_lang_translations": {
                                "ru": "Пойду <посмотрю>, спят ли уже дети."
                            }
                        },
                        {
                            "text": "Musimy <zobaczyć>, co da się zrobić w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Мы должны <посмотреть>, что можно сделать в этом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "spotkać się"
                    ],
                    "common_phrases": [
                        "poczekamy-zobaczymy",
                        "zobaczymy co da się zrobić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверить состояние чего-либо, убедиться в чем-то или узнать ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "проверить, выяснить"
                            },
                            {
                                "target_lang_word": "проверить",
                                "target_lang_sense_clarification": "удостовериться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0a475ee-20c2-3010-b124-430259fb9096",
                    "sense_definition": "Ujrzeć własne odbicie, na przykład w lustrze (używane w formie zwrotnej 'zobaczyć się').",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "reflection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spojrzała w lustro, aby się <zobaczyć> w nowej sukience.",
                            "target_lang_translations": {
                                "ru": "Она посмотрела в зеркало, чтобы <увидеть себя> в новом платье."
                            }
                        },
                        {
                            "text": "Możesz się <zobaczyć> w odbiciu w szybie.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <увидеть себя> в отражении в стекле."
                            }
                        }
                    ],
                    "synonyms": [
                        "ujrzeć się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увидеть свое отражение, например, в зеркале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увидеть себя",
                                "target_lang_sense_clarification": "в отражении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
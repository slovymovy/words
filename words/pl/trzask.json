            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6379110-51d6-32ac-8f69-52fdbaad75f1",
                    "sense_definition": "Krótki, głośny i ostry dźwięk, powstający zazwyczaj przy uderzeniu, pękaniu lub gwałtownym zamykaniu czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyszła z pokoju zdenerwowana i zamknęła drzwi z głośnym <trzaskiem>.",
                            "target_lang_translations": {
                                "ru": "Она вышла из комнаты расстроенная и закрыла дверь с громким <хлопком>."
                            }
                        },
                        {
                            "text": "W nocy obudził nas nagły <trzask> łamanej gałęzi za oknem.",
                            "target_lang_translations": {
                                "ru": "Ночью нас разбудил внезапный <треск> ломающейся ветки за окном."
                            }
                        }
                    ],
                    "synonyms": [
                        "huk",
                        "stuk",
                        "łoskot"
                    ],
                    "antonyms": [
                        "cisza"
                    ],
                    "common_phrases": [
                        "z trzaskiem",
                        "trzask drzwi",
                        "trzask bicza",
                        "głośny trzask"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий, громкий и резкий звук, возникающий обычно при ударе, разломе или резком закрывании чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "треск",
                                "target_lang_sense_clarification": "звук ломающегося предмета"
                            },
                            {
                                "target_lang_word": "хлопок",
                                "target_lang_sense_clarification": "звук удара или закрытия двери"
                            },
                            {
                                "target_lang_word": "грохот",
                                "target_lang_sense_clarification": "очень громкий звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52f70a53-f40b-3f25-aea7-93575d3b079c",
                    "sense_definition": "Seria cichych, suchych odgłosów, słyszalna na przykład podczas palenia się drewna lub jako zakłócenia w urządzeniach elektronicznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_continuous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem siedzieliśmy przy kominku, słuchając przyjemnego <trzasku> ognia.",
                            "target_lang_translations": {
                                "ru": "Вечером мы сидели у камина, слушая приятный <треск> огня."
                            }
                        },
                        {
                            "text": "Przez ciągłe <trzaski> w radiu nie mogliśmy usłyszeć wiadomości.",
                            "target_lang_translations": {
                                "ru": "Из-за постоянного <треска> в радио мы не могли услышать новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "trzeszczenie",
                        "skwierczenie"
                    ],
                    "common_phrases": [
                        "trzask ognia",
                        "trzaski w radiu",
                        "trzaski na linii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия тихих, сухих звуков, слышимая, например, при горении дерева или как помехи в электронных устройствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "треск",
                                "target_lang_sense_clarification": "звук огня или радиопомех"
                            },
                            {
                                "target_lang_word": "потрескивание",
                                "target_lang_sense_clarification": "легкий, продолжительный звук"
                            },
                            {
                                "target_lang_word": "помехи",
                                "target_lang_sense_clarification": "шум в эфире"
                            }
                        ]
                    }
                }
            ]
        }
    ]

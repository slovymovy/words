            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "73024515-ac5b-3f85-a77d-3f9eebe5dc23",
                    "sense_definition": "Wulgarny przymiotnik używany do wyrażenia silnych negatywnych emocji, takich jak gniew, irytacja czy pogarda; często pełni funkcję wzmacniającą wypowiedź.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "negative_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>jebany</w> samochód znowu nie chce odpalić, a spieszę się do pracy.",
                            "target_lang_translations": {
                                "ru": "Эта <w>ёбаная</w> машина опять не заводится, а я спешу на работу."
                            }
                        },
                        {
                            "text": "Zabieraj te swoje <w>jebane</w> graty i wynoś się z mojego domu.",
                            "target_lang_translations": {
                                "ru": "Забирай это своё <w>сраное</w> барахло и убирайся из моего дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "pierdolony",
                        "kurewski",
                        "cholerny",
                        "nieszczęsny"
                    ],
                    "common_phrases": [
                        "ten jebany świat",
                        "jebany pech",
                        "jebany hałas"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wulgarny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное прилагательное, используемое для выражения сильных негативных эмоций, таких как гнев, раздражение или презрение; часто выполняет функцию усилителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ёбаный",
                                "target_lang_sense_clarification": "проклятый, чёртов (вульг.)"
                            },
                            {
                                "target_lang_word": "сраный",
                                "target_lang_sense_clarification": "никчёмный, плохой (вульг.)"
                            },
                            {
                                "target_lang_word": "блядский",
                                "target_lang_sense_clarification": "отвратительный (вульг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be2ca2dd-156c-347b-9e47-f638b0653f03",
                    "sense_definition": "Wulgarnie o kimś lub czymś, co wzbudza podziw, uznanie lub zazdrość; często używane z niedowierzaniem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "positive_admiration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale on jest <w>jebany</w>, naprawił ten silnik w piętnaście minut.",
                            "target_lang_translations": {
                                "ru": "Ну он и <w>сукин сын</w>, починил этот двигатель за пятнадцать минут."
                            }
                        },
                        {
                            "text": "Ten <w>jebany</w> magik sprawił, że karta zniknęła na moich oczach.",
                            "target_lang_translations": {
                                "ru": "Этот <w>чёртов</w> фокусник сделал так, что карта исчезла на моих глазах."
                            }
                        }
                    ],
                    "synonyms": [
                        "skubany"
                    ],
                    "common_phrases": [
                        "o jebany"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgarny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное выражение о ком-то или чём-то, что вызывает восхищение, признание или зависть; часто используется с оттенком недоверия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хитровыебанный",
                                "target_lang_sense_clarification": "хитрый, ловкий, сложный (вульг.)"
                            },
                            {
                                "target_lang_word": "чёртов",
                                "target_lang_sense_clarification": "усилитель восхищения (напр. чёртов гений)"
                            },
                            {
                                "target_lang_word": "сукин",
                                "target_lang_sense_clarification": "в выражениях типа 'сукин сын' (одобрительно)"
                            }
                        ]
                    }
                }
            ]
        },

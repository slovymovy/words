{
    "word_family": [
        "graagte"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ff16143d-a75e-3b2c-958a-118231829609",
                    "sense_definition": "Met plezier; wordt gebruikt om aan te geven dat men iets leuk vindt om te doen of wenst.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "desire_pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik loop <graag> op het strand in de zomer.",
                            "target_lang_translations": {
                                "en": "I <like to> walk on the beach in the summer."
                            }
                        },
                        {
                            "text": "Mijn moeder past <graag> op haar kleinkinderen.",
                            "target_lang_translations": {
                                "en": "My mother <likes to> look after her grandchildren."
                            }
                        },
                        {
                            "text": "Ik wil <graag> een afspraak maken voor morgen.",
                            "target_lang_translations": {
                                "en": "I <would like> to make an appointment for tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaarne"
                    ],
                    "common_phrases": [
                        "graag gedaan",
                        "ik wil graag",
                        "heel graag",
                        "graag of niet"
                    ],
                    "target_lang_definitions": {
                        "en": "With pleasure; used to indicate that one enjoys doing something or desires something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "like to",
                                "target_lang_sense_clarification": "Used to indicate enjoyment of an action (e.g., 'ik lees graag' = 'I like to read')"
                            },
                            {
                                "target_lang_word": "would like",
                                "target_lang_sense_clarification": "Used in the phrase 'ik wil graag' to express a polite wish"
                            },
                            {
                                "target_lang_word": "gladly",
                                "target_lang_sense_clarification": "Doing something with pleasure or willingness"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "320142a8-9dd2-33b2-9bc3-632df581f49e",
                    "sense_definition": "Een beleefd antwoord om een aanbod te accepteren; ja, alstublieft.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "desire_pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"Wilt u nog een kopje thee?\" \"Ja, <graag>.\"",
                            "target_lang_translations": {
                                "en": "\"Would you like another cup of tea?\" \"Yes, <please>.\""
                            }
                        },
                        {
                            "text": "\"Zal ik je even helpen met die zware tas?\" \"Heel <graag>!\"",
                            "target_lang_translations": {
                                "en": "\"Shall I help you with that heavy bag?\" \"Yes <please>!\""
                            }
                        }
                    ],
                    "synonyms": [
                        "gaarne"
                    ],
                    "common_phrases": [
                        "ja graag"
                    ],
                    "target_lang_definitions": {
                        "en": "A polite response to accept an offer; yes, please."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "please",
                                "target_lang_sense_clarification": "Used to politely accept an offer, usually following 'yes'"
                            },
                            {
                                "target_lang_word": "gladly",
                                "target_lang_sense_clarification": "Used to accept an offer with enthusiasm"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "db52f275-f79a-314b-b738-3330d36b835b",
                    "sense_definition": "Gretig; met een sterk verlangen om iets te hebben of te doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "desire_pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een <grage> gast op onze wekelijkse bijeenkomsten.",
                            "target_lang_translations": {
                                "en": "He is an <eager> guest at our weekly meetings."
                            }
                        },
                        {
                            "text": "De nieuwe roman vond <grage> aftrek bij het lezende publiek.",
                            "target_lang_translations": {
                                "en": "The new novel found <eager> buyers among the reading public."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Eager; showing a strong desire to have or do something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eager",
                                "target_lang_sense_clarification": "Showing strong desire or interest"
                            },
                            {
                                "target_lang_word": "willing",
                                "target_lang_sense_clarification": "Ready and happy to do something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
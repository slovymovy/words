    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5e21292f-4467-3ae4-b0d7-8aa11e5fdef6",
                    "sense_definition": "Относящийся к общему объёму продукции или дохода, произведённому за определённый период, взятому целиком без вычета расходов.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом году <w>валовой</w> внутренний продукт страны значительно вырос.",
                            "target_lang_translations": {
                                "en": "This year the country's <w>gross</w> domestic product increased significantly."
                            }
                        },
                        {
                            "text": "Компания рассчитывает получить высокую <w>валовую</w> прибыль по итогам квартала.",
                            "target_lang_translations": {
                                "en": "The company expects to receive a high <w>gross</w> profit at the end of the quarter."
                            }
                        }
                    ],
                    "synonyms": [
                        "общий",
                        "совокупный"
                    ],
                    "common_phrases": [
                        "валовой внутренний продукт",
                        "валовой доход",
                        "валовая прибыль",
                        "валовая продукция"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the total volume of production or income produced over a certain period, taken as a whole without deduction of expenses."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gross",
                                "target_lang_sense_clarification": "economic/financial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "885b74ff-fce8-3ade-a50b-c91fe3921bce",
                    "sense_definition": "Охватывающий множество объектов или всё пространство; массовый, сплошной.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осенью начинается <w>валовой</w> пролёт перелётных птиц на юг.",
                            "target_lang_translations": {
                                "en": "In autumn, the <w>mass</w> migration of birds to the south begins."
                            }
                        },
                        {
                            "text": "Это был не единичный случай, а <w>валовой</w> поток жалоб от клиентов.",
                            "target_lang_translations": {
                                "en": "It was not an isolated case, but a <w>mass</w> stream of complaints from customers."
                            }
                        }
                    ],
                    "synonyms": [
                        "массовый",
                        "сплошной",
                        "повсеместный"
                    ],
                    "antonyms": [
                        "единичный"
                    ],
                    "common_phrases": [
                        "валовой пролёт"
                    ],
                    "target_lang_definitions": {
                        "en": "Covering many objects or the entire space; mass, continuous."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mass",
                                "target_lang_sense_clarification": "large scale/quantity"
                            },
                            {
                                "target_lang_word": "wholesale",
                                "target_lang_sense_clarification": "indiscriminate/total"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "b07d1d57-3c80-366e-9507-96b97c17539e",
                    "sense_definition": "Zaimek oznaczający brak jakiejkolwiek rzeczy, sprawy lub zjawiska; używany w zdaniach przeczących.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam <w>nic</w> do powiedzenia w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Мне <w>нечего</w> сказать по этому делу."
                            }
                        },
                        {
                            "text": "W lodówce nie ma <w>nic</w> do jedzenia.",
                            "target_lang_translations": {
                                "ru": "В холодильнике нет <w>ничего</w> поесть."
                            }
                        }
                    ],
                    "antonyms": [

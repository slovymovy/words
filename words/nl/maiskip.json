                {
                    "sense_id": "7ba2e545-42fd-36c6-9cac-fa03e1c957fc",
                    "sense_definition": "Een kip die voornamelijk met mais is gevoerd, wat zorgt voor een gele huidskleur.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De slager verkoopt een hele <w>maiskip</w> voor in de oven.",
                            "target_lang_translations": {
                                "ru": "Мясник продает целую <w>курицу кукурузного откорма</w> для запекания в духовке."
                            }
                        },
                        {
                            "text": "Een <w>maiskip</w> heeft vaak een iets vollere smaak dan een gewone kip.",
                            "target_lang_translations": {
                                "ru": "<w>Курица кукурузного откорма</w> часто имеет более насыщенный вкус, чем обычная курица."
                            }
                        }
                    ],
                    "synonyms": [
                        "maïskip"
                    ],
                    "common_phrases": [
                        "hele maiskip",
                        "gebraden maiskip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Курица, которую кормили преимущественно кукурузой, что придает её коже желтый оттенок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курица кукурузного откорма",
                                "target_lang_sense_clarification": "птица"

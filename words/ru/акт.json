                    ],
                    "target_lang_definitions": {
                        "en": "A solemn assembly in educational institutions regarding graduation, awards, or other important events."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ceremony",
                                "target_lang_sense_clarification": "formal event"
                            },
                            {
                                "target_lang_word": "assembly",
                                "target_lang_sense_clarification": "gathering"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "6e535076-acfd-30b1-b6d9-be02d10743db",
                    "sense_definition": "A green chemical compound, iron(II) sulfate, historically used in making ink and dyes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sulfate_compounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manufacturer used <w>copperas</w> to fix the dye into the cloth.",
                            "target_lang_translations": {
                                "ru": "Производитель использовал <w>железный купорос</w>, чтобы закрепить краску на ткани."
                            }
                        },
                        {
                            "text": "Traditional ink is made by combining oak galls and green <w>copperas</w>.",
                            "target_lang_translations": {
                                "ru": "Традиционные чернила изготавливают, смешивая дубовые галлы и зелёный <w>купорос</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "green copperas",
                        "copperas water"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зеленое химическое соединение, сульфат железа(II), исторически использовавшееся при изготовлении чернил и красителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железный купорос",
                                "target_lang_sense_clarification": "сульфат железа"
                            },
                            {
                                "target_lang_word": "зелёный купорос",
                                "target_lang_sense_clarification": "историческое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b418576-deb7-347e-a0c7-bc12d45a7bcc",
                    "sense_definition": "An obsolete term for various sulfate compounds containing zinc, copper, or iron.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sulfate_compounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chemists once referred to zinc sulfate as white <w>copperas</w>.",
                            "target_lang_translations": {
                                "ru": "Химики когда-то называли сульфат цинка белым <w>купоросом</w>."
                            }
                        },
                        {
                            "text": "This type of <w>copperas</w> was known for its medicinal properties.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>купороса</w> был известен своими лечебными свойствами."
                            }
                        }
                    ],
                    "synonyms": [
                        "vitriol"
                    ],
                    "common_phrases": [
                        "blue copperas",
                        "white copperas"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "chemical terminology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревший термин для различных сульфатных соединений, содержащих цинк, медь или железо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купорос",
                                "target_lang_sense_clarification": "общее название сульфатов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "54b44b19-6d1b-3ca6-b8f7-1f47d4cf89c4",
                    "sense_definition": "A person who is considered mean, despicable, or very rude.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some <w>bastard</w> scratched my car with a key while I was shopping.",
                            "target_lang_translations": {
                                "ru": "Какая-то <w>сволочь</w> поцарапала мою машину ключом, пока я ходил по магазинам."
                            }
                        },
                        {
                            "text": "Don't be such a <w>bastard</w> and help your brother with his homework.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>гадом</w> и помоги брату с домашним заданием."
                            }
                        }
                    ],
                    "synonyms": [
                        "scumbag",
                        "jerk",
                        "swine",
                        "prick",
                        "git"
                    ],
                    "common_phrases": [

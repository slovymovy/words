                {
                    "sense_id": "9f391c5a-5e39-3df5-96da-50c891261f10",
                    "sense_definition": "The quality of being easily offended, upset, or aware of other people's feelings.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <sensitivity> to criticism makes it difficult to give her honest feedback.",
                            "target_lang_translations": {
                                "ru": "Её <чувствительность> к критике мешает давать ей честную обратную связь."
                            }
                        },
                        {
                            "text": "The manager handled the personal issue with great <sensitivity> and discretion.",
                            "target_lang_translations": {
                                "ru": "Менеджер решил личный вопрос с большой <чуткостью> и тактом."
                            }
                        }
                    ],
                    "synonyms": [
                        "touchiness",
                        "tact"
                    ],
                    "antonyms": [
                        "insensitivity"
                    ],
                    "common_phrases": [
                        "cultural sensitivity",
                        "sensitivity training",
                        "extreme sensitivity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство личности, выражающееся в легкой обидчивости, ранимости или способности тонко чувствовать переживания других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительность",
                                "target_lang_sense_clarification": "эмоциональная восприимчивость"
                            },
                            {
                                "target_lang_word": "чуткость",
                                "target_lang_sense_clarification": "внимание к чувствам других"
                            },
                            {
                                "target_lang_word": "впечатлительность",
                                "target_lang_sense_clarification": "легкая возбудимость чувств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcc2323e-fab5-3ea3-bdc4-373d37e0e793",
                    "sense_definition": "The physical ability of a body or organism to react to stimuli such as light, sound, or touch.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some people develop a <sensitivity> to certain foods as they get older.",
                            "target_lang_translations": {
                                "ru": "У некоторых людей с возрастом развивается <чувствительность> к определенным продуктам."
                            }
                        },
                        {
                            "text": "The eye drops may cause temporary light <sensitivity>.",
                            "target_lang_translations": {
                                "ru": "Глазные капли могут вызвать временную <чувствительность> к свету."
                            }
                        }
                    ],
                    "synonyms": [
                        "reactivity",
                        "susceptibility"
                    ],
                    "antonyms": [
                        "insensitivity"
                    ],
                    "common_phrases": [
                        "light sensitivity",
                        "skin sensitivity",
                        "tooth sensitivity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физиологическая способность организма реагировать на внешние раздражители, такие как свет, звук или прикосновение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительность",
                                "target_lang_sense_clarification": "реакция на раздражители"
                            },
                            {
                                "target_lang_word": "восприимчивость",
                                "target_lang_sense_clarification": "подверженность воздействию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b90db84-f799-370d-9754-1805364168ed",
                    "sense_definition": "The degree to which a device or instrument detects and reacts to small changes.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can adjust the mouse <sensitivity> in the computer settings.",
                            "target_lang_translations": {
                                "ru": "Вы можете настроить <чувствительность> мыши в параметрах компьютера."
                            }
                        },
                        {
                            "text": "The high <sensitivity> of the microphone allowed it to record distant whispers.",
                            "target_lang_translations": {
                                "ru": "Высокая <чувствительность> микрофона позволила записать отдаленный шепот."
                            }
                        }
                    ],
                    "synonyms": [
                        "responsiveness",
                        "precision"
                    ],
                    "common_phrases": [
                        "high sensitivity",
                        "sensor sensitivity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень, в которой техническое устройство или прибор обнаруживает малейшие изменения и реагирует на них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительность",
                                "target_lang_sense_clarification": "характеристика прибора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31563754-1eab-3344-8888-b3c3e6a0cacc",
                    "sense_definition": "The probability that a test correctly identifies a positive result or condition.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "statistical_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new medical test has high <sensitivity> but low specificity.",
                            "target_lang_translations": {
                                "ru": "Новый медицинский тест имеет высокую <чувствительность>, но низкую специфичность."
                            }
                        },
                        {
                            "text": "Researchers aim to improve the <sensitivity> of the screening process to avoid missing cases.",
                            "target_lang_translations": {
                                "ru": "Исследователи стремятся повысить <чувствительность> процесса скрининга, чтобы не упустить ни одного случая."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "statistics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вероятность того, что диагностический тест правильно выявит наличие искомого заболевания или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствительность",
                                "target_lang_sense_clarification": "статистический показатель точности"

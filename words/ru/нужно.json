    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8ac04567-efa5-3ccc-85e1-2cbefb8d79c0",
                    "sense_definition": "Необходимо совершить какое-либо действие; следует поступить определённым образом.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "necessity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне срочно <w>нужно</w> позвонить домой.",
                            "target_lang_translations": {
                                "en": "I urgently <w>need</w> to call home."
                            }
                        },
                        {
                            "text": "Тебе <w>нужно</w> больше отдыхать и гулять.",
                            "target_lang_translations": {
                                "en": "You <w>need</w> to rest and walk more."
                            }
                        }
                    ],
                    "synonyms": [
                        "надо",
                        "необходимо",
                        "следует",
                        "требуется"
                    ],
                    "common_phrases": [
                        "нужно сделать",
                        "нужно идти",
                        "так нужно",
                        "очень нужно"
                    ],
                    "target_lang_definitions": {
                        "en": "It is necessary to perform an action; one should act in a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "need to",
                                "target_lang_sense_clarification": "indicates necessity or obligation to do something"
                            },
                            {
                                "target_lang_word": "have to",
                                "target_lang_sense_clarification": "implies a strong necessity or external obligation"
                            },
                            {
                                "target_lang_word": "it is necessary",
                                "target_lang_sense_clarification": "formal expression of necessity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d578b143-c54c-3a91-867d-2f260f6439b6",
                    "sense_definition": "Требуется наличие кого-либо или чего-либо (обычно в сочетании с существительными в родительном падеже или среднего рода).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "necessity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для этого рецепта <w>нужно</w> три яйца.",
                            "target_lang_translations": {
                                "en": "Three eggs are <w>needed</w> for this recipe."
                            }
                        },
                        {
                            "text": "Нам <w>нужно</w> немного времени для подготовки.",
                            "target_lang_translations": {
                                "en": "We <w>need</w> a little time for preparation."
                            }
                        }
                    ],
                    "synonyms": [
                        "надо",
                        "необходимо",
                        "требуется"
                    ],
                    "common_phrases": [
                        "ничего не нужно",
                        "сколько нужно",
                        "если нужно"
                    ],
                    "target_lang_definitions": {
                        "en": "The presence of someone or something is required."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "need",
                                "target_lang_sense_clarification": "to require something"
                            },
                            {
                                "target_lang_word": "require",
                                "target_lang_sense_clarification": "formal synonym for need"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f48dd308-278e-34ee-908d-c850ad087b8c",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>нужный</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это лекарство было <w>нужно</w> больному.",
                            "target_lang_translations": {
                                "en": "This medicine was <w>necessary</w> for the patient."
                            }
                        },
                        {
                            "text": "Твоё мнение здесь никому не <w>нужно</w>.",
                            "target_lang_translations": {
                                "en": "Your opinion is not <w>needed</w> by anyone here."
                            }

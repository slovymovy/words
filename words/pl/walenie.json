    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80b58701-369a-3992-acf4-6ec48ee8527f",
                    "sense_definition": "Rzeczownik odczasownikowy od słowa <w>walić</w>, oznaczający czynność mocnego uderzania w coś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_hitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągłe <w>walenie</w> młotkiem w ścianę nie dawało mi spać.",
                            "target_lang_translations": {
                                "ru": "Постоянный <w>стук</w> молотком в стену не давал мне спать."
                            }
                        },
                        {
                            "text": "Jego głośne <w>walenie</w> w drzwi obudziło wszystkich domowników.",
                            "target_lang_translations": {
                                "ru": "Его громкий <w>стук</w> в дверь разбудил всех домочадцев."
                            }
                        }
                    ],
                    "antonyms": [
                        "niewalenie"
                    ],
                    "common_phrases": [
                        "walenie w drzwi",
                        "walenie serca",
                        "walenie głową w mur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от слова <w>walić</w>, обозначающее действие сильного удара по чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стук",
                                "target_lang_sense_clarification": "в дверь, по стене"
                            },
                            {
                                "target_lang_word": "удары",
                                "target_lang_sense_clarification": "нанесение ударов"
                            },
                            {
                                "target_lang_word": "биение",
                                "target_lang_sense_clarification": "сердца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fdfb21d-591b-3ac1-a642-57f80e55f3dc",
                    "sense_definition": "Odgłos powstający podczas głośnego uderzania lub stukania w coś twardego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_hitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nocy usłyszałem dziwne <w>walenie</w> dochodzące z piwnicy.",
                            "target_lang_translations": {
                                "ru": "Ночью я услышал странный <w>грохот</w>, доносящийся из подвала."
                            }
                        },
                        {
                            "text": "To rytmiczne <w>walenie</w> jest bardzo irytujące i przeszkadza w pracy.",
                            "target_lang_translations": {
                                "ru": "Этот ритмичный <w>стук</w> очень раздражает и мешает работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "łomotanie",
                        "głośne stukanie",
                        "głośne pukanie"
                    ],
                    "common_phrases": [
                        "głośne walenie",
                        "rytmiczne walenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, возникающий при громком ударе или стуке о что-то твердое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грохот",
                                "target_lang_sense_clarification": "громкий звук"
                            },
                            {
                                "target_lang_word": "стук",
                                "target_lang_sense_clarification": "звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0c556f9-6e1f-3258-95b4-c42a54cc6d23",
                    "sense_definition": "Forma liczby mnogiej słowa <w>waleń</w> (duży ssak morski).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym oceanarium można oglądać ogromne <w>walenie</w> pływające w basenie.",
                            "target_lang_translations": {
                                "ru": "В этом океанариуме можно увидеть огромных <w>китообразных</w>, плавающих в бассейне."
                            }
                        },
                        {
                            "text": "Na plażę zostały wyrzucone dwa martwe <w>walenie</w>.",
                            "target_lang_translations": {
                                "ru": "На пляж выбросило двух мертвых <w>китообразных</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of waleń"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма множественного числа слова <w>waleń</w> (крупное морское млекопитающее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китообразные",
                                "target_lang_sense_clarification": "животные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f4e8325-a1af-3d3e-9a51-f0d06e377e96",
                    "sense_definition": "Rząd ssaków żyjących wyłącznie w wodzie, do którego należą m.in. wieloryby i delfiny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Walenie</w> są ssakami, które całkowicie przystosowały się do życia w środowisku wodnym.",
                            "target_lang_translations": {
                                "ru": "<w>Китообразные</w> — это млекопитающие, которые полностью приспособились к жизни в водной среде."
                            }
                        },
                        {
                            "text": "Wiele gatunków zaliczanych do rzędu <w>waleni</w> jest obecnie zagrożonych wyginięciem.",
                            "target_lang_translations": {
                                "ru": "Многие виды, относящиеся к отряду <w>китообразных</w>, в настоящее время находятся под угрозой исчезновения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rząd waleni",
                        "ochrona waleni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отряд млекопитающих, живущих исключительно в воде, к которому относятся, в частности, киты и дельфины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китообразные",
                                "target_lang_sense_clarification": "отряд"
                            }
                        ]
                    }
                }
            ]

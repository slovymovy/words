{
    "word_family": [
        "installeren",
        "installateur"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4805950d-e1a6-31eb-b141-2c72ec9069f4",
                    "sense_definition": "Het proces van het zetten van een computerprogramma op een computer zodat het gebruikt kan worden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>installatie</w> van de nieuwe update duurde slechts enkele minuten.",
                            "target_lang_translations": {
                                "ru": "<w>Установка</w> нового обновления заняла всего несколько минут."
                            }
                        },
                        {
                            "text": "Je moet de computer opnieuw opstarten na de <w>installatie</w> van het programma.",
                            "target_lang_translations": {
                                "ru": "Вам нужно перезагрузить компьютер после <w>установки</w> программы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schone installatie",
                        "installatie voltooien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс установки компьютерной программы на компьютер для её использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "программного обеспечения"
                            },
                            {
                                "target_lang_word": "инсталляция",
                                "target_lang_sense_clarification": "компьютерный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6de70a4-8955-33a6-a72f-c0d416461737",
                    "sense_definition": "Het plaatsen, aansluiten en gebruiksklaar maken van apparatuur of technische systemen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor de <w>installatie</w> van de zonnepanelen hebben we een vakman ingehuurd.",
                            "target_lang_translations": {
                                "ru": "Для <w>монтажа</w> солнечных панелей мы наняли специалиста."
                            }
                        },
                        {
                            "text": "De <w>installatie</w> van de nieuwe keuken nam twee dagen in beslag.",
                            "target_lang_translations": {
                                "ru": "<w>Установка</w> новой кухни заняла два дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "inrichting",
                        "inwijding"
                    ],
                    "common_phrases": [
                        "technische installatie",
                        "installatie van apparatuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размещение, подключение и подготовка к работе оборудования или технических систем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "размещение оборудования"
                            },
                            {
                                "target_lang_word": "монтаж",
                                "target_lang_sense_clarification": "сборка и подключение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a968ef5-aa04-33a3-b3a8-c5e1dd70a280",
                    "sense_definition": "Een samenhangend geheel van apparaten, machines of leidingen die een bepaalde functie vervullen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De elektrische <w>installatie</w> in het oude huis moest volledig worden vervangen.",
                            "target_lang_translations": {
                                "ru": "Электрическая <w>проводка</w> в старом доме подлежала полной замене."
                            }
                        },
                        {
                            "text": "Hij liet vol trots zijn nieuwe stereo-<w>installatie</w> aan zijn vrienden horen.",
                            "target_lang_translations": {
                                "ru": "Он с гордостью продемонстрировал друзьям свою новую <w>стереосистему</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "inrichting"
                    ],
                    "common_phrases": [
                        "elektrische installatie",
                        "cv-installatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплекс устройств, машин или коммуникаций, выполняющих определенную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "комплекс устройств"
                            },
                            {
                                "target_lang_word": "оборудование",
                                "target_lang_sense_clarification": "техническое оснащение"
                            },
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "техническое сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd4bc36e-b857-35c3-89a0-05a8503da1b0",
                    "sense_definition": "Een officiële en plechtige ceremonie waarbij iemand in een ambt of functie begint.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ceremony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>installatie</w> van de nieuwe burgemeester vindt volgende week plaats op het stadhuis.",
                            "target_lang_translations": {
                                "ru": "<w>Вступление в должность</w> нового мэра состоится на следующей неделе в ратуше."
                            }
                        },
                        {
                            "text": "Na de <w>installatie</w> hield de voorzitter een korte toespraak voor de aanwezigen.",
                            "target_lang_translations": {
                                "ru": "После <w>инаугурации</w> председатель произнес короткую речь перед собравшимися."
                            }
                        }
                    ],
                    "synonyms": [
                        "inauguratie",
                        "inwijding"
                    ],
                    "common_phrases": [
                        "plechtige installatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная и торжественная церемония вступления кого-либо в должность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступление в должность",
                                "target_lang_sense_clarification": "официальная процедура"
                            },
                            {
                                "target_lang_word": "инаугурация",
                                "target_lang_sense_clarification": "торжественная церемония"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84346e1c-58d1-322d-b2f2-de189f8e891a",
                    "sense_definition": "Een ruimtelijk kunstwerk dat bestaat uit een compositie van verschillende voorwerpen, materialen of media.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de grote zaal van het museum staat een indrukwekkende <w>installatie</w> van metaal en licht.",
                            "target_lang_translations": {
                                "ru": "В большом зале музея находится впечатляющая <w>инсталляция</w> из металла и света."
                            }
                        },
                        {
                            "text": "De kunstenaar werkt aan een nieuwe <w>installatie</w> die reageert op geluid.",
                            "target_lang_translations": {
                                "ru": "Художник работает над новой <w>инсталляцией</w>, которая реагирует на звук."
                            }
                        }
                    ],
                    "common_phrases": [
                        "video-installatie",
                        "moderne installatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространственное художественное произведение, состоящее из композиции различных предметов или материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инсталляция",
                                "target_lang_sense_clarification": "вид современного искусства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
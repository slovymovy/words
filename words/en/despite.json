    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "56e3ef00-75d5-36f2-a5da-4fe21db69a3b",
                    "sense_definition": "In spite of; without being affected by something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went for a walk <w>despite</w> the heavy rain.",
                            "target_lang_translations": {
                                "ru": "Мы пошли гулять <w>несмотря на</w> сильный дождь."
                            }
                        },
                        {
                            "text": "<w>Despite</w> the fact that he was tired, he finished his homework.",
                            "target_lang_translations": {
                                "ru": "<w>Несмотря на</w> то, что он устал, он закончил домашнюю работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "in spite of",
                        "regardless of",
                        "notwithstanding",
                        "for all",
                        "irrespective of"
                    ],
                    "antonyms": [
                        "because of"
                    ],
                    "common_phrases": [
                        "despite the fact",
                        "despite the weather",
                        "despite everything",
                        "despite all efforts"
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "życzenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1eb1670a-5a9a-3bd6-9751-7e73763836b0",
                    "sense_definition": "Pragnąć, aby kogoś spotkało coś dobrego (lub rzadziej złego) i wyrażać to słowami, zazwyczaj przy okazji świąt lub ważnych wydarzeń.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wishing_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z okazji twoich urodzin <w>życzę</w> ci wszystkiego najlepszego oraz spełnienia marzeń.",
                            "target_lang_translations": {
                                "ru": "По случаю твоего дня рождения <w>желаю</w> тебе всего наилучшего и исполнения желаний."
                            }
                        },
                        {
                            "text": "Nauczyciel <w>życzył</w> wszystkim uczniom powodzenia na trudnym egzaminie końcowym.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>пожелал</w> всем ученикам удачи на сложном выпускном экзамене."
                            }
                        }
                    ],
                    "synonyms": [
                        "winszować"
                    ],
                    "antonyms": [
                        "przeklinać (kogoś)",
                        "rzucać klątwy"
                    ],
                    "common_phrases": [
                        "życzyć wszystkiego najlepszego",
                        "życzyć powodzenia",
                        "życzyć zdrowia",
                        "życzyć miłego dnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желать, чтобы с кем-то произошло что-то хорошее (или реже плохое), и выражать это словами, обычно по случаю праздников или важных событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "высказывать пожелание"
                            },
                            {
                                "target_lang_word": "пожелать",
                                "target_lang_sense_clarification": "высказать пожелание (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3314940-0172-3fad-8e28-9a434f9f443e",
                    "sense_definition": "Chcieć czegoś, mieć na coś ochotę lub wymagać czegoś (używane w połączeniu z zaimkiem zwrotnym „sobie”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "desire_volition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stanowczo nie <w>życzę</w> sobie, abyś wchodził do mojego pokoju bez pukania.",
                            "target_lang_translations": {
                                "ru": "Я категорически не <w>желаю</w>, чтобы ты входил в мою комнату без стука."
                            }
                        },
                        {
                            "text": "Czy <w>życzą</w> sobie państwo coś do picia przed podaniem głównego posiłku?",
                            "target_lang_translations": {
                                "ru": "<w>Желаете</w> ли вы что-нибудь выпить перед подачей основного блюда?"
                            }
                        }
                    ],
                    "synonyms": [
                        "chcieć",
                        "pragnąć",
                        "żądać",
                        "łaknąć"
                    ],
                    "common_phrases": [
                        "nie życzę sobie",
                        "czego sobie życzysz",
                        "życzyć sobie czegoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хотеть чего-либо, иметь желание или требовать чего-либо (употребляется в сочетании с возвратным местоимением «sobie»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "хотеть"
                            },
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "иметь желание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e29b5471-8acd-3b64-9312-000ee00308c1",
                    "sense_definition": "To encourage something to happen, develop, or become more active, often regarding economies, organizations, or processes.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government lowered taxes to <w>stimulate</w> the economy and create jobs.",
                            "target_lang_translations": {
                                "ru": "Правительство снизило налоги, чтобы <w>стимулировать</w> экономику и создать рабочие места."
                            }
                        },
                        {
                            "text": "Teachers use games to <w>stimulate</w> interest in mathematics among students.",
                            "target_lang_translations": {
                                "ru": "Учителя используют игры, чтобы <w>стимулировать</w> интерес к математике у учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "encourage",
                        "promote",
                        "boost",
                        "spur",
                        "motivate",
                        "foment"
                    ],
                    "antonyms": [
                        "stifle",
                        "hinder",
                        "discourage",
                        "suppress"
                    ],
                    "common_phrases": [
                        "stimulate the economy",
                        "stimulate growth",
                        "stimulate demand",
                        "stimulate investment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побуждать к действию, развитию или повышению активности (часто об экономике, организациях или процессах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стимулировать",
                                "target_lang_sense_clarification": "развитие, экономику"
                            },
                            {
                                "target_lang_word": "побуждать",
                                "target_lang_sense_clarification": "к действию"
                            },
                            {
                                "target_lang_word": "поощрять",
                                "target_lang_sense_clarification": "поддерживать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "308a673b-3555-3f42-a583-772aca4c699d",
                    "sense_definition": "To cause a physical or mental reaction in a body part, organ, or organism; to make someone feel interested or excited.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Caffeine helps to <w>stimulate</w> the central nervous system and keep you awake.",
                            "target_lang_translations": {
                                "ru": "Кофеин помогает <w>стимулировать</w> центральную нервную систему и не даёт уснуть."
                            }
                        },
                        {
                            "text": "Bright colors and loud noises can <w>stimulate</w> a baby's senses too much.",
                            "target_lang_translations": {
                                "ru": "Яркие цвета и громкие звуки могут слишком сильно <w>возбуждать</w> органы чувств ребёнка."
                            }
                        }
                    ],
                    "synonyms": [
                        "arouse",
                        "excite",
                        "energize",
                        "activate",
                        "animate"
                    ],
                    "antonyms": [
                        "sedate",
                        "calm",
                        "de-energize"
                    ],
                    "common_phrases": [
                        "stimulate the brain",
                        "stimulate blood flow",
                        "visually stimulating",
                        "sexually stimulate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать физическую или ментальную реакцию в органе или организме; вызывать интерес или возбуждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стимулировать",

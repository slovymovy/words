                    "sense_id": "7c9c3aaa-dcd8-3a7a-951c-67f797ec89dc",
                    "sense_definition": "Een groenblijvende boom of heester met stekelige, leerachtige bladeren en rode bessen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De klimop en de <w>hulst</w> werden in heidense tijden gezien als een symbool van de komende lente.",
                            "target_lang_translations": {
                                "ru": "Плющ и <w>падуб</w> в языческие времена считались символом грядущей весны."
                            }
                        },
                        {
                            "text": "In de winter gebruiken we takken van de <w>hulst</w> om het huis te versieren.",
                            "target_lang_translations": {
                                "ru": "Зимой мы используем ветки <w>падуба</w> для украшения дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "steekpalm"
                    ],
                    "common_phrases": [
                        "takje hulst",
                        "hulst en klimop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечнозеленое дерево или кустарник с колючими кожистыми листьями и красными ягодами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падуб",
                                "target_lang_sense_clarification": "ботаническое название"
                            },
                            {
                                "target_lang_word": "остролист",
                                "target_lang_sense_clarification": "общеупотребительное название"
                            }
                        ]
                    }
                }
            ]
        },

                {
                    "sense_id": "a77c8241-9ffe-365b-907d-1306ee0387e7",
                    "sense_definition": "Budowla obronna, taka jak wał, mur, palisada lub system fortyfikacji, służąca do ochrony terenu lub miasta przed atakiem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare <w>obwarowania</w> miejskie zostały przekształcone w park dla mieszkańców.",
                            "target_lang_translations": {
                                "ru": "Старые городские <w>укрепления</w> были превращены в парк для жителей."
                            }
                        },
                        {
                            "text": "Żołnierze przez całą noc naprawiali uszkodzone <w>obwarowania</w>, spodziewając się kolejnego ataku.",
                            "target_lang_translations": {
                                "ru": "Солдаты всю ночь ремонтировали поврежденные <w>укрепления</w>, ожидая следующей атаки."
                            }
                        }
                    ],
                    "synonyms": [
                        "fortyfikacja",
                        "umocnienie"
                    ],
                    "common_phrases": [
                        "obwarowania miejskie",
                        "potężne obwarowania",
                        "system obwarowań"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оборонительное сооружение, такое как вал, стена, частокол или система укреплений, служащее для защиты территории или города от нападения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрепление",
                                "target_lang_sense_clarification": "военное сооружение"

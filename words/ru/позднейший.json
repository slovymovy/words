            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1cf8e609-dd68-3a63-bf7d-8070ff522607",
                    "sense_definition": "Превосходная степень прилагательного <w>поздний</w>; самый последний по времени наступления или появления.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы получили <w>позднейшие</w> сведения о ходе научной экспедиции.",
                            "target_lang_translations": {
                                "en": "We received the <w>latest</w> information about the progress of the scientific expedition."
                            }
                        },
                        {
                            "text": "Это <w>позднейшая</w> из известных нам работ этого знаменитого мастера.",
                            "target_lang_translations": {
                                "en": "This is the <w>latest</w> of the known works by this famous master."
                            }
                        }
                    ],
                    "antonyms": [
                        "ранний"
                    ],
                    "common_phrases": [
                        "позднейшие известия",
                        "в позднейшее время"
                    ],
                    "target_lang_definitions": {
                        "en": "Superlative degree of the adjective 'late'; the very last in time of occurrence or appearance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "latest",
                                "target_lang_sense_clarification": "most recent"
                            },
                            {
                                "target_lang_word": "most recent",
                                "target_lang_sense_clarification": "occurring nearest to the present"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c268fde5-50fc-3a8c-abcc-c486084c90b7",
                    "sense_definition": "Относящийся к более позднему времени, появившийся много времени спустя после чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Историки считают, что этот абзац — <w>позднейшая</w> вставка в древнюю летопись.",
                            "target_lang_translations": {
                                "en": "Historians believe that this paragraph is a <w>later</w> insertion into the ancient chronicle."
                            }
                        },
                        {
                            "text": "На стенах храма были обнаружены <w>позднейшие</w> росписи, скрывавшие оригинальные фрески.",
                            "target_lang_translations": {
                                "en": "<w>Later</w> paintings were discovered on the temple walls, concealing the original frescoes."
                            }
                        }
                    ],
                    "synonyms": [
                        "последующий"
                    ],
                    "antonyms": [
                        "первоначальный",
                        "ранний"
                    ],
                    "common_phrases": [
                        "позднейшие наслоения",
                        "позднейшая вставка"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a later time, appearing a long time after something else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "later",
                                "target_lang_sense_clarification": "occurring at a subsequent time, often implying an addition to an original"
                            },
                            {
                                "target_lang_word": "subsequent",
                                "target_lang_sense_clarification": "following in time or order"
                            }
                        ]
                    }
                }

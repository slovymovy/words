{
    "word_family": [
        "повторение",
        "повтор",
        "повторять",
        "повторный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "886e2e0b-c6ef-3372-971e-7d687cf91d70",
                    "sense_definition": "Сказать или сделать что-либо ещё раз.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель попросил ученика <повторить> ответ громче.",
                            "target_lang_translations": {
                                "en": "The teacher asked the student to <repeat> the answer louder."
                            }
                        },
                        {
                            "text": "Я не расслышал ваш вопрос, не могли бы вы его <повторить>?",
                            "target_lang_translations": {
                                "en": "I didn't hear your question, could you <repeat> it?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "повторить вопрос",
                        "повторить ошибку",
                        "повторить просьбу",
                        "повторить за мной"
                    ],
                    "target_lang_definitions": {
                        "en": "To say or perform an action again."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "repeat",
                                "target_lang_sense_clarification": "to do or say something again"
                            },
                            {
                                "target_lang_word": "reiterate",
                                "target_lang_sense_clarification": "to say something again, often for emphasis"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2df2bdf-558e-334f-bf99-1acfbdb39eac",
                    "sense_definition": "Возобновить в памяти ранее выученную или усвоенную информацию.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед экзаменом студентам нужно <повторить> весь пройденный материал.",
                            "target_lang_translations": {
                                "en": "Before the exam, students need to <review> all the covered material."
                            }
                        },
                        {
                            "text": "Летом полезно <повторить> правила грамматики, чтобы не забыть их к сентябрю.",
                            "target_lang_translations": {
                                "en": "In the summer, it is useful to <revise> grammar rules so as not to forget them by September."
                            }
                        }
                    ],
                    "synonyms": [
                        "освежить"
                    ],
                    "antonyms": [
                        "забыть"
                    ],
                    "common_phrases": [
                        "повторить урок",
                        "повторить материал",
                        "повторить слова",
                        "повторить перед тестом"
                    ],
                    "target_lang_definitions": {
                        "en": "To refresh previously learned information in one's memory."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "review",
                                "target_lang_sense_clarification": "to look over material again (common in US English)"
                            },
                            {
                                "target_lang_word": "revise",
                                "target_lang_sense_clarification": "to study again in preparation for an exam (common in British English)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
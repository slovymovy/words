    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0b6f72f0-d6b5-3413-b38f-f9f23425ccad",
                    "sense_definition": "Wat verbazing of verwondering oproept omdat het ongewoon of onverwacht is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is niet <w>verwonderlijk</w> dat hij geslaagd is, want hij heeft hard gestudeerd.",
                            "target_lang_translations": {
                                "en": "It is not <w>surprising</w> that he passed, because he studied hard."
                            }
                        },
                        {
                            "text": "Zij vertelde een <w>verwonderlijk</w> verhaal over haar avonturen in het buitenland.",
                            "target_lang_translations": {
                                "en": "She told an <w>astonishing</w> story about her adventures abroad."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbazingwekkend",
                        "opmerkelijk",
                        "verrassend",
                        "wonderlijk"
                    ],
                    "antonyms": [
                        "vanzelfsprekend",
                        "normaal",
                        "gewoon",
                        "alledaags"
                    ],
                    "common_phrases": [
                        "niet verwonderlijk",
                        "hoogst verwonderlijk",
                        "geenszins verwonderlijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Causing surprise or wonder because it is unusual or unexpected."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surprising",
                                "target_lang_sense_clarification": "causing surprise; unexpected"
                            },
                            {
                                "target_lang_word": "astonishing",
                                "target_lang_sense_clarification": "very surprising or impressive"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1bdc4849-874b-3f61-ba3f-c22234b4ac1c",
                    "sense_definition": "Op een manier die verbazing wekt; in hoge mate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude man was nog <w>verwonderlijk</w> fit voor zijn leeftijd.",
                            "target_lang_translations": {
                                "en": "The old man was still <w>remarkably</w> fit for his age."
                            }
                        },
                        {
                            "text": "Het bleef <w>verwonderlijk</w> stil in de grote zaal.",
                            "target_lang_translations": {
                                "en": "It remained <w>surprisingly</w> quiet in the large hall."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbazingwekkend",
                        "buitengewoon",
                        "wonderbaarlijk"
                    ],
                    "common_phrases": [
                        "verwonderlijk goed",
                        "verwonderlijk snel"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that arouses surprise; to a high degree."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surprisingly",
                                "target_lang_sense_clarification": "in an unexpected way"
                            },
                            {
                                "target_lang_word": "remarkably",
                                "target_lang_sense_clarification": "to a notable degree"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "708f589d-b796-3f75-811e-3f8e25461326",
                    "sense_definition": "An owner or occupant of a ranch.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ranch_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy <w>ranchman</w> surveyed his vast herds of cattle from the porch.",
                            "target_lang_translations": {
                                "ru": "Богатый <w>ранчер</w> оглядывал свои огромные стада скота с веранды."
                            }
                        },
                        {
                            "text": "Every local <w>ranchman</w> was invited to the town hall to discuss water rights.",
                            "target_lang_translations": {
                                "ru": "Каждого местного <w>владельца ранчо</w> пригласили в ратушу, чтобы обсудить права на воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "rancher",
                        "cattleman"
                    ],
                    "common_phrases": [
                        "wealthy ranchman",
                        "local ranchman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владелец или житель ранчо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранчер",
                                "target_lang_sense_clarification": "владелец ранчо"
                            },
                            {
                                "target_lang_word": "владелец ранчо",
                                "target_lang_sense_clarification": "собственник"
                            },
                            {
                                "target_lang_word": "скотовод",
                                "target_lang_sense_clarification": "занимающийся разведением скота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "258f4267-89d0-35fa-9bb5-ea7ea7e5619a",
                    "sense_definition": "A laborer on a ranch.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ranch_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was hired as a <w>ranchman</w> to help with the spring roundup.",
                            "target_lang_translations": {
                                "ru": "Его наняли <w>работником на ранчо</w> в помощь на весенний сгон скота."
                            }
                        },
                        {
                            "text": "The hard-working <w>ranchman</w> repaired the fences before winter arrived.",
                            "target_lang_translations": {
                                "ru": "Трудолюбивый <w>работник ранчо</w> починил заборы до наступления зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ranch hand",
                        "cowboy",
                        "cowhand"
                    ],
                    "common_phrases": [
                        "hired ranchman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наемный работник на ранчо."
                    },
                    "translations": {

{
    "word_family": [
        "reason",
        "reasonable",
        "reasonably",
        "reasoning",
        "reasonableness",
        "unreasonableness",
        "unreasoning"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a0095439-b86f-382f-b5a2-6d6fe198aa1e",
                    "sense_definition": "Not fair or acceptable; going beyond what is right, practical, or expected.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "not_fair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>unreasonable</w> to expect a child to sit quietly for three hours.",
                            "target_lang_translations": {
                                "ru": "<w>Неразумно</w> ожидать, что ребенок будет сидеть тихо в течение трех часов."
                            }
                        },
                        {
                            "text": "The customer made an <w>unreasonable</w> demand for a full refund after using the product.",
                            "target_lang_translations": {
                                "ru": "Клиент выдвинул <w>необоснованное</w> требование о полном возврате средств после использования продукта."
                            }
                        },
                        {
                            "text": "We cannot accept such an <w>unreasonable</w> delay in the project timeline.",
                            "target_lang_translations": {
                                "ru": "Мы не можем принять такую <w>чрезмерную</w> задержку в графике проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "excessive",
                        "unfair",
                        "unjust"
                    ],
                    "antonyms": [
                        "reasonable"
                    ],
                    "common_phrases": [
                        "unreasonable demand",
                        "unreasonable expectation",
                        "unreasonable delay",
                        "unreasonable price",
                        "unreasonable behavior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несправедливый или неприемлемый; выходящий за рамки правильного, практичного или ожидаемого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необоснованный",
                                "target_lang_sense_clarification": "лишенный веских причин или оснований (о требованиях, критике)"
                            },
                            {
                                "target_lang_word": "чрезмерный",
                                "target_lang_sense_clarification": "превышающий разумные пределы (о цене, ожиданиях)"
                            },
                            {
                                "target_lang_word": "неразумный",
                                "target_lang_sense_clarification": "лишенный здравого смысла, нецелесообразный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45ea09f7-511b-3889-91ec-e789c6fb29eb",
                    "sense_definition": "Lacking the ability to think clearly, logically, or rationally.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "no_logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was consumed by an <w>unreasonable</w> fear of the dark even as an adult.",
                            "target_lang_translations": {
                                "ru": "Даже будучи взрослым, он был охвачен <w>иррациональным</w> страхом темноты."
                            }
                        },
                        {
                            "text": "The mob acted with <w>unreasonable</w> fury, destroying everything in its path.",
                            "target_lang_translations": {
                                "ru": "Толпа действовала с <w>бессмысленной</w> яростью, уничтожая все на своем пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "irrational",
                        "unreasoning",
                        "illogical"
                    ],
                    "antonyms": [
                        "reasonable"
                    ],
                    "common_phrases": [
                        "unreasonable fear",
                        "unreasonable hatred",
                        "unreasonable fury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишенный способности мыслить ясно, логично или рационально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иррациональный",
                                "target_lang_sense_clarification": "не поддающийся логическому объяснению"
                            },
                            {
                                "target_lang_word": "безотчетный",
                                "target_lang_sense_clarification": "инстинктивный, не контролируемый разумом (часто о страхе)"
                            },
                            {
                                "target_lang_word": "бессмысленный",
                                "target_lang_sense_clarification": "лишенный смысла или цели"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
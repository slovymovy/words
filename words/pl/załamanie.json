                {
                    "sense_id": "a29b94c5-9bd5-3d29-a366-25001278ca82",
                    "sense_definition": "Miejsce, w którym coś jest zgięte, przełamane lub ma wgłębienie",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7286027-48a8-3e4a-a9ca-a413c238e7bd",
                    "sense_definition": "Хищное млекопитающее семейства псовых с длинным пушистым хвостом и обычно рыжим мехом.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В зоопарке мы долго наблюдали за рыжей <лисой>.",
                            "target_lang_translations": {
                                "en": "At the zoo, we watched the red <w>fox</w> for a long time."
                            }
                        },
                        {
                            "text": "Из леса выбежала <лиса>, замерла, взмахнула хвостом и исчезла в зарослях.",
                            "target_lang_translations": {
                                "en": "A <w>fox</w> ran out of the forest, froze, waved its tail, and disappeared into the thicket."
                            }
                        }
                    ],
                    "synonyms": [
                        "лисица",
                        "лиси́чка"
                    ],
                    "common_phrases": [
                        "рыжая лиса",
                        "хитрая лиса"
                    ],
                    "target_lang_definitions": {
                        "en": "A predatory mammal of the canine family with a long bushy tail and usually red fur."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fox",
                                "target_lang_sense_clarification": "animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "158d14b0-36e9-33df-8522-b71fc8cea894",
                    "sense_definition": "Хитрый, льстивый, изворотливый человек.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_group",
                    "name_type": "no",
                    "examples": [
                        {

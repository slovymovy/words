    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07a67ede-768f-3454-86a3-61bbe49ae875",
                    "sense_definition": "An inland body of standing water, either natural or man-made, that is smaller than a lake.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children love to feed the ducks at the village <w>pond</w>.",
                            "target_lang_translations": {
                                "ru": "Дети любят кормить уток на деревенском <w>пруду</w>."
                            }
                        },
                        {
                            "text": "We dug a small <w>pond</w> in the garden to keep some fish.",
                            "target_lang_translations": {
                                "ru": "Мы выкопали в саду маленький <w>пруд</w>, чтобы разводить рыбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pool"
                    ],
                    "common_phrases": [
                        "garden pond",
                        "fish pond",
                        "duck pond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой естественный или искусственный пресный водоем, меньше озера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пруд",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "водоём",
                                "target_lang_sense_clarification": "общее название водного объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "276e3879-76d2-33e2-a1e6-a97fb038cf32",
                    "sense_definition": "The Atlantic Ocean.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother moved across the <w>pond</w> to New York five years ago.",
                            "target_lang_translations": {
                                "ru": "Мой брат переехал за <w>океан</w>, в Нью-Йорк, пять лет назад."
                            }
                        },
                        {
                            "text": "I wonder how our colleagues on the other side of the <w>pond</w> are handling the news.",
                            "target_lang_translations": {
                                "ru": "Интересно, как наши коллеги по ту сторону <w>океана</w> восприняли эту новость."
                            }
                        }
                    ],
                    "common_phrases": [
                        "across the pond",
                        "this side of the pond"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "humorous or informal"
                        }
                    ],
                    "target_lang_definitions": {

    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7d00720c-4932-33c9-acce-300a4215175f",
                    "sense_definition": "More; additional; supplementary.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "additional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you have any <w>further</w> questions before we finish?",
                            "target_lang_translations": {
                                "ru": "У вас есть какие-нибудь <w>дополнительные</w> вопросы, прежде чем мы закончим?"
                            }
                        },
                        {
                            "text": "For <w>further</w> information, please visit our website.",
                            "target_lang_translations": {
                                "ru": "Для получения <w>дальнейшей</w> информации, пожалуйста, посетите наш сайт."
                            }
                        }
                    ],
                    "synonyms": [
                        "additional",
                        "extra",
                        "supplementary",
                        "more"
                    ],
                    "common_phrases": [
                        "further information",
                        "further details",
                        "further questions",
                        "until further notice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительный; добавочный; последующий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальнейший",
                                "target_lang_sense_clarification": "последующий, будущий"
                            },
                            {
                                "target_lang_word": "дополнительный",
                                "target_lang_sense_clarification": "добавочный, сверх того"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd3c87d7-19c3-3f0b-a0a8-926e13f47763",
                    "sense_definition": "More distant in space or time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "distant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was standing at the <w>further</w> end of the long corridor.",
                            "target_lang_translations": {
                                "ru": "Он стоял в <w>дальнем</w> конце длинного коридора."
                            }
                        },
                        {
                            "text": "The <w>further</w> side of the river is much steeper.",
                            "target_lang_translations": {
                                "ru": "<w>Дальняя</w> сторона реки гораздо круче."
                            }
                        }
                    ],
                    "synonyms": [
                        "farther",
                        "more distant"
                    ],
                    "antonyms": [
                        "nearer",
                        "closer"
                    ],
                    "common_phrases": [
                        "further away",
                        "further side"
                    ],
                    "target_lang_definitions": {
                        "ru": "Более отдалённый в пространстве или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальний",
                                "target_lang_sense_clarification": "находящийся далеко"
                            },
                            {
                                "target_lang_word": "далёкий",
                                "target_lang_sense_clarification": "находящийся на большом расстоянии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "48fcb884-26e2-3c6c-ba9f-d81b98e78864",
                    "sense_definition": "To a greater extent or degree.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to discuss this matter <w>further</w> before making a decision.",
                            "target_lang_translations": {
                                "ru": "Нам нужно обсудить этот вопрос <w>дальше</w>, прежде чем принимать решение."
                            }
                        },
                        {
                            "text": "The situation was <w>further</w> complicated by the bad weather.",
                            "target_lang_translations": {
                                "ru": "Ситуация <w>ещё более</w> осложнилась из-за плохой погоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "more"
                    ],
                    "common_phrases": [
                        "develop further",
                        "investigate further",
                        "explain further"
                    ],
                    "target_lang_definitions": {
                        "ru": "В большей степени; глубже; подробнее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальше",
                                "target_lang_sense_clarification": "продолжая действие"
                            },
                            {
                                "target_lang_word": "более",
                                "target_lang_sense_clarification": "в большей степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dad36e0-acca-3408-be14-c5905d7f0a1f",
                    "sense_definition": "At or to a greater distance in space or time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distance_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am too tired to walk any <w>further</w> today.",
                            "target_lang_translations": {
                                "ru": "Я слишком устал, чтобы идти <w>дальше</w> сегодня."
                            }
                        },
                        {
                            "text": "We must look <w>further</w> back in history to find the answer.",
                            "target_lang_translations": {
                                "ru": "Мы должны заглянуть <w>дальше</w> в прошлое, чтобы найти ответ."
                            }
                        }
                    ],
                    "synonyms": [
                        "farther",
                        "beyond"
                    ],
                    "antonyms": [
                        "closer",
                        "nearer"
                    ],
                    "common_phrases": [
                        "go further",
                        "move further",
                        "further away",
                        "further back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дальше; на более далекое расстояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальше",
                                "target_lang_sense_clarification": "сравнительная степень от 'далеко'"
                            },
                            {
                                "target_lang_word": "далее",
                                "target_lang_sense_clarification": "затем, потом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d16550d9-49ad-3083-aac7-79826bef8033",
                    "sense_definition": "Also; in addition; moreover.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "connector",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plan is too expensive, and <w>further</w>, it is not practical.",
                            "target_lang_translations": {
                                "ru": "План слишком дорогой, и, <w>кроме того</w>, он непрактичен."
                            }
                        },
                        {
                            "text": "<w>Further</w>, I would like to thank everyone for their hard work.",
                            "target_lang_translations": {
                                "ru": "<w>Далее</w>, я хотел бы поблагодарить всех за их усердную работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "furthermore",
                        "moreover",
                        "in addition",
                        "additionally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кроме того; также; более того."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме того",
                                "target_lang_sense_clarification": "вводное слово: к тому же"
                            },
                            {
                                "target_lang_word": "далее",
                                "target_lang_sense_clarification": "затем, вдобавок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71355cba-00c4-3be8-8b6d-c3deb8d40f45",
                    "sense_definition": "Following on from; referring to a previous communication.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Further</w> to our telephone conversation, I am sending you the report.",
                            "target_lang_translations": {
                                "ru": "<w>В продолжение</w> нашего телефонного разговора, я высылаю вам отчет."
                            }
                        },
                        {
                            "text": "I am writing <w>further</w> to your request for more details.",
                            "target_lang_translations": {
                                "ru": "Я пишу <w>в дополнение</w> к вашему запросу о подробностях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "further to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal business correspondence"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В продолжение; ссылаясь на (в деловой переписке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в продолжение",
                                "target_lang_sense_clarification": "ссылаясь на предыдущее общение"
                            },
                            {
                                "target_lang_word": "в дополнение",
                                "target_lang_sense_clarification": "добавляя к сказанному"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ff0014a1-1769-3d77-b7f9-f78aa913d299",

{
    "word_family": [
        "sign",
        "signatory",
        "signer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d9943ed-b46d-3311-b625-8077937e8d06",
                    "sense_definition": "A person's name written in a distinctive way by themselves, used to identify them or authorize a document.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "identity_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put your <w>signature</w> at the bottom of the page.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставьте вашу <w>по́дпись</w> внизу страницы."
                            }
                        },
                        {
                            "text": "The bank refused the check because the <w>signature</w> did not match.",
                            "target_lang_translations": {
                                "ru": "Банк отклонил чек, потому что <w>по́дпись</w> не совпала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "official signature",
                        "witness a signature",
                        "forge a signature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имя человека, написанное им самим особым образом для идентификации личности или заверения документа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́дпись",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "ро́спись",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2646994d-2068-3df0-82a3-16a02225f317",
                    "sense_definition": "A block of text or images automatically appended to the end of an email or forum post, often containing contact details.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to update my email <w>signature</w> with my new phone number.",
                            "target_lang_translations": {
                                "ru": "Мне нужно обновить мою <w>по́дпись</w> в электронной почте, добавив новый номер телефона."
                            }
                        },
                        {
                            "text": "Her forum <w>signature</w> includes a funny quote from a movie.",
                            "target_lang_translations": {
                                "ru": "Ее <w>по́дпись</w> на форуме включает забавную цитату из фильма."
                            }
                        }
                    ],
                    "synonyms": [
                        "sig",
                        "siggy"
                    ],
                    "common_phrases": [
                        "email signature"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "synonyms 'sig' and 'siggy' are slang/informal clippings"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Блок текста или изображений, автоматически добавляемый в конец электронного письма или сообщения на форуме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́дпись",
                                "target_lang_sense_clarification": "в электронной переписке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea6ce719-d2b3-333a-b9ea-9ec8927da0f6",
                    "sense_definition": "A distinguishing feature, product, or quality that identifies a particular person or thing.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The truffle pasta is one of the chef's most famous <w>signatures</w>.",
                            "target_lang_translations": {
                                "ru": "Паста с трюфелями — одно из самых известных <w>фирменных блюд</w> шеф-повара."
                            }
                        },
                        {
                            "text": "Bright colors are the <w>signature</w> of this artist's work.",
                            "target_lang_translations": {
                                "ru": "Яркие цвета — это <w>визитная карточка</w> работ этого художника."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличительная черта, продукт или качество, идентифицирующие конкретного человека или вещь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "визитная карточка",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "фирменный знак",
                                "target_lang_sense_clarification": "отличительная особенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "864a1024-a92b-3724-8ca8-d30174c5b68a",
                    "sense_definition": "Signs placed at the beginning of a musical staff to indicate the key or tempo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_notation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The time <w>signature</w> changed from 4/4 to 3/4 in the middle of the song.",
                            "target_lang_translations": {
                                "ru": "Музыкальный <w>размер</w> изменился с 4/4 на 3/4 в середине песни."
                            }
                        },
                        {
                            "text": "You must check the key <w>signature</w> to know which notes are sharp or flat.",
                            "target_lang_translations": {
                                "ru": "Вы должны проверить <w>ключевые знаки</w>, чтобы знать, какие ноты являются диезами или бемолями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "time signature",
                        "key signature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знаки в начале нотного стана, указывающие тональность или размер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "музыкальный ритм"
                            },
                            {
                                "target_lang_word": "ключевые знаки",
                                "target_lang_sense_clarification": "тональность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f783a19-18af-38a7-910b-827e7c0fcdef",
                    "sense_definition": "The formal act of signing a document or agreement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "identity_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>signature</w> of the peace treaty was a historic moment.",
                            "target_lang_translations": {
                                "ru": "<w>Подписание</w> мирного договора стало историческим моментом."
                            }
                        },
                        {
                            "text": "We are waiting for the <w>signature</w> of the contract before starting work.",
                            "target_lang_translations": {
                                "ru": "Мы ждем <w>подписания</w> контракта перед началом работ."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный акт подписания документа или соглашения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписание",
                                "target_lang_sense_clarification": "процесс или событие"
                            },
                            {
                                "target_lang_word": "по́дпись",
                                "target_lang_sense_clarification": "факт заверения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f8ccf859-9597-3965-af2d-a061e1efea1d",
                    "sense_definition": "Distinctive and characteristic of a particular person or thing; serving to identify someone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant is known for its <w>signature</w> dish, the spicy crab.",
                            "target_lang_translations": {
                                "ru": "Ресторан известен своим <w>фирменным</w> блюдом — острым крабом."
                            }
                        },
                        {
                            "text": "Wearing a fedora became his <w>signature</w> look over the years.",
                            "target_lang_translations": {
                                "ru": "Ношение фетровой шляпы с годами стало его <w>фирменным</w> стилем."
                            }
                        }
                    ],
                    "synonyms": [
                        "iconic"
                    ],
                    "common_phrases": [
                        "signature dish",
                        "signature style",
                        "signature move",
                        "signature scent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличительный и характерный для конкретного человека или вещи; служащий для идентификации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фи́рменный",
                                "target_lang_sense_clarification": "характерный, узнаваемый"
                            },
                            {
                                "target_lang_word": "коронный",
                                "target_lang_sense_clarification": "об известном приеме или блюде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
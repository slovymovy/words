    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a9ea6662-e37d-3672-87bf-51bb821f48b8",
                    "sense_definition": "Uderzać w coś rytmicznie i wielokrotnie, wydając przy tym charakterystyczny odgłos stukania (często o deszczu lub palcach).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ulewny deszcz zaczął głośno <bębnić> o blaszany parapet za oknem.",
                            "target_lang_translations": {
                                "ru": "Проливной дождь начал громко <w>барабанить</w> по жестяному подоконнику за окном."
                            }
                        },
                        {
                            "text": "Zniecierpliwiony klient siedział przy biurku i zaczął <bębnić> palcami w blat.",
                            "target_lang_translations": {
                                "ru": "Нетерпеливый клиент сидел за столом и начал <w>барабанить</w> пальцами по столешнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "stukać",
                        "łomotać",
                        "walić",
                        "dudnić"
                    ],
                    "common_phrases": [
                        "bębnić palcami",
                        "deszcz bębni",
                        "bębnić w szybę",
                        "bębnić do drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмично и многократно ударять по чему-либо, издавая характерный стук (часто о дожде или пальцах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барабанить",
                                "target_lang_sense_clarification": "стучать пальцами или о дожде"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "издавать звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9965a23e-0ce5-30ea-8417-049472bc1037",
                    "sense_definition": "Grać na bębnie lub innym instrumencie perkusyjnym poprzez uderzanie w niego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody muzyk uwielbiał <bębnić> na swoim zestawie perkusyjnym całymi dniami.",
                            "target_lang_translations": {
                                "ru": "Молодой музыкант обожал целыми днями <w>барабанить</w> на своей ударной установке."
                            }
                        },
                        {
                            "text": "Podczas parady orkiestra zaczęła głośno <bębnić>, nadając rytm marszowi.",
                            "target_lang_translations": {
                                "ru": "Во время парада оркестр начал громко <w>бить в барабаны</w>, задавая ритм маршу."
                            }
                        }
                    ],
                    "synonyms": [
                        "walić"
                    ],
                    "common_phrases": [
                        "bębnić w bęben",
                        "bębnić marsza",
                        "głośno bębnić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть на барабане или другом ударном инструменте посредством ударов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барабанить",
                                "target_lang_sense_clarification": "играть на инструменте"
                            },
                            {
                                "target_lang_word": "бить в барабан",
                                "target_lang_sense_clarification": "играть на барабане"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eaa56772-703c-37e0-8dbf-96b1ffb9237a",
                    "sense_definition": "Mówić o czymś ciągle, powtarzać coś natrętnie lub przypominać o czymś wielokrotnie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speech_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama ciągle <bębni> mi nad uchem, żebym wreszcie posprzątał swój pokój.",
                            "target_lang_translations": {
                                "ru": "Мама постоянно <w>жужжит</w> мне над ухом, чтобы я наконец убрал свою комнату."
                            }
                        },
                        {
                            "text": "Nie musisz wciąż o tym <bębnić>, bo doskonale pamiętam o naszej umowie.",
                            "target_lang_translations": {
                                "ru": "Не нужно постоянно об этом <w>твердить</w>, я прекрасно помню о нашем договоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "bić",
                        "stukać"
                    ],
                    "common_phrases": [
                        "bębnić o tym samym",
                        "bębnić nad uchem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно говорить об одном и том же, назойливо повторять или напоминать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твердить",
                                "target_lang_sense_clarification": "постоянно повторять"
                            },
                            {
                                "target_lang_word": "долбить",
                                "target_lang_sense_clarification": "разг. надоедливо повторять"
                            },
                            {
                                "target_lang_word": "жужжать",
                                "target_lang_sense_clarification": "надоедать разговорами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10181322-8e9a-3876-bbbc-a1c37817afe2",
                    "sense_definition": "Rozpowiadać informacje szerokiemu gronu osób, często w sposób natrętny lub chwalipięcki.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "speech_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie lokalne gazety zaczęły <bębnić> o wielkim sukcesie naszej drużyny.",
                            "target_lang_translations": {
                                "ru": "Все местные газеты начали <w>трубить</w> о большом успехе нашей команды."
                            }
                        },
                        {
                            "text": "On lubi <bębnić> na prawo i lewo o swoich rzekomych znajomościach.",
                            "target_lang_translations": {
                                "ru": "Он любит <w>раззванивать</w> направо и налево о своих мнимых знакомствах."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozgłaszać",
                        "trąbić",
                        "rozgadywać"
                    ],
                    "common_phrases": [
                        "bębnić na prawo i lewo",
                        "bębnić o sukcesie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространять информацию среди широкого круга людей, часто навязчиво или хвастливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубить",
                                "target_lang_sense_clarification": "разг. разглашать всем"
                            },
                            {
                                "target_lang_word": "раззванивать",
                                "target_lang_sense_clarification": "разг. сплетничать, рассказывать всем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82f3286d-7b1f-3ccf-826c-4504fc51da3f",
                    "sense_definition": "Wydawać charakterystyczny, głuchy odgłos (o niektórych ptakach, np. bąkach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ukryty w trzcinach bąk zaczął <bębnić>, co było słychać w całej okolicy.",
                            "target_lang_translations": {
                                "ru": "Скрывшаяся в камышах выпь начала <w>ухать</w>, что было слышно во всей округе."
                            }
                        },
                        {
                            "text": "Ornitolodzy pojechali nad jezioro, aby usłyszeć, jak ptaki zaczynają <bębnić> o świcie.",
                            "target_lang_translations": {
                                "ru": "Орнитологи поехали на озеро, чтобы услышать, как птицы начинают <w>токовать</w> на рассвете."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать характерный глухой звук (о некоторых птицах, например, выпях или дятлах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухать",
                                "target_lang_sense_clarification": "о выпи"
                            },
                            {
                                "target_lang_word": "барабанить",
                                "target_lang_sense_clarification": "о дятле или бекасе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

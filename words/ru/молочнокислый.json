    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "97fc4427-bf71-33db-aaf4-53221529ea6e",
                    "sense_definition": "Связанный с образованием молочной кислоты, обычно в результате деятельности бактерий.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biochemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В кишечнике человека обитают полезные <w>молочнокислые</w> бактерии.",
                            "target_lang_translations": {
                                "en": "Beneficial <w>lactic acid</w> bacteria inhabit the human intestine."
                            }
                        },
                        {
                            "text": "<w>Молочнокислое</w> брожение играет ключевую роль в приготовлении квашеной капусты.",
                            "target_lang_translations": {
                                "en": "<w>Lactic acid</w> fermentation plays a key role in the preparation of sauerkraut."
                            }
                        }
                    ],
                    "common_phrases": [
                        "молочнокислое брожение",
                        "молочнокислые бактерии",
                        "молочнокислая закваска"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the formation of lactic acid, usually as a result of bacterial activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lactic acid",
                                "target_lang_sense_clarification": "used attributively (e.g. bacteria, fermentation)"
                            },
                            {
                                "target_lang_word": "lactic",
                                "target_lang_sense_clarification": "relating to milk acid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f676687-0fa7-3346-a653-0fd9b1be3e7c",
                    "sense_definition": "Приготовленный из молока путём сквашивания; то же, что кисломолочный.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "food_products",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи рекомендуют употреблять <w>молочнокислые</w> продукты для улучшения пищеварения.",
                            "target_lang_translations": {
                                "en": "Doctors recommend consuming <w>fermented milk</w> products to improve digestion."
                            }
                        },
                        {
                            "text": "Кефир и ряженка — это популярные <w>молочнокислые</w> напитки.",
                            "target_lang_translations": {
                                "en": "Kefir and ryazhenka are popular <w>fermented milk</w> drinks."
                            }
                        }
                    ],
                    "synonyms": [
                        "кисломолочный"
                    ],
                    "common_phrases": [
                        "молочнокислые продукты",
                        "молочнокислые изделия"
                    ],
                    "target_lang_definitions": {
                        "en": "Prepared from milk by fermentation; synonymous with fermented milk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fermented milk",
                                "target_lang_sense_clarification": "describing dairy products"
                            },
                            {
                                "target_lang_word": "cultured milk",
                                "target_lang_sense_clarification": "referring to products like yogurt or kefir"
                            }
                        ]
                    }
                }
            ]
        }
    ]

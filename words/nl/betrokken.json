    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1a91c807-64d8-3b3c-9b04-9804233006f6",
                    "sense_definition": "Die ergens mee te maken heeft of ergens deel van uitmaakt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_involved",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alle <w>betrokken</w> partijen moeten het contract ondertekenen voor het geldig is.",
                            "target_lang_translations": {
                                "ru": "Все <w>вовлечённые</w> стороны должны подписать контракт до того, как он вступит в силу.",
                                "en": "All <w>involved</w> parties must sign the contract before it is valid."
                            }
                        },
                        {
                            "text": "De politie heeft de <w>betrokken</w> personen verhoord over het incident.",
                            "target_lang_translations": {
                                "ru": "Полиция допросила <w>причастных</w> лиц об инциденте.",
                                "en": "The police interrogated the persons <w>involved</w> about the incident."
                            }
                        }
                    ],
                    "synonyms": [
                        "geïmpliceerd"
                    ],
                    "antonyms": [
                        "onbetrokken"
                    ],
                    "common_phrases": [
                        "de betrokken partijen",
                        "betrokken raken bij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий отношение к чему-либо или являющийся частью чего-либо.",
                        "en": "Having to do with something or being part of something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причастный",
                                "target_lang_sense_clarification": "имеющий отношение к делу"
                            },
                            {
                                "target_lang_word": "вовлечённый",
                                "target_lang_sense_clarification": "участвующий в процессе"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "involved",
                                "target_lang_sense_clarification": "participating or implicated"
                            },
                            {
                                "target_lang_word": "concerned",
                                "target_lang_sense_clarification": "relevant to the matter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d02d276-db2e-3888-9c54-26dcc712034c",
                    "sense_definition": "Die zich ergens sterk mee verbonden voelt en zich ervoor inzet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_involved",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een zeer <w>betrokken</w> lerares die altijd klaarstaat voor haar leerlingen.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>неравнодушная</w> учительница, которая всегда готова помочь своим ученикам.",
                                "en": "She is a very <w>committed</w> teacher who is always there for her students."
                            }
                        },
                        {
                            "text": "Wij zoeken maatschappelijk <w>betrokken</w> vrijwilligers voor ons buurtproject.",
                            "target_lang_translations": {
                                "ru": "Мы ищем социально <w>активных</w> волонтеров для нашего районного проекта.",
                                "en": "We are looking for socially <w>engaged</w> volunteers for our neighborhood project."
                            }
                        }
                    ],
                    "synonyms": [
                        "begaan",
                        "geëngageerd",
                        "toegewijd"
                    ],
                    "antonyms": [
                        "onverschillig",
                        "afstandelijk"
                    ],
                    "common_phrases": [
                        "maatschappelijk betrokken",
                        "nauw betrokken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий сильную связь с чем-либо и активно участвующий в этом.",
                        "en": "Feeling strongly connected to something and actively dedicating oneself to it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неравнодушный",
                                "target_lang_sense_clarification": "проявляющий участие"
                            },
                            {
                                "target_lang_word": "преданный делу",
                                "target_lang_sense_clarification": "сильно увлечённый работой"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "committed",
                                "target_lang_sense_clarification": "dedicated to a cause or job"
                            },
                            {
                                "target_lang_word": "engaged",
                                "target_lang_sense_clarification": "showing active interest and participation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66505936-dcbb-32b8-9370-845f2c9e1351",
                    "sense_definition": "Geheel of gedeeltelijk met wolken bedekt (van de lucht).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lucht was zwaar <w>betrokken</w> en het begon zachtjes te regenen.",
                            "target_lang_translations": {
                                "ru": "Небо было сильно <w>затянуто</w>, и начал накрапывать дождь.",
                                "en": "The sky was heavily <w>overcast</w> and it started raining gently."
                            }
                        },
                        {
                            "text": "Op een <w>betrokken</w> dag gaan we liever niet naar het strand.",
                            "target_lang_translations": {
                                "ru": "В <w>пасмурный</w> день мы предпочитаем не ходить на пляж.",
                                "en": "On an <w>overcast</w> day we prefer not to go to the beach."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewolkt",
                        "grijs"
                    ],
                    "antonyms": [
                        "helder",
                        "onbewolkt"
                    ],
                    "common_phrases": [
                        "een betrokken lucht",
                        "zwaar betrokken"
                    ],
                    "target_lang_definitions": {

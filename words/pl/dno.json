                    ],
                    "common_phrases": [
                        "dno morskie",
                        "dno rzeki",
                        "iść na dno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грунт, находящийся под водой в реке, озере, море или океане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "водоёма"

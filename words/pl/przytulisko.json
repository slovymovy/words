                            },
                            {
                                "target_lang_word": "прибежище",
                                "target_lang_sense_clarification": "укрытие, безопасное место"
                            },
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "переносное значение: место, где приняли"

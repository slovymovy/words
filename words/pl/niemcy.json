                    }
                },
                {
                    "sense_id": "d8541264-3808-3fbc-85b5-74bcb0a419dd",
                    "sense_definition": "naród zamieszkujący głównie Europę Środkową; obywatele państwa niemieckiego",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "people_group",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "<w>Niemcy</w> są znani na świecie ze swojej punktualności i solidności.",
                            "target_lang_translations": {
                                "ru": "<w>Немцы</w> известны в мире своей пунктуальностью и надежностью."
                            }
                        },
                        {
                            "text": "W tym regionie <w>Niemcy</w> stanowią znaczną mniejszość narodową.",
                            "target_lang_translations": {
                                "ru": "В этом регионе <w>немцы</w> составляют значительное национальное меньшинство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "my i Niemcy"
                    ],
                    "target_lang_definitions": {
                        "ru": "народ, проживающий в основном в Центральной Европе; граждане Германии"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немцы",
                                "target_lang_sense_clarification": "народ"
                            }
                        ]
                    }
                }
            ]
        },
        {

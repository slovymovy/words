{
    "word_family": [
        "residence",
        "residency",
        "resident",
        "residential"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fe5bd285-8263-3ddd-9162-919a0767fc77",
                    "sense_definition": "To live in a particular place permanently or for an extended period.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "live_dwell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He still <w>resides</w> at his parents' house despite having a job.",
                            "target_lang_translations": {
                                "ru": "Он все еще <w>проживает</w> в доме своих родителей, несмотря на то, что у него есть работа."
                            }
                        },
                        {
                            "text": "Many retired people choose to <w>reside</w> in warmer climates during the winter.",
                            "target_lang_translations": {
                                "ru": "Многие пенсионеры предпочитают <w>жить</w> в более теплом климате зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "live",
                        "dwell",
                        "inhabit",
                        "occupy",
                        "stay"
                    ],
                    "antonyms": [
                        "leave",
                        "vacate"
                    ],
                    "common_phrases": [
                        "reside in",
                        "reside at",
                        "reside with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жить в определенном месте постоянно или в течение длительного периода"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проживать",
                                "target_lang_sense_clarification": "официально или постоянно"
                            },
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "обитать",
                                "target_lang_sense_clarification": "населять место, книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c9d5c17-b284-31ff-9e08-0c271f5182ec",
                    "sense_definition": "To be present as a quality, right, or power within something or someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "inhere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ultimate power in a democracy <w>resides</w> in the people.",
                            "target_lang_translations": {
                                "ru": "Высшая власть в демократии <w>принадлежит</w> народу."
                            }
                        },
                        {
                            "text": "The true value of the artwork <w>resides</w> in its emotional impact.",
                            "target_lang_translations": {
                                "ru": "Истинная ценность произведения искусства <w>заключается</w> в его эмоциональном воздействии."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwell",
                        "abide"
                    ],
                    "common_phrases": [
                        "reside in",
                        "power resides"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть присущим; заключаться в чем-либо (о качестве, праве или силе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключаться",
                                "target_lang_sense_clarification": "содержаться в чем-либо"
                            },
                            {
                                "target_lang_word": "принадлежать",
                                "target_lang_sense_clarification": "о власти или праве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f43c008e-613c-37f6-99e3-ed6814fc15a5",
                    "sense_definition": "To sink to the bottom or settle, especially regarding sediment or liquid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "settle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the storm passed, the dust slowly <w>resided</w> to the ground.",
                            "target_lang_translations": {
                                "ru": "После того как буря утихла, пыль медленно <w>осела</w> на землю."
                            }
                        },
                        {
                            "text": "The heavy particles <w>resided</w> at the bottom of the container.",
                            "target_lang_translations": {
                                "ru": "Тяжелые частицы <w>осели</w> на дне контейнера."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оседать на дно или отстаиваться (особенно об осадке или жидкости)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оседать",
                                "target_lang_sense_clarification": "об осадке или пыли"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "26894a65-35e0-3972-8506-0a007e0f5a3c",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We met Mr. <w>Reside</w> at the conference last week.",
                            "target_lang_translations": {
                                "ru": "Мы встретили мистера <w>Ресайда</w> на конференции на прошлой неделе."
                            }
                        },
                        {
                            "text": "The <w>Reside</w> family has lived in this area for over a century.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Ресайд</w> живет в этом районе уже более ста лет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ресайд",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
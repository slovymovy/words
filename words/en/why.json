    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "6d201baf-9ad1-31a6-a66f-d73027061a13",
                    "sense_definition": "Used to ask for a cause, reason, or purpose.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I don’t know <w>why</w> he did that.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, <w>почему</w> он это сделал."
                            }
                        },
                        {
                            "text": "<w>Why</w> is the sky blue?",
                            "target_lang_translations": {
                                "ru": "<w>Почему</w> небо голубое?"
                            }
                        }
                    ],
                    "synonyms": [
                        "how come",
                        "what for"
                    ],
                    "common_phrases": [
                        "why not",
                        "why so"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы спросить о причине, поводе или цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почему",
                                "target_lang_sense_clarification": "о причине"
                            },
                            {
                                "target_lang_word": "зачем",
                                "target_lang_sense_clarification": "о цели"
                            },
                            {
                                "target_lang_word": "отчего",
                                "target_lang_sense_clarification": "по какой причине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddf87375-7759-31ed-8976-dccc03b09895",
                    "sense_definition": "Used with a negative to make a suggestion or give advice.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "suggestion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Why</w> don't you ask her out for dinner?",
                            "target_lang_translations": {
                                "ru": "<w>Почему бы</w> тебе не пригласить ее на ужин?"
                            }
                        },
                        {
                            "text": "<w>Why</w> not tell him how you feel?",
                            "target_lang_translations": {
                                "ru": "<w>Почему бы</w> не сказать ему, что ты чувствуешь?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "why don't you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется с отрицанием, чтобы внести предложение или дать совет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почему бы",
                                "target_lang_sense_clarification": "в конструкции предложения или совета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a8ef221-ac01-317f-8455-29760793412d",
                    "sense_definition": "For which; used to refer to a reason or cause mentioned earlier.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That is <w>why</w> the sky is blue.",
                            "target_lang_translations": {
                                "ru": "Вот <w>почему</w> небо голубое."
                            }
                        },
                        {
                            "text": "That's the reason <w>why</w> I did that.",
                            "target_lang_translations": {
                                "ru": "Это причина, <w>почему</w> я это сделал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "that is why",
                        "the reason why"
                    ],
                    "target_lang_definitions": {
                        "ru": "По причине чего; используется для ссылки на упомянутую ранее причину или повод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почему",
                                "target_lang_sense_clarification": "союзное слово"
                            },
                            {
                                "target_lang_word": "отчего",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c09b9c77-fd8c-360e-ab5a-e7915299607f",
                    "sense_definition": "An exclamation used to express pleasant or unpleasant mild surprise, indignation, or impatience.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Why</w>, that’s ridiculous!",
                            "target_lang_translations": {
                                "ru": "<w>Да</w> это же смешно!"
                            }
                        },
                        {
                            "text": "<w>Why</w> yes, that’s true.",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w> да, это правда."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее приятное или неприятное легкое удивление, возмущение или нетерпение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "усиление сказанного"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "выражение нетерпения или удивления"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "выражение удивления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "660916ea-3a9a-38d3-ad80-7448f7e927a2",
                    "sense_definition": "A reason or explanation for something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A good article will cover the who, the what, the when, the where, the <w>why</w> and the how.",
                            "target_lang_translations": {
                                "ru": "Хорошая статья охватит «кто», «что», «когда», «где», «<w>почему</w>» и «как»."
                            }
                        },
                        {
                            "text": "I need to understand the <w>why</w> of this decision before I agree.",
                            "target_lang_translations": {
                                "ru": "Мне нужно понять <w>причину</w> этого решения, прежде чем я соглашусь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wherefore"
                    ],
                    "common_phrases": [
                        "the why and wherefore"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причина или объяснение чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причина",
                                "target_lang_sense_clarification": "основание"
                            },
                            {
                                "target_lang_word": "обоснование",
                                "target_lang_sense_clarification": "объяснение"
                            }
                        ]
                    }
                },
                {

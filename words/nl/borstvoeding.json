                {
                    "sense_id": "d1df00b9-95c0-3641-a588-35a81ef40fea",
                    "sense_definition": "Het geven van moedermelk uit de borst aan een baby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel moeders kiezen ervoor om hun kind zelf <w>borstvoeding</w> te geven.",
                            "target_lang_translations": {
                                "ru": "Многие матери решают сами <w>кормить грудью</w> своего ребенка."
                            }
                        },
                        {
                            "text": "In het begin kan het geven van <w>borstvoeding</w> best even wennen zijn.",
                            "target_lang_translations": {
                                "ru": "В первое время к <w>кормлению грудью</w> бывает нужно привыкнуть."
                            }
                        }
                    ],
                    "antonyms": [
                        "flesvoeding"
                    ],
                    "common_phrases": [
                        "borstvoeding geven",
                        "borstvoeding krijgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кормление младенца материнским молоком непосредственно из груди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грудное вскармливание",
                                "target_lang_sense_clarification": "процесс питания"
                            },
                            {
                                "target_lang_word": "кормление грудью",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50a5c5af-4b7f-39e9-8061-8f5a11fd3c5c",
                    "sense_definition": "De melk die door de moeder wordt geproduceerd voor de baby.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De baby groeit erg goed op de <w>borstvoeding</w>.",
                            "target_lang_translations": {
                                "ru": "Малыш очень хорошо растет на <w>грудном молоке</w>."
                            }
                        },
                        {
                            "text": "Zij bewaart de afgekolfde <w>borstvoeding</w> in de koelkast voor later.",
                            "target_lang_translations": {
                                "ru": "Она хранит сцеженное <w>грудное молоко</w> в холодильнике на потом."

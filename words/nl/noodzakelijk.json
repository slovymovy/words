    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "723d42af-84d4-3814-ade5-2ab6f69bcf2d",
                    "sense_definition": "Erg nodig; wat absoluut moet gebeuren of aanwezig moet zijn en niet gemist kan worden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>noodzakelijk</w> om voldoende water te drinken tijdens warm weer.",
                            "target_lang_translations": {
                                "en": "It is <w>necessary</w> to drink enough water during hot weather."
                            }
                        },
                        {
                            "text": "Voor deze baan is ervaring niet strikt <w>noodzakelijk</w>, maar wel handig.",
                            "target_lang_translations": {
                                "en": "Experience is not strictly <w>necessary</w> for this job, but it is useful."
                            }
                        }
                    ],
                    "synonyms": [
                        "nodig",
                        "vereist",
                        "onmisbaar",
                        "imperatief"
                    ],
                    "antonyms": [
                        "niet-noodzakelijk"
                    ],
                    "common_phrases": [
                        "noodzakelijk kwaad",
                        "strikt noodzakelijk",
                        "noodzakelijke voorwaarde",
                        "noodzakelijke maatregelen"
                    ],
                    "target_lang_definitions": {
                        "en": "Very needed; something that is absolutely required or must be present and cannot be done without."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "necessary",
                                "target_lang_sense_clarification": "needed to achieve a result or avoid a penalty"
                            },
                            {
                                "target_lang_word": "essential",
                                "target_lang_sense_clarification": "absolutely vital or indispensable"
                            },
                            {
                                "target_lang_word": "required",
                                "target_lang_sense_clarification": "mandatory or needed"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "797aafd0-d1f1-3e53-b9a6-282451817fb4",
                    "sense_definition": "Op een manier die onvermijdelijk is of logischerwijs volgt; per se.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een duurder product is niet <w>noodzakelijk</w> beter van kwaliteit.",
                            "target_lang_translations": {
                                "en": "A more expensive product is not <w>necessarily</w> of better quality."
                            }
                        },
                        {
                            "text": "Dat hoeft niet <w>noodzakelijk</w> waar te zijn, er zijn uitzonderingen.",
                            "target_lang_translations": {
                                "en": "That does not <w>necessarily</w> have to be true; there are exceptions."
                            }
                        }
                    ],
                    "synonyms": [
                        "per se",
                        "beslist",
                        "onvermijdelijk"
                    ],
                    "common_phrases": [
                        "niet noodzakelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that is inevitable or follows logically; per se."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "necessarily",
                                "target_lang_sense_clarification": "inevitably or as a logical consequence"
                            }
                        ]
                    }
                }
            ]
        }
    ]

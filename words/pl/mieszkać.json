    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e22cd606-f5b1-36d8-947b-7bc979d8ebb8",
                    "sense_definition": "Mieć gdzieś swój dom, zajmować lokal mieszkalny lub przebywać w jakimś miejscu na stałe.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie od wielu lat <mieszkają> w małym domku na wsi.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка уже много лет <живут> в маленьком домике в деревне."
                            }
                        },
                        {
                            "text": "Czy ty nadal <mieszkasz> z rodzicami, czy już się wyprowadziłeś?",
                            "target_lang_translations": {
                                "ru": "Ты всё ещё <живёшь> с родителями или уже съехал?"
                            }
                        }
                    ],
                    "synonyms": [
                        "żyć",
                        "przebywać"
                    ],
                    "common_phrases": [
                        "mieszkać w bloku",
                        "mieszkać na wsi",
                        "mieszkać z rodzicami",
                        "mieszkać samemu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь где-либо свой дом, занимать жилое помещение или постоянно находиться в каком-либо месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "основное значение; обитать где-либо"
                            },
                            {
                                "target_lang_word": "проживать",
                                "target_lang_sense_clarification": "официально; занимать жилплощадь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65f5f24e-4375-355d-9706-f36e32fbfab1",
                    "sense_definition": "Znajdować się w czymś lub w kimś, być stale obecnym (zwykle o uczuciach, cechach lub zjawiskach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jej sercu zawsze <mieszkała> głęboka nadzieja na lepsze jutro.",
                            "target_lang_translations": {
                                "ru": "В её сердце всегда <жила> глубокая надежда на лучшее завтра."
                            }
                        },
                        {
                            "text": "Mówi się, że w tym starym zamku <mieszka> duch dawnego właściciela.",
                            "target_lang_translations": {
                                "ru": "Говорят, что в этом старом замке <обитает> дух прежнего владельца."
                            }
                        }
                    ],
                    "synonyms": [
                        "trwać",
                        "pozostawać"
                    ],
                    "common_phrases": [
                        "mieszkać w sercu",
                        "mieszkać w pamięci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в чём-либо или в ком-либо, постоянно присутствовать (обычно о чувствах, качествах или явлениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "переносное значение; существовать (о чувствах, идеях)"
                            },
                            {
                                "target_lang_word": "обитать",
                                "target_lang_sense_clarification": "водиться, находиться (о духах, существах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ea4754a-2c6f-3a71-8224-67ebce1232ac",
                    "sense_definition": "Ociągać się z czymś, zwlekać lub spóźniać się z wykonaniem jakiejś czynności.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "delay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sługa nie <mieszkał> z wykonaniem pilnego rozkazu swojego pana.",
                            "target_lang_translations": {
                                "ru": "Слуга не <мешкал> с выполнением срочного приказа своего господина."
                            }
                        },
                        {
                            "text": "Proszę, nie <mieszkaj> z odpowiedzią na mój list, bo sprawa jest ważna.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <медли> с ответом на моё письмо, так как дело важное."
                            }
                        }
                    ],
                    "synonyms": [
                        "spóźniać się"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie historyczne, obecnie nieużywane"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Медлить с чем-либо, тянуть время или опаздывать с выполнением какого-либо действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешкать",
                                "target_lang_sense_clarification": "медлить, копаться (этимологически родственное слово)"
                            },
                            {
                                "target_lang_word": "медлить",
                                "target_lang_sense_clarification": "затягивать выполнение действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea4a20f9-58a8-356f-8ba5-94fbe91aa654",
                    "sense_definition": "Rodzaj nakrycia głowy o różnym kształcie, najczęściej z płaskim rondem i częścią wypukłą pośrodku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem zawsze zakładam <w>kapelusz</w> słomkowy, żeby chronić głowę przed słońcem.",
                            "target_lang_translations": {
                                "ru": "Летом я всегда надеваю соломенную <w>шляпу</w>, чтобы защитить голову от солнца."
                            }
                        },
                        {
                            "text": "Wiatr zdmuchnął Sewerynowi <w>kapelusz</w> z głowy.",
                            "target_lang_translations": {
                                "ru": "Ветер сдул <w>шляпу</w> с головы Северина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kapelusz słomkowy",
                        "zdejmować kapelusz",
                        "dama w kapeluszu",
                        "kapelusz z rondem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Головной убор, обычно имеющий поля и тулью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шляпа",
                                "target_lang_sense_clarification": "головной убор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "593680d8-fa0f-35b7-9506-d4c89d8585ef",
                    "sense_definition": "Górna, rozszerzona część owocnika grzybów, osadzona na trzonie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten grzyb ma czerwony <w>kapelusz</w> z białymi kropkami.",
                            "target_lang_translations": {
                                "ru": "У этого гриба красная <w>шляпка</w> с белыми точками."
                            }
                        },
                        {
                            "text": "Mrówka chodzi po <w>kapeluszu</w> muchomora.",
                            "target_lang_translations": {
                                "ru": "Муравей ползает по <w>шляпке</w> мухомора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kapelusz grzyba",
                        "duży kapelusz",
                        "pod kapeluszem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя расширенная часть гриба, расположенная на ножке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шляпка",
                                "target_lang_sense_clarification": "часть гриба"
                            }
                        ]
                    }
                }
            ]
        }
    ]

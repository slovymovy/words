{
    "word_family": [
        "koło",
        "kołowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4352db8f-b8df-30da-b4a8-96de9e9a894f",
                    "sense_definition": "Mały przedmiot w kształcie koła lub figura geometryczna; zdrobnienie od <w>koło</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko narysowało na kartce małe <kółko>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок нарисовал на листке маленький <кружок>."
                            }
                        },
                        {
                            "text": "Muszę wymienić pęknięte <kółko> w mojej walizce.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заменить треснувшее <колёсико> в моём чемодане."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kółko i krzyżyk",
                        "kółko graniaste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький предмет круглой формы или геометрическая фигура; уменьшительное от слова «koło»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружок",
                                "target_lang_sense_clarification": "геометрическая фигура или рисунок"
                            },
                            {
                                "target_lang_word": "колёсико",
                                "target_lang_sense_clarification": "маленькое колесо в механизме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ae09fc7-3d97-3c6c-9bac-b07946509997",
                    "sense_definition": "Grupa osób spotykająca się regularnie, aby rozwijać wspólne zainteresowania, lub zajęcia dodatkowe w szkole.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zosia zapisała się na szkolne <kółko> plastyczne.",
                            "target_lang_translations": {
                                "ru": "Зося записалась в школьный <кружок> рисования."
                            }
                        },
                        {
                            "text": "Nasze <kółko> szachowe spotyka się w bibliotece.",
                            "target_lang_translations": {
                                "ru": "Наш шахматный <кружок> встречается в библиотеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zajęcia fakultatywne"
                    ],
                    "common_phrases": [
                        "kółko zainteresowań",
                        "kółko teatralne",
                        "kółko różańcowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, регулярно встречающихся для развития общих интересов, или дополнительные занятия в школе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружок",
                                "target_lang_sense_clarification": "группа по интересам, секция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95703ec1-b652-3fbe-a6e3-976084e5e19c",
                    "sense_definition": "Ustawienie osób trzymających się za ręce w zamkniętym kręgu, często podczas zabawy.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "game_formation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przedszkolaki stanęły w <kółku>, żeby zagrać w grę.",
                            "target_lang_translations": {
                                "ru": "Дошкольники встали в <круг>, чтобы поиграть в игру."
                            }
                        },
                        {
                            "text": "Zróbmy <kółko> i zaśpiewajmy razem piosenkę.",
                            "target_lang_translations": {
                                "ru": "Давайте встанем в <круг> и споём вместе песню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "robić kółko",
                        "tańczyć w kółku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположение людей, держащихся за руки в замкнутом кругу, часто во время игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "построение людей"
                            },
                            {
                                "target_lang_word": "хоровод",
                                "target_lang_sense_clarification": "танец по кругу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5ef17c4-ce37-3c99-b50a-d3950465b4ab",
                    "sense_definition": "Kierownica w samochodzie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "steering_wheel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój tata rzadko siada za <kółko>, woli jeździć autobusem.",
                            "target_lang_translations": {
                                "ru": "Мой папа редко садится за <руль>, он предпочитает ездить на автобусе."
                            }
                        },
                        {
                            "text": "Kierowca pewnie trzymał <kółko> podczas jazdy autostradą.",
                            "target_lang_translations": {
                                "ru": "Водитель уверенно держал <руль> во время езды по шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierownica"
                    ],
                    "common_phrases": [
                        "siadać za kółko",
                        "siedzieć za kółkiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рулевое колесо в автомобиле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руль",
                                "target_lang_sense_clarification": "управление автомобилем"
                            },
                            {
                                "target_lang_word": "баранка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

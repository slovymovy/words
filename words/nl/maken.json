                {
                    "sense_id": "60e59150-2857-3be5-be72-ae775f1aa584",
                    "sense_definition": "Iets nieuws vormen, bouwen, bereiden of tot stand brengen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>maak</w> elke ochtend een vers ontbijt voor mijn kinderen.",
                            "target_lang_translations": {
                                "ru": "Я <w>готовлю</w> свежий завтрак для своих детей каждое утро."
                            }
                        },
                        {
                            "text": "De kunstenaar <w>maakte</w> een prachtig schilderij van het landschap.",
                            "target_lang_translations": {
                                "ru": "Художник <w>написал</w> прекрасную картину пейзажа."
                            }
                        },
                        {
                            "text": "We moeten snel een afspraak <w>maken</w> met de dokter.",
                            "target_lang_translations": {
                                "ru": "Нам нужно срочно <w>записаться</w> к врачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "creëren",
                        "vervaardigen",
                        "fabriceren"
                    ],
                    "common_phrases": [
                        "een afspraak maken",
                        "een fout maken",
                        "huiswerk maken",
                        "een foto maken",
                        "lawaai maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать, строить, готовить или производить что-то новое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать",
                                "target_lang_sense_clarification": "создавать, выполнять"
                            },
                            {
                                "target_lang_word": "сделать",
                                "target_lang_sense_clarification": "совершить, создать"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "творить"
                            },
                            {
                                "target_lang_word": "готовить",
                                "target_lang_sense_clarification": "пищу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1f08b22-1051-36a4-8c6a-1db891d3408b",
                    "sense_definition": "Ervoor zorgen dat iemand of iets in een bepaalde toestand komt; veroorzaken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het goede nieuws <w>maakte</w> hem erg gelukkig.",
                            "target_lang_translations": {
                                "ru": "Хорошие новости <w>сделали</w> его очень счастливым."
                            }
                        },
                        {
                            "text": "Je moet je geen zorgen <w>maken</w> over de resultaten van de toets.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>волноваться</w> о результатах теста."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zorgen maken",
                        "bekend maken",
                        "blij maken",
                        "bang maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить кого-либо или что-либо в определенное состояние; быть причиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать",

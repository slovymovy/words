    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "41107b9f-b999-3bc0-a622-f6aa6d8c022b",
                    "sense_definition": "Pomieszczenie w domu, mieszkaniu lub lokalu gastronomicznym służące do przygotowywania posiłków.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja mama spędza dużo czasu w <w>kuchni</w>, gotując obiady.",
                            "target_lang_translations": {
                                "ru": "Моя мама проводит много времени на <w>кухне</w>, готовя обеды."
                            }
                        },
                        {
                            "text": "Nasza nowa <w>kuchnia</w> jest bardzo jasna i przestronna.",
                            "target_lang_translations": {
                                "ru": "Наша новая <w>кухня</w> очень светлая и просторная."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jasna kuchnia",
                        "siedzieć w kuchni",
                        "kuchnia z jadalnią",
                        "wejść do kuchni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение в доме, квартире или заведении общепита, предназначенное для приготовления пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухня",
                                "target_lang_sense_clarification": "помещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2743a46a-135d-39d9-b5c5-73ec7a2b124f",
                    "sense_definition": "Ogół potraw, przepisów i zwyczajów kulinarnych charakterystycznych dla danego regionu, narodu lub kultury.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji poznaliśmy smaki <w>kuchni</w> włoskiej.",
                            "target_lang_translations": {
                                "ru": "Во время каникул мы познакомились со вкусами итальянской <w>кухни</w>."
                            }
                        },
                        {
                            "text": "Tradycyjna <w>kuchnia</w> polska zawiera dużo dań mięsnych.",
                            "target_lang_translations": {
                                "ru": "Традиционная польская <w>кухня</w> содержит много мясных блюд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kuchnia polska",
                        "kuchnia włoska",
                        "kuchnia regionalna",
                        "kuchnia śródziemnomorska",
                        "kuchnia azjatycka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность блюд, рецептов и кулинарных традиций, характерных для определенного региона, народа или культуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухня",
                                "target_lang_sense_clarification": "кулинарные традиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5412407c-1966-3dfe-b061-8660d4d67cd8",
                    "sense_definition": "Urządzenie służące do gotowania, pieczenia i smażenia potraw, zasilane gazem, prądem lub węglem.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wymienić starą <w>kuchnię</w> gazową na nową.",
                            "target_lang_translations": {
                                "ru": "Мы должны поменять старую газовую <w>плиту</w> на новую."
                            }
                        },
                        {
                            "text": "Babcia postawiła duży garnek z wodą na <w>kuchni</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка поставила большую кастрюлю с водой на <w>плиту</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kuchenka"
                    ],
                    "common_phrases": [
                        "kuchnia gazowa",
                        "kuchnia elektryczna",
                        "kuchnia węglowa",
                        "stać przy kuchni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для приготовления пищи (варки, жарки, выпечки), работающее на газе, электричестве или угле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плита",
                                "target_lang_sense_clarification": "бытовой прибор"
                            },
                            {
                                "target_lang_word": "кухонная плита",
                                "target_lang_sense_clarification": "устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2211ae1-cd73-32ff-b7c5-616e6c7e80f0",
                    "sense_definition": "Zestaw mebli zaprojektowanych do wyposażenia pomieszczenia kuchennego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stolarz zamontował nam wczoraj nową <w>kuchnię</w> na wymiar.",
                            "target_lang_translations": {
                                "ru": "Столяр установил нам вчера новую <w>кухню</w> по индивидуальному заказу."
                            }
                        },
                        {
                            "text": "Planujemy kupić nowoczesną <w>kuchnię</w> z drewnianymi frontami.",
                            "target_lang_translations": {
                                "ru": "Мы планируем купить современную <w>кухню</w> с деревянными фасадами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kuchnia na wymiar",
                        "kuchnia pod zabudowę",
                        "projekt kuchni",
                        "montaż kuchni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект мебели, спроектированный для оборудования кухонного помещения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухонный гарнитур",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "кухня",
                                "target_lang_sense_clarification": "набор мебели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88ff7a73-9a69-3f3e-8542-e2f9fbbb3b1b",
                    "sense_definition": "Zespół pracowników (kucharzy i pomocy) przygotowujących posiłki w restauracji.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner powiedział, że <w>kuchnia</w> ma dzisiaj duże opóźnienia.",
                            "target_lang_translations": {
                                "ru": "Официант сказал, что <w>кухня</w> сегодня сильно задерживает заказы."
                            }
                        },
                        {
                            "text": "Szef <w>kuchni</w> zarządza całym zespołem kucharzy.",
                            "target_lang_translations": {
                                "ru": "Шеф <w>кухни</w> руководит всей командой поваров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szef kuchni",
                        "pomoc kuchni",
                        "kuchnia poleca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коллектив работников (поваров и помощников), готовящих еду в ресторане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухня",
                                "target_lang_sense_clarification": "персонал"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "080cdcb8-4129-3c1e-b2ba-81e72d33ba83",
                    "sense_definition": "Eufemistyczne przekleństwo wyrażające nagłą złość, irytację lub zaskoczenie (używane zamiast wulgarnego słowa).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O <w>kuchnia</w>, zapomniałem zabrać dokumentów z domu!",
                            "target_lang_translations": {
                                "ru": "О <w>блин</w>, я забыл забрать документы из дома!"
                            }
                        },
                        {
                            "text": "<w>Kuchnia</w>, znowu uciekł mi ostatni autobus.",
                            "target_lang_translations": {
                                "ru": "<w>Блин</w>, опять ушел мой последний автобус."
                            }
                        }
                    ],
                    "common_phrases": [
                        "o kuchnia",
                        "kuchnia felek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "eufemizm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Эвфемизм, выражающий внезапную злость, раздражение или удивление (используется вместо грубого ругательства)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блин",
                                "target_lang_sense_clarification": "эвфемизм"
                            },
                            {
                                "target_lang_word": "чёрт",
                                "target_lang_sense_clarification": "восклицание"
                            }
                        ]
                    }
                }
            ]
        },
        {

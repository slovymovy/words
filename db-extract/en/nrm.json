                        ],
                        "examples": [
                            {
                                "example_id": "a4ca39a4-1efe-4b7d-b5f3-c1ce6af47caf",
                                "sense_id": "22d0f23a-9224-35b1-a6b1-5f6395faafe0",
                                "text": "Local knowledge, experience, and traditions of NRM are valuable assets when harnessed for research and action."

                },
                {
                    "sense_id": "146e9546-1de5-326f-a005-4a557ad2791b",
                    "sense_definition": "Geeft de toestand, wijze of omstandigheid aan waarin iets verkeert of gebeurt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "toestand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij las het spannende boek <w>in</w> één ruk uit.",
                            "target_lang_translations": {
                                "en": "He finished the exciting book <w>in</w> one go."
                            }
                        },
                        {
                            "text": "We moeten <w>in</w> elk geval morgen vertrekken.",
                            "target_lang_translations": {
                                "en": "We must leave tomorrow <w>in</w> any case."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het algemeen",
                        "in het bijzonder",
                        "in orde",
                        "in staat zijn"
                    ],
                    "target_lang_definitions": {

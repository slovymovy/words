    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8104a7ef-96a8-31c7-9bf5-b12fbfca97a3",
                    "sense_definition": "Sprawić, że ciemne miejsce staje się jasne dzięki źródłu światła; wypełnić przestrzeń światłem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jasna lampa uliczna <w>rozświetliła</w> ciemny zaułek, którym szliśmy.",
                            "target_lang_translations": {
                                "ru": "Яркий уличный фонарь <w>осветил</w> темный переулок, по которому мы шли."
                            }
                        },
                        {
                            "text": "Nagła błyskawica na moment <w>rozświetliła</w> całe nocne niebo.",
                            "target_lang_translations": {
                                "ru": "Внезапная молния на мгновение <w>озарила</w> все ночное небо."
                            }
                        }
                    ],
                    "synonyms": [
                        "oświetlić",
                        "rozjaśnić"
                    ],
                    "antonyms": [
                        "zaciemnić",
                        "zgasić"
                    ],
                    "common_phrases": [
                        "rozświetlić mrok",
                        "rozświetlić noc",
                        "rozświetlić pomieszczenie",
                        "rozświetlić ciemność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать темное место светлым с помощью источника света; наполнить пространство светом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осветить",
                                "target_lang_sense_clarification": "сделать светлым"
                            },
                            {
                                "target_lang_word": "озарить",
                                "target_lang_sense_clarification": "ярко осветить (часто внезапно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8848c671-20fa-3119-a1c3-2ba332c6710a",
                    "sense_definition": "Stać się jasnym lub wypełnionym światłem (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy wzeszło słońce, niebo nad górami <w>rozświetliło się</w> pięknymi kolorami.",
                            "target_lang_translations": {
                                "ru": "Когда взошло солнце, небо над горами <w>озарилось</w> красивыми красками."
                            }
                        },
                        {
                            "text": "Ekran mojego telefonu nagle <w>rozświetlił się</w> w ciemnym pokoju.",
                            "target_lang_translations": {
                                "ru": "Экран моего телефона внезапно <w>засветился</w> в темной комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozjaśnić się"
                    ],
                    "antonyms": [
                        "ściemnić się",
                        "pociemnieć"
                    ],
                    "common_phrases": [
                        "niebo się rozświetliło",
                        "horyzont się rozświetlił",
                        "ekran się rozświetlił"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать светлым или наполниться светом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "озариться",
                                "target_lang_sense_clarification": "наполниться светом (о небе, горизонте)"
                            },
                            {
                                "target_lang_word": "засветиться",
                                "target_lang_sense_clarification": "начать светиться (об экране, лампе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "331e8fbd-4bdd-35f4-b86e-476978b3e804",
                    "sense_definition": "Sprawić, że czyjaś twarz wygląda na radosną, pogodną, a oczy nabierają blasku.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szczery i szeroki uśmiech nagle <w>rozświetlił</w> jej zmęczoną twarz.",
                            "target_lang_translations": {
                                "ru": "Искренняя и широкая улыбка внезапно <w>озарила</w> ее усталое лицо."
                            }
                        },
                        {
                            "text": "Dobra wiadomość od rodziny <w>rozświetliła</w> jego ponure dotąd spojrzenie.",
                            "target_lang_translations": {
                                "ru": "Хорошая новость от семьи <w>озарила</w> его доселе мрачный взгляд."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpromienić",
                        "ożywić"
                    ],
                    "antonyms": [
                        "zasmucić",
                        "zachmurzyć"
                    ],
                    "common_phrases": [
                        "uśmiech rozświetlił twarz",
                        "radość rozświetliła oczy",
                        "szczęście rozświetliło oblicze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придать лицу радостное, безмятежное выражение; заставить глаза засиять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "озарить",
                                "target_lang_sense_clarification": "придать радостное выражение лицу"
                            },
                            {
                                "target_lang_word": "осветить",
                                "target_lang_sense_clarification": "сделать лицо светлым (метафорически)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "264d15cc-c6b6-3e2d-af43-a9683ae1204f",
                    "sense_definition": "Nabrać pogodnego, radosnego wyrazu pod wpływem pozytywnych emocji (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej twarz <w>rozświetliła się</w>, gdy zobaczyła w drzwiach ukochanego syna.",
                            "target_lang_translations": {
                                "ru": "Ее лицо <w>озарилось</w>, когда она увидела в дверях любимого сына."
                            }
                        },
                        {
                            "text": "Oczy dziecka <w>rozświetliły się</w> na widok wymarzonego prezentu pod choinką.",
                            "target_lang_translations": {
                                "ru": "Глаза ребенка <w>засияли</w> при виде желанного подарка под елкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpromienić się",
                        "rozpogodzić się"
                    ],
                    "antonyms": [
                        "posmutnieć",
                        "zachmurzyć się"
                    ],
                    "common_phrases": [
                        "twarz się rozświetliła",
                        "oczy się rozświetliły",
                        "spojrzenie się rozświetliło"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приобрести безмятежное, радостное выражение под влиянием положительных эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "озариться",
                                "target_lang_sense_clarification": "стать радостным (о лице)"
                            },
                            {
                                "target_lang_word": "засиять",
                                "target_lang_sense_clarification": "заблестеть от радости (о глазах)"
                            },
                            {
                                "target_lang_word": "просветлеть",
                                "target_lang_sense_clarification": "стать ясным, радостным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e53d98-7ffb-3de4-b2e7-101f56a5107a",
                    "sense_definition": "Uczynić coś bardziej zrozumiałym, rzucić światło na trudną lub niejasną sprawę.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mental_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe dowody znalezione przez policję mogą <w>rozświetlić</w> tajemnicę tego zniknięcia.",
                            "target_lang_translations": {
                                "ru": "Новые улики, найденные полицией, могут <w>пролить свет</w> на тайну этого исчезновения."
                            }
                        },
                        {
                            "text": "Lektura tej książki <w>rozświetliła</w> mi wiele skomplikowanych zagadnień filozoficznych.",
                            "target_lang_translations": {
                                "ru": "Чтение этой книги <w>прояснило</w> мне многие сложные философские вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjaśnić",
                        "rozjaśnić"
                    ],
                    "antonyms": [
                        "zagmatwać"
                    ],
                    "common_phrases": [
                        "rozświetlić umysł",
                        "rozświetlić mroki niewiedzy",
                        "rozświetlić zagadkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то более понятным, пролить свет на трудное или неясное дело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пролить свет",
                                "target_lang_sense_clarification": "сделать ясным, понятным"
                            },
                            {
                                "target_lang_word": "прояснить",
                                "target_lang_sense_clarification": "внести ясность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
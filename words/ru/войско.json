            "senses": [
                {
                    "sense_id": "999ccb74-6fec-33a8-b98f-793ed3b4fdca",
                    "sense_definition": "Совокупность вооружённых сил государства; армия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генерал отдал приказ своим <w>войскам</w> готовиться к наступлению.",
                            "target_lang_translations": {
                                "en": "The general gave the order to his <w>troops</w> to prepare for the offensive."
                            }
                        },
                        {
                            "text": "В древности это <w>войско</w> считалось непобедимым и очень сильным.",
                            "target_lang_translations": {
                                "en": "In ancient times this <w>army</w> was considered invincible and very strong."
                            }
                        }
                    ],
                    "synonyms": [
                        "армия"
                    ],
                    "common_phrases": [
                        "сухопутные войска",
                        "ввод войск",
                        "командовать войском",
                        "разбить войско"
                    ],
                    "target_lang_definitions": {
                        "en": "The armed forces of a state or a large organized body of soldiers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "army",
                                "target_lang_sense_clarification": "general military force"
                            },
                            {
                                "target_lang_word": "troops",
                                "target_lang_sense_clarification": "soldiers collectively"
                            },
                            {
                                "target_lang_word": "forces",
                                "target_lang_sense_clarification": "military units"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "439e6355-56b6-32f9-94f4-59a952da554d",
                    "sense_definition": "Группа людей, находящихся под чьим-либо руководством или опекой (обычно в шутливом или ироничном смысле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка собрала своё маленькое <w>войско</w> и повела внуков на обед.",
                            "target_lang_translations": {
                                "en": "Grandma gathered her little <w>army</w> and led the grandkids to lunch."
                            }
                        },
                        {
                            "text": "— Имейте в виду, — сказал Остап, строго оглядывая своё <w>войско</w>, — мародёрства не допущу.",
                            "target_lang_translations": {
                                "en": "“Keep in mind,” said Ostap, strictly looking over his <w>army</w>, “I will not tolerate looting.”"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "шутливо или иронично"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people under someone's leadership or care, often used humorously."
                    },

            "senses": [
                {
                    "sense_id": "1770a356-8c96-3d13-810e-19559387e457",
                    "sense_definition": "A small brown owl (Ninox novaeseelandiae) native to New Zealand and Tasmania, known for its distinctive cry that sounds like its name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>morepork</w> is a nocturnal bird that hunts insects and small animals.",
                            "target_lang_translations": {
                                "ru": "<w>Кукушечья иглоногая сова</w> — это ночная птица, которая охотится на насекомых и мелких животных."
                            }
                        },
                        {
                            "text": "We could hear the haunting call of a <w>morepork</w> echoing through the forest.",
                            "target_lang_translations": {
                                "ru": "Мы слышали, как навязчивый крик <w>морепорка</w> эхом разносится по лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruru",
                        "mopoke"
                    ],
                    "common_phrases": [
                        "call of the morepork"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "New Zealand, Australia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая коричневая сова (Ninox novaeseelandiae), обитающая в Новой Зеландии и Тасмании, известная своим характерным криком, напоминающим её название."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кукушечья иглоногая сова",
                                "target_lang_sense_clarification": "зоологическое название"
                            },
                            {
                                "target_lang_word": "морепорк",
                                "target_lang_sense_clarification": "транслитерация, часто используется в литературе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63518a79-02a6-32ea-a32a-29d006062ac5",
                    "sense_definition": "A person who is slow-moving, lazy, or lacks energy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "character_flaws",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hurry up, you old <w>morepork</w>, or we will be late for the show!",
                            "target_lang_translations": {
                                "ru": "Поторапливайся, старая <w>копуша</w>, иначе мы опоздаем на представление!"
                            }
                        },
                        {
                            "text": "Don't be such a <w>morepork</w>; get up and help us with the cleaning.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>лентяем</w>, вставай и помоги нам с уборкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawdler",
                        "slacker",
                        "lazybones",
                        "loafer",
                        "slowpoke"
                    ],
                    "common_phrases": [
                        "old morepork"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "New Zealand, Australia"
                        }
                    ],
                    "target_lang_definitions": {

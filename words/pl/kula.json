    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7adf8dd-8186-34d4-bf44-e6e554728cb8",
                    "sense_definition": "Przedmiot o kształcie okrągłym, przypominający piłkę; w geometrii bryła, której wszystkie punkty powierzchni są w równej odległości od środka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ziemia jest wielką <kulą> krążącą wokół Słońca.",
                            "target_lang_translations": {
                                "ru": "Земля — это огромный <шар>, вращающийся вокруг Солнца."
                            }
                        },
                        {
                            "text": "Dzieci ulepiły ze śniegu ogromną <kulę>.",
                            "target_lang_translations": {
                                "ru": "Дети слепили из снега огромный <шар>."
                            }
                        }
                    ],
                    "synonyms": [
                        "piłka",
                        "bila"
                    ],
                    "common_phrases": [
                        "kula ziemska",
                        "kula śnieżna",
                        "szklana kula",
                        "kula u nogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет круглой формы, напоминающий мяч; в геометрии — тело, все точки поверхности которого равноудалены от центра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шар",
                                "target_lang_sense_clarification": "геометрическое тело или предмет"
                            },
                            {
                                "target_lang_word": "сфера",
                                "target_lang_sense_clarification": "геометрическая поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b94c4d59-5fb1-3f3f-9cc7-8e3f128a8696",
                    "sense_definition": "Metalowy pocisk wystrzeliwany z broni palnej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz został trafiony <kulą> w ramię podczas bitwy.",
                            "target_lang_translations": {
                                "ru": "Солдат был ранен <пулей> в плечо во время битвы."
                            }
                        },
                        {
                            "text": "Policjanci znaleźli łuski po <kulach> na miejscu zbrodni.",
                            "target_lang_translations": {
                                "ru": "Полицейские нашли гильзы от <пуль> на месте преступления."
                            }
                        }
                    ],
                    "synonyms": [
                        "pocisk"
                    ],
                    "common_phrases": [
                        "dostać kulą",
                        "zbłąkana kula",
                        "kula w łeb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический снаряд, выстреливаемый из огнестрельного оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пуля",
                                "target_lang_sense_clarification": "боеприпас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5f5751d-82d3-3984-9bf5-33631a287e21",
                    "sense_definition": "Podpora ułatwiająca chodzenie osobom niepełnosprawnym lub po urazach nóg, często używana parami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po złamaniu nogi musiał przez dwa miesiące chodzić o <kulach>.",
                            "target_lang_translations": {
                                "ru": "После перелома ноги ему пришлось два месяца ходить на <костылях>."
                            }
                        },
                        {
                            "text": "Pacjent oparł się o <kule>, żeby chwilę odpocząć.",
                            "target_lang_translations": {
                                "ru": "Пациент опёрся на <костыли>, чтобы немного отдохнуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chodzić o kulach",
                        "poruszać się o kulach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опора, облегчающая ходьбу людям с инвалидностью или травмами ног, часто используется парами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костыль",
                                "target_lang_sense_clarification": "медицинское приспособление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1db2ca75-5215-3e2b-8674-1532b2740d95",
                    "sense_definition": "Ciężka, metalowa piłka używana w konkurencji lekkoatletycznej polegającej na jej pchnięciu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski zawodnik zdobył złoty medal w pchnięciu <kulą>.",
                            "target_lang_translations": {
                                "ru": "Польский спортсмен завоевал золотую медаль в толкании <ядра>."
                            }
                        },
                        {
                            "text": "Trening siłowy jest niezbędny, aby daleko rzucać <kulą>.",
                            "target_lang_translations": {
                                "ru": "Силовая тренировка необходима, чтобы далеко толкать <ядро>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pchnięcie kulą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжёлый металлический шар, используемый в легкоатлетической дисциплине (толкание ядра)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ядро",
                                "target_lang_sense_clarification": "спортивный снаряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5941212c-c08d-3472-a011-f7eacd68f38a",
                    "sense_definition": "Pospolite określenie na jądra męskie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz dostał piłką w <kule> i upadł na murawę.",
                            "target_lang_translations": {
                                "ru": "Вратарь получил мячом по <яйцам> и упал на газон."
                            }
                        },
                        {
                            "text": "To przypadkowe uderzenie w <kule> musiało bardzo boleć.",
                            "target_lang_translations": {
                                "ru": "Этот случайный удар по <яйцам> наверняка был очень болезненным."
                            }
                        }
                    ],
                    "synonyms": [
                        "jądro",
                        "jajo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered slightly vulgar or slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Просторечное название мужских яичек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яйца",
                                "target_lang_sense_clarification": "анатомия (грубое/разговорное)"
                            },
                            {
                                "target_lang_word": "шары",
                                "target_lang_sense_clarification": "анатомия (сленг)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e44d9851-0c1c-31b6-ac3d-37f838150b68",
                    "sense_definition": "Trzecia osoba liczby pojedynczej czasu teraźniejszego czasownika <kulać>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnik powoli <kula> ciężką beczkę do magazynu.",
                            "target_lang_translations": {
                                "ru": "Рабочий медленно <катит> тяжёлую бочку на склад."
                            }
                        },
                        {
                            "text": "Dziecko <kula> piłkę po podłodze w stronę psa.",
                            "target_lang_translations": {
                                "ru": "Ребёнок <катит> мяч по полу в сторону собаки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица единственного числа настоящего времени глагола «kulać» (катать/катить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катит",
                                "target_lang_sense_clarification": "перемещает вращая"
                            },
                            {
                                "target_lang_word": "катает",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "602d18c9-2567-3dfe-a3d3-38fead2af827",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Kula> jest naszym nowym sąsiadem z naprzeciwka.",
                            "target_lang_translations": {
                                "ru": "Господин <Куля> — наш новый сосед напротив."
                            }
                        },
                        {
                            "text": "Nie ma dzisiaj w pracy pani <Kuli> z księgowości.",
                            "target_lang_translations": {
                                "ru": "Сегодня нет на работе пани <Кули> из бухгалтерии."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия (мужская или женская)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Куля",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
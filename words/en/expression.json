    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "962577d4-3636-319b-b67e-a38e2a5ae9eb",
                    "sense_definition": "A look on a person's face that shows their thoughts or feelings.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "facial_look",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She looked at him with a puzzled <expression>.",
                            "target_lang_translations": {
                                "ru": "Она посмотрела на него с озадаченным <w>выражением</w> лица."
                            }
                        },
                        {
                            "text": "His facial <expression> changed from joy to sadness when he heard the news.",
                            "target_lang_translations": {
                                "ru": "Его <w>выражение</w> лица сменилось с радостного на печальное, когда он услышал новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "look",
                        "countenance",
                        "face"
                    ],
                    "common_phrases": [
                        "facial expression",
                        "blank expression",
                        "pained expression"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение лица, которое отражает мысли или чувства человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выраже́ние",
                                "target_lang_sense_clarification": "лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c6c7a05-5ec6-385c-85e4-02f3892de06b",
                    "sense_definition": "A word or group of words used in a particular way, often having a special meaning.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "language_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <expression> \"break a leg\" is used to wish someone good luck in the theater.",
                            "target_lang_translations": {
                                "ru": "<w>Выражение</w> \"break a leg\" используется, чтобы пожелать кому-то удачи в театре."
                            }
                        },
                        {
                            "text": "I am not familiar with that idiomatic <expression>.",
                            "target_lang_translations": {
                                "ru": "Я не знаком с этим идиоматическим <w>выражением</w>."
                            }

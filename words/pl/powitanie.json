            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee48bebf-e4ea-3242-b159-f1d6fd0efd22",
                    "sense_definition": "Słowa lub gest, którymi wita się inną osobę przy spotkaniu; wyraz szacunku lub radości z czyjegoś przybycia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To było bardzo ciepłe i serdeczne <powitanie>.",
                            "target_lang_translations": {
                                "ru": "Это было очень теплое и сердечное <w>приветствие</w>."
                            }
                        },
                        {
                            "text": "Na <powitanie> podali sobie ręce i uśmiechnęli się.",
                            "target_lang_translations": {
                                "ru": "В знак <w>приветствия</w> они пожали друг другу руки и улыбнулись."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywitanie"
                    ],
                    "antonyms": [
                        "pożegnanie"
                    ],
                    "common_phrases": [
                        "serdeczne powitanie",
                        "krótkie powitanie",
                        "gest powitania",
                        "słowa powitania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слова или жест, которыми приветствуют другого человека при встрече; выражение уважения или радости от чьего-либо прибытия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствие",
                                "target_lang_sense_clarification": "слова или жест"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "269b0cf4-aa0d-350a-b911-eae1e4e4303f",
                    "sense_definition": "Uroczystość lub oficjalne spotkanie zorganizowane w celu przyjęcia przybywających gości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "greeting_ceremony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uroczyste <powitanie> prezydenta odbyło się na lotnisku.",
                            "target_lang_translations": {
                                "ru": "Торжественная <w>встреча</w> президента состоялась в аэропорту."
                            }
                        },
                        {
                            "text": "Przygotowaliśmy specjalne <powitanie> dla zagranicznej delegacji.",
                            "target_lang_translations": {
                                "ru": "Мы подготовили специальный <w>приём</w> для иностранной делегации."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjęcie",
                        "uroczystość"
                    ],
                    "common_phrases": [
                        "uroczyste powitanie",
                        "oficjalne powitanie",
                        "ceremonia powitania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжество или официальная встреча, организованная для приема прибывающих гостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "церемония"
                            },
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "официальное мероприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8af4ae6e-cc47-3178-ac3f-c1f414096dba",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>powitać</w>, oznaczający sam proces lub czynność witania.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samo <powitanie> wszystkich zebranych zajęło gospodarzowi sporo czasu.",
                            "target_lang_translations": {
                                "ru": "Само <w>приветствие</w> всех собравшихся заняло у хозяина немало времени."
                            }
                        },
                        {
                            "text": "Przez <powitanie> rozumiemy tutaj czynność wykonaną na początku spotkania.",
                            "target_lang_translations": {
                                "ru": "Под <w>приветствием</w> мы понимаем здесь действие, совершаемое в начале встречи."
                            }
                        }
                    ],
                    "synonyms": [

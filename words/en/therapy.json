            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d5b0123-3135-315c-aa5e-6e87b6220146",
                    "sense_definition": "Attempted remediation of a health problem following a diagnosis.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_treatment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor recommended a new drug <w>therapy</w> to fight the infection.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал новую медикаментозную <w>терапию</w> для борьбы с инфекцией."
                            }
                        },
                        {
                            "text": "He needs physical <w>therapy</w> to regain strength in his leg after the accident.",
                            "target_lang_translations": {
                                "ru": "Ему нужна <w>физиотерапия</w>, чтобы восстановить силу в ноге после аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "treatment"
                    ],
                    "common_phrases": [
                        "physical therapy",
                        "drug therapy",
                        "radiation therapy",
                        "gene therapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс лечения заболевания или расстройства здоровья после постановки диагноза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терапия",
                                "target_lang_sense_clarification": "метод лечения"
                            },
                            {
                                "target_lang_word": "лечение",
                                "target_lang_sense_clarification": "общий процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d0148ab-6bfc-3e19-a36b-c53e05795470",
                    "sense_definition": "Psychotherapy; the treatment of mental or psychological disorders.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_treatment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been in <w>therapy</w> for two years to deal with her anxiety.",
                            "target_lang_translations": {
                                "ru": "Она ходит на <w>терапию</w> уже два года, чтобы справиться с тревожностью."
                            }
                        },
                        {
                            "text": "Couples <w>therapy</w> helped them communicate better and save their marriage.",
                            "target_lang_translations": {
                                "ru": "Семейная <w>терапия</w> помогла им наладить общение и спасти брак."
                            }
                        }
                    ],
                    "synonyms": [
                        "treatment",
                        "therapy"
                    ],
                    "common_phrases": [
                        "in therapy",
                        "go to therapy",
                        "group therapy",
                        "couples therapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психотерапия; лечение ментальных или психологических проблем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терапия",
                                "target_lang_sense_clarification": "психологическая помощь"
                            },
                            {
                                "target_lang_word": "психотерапия",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27fd5abc-f5ed-3594-841d-525b548d3428",
                    "sense_definition": "Healing power or quality.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many people find that retail <w>therapy</w> improves their mood after a bad week.",
                            "target_lang_translations": {
                                "ru": "Многие находят, что шопинг-<w>терапия</w> улучшает настроение после тяжелой недели."
                            }
                        },
                        {
                            "text": "The <w>therapy</w> of silence allowed him to clear his mind and relax.",
                            "target_lang_translations": {
                                "ru": "<w>Терапия</w> тишиной позволила ему очистить разум и расслабиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "retail therapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Целебная сила или успокаивающее воздействие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терапия",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f2ce0c1f-3879-3ec6-abb4-7c31c495add0",
                    "sense_definition": "To treat with a therapy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_treat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The specialist is <w>therapying</w> the patient using a new experimental method.",
                            "target_lang_translations": {
                                "ru": "Специалист <w>лечит</w> пациента, используя новый экспериментальный метод."
                            }
                        },
                        {
                            "text": "They successfully <w>therapied</w> the injury without needing surgery.",
                            "target_lang_translations": {
                                "ru": "Они успешно <w>вылечили</w> травму без необходимости операции."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лечить, проводить терапию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечить",
                                "target_lang_sense_clarification": "оказывать медицинскую помощь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff4fb89b-c000-3c3c-bae7-4f0318c47bc4",
                    "sense_definition": "To undergo a therapy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_undergo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has been <w>therapying</w> for months to overcome his childhood trauma.",
                            "target_lang_translations": {
                                "ru": "Он <w>проходит терапию</w> уже несколько месяцев, чтобы преодолеть детскую травму."
                            }
                        },
                        {
                            "text": "After <w>therapying</w> for a long time, she finally felt ready to move on.",
                            "target_lang_translations": {
                                "ru": "После долгого <w>прохождения терапии</w> она наконец почувствовала готовность двигаться дальше."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить курс лечения или терапии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходить терапию",
                                "target_lang_sense_clarification": "быть пациентом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

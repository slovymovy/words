            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "43ec7b39-3960-383f-91d7-d2de56075071",
                    "sense_definition": "Bezig met sterven; in de laatste levensfase.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voetballer ging neer als een <w>stervende</w> zwaan.",
                            "target_lang_translations": {
                                "ru": "Футболист упал, как <w>умирающий</w> лебедь.",
                                "en": "The football player went down like a <w>dying</w> swan."
                            }
                        },
                        {
                            "text": "De arts deed alles wat hij kon voor de <w>stervende</w> patiënt.",
                            "target_lang_translations": {
                                "ru": "Врач делал всё возможное для <w>умирающего</w> пациента.",
                                "en": "The doctor did everything he could for the <w>dying</w> patient."
                            }
                        }
                    ],
                    "synonyms": [
                        "doodgaand"
                    ],
                    "antonyms": [
                        "levend",
                        "springlevend"
                    ],
                    "common_phrases": [
                        "een stervende zwaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в процессе умирания; на последнем этапе жизни.",
                        "en": "In the process of dying; in the final phase of life."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирающий",
                                "target_lang_sense_clarification": "причастие/прилагательное"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "dying",
                                "target_lang_sense_clarification": "adjective"
                            }
                        ]
                    }
                }
            ]
        },
        {

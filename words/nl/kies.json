                    "sense_id": "16df2e27-dc2c-308f-ada7-25c9bf7ec09e",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kiezen</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_kiezen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kies</w> voor de rode auto.",
                            "target_lang_translations": {
                                "ru": "Я <w>выбираю</w> красную машину."
                            }
                        },
                        {
                            "text": "Elke ochtend <w>kies</w> ik mijn eigen kleren uit.",
                            "target_lang_translations": {

{
    "word_family": [
        "brakować",
        "wybrakowany"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4b3b99cc-5996-326c-a6c9-a506b94a416a",
                    "sense_definition": "Słowo oznaczające nieobecność lub niedostateczną ilość kogoś lub czegoś; używane w funkcji orzecznika.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "absence_predicative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej lodówce <w>brak</w> świeżego mleka.",
                            "target_lang_translations": {
                                "ru": "В нашем холодильнике <w>нет</w> свежего молока."
                            }
                        },
                        {
                            "text": "Niestety <w>brak</w> mi dzisiaj czasu na dłuższą rozmowę.",
                            "target_lang_translations": {
                                "ru": "К сожалению, у меня сегодня <w>нет</w> времени на долгий разговор."
                            }
                        }
                    ],
                    "synonyms": [
                        "brakować"
                    ],
                    "common_phrases": [
                        "brak słów",
                        "brak miejsc",
                        "brak danych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, обозначающее отсутствие или недостаточное количество кого-либо или чего-либо; используется в функции сказуемого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отсутствует"
                            },
                            {
                                "target_lang_word": "не хватает",
                                "target_lang_sense_clarification": "недостаточно"
                            },
                            {
                                "target_lang_word": "отсутствие",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ff2b52a-0a17-3803-b01d-cb98ffbb3112",
                    "sense_definition": "Regionalne lub gwarowe określenie oznaczające konieczność lub potrzebę posiadania czegoś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "need_dialectal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do naprawy tego płotu <w>brak</w> nam jeszcze kilku desek.",
                            "target_lang_translations": {
                                "ru": "Для починки этого забора нам <w>не хватает</w> еще нескольких досок."
                            }
                        },
                        {
                            "text": "Jeszcze dużo pieniędzy <w>brak</w> na dokończenie budowy.",
                            "target_lang_translations": {
                                "ru": "На завершение строительства <w>не хватает</w> еще много денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "potrzeba"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "użycie w znaczeniu 'trzeba' lub 'potrzeba'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное или диалектное выражение, означающее необходимость или потребность в чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "требуется"
                            },
                            {
                                "target_lang_word": "не хватает",
                                "target_lang_sense_clarification": "недостает"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22d36979-d6aa-37fc-8880-86ece1bf0b3f",
                    "sense_definition": "Stan, w którym czegoś nie ma, jest niedostępne lub występuje w niewystarczającej ilości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "absence_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągły <w>brak</w> pieniędzy był ich największym zmartwieniem.",
                            "target_lang_translations": {
                                "ru": "Постоянная <w>нехватка</w> денег была их самой большой заботой."
                            }
                        },
                        {
                            "text": "Przez <w>brak</w> dowodów sędzia musiał uniewinnić oskarżonego.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>отсутствия</w> улик судья был вынужден оправдать обвиняемого."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedostatek",
                        "posucha"
                    ],
                    "antonyms": [
                        "nadmiar",
                        "dostatek",
                        "obfitość"
                    ],
                    "common_phrases": [
                        "z braku laku",
                        "brak zainteresowania",
                        "brak wyobraźni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором чего-либо нет, оно недоступно или имеется в недостаточном количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нехватка",
                                "target_lang_sense_clarification": "дефицит"
                            },

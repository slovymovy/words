    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1fdf90fe-d911-3429-bad5-02a87ace9023",
                    "sense_definition": "Een zeer harde wind (windkracht 9 of hoger), vaak gepaard met regen, hagel of sneeuw.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de zware <w>storm</w> zijn er veel bomen omgewaaid in het park.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного <w>шторма</w> в парке повалило много деревьев."
                            }
                        },
                        {
                            "text": "We besloten binnen te blijven totdat de <w>storm</w> weer was gaan liggen.",
                            "target_lang_translations": {
                                "ru": "Мы решили остаться дома, пока <w>буря</w> не утихнет."
                            }
                        }
                    ],
                    "synonyms": [
                        "noodweer"
                    ],
                    "antonyms": [
                        "windstilte"
                    ],
                    "common_phrases": [
                        "de storm gaat liggen",
                        "een zware storm",
                        "stilte voor de storm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильный ветер (9 баллов и выше), часто сопровождающийся дождем, градом или снегом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шторм",
                                "target_lang_sense_clarification": "погодное явление"
                            },
                            {
                                "target_lang_word": "буря",
                                "target_lang_sense_clarification": "сильный ветер с осадками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d67a4172-5e60-3093-8812-de5726500fa7",
                    "sense_definition": "Grote ophef, drukte of verontwaardiging over een bepaald onderwerp, vaak in de media.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_commotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn onhandige uitspraak veroorzaakte een <w>storm</w> van kritiek op sociale media.",
                            "target_lang_translations": {
                                "ru": "Его неловкое высказывание вызвало <w>бурю</w> критики в социальных сетях."
                            }
                        },
                        {
                            "text": "De politicus wachtte rustig af tot de <w>storm</w> in de pers voorbij was.",
                            "target_lang_translations": {
                                "ru": "Политик спокойно ждал, пока <w>буря</w> в прессе уляжется."
                            }
                        }
                    ],
                    "common_phrases": [
                        "storm van kritiek",
                        "storm van protest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурная реакция, переполох или возмущение по какому-либо поводу, часто в СМИ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буря",
                                "target_lang_sense_clarification": "переносное значение: сильное волнение"
                            },
                            {
                                "target_lang_word": "шквал",
                                "target_lang_sense_clarification": "поток (критики, протестов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e21e37c-5f41-3fdf-b193-ba75ce7e70c2",
                    "sense_definition": "Een plotselinge, felle militaire aanval op een vesting of positie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten bereidden zich voor op de <w>storm</w> op de vijandelijke vesting.",
                            "target_lang_translations": {
                                "ru": "Солдаты готовились к <w>штурму</w> вражеской крепости."
                            }
                        },
                        {
                            "text": "Tijdens de <w>storm</w> wisten ze de muren van de stad te doorbreken.",
                            "target_lang_translations": {
                                "ru": "Во время <w>штурма</w> им удалось прорваться сквозь городские стены."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestorming"
                    ],
                    "common_phrases": [
                        "storm lopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапная и яростная военная атака на укрепленную позицию или крепость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штурм",
                                "target_lang_sense_clarification": "военная атака"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "67e756e7-fbe2-3d67-9c2b-f4471c7c6480",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stormen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>storm</w> de kamer binnen omdat ik haast heb.",
                            "target_lang_translations": {
                                "ru": "Я <w>врываюсь</w> в комнату, потому что спешу."
                            }
                        },
                        {
                            "text": "Soms <w>storm</w> ik iets te enthousiast op mijn doel af.",
                            "target_lang_translations": {
                                "ru": "Иногда я <w>несусь</w> к своей цели со слишком большим энтузиазмом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb conjugation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 1-го лица ед. ч. глагола 'stormen': стремительно двигаться или врываться куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "врываюсь",
                                "target_lang_sense_clarification": "стремительно входить"
                            },
                            {
                                "target_lang_word": "несусь",
                                "target_lang_sense_clarification": "быстро двигаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f913b08d-ad12-3fea-bb42-86e9a575f644",
                    "sense_definition": "Gebiedende wijs van <w>stormen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Storm</w> niet zo naar binnen zonder eerst te kloppen!",
                            "target_lang_translations": {
                                "ru": "Не <w>врывайся</w> так внутрь, не постучав сначала!"
                            }
                        },
                        {
                            "text": "<w>Storm</w> eropaf en pak die kans voordat het te laat is.",
                            "target_lang_translations": {
                                "ru": "<w>Бросайся</w> вперед и хватай этот шанс, пока не поздно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb conjugation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения глагола 'stormen': приказ стремительно двигаться или атаковать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "врывайся",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "бросайся",
                                "target_lang_sense_clarification": "призыв к действию"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "przemysł żelazny",
                        "ruda żelazna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с железом как сырьем, элементом или историческим периодом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железный",
                                "target_lang_sense_clarification": "терминологическое значение"
                            }
                        ]
                    }
                }
            ]

                {
                    "sense_id": "61f5a0e1-346e-3c08-b726-02726ac8e684",
                    "sense_definition": "Dodany do tego, co już istnieje, w celu uzupełnienia całości lub zwiększenia jej liczby.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę znaleźć <w>dodatkową</w> pracę, żeby opłacić wszystkie bieżące rachunki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно найти <w>дополнительную</w> работу, чтобы оплатить все текущие счета."
                            }
                        },
                        {
                            "text": "Nauczyciel zadał nam <w>dodatkowe</w> ćwiczenia do zrobienia w domu.",
                            "target_lang_translations": {
                                "ru": "Учитель задал нам <w>дополнительные</w> упражнения на дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "uzupełniający",
                        "nadmiarowy",
                        "nadwyżkowy",
                        "bonusowy"
                    ],
                    "antonyms": [
                        "główny"
                    ],
                    "common_phrases": [
                        "dodatkowy koszt",
                        "dodatkowa opłata",
                        "dodatkowy czas",
                        "dodatkowe informacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавленный к уже существующему для дополнения целого или увеличения количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнительный",
                                "target_lang_sense_clarification": "основное значение; добавленный к чему-либо"
                            },
                            {
                                "target_lang_word": "добавочный",
                                "target_lang_sense_clarification": "служащий дополнением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "664e6b64-6395-3000-876a-5b06b6d0275b",
                    "sense_definition": "Pełniący funkcję pomocniczą, wspierającą lub stanowiący rezerwę dla głównego elementu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samolot jest wyposażony w <w>dodatkowy</w> zbiornik paliwa na wypadek awarii.",
                            "target_lang_translations": {
                                "ru": "Самолет оснащен <w>дополнительным</w> топливным баком на случай аварии."
                            }
                        },
                        {
                            "text": "W razie przerwy w dostawie prądu włączy się <w>dodatkowe</w> zasilanie.",
                            "target_lang_translations": {
                                "ru": "В случае перебоев с электричеством включится <w>резервное</w> питание."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomocniczy",
                        "wspomagający",
                        "rezerwowy"
                    ],
                    "antonyms": [
                        "główny"
                    ],
                    "common_phrases": [

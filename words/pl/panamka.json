    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c90cee7b-0825-31cf-b07a-1220c560f16a",
                    "sense_definition": "Rodzaj lekkiego, letniego kapelusza z materiału, zazwyczaj z miękkim rondem, chroniącego głowę przed słońcem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_hats",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalne dni zawsze zakładam na głowę białą <w>panamkę</w>.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни я всегда надеваю на голову белую <w>панамку</w>."
                            }
                        },
                        {
                            "text": "Mała dziewczynka zgubiła swoją ulubioną <w>panamkę</w> podczas zabawy na plaży.",
                            "target_lang_translations": {
                                "ru": "Маленькая девочка потеряла свою любимую <w>панамку</w> во время игры на пляже."
                            }
                        }
                    ],
                    "synonyms": [
                        "rybaczka"
                    ],
                    "common_phrases": [
                        "płócienna panamka",
                        "nosić panamkę",
                        "włożyć panamkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisownia małą literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лёгкая летняя шляпа из ткани, обычно с мягкими полями, защищающая голову от солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панамка",
                                "target_lang_sense_clarification": "головной убор"
                            },
                            {
                                "target_lang_word": "панама",
                                "target_lang_sense_clarification": "шляпа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a8d976e-67eb-3e4b-b31a-623e8ba6359b",
                    "sense_definition": "Kobieta lub dziewczyna będąca obywatelką Panamy (państwa w Ameryce Środkowej).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "demonyms_panama",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Ta <w>Panamka</w> bardzo ciekawie opowiadała nam o Kanale Panamskim.",
                            "target_lang_translations": {
                                "ru": "Эта <w>панамка</w> очень интересно рассказывала нам о Панамском канале."
                            }
                        },
                        {
                            "text": "Podczas wakacji naszą przewodniczką była sympatyczna <w>Panamka</w>.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска нашим гидом была симпатичная <w>панамка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodowita Panamka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisownia wielką literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девушка, являющаяся гражданкой Панамы (государства в Центральной Америке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панамка",
                                "target_lang_sense_clarification": "жительница страны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73e12a82-6ae0-38e7-977c-54c66f85fec6",
                    "sense_definition": "Mieszkanka miasta Panama, będącego stolicą państwa o tej samej nazwie.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "demonyms_panama",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako rodowita <w>panamka</w>, znała wszystkie historyczne zakamarki stolicy.",
                            "target_lang_translations": {
                                "ru": "Как коренная <w>панамка</w>, она знала все исторические уголки столицы."
                            }
                        },
                        {
                            "text": "Spotkana na ulicy <w>panamka</w> poleciła nam najlepszą restaurację w mieście.",
                            "target_lang_translations": {
                                "ru": "Встреченная на улице <w>панамка</w> порекомендовала нам лучший ресторан в городе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisownia małą literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жительница города Панама, столицы одноимённого государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панамка",
                                "target_lang_sense_clarification": "жительница города"
                            },
                            {
                                "target_lang_word": "жительница Панамы",
                                "target_lang_sense_clarification": "города"
                            }
                        ]
                    }
                }
            ]
        }

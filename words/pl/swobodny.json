    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "54966f89-d7fd-35a2-a13f-4cc9bfdd40b2",
                    "sense_definition": "Zachowujący się w sposób naturalny, niewymuszony i bez zbędnego skrępowania; świadczący o takim zachowaniu",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek to bardzo <w>swobodny</w> człowiek, z którym łatwo nawiązać kontakt.",
                            "target_lang_translations": {
                                "ru": "Марек — очень <w>непринуждённый</w> человек, с которым легко найти общий язык."
                            }
                        },
                        {
                            "text": "Atmosfera na wczorajszym spotkaniu była bardzo <w>swobodna</w> i przyjazna.",
                            "target_lang_translations": {
                                "ru": "Атмосфера на вчерашней встрече была очень <w>непринуждённой</w> и дружелюбной."
                            }
                        }
                    ],
                    "synonyms": [
                        "naturalny",
                        "bezpośredni",
                        "nieskrępowany",
                        "niewymuszony",
                        "otwarty",
                        "spontaniczny",
                        "luźny",
                        "bezceremonialny",
                        "nonszalancki"
                    ],
                    "common_phrases": [
                        "czuć się swobodnie",
                        "swobodna atmosfera",
                        "swobodna rozmowa",
                        "swobodny styl bycia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ведущий себя естественно, без напряжения и стеснения; свидетельствующий о таком поведении"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непринуждённый",
                                "target_lang_sense_clarification": "естественный, раскованный"
                            },
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "не стеснённый, легкий в общении"
                            },
                            {
                                "target_lang_word": "естественный",
                                "target_lang_sense_clarification": "натуральный, без притворства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcd822de-a56c-3491-962a-a6f16c37a7ab",
                    "sense_definition": "O ubraniu: nieprzylegający ściśle do ciała, niekrępujący ruchów",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wycieczkę w góry najlepiej założyć wygodny i <w>swobodny</w> strój sportowy.",
                            "target_lang_translations": {
                                "ru": "Для похода в горы лучше всего надеть удобную и <w>свободную</w> спортивную одежду."
                            }
                        },
                        {
                            "text": "W upalne dni najchętniej noszę <w>swobodne</w> lniane koszule.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни я охотнее всего ношу <w>свободные</w> льняные рубашки."
                            }
                        }
                    ],
                    "synonyms": [
                        "luźny"
                    ],
                    "common_phrases": [
                        "swobodny strój",
                        "swobodny krój",
                        "swobodna sukienka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об одежде: не облегающий плотно тело, не стесняющий движений"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "не тесный"
                            },
                            {
                                "target_lang_word": "просторный",
                                "target_lang_sense_clarification": "широкий, вместительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78c22d3d-5dcf-362b-8a39-173261c110fd",
                    "sense_definition": "Niepodlegający przymusowi, zakazom ani zewnętrznym ograniczeniom; mający pełną wolność wyboru lub działania",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "freedom_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy obywatel w demokratycznym państwie powinien mieć <w>swobodny</w> dostęp do informacji.",
                            "target_lang_translations": {
                                "ru": "Каждый гражданин в демократическом государстве должен иметь <w>свободный</w> доступ к информации."
                            }
                        },
                        {
                            "text": "Masz <w>swobodny</w> wybór i możesz zdecydować, co chcesz robić w przyszłości.",
                            "target_lang_translations": {
                                "ru": "У тебя <w>свободный</w> выбор, и ты можешь решить, чем хочешь заниматься в будущем."
                            }
                        }
                    ],
                    "synonyms": [
                        "wolny",
                        "nieograniczony",
                        "nieskrępowany"
                    ],
                    "antonyms": [
                        "ograniczony"
                    ],
                    "common_phrases": [
                        "swobodny dostęp",
                        "swobodny wybór",
                        "swobodna decyzja",
                        "swobodny rynek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не ограниченный запретами, принуждением или внешними рамками; имеющий полную свободу выбора"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "независимый, не ограниченный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67640442-7693-37e5-9691-ab79b3f78d8e",
                    "sense_definition": "Mogący się przemieszczać, przepływać lub odbywać bez przeszkód fizycznych",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzeka ma tutaj <w>swobodny</w> bieg, ponieważ nie zbudowano na niej żadnych tam.",
                            "target_lang_translations": {
                                "ru": "Река имеет здесь <w>свободное</w> течение, так как на ней не построено никаких плотин."
                            }
                        },
                        {
                            "text": "Należy zapewnić poszkodowanemu <w>swobodny</w> dopływ świeżego powietrza.",
                            "target_lang_translations": {
                                "ru": "Следует обеспечить пострадавшему <w>свободный</w> приток свежего воздуха."
                            }
                        }
                    ],
                    "synonyms": [

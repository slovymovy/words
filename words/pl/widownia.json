                        "arena"
                    ],
                    "common_phrases": [
                        "widownia polityczna",
                        "zejść z widowni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или фон, на котором происходят важные общественные, политические или исторические события."
                    },

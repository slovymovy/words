        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "06e0c879-7004-3695-925e-432982402962",
                    "sense_definition": "To make someone feel annoyed, frustrated, or worried.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The difficult math problem began to <w>vex</w> the students who could not solve it.",
                            "target_lang_translations": {
                                "ru": "Трудная задача по математике начала <w>раздражать</w> студентов, которые не могли её решить."
                            }
                        },
                        {
                            "text": "He was <w>vexed</w> by the constant delays in the project schedule.",
                            "target_lang_translations": {
                                "ru": "Он был <w>раздражён</w> постоянными задержками в графике проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "annoy",
                        "irritate",
                        "bother",
                        "irk",
                        "exasperate",
                        "provoke",
                        "frustrate",
                        "rile"
                    ],
                    "antonyms": [
                        "comfort"
                    ],
                    "common_phrases": [
                        "vexed by",
                        "vexed at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-либо чувствовать раздражение, разочарование или беспокойство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздражать",
                                "target_lang_sense_clarification": "вызывать недовольство или злость"
                            },
                            {
                                "target_lang_word": "досаждать",
                                "target_lang_sense_clarification": "причинять неприятности, надоедать"
                            },
                            {
                                "target_lang_word": "сердить",
                                "target_lang_sense_clarification": "приводить в состояние гнева"
                            }
                        ]
                    }
                },
                {

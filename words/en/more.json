    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "e4a0d721-2a6b-380d-b421-3eff75d97ffa",
                    "sense_definition": "The comparative form of <w>many</w>: in greater number.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_discrete",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are <w>more</w> ways to solve this problem than I can count right now.",
                            "target_lang_translations": {
                                "ru": "Существует <w>больше</w> способов решить эту проблему, чем я могу сейчас сосчитать."
                            }
                        },
                        {
                            "text": "Last year, we received <w>more</w> applications from new students than ever before.",
                            "target_lang_translations": {
                                "ru": "В прошлом году мы получили <w>больше</w> заявок от новых студентов, чем когда-либо прежде."
                            }
                        }
                    ],
                    "antonyms": [
                        "fewer"
                    ],
                    "common_phrases": [
                        "more than",
                        "many more",
                        "one more"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень от \"many\" (много): в большем количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "сравнительная степень (по количеству)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "840152a0-147e-33b4-bd06-48e8dac33cbe",
                    "sense_definition": "The comparative form of <w>much</w>: in greater quantity, amount, or proportion.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_continuous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is <w>more</w> caffeine in this coffee than in the tea you usually drink.",
                            "target_lang_translations": {
                                "ru": "В этом кофе <w>больше</w> кофеина, чем в чае, который ты обычно пьешь."
                            }
                        },
                        {
                            "text": "We need to spend <w>more</w> time working on this project to finish it.",
                            "target_lang_translations": {
                                "ru": "Нам нужно потратить <w>больше</w> времени на этот проект, чтобы закончить его."
                            }
                        }
                    ],
                    "antonyms": [
                        "less"
                    ],
                    "common_phrases": [
                        "much more",
                        "no more",
                        "little more"
                    ],
                    "target_lang_definitions": {

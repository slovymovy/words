            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f886af8f-4e6b-366d-bc25-8de55b590353",
                    "sense_definition": "Pozbawiać kogoś prawa do dziedziczenia majątku po zmarłym, zazwyczaj poprzez odpowiedni zapis w testamencie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_family",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawo spadkowe dokładnie określa przyczyny, dla których można kogoś skutecznie <w>wydziedziczać</w>.",
                            "target_lang_translations": {
                                "ru": "Наследственное право точно определяет причины, по которым можно кого-то законно <w>лишать наследства</w>."
                            }
                        },
                        {
                            "text": "Ojciec groził, że będzie <w>wydziedziczać</w> każde dziecko, które sprzeciwi się jego woli.",
                            "target_lang_translations": {
                                "ru": "Отец грозил, что будет <w>лишать наследства</w> каждого ребенка, который воспротивится его воле."
                            }
                        }
                    ],
                    "antonyms": [
                        "uposażać"
                    ],
                    "common_phrases": [
                        "wydziedziczać syna",
                        "wydziedziczać córkę",
                        "wydziedziczać z testamentu",
                        "wydziedziczać krewnych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать кого-либо права на наследование имущества умершего, обычно посредством соответствующей записи в завещании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишать наследства",
                                "target_lang_sense_clarification": "юридически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "282af3ad-eaec-3ad2-803c-4a3bcdfe6f07",
                    "sense_definition": "Rezygnować z praw do spadku lub dobrowolnie odcinać się od własnej rodziny i jej tradycji (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_family",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody arystokrata postanowił <w>wydziedziczać</w> się ze swojej klasy społecznej, by żyć jak zwykły obywatel.",
                            "target_lang_translations": {
                                "ru": "Молодой аристократ решил <w>отрекаться</w> от своего социального класса, чтобы жить как обычный гражданин."
                            }
                        },
                        {
                            "text": "Nie warto <w>wydziedziczać</w> się z rodzinnej tradycji tylko z powodu chwilowego buntu.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>отрекаться</w> от семейной традиции только из-за минутного бунта."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrzekać się"
                    ],
                    "antonyms": [
                        "dziedziczyć"
                    ],
                    "common_phrases": [
                        "wydziedziczać się z rodziny",
                        "wydziedziczać się z tradycji"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny używany z „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказываться от прав на наследство или добровольно разрывать связи с собственной семьей и её традициями (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрекаться",
                                "target_lang_sense_clarification": "от семьи или принципов"
                            },
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "от прав или связей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

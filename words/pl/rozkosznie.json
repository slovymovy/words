{
    "word_family": [
        "rozkosz",
        "rozkoszny",
        "rozkoszować się"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f2ba1713-e6fe-335e-871f-856cc73d5a2a",
                    "sense_definition": "W sposób dający poczucie błogości, wielkiej przyjemności lub zachwytu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wolne dni lubię <w>rozkosznie</w> leniuchować w łóżku aż do południa.",
                            "target_lang_translations": {
                                "ru": "В выходные я люблю <w>блаженно</w> лениться в кровати до самого полдня."
                            }
                        },
                        {
                            "text": "Ten czekoladowy deser smakuje <w>rozkosznie</w> i rozpływa się w ustach.",
                            "target_lang_translations": {
                                "ru": "Этот шоколадный десерт <w>восхитителен</w> на вкус и тает во рту."
                            }
                        }
                    ],
                    "synonyms": [
                        "błogo",
                        "cudownie",
                        "przyjemnie",
                        "zachwycająco"
                    ],
                    "antonyms": [
                        "nieprzyjemnie",
                        "okropnie",
                        "boleśnie"
                    ],
                    "common_phrases": [
                        "rozkosznie leniwy",
                        "przeciągnąć się rozkosznie",
                        "smakować rozkosznie",
                        "rozkosznie ciepły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, доставляющим чувство блаженства, огромного удовольствия или восторга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блаженно",
                                "target_lang_sense_clarification": "выражая или испытывая высшее наслаждение и покой"
                            },
                            {
                                "target_lang_word": "восхитительно",
                                "target_lang_sense_clarification": "очень приятно, вызывая восторг (часто о вкусе или ощущениях)"
                            },
                            {
                                "target_lang_word": "упоительно",
                                "target_lang_sense_clarification": "доставляя сильное наслаждение, заставляя забыть обо всем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec34d9b8-6ba5-3f56-bead-68d8beeffa0b",
                    "sense_definition": "W sposób budzący sympatię swoim urokiem, wdziękiem lub słodyczą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "charm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały szczeniak wyglądał <w>rozkosznie</w>, gdy próbował wejść na kanapę.",
                            "target_lang_translations": {
                                "ru": "Маленький щенок выглядел <w>умилительно</w>, когда пытался забраться на диван."
                            }
                        },
                        {
                            "text": "Jej córeczka uśmiecha się tak <w>rozkosznie</w>, że każdemu od razu poprawia humor.",
                            "target_lang_translations": {
                                "ru": "Её дочурка улыбается так <w>очаровательно</w>, что у всех сразу поднимается настроение."
                            }
                        }
                    ],
                    "synonyms": [
                        "uroczo",
                        "słodko",
                        "wdzięcznie",
                        "czarująco"
                    ],
                    "antonyms": [
                        "odpychająco",
                        "wstrętnie"
                    ],
                    "common_phrases": [
                        "wyglądać rozkosznie",
                        "uśmiechać się rozkosznie",
                        "rozkosznie nieporadny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, вызывающим симпатию своим обаянием, грацией или миловидностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очаровательно",
                                "target_lang_sense_clarification": "привлекательно, пленяя своим шармом"
                            },
                            {
                                "target_lang_word": "умилительно",
                                "target_lang_sense_clarification": "трогательно, вызывая нежные чувства"
                            },
                            {
                                "target_lang_word": "прелестно",
                                "target_lang_sense_clarification": "очень мило и красиво"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
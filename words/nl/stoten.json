                {
                    "sense_id": "e3b17deb-5fc8-3d63-8ddf-accb7e0b46fa",
                    "sense_definition": "Met een korte, snelle beweging duwen of raken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "push_bump",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>stootte</w> per ongeluk zijn glas water om.",
                            "target_lang_translations": {
                                "en": "He accidentally <w>knocked</w> over his glass of water.",
                                "ru": "Он случайно <w>задел</w> и опрокинул свой стакан с водой."
                            }
                        },
                        {
                            "text": "Pas op dat je je hoofd niet <w>stoot</w> aan het lage plafond.",
                            "target_lang_translations": {
                                "en": "Watch out you don't <w>bump</w> your head on the low ceiling.",
                                "ru": "Осторожно, не <w>ударься</w> головой о низкий потолок."
                            }
                        }
                    ],
                    "synonyms": [
                        "duwen",
                        "stompen",
                        "botsen"
                    ],
                    "common_phrases": [
                        "zijn hoofd stoten",
                        "zijn teen stoten",
                        "iemand voor het hoofd stoten"
                    ],
                    "target_lang_definitions": {
                        "en": "To push or hit with a short, quick movement.",
                        "ru": "Толкать или ударять коротким, быстрым движением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bump",
                                "target_lang_sense_clarification": "to hit a body part against something hard"
                            },
                            {
                                "target_lang_word": "knock",
                                "target_lang_sense_clarification": "to strike or hit, often causing something to fall"
                            },
                            {
                                "target_lang_word": "jab",
                                "target_lang_sense_clarification": "to poke or thrust abruptly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ударяться",
                                "target_lang_sense_clarification": "обо что-то (о частях тела)"
                            },
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "резким движением"
                            },
                            {
                                "target_lang_word": "задевать",
                                "target_lang_sense_clarification": "случайно касаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "412036ab-76ff-3c57-a8f9-a6d254c50f97",
                    "sense_definition": "Schokken of hevig door elkaar schudden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shock_shake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude wagen <w>stootte</w> en rammelde over de slechte weg.",
                            "target_lang_translations": {
                                "en": "The old car <w>bumped</w> and rattled over the bad road.",
                                "ru": "Старая машина <w>тряслась</w> и дребезжала на плохой дороге."
                            }
                        },
                        {
                            "text": "Het voertuig <w>stootte</w> hevig toen het door een diepe kuil reed.",
                            "target_lang_translations": {
                                "en": "The vehicle <w>jolted</w> heavily when it drove through a deep pothole.",
                                "ru": "Транспортное средство сильно <w>тряхнуло</w>, когда оно проезжало через глубокую яму."
                            }
                        }
                    ],
                    "synonyms": [

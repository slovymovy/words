{
    "word_family": [
        "vrijlating"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "98135117-0b1b-324f-9139-2967b57a3fbf",
                    "sense_definition": "Toestaan dat iemand of iets uit gevangenschap of opsluiting vertrekt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft de verdachte wegens gebrek aan bewijs <w>vrijgelaten</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>освободила</w> подозреваемого из-за недостатка улик.",
                                "en": "The police <w>released</w> the suspect due to lack of evidence."
                            }
                        },
                        {
                            "text": "De dieren werden in het wild <w>vrijgelaten</w> nadat ze waren verzorgd.",
                            "target_lang_translations": {
                                "ru": "Животных <w>выпустили</w> на волю после того, как их вылечили.",
                                "en": "The animals were <w>released</w> into the wild after they had been cared for."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevrijden",
                        "loslaten",
                        "in vrijheid stellen"
                    ],
                    "antonyms": [
                        "opsluiten",
                        "gevangennemen",
                        "vastzetten",
                        "arresteren"
                    ],
                    "common_phrases": [
                        "op borgtocht vrijlaten",
                        "in het wild vrijlaten",
                        "wegens gebrek aan bewijs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволить кому-либо или чему-либо покинуть место заключения или ограничения свободы.",
                        "en": "To allow someone or something to leave captivity or confinement."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освобождать",
                                "target_lang_sense_clarification": "из заключения"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "на свободу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "from captivity or custody"
                            },
                            {
                                "target_lang_word": "set free",
                                "target_lang_sense_clarification": "liberate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8906dd9-02b8-395b-bc92-9105f6024b5e",
                    "sense_definition": "Een doorgang of ruimte onbezet houden zodat deze toegankelijk blijft.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor onze veiligheid moeten we de brandtrappen altijd <w>vrijlaten</w>.",
                            "target_lang_translations": {
                                "ru": "Ради нашей безопасности мы должны всегда <w>оставлять свободными</w> пожарные лестницы.",
                                "en": "For our safety, we must always <w>keep</w> the fire escapes <w>clear</w>."
                            }
                        },
                        {
                            "text": "Wilt u alstublieft de uitrit voor de ambulance <w>vrijlaten</w>?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы, пожалуйста, <w>оставить свободным</w> выезд для скорой помощи?",
                                "en": "Could you please <w>keep</w> the exit <w>clear</w> for the ambulance?"
                            }
                        }
                    ],
                    "synonyms": [
                        "vrijhouden",
                        "openlaten"
                    ],
                    "antonyms": [
                        "blokkeren",
                        "versperren"
                    ],
                    "common_phrases": [
                        "de doorgang vrijlaten",
                        "een plek vrijlaten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять проход или пространство незанятым для обеспечения доступа.",
                        "en": "To keep a passage or space unoccupied so that it remains accessible."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять свободным",
                                "target_lang_sense_clarification": "проход, место"
                            },
                            {
                                "target_lang_word": "не занимать",
                                "target_lang_sense_clarification": "пространство"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "keep clear",
                                "target_lang_sense_clarification": "a passage or exit"
                            },
                            {
                                "target_lang_word": "leave clear",
                                "target_lang_sense_clarification": "ensure space remains open"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
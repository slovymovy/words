                },
                {
                    "sense_id": "0da9368a-2d60-3013-81d3-fa9052cd33ea",
                    "sense_definition": "miasto we wschodnich Niemczech położone nad rzeką Odrą, bezpośrednio przy granicy z Polską",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cities",
                    "name_type": "place_name",
                    "examples": [

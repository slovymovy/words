                {
                    "sense_id": "89e0c3b5-14ce-35c4-87f3-db3bb6583ac9",
                    "sense_definition": "nieunikniona przyszłość, los lub siła wyższa, która decyduje o życiu człowieka; coś, co musi się stać",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destiny",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wróżka wywróżyła mi, że moim <w>przeznaczeniem</w> jest zawód aktora i życie w bogactwie.",
                            "target_lang_translations": {
                                "ru": "Гадалка нагадала мне, что моим <w>предназначением</w> является профессия актёра и жизнь в богатстве."
                            }
                        },
                        {
                            "text": "Wielu ludzi wierzy, że ich spotkanie było zapisane przez <w>przeznaczenie</w>.",
                            "target_lang_translations": {
                                "ru": "Многие люди верят, что их встреча была предначертана <w>судьбой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "los",
                        "fatum"
                    ],
                    "common_phrases": [
                        "wierzyć w przeznaczenie",
                        "oszukać przeznaczenie",
                        "twoje przeznaczenie",
                        "złe przeznaczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неизбежное будущее, судьба или высшая сила, определяющая жизнь человека"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судьба",
                                "target_lang_sense_clarification": "участь, доля, рок"
                            },
                            {
                                "target_lang_word": "предназначение",
                                "target_lang_sense_clarification": "то, что предопределено; высшая цель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d00369d3-4c23-3732-97b8-fe7f064ada9d",
                    "sense_definition": "cel, dla którego dana rzecz została stworzona lub do którego ma służyć",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stworzyłem urządzenie, którego <w>przeznaczeniem</w> jest pomoc osobom niesłyszącym.",
                            "target_lang_translations": {
                                "ru": "Я создал устройство, <w>предназначением</w> которого является помощь глухим людям."
                            }
                        },
                        {
                            "text": "Należy zawsze używać narzędzi zgodnie z ich pierwotnym <w>przeznaczeniem</w>.",
                            "target_lang_translations": {
                                "ru": "Следует всегда использовать инструменты в соответствии с их первоначальным <w>назначением</w>."
                            }
                        }
                    ],
                    "synonyms": [

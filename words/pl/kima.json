    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6628813e-c215-3494-b38d-717cf665ec79",
                    "sense_definition": "trzecia osoba liczby pojedynczej czasu teraźniejszego czasownika <w>kimać</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sleep_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat teraz <kima> w swoim pokoju po szkole.",
                            "target_lang_translations": {
                                "ru": "Мой брат сейчас <кемарит> в своей комнате после школы."
                            }
                        },
                        {
                            "text": "Ona zawsze <kima> przed telewizorem wieczorem.",
                            "target_lang_translations": {
                                "ru": "Она всегда <кемарит> перед телевизором по вечерам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "base verb is colloquial"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени глагола kimać (кемарить)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кемарит",
                                "target_lang_sense_clarification": "дремлет, спит (разговорное)"
                            },
                            {
                                "target_lang_word": "дрыхнет",
                                "target_lang_sense_clarification": "крепко спит (просторечное)"
                            }
                        ]
                    }
                }
            ]
        },
        {

            "senses": [
                {
                    "sense_id": "c0ffe819-777f-3467-9dfd-8527da528a75",
                    "sense_definition": "Showing a willingness to take surprisingly bold risks.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "risk_taking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general formulated an <w>audacious</w> plan to capture the enemy city overnight.",
                            "target_lang_translations": {
                                "ru": "Генерал разработал <w>дерзкий</w> план захвата вражеского города за одну ночь."
                            }
                        },
                        {
                            "text": "It was an <w>audacious</w> move to start a new business during the economic crisis.",
                            "target_lang_translations": {
                                "ru": "Это был <w>смелый</w> шаг — начать новый бизнес во время экономического кризиса."
                            }
                        },
                        {
                            "text": "The climbers made an <w>audacious</w> attempt to reach the summit in winter.",
                            "target_lang_translations": {
                                "ru": "Альпинисты предприняли <w>отважную</w> попытку достичь вершины зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bold",
                        "daring",
                        "courageous",
                        "intrepid",
                        "venturesome"
                    ],
                    "antonyms": [
                        "cautious",
                        "prudent"
                    ],
                    "common_phrases": [
                        "audacious plan",
                        "audacious move",
                        "audacious attempt",
                        "audacious goal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявляющий готовность идти на смелые, неожиданные риски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дерзкий",
                                "target_lang_sense_clarification": "рискованный, новаторский"
                            },
                            {
                                "target_lang_word": "сме́лый",
                                "target_lang_sense_clarification": "храбрый, решительный"
                            },
                            {
                                "target_lang_word": "отва́жный",

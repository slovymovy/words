                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta stara pieśń <w>bandoska</w> opowiada o trudach pracy na obczyźnie.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>батрацкая</w> песня рассказывает о тяготах работы на чужбине."
                            }
                        },
                        {
                            "text": "Tradycja <w>bandoska</w> zanikła wraz ze zmianami w rolnictwie.",
                            "target_lang_translations": {
                                "ru": "<w>Батрацкая</w> традиция исчезла вместе с изменениями в сельском хозяйстве."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rodzaj żeński"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская форма прилагательного, относящегося к сезонным сельскохозяйственным рабочим (бандосам)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "батрацкая",
                                "target_lang_sense_clarification": "относящаяся к наемным рабочим"
                            },
                            {
                                "target_lang_word": "сезонная",
                                "target_lang_sense_clarification": "относящаяся к сезонным работам"
                            }
                        ]
                    }
                }
            ]
        }

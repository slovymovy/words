                            {
                                "target_lang_word": "законодательство",
                                "target_lang_sense_clarification": "совокупность законов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25fae828-bf31-3734-b971-b4d13fcdea4d",
                    "sense_definition": "Kierunek studiów uniwersyteckich lub dyscyplina naukowa zajmująca się systemami prawnymi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "study_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra studiuje <w>prawo</w> na Uniwersytecie Jagiellońskim.",
                            "target_lang_translations": {
                                "ru": "Моя сестра изучает <w>право</w> в Ягеллонском университете."
                            }
                        },
                        {
                            "text": "Dostanie się na <w>prawo</w> wymaga zdania egzaminów z dobrym wynikiem.",
                            "target_lang_translations": {
                                "ru": "Поступление на <w>юридический факультет</w> требует сдачи экзаменов с хорошим результатом."
                            }

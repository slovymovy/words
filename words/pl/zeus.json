    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f9a84cb9-5f76-35a2-bdd0-bf94cd72dc3a",
                    "sense_definition": "najwyższy bóg w mitologii greckiej, władca nieba i ziemi, pan Olimpu, utożsamiany z rzymskim Jowiszem",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Atrybutami <w>Zeusa</w> były pioruny, berło, egida, dąb oraz orzeł.",
                            "target_lang_translations": {
                                "ru": "Атрибутами <w>Зевса</w> были молнии, скипетр, эгида, дуб и орел."
                            }
                        },
                        {
                            "text": "Spartakus był rozgniewany jak <w>Zeus</w>, gdy ciska piorunami.",
                            "target_lang_translations": {
                                "ru": "Спартак был разгневан, как <w>Зевс</w>, когда тот мечет молнии."
                            }
                        }
                    ],
                    "synonyms": [
                        "Dzeus",
                        "Jowisz",
                        "Jupiter"
                    ],
                    "common_phrases": [
                        "gromowładny Zeus",
                        "posąg Zeusa",
                        "świątynia Zeusa"
                    ],
                    "target_lang_definitions": {
                        "ru": "верховный бог в древнегреческой мифологии, владыка неба и земли, повелитель Олимпа, отождествляемый с римским Юпитером"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Зевс",
                                "target_lang_sense_clarification": "бог в греческой мифологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43a8319f-7397-3974-bdf5-ec3664681e60",
                    "sense_definition": "planetoida o numerze katalogowym 5731, należąca do grupy Apolla",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "<w>Zeus</w> porusza się po orbicie, która ma peryhelium położone bliżej Słońca niż Ziemia.",
                            "target_lang_translations": {
                                "ru": "<w>Зевс</w> движется по орбите, перигелий которой расположен ближе к Солнцу, чем Земля."
                            }
                        },
                        {
                            "text": "Astronomowie dokładnie zbadali orbitę, po której krąży <w>Zeus</w>.",
                            "target_lang_translations": {
                                "ru": "Астрономы тщательно изучили орбиту, по которой вращается <w>Зевс</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "planetoida Zeus"
                    ],
                    "target_lang_definitions": {
                        "ru": "астероид под номером 5731, принадлежащий к группе Аполлона"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Зевс",
                                "target_lang_sense_clarification": "астероид"
                            }
                        ]
                    }
                }
            ]
        }
    ]

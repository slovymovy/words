                {
                    "sense_id": "412a960c-fdb2-3ffd-b51d-bc0fe6d9717c",
                    "sense_definition": "Przerwa w trakcie podróży lub pracy, mająca na celu odpoczynek lub załatwienie potrzeb.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "break_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autobus zrobił piętnastominutowy <w>postój</w> na stacji benzynowej.",
                            "target_lang_translations": {
                                "ru": "Автобус сделал пятнадцатиминутную <w>остановку</w> на заправке."
                            }
                        },
                        {
                            "text": "Ten krótki <w>postój</w> pozwolił nam rozprostować nogi przed dalszą jazdą.",
                            "target_lang_translations": {
                                "ru": "Эта короткая <w>остановка</w> позволила нам размять ноги перед дальнейшей дорогой."
                            }
                        }
                    ],
                    "synonyms": [
                        "przerwa"
                    ],
                    "common_phrases": [
                        "krótki postój",
                        "dłuższy postój",
                        "postój w podróży",
                        "czas na postój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перерыв во время поездки или работы для отдыха или удовлетворения потребностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "прекращение движения"
                            },
                            {
                                "target_lang_word": "стоянка",
                                "target_lang_sense_clarification": "длительная остановка"
                            },
                            {
                                "target_lang_word": "привал",
                                "target_lang_sense_clarification": "отдых в пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1f0f384-5c84-37ea-a003-4300e00eef6b",
                    "sense_definition": "Wyznaczone miejsce, w którym pojazdy (zwykle taksówki) oczekują na pasażerów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekam na ciebie na <w>postoju</w> taksówek przy głównym dworcu.",
                            "target_lang_translations": {
                                "ru": "Я жду тебя на <w>стоянке</w> такси у главного вокзала."
                            }
                        },
                        {
                            "text": "Na tym <w>postoju</w> rzadko stoją wolne samochody w godzinach szczytu.",
                            "target_lang_translations": {
                                "ru": "На этой <w>стоянке</w> в часы пик редко бывают свободные машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "stacja",
                        "przystanek"
                    ],
                    "common_phrases": [
                        "postój taksówek",
                        "postój dorożek",
                        "czekać na postoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально отведенное место, где транспортные средства (обычно такси) ожидают пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоянка",
                                "target_lang_sense_clarification": "место ожидания"
                            }

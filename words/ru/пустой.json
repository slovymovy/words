            "senses": [
                {
                    "sense_id": "3dd2179b-63a1-3e3e-b762-e570f97e5ce9",
                    "sense_definition": "Ничем не наполненный внутри; не содержащий ничего (о сосудах, вместилищах, помещениях).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_emptiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На кухонном столе стояла грязная <w>пустая</w> тарелка.",
                            "target_lang_translations": {
                                "en": "There was a dirty <w>empty</w> plate on the kitchen table."
                            }
                        },
                        {
                            "text": "Коробка оказалась совершенно <w>пустой</w>, внутри ничего не было.",
                            "target_lang_translations": {
                                "en": "The box turned out to be completely <w>empty</w>; there was nothing inside."
                            }
                        }
                    ],
                    "synonyms": [
                        "порожний",
                        "полый"
                    ],
                    "antonyms": [
                        "полный"
                    ],
                    "common_phrases": [
                        "пустая бутылка",
                        "пустой стакан",
                        "пустая комната",
                        "пустые руки"
                    ],
                    "target_lang_definitions": {
                        "en": "Containing nothing inside; having no contents (of containers, vessels, rooms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "empty",
                                "target_lang_sense_clarification": "containing nothing"
                            }
                        ]
                    }

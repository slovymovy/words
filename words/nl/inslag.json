{
    "word_family": [
        "inslaan",
        "slag"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "642a8eed-364b-3bd8-9e02-60447df44200",
                    "sense_definition": "Een krachtige botsing waarbij een voorwerp, zoals een bom of meteoriet, een oppervlak raakt of binnendringt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De krater is ontstaan door de <w>inslag</w> van een grote meteoriet.",
                            "target_lang_translations": {
                                "ru": "Кратер образовался в результате <w>удара</w> крупного метеорита."
                            }
                        },
                        {
                            "text": "Na de <w>inslag</w> van de bom was de schade aan het gebouw enorm.",
                            "target_lang_translations": {
                                "ru": "После <w>попадания</w> бомбы ущерб зданию был огромным."
                            }
                        }
                    ],
                    "synonyms": [
                        "botsing",
                        "impact"
                    ],
                    "common_phrases": [
                        "inslag van een meteoriet",
                        "inslag van een bom",
                        "krater na inslag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мощное столкновение, при котором объект, например бомба или метеорит, ударяет по поверхности или проникает в неё."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "столкновение"
                            },
                            {
                                "target_lang_word": "попадание",
                                "target_lang_sense_clarification": "снаряда, бомбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "704c83f9-7ca7-38ed-a91c-9d1bfb589a27",
                    "sense_definition": "Iemands natuurlijke aard, karaktertrek of aanleg.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die kunstzinnige <w>inslag</w> zit duidelijk in de hele familie.",
                            "target_lang_translations": {
                                "ru": "Эта художественная <w>жилка</w> явно присутствует у всей семьи."
                            }
                        },
                        {
                            "text": "Zij heeft een sociale <w>inslag</w> en helpt graag andere mensen.",
                            "target_lang_translations": {
                                "ru": "У неё социальный <w>склад</w> характера, и она любит помогать людям."
                            }
                        }
                    ],
                    "synonyms": [
                        "aard",
                        "karakter",
                        "aanleg"
                    ],
                    "common_phrases": [
                        "sociale inslag",
                        "artistieke inslag",
                        "zakelijke inslag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Природная сущность, черта характера или склонность человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склад",
                                "target_lang_sense_clarification": "характера, ума"
                            },
                            {
                                "target_lang_word": "жилка",
                                "target_lang_sense_clarification": "способность, склонность"
                            },
                            {
                                "target_lang_word": "наклонность",
                                "target_lang_sense_clarification": "предрасположенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9cc13ef-8431-3982-8e5a-443f1e7a473e",
                    "sense_definition": "De horizontale draden die tijdens het weven dwars door de gespannen lengtedraden worden geweven.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "weaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wever controleerde of de draad van de <w>inslag</w> niet gebroken was.",
                            "target_lang_translations": {
                                "ru": "Ткач проверил, не оборвалась ли нить <w>утка</w>."
                            }
                        },
                        {
                            "text": "Bij dit patroon hebben de schering en de <w>inslag</w> verschillende kleuren.",
                            "target_lang_translations": {
                                "ru": "В этом узоре основа и <w>уток</w> имеют разные цвета."
                            }
                        }
                    ],
                    "antonyms": [
                        "schering"
                    ],
                    "common_phrases": [
                        "draad van de inslag",
                        "schering en inslag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горизонтальные нити, которые при ткачестве переплетаются с натянутыми продольными нитями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уток",
                                "target_lang_sense_clarification": "текстиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b094d90-ec34-32af-8f7a-a96694dea4c1",
                    "sense_definition": "Het naar binnen gevouwen deel van de papieren omslag van een boek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "book",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de <w>inslag</w> van het boek staat een korte biografie van de auteur.",
                            "target_lang_translations": {
                                "ru": "На <w>клапане</w> книги размещена краткая биография автора."
                            }
                        },
                        {
                            "text": "De samenvatting van het verhaal is te lezen op de voorste <w>inslag</w>.",
                            "target_lang_translations": {
                                "ru": "Краткое содержание повести можно прочитать на переднем <w>клапане</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tekst op de inslag",
                        "voorste inslag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Загнутая внутрь часть бумажной обложки книги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клапан",
                                "target_lang_sense_clarification": "обложки, суперобложки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            },
                            {
                                "target_lang_word": "to slag off",
                                "target_lang_sense_clarification": "slang, to criticize or insult harshly"
                            },
                            {
                                "target_lang_word": "to abuse",
                                "target_lang_sense_clarification": "to speak to someone in an insulting and offensive way"
                            }

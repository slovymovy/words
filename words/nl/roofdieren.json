            "senses": [
                {
                    "sense_id": "cd94374b-d78c-3a93-aca2-534723d84638",
                    "sense_definition": "Meervoud van <w>roofdier</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de dierentuin leven verschillende soorten grote <w>roofdieren</w>.",
                            "target_lang_translations": {
                                "ru": "В зоопарке живут различные виды крупных <w>хищников</w>."
                            }
                        },
                        {
                            "text": "Kleine <w>roofdieren</w> jagen vaak op muizen en insecten.",
                            "target_lang_translations": {
                                "ru": "Мелкие <w>хищники</w> часто охотятся на мышей и насекомых."
                            }
                        }
                    ],
                    "antonyms": [
                        "prooidieren"
                    ],
                    "common_phrases": [
                        "gevaarlijke roofdieren",
                        "grote roofdieren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Животные, которые охотятся на других животных для пропитания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хищники",
                                "target_lang_sense_clarification": "животные"
                            },
                            {
                                "target_lang_word": "хищные животные",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "130f8658-ed53-3c34-96ad-430b653a4d88",
                    "sense_definition": "Een orde van zoogdieren die zich voornamelijk voeden met vlees (Carnivora).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Honden en katten behoren biologisch gezien tot de orde van de <w>roofdieren</w>.",
                            "target_lang_translations": {
                                "ru": "С биологической точки зрения собаки и кошки относятся к отряду <w>хищных</w>."
                            }
                        },
                        {
                            "text": "Niet alle <w>roofdieren</w> eten uitsluitend vlees; de reuzenpanda eet vooral bamboe.",
                            "target_lang_translations": {
                                "ru": "Не все <w>хищные</w> питаются исключительно мясом; гигантская панда ест в основном бамбук."
                            }
                        }
                    ],
                    "synonyms": [
                        "carnivoren"
                    ],
                    "common_phrases": [
                        "orde der roofdieren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отряд млекопитающих (Carnivora), питающихся преимущественно мясом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хищные",
                                "target_lang_sense_clarification": "биологический отряд"
                            },
                            {
                                "target_lang_word": "плотоядные",
                                "target_lang_sense_clarification": "тип питания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d939ecae-2aaf-31a1-85a6-3a2fa9c597f7",
                    "sense_definition": "Zeer agressieve mensen die meedogenloos handelen ten koste van anderen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de harde zakenwereld gedragen sommige managers zich als ware <w>roofdieren</w>.",
                            "target_lang_translations": {
                                "ru": "В жестком мире бизнеса некоторые менеджеры ведут себя как настоящие <w>хищники</w>."
                            }
                        },
                        {
                            "text": "De schrijver omschreef de soldaten als <w>roofdieren</w> die op zoek waren naar buit.",
                            "target_lang_translations": {
                                "ru": "Писатель описал солдат как <w>хищников</w>, ищущих добычу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, действующие агрессивно и безжалостно ради собственной выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хищники",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "акулы",
                                "target_lang_sense_clarification": "метафора (например, в бизнесе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "eaabb816-f8ce-3ad4-8b40-6d99e4ee2e66",
                    "sense_definition": "Not highlighted, promoted, or given special prominence, especially in a publication or on a website.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The website displays <w>unfeatured</w> content in a separate archive section.",
                            "target_lang_translations": {
                                "ru": "Веб-сайт отображает <w>невыделенный</w> контент в отдельном разделе архива."
                            }
                        },
                        {
                            "text": "Even an <w>unfeatured</w> artist can become popular through word of mouth.",
                            "target_lang_translations": {
                                "ru": "Даже <w>непродвигаемый</w> художник может стать популярным благодаря сарафанному радио."
                            }
                        }
                    ],
                    "synonyms": [
                        "unhighlighted"
                    ],
                    "antonyms": [
                        "featured",
                        "promoted"
                    ],
                    "common_phrases": [
                        "unfeatured content",
                        "unfeatured artist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не выделенный специально, не продвигаемый или не получивший особого внимания, особенно в публикации или на веб-сайте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невыделенный",
                                "target_lang_sense_clarification": "не отмеченный как важный или особенный"
                            },
                            {
                                "target_lang_word": "непродвигаемый",
                                "target_lang_sense_clarification": "не получающий активной рекламы или поддержки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "576dc7cf-e6da-318d-874c-d97c1e880983",
                    "sense_definition": "Lacking distinct or regular facial features; shapeless or deformed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ghost was described as having a pale, <w>unfeatured</w> face.",
                            "target_lang_translations": {
                                "ru": "Призрака описывали как существо с бледным, <w>безликим</w> лицом."
                            }
                        },
                        {
                            "text": "He stared at the <w>unfeatured</w> landscape covered in deep snow.",
                            "target_lang_translations": {
                                "ru": "Он смотрел на <w>невыразительный</w> пейзаж, покрытый глубоким снегом."
                            }
                        }
                    ],
                    "synonyms": [
                        "featureless",
                        "shapeless",
                        "amorphous"
                    ],
                    "antonyms": [
                        "distinct",
                        "defined"
                    ],
                    "common_phrases": [
                        "unfeatured face",
                        "unfeatured mass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишенный отчетливых или правильных черт; бесформенный или невыразительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безликий",
                                "target_lang_sense_clarification": "лишенный индивидуальных черт лица"
                            },
                            {
                                "target_lang_word": "невыразительный",
                                "target_lang_sense_clarification": "лишенный заметных деталей или особенностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

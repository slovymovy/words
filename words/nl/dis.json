                {
                    "sense_id": "ecbb5171-b658-3a1c-9719-62c3cbf4dc8b",
                    "sense_definition": "Een gedekte tafel waaraan men eet, vaak gebruikt in een plechtige of sfeervolle context.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "table_meal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hele familie zat gezamenlijk aan de <w>dis</w>.",
                            "target_lang_translations": {
                                "en": "The whole family sat together at the <w>table</w>.",
                                "ru": "Вся семья вместе сидела за <w>столом</w>."
                            }
                        },
                        {
                            "text": "Wij nodigen u van harte uit aan onze <w>dis</w>.",
                            "target_lang_translations": {
                                "en": "We cordially invite you to our <w>table</w>.",
                                "ru": "Мы сердечно приглашаем вас к нашему <w>столу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tafel"
                    ],
                    "common_phrases": [
                        "aan de dis"
                    ],
                    "target_lang_definitions": {
                        "en": "A set table where people eat, often used in a formal or atmospheric context.",
                        "ru": "Накрытый стол, за которым едят; часто используется в торжественном или литературном контексте."
                    },
                    "translations": {

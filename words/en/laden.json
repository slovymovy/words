                {
                    "sense_id": "5128f926-9d83-3cd7-85f7-3434a247365d",
                    "sense_definition": "Weighed down with a load; burdened.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "physically_loaded",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The camels were <laden> with heavy sacks of grain for the journey.",
                            "target_lang_translations": {
                                "ru": "Верблюды были <нагружены> тяжелыми мешками с зерном для путешествия."
                            }
                        },
                        {
                            "text": "She returned from the market <laden> with bags of fresh vegetables.",
                            "target_lang_translations": {
                                "ru": "Она вернулась с рынка, <нагруженная> сумками со свежими овощами."
                            }
                        }
                    ],
                    "synonyms": [
                        "loaded",
                        "burdened",
                        "weighted",
                        "encumbered"
                    ],
                    "antonyms": [
                        "unladen",
                        "empty",
                        "light"
                    ],
                    "common_phrases": [
                        "laden with",
                        "heavily laden",
                        "laden with goods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отягощенный грузом; нагруженный чем-либо тяжелым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагруженный",
                                "target_lang_sense_clarification": "несущий груз"

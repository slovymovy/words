            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c78d90b-e769-3b34-a949-14fed4ef1a91",
                    "sense_definition": "Pojedynczy egzemplarz organizmu, przedmiotu lub minerału, który reprezentuje cechy swojego gatunku lub rodzaju, często wyróżniający się wielkością lub urodą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "specimen_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum przyrodniczym oglądaliśmy zachowany <w>okaz</w> prehistorycznego gada.",
                            "target_lang_translations": {
                                "ru": "В музее естествознания мы рассматривали сохранившийся <w>экземпляр</w> доисторической рептилии."
                            }
                        },
                        {
                            "text": "To wyjątkowo piękny <w>okaz</w> dębu, który ma ponad trzysta lat.",
                            "target_lang_translations": {
                                "ru": "Это исключительно красивый <w>экземпляр</w> дуба, которому более трехсот лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "egzemplarz",
                        "sztuka"
                    ],
                    "common_phrases": [
                        "rzadki okaz",
                        "okaz muzealny",
                        "wspaniały okaz",
                        "piękny okaz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный представитель организма, предмета или минерала, обладающий характерными чертами своего вида, часто выделяющийся размером или красотой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экземпляр",
                                "target_lang_sense_clarification": "отдельный предмет или особь"
                            },
                            {
                                "target_lang_word": "образец",
                                "target_lang_sense_clarification": "показательный пример"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3323e1c2-f7d8-3078-939a-4aaff67a5161",
                    "sense_definition": "Osoba lub rzecz będąca doskonałym, wyrazistym przykładem jakiejś cechy, stanu lub zjawiska.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "epitome_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek to prawdziwy <w>okaz</w> zdrowia, mimo że skończył dziewięćdziesiąt lat.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка — настоящий <w>образец</w> здоровья, хотя ему уже исполнилось девяносто лет."
                            }
                        },
                        {
                            "text": "Ten stary samochód to jeżdżący <w>okaz</w> dawnej technologii.",
                            "target_lang_translations": {
                                "ru": "Этот старый автомобиль — ездящий <w>образец</w> старинной технологии."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzór",
                        "przykład",
                        "uosobienie"
                    ],
                    "common_phrases": [
                        "okaz zdrowia",
                        "okaz głupoty",
                        "niezły okaz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, являющийся совершенным, ярким примером какого-либо качества, состояния или явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образец",
                                "target_lang_sense_clarification": "эталон качества или состояния"
                            },
                            {
                                "target_lang_word": "экземпляр",
                                "target_lang_sense_clarification": "человек с яркими особенностями (часто иронично)"
                            }
                        ]
                    }
                }
            ]
        },

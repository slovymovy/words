            "senses": [
                {
                    "sense_id": "f80abd4b-3ff4-309d-8047-8ea3a0168a79",
                    "sense_definition": "Разведчик, тайно проникающий в тыл неприятеля для сбора сведений.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_scout",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир решил отправить опытного <w>лазутчика</w> в лагерь противника.",
                            "target_lang_translations": {
                                "en": "The commander decided to send an experienced <w>scout</w> to the enemy camp."
                            }
                        },
                        {
                            "text": "Вражеский <w>лазутчик</w> был замечен охраной возле склада с боеприпасами.",
                            "target_lang_translations": {
                                "en": "An enemy <w>infiltrator</w> was spotted by the guards near the ammunition depot."
                            }
                        }
                    ],
                    "synonyms": [
                        "разведчик",
                        "шпион"
                    ],
                    "common_phrases": [
                        "вражеский лазутчик",
                        "поймать лазутчика",
                        "отправить лазутчика"
                    ],
                    "target_lang_definitions": {
                        "en": "A scout who secretly penetrates the enemy rear to gather information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scout",
                                "target_lang_sense_clarification": "military reconnaissance"
                            },
                            {
                                "target_lang_word": "spy",
                                "target_lang_sense_clarification": "espionage agent"
                            },
                            {
                                "target_lang_word": "infiltrator",
                                "target_lang_sense_clarification": "one who penetrates defenses"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0165dccc-a5f1-3418-9511-b92fbeb4124e",
                    "sense_definition": "Человек, который тайно следит за кем-либо и передаёт сведения; доносчик.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго не могли понять, кто в нашем отделе <w>лазутчик</w>, докладывающий обо всём начальству.",
                            "target_lang_translations": {
                                "en": "For a long time, we couldn't figure out who the <w>snitch</w> in our department was, reporting everything to the boss."
                            }
                        },
                        {
                            "text": "Маленький <w>лазутчик</w> подслушал разговор взрослых и всё рассказал бабушке.",
                            "target_lang_translations": {
                                "en": "The little <w>spy</w> overheard the adults' conversation and told Grandma everything."
                            }
                        }
                    ],
                    "synonyms": [
                        "шпион"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "conversational style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who secretly watches someone and passes on information; an informant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snitch",
                                "target_lang_sense_clarification": "informant (negative/colloquial)"
                            },
                            {
                                "target_lang_word": "spy",
                                "target_lang_sense_clarification": "metaphorical or playful use"
                            },
                            {
                                "target_lang_word": "mole",
                                "target_lang_sense_clarification": "insider informant"
                            }
                        ]
                    }
                }
            ]
        }

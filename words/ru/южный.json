        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c4287a95-93fe-367a-862b-c88e671cfe0b",
                    "sense_definition": "Находящийся на юге; связанный с югом, расположенный в направлении юга.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили провести летний отпуск на <w>южном</w> берегу моря.",
                            "target_lang_translations": {
                                "en": "We decided to spend our summer vacation on the <w>southern</w> coast of the sea."
                            }
                        },
                        {
                            "text": "Окна нашей новой квартиры выходят на <w>южную</w> сторону, поэтому там всегда светло.",
                            "target_lang_translations": {
                                "en": "The windows of our new apartment face the <w>south</w> side, so it is always bright there."
                            }
                        }
                    ],
                    "antonyms": [
                        "северный"
                    ],
                    "common_phrases": [
                        "Южный полюс",
                        "Южная Америка",
                        "южный берег",
                        "южная сторона"
                    ],
                    "target_lang_definitions": {
                        "en": "Located in the south; related to the south, situated in a southward direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "southern",
                                "target_lang_sense_clarification": "situated in or related to the south"
                            },
                            {
                                "target_lang_word": "south",
                                "target_lang_sense_clarification": "situated in the south (often used in place names or fixed phrases)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "059ad4f6-eab2-3ddf-af6a-11bb50a8a117",
                    "sense_definition": "Дующий с юга (о ветре); приходящий с юга (о воздушных массах).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тёплый <w>южный</w> ветер принёс долгожданное потепление в наш город.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6bd4f770-eb24-3f2a-9e5d-93ed7e68fa70",
                    "sense_definition": "A single instruction or script that expands automatically into a set of instructions to perform a particular task, often used to automate repetitive actions in software.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computer_science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I created a <w>macro</w> to automatically format the monthly report in the spreadsheet.",
                            "target_lang_translations": {
                                "ru": "Я создал <w>макрос</w>, чтобы автоматически отформатировать ежемесячный отчет в таблице."
                            }
                        },
                        {
                            "text": "Using a keyboard <w>macro</w> can save you a lot of time on repetitive data entry.",
                            "target_lang_translations": {
                                "ru": "Использование клавиатурного <w>макроса</w> может сэкономить вам много времени при повторяющемся вводе данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "script",
                        "shortcut"
                    ],
                    "common_phrases": [
                        "run a macro",
                        "record a macro",
                        "keyboard macro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерный алгоритм, скрипт или последовательность команд, используемые для автоматизации повторяющихся задач."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "макрос",
                                "target_lang_sense_clarification": "наиболее употребительный термин в IT"
                            },
                            {
                                "target_lang_word": "макрокоманда",
                                "target_lang_sense_clarification": "формальный технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7be3cea2-30a0-314a-919e-9f38896e0f12",
                    "sense_definition": "Short for <w>macronutrient</w>; a type of food required in large amounts in the diet, specifically protein, carbohydrates, and fat.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "nutrition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Athletes often count their <w>macros</w> to ensure they are getting enough protein for muscle recovery.",
                            "target_lang_translations": {
                                "ru": "Спортсмены часто считают свои <w>макронутриенты</w> (БЖУ), чтобы убедиться, что получают достаточно белка для восстановления мышц."
                            }
                        },
                        {
                            "text": "You can enjoy a treat as long as it fits your daily <w>macros</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете позволить себе угощение, если оно вписывается в вашу дневную норму <w>БЖУ</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "count macros",
                        "fit your macros",
                        "daily macros"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping of macronutrient"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Питательные вещества, необходимые организму в больших количествах: белки, жиры и углеводы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "макронутриенты",
                                "target_lang_sense_clarification": "полный термин"
                            },
                            {
                                "target_lang_word": "БЖУ",
                                "target_lang_sense_clarification": "аббревиатура (белки, жиры, углеводы), часто используемая в контексте подсчета калорий"
                            },
                            {
                                "target_lang_word": "макросы",
                                "target_lang_sense_clarification": "сленг, калька с английского"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26411f64-4b7e-3c26-810b-96f4134eef9e",
                    "sense_definition": "Short for <w>macroeconomics</w>; the study or analysis of the economy as a whole, including factors like inflation and growth.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The current <w>macro</w> environment is challenging due to rising inflation rates.",
                            "target_lang_translations": {
                                "ru": "Текущая <w>макроэкономическая</w> обстановка сложна из-за растущей инфляции."
                            }
                        },
                        {
                            "text": "Investors are worried about global <w>macro</w> risks affecting the stock market.",
                            "target_lang_translations": {
                                "ru": "Инвесторы обеспокоены глобальными <w>макроэкономическими</w> рисками, влияющими на фондовый рынок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "macro trends",
                        "macro outlook",
                        "global macro"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping of macroeconomics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к макроэкономике; анализ экономики в целом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "макроэкономика",
                                "target_lang_sense_clarification": "существительное, название дисциплины"
                            },
                            {
                                "target_lang_word": "макроэкономический",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6c67f2c-0cbf-37a3-bb05-26e35cf1f7c7",
                    "sense_definition": "Short for <w>macro lens</w>; a camera lens designed for taking photographs at very close range to capture fine details.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The photographer used a <w>macro</w> to capture the intricate patterns on the butterfly's wings.",
                            "target_lang_translations": {
                                "ru": "Фотограф использовал <w>макрообъектив</w>, чтобы запечатлеть сложные узоры на крыльях бабочки."
                            }
                        },
                        {
                            "text": "If you want to shoot close-ups of flowers, you should invest in a good <w>macro</w>.",
                            "target_lang_translations": {
                                "ru": "Если вы хотите снимать крупные планы цветов, вам стоит вложиться в хороший <w>макрообъектив</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "macro shot",
                        "shoot in macro"

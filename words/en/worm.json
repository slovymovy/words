    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9066ce1f-ff31-3ad3-a47b-bf113c51fcd7",
                    "sense_definition": "A small, soft, long animal with no legs or bones, such as an earthworm or maggot, that lives in the ground or in decomposing matter.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_bio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The early bird catches the <w>worm</w>.",
                            "target_lang_translations": {
                                "ru": "Ранняя пташка ловит <w>червяка</w>."
                            }
                        },
                        {
                            "text": "He put a <w>worm</w> on the hook to catch a fish.",
                            "target_lang_translations": {
                                "ru": "Он насадил <w>червяка</w> на крючок, чтобы поймать рыбу."
                            }
                        },
                        {
                            "text": "After the rain, a long pink <w>worm</w> crawled across the sidewalk.",
                            "target_lang_translations": {
                                "ru": "После дождя длинный розовый <w>червяк</w> полз по тротуару."
                            }
                        }
                    ],
                    "synonyms": [
                        "earthworm",
                        "grub",
                        "larva"
                    ],
                    "common_phrases": [
                        "can of worms",
                        "food for worms",
                        "the early bird catches the worm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое мягкое длинное животное без ног и костей, такое как дождевой червь или личинка, живущее в земле или в разлагающейся материи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "червяк",
                                "target_lang_sense_clarification": "разговорное, общее название"
                            },
                            {
                                "target_lang_word": "червь",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4f03824-18ee-3e44-a98b-8fe2eceb424c",
                    "sense_definition": "A harmful computer program that copies itself and spreads to other computers through a network.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The IT department warned us about a new <w>worm</w> attacking email systems.",
                            "target_lang_translations": {
                                "ru": "IT-отдел предупредил нас о новом <w>черве</w>, атакующем системы электронной почты."
                            }
                        },
                        {
                            "text": "Unlike a virus, a computer <w>worm</w> does not need a host file to travel.",
                            "target_lang_translations": {
                                "ru": "В отличие от вируса, компьютерному <w>червю</w> не нужен файл-хост для перемещения."
                            }
                        }
                    ],
                    "synonyms": [
                        "malware"
                    ],
                    "common_phrases": [
                        "computer worm",
                        "internet worm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вредоносная компьютерная программа, которая копирует себя и распространяется на другие компьютеры через сеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "червь",
                                "target_lang_sense_clarification": "компьютерный вирус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee14dcc1-dbcb-328b-8155-8267ee529e22",
                    "sense_definition": "A weak, unpleasant, or despicable person who is not respected.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be such a <w>worm</w>; stand up for yourself!",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>червяком</w>, постой за себя!"
                            }
                        },
                        {
                            "text": "He is a lying <w>worm</w> who betrayed his own family for money.",
                            "target_lang_translations": {
                                "ru": "Он лживый <w>червяк</w>, который предал свою собственную семью ради денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "wretch",
                        "creep",
                        "coward"
                    ],
                    "common_phrases": [
                        "you little worm"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слабый, неприятный или подлый человек, которого не уважают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "червяк",
                                "target_lang_sense_clarification": "перен. ничтожество"
                            },
                            {
                                "target_lang_word": "ничтожество",
                                "target_lang_sense_clarification": "жалкий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b88c60d4-738b-3855-96a2-d63c3887d819",
                    "sense_definition": "A screw or spiral component in a machine, especially one that fits into the teeth of a wheel to turn it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic replaced the damaged <w>worm</w> in the steering gear.",
                            "target_lang_translations": {
                                "ru": "Механик заменил поврежденный <w>червяк</w> в рулевом механизме."
                            }
                        },
                        {
                            "text": "A <w>worm</w> drive is often used to reduce speed and increase torque.",
                            "target_lang_translations": {
                                "ru": "<w>Червячная</w> передача часто используется для снижения скорости и увеличения крутящего момента."
                            }
                        }
                    ],
                    "synonyms": [
                        "screw thread"
                    ],
                    "common_phrases": [
                        "worm gear",
                        "worm drive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Винт или спиральный компонент в механизме, который сцепляется с зубьями колеса для его вращения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "червяк",
                                "target_lang_sense_clarification": "деталь механизма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5990b14d-0d68-3027-88e6-b5948c988733",
                    "sense_definition": "A dance move where the dancer lies on the floor and ripples their body like a crawling animal.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone cheered when he started doing the <w>worm</w> at the party.",
                            "target_lang_translations": {
                                "ru": "Все зааплодировали, когда он начал делать <w>червячка</w> на вечеринке."
                            }
                        },
                        {
                            "text": "She hurt her back trying to perform the <w>worm</w> on the dance floor.",
                            "target_lang_translations": {
                                "ru": "Она повредила спину, пытаясь изобразить <w>червяка</w> на танцполе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do the worm"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "dance style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Танцевальное движение, при котором танцор лежит на полу и извивается телом, подобно ползущему животному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "червячок",
                                "target_lang_sense_clarification": "танец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9841923-9757-358f-8784-85bbb3039e00",
                    "sense_definition": "A mythical dragon or giant sea serpent, typically described as wingless.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mythology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight rode out to slay the great <w>worm</w> that terrorized the village.",
                            "target_lang_translations": {
                                "ru": "Рыцарь выехал, чтобы сразить великого <w>змея</w>, терроризировавшего деревню."
                            }
                        },
                        {
                            "text": "Legends tell of a giant <w>worm</w> living in the depths of the ocean.",
                            "target_lang_translations": {
                                "ru": "Легенды рассказывают о гигантском <w>черве</w>, живущем в глубинах океана."
                            }
                        }
                    ],
                    "synonyms": [
                        "dragon",
                        "serpent",
                        "wyrm"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or literary/fantasy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мифический дракон или гигантский морской змей, обычно описываемый как бескрылый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "змей",
                                "target_lang_sense_clarification": "мифический"
                            },
                            {
                                "target_lang_word": "дракон",
                                "target_lang_sense_clarification": "устаревшее/фэнтези"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6c860e03-54a5-3e7d-a28d-00f7b6e3efd6",
                    "sense_definition": "To move slowly and quietly by crawling or dragging the body, often through a tight space.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier had to <w>worm</w> through the mud to avoid being seen.",
                            "target_lang_translations": {
                                "ru": "Солдату пришлось <w>ползти</w> по грязи, чтобы его не заметили."
                            }
                        },
                        {
                            "text": "The puppy managed to <w>worm</w> under the garden fence.",
                            "target_lang_translations": {
                                "ru": "Щенку удалось <w>пролезть</w> под садовым забором."
                            }
                        }
                    ],
                    "synonyms": [
                        "crawl",
                        "wriggle",
                        "creep"
                    ],
                    "common_phrases": [
                        "worm through",
                        "worm along"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться медленно и тихо, ползком или волоча тело, часто через узкое пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ползти",
                                "target_lang_sense_clarification": "передвигаться ползком"
                            },
                            {
                                "target_lang_word": "пробираться",
                                "target_lang_sense_clarification": "с трудом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f9a38a6-eb3e-397d-a706-91f022fde796",
                    "sense_definition": "To move or get into a position or situation slowly and cleverly, often by being tricky or persistent.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He managed to <w>worm</w> his way into the boss's favor.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <w>втереться</w> в доверие к начальнику."
                            }
                        },
                        {
                            "text": "She tried to <w>worm</w> herself into the exclusive club without an invitation.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>проникнуть</w> в элитный клуб без приглашения."
                            }
                        }
                    ],
                    "synonyms": [
                        "insinuate",
                        "infiltrate"
                    ],
                    "common_phrases": [
                        "worm one's way in",
                        "worm into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно и хитро проникать в какое-либо положение или ситуацию, часто с помощью уловок или настойчивости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "втираться",
                                "target_lang_sense_clarification": "в доверие"
                            },
                            {
                                "target_lang_word": "пролезать",
                                "target_lang_sense_clarification": "проникать хитростью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebab0272-5360-3a1a-8248-d5a984cb2d30",
                    "sense_definition": "To get information from someone who does not want to give it, usually by asking many questions or being manipulative.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "extract_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective tried to <w>worm</w> the truth out of the suspect.",
                            "target_lang_translations": {
                                "ru": "Детектив пытался <w>вытянуть</w> правду из подозреваемого."
                            }
                        },
                        {
                            "text": "I finally managed to <w>worm</w> the secret out of my brother.",
                            "target_lang_translations": {
                                "ru": "Мне наконец удалось <w>выведать</w> секрет у брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "extract",
                        "wheedle"
                    ],
                    "common_phrases": [
                        "worm out of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выведывать информацию у того, кто не хочет её давать, обычно задавая много вопросов или манипулируя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выуживать",
                                "target_lang_sense_clarification": "информацию"
                            },
                            {
                                "target_lang_word": "выпытывать",
                                "target_lang_sense_clarification": "секреты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b078c562-6d8e-3233-89bd-ff1ef80ea3e9",
                    "sense_definition": "To give medicine to an animal to kill internal parasites.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "veterinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>worm</w> your puppy every few months to keep it healthy.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>глистогонить</w> щенка каждые несколько месяцев, чтобы он был здоров."
                            }
                        },
                        {
                            "text": "The farmer will <w>worm</w> the sheep before releasing them into the new pasture.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>даст средство от глистов</w> овцам перед выпуском их на новое пастбище."
                            }
                        }
                    ],
                    "synonyms": [
                        "deworm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать лекарство животному для уничтожения внутренних паразитов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глистогонить",
                                "target_lang_sense_clarification": "лечить от глистов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {

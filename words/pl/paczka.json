            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d96e5809-d380-37ae-a100-6faf4752b508",
                    "sense_definition": "Przesyłka pocztowa lub przedmiot zapakowany w papier lub pudełko w celu przeniesienia lub wysłania.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "container_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść na pocztę, żeby odebrać <paczkę> od babci.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти на почту, чтобы забрать <w>посылку</w> от бабушки."
                            }
                        },
                        {
                            "text": "Kurier przyniósł ciężką <paczkę>, w której były zamówione książki.",
                            "target_lang_translations": {
                                "ru": "Курьер принес тяжелую <w>посылку</w>, в которой были заказанные книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "pakiet"
                    ],
                    "common_phrases": [
                        "nadać paczkę",
                        "odebrać paczkę",
                        "paczka pocztowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почтовое отправление или предмет, упакованный в бумагу или коробку для переноски или отправки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посылка",
                                "target_lang_sense_clarification": "почтовое отправление"
                            },
                            {
                                "target_lang_word": "свёрток",
                                "target_lang_sense_clarification": "упакованный предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3def5eed-0ba8-3a8d-b0ea-67d67fa04a97",
                    "sense_definition": "Małe opakowanie zawierające określoną ilość produktów, na przykład ciastek, kawy lub papierosów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie kupiłem <paczkę> gum do żucia i gazetę.",
                            "target_lang_translations": {
                                "ru": "В магазине я купил <w>пачку</w> жевательной резинки и газету."
                            }
                        },
                        {
                            "text": "Zjadł całą <paczkę> czipsów podczas oglądania filmu.",
                            "target_lang_translations": {
                                "ru": "Он съел целую <w>пачку</w> чипсов во время просмотра фильма."
                            }
                        }
                    ],
                    "synonyms": [
                        "opakowanie"
                    ],
                    "antonyms": [
                        "zgrzewka"
                    ],
                    "common_phrases": [
                        "paczka papierosów",
                        "paczka kawy",
                        "cała paczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая упаковка, содержащая определенное количество продуктов, например печенья, кофе или сигарет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пачка",
                                "target_lang_sense_clarification": "сигарет, масла, чипсов"
                            },
                            {
                                "target_lang_word": "упаковка",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02041a41-5394-339b-96f1-b0f8c2b576d5",
                    "sense_definition": "Prezent, zwykle ładnie zapakowany, wręczany z okazji świąt Bożego Narodzenia lub Mikołajek.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "container_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci z niecierpliwością czekały na świąteczne <paczki> pod choinką.",
                            "target_lang_translations": {
                                "ru": "Дети с нетерпением ждали рождественские <w>подарки</w> под ёлкой."
                            }
                        },
                        {
                            "text": "W tym roku każda <paczka> od Mikołaja była pełna słodyczy.",
                            "target_lang_translations": {
                                "ru": "В этом году каждый <w>подарок</w> от Святого Николая был полон сладостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paczka świąteczna",
                        "paczka od Mikołaja",
                        "robić paczki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подарок, обычно красиво упакованный, вручаемый по случаю Рождества или Дня Святого Николая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подарок",
                                "target_lang_sense_clarification": "рождественский"
                            },
                            {
                                "target_lang_word": "кулёк",
                                "target_lang_sense_clarification": "с конфетами (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe5a7e36-1d61-37f6-a67a-30e3300fb6c6",
                    "sense_definition": "Zżyta grupa przyjaciół lub znajomych, którzy często spędzają ze sobą czas.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest moja stara <paczka> ze studiów, wciąż się spotykamy.",
                            "target_lang_translations": {
                                "ru": "Это моя старая <w>компания</w> со времен учебы, мы все еще встречаемся."
                            }
                        },
                        {
                            "text": "Oni tworzą zgraną <paczkę> i zawsze trzymają się razem.",
                            "target_lang_translations": {
                                "ru": "Они — сплоченная <w>компания</w> и всегда держатся вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "znajomi",
                        "kumple",
                        "banda",
                        "reg. pozn. eka"
                    ],
                    "common_phrases": [
                        "zgrana paczka",
                        "nasza paczka",
                        "trzymać się w paczce"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сплоченная группа друзей или знакомых, которые часто проводят время вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "группа друзей"
                            },
                            {
                                "target_lang_word": "компашка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b959ed46-6659-3c7a-8797-e188e1483f82",
                    "sense_definition": "Zbiór plików lub programów komputerowych przygotowanych do instalacji lub dystrybucji jako całość.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pobrałem nową <paczkę> sterowników ze strony producenta.",
                            "target_lang_translations": {
                                "ru": "Я скачал новый <w>пакет</w> драйверов с сайта производителя."
                            }
                        },
                        {
                            "text": "Ta <paczka> instalacyjna zajmuje bardzo dużo miejsca na dysku.",
                            "target_lang_translations": {
                                "ru": "Этот установочный <w>пакет</w> занимает очень много места на диске."
                            }
                        }
                    ],
                    "synonyms": [
                        "pakiet"
                    ],
                    "common_phrases": [
                        "paczka danych",
                        "paczka aktualizacji"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "informatyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор компьютерных файлов или программ, подготовленных для установки или распространения как единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакет",
                                "target_lang_sense_clarification": "программного обеспечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e293396-f636-3253-8832-0753dd2b3ac5",
                    "sense_definition": "Krótka, sztywna i stercząca spódniczka noszona przez tancerki baletowe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Balerina weszła na scenę w pięknej, białej <paczce>.",
                            "target_lang_translations": {
                                "ru": "Балерина вышла на сцену в красивой белой <w>пачке</w>."
                            }
                        },
                        {
                            "text": "Do tego występu potrzebna mi jest nowa tiulowa <paczka>.",
                            "target_lang_translations": {
                                "ru": "Для этого выступления мне нужна новая тюлевая <w>пачка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tutu"
                    ],
                    "common_phrases": [
                        "tiulowa paczka",
                        "tańczyć w paczce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая, жесткая и торчащая юбка, которую носят балерины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пачка",
                                "target_lang_sense_clarification": "балетная"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "a5ed14d5-e5d7-33c4-9de5-224e42333811",
                    "sense_definition": "O sposobie poruszania się konia lub jeźdźca: najszybszym biegiem, pełnym galopem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Usłyszeliśmy wierzchowca pędzącego <w>cwałem</w> przez most.",
                            "target_lang_translations": {
                                "ru": "Мы услышали всадника, мчащегося <w>галопом</w> через мост."
                            }
                        },
                        {
                            "text": "Konie ruszyły <w>cwałem</w> w stronę lasu, wzbijając tumany kurzu.",
                            "target_lang_translations": {
                                "ru": "Лошади пустились <w>в карьер</w> в сторону леса, поднимая столбы пыли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pędzić cwałem",
                        "ruszyć cwałem",
                        "jechać cwałem"
                    ],
                    "target_lang_definitions": {
                        "ru": "О способе передвижения лошади или всадника: самым быстрым бегом, полным галопом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галопом",
                                "target_lang_sense_clarification": "аллюр"
                            },
                            {
                                "target_lang_word": "в карьер",
                                "target_lang_sense_clarification": "самый быстрый аллюр"
                            },
                            {
                                "target_lang_word": "карьером",
                                "target_lang_sense_clarification": "аллюр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d275b587-b1a6-3fdf-9430-75e8ac3453f7",
                    "sense_definition": "Bardzo szybko, w wielkim pośpiechu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiadomość o wygranej rozeszła się <w>cwałem</w> po całej okolicy.",

{
    "word_family": [
        "некий",
        "некто",
        "нечто",
        "который"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "119903eb-2161-346a-a31f-47e756a0a2de",
                    "sense_definition": "Часть людей или предметов из какой-либо группы, не все.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "subset_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Некоторые> ученики сегодня опоздали на первый урок.",
                            "target_lang_translations": {
                                "en": "<Some> students were late for the first lesson today."
                            }
                        },
                        {
                            "text": "<Некоторые> из них решили остаться дома.",
                            "target_lang_translations": {
                                "en": "<Some> of them decided to stay home."
                            }
                        }
                    ],
                    "synonyms": [
                        "часть",
                        "не́сколько"
                    ],
                    "antonyms": [
                        "все"
                    ],
                    "common_phrases": [
                        "некоторые из них",
                        "некоторые люди",
                        "некоторые страны"
                    ],
                    "target_lang_definitions": {
                        "en": "A portion of people or things from a group, but not all of them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "referring to a part of a group"
                            },
                            {
                                "target_lang_word": "certain",
                                "target_lang_sense_clarification": "specific but unnamed members of a group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ff68108-29ea-3bd6-bd31-d6108044a661",
                    "sense_definition": "Какой-то, точно не названный или не вполне определённый (о времени, качестве, предмете).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы молча шли по улице <некоторое> время.",
                            "target_lang_translations": {
                                "en": "We walked silently down the street for <some> time."
                            }
                        },
                        {
                            "text": "В его словах есть <некоторая> правда.",
                            "target_lang_translations": {
                                "en": "There is <some> truth in his words."
                            }
                        }
                    ],
                    "synonyms": [
                        "какой-то",
                        "кое-какой"
                    ],
                    "antonyms": [
                        "определённый"
                    ],
                    "common_phrases": [
                        "некоторое время",
                        "в некотором роде",
                        "до некоторой степени",
                        "с некоторых пор"
                    ],
                    "target_lang_definitions": {
                        "en": "Indefinite or not fully specified (referring to time, quality, or an object)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "indefinite amount or period"
                            },
                            {
                                "target_lang_word": "a certain",
                                "target_lang_sense_clarification": "specific but unnamed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d557b54-118f-32b1-ad81-6851845b74df",
                    "sense_definition": "Небольшой, но заметный; незначительный по степени проявления.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "degree_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он говорил с <некоторым> волнением в голосе.",
                            "target_lang_translations": {
                                "en": "He spoke with <some> agitation in his voice."
                            }
                        },
                        {
                            "text": "Эта задача вызвала у студентов <некоторые> трудности.",
                            "target_lang_translations": {
                                "en": "This task caused the students <some> difficulties."
                            }
                        }
                    ],
                    "synonyms": [
                        "незначительный"
                    ],
                    "antonyms": [
                        "значительный"
                    ],
                    "common_phrases": [
                        "с некоторым трудом",
                        "с некоторым опозданием"
                    ],
                    "target_lang_definitions": {
                        "en": "Small but noticeable; slight in degree."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "moderate or slight amount"
                            },
                            {
                                "target_lang_word": "slight",
                                "target_lang_sense_clarification": "small in degree"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
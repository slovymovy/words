    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8811702a-2376-3e2d-aaa0-82595e396916",
                    "sense_definition": "Relating to living beings, such as animals, that are biologically lower on the evolutionary scale than humans.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The research compares the brain functions of humans and <w>infrahuman</w> primates.",
                            "target_lang_translations": {
                                "ru": "Исследование сравнивает функции мозга человека и <w>низших</w> приматов."
                            }
                        },
                        {
                            "text": "Many <w>infrahuman</w> species exhibit complex social behaviors.",
                            "target_lang_translations": {
                                "ru": "Многие <w>субчеловеческие</w> виды демонстрируют сложное социальное поведение."
                            }
                        }
                    ],
                    "synonyms": [
                        "subhuman",
                        "nonhuman"
                    ],
                    "common_phrases": [
                        "infrahuman primates",
                        "infrahuman species"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к живым существам, стоящим ниже человека на эволюционной лестнице."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "субчеловеческий",
                                "target_lang_sense_clarification": "биология"
                            },
                            {
                                "target_lang_word": "низший",
                                "target_lang_sense_clarification": "в контексте эволюции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1e8a458-a333-3827-ae82-3817019c77c1",
                    "sense_definition": "Existing below the moral, spiritual, or intellectual level characteristic of humans.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author depicts a gloomy, <w>infrahuman</w> existence devoid of hope or reason.",
                            "target_lang_translations": {
                                "ru": "Автор описывает мрачное, <w>недочеловеческое</w> существование, лишенное надежды или смысла."
                            }
                        },
                        {
                            "text": "He felt trapped in an <w>infrahuman</w> realm where logic did not apply.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя в ловушке в <w>недочеловеческом</w> мире, где логика не действовала."
                            }
                        }
                    ],
                    "synonyms": [
                        "subhuman"
                    ],
                    "antonyms": [
                        "superhuman",
                        "suprahuman"
                    ],
                    "common_phrases": [
                        "infrahuman realm",
                        "infrahuman state"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся ниже морального, духовного или интеллектуального уровня, характерного для человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недочеловеческий",
                                "target_lang_sense_clarification": "о состоянии или условиях"
                            },
                            {
                                "target_lang_word": "животный",
                                "target_lang_sense_clarification": "о поведении или инстинктах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0352b58e-084f-31b3-86f6-b2a1a1f40b4c",
                    "sense_definition": "A living being, such as an animal, that has not reached the level of human consciousness or development.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The biologist observed the <w>infrahumans</w> interacting in their enclosure.",
                            "target_lang_translations": {
                                "ru": "Биолог наблюдал, как <w>низшие существа</w> взаимодействуют в своем вольере."
                            }
                        },
                        {
                            "text": "Cognitive tests were administered to various <w>infrahumans</w> to gauge their problem-solving skills.",
                            "target_lang_translations": {
                                "ru": "Когнитивные тесты проводились на различных <w>низших существах</w>, чтобы оценить их навыки решения проблем."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Живое существо, не достигшее уровня человеческого сознания или развития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низшее существо",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
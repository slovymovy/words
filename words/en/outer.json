    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0c70d8ef-f845-37ac-8c92-9365cd3a1c76",
                    "sense_definition": "Situated on the outside or farther from the center.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>outer</w> layer of the skin protects the body from infection.",
                            "target_lang_translations": {
                                "ru": "<w>Внешний</w> слой кожи защищает организм от инфекции."
                            }
                        },
                        {
                            "text": "Astronauts travel into <w>outer</w> space to explore distant planets.",
                            "target_lang_translations": {
                                "ru": "Астронавты путешествуют в <w>открытый</w> космос, чтобы исследовать далекие планеты."
                            }
                        }
                    ],
                    "antonyms": [
                        "inner"
                    ],
                    "common_phrases": [
                        "outer space",
                        "outer layer",
                        "outer edge",
                        "outer wall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся снаружи или дальше от центра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внешний",
                                "target_lang_sense_clarification": "находящийся снаружи"
                            },
                            {
                                "target_lang_word": "наружный",
                                "target_lang_sense_clarification": "относящийся к поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f53aef39-e145-3e9d-968f-e15f7375a686",
                    "sense_definition": "Comparative form of <w>out</w>: more open about one's sexuality or identity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is much <w>outer</w> and more confident since he told his family the truth.",
                            "target_lang_translations": {
                                "ru": "Он стал гораздо <w>более открытым</w> и уверенным в себе с тех пор, как рассказал правду семье."
                            }
                        },
                        {
                            "text": "She became <w>outer</w> about her political beliefs as she got older.",
                            "target_lang_translations": {
                                "ru": "С возрастом она стала <w>более открытой</w> в отношении своих политических убеждений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative of out"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень от \"out\": более открытый в вопросах своей сексуальности или идентичности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "более открытый",
                                "target_lang_sense_clarification": "не скрывающий свою идентичность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1074433-a453-36d1-9cdd-696988e125f0",
                    "sense_definition": "A person who supports leaving the European Union.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The debate between the inners and the <w>outers</w> dominated the news for months.",
                            "target_lang_translations": {
                                "ru": "Дебаты между противниками и <w>сторонниками выхода</w> доминировали в новостях месяцами."
                            }
                        },
                        {
                            "text": "Many <w>outers</w> celebrated the result of the referendum.",
                            "target_lang_translations": {
                                "ru": "Многие <w>сторонники выхода</w> праздновали результаты референдума."
                            }
                        }
                    ],
                    "synonyms": [
                        "Brexiter"
                    ],
                    "antonyms": [
                        "inner"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK politics"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, поддерживающий выход из Европейского союза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сторонник выхода",
                                "target_lang_sense_clarification": "политика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c7c91f9-7be4-3468-8187-bdd5d4a68b50",
                    "sense_definition": "The smallest unit of goods sold by wholesalers to retailers, usually a box containing multiple items.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "business",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop manager ordered five <w>outers</w> of the new chocolate bars.",
                            "target_lang_translations": {
                                "ru": "Управляющий магазином заказал пять <w>коробок</w> новых шоколадных батончиков."
                            }
                        },
                        {
                            "text": "Please check the label on the <w>outer</w> to see how many units are inside.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверь этикетку на <w>внешней упаковке</w>, чтобы узнать, сколько единиц внутри."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешняя упаковка, содержащая несколько единиц товара (обычно для оптовой продажи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внешняя упаковка",
                                "target_lang_sense_clarification": "тара"
                            },
                            {
                                "target_lang_word": "коробка",
                                "target_lang_sense_clarification": "с товаром"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4365e65-89f2-37da-b169-4865bb4b9ef1",
                    "sense_definition": "Someone who reveals another person's secret, especially their sexual orientation, without consent.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The celebrity criticized the <w>outer</w> who invaded his privacy.",
                            "target_lang_translations": {
                                "ru": "Знаменитость раскритиковала <w>разоблачителя</w>, вторгшегося в его частную жизнь."
                            }
                        },
                        {
                            "text": "Gossip magazines sometimes act as <w>outers</w> of private information.",
                            "target_lang_translations": {
                                "ru": "Журналы сплетен иногда выступают в роли <w>разоблачителей</w> личной информации."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто раскрывает чужие секреты (особенно ориентацию) без согласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разоблачитель",
                                "target_lang_sense_clarification": "раскрывающий чужие тайны"
                            }
                        ]
                    }
                }
            ]
        }

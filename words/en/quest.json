{
    "word_family": [
        "quester",
        "inquest",
        "request",
        "conquest",
        "question"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "09ef830a-6cb2-3fde-a028-5adbbb35d969",
                    "sense_definition": "A long or arduous search for something, often involving a journey; a mission to achieve a specific goal.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "search_mission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight went on a <w>quest</w> to find the legendary sword.",
                            "target_lang_translations": {
                                "ru": "Рыцарь отправился на <w>поиски</w> легендарного меча."
                            }
                        },
                        {
                            "text": "She traveled around the world in <w>quest</w> of spiritual enlightenment.",
                            "target_lang_translations": {
                                "ru": "Она путешествовала по всему миру в <w>поисках</w> духовного просветления."
                            }
                        },
                        {
                            "text": "Scientists are on a <w>quest</w> to discover a cure for the disease.",
                            "target_lang_translations": {
                                "ru": "Ученые ведут <w>поиски</w> лекарства от этой болезни."
                            }
                        }
                    ],
                    "synonyms": [
                        "mission",
                        "expedition",
                        "search",
                        "pursuit",
                        "journey"
                    ],
                    "common_phrases": [
                        "in quest of",
                        "quest for",
                        "spiritual quest",
                        "vision quest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительный или трудный поиск чего-либо, часто связанный с путешествием; миссия для достижения определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поиск",
                                "target_lang_sense_clarification": "действие, направленное на то, чтобы найти что-либо"
                            },
                            {
                                "target_lang_word": "поиски",
                                "target_lang_sense_clarification": "процесс разыскивания (часто употребляется во множественном числе в контексте приключений)"
                            },
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "ответственное задание или поручение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdaefec3-ec9e-3720-90a3-9b462f4bda0f",
                    "sense_definition": "A specific task or mission that a character in a video game must complete to gain a reward or advance the story.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "gaming_task",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to finish this side <w>quest</w> to level up my character.",
                            "target_lang_translations": {
                                "ru": "Мне нужно закончить этот побочный <w>квест</w>, чтобы повысить уровень моего персонажа."
                            }
                        },
                        {
                            "text": "The villager gave the player a <w>quest</w> to retrieve a stolen item.",
                            "target_lang_translations": {
                                "ru": "Житель деревни дал игроку <w>квест</w> вернуть украденный предмет."
                            }
                        }
                    ],
                    "synonyms": [
                        "mission",
                        "task",
                        "objective"
                    ],
                    "common_phrases": [
                        "side quest",
                        "main quest",
                        "complete a quest",
                        "accept a quest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное задание или миссия, которую персонаж видеоигры должен выполнить для получения награды или продвижения по сюжету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квест",
                                "target_lang_sense_clarification": "задание в компьютерной игре"
                            },
                            {
                                "target_lang_word": "задание",
                                "target_lang_sense_clarification": "поручение, которое нужно выполнить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06303d40-e817-3aa1-b290-dade5157cb44",
                    "sense_definition": "A short assessment in school that is longer than a quiz but shorter than a test.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "education_test",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The math teacher announced a <w>quest</w> on fractions for next Tuesday.",
                            "target_lang_translations": {
                                "ru": "Учитель математики объявил о <w>проверочной работе</w> по дробям в следующий вторник."
                            }
                        },
                        {
                            "text": "He was relieved that the assessment was only a <w>quest</w> and not a full exam.",
                            "target_lang_translations": {
                                "ru": "Он с облегчением узнал, что это была всего лишь небольшая <w>проверочная работа</w>, а не полноценный экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "test",
                        "quiz",
                        "assessment"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Portmanteau of quiz and test"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая проверочная работа в школе, которая длиннее опроса, но короче полноценного экзамена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверочная работа",
                                "target_lang_sense_clarification": "форма контроля знаний"
                            },
                            {
                                "target_lang_word": "тест",
                                "target_lang_sense_clarification": "испытание, проверка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ee30f1f1-70cd-3a01-bc04-944923586855",
                    "sense_definition": "To search for something; to go on a journey to find or obtain something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "search_mission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dogs <w>quested</w> back and forth until they found the scent.",
                            "target_lang_translations": {
                                "ru": "Собаки <w>рыскали</w> взад и вперед, пока не нашли след."
                            }
                        },
                        {
                            "text": "He <w>quested</w> after truth and wisdom throughout his entire life.",
                            "target_lang_translations": {
                                "ru": "Он <w>искал</w> истину и мудрость на протяжении всей своей жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "seek",
                        "search",
                        "pursue",
                        "hunt"
                    ],
                    "common_phrases": [
                        "quest for",
                        "quest after"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать что-либо; отправляться в путешествие, чтобы найти или получить что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "стараться найти, обнаружить"
                            },
                            {
                                "target_lang_word": "разыскивать",
                                "target_lang_sense_clarification": "заниматься поисками кого-либо или чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e3343a03-0240-353c-a128-225c35837f73",
                    "sense_definition": "A surname of German origin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Quest</w> arrived at the hotel late last night.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Квест</w> прибыл в отель поздно ночью."
                            }
                        },
                        {
                            "text": "We are looking for a file under the name <w>Quest</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем файл на имя <w>Квест</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия немецкого происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Квест",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
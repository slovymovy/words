                {
                    "sense_id": "f75e43b6-a6c0-3441-aab8-ad4cb2b408f2",
                    "sense_definition": "Een gebouw waar mensen tegen betaling kunnen overnachten en vaak ook eten",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We slapen vannacht in een luxe <w>hotel</w> in het centrum.",
                            "target_lang_translations": {
                                "ru": "Сегодня ночью мы ночуем в роскошном <w>отеле</w> в центре."
                            }
                        },
                        {
                            "text": "Heb jij al een <w>hotel</w> geboekt voor de vakantie?",
                            "target_lang_translations": {
                                "ru": "Ты уже забронировал <w>отель</w> на время отпуска?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "een hotel boeken",
                        "in een hotel slapen",
                        "vijfsterrenhotel"
                    ],
                    "target_lang_definitions": {

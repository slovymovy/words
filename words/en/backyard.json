            "senses": [
                {
                    "sense_id": "d5832ab4-04d6-304a-a802-e6bae3907469",
                    "sense_definition": "A yard to the rear of a house or similar residence.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children love to play soccer in the <w>backyard</w> after school.",
                            "target_lang_translations": {
                                "ru": "Дети любят играть в футбол на <w>заднем дворе</w> после школы."
                            }
                        },
                        {
                            "text": "We are planning a <w>backyard</w> barbecue for all our neighbors this weekend.",
                            "target_lang_translations": {
                                "ru": "Мы планируем барбекю на <w>заднем дворе</w> для всех наших соседей в эти выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "garden",
                        "rear yard"
                    ],
                    "antonyms": [
                        "front yard#Noun"
                    ],
                    "common_phrases": [
                        "in the backyard",
                        "backyard barbecue",
                        "backyard fence",
                        "big backyard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двор, расположенный позади дома или жилого здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задний двор",
                                "target_lang_sense_clarification": "территория за домом"
                            },
                            {
                                "target_lang_word": "двор",
                                "target_lang_sense_clarification": "общая придомовая территория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f43b1caf-36f5-3b22-883b-af5695b916da",
                    "sense_definition": "A person's neighborhood, or an area nearby to a person's usual residence or place of work.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I know every street in this city because it is my <w>backyard</w>.",
                            "target_lang_translations": {
                                "ru": "Я знаю каждую улицу в этом городе, потому что это мой <w>родной район</w>."
                            }
                        },
                        {
                            "text": "Many people ignore problems until they happen in their own <w>backyard</w>.",
                            "target_lang_translations": {
                                "ru": "Многие люди игнорируют проблемы, пока они не случаются у них <w>под носом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "neighborhood",
                        "turf",
                        "territory"
                    ],
                    "common_phrases": [
                        "in my own backyard",
                        "not in my backyard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Район проживания человека или хорошо знакомая ему местность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свой район",
                                "target_lang_sense_clarification": "место жительства"
                            },
                            {
                                "target_lang_word": "округа",
                                "target_lang_sense_clarification": "окрестности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "031d2edd-2937-3c86-9e72-0d330b5361da",
                    "sense_definition": "An area nearby to a country or other jurisdiction's legal boundaries, particularly an area in which the country feels it has an interest.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The superpower has always considered the island nation to be its political <w>backyard</w>.",
                            "target_lang_translations": {
                                "ru": "Сверхдержава всегда считала это островное государство своим политическим <w>задним двором</w>."
                            }
                        },
                        {
                            "text": "Diplomats are concerned about the growing instability in the country's <w>backyard</w>.",
                            "target_lang_translations": {
                                "ru": "Дипломаты обеспокоены растущей нестабильностью в <w>зоне влияния</w> страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "sphere of influence"
                    ],
                    "common_phrases": [
                        "political backyard",
                        "America's backyard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория вблизи границ страны, входящая в сферу её интересов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задний двор",
                                "target_lang_sense_clarification": "политическая метафора"
                            },
                            {
                                "target_lang_word": "сфера влияния",
                                "target_lang_sense_clarification": "геополитика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

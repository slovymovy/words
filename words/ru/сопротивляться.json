    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5cba174d-b4ac-3fa0-9eec-4033f297c92f",
                    "sense_definition": "Оказывать активное физическое противодействие силе, нападению или принуждению, стараясь не дать себя победить или захватить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грабитель пытался <w>сопротивляться</w> при задержании, но полиция действовала быстро."
                        },
                        {
                            "text": "Мы будем <w>сопротивляться</w> врагу до последнего патрона."
                        }
                    ],
                    "synonyms": [
                        "обороняться",
                        "защищаться",
                        "отбиваться"
                    ],
                    "antonyms": [
                        "сдаваться",
                        "капитулировать",
                        "подчиняться"
                    ],
                    "common_phrases": [
                        "яростно сопротивляться",
                        "сопротивляться аресту",
                        "перестать сопротивляться",
                        "активно сопротивляться"
                    ]
                },
                {
                    "sense_id": "ce390b0a-5360-3567-b2cd-9512be403817",
                    "sense_definition": "Выступать против чьих-либо действий, мнений или воли; не соглашаться подчиняться или принимать что-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подросток долго <w>сопротивлялся</w> влиянию родителей, желая жить самостоятельно."
                        },
                        {
                            "text": "Бесполезно <w>сопротивляться</w> прогрессу, нужно учиться использовать новые технологии."
                        }
                    ],
                    "synonyms": [
                        "противиться",
                        "возражать",
                        "упрямиться",
                        "противодействовать"
                    ],
                    "antonyms": [
                        "соглашаться",
                        "уступать",
                        "покоряться",
                        "смиряться"
                    ],
                    "common_phrases": [
                        "сопротивляться давлению",
                        "сопротивляться переменам",
                        "внутренне сопротивляться",
                        "сопротивляться воле"
                    ]
                },
                {
                    "sense_id": "9cef3b9e-39b1-33be-b3e9-e8d57d37e6b0",
                    "sense_definition": "Стараться не поддаться влиянию какого-либо состояния, чувства, болезни или искушения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "internal_resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я изо всех сил старался <w>сопротивляться</w> сну, чтобы доделать работу."
                        },
                        {
                            "text": "Ослабленный организм уже не мог <w>сопротивляться</w> тяжелой инфекции."
                        }
                    ],
                    "synonyms": [
                        "бороться",
                        "противостоять"
                    ],
                    "antonyms": [
                        "поддаваться"
                    ],
                    "common_phrases": [
                        "сопротивляться болезни",
                        "сопротивляться искушению",
                        "сопротивляться желанию",
                        "сопротивляться усталости"
                    ]
                },
                {
                    "sense_id": "e6878f58-9a7e-3aa9-8e40-ad6ed8fc354d",
                    "sense_definition": "Не поддаваться разрушительному внешнему воздействию или обработке (о материалах и предметах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот новый сплав отлично <w>сопротивляется</w> коррозии даже в морской воде."
                        },
                        {
                            "text": "Твердое дерево <w>сопротивлялось</w> топору, и рубить его было очень тяжело."
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "3afd0d5f-1794-327e-9059-aef5ab97e9f1",
                    "sense_definition": "From one side or end of something to the other, entering and then leaving it.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train went fast <w>through</w> the long dark tunnel.",
                            "target_lang_translations": {
                                "ru": "Поезд быстро проехал <w>через</w> длинный темный туннель."
                            }
                        },
                        {
                            "text": "You must walk <w>through</w> the park to get to the school.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно пройти <w>через</w> парк, чтобы попасть в школу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "through the door",
                        "through the window",
                        "walk through"
                    ],
                    "target_lang_definitions": {
                        "ru": "С одной стороны или конца чего-либо на другую, входя внутрь и выходя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "через",
                                "target_lang_sense_clarification": "общее значение движения сквозь пространство"
                            },
                            {
                                "target_lang_word": "сквозь",
                                "target_lang_sense_clarification": "с усилием или через препятствие/среду"
                            }
                        ]
                    }
                },
                {

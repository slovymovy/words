        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab7b2622-de38-3f7e-9844-0f3a28191221",
                    "sense_definition": "Het aandeel of de inbreng die iemand levert om een gemeenschappelijk doel te bereiken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contribution_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>bijdrage</w> aan het project was onmisbaar voor het eindresultaat.",
                            "target_lang_translations": {
                                "en": "His <w>contribution</w> to the project was indispensable for the final result.",
                                "ru": "Его <w>вклад</w> в проект был незаменим для конечного результата."
                            }
                        },
                        {
                            "text": "Wij willen graag een positieve <w>bijdrage</w> leveren aan de samenleving.",
                            "target_lang_translations": {
                                "en": "We would like to make a positive <w>contribution</w> to society.",
                                "ru": "Мы хотели бы внести положительный <w>вклад</w> в жизнь общества."
                            }
                        }
                    ],
                    "synonyms": [
                        "contributie",
                        "contributie"
                    ],
                    "common_phrases": [
                        "een bijdrage leveren",
                        "een belangrijke bijdrage",
                        "actieve bijdrage"
                    ],
                    "target_lang_definitions": {
                        "en": "The share or input that someone provides to achieve a common goal.",
                        "ru": "Доля участия или усилия, которые кто-то прилагает для достижения общей цели."
                    },
                    "translations": {

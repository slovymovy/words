    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ee6c7747-e34c-3af0-a7f6-3b96fa250e92",
                    "sense_definition": "Открывать, разворачивать или размыкать что-либо закрытое, сложенное или сомкнутое (дверь, книгу, зонт, объятия).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_opening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я люблю <раскрывать> окно по утрам, чтобы впустить свежий воздух.",
                            "target_lang_translations": {
                                "en": "I like to <open> the window in the morning to let in fresh air."
                            }
                        },
                        {
                            "text": "Ученики начали <раскрывать> учебники на десятой странице.",
                            "target_lang_translations": {
                                "en": "The students began to <open> their textbooks to page ten."
                            }
                        },
                        {
                            "text": "При встрече он всегда спешил <раскрывать> объятия друзьям.",
                            "target_lang_translations": {
                                "en": "Upon meeting, he always hurried to <open> his arms to friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "открывать",
                        "разворачивать",
                        "распахивать"
                    ],
                    "common_phrases": [
                        "раскрывать книгу",
                        "раскрывать зонт",
                        "раскрывать объятия",
                        "раскрывать рот"
                    ],
                    "target_lang_definitions": {
                        "en": "To open, unfold, or spread out something that is closed, folded, or joined."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "general physical action (window, book, arms)"
                            },
                            {
                                "target_lang_word": "unfold",
                                "target_lang_sense_clarification": "for items that are folded (umbrella, map)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e5e30b7-2f2e-33d8-a1ab-7b1f7168f1f5",
                    "sense_definition": "Делать известным то, что было тайным, неизвестным или скрытым; сообщать правду.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_revealing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы не имеем права <раскрывать> личные данные наших клиентов.",
                            "target_lang_translations": {
                                "en": "We do not have the right to <disclose> our clients' personal data."
                            }
                        },
                        {
                            "text": "Ей было трудно <раскрывать> душу перед незнакомым человеком.",
                            "target_lang_translations": {
                                "en": "It was hard for her to <reveal> her soul to a stranger."
                            }

                    "sense_id": "7d1bc427-14eb-3928-abde-b7fa1be6055f",
                    "sense_definition": "nazwa wielu wsi i miejscowości w Polsce",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają w małej wsi o nazwie <w>Róża</w>.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка живут в маленькой деревне под названием <w>Ружа</w>."
                            }
                        },
                        {
                            "text": "Autobus do miejscowości <w>Róża</w> odjeżdża o godzinie piętnastej.",
                            "target_lang_translations": {
                                "ru": "Автобус до населённого пункта <w>Ружа</w> отправляется в пятнадцать часов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "название многих деревень и местностей в Польше"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ружа",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86d1d9a3-7be3-3358-9885-5f9237dca7d1",
                    "sense_definition": "nazwa polskiego herbu szlacheckiego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heraldry",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "Na tarczy herbowej rodu widniała srebrna pięciopłatkowa <w>Róża</w>.",
                            "target_lang_translations": {
                                "ru": "На гербовом щите рода красовалась серебряная пятилепестковая <w>Роза</w>."
                            }
                        },
                        {
                            "text": "Szlachcic ten pieczętował się herbem <w>Róża</w>.",
                            "target_lang_translations": {
                                "ru": "Этот шляхтич пользовался гербом <w>Роза</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Poraj"
                    ],
                    "target_lang_definitions": {
                        "ru": "название польского дворянского герба"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Роза",
                                "target_lang_sense_clarification": "герб"
                            }
                        ]
                    }
                }
            ]
        }

                        "en": [
                            {
                                "target_lang_word": "camp",
                                "target_lang_sense_clarification": "temporary accommodation"
                            },
                            {
                                "target_lang_word": "summer camp",
                                "target_lang_sense_clarification": "holiday activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe580b1b-0818-3b5f-86e0-8b0a4599616f",
                    "sense_definition": "Een bewaakt of afgesloten terrein waar grote groepen mensen, zoals militairen, vluchtelingen of gevangenen, (tijdelijk) gehuisvest zijn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verblijfplaats",
                    "name_type": "no",
                    "examples": [

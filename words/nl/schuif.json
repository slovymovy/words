                            },
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "to move an object"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подвинься",
                                "target_lang_sense_clarification": "сдвинься с места (о человеке)"
                            },
                            {
                                "target_lang_word": "подвинь",

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed9cf426-9f94-3729-bb2a-9f860c889008",
                    "sense_definition": "Meervoud van <w>klauw</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leeuw zette zijn scherpe <w>klauwen</w> in de prooi.",
                            "target_lang_translations": {
                                "ru": "Лев вонзил свои острые <w>когти</w> в добычу.",
                                "en": "The lion sank its sharp <w>claws</w> into the prey."
                            }
                        },
                        {
                            "text": "Pas op dat de kat je niet krabt met zijn <w>klauwen</w>.",
                            "target_lang_translations": {
                                "ru": "Осторожно, чтобы кот не поцарапал тебя своими <w>когтями</w>.",
                                "en": "Watch out that the cat doesn't scratch you with its <w>claws</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "scherpe klauwen",
                        "in de klauwen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Когти (роговые образования на концах пальцев животных)",
                        "en": "The sharp curved nails on the feet of animals (plural)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когти",
                                "target_lang_sense_clarification": "острые образования на пальцах"
                            },
                            {
                                "target_lang_word": "лапы",
                                "target_lang_sense_clarification": "конечности животных (разг.)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "claws",
                                "target_lang_sense_clarification": "sharp nails of animals"
                            },
                            {
                                "target_lang_word": "talons",
                                "target_lang_sense_clarification": "claws of a bird of prey"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "948841bd-642b-37a2-b1d7-1475e21085e1",
                    "sense_definition": "Iets wegnemen van iemand en het zich onrechtmatig toe-eigenen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iemand heeft gisteren mijn portemonnee <w>geklauwd</w>.",
                            "target_lang_translations": {
                                "ru": "Кто-то вчера <w>стащил</w> мой кошелек.",
                                "en": "Someone <w>swiped</w> my wallet yesterday."
                            }
                        },
                        {
                            "text": "Je mag niet zomaar snoep uit de winkel <w>klauwen</w>.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто так <w>таскать</w> конфеты из магазина.",
                                "en": "You can't just <w>swipe</w> candy from the store."
                            }
                        }
                    ],
                    "synonyms": [
                        "stelen",
                        "jatten",
                        "pikken",
                        "gappen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украсть или присвоить что-то чужое (разговорное)",
                        "en": "To take something unlawfully; to steal (informal)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стащить",
                                "target_lang_sense_clarification": "украсть (разг.)"
                            },
                            {
                                "target_lang_word": "спереть",
                                "target_lang_sense_clarification": "украсть (груб. прост.)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "swipe",
                                "target_lang_sense_clarification": "to steal (informal)"
                            },
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "to take quickly or illicitly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8a2c5f9-1df8-324b-9d0b-950e420b9b48",
                    "sense_definition": "Met de nagels of handen grijpen, krabben of zich ergens aan vasthouden",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bange kat <w>klauwde</w> wild om zich heen.",
                            "target_lang_translations": {
                                "ru": "Испуганный кот дико <w>царапался</w> вокруг себя.",
                                "en": "The scared cat <w>clawed</w> wildly around itself."
                            }
                        },
                        {
                            "text": "Hij <w>klauwde</w> zich wanhopig vast aan de rand van de afgrond.",
                            "target_lang_translations": {
                                "ru": "Он отчаянно <w>цеплялся</w> за край пропасти.",
                                "en": "He desperately <w>clutched</w> onto the edge of the abyss."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich vast klauwen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хватать, царапать или судорожно держаться руками или когтями",
                        "en": "To scratch, grab, or hold onto something tightly with claws or hands"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепляться",
                                "target_lang_sense_clarification": "крепко держаться"
                            },
                            {
                                "target_lang_word": "царапать",
                                "target_lang_sense_clarification": "повреждать когтями"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "claw",
                                "target_lang_sense_clarification": "to scratch or tear with claws"
                            },
                            {
                                "target_lang_word": "clutch",
                                "target_lang_sense_clarification": "to grasp tightly"
                            }
                        ]
                    }
                }
            ]
        }
    ]

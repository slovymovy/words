                {
                    "sense_id": "0400285a-016e-392e-beb0-3c8fc928463b",
                    "sense_definition": "С более высокого места вниз.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго смотрели на город <сверху>.",
                            "target_lang_translations": {
                                "en": "We looked at the city <from above> for a long time."
                            }
                        },
                        {
                            "text": "Камень неожиданно упал <сверху>.",
                            "target_lang_translations": {
                                "en": "A stone unexpectedly fell <from above>."
                            }
                        }
                    ],
                    "antonyms": [
                        "снизу"
                    ],
                    "common_phrases": [
                        "смотреть сверху",
                        "падать сверху",
                        "вид сверху"
                    ],
                    "target_lang_definitions": {
                        "en": "From a higher place downwards"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from above",
                                "target_lang_sense_clarification": "direction or origin"
                            },
                            {
                                "target_lang_word": "from the top",
                                "target_lang_sense_clarification": "from the highest point"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bfef948-e851-3499-921f-85298d7699be",

                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miners built a brick <w>stopping</w> to seal off the old tunnel.",
                            "target_lang_translations": {
                                "ru": "Шахтеры построили кирпичную <w>перемычку</w>, чтобы изолировать старый туннель."

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лавровый лист",
                                "target_lang_sense_clarification": "пряность"
                            },
                            {
                                "target_lang_word": "лавр",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22778cf7-390a-3332-b91e-ebf6af3c9235",
                    "sense_definition": "A marked section of ground or a compartment inside a building or vehicle used for a specific purpose.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5b17490-ef3e-3d0a-b610-2369a3e8bf4e",
                    "sense_definition": "Een mannelijke inwoner van Italië of iemand van Italiaanse afkomst.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman is een <w>Italiaan</w> en hij maakt de beste pasta van de buurt.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <w>итальянец</w>, и он готовит лучшую пасту в округе."
                            }
                        },
                        {
                            "text": "Tijdens onze vakantie spraken we met een vriendelijke <w>Italiaan</w> die ons de weg wees.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы разговорились с дружелюбным <w>итальянцем</w>, который показал нам дорогу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een echte Italiaan",
                        "trotse Italiaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Житель Италии мужского пола или человек итальянского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "итальянец",
                                "target_lang_sense_clarification": "житель страны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa494ce1-1b4b-3cd5-91ca-8ee1a459d242",
                    "sense_definition": "Een restaurant waar gerechten uit de Italiaanse keuken worden geserveerd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we vanavond gezellig bij de <w>italiaan</w> op het plein gaan eten?",
                            "target_lang_translations": {
                                "ru": "Может, поужинаем сегодня в уютном <w>итальянском ресторане</w> на площади?"
                            }
                        },
                        {
                            "text": "De nieuwe <w>italiaan</w> in de stad heeft heerlijke pizza's op het menu staan.",
                            "target_lang_translations": {
                                "ru": "Новый <w>итальянский ресторан</w> в городе предлагает в меню отличную пиццу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bij de italiaan eten",
                        "naar de italiaan gaan"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often written in lowercase when referring to the restaurant"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ресторан, где подают блюда итальянской кухни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "итальянский ресторан",
                                "target_lang_sense_clarification": "заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fcb2c46-6f7a-3f81-bc11-bb4224c6e43f",
                    "sense_definition": "Een auto van een Italiaans merk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij rijdt al jaren in een stijlvolle <w>italiaan</w> die veel bekijks trekt.",
                            "target_lang_translations": {
                                "ru": "Он уже много лет ездит на стильном <w>итальянце</w>, который привлекает много внимания."
                            }
                        },
                        {
                            "text": "Hoewel een <w>italiaan</w> er vaak mooi uitziet, is het onderhoud soms prijzig.",
                            "target_lang_translations": {
                                "ru": "Хотя <w>итальянец</w> часто выглядит красиво, его обслуживание иногда обходится дорого."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often written in lowercase when referring to the car"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобиль итальянской марки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "итальянец",
                                "target_lang_sense_clarification": "автомобиль (разг.)"
                            },
                            {
                                "target_lang_word": "итальянское авто",
                                "target_lang_sense_clarification": "автомобиль"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
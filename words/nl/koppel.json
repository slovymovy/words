    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abe42e4b-109d-39c5-91ec-2c34623e0637",
                    "sense_definition": "Twee personen die een liefdesrelatie hebben of een eenheid vormen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_pair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het pasgetrouwde <w>koppel</w> ging op huwelijksreis naar Italië.",
                            "target_lang_translations": {
                                "ru": "Молодоженая <w>пара</w> отправилась в свадебное путешествие в Италию."
                            }
                        },
                        {
                            "text": "Zij vormen samen een heel leuk <w>koppel</w>.",
                            "target_lang_translations": {
                                "ru": "Они составляют очень милую <w>пару</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stel",
                        "paar",
                        "duo"
                    ],
                    "common_phrases": [
                        "een jong koppel",
                        "een getrouwd koppel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Два человека, состоящие в любовных отношениях или образующие единство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пара",
                                "target_lang_sense_clarification": "двое людей в отношениях"
                            },
                            {
                                "target_lang_word": "чета",
                                "target_lang_sense_clarification": "супружеская пара (официально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dbdc3a3-9818-330c-ad90-6825510e5a46",
                    "sense_definition": "Natuurkundige grootheid die de draaiende kracht van een motor of as uitdrukt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een elektrische auto heeft direct een hoog <w>koppel</w> beschikbaar.",
                            "target_lang_translations": {
                                "ru": "У электромобиля сразу доступен высокий <w>крутящий момент</w>."
                            }
                        },
                        {
                            "text": "De vrachtwagen heeft veel <w>koppel</w> nodig om de zware lading te trekken.",
                            "target_lang_translations": {
                                "ru": "Грузовику нужен большой <w>крутящий момент</w>, чтобы тянуть тяжелый груз."
                            }
                        }
                    ],
                    "synonyms": [
                        "draaimoment"
                    ],
                    "common_phrases": [
                        "hoog koppel",
                        "maximaal koppel"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая величина, выражающая вращающую силу двигателя или вала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крутящий момент",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "момент силы",
                                "target_lang_sense_clarification": "физика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd6efb34-2850-3590-8f8d-0843307f4da6",
                    "sense_definition": "Een stevige riem die deel uitmaakt van een uniform, vaak gebruikt om uitrusting aan te dragen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat bevestigde zijn veldfles aan zijn <w>koppel</w>.",
                            "target_lang_translations": {
                                "ru": "Солдат прикрепил флягу к своему <w>ремню</w>."
                            }
                        },
                        {
                            "text": "Hij poetste het leer van zijn <w>koppel</w> tot het glom.",
                            "target_lang_translations": {
                                "ru": "Он начищал кожу своего <w>ремня</w>, пока она не заблестела."
                            }
                        }
                    ],
                    "synonyms": [
                        "riem",
                        "gordel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочный ремень, являющийся частью униформы, часто используемый для ношения снаряжения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремень",
                                "target_lang_sense_clarification": "часть формы"
                            },
                            {
                                "target_lang_word": "портупея",
                                "target_lang_sense_clarification": "военное снаряжение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31b767d2-f13a-3912-90de-b82d8a1a0189",
                    "sense_definition": "Een groep dieren van dezelfde soort die bij elkaar horen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_pair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de wei liep een <w>koppel</w> schapen rustig te grazen.",
                            "target_lang_translations": {
                                "ru": "На лугу мирно паслось <w>стадо</w> овец."
                            }
                        },
                        {
                            "text": "De jager zag een <w>koppel</w> patrijzen opvliegen uit het struikgewas.",
                            "target_lang_translations": {
                                "ru": "Охотник увидел, как из кустов взлетела <w>стая</w> куропаток."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een koppel schapen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа животных одного вида, держащихся вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стадо",
                                "target_lang_sense_clarification": "о животных (например, овцы)"
                            },
                            {
                                "target_lang_word": "стая",
                                "target_lang_sense_clarification": "о птицах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9ead9e87-bb19-344f-8904-f73b6dfd2238",
                    "sense_definition": "Vervoeging van <w>koppelen</w>: eerste persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>koppel</w> de aanhangwagen voorzichtig aan de auto.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>прицепляю</w> прицеп к машине."
                            }
                        },
                        {
                            "text": "In dit bestand <w>koppel</w> ik de namen aan de juiste adressen.",
                            "target_lang_translations": {
                                "ru": "В этом файле я <w>связываю</w> имена с правильными адресами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола koppelen: первое лицо единственного числа настоящего времени (я соединяю)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прицепляю",
                                "target_lang_sense_clarification": "механически соединяю"
                            },
                            {
                                "target_lang_word": "связываю",
                                "target_lang_sense_clarification": "устанавливаю связь"
                            },
                            {
                                "target_lang_word": "соединяю",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff14247e-0386-3fab-bbf0-cc8d786f9db6",
                    "sense_definition": "Vervoeging van <w>koppelen</w>: gebiedende wijs.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Koppel</w> de slang los voordat je de kraan repareert.",
                            "target_lang_translations": {
                                "ru": "<w>Отсоедини</w> шланг перед тем, как чинить кран."
                            }
                        },
                        {
                            "text": "<w>Koppel</w> je account aan je e-mailadres voor meer veiligheid.",
                            "target_lang_translations": {
                                "ru": "<w>Привяжи</w> свой аккаунт к адресу электронной почты для большей безопасности."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола koppelen: повелительное наклонение (соедини!)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отсоедини",
                                "target_lang_sense_clarification": "в сочетании с 'los'"
                            },
                            {
                                "target_lang_word": "привяжи",
                                "target_lang_sense_clarification": "аккаунт или данные"
                            },
                            {
                                "target_lang_word": "соедини",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

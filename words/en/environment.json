                    "semantic_group_id": "computing_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The application runs faster in a secure <w>environment</w>.",
                            "target_lang_translations": {
                                "ru": "Приложение работает быстрее в безопасной <w>среде</w>."

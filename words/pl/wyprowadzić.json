{
    "word_family": [
        "wyprowadzać",
        "wyprowadzka",
        "wyprowadzenie",
        "wprowadzić",
        "przeprowadzić",
        "odprowadzić",
        "zaprowadzić",
        "naprowadzić",
        "prowadzić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f59f42c9-8c2e-3c10-aa0e-42e1cdd296b3",
                    "sense_definition": "Sprawić, że ktoś lub coś opuści jakieś pomieszczenie lub budynek, idąc razem z tą osobą i wskazując jej drogę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ochroniarz musiał siłą <w>wyprowadzić</w> awanturującego się klienta ze sklepu.",
                            "target_lang_translations": {
                                "ru": "Охраннику пришлось силой <w>вывести</w> буйного клиента из магазина."
                            }
                        },
                        {
                            "text": "Nauczycielka spokojnie <w>wyprowadziła</w> wszystkich uczniów ze szkoły podczas alarmu.",
                            "target_lang_translations": {
                                "ru": "Учительница спокойно <w>вывела</w> всех учеников из школы во время тревоги."
                            }
                        }
                    ],
                    "antonyms": [
                        "wprowadzić"
                    ],
                    "common_phrases": [
                        "wyprowadzić gości",
                        "wyprowadzić ludzi",
                        "wyprowadzić na zewnątrz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то или что-то покинуть помещение или здание, сопровождая и указывая путь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывести",
                                "target_lang_sense_clarification": "из помещения, здания"
                            },
                            {
                                "target_lang_word": "выпроводить",
                                "target_lang_sense_clarification": "заставить уйти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ebc87d3-8a05-3b0a-adde-9bdfdd5f14e0",
                    "sense_definition": "Pójść ze zwierzęciem domowym na zewnątrz, aby mogło pospacerować i załatwić swoje potrzeby fizjologiczne.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę wcześnie rano <w>wyprowadzić</w> psa, zanim pójdę do pracy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно рано утром <w>выгулять</w> собаку перед уходом на работу."
                            }
                        },
                        {
                            "text": "Czy możesz <w>wyprowadzić</w> Azora na krótki spacer po parku?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>вывести</w> Азора на короткую прогулку в парк?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyprowadzić psa",
                        "wyprowadzić na spacer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выйти с домашним животным на улицу для прогулки и туалета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгулять",
                                "target_lang_sense_clarification": "собаку, питомца"
                            },
                            {
                                "target_lang_word": "вывести",
                                "target_lang_sense_clarification": "на прогулку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b297cdb2-e7b9-3d2e-9d4a-8b357d0b91d5",
                    "sense_definition": "Zmienić miejsce zamieszkania, opuszczając dotychczasowy dom lub mieszkanie (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat postanowił <w>wyprowadzić</w> się od rodziców i zamieszkać sam.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат решил <w>съехать</w> от родителей и жить один."
                            }
                        },
                        {
                            "text": "Sąsiedzi planują <w>wyprowadzić</w> się z bloku w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Соседи планируют <w>съехать</w> из многоквартирного дома в следующем месяце."
                            }
                        }
                    ],
                    "antonyms": [
                        "wprowadzić się"
                    ],
                    "common_phrases": [
                        "wyprowadzić się z domu",
                        "wyprowadzić się na swoje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "z zaimkiem 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сменить место жительства, покидая нынешний дом или квартиру (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съехать",
                                "target_lang_sense_clarification": "с квартиры, из дома"
                            },
                            {
                                "target_lang_word": "выехать",
                                "target_lang_sense_clarification": "покинуть жилье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff222e0f-af0c-32f5-a1f3-e3664a8cd239",
                    "sense_definition": "Wyjechać pojazdem z garażu, parkingu lub innego miejsca postojowego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą bardzo trudno jest <w>wyprowadzić</w> samochód z zasypanego śniegiem podjazdu.",
                            "target_lang_translations": {
                                "ru": "Зимой очень трудно <w>вывести</w> машину с засыпанного снегом подъезда."
                            }
                        },
                        {
                            "text": "Tata poprosił mnie, żebym <w>wyprowadził</w> auto z garażu przed dom.",
                            "target_lang_translations": {
                                "ru": "Папа попросил меня, чтобы я <w>выгнал</w> машину из гаража к дому."
                            }
                        }
                    ],
                    "antonyms": [
                        "wprowadzić"
                    ],
                    "common_phrases": [
                        "wyprowadzić samochód",
                        "wyprowadzić auto z garażu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выехать на транспортном средстве из гаража, с парковки или другого места стоянки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывести",
                                "target_lang_sense_clarification": "транспортное средство"
                            },
                            {
                                "target_lang_word": "выгнать",
                                "target_lang_sense_clarification": "машину из гаража"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21710efb-40fd-3503-b07b-eec63f7452ab",
                    "sense_definition": "Pomóc komuś rozwiązać problem lub zmienić czyjś stan psychiczny, często na lepszy lub bardziej świadomy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobra terapia może <w>wyprowadzić</w> pacjenta z głębokiej depresji.",
                            "target_lang_translations": {
                                "ru": "Хорошая терапия может <w>вывести</w> пациента из глубокой депрессии."
                            }
                        },
                        {
                            "text": "Muszę cię <w>wyprowadzić</w> z błędu, to nie jest właściwa droga do celu.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>вывести</w> тебя из заблуждения, это не правильный путь к цели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyprowadzić z błędu",
                        "wyprowadzić na prostą",
                        "wyprowadzić z równowagi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помочь кому-то решить проблему или изменить психическое состояние, часто к лучшему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывести",
                                "target_lang_sense_clarification": "из состояния, из заблуждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19456b81-1e02-3f4f-9e48-d33ffbbb32e4",
                    "sense_definition": "Utworzyć coś nowego, na przykład wniosek lub formułę matematyczną, na podstawie logicznego rozumowania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logic_science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji fizyki musieliśmy samodzielnie <w>wyprowadzić</w> wzór na prędkość.",
                            "target_lang_translations": {
                                "ru": "На уроке физики мы должны были самостоятельно <w>вывести</w> формулу скорости."
                            }
                        },
                        {
                            "text": "Z analizy zebranych danych udało się <w>wyprowadzić</w> bardzo ciekawe wnioski.",
                            "target_lang_translations": {
                                "ru": "Из анализа собранных данных удалось <w>вывести</w> очень интересные заключения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyprowadzić wzór",
                        "wyprowadzić wniosek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать что-то новое, например вывод или математическую формулу, на основе логических рассуждений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывести",
                                "target_lang_sense_clarification": "формулу, закон, заключение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
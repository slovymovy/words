                            {
                                "target_lang_word": "смертоносный",
                                "target_lang_sense_clarification": "несущий смерть (об оружии, силе)"
                            },
                            {
                                "target_lang_word": "летальный",
                                "target_lang_sense_clarification": "медицинский или технический термин"
                            }

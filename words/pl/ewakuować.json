{
    "word_family": [
        "ewakuacja",
        "ewakuacyjny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "64d81b6f-63e8-3db0-826f-d7f24763a792",
                    "sense_definition": "Przemieszczać ludzi, zwierzęta lub mienie z obszaru zagrożonego do miejsca bezpiecznego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z powodu pożaru strażacy musieli natychmiast <w>ewakuować</w> mieszkańców bloku.",
                            "target_lang_translations": {
                                "ru": "Из-за пожара пожарным пришлось немедленно <w>эвакуировать</w> жильцов дома."
                            }
                        },
                        {
                            "text": "Władze postanowiły <w>ewakuować</w> całą wioskę przed nadejściem wielkiej wody.",
                            "target_lang_translations": {
                                "ru": "Власти решили <w>эвакуировать</w> всю деревню до прихода большой воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyprowadzać",
                        "wywozić"
                    ],
                    "common_phrases": [
                        "ewakuować ludność",
                        "ewakuować budynek",
                        "ewakuować rannych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать людей, животных или имущество из опасной зоны в безопасное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эвакуировать",
                                "target_lang_sense_clarification": "вывозить в безопасное место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73e491c2-e635-3f7b-92b5-c4bb23ab77ce",
                    "sense_definition": "Opuszczać samodzielnie miejsce niebezpieczne lub zagrożone (używane z zaimkiem \"się\").",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy usłyszeliśmy alarm przeciwpożarowy, musieliśmy się szybko <w>ewakuować</w> z biura.",
                            "target_lang_translations": {
                                "ru": "Когда мы услышали пожарную тревогу, нам пришлось быстро <w>эвакуироваться</w> из офиса."
                            }
                        },
                        {
                            "text": "Pilot zdołał się <w>ewakuować</w> z uszkodzonego samolotu w ostatniej chwili.",
                            "target_lang_translations": {
                                "ru": "Пилот сумел <w>эвакуироваться</w> из поврежденного самолета в последний момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciekać",
                        "oddalać się"
                    ],
                    "common_phrases": [
                        "ewakuować się z budynku",
                        "sprawnie się ewakuować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самостоятельно покидать опасное место (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эвакуироваться",
                                "target_lang_sense_clarification": "покидать опасное место"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "9b0e36a5-3600-3340-a2fc-14b2e8c3972a",
                    "sense_definition": "Wykrzyknik wyrażający zakłopotanie z powodu drobnego błędu, pomyłki, potknięcia się lub niezręcznej sytuacji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ups</w>, przepraszam, nie zauważyłem cię w drzwiach.",
                            "target_lang_translations": {
                                "ru": "<w>Упс</w>, извини, я не заметил тебя в дверях."
                            }
                        },
                        {
                            "text": "<w>Ups</w>, chyba wylałem trochę soku na obrus.",
                            "target_lang_translations": {
                                "ru": "<w>Ой</w>, кажется, я пролил немного сока на скатерть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ups, przepraszam"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее смущение из-за мелкой ошибки, оплошности или неловкой ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упс",
                                "target_lang_sense_clarification": "междометие при ошибке"
                            },
                            {
                                "target_lang_word": "ой",
                                "target_lang_sense_clarification": "возглас при внезапной ошибке или испуге"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c631359-e18e-3758-803b-0e02a7f356b2",
                    "sense_definition": "Urządzenie elektroniczne (zasilacz awaryjny), które podtrzymuje działanie sprzętu, np. komputera, w przypadku zaniku prądu w sieci.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki <w>UPS-owi</w> zdążyłem zapisać wszystkie pliki przed wyłączeniem komputera.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>бесперебойнику</w> я успел сохранить все файлы до выключения компьютера."
                            }
                        },
                        {
                            "text": "W serwerowni zainstalowano nowy <w>UPS</w>, aby zapobiec utracie danych.",
                            "target_lang_translations": {
                                "ru": "В серверной установили новый <w>ИБП</w>, чтобы предотвратить потерю данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "upees"
                    ],
                    "common_phrases": [
                        "zasilacz UPS"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "skrótowiec"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство (источник бесперебойного питания), поддерживающее работу оборудования при отключении электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ИБП",
                                "target_lang_sense_clarification": "аббревиатура: источник бесперебойного питания"
                            },
                            {
                                "target_lang_word": "бесперебойник",
                                "target_lang_sense_clarification": "разговорное название устройства"
                            }
                        ]
                    }
                }
            ]
        }

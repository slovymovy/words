        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "15c94c89-c937-3f74-9f96-574e6a8d56fb",
                    "sense_definition": "Describing serious art music, often played by an orchestra, as opposed to pop, jazz, or folk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "music_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She enjoys listening to <w>classical</w> music while she studies.",
                            "target_lang_translations": {
                                "ru": "Она любит слушать <w>классическую</w> музыку во время учебы."
                            }
                        },
                        {
                            "text": "The radio station plays a mix of jazz and <w>classical</w> pieces.",
                            "target_lang_translations": {
                                "ru": "Радиостанция ставит в эфир смесь джаза и <w>классических</w> произведений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "classical music",
                        "classical concert",
                        "classical composer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к серьезной художественной музыке, часто исполняемой оркестром, в отличие от поп-музыки, джаза или фолка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классический",
                                "target_lang_sense_clarification": "о музыке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fd7c13a-6601-38cd-a570-0d40e9d0ec48",
                    "sense_definition": "Relating to the language, literature, history, and culture of ancient Greece and Rome.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ancient_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to study <w>classical</w> mythology at the university.",
                            "target_lang_translations": {
                                "ru": "Он решил изучать <w>классическую</w> мифологию в университете."
                            }
                        },
                        {
                            "text": "The museum contains many statues from the <w>classical</w> period.",
                            "target_lang_translations": {
                                "ru": "В музее находится много статуй <w>классического</w> периода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "classical mythology",
                        "classical civilization",
                        "classical studies",
                        "classical languages"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к языку, литературе, истории и культуре Древней Греции и Рима."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классический",
                                "target_lang_sense_clarification": "относящийся к античности"
                            },
                            {
                                "target_lang_word": "античный",
                                "target_lang_sense_clarification": "древнегреческий или древнеримский"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afbe9a69-0556-3aaf-8801-1e0719720ba3",
                    "sense_definition": "Following established, traditional styles or principles in art, literature, or science, rather than modern or experimental ones.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "traditional_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been trained in <w>classical</w> ballet since she was a child.",
                            "target_lang_translations": {
                                "ru": "Она обучалась <w>классическому</w> балету с детства."
                            }
                        },
                        {
                            "text": "The building features a <w>classical</w> design with tall white columns.",
                            "target_lang_translations": {
                                "ru": "Здание отличается <w>классическим</w> дизайном с высокими белыми колоннами."
                            }
                        }
                    ],
                    "synonyms": [
                        "classic",
                        "classic"
                    ],
                    "common_phrases": [
                        "classical ballet",
                        "classical architecture",
                        "classical education"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий устоявшимся, традиционным стилям или принципам в искусстве, литературе или науке, в отличие от современных или экспериментальных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классический",
                                "target_lang_sense_clarification": "традиционный"

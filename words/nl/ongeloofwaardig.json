    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "742f9a89-fca9-33ab-9483-1ff0fcdfdb66",
                    "sense_definition": "Niet in staat om geloofd te worden; niet aannemelijk of betrouwbaar overkomend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn verklaring voor de vertraging klonk erg <w>ongeloofwaardig</w>.",
                            "target_lang_translations": {
                                "en": "His explanation for the delay sounded very <w>implausible</w>.",
                                "ru": "Его объяснение задержки звучало очень <w>неправдоподобно</w>."
                            }
                        },
                        {
                            "text": "Het is volstrekt <w>ongeloofwaardig</w> dat niemand van de problemen afwist.",
                            "target_lang_translations": {
                                "en": "It is completely <w>implausible</w> that no one knew about the problems.",
                                "ru": "Совершенно <w>неправдоподобно</w>, что никто не знал о проблемах."
                            }
                        }
                    ],
                    "synonyms": [
                        "onaannemelijk",
                        "onwaarschijnlijk",
                        "dubieus"
                    ],
                    "antonyms": [
                        "geloofwaardig",
                        "aannemelijk"
                    ],
                    "common_phrases": [
                        "ongeloofwaardig overkomen",
                        "een ongeloofwaardig verhaal",
                        "volstrekt ongeloofwaardig"
                    ],
                    "target_lang_definitions": {
                        "en": "Not capable of being believed; appearing implausible or unreliable.",
                        "ru": "Не заслуживающий доверия; кажущийся неправдоподобным или ненадежным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "implausible",
                                "target_lang_sense_clarification": "difficult to believe; not seeming reasonable"
                            },
                            {
                                "target_lang_word": "unconvincing",
                                "target_lang_sense_clarification": "failing to make you believe something is true"
                            },
                            {
                                "target_lang_word": "incredible",
                                "target_lang_sense_clarification": "impossible to believe (literal sense)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неправдоподобный",
                                "target_lang_sense_clarification": "кажущийся ложным или выдуманным"
                            },
                            {
                                "target_lang_word": "неубедительный",
                                "target_lang_sense_clarification": "не вызывающий уверенности или доверия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0db37692-4e24-3545-b5d3-77924bab48bb",
                    "sense_definition": "Op een manier die geen geloof verdient of onwaarschijnlijk overkomt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De getuige sprak <w>ongeloofwaardig</w> en sprak zichzelf meerdere keren tegen.",
                            "target_lang_translations": {
                                "en": "The witness spoke <w>unconvincingly</w> and contradicted himself several times.",
                                "ru": "Свидетель говорил <w>неубедительно</w> и несколько раз противоречил сам себе."
                            }
                        },
                        {
                            "text": "Hij kwam nogal <w>ongeloofwaardig</w> over tijdens het sollicitatiegesprek.",
                            "target_lang_translations": {
                                "en": "He came across as rather <w>unconvincing</w> during the job interview.",
                                "ru": "Во время собеседования он выглядел довольно <w>неубедительно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onaannemelijk"
                    ],
                    "antonyms": [
                        "geloofwaardig"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner that does not deserve belief or appears improbable.",
                        "ru": "Образом, не вызывающим доверия или кажущимся маловероятным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unconvincingly",
                                "target_lang_sense_clarification": "in a way that fails to persuade"
                            },
                            {
                                "target_lang_word": "implausibly",
                                "target_lang_sense_clarification": "in a way that seems unlikely to be true"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неубедительно",
                                "target_lang_sense_clarification": "без должной аргументации или уверенности"
                            },
                            {
                                "target_lang_word": "неправдоподобно",
                                "target_lang_sense_clarification": "маловероятно, с оттенком лжи"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            {
                                "target_lang_word": "комплектова́ть",
                                "target_lang_sense_clarification": "собирать в комплект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0349458-40b7-30fa-b1d7-bc88f17194b9",
                    "sense_definition": "To live as a bachelor for a short time, usually while one's spouse is away.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "lifestyle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My wife is out of town, so I am <w>batching</w> it this week.",
                            "target_lang_translations": {
                                "ru": "Моя жена в отъезде, так что на этой неделе я <w>холостякую</w>."
                            }
                        },
                        {
                            "text": "He didn't mind <w>batching</w> for a few days while his family visited relatives.",
                            "target_lang_translations": {
                                "ru": "Он был не против <w>похолостяковать</w> несколько дней, пока его семья гостила у родственников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "batching it"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жить холостяцкой жизнью в течение короткого времени, обычно пока супруга в отъезде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холостякова́ть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fbd62ce5-3eda-3167-9937-245c8fc4c68d",
                    "sense_definition": "Relating to a process that operates on a specific group of items at once rather than continuously.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "process_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plant switched from continuous production to <w>batch</w> processing.",
                            "target_lang_translations": {
                                "ru": "Завод перешел с непрерывного производства на <w>серийную</w> обработку."
                            }
                        },
                        {
                            "text": "We use a <w>batch</w> system to manage the laundry services.",
                            "target_lang_translations": {
                                "ru": "Мы используем <w>пакетную</w> систему для управления услугами прачечной."
                            }
                        }
                    ],
                    "antonyms": [
                        "continuous"
                    ],
                    "common_phrases": [
                        "batch processing",
                        "batch production"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к процессу, который обрабатывает группу элементов одновременно, а не по одному или непрерывно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паке́тный",
                                "target_lang_sense_clarification": "режим, обработка"
                            },
                            {
                                "target_lang_word": "сери́йный",
                                "target_lang_sense_clarification": "тип производства"
                            }
                        ]
                    }
                }
            ]
        },

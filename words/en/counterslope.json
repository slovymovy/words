    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79b5d37a-20ae-35f3-9372-ff9206f92f8b",
                    "sense_definition": "An overhanging slope or a slope that inclines in the opposite direction to the surrounding terrain.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer designed a retaining wall to support the unstable <w>counterslope</w>.",
                            "target_lang_translations": {
                                "ru": "Инженер спроектировал подпорную стену для укрепления неустойчивого <w>обратного ската</w>."
                            }
                        },
                        {
                            "text": "Soldiers took cover behind the <w>counterslope</w> to avoid enemy fire.",
                            "target_lang_translations": {
                                "ru": "Солдаты укрылись за <w>обратным скатом</w>, чтобы избежать вражеского огня."
                            }
                        }
                    ],
                    "synonyms": [
                        "reverse slope",
                        "acclivity"
                    ],
                    "antonyms": [
                        "downslope"
                    ],
                    "common_phrases": [
                        "steep counterslope",
                        "counterslope wall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склон, идущий в направлении, противоположном общему уклону местности, или нависающий склон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обратный скат",
                                "target_lang_sense_clarification": "геология, военное дело"
                            },
                            {
                                "target_lang_word": "контруклон",
                                "target_lang_sense_clarification": "строительство, инженерия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "176b0c2a-62d9-3e5b-86e4-ca066cc06fdf",
                    "sense_definition": "To slope or incline in the opposite side or direction.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The terrain begins to <w>counterslope</w> sharply as you approach the riverbank.",
                            "target_lang_translations": {
                                "ru": "При приближении к берегу реки местность начинает резко <w>уходить в обратный уклон</w>."
                            }
                        },
                        {
                            "text": "The architect decided to <w>counterslope</w> the roof to improve drainage.",
                            "target_lang_translations": {
                                "ru": "Архитектор решил <w>сделать контруклон</w> крыши для улучшения дренажа."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь наклон в противоположную сторону или создавать такой наклон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь обратный уклон",
                                "target_lang_sense_clarification": "о местности"
                            },
                            {
                                "target_lang_word": "делать контруклон",
                                "target_lang_sense_clarification": "в строительстве"
                            }
                        ]
                    }
                }
            ]
        }
    ]

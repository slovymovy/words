    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab55d36b-da0e-3275-abcd-a93b267d0815",
                    "sense_definition": "Element urządzenia elektrycznego lub elektronicznego, taki jak gniazdo lub wtyczka, umożliwiający podłączenie innego urządzenia lub przewodu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "tech_connector",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy laptop nie posiada standardowego <w>złącza</w> USB.",
                            "target_lang_translations": {
                                "ru": "У моего нового ноутбука нет стандартного <w>разъёма</w> USB."
                            }
                        },
                        {
                            "text": "Musisz delikatnie włożyć kabel w <w>złącze</w>, aby go nie uszkodzić.",
                            "target_lang_translations": {
                                "ru": "Нужно аккуратно вставить кабель в <w>разъём</w>, чтобы не повредить его."
                            }
                        }
                    ],
                    "common_phrases": [
                        "złącze USB",
                        "złącze HDMI",
                        "złącze zasilania",
                        "złącze audio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент электрического или электронного устройства, такой как гнездо или штекер, позволяющий подключить другое устройство или провод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъём",
                                "target_lang_sense_clarification": "электрический порт или гнездо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca405ecb-963f-3280-8c2c-96e5e7e3c7e1",
                    "sense_definition": "Miejsce lub sposób technicznego połączenia dwóch lub więcej elementów konstrukcji, maszyny lub materiału.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "structural_joint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>złącze</w> rur jest nieszczelne i wymaga natychmiastowej naprawy.",
                            "target_lang_translations": {
                                "ru": "Это <w>соединение</w> труб негерметично и требует немедленного ремонта."
                            }
                        },
                        {
                            "text": "Inżynier zaprojektował wytrzymałe <w>złącze</w> spawane dla nowej konstrukcji mostu.",
                            "target_lang_translations": {
                                "ru": "Инженер спроектировал прочное сварное <w>соединение</w> для новой конструкции моста."
                            }
                        }
                    ],
                    "synonyms": [
                        "złączenie"
                    ],
                    "common_phrases": [
                        "złącze spawane",
                        "złącze stolarskie",
                        "złącze śrubowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или способ технического соединения двух или более элементов конструкции, машины или материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "место скрепления деталей"
                            },
                            {
                                "target_lang_word": "стык",
                                "target_lang_sense_clarification": "место соприкосновения элементов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

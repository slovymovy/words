    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "efb05c57-80ba-30c5-b09e-0e5ba7a9fbe3",
                    "sense_definition": "Rozpoczynać istnienie czegoś, co łączy ludzi, na przykład rozmowy, znajomości, kontaktu lub współpracy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contact_relations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <nawiązać> kontakt z nowym klientem, aby przedstawić mu ofertę.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <установить> контакт с новым клиентом, чтобы представить ему предложение."
                            }
                        },
                        {
                            "text": "Podczas wakacji łatwo jest <nawiązywać> nowe znajomości z ludźmi z całego świata.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска легко <завязывать> новые знакомства с людьми со всего мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaczynać"
                    ],
                    "antonyms": [
                        "zrywać",
                        "kończyć"
                    ],
                    "common_phrases": [
                        "nawiązywać kontakt",
                        "nawiązywać współpracę",
                        "nawiązywać znajomości",
                        "nawiązywać relacje",
                        "nawiązywać dialog"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать что-то, что связывает людей, например, разговор, знакомство, контакт или сотрудничество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завязывать",
                                "target_lang_sense_clarification": "отношения, знакомство, разговор"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "контакт, связь"
                            },
                            {
                                "target_lang_word": "налаживать",
                                "target_lang_sense_clarification": "сотрудничество, отношения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31a35abf-ba87-36c0-9f82-a1d705731f39",
                    "sense_definition": "Wspominać o czymś w wypowiedzi, odnosić się do czegoś, co jest znane lub było wcześniej poruszane.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor w swojej najnowszej książce często <nawiązuje> do wydarzeń historycznych.",
                            "target_lang_translations": {
                                "ru": "Автор в своей новейшей книге часто <ссылается> на исторические события."
                            }
                        },
                        {
                            "text": "Nie chcę <nawiązywać> do tej przykrej sytuacji, o której wszyscy chcą zapomnieć.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <касаться> этой неприятной ситуации, о которой все хотят забыть."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnosić się",
                        "odwoływać się",
                        "przywoływać"
                    ],
                    "common_phrases": [
                        "nawiązywać do tematu",
                        "nawiązywać do przeszłości",
                        "nawiązywać do wypowiedzi",
                        "nawiązywać do tradycji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упоминать о чем-либо в речи, ссылаться на что-то известное или ранее обсуждавшееся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссылаться",
                                "target_lang_sense_clarification": "на факты, события, слова"
                            },
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "темы, вопроса"
                            },
                            {
                                "target_lang_word": "делать отсылку",
                                "target_lang_sense_clarification": "к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "538b82ef-7f78-3d80-9f88-d992e23b6c9f",
                    "sense_definition": "Przymocowywać coś do czegoś poprzez wiązanie węzłów; łączyć końce czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rybak zaczął <nawiązywać> zerwaną sieć, aby móc rano wypłynąć w morze.",
                            "target_lang_translations": {
                                "ru": "Рыбак начал <связывать> порванную сеть, чтобы утром выйти в море."
                            }
                        },
                        {
                            "text": "Musisz dokładniej <nawiązywać> sznurek na paczce, żeby się nie rozpadła.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательнее <привязывать> веревку на посылке, чтобы она не развалилась."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywiązywać",
                        "łączyć",
                        "przyczepiać"
                    ],
                    "antonyms": [
                        "rozwiązywać"
                    ],
                    "common_phrases": [
                        "nawiązywać węzeł",
                        "nawiązywać linę",
                        "nawiązywać nić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять что-то к чему-то, завязывая узлы; соединять концы чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привязывать",
                                "target_lang_sense_clarification": "веревку, нить к чему-то"
                            },
                            {
                                "target_lang_word": "навязывать",
                                "target_lang_sense_clarification": "узел, часть сети"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "концы"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "4e3198f0-fa17-32ab-93f1-b70ba0b5e8a8",
                    "sense_definition": "The past participle of <w>begin</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The concert has already <w>begun</w> so we must be quiet.",
                            "target_lang_translations": {
                                "ru": "Концерт уже <w>начался</w>, поэтому мы должны вести себя тихо."
                            }
                        },
                        {
                            "text": "They have <w>begun</w> to build a new house down the street.",
                            "target_lang_translations": {
                                "ru": "Они <w>начали</w> строить новый дом в конце улицы."
                            }
                        }
                    ],
                    "synonyms": [

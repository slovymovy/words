{
    "word_family": [
        "hirer",
        "rehire",
        "hireable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7289f3cf-116c-3072-be25-5e8a5213e04a",
                    "sense_definition": "To give someone a job in exchange for payment.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "employment_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company decided to <w>hire</w> three new engineers for the project.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>нанять</w> трех новых инженеров для проекта."
                            }
                        },
                        {
                            "text": "She was <w>hired</w> to manage the sales team last month.",
                            "target_lang_translations": {
                                "ru": "Ее <w>наняли</w> руководить отделом продаж в прошлом месяце."
                            }
                        }
                    ],
                    "synonyms": [
                        "employ"
                    ],
                    "antonyms": [
                        "fire"
                    ],
                    "common_phrases": [
                        "hire new staff",
                        "hire a worker",
                        "hire an assistant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать кого-либо на работу в обмен на оплату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанимать",
                                "target_lang_sense_clarification": "брать на работу (несовершенный вид)"
                            },
                            {
                                "target_lang_word": "нанять",
                                "target_lang_sense_clarification": "взять на работу (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66f7cc93-c880-324f-8565-273969b65e12",
                    "sense_definition": "To pay to use something for a short period of time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "renting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>hired</w> a car to travel around the island during our holiday.",
                            "target_lang_translations": {
                                "ru": "Мы <w>взяли напрокат</w> машину, чтобы путешествовать по острову во время отпуска."
                            }
                        },
                        {
                            "text": "You can <w>hire</w> skis and boots at the resort.",
                            "target_lang_translations": {
                                "ru": "На курорте можно <w>взять напрокат</w> лыжи и ботинки."
                            }
                        }
                    ],
                    "synonyms": [
                        "rent"
                    ],
                    "common_phrases": [
                        "hire a car",
                        "hire a bike",
                        "hire a boat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Платить за временное использование чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брать напрокат",
                                "target_lang_sense_clarification": "брать вещи во временное пользование"
                            },
                            {
                                "target_lang_word": "арендовать",
                                "target_lang_sense_clarification": "брать во временное пользование (помещение, транспорт)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "345aaa3c-5f6f-3bae-8c30-defbb1fbcfbe",
                    "sense_definition": "To allow someone to use your services or property in exchange for payment.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "renting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer <w>hired</w> out his tractor to neighbors during the harvest.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>сдавал</w> свой трактор соседям во время сбора урожая."
                            }
                        },
                        {
                            "text": "He <w>hired</w> himself out as a tour guide for the summer.",
                            "target_lang_translations": {
                                "ru": "Он <w>нанялся</w> гидом на лето."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hire out",
                        "hire yourself out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять свои услуги или имущество в пользование за плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать внаём",
                                "target_lang_sense_clarification": "предоставлять имущество в пользование"
                            },
                            {
                                "target_lang_word": "наниматься",
                                "target_lang_sense_clarification": "предлагать свои услуги за плату"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2dd0f6f1-5046-35f5-823d-3c152095efbc",
                    "sense_definition": "A person who has recently been given a job.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "employment_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager introduced the new <w>hires</w> to the rest of the team.",
                            "target_lang_translations": {
                                "ru": "Менеджер представил <w>новых сотрудников</w> остальной части команды."
                            }
                        },
                        {
                            "text": "All new <w>hires</w> must complete the safety training program.",
                            "target_lang_translations": {
                                "ru": "Все <w>новые сотрудники</w> должны пройти программу обучения технике безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "new hire",
                        "recent hire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого недавно приняли на работу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "новый сотрудник",
                                "target_lang_sense_clarification": "новичок в компании"
                            },
                            {
                                "target_lang_word": "нанятый работник",
                                "target_lang_sense_clarification": "человек, работающий по найму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19d0297d-725b-3d08-b445-0faeb5dbbb10",
                    "sense_definition": "The act of paying to use something temporarily, or the arrangement for this use.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "renting_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boats are available for <w>hire</w> by the hour.",
                            "target_lang_translations": {
                                "ru": "Лодки доступны для <w>проката</w> с почасовой оплатой."
                            }
                        },
                        {
                            "text": "The cost of <w>hire</w> includes insurance and fuel.",
                            "target_lang_translations": {
                                "ru": "Стоимость <w>проката</w> включает страховку и топливо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for hire",
                        "on hire",
                        "car hire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по оплате временного использования чего-либо или договоренность об этом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокат",
                                "target_lang_sense_clarification": "сдача вещей во временное пользование"
                            },
                            {
                                "target_lang_word": "наём",
                                "target_lang_sense_clarification": "взятие в пользование за плату"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a376330f-60d0-3530-83e6-d1d2c6a87466",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Hires</w> signed the document at the bottom of the page.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Хайерс</w> подписал документ внизу страницы."
                            }
                        },
                        {
                            "text": "We met a family named <w>Hires</w> while visiting the town.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью по фамилии <w>Хайерс</w> во время посещения города."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хайерс",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
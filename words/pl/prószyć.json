            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15a56e61-f9a1-3fed-80d4-34a531e159ed",
                    "sense_definition": "Padać rzadko i drobno (zwykle o śniegu).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather_snow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą często <w>prószy</w> śnieg, tworząc piękny, biały krajobraz.",
                            "target_lang_translations": {
                                "ru": "Зимой часто <w>порошит</w> снег, создавая красивый белый пейзаж."
                            }
                        },
                        {
                            "text": "Dziś za oknem tylko lekko <w>prószy</w>, więc nie będzie dużych zasp.",
                            "target_lang_translations": {
                                "ru": "Сегодня за окном лишь слегка <w>порошит</w>, поэтому больших сугробов не будет."
                            }
                        }
                    ],
                    "synonyms": [
                        "padać",
                        "śnieżyć",
                        "popadywać",
                        "sypać"
                    ],
                    "common_phrases": [
                        "śnieg prószy",
                        "lekko prószy",
                        "prószy za oknem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти редко и мелко (обычно о снеге)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порошить",
                                "target_lang_sense_clarification": "о снеге"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "об осадках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "495e0b03-5998-3067-b780-258b145acbdf",
                    "sense_definition": "Sypać czymś drobnym lub sypkim na powierzchnię czegoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "scattering_particles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz zaczął delikatnie <w>prószyć</w> ciasto cukrem pudrem.",
                            "target_lang_translations": {
                                "ru": "Повар начал аккуратно <w>посыпать</w> пирог сахарной пудрой."
                            }
                        },
                        {
                            "text": "Musisz <w>prószyć</w> mąką stolnicę, żeby ciasto się do niej nie przykleiło.",
                            "target_lang_translations": {
                                "ru": "Нужно <w>посыпать</w> доску мукой, чтобы тесто к ней не прилипло."
                            }
                        }
                    ],
                    "synonyms": [
                        "posypywać",
                        "oprószać",
                        "sypać",
                        "obsypywać",
                        "rozsypywać"
                    ],
                    "common_phrases": [
                        "prószyć mąką",
                        "prószyć cukrem",
                        "prószyć solą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сыпать чем-то мелким или сыпучим на поверхность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посыпать",
                                "target_lang_sense_clarification": "покрывать сыпучим веществом"
                            },
                            {
                                "target_lang_word": "присыпать",
                                "target_lang_sense_clarification": "слегка покрывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75f5ec28-1593-3649-b917-b350059a874e",
                    "sense_definition": "Rozpylać substancję, taką jak farba lub lakier, tworząc delikatną warstwę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scattering_particles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artysta lubił <w>prószyć</w> farbą na płótno, aby uzyskać efekt mgły.",
                            "target_lang_translations": {
                                "ru": "Художник любил <w>распылять</w> краску на холст, чтобы добиться эффекта тумана."
                            }
                        },
                        {
                            "text": "Specjalna maszyna w fabryce zaczęła <w>prószyć</w> lakierem na metalowe elementy.",
                            "target_lang_translations": {
                                "ru": "Специальная машина на заводе начала <w>напылять</w> лак на металлические элементы."
                            }
                        }
                    ],
                    "synonyms": [
                        "opylać",
                        "pokrywać"
                    ],
                    "common_phrases": [
                        "prószyć farbą",
                        "prószyć lakierem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распылять вещество, такое как краска или лак, создавая тонкий слой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распылять",
                                "target_lang_sense_clarification": "разбрызгивать мельчайшими частицами"
                            },
                            {
                                "target_lang_word": "напылять",
                                "target_lang_sense_clarification": "наносить слой вещества"
                            }
                        ]
                    }
                }
            ]
        }
    ]

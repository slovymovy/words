                    "sense_id": "c343e8b4-fb41-3013-bfec-d73b68b64264",
                    "sense_definition": "zaimek oznaczający grupę osób, do której należy osoba mówiąca; pierwsza osoba liczby mnogiej",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_pronoun_we",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>My</w> mieszkamy w Polsce, a oni we Francji.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> живём в Польше, а они — во Франции."
                            }
                        },
                        {
                            "text": "To właśnie <w>my</w> zapłaciliśmy za całą waszą podróż.",
                            "target_lang_translations": {
                                "ru": "Именно <w>мы</w> заплатили за всё ваше путешествие."
                            }
                        },
                        {
                            "text": "Teraz <w>my</w> musimy podjąć tę trudną decyzję.",
                            "target_lang_translations": {
                                "ru": "Теперь <w>мы</w> должны принять это трудное решение."
                            }
                        }
                    ],
                    "antonyms": [
                        "oni"
                    ],
                    "common_phrases": [
                        "my wszyscy",
                        "a my",
                        "tylko my",
                        "my Polacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "местоимение, обозначающее группу лиц, к которой принадлежит говорящий; первое лицо множественного числа"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "местоимение"
                            }
                        ]
                    }
                }
            ]
        },

                },
                {
                    "sense_id": "cf170e5a-7f6e-39c4-ac09-437d543d251e",
                    "sense_definition": "Środek, metoda lub dokument (często prawny lub finansowy) służący do osiągnięcia określonego celu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podatki są ważnym <w>instrumentem</w> polityki gospodarczej państwa.",
                            "target_lang_translations": {
                                "ru": "Налоги являются важным <w>инструментом</w> экономической политики государства."
                            }
                        },
                        {
                            "text": "Bank oferuje firmom różne <w>instrumenty</w> finansowe wspierające rozwój.",
                            "target_lang_translations": {
                                "ru": "Банк предлагает компаниям различные финансовые <w>инструменты</w> для поддержки развития."
                            }
                        }
                    ],
                    "common_phrases": [

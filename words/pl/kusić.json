            "senses": [
                {
                    "sense_id": "e7746548-e3c8-3875-baf0-85ee01c51ee4",
                    "sense_definition": "Wzbudzać w kimś silną chęć zrobienia czegoś (często czegoś zakazanego, ryzykownego lub niewłaściwego) lub posiadania czegoś; być pociągającym i atrakcyjnym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapach świeżego ciasta <w>kusi</w> mnie, żeby zjeść jeszcze jeden kawałek.",
                            "target_lang_translations": {
                                "ru": "Запах свежего пирога <w>соблазняет</w> меня съесть ещё один кусочек."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>kusić</w> losu, jeżdżąc samochodem bez zapiętych pasów.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>искушать</w> судьбу, ездя на машине с непристёгнутыми ремнями."
                            }
                        }
                    ],
                    "synonyms": [
                        "wabić",
                        "nęcić",
                        "namawiać",
                        "zachęcać"
                    ],
                    "antonyms": [
                        "zniechęcać"
                    ],
                    "common_phrases": [
                        "kusić los",
                        "licho kusi",
                        "diabeł kusi",
                        "kusić wyglądem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-то сильное желание сделать что-то (часто запретное, рискованное или неуместное) или обладать чем-то; быть притягательным и привлекательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искушать",
                                "target_lang_sense_clarification": "вызывать желание сделать что-то запретное; подвергать испытанию (судьбу)"
                            },
                            {
                                "target_lang_word": "соблазнять",
                                "target_lang_sense_clarification": "прельщать, склонять к чему-либо заманчивому"
                            },
                            {
                                "target_lang_word": "манить",
                                "target_lang_sense_clarification": "притягивать к себе, быть привлекательным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a081341-7a9f-3df7-b120-4fca1730657e",
                    "sense_definition": "Straszyć kogoś, wywoływać lęk lub niepokój (znaczenie regionalne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary dom na wzgórzu podobno <w>kusi</w> po nocach.",
                            "target_lang_translations": {
                                "ru": "Говорят, что старый дом на холме по ночам <w>пугает</w>."
                            }
                        },
                        {
                            "text": "Dziadek lubił <w>kusić</w> dzieci opowieściami o duchach.",
                            "target_lang_translations": {
                                "ru": "Дедушка любил <w>пугать</w> детей историями о привидениях."
                            }
                        }
                    ],

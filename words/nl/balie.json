                {
                    "sense_id": "442930bb-9121-363d-895f-d686302e178e",
                    "sense_definition": "Een langgerekte tafel of bureau waarachter medewerkers klanten ontvangen of bedienen, bijvoorbeeld in een winkel, hotel of kantoor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "counter_desk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De receptionist staat de hele dag achter de <w>balie</w>.",
                            "target_lang_translations": {
                                "en": "The receptionist stands behind the <w>counter</w> all day."
                            }
                        },
                        {
                            "text": "Je kunt je melden bij de <w>balie</w> als je aankomt.",
                            "target_lang_translations": {
                                "en": "You can report to the <w>desk</w> when you arrive."
                            }
                        }
                    ],
                    "synonyms": [
                        "toonbank"
                    ],
                    "common_phrases": [
                        "achter de balie",
                        "aan de balie",
                        "bij de balie"
                    ],
                    "target_lang_definitions": {
                        "en": "A long table or desk behind which employees receive or serve customers, for example in a shop, hotel, or office."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "counter",
                                "target_lang_sense_clarification": "service point in a shop or business"
                            },
                            {
                                "target_lang_word": "desk",
                                "target_lang_sense_clarification": "reception or information desk"
                            },
                            {
                                "target_lang_word": "reception",
                                "target_lang_sense_clarification": "front desk area"
                            }
                        ]

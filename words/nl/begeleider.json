        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1a2f61f-2474-30b4-9a0a-518f6d682fc4",
                    "sense_definition": "Iemand die met een ander meegaat om te helpen, te beschermen of gezelschap te houden",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen mogen het museum alleen bezoeken met een volwassen <w>begeleider</w>.",
                            "target_lang_translations": {
                                "en": "The children are only allowed to visit the museum with an adult <w>chaperone</w>.",
                                "ru": "Дети могут посещать музей только со взрослым <w>сопровождающим</w>."
                            }
                        },
                        {
                            "text": "Tijdens de excursie fungeerde hij als <w>begeleider</w> voor de groep toeristen.",
                            "target_lang_translations": {
                                "en": "During the excursion, he acted as a <w>guide</w> for the group of tourists.",
                                "ru": "Во время экскурсии он выполнял роль <w>сопровождающего</w> для группы туристов."
                            }
                        }
                    ],
                    "synonyms": [
                        "metgezel",
                        "chaperonne"
                    ],
                    "common_phrases": [
                        "vaste begeleider",
                        "volwassen begeleider",
                        "medisch begeleider"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who accompanies another to help, protect, or keep company",
                        "ru": "Тот, кто сопровождает другого человека, чтобы помочь, защитить или составить компанию"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chaperone",
                                "target_lang_sense_clarification": "supervision, especially for children"
                            },
                            {
                                "target_lang_word": "escort",
                                "target_lang_sense_clarification": "protection or formal accompaniment"
                            },
                            {
                                "target_lang_word": "companion",
                                "target_lang_sense_clarification": "general accompaniment"
                            },
                            {
                                "target_lang_word": "guide",
                                "target_lang_sense_clarification": "leading or showing the way"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сопровождающий",
                                "target_lang_sense_clarification": "человек, который сопровождает кого-либо (например, детей или группу)"
                            },
                            {
                                "target_lang_word": "спутник",
                                "target_lang_sense_clarification": "компаньон в пути"
                            },
                            {
                                "target_lang_word": "провожатый",
                                "target_lang_sense_clarification": "тот, кто указывает путь или охраняет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "945bfbd8-6154-3120-b77c-b65f791ad1da",
                    "sense_definition": "Iemand die een ander professioneel ondersteunt met advies en hulp bij een leerproces of werk",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>begeleider</w> op de universiteit gaf nuttig advies over mijn scriptie.",
                            "target_lang_translations": {
                                "en": "My <w>supervisor</w> at the university gave useful advice on my thesis.",
                                "ru": "Мой <w>научный руководитель</w> в университете дал полезный совет по поводу моей дипломной работы."
                            }
                        },
                        {
                            "text": "Als ambulant <w>begeleider</w> helpt hij jongeren om zelfstandig te wonen.",
                            "target_lang_translations": {
                                "en": "As an ambulatory <w>support worker</w>, he helps young people to live independently.",
                                "ru": "Как приходящий <w>куратор</w>, он помогает молодым людям жить самостоятельно."
                            }
                        }
                    ],
                    "synonyms": [
                        "coach",
                        "mentor",
                        "raadsman"
                    ],
                    "common_phrases": [

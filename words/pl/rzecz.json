                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Rzecz</w> mi prawdę, czy to ty zrobiłeś?",
                            "target_lang_translations": {
                                "ru": "<w>Скажи</w> мне правду, это ты сделал?"
                            }
                        },
                        {
                            "text": "Nie milcz, lecz <w>rzecz</w> słowo.",
                            "target_lang_translations": {
                                "ru": "Не молчи, а <w>молви</w> слово."
                            }

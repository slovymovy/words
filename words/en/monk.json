    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a87a4d4-2741-3d1b-8e91-1178520e5fda",
                    "sense_definition": "A male member of a religious community who lives in a monastery and typically follows vows of poverty, chastity, and obedience.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>monk</w> spent his entire morning in silent prayer.",
                            "target_lang_translations": {
                                "ru": "<w>Мона́х</w> провёл всё утро в молчаливой молитве."
                            }
                        },
                        {
                            "text": "He decided to leave the city and live as a <w>monk</w> in the mountains.",
                            "target_lang_translations": {
                                "ru": "Он решил уехать из города и жить в горах как <w>мона́х</w>."
                            }
                        },
                        {
                            "text": "A Buddhist <w>monk</w> offered us tea when we visited the temple.",
                            "target_lang_translations": {
                                "ru": "Буддийский <w>мона́х</w> предложил нам чаю, когда мы посетили храм."
                            }
                        }
                    ],
                    "synonyms": [
                        "friar",
                        "brother"
                    ],
                    "antonyms": [
                        "nun"
                    ],
                    "common_phrases": [
                        "Buddhist monk",
                        "Shaolin monk",
                        "Benedictine monk",
                        "Tibetan monk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина — член религиозной общины, живущий в монастыре и давший обеты бедности, целомудрия и послушания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мона́х",
                                "target_lang_sense_clarification": "член религиозной общины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "973e8745-a679-39f8-a660-ca5eebd079b4",
                    "sense_definition": "A person who withdraws from society and lives a solitary life; a loner.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "solitary_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the scandal, he became a <w>monk</w> of sorts, rarely leaving his apartment.",
                            "target_lang_translations": {
                                "ru": "После скандала он стал своего рода <w>затво́рником</w>, редко покидая свою квартиру."
                            }
                        },
                        {
                            "text": "She lives like a <w>monk</w>, avoiding parties and social gatherings to focus on her art.",
                            "target_lang_translations": {
                                "ru": "Она живёт как <w>мона́х</w>, избегая вечеринок и светских собраний, чтобы сосредоточиться на своём искусстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "hermit",
                        "recluse",
                        "loner",
                        "solitary"
                    ],
                    "antonyms": [
                        "socialite",
                        "extrovert"
                    ],
                    "common_phrases": [
                        "live like a monk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "figurative usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который сторонится общества и ведёт уединённый образ жизни; нелюдим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отше́льник",
                                "target_lang_sense_clarification": "человек, живущий в уединении"
                            },
                            {
                                "target_lang_word": "затво́рник",
                                "target_lang_sense_clarification": "человек, избегающий общения"
                            },
                            {
                                "target_lang_word": "мона́х",
                                "target_lang_sense_clarification": "перен. аскет, нелюдим"
                            }
                        ]
                    }
                },

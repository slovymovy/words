    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f55fe73b-6c35-3b60-a8d8-3598fe3a6734",
                    "sense_definition": "Vervoeging van <w>wachten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd, en gebiedende wijs",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wacht</w> al meer dan tien minuten op de bus.",
                            "target_lang_translations": {
                                "ru": "Я <w>жду</w> автобус уже больше десяти минут."
                            }
                        },
                        {
                            "text": "<w>Wacht</w> even op mij, ik ben zo klaar!",
                            "target_lang_translations": {
                                "ru": "<w>Подожди</w> меня немного, я скоро буду готов!"
                            }
                        },
                        {
                            "text": "Hij <w>wacht</w> geduldig tot zijn naam wordt omgeroepen.",
                            "target_lang_translations": {
                                "ru": "Он терпеливо <w>ждёт</w>, пока назовут его имя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wacht even",
                        "wacht eens"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола <w>wachten</w>: 1-е, 2-е и 3-е лицо единственного числа настоящего времени, а также повелительное наклонение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "находиться где-л. или в каком-л. состоянии в надежде на появление кого-л. или чего-л."
                            },
                            {
                                "target_lang_word": "подождать",
                                "target_lang_sense_clarification": "провести некоторое время в ожидании (часто используется в повелительном наклонении)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cdfb4838-396c-32d8-a3a0-7e6cad4288d9",
                    "sense_definition": "De taak om iets of iemand te bewaken, of de periode waarin men dit doet",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "guarding_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat hield de hele nacht de <w>wacht</w> bij de poort.",
                            "target_lang_translations": {
                                "ru": "Солдат всю ночь стоял в <w>карауле</w> у ворот."
                            }
                        },
                        {
                            "text": "Tijdens zijn <w>wacht</w> zag hij niets verdachts gebeuren.",
                            "target_lang_translations": {
                                "ru": "Во время его <w>вахты</w> он не заметил ничего подозрительного."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de wacht houden",
                        "op wacht staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязанность охранять что-либо или кого-либо, или период времени, в течение которого это происходит"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "караул",
                                "target_lang_sense_clarification": "вооруженная охрана или обязанность по охране"
                            },
                            {
                                "target_lang_word": "вахта",
                                "target_lang_sense_clarification": "дежурство, смена (особенно на корабле или предприятии)"
                            },
                            {
                                "target_lang_word": "стража",
                                "target_lang_sense_clarification": "действие по глаголу стеречь; охрана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95f1d944-d4fc-3932-830f-e53794a5cb57",
                    "sense_definition": "Iemand die de taak heeft om een gebouw, persoon of object te bewaken",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "guarding_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>wacht</w> vroeg ons om onze identiteitskaarten te laten zien.",
                            "target_lang_translations": {
                                "ru": "<w>Охранник</w> попросил нас предъявить удостоверения личности."
                            }
                        },
                        {
                            "text": "We liepen zachtjes langs de slapende <w>wacht</w>.",
                            "target_lang_translations": {
                                "ru": "Мы тихо прошли мимо спящего <w>часового</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wachter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, в чьи обязанности входит охрана здания, лица или объекта"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часовой",
                                "target_lang_sense_clarification": "вооруженный караульный на посту"
                            },
                            {
                                "target_lang_word": "охранник",
                                "target_lang_sense_clarification": "человек, занимающийся охраной"
                            },
                            {
                                "target_lang_word": "сторож",
                                "target_lang_sense_clarification": "человек, охраняющий имущество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1531a080-2f84-36d4-a2b0-ae2b6fc7e372",
                    "sense_definition": "Een groep mensen die samen verantwoordelijk is voor de bewaking",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "guarding_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe <w>wacht</w> kwam het plein opmarcheren om de dienst over te nemen.",
                            "target_lang_translations": {
                                "ru": "Новый <w>караул</w> вышел на площадь, чтобы заступить на дежурство."
                            }
                        },
                        {
                            "text": "De nachtelijke <w>wacht</w> werd volledig overrompeld door de aanval.",
                            "target_lang_translations": {
                                "ru": "Ночная <w>стража</w> была полностью застигнута врасплох нападением."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, совместно отвечающих за охрану"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стража",
                                "target_lang_sense_clarification": "группа вооруженных людей, несущих охрану"
                            },
                            {
                                "target_lang_word": "караул",
                                "target_lang_sense_clarification": "вооруженное подразделение, назначенное для охраны"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
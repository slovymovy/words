    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62299365-db62-30d8-a508-39eeb4f01455",
                    "sense_definition": "Zdrobnienie od słowa <w>świnia</w>; młode lub małe zwierzę hodowlane.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiejskim podwórku biegała mała, różowa <w>świnka</w>.",
                            "target_lang_translations": {
                                "ru": "На деревенском дворе бегала маленькая розовая <w>свинка</w>."
                            }
                        },
                        {
                            "text": "Dziecko narysowało w zeszycie wesołą <w>świnkę</w>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок нарисовал в тетради весёлую <w>свинку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "świneczka",
                        "prosiaczek",
                        "prosię",
                        "prosiak",
                        "warchlak"
                    ],
                    "common_phrases": [
                        "różowa świnka",
                        "mała świnka",
                        "brudny jak świnka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zdrobnienie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «свинья»; молодое или маленькое животное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинка",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            },
                            {
                                "target_lang_word": "поросёнок",
                                "target_lang_sense_clarification": "детеныш свиньи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf3e7446-2ea0-3f52-abce-584a9b04d0ed",
                    "sense_definition": "Potoczne określenie świnki morskiej, małego gryzonia często hodowanego w domu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <w>świnka</w> uwielbia jeść świeżą marchewkę i sałatę.",
                            "target_lang_translations": {
                                "ru": "Моя <w>свинка</w> обожает есть свежую морковку и салат."
                            }
                        },
                        {
                            "text": "Muszę kupić nową klatkę dla mojej <w>świnki</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую клетку для моей <w>свинки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świnka morska",
                        "klatka dla świnki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "skrót od świnka morska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название морской свинки, маленького грызуна, которого часто держат дома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морская свинка",
                                "target_lang_sense_clarification": "полное название животного"
                            },
                            {
                                "target_lang_word": "свинка",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "001f3dd5-19e3-35f3-917b-7ec09dd431ee",
                    "sense_definition": "Choroba zakaźna, nagminne zapalenie przyusznic, objawiające się bolesnym obrzękiem szyi.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dzieciństwie zaraziłem się <w>świnką</w> w przedszkolu.",
                            "target_lang_translations": {
                                "ru": "В детстве я заразился <w>свинкой</w> в детском саду."
                            }
                        },
                        {
                            "text": "Lekarz potwierdził, że to <w>świnka</w> i zalecił odpoczynek w domu.",
                            "target_lang_translations": {
                                "ru": "Врач подтвердил, что это <w>свинка</w>, и порекомендовал отдых дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zachorować na świnkę",
                        "szczepionka na świnkę",
                        "objawy świnki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инфекционное заболевание (эпидемический паротит), проявляющееся болезненным отёком шеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинка",
                                "target_lang_sense_clarification": "разговорное название болезни"
                            },
                            {
                                "target_lang_word": "паротит",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbea5436-6753-3c5e-a4d0-7d3a0701b63f",
                    "sense_definition": "Skarbonka, zazwyczaj ceramiczna, w kształcie małej świni.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam już pełną <w>świnkę</w>, więc jutro kupię sobie nową piłkę.",
                            "target_lang_translations": {
                                "ru": "У меня уже полная <w>свинка</w>, так что завтра я куплю себе новый мяч."
                            }
                        },
                        {
                            "text": "Każdego dnia wrzucam drobne monety do mojej <w>świnki</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый день я бросаю мелочь в мою <w>свинку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "świnka-skarbonka"
                    ],
                    "common_phrases": [
                        "rozbić świnkę",
                        "pełna świnka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копилка, обычно керамическая, в форме маленькой свиньи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинка-копилка",
                                "target_lang_sense_clarification": "копилка в форме свиньи"
                            },
                            {
                                "target_lang_word": "копилка",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "295aa61a-963f-328f-a399-cdcd810709c3",
                    "sense_definition": "Ryba rzeczna z rodziny karpiowatych (Chondrostoma nasus).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tarło <w>świnki</w> w rzekach trwa zazwyczaj od kwietnia do maja.",
                            "target_lang_translations": {
                                "ru": "Нерест <w>подуста</w> в реках обычно длится с апреля по май."
                            }
                        },
                        {
                            "text": "Wędkarz złowił dużą <w>świnkę</w> na specjalną przynętę.",
                            "target_lang_translations": {
                                "ru": "Рыбак поймал крупного <w>подуста</w> на специальную наживку."
                            }
                        }
                    ],
                    "synonyms": [
                        "świnka pospolita"
                    ],
                    "common_phrases": [
                        "łowienie świnek",
                        "populacja świnki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Речная рыба из семейства карповых (обыкновенный подуст)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подуст",
                                "target_lang_sense_clarification": "вид рыбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aeb96de3-3fb1-3930-a0f2-f37a1783d33b",
                    "sense_definition": "Dawna, złota carska moneta pięciorublowa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek wszystkie oszczędności lokował w <w>świnkach</w> chowanych na czarną godzinę.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка все сбережения вкладывал в <w>свинки</w>, припрятанные на чёрный день."
                            }
                        },
                        {
                            "text": "Kolekcjoner zapłacił wysoką cenę za rzadką złotą <w>świnkę</w>.",
                            "target_lang_translations": {
                                "ru": "Коллекционер заплатил высокую цену за редкую золотую <w>свинку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "złota pięciorublówka"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная золотая царская пятирублёвая монета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золотая пятирублёвка",
                                "target_lang_sense_clarification": "официальное название"
                            },
                            {
                                "target_lang_word": "свинка",
                                "target_lang_sense_clarification": "исторический жаргонизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bfa1c91-0ce8-3d2e-a37d-510f0c1b7326",
                    "sense_definition": "Mała kulka używana w grze w bule (petance), do której rzuca się większymi kulami.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "game_element",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W następnej rozgrywce rzucę <w>świnkę</w> trochę bliżej nas.",
                            "target_lang_translations": {
                                "ru": "В следующем раунде я брошу <w>свинку</w> немного ближе к нам."
                            }
                        },
                        {
                            "text": "Zawodnik umieścił swoją kulę tuż przy samej <w>śwince</w>.",
                            "target_lang_translations": {
                                "ru": "Игрок разместил свой шар прямо у самой <w>свинки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzucić świnkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький шарик, используемый в игре петанк, к которому бросают большие шары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошонет",
                                "target_lang_sense_clarification": "термин в петанке"
                            },
                            {
                                "target_lang_word": "свинка",
                                "target_lang_sense_clarification": "калька с французского"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "feed70ff-1325-3193-bc39-e353945e8622",
                    "sense_definition": "Prosta gra w karty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorami dla zabicia czasu graliśmy w <w>świnkę</w>.",
                            "target_lang_translations": {
                                "ru": "По вечерам, чтобы убить время, мы играли в <w>свинку</w>."
                            }
                        },
                        {
                            "text": "Zasady gry w <w>świnkę</w> są bardzo łatwe do opanowania.",
                            "target_lang_translations": {
                                "ru": "Правила игры в <w>свинку</w> очень легко освоить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać w świnkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простая карточная игра."
                    },
                    "translations": {
                        "ru": [

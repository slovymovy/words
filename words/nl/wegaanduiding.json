            "senses": [
                {
                    "sense_id": "1112dbd6-96b5-32b2-bae9-62003582d130",
                    "sense_definition": "Een bord of teken dat de richting van een te volgen weg of route aangeeft.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "traffic_signs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de onduidelijke <w>wegaanduiding</w> wisten we niet welke kant we op moesten.",
                            "target_lang_translations": {
                                "en": "Due to the unclear <w>signage</w>, we didn't know which way to go.",
                                "ru": "Из-за неясного <w>дорожного указателя</w> мы не знали, в какую сторону ехать."
                            }
                        },
                        {
                            "text": "De gemeente heeft besloten om de <w>wegaanduiding</w> in het centrum te verbeteren.",
                            "target_lang_translations": {
                                "en": "The municipality has decided to improve the <w>signage</w> in the city center.",
                                "ru": "Муниципалитет решил улучшить <w>дорожные указатели</w> в центре города."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegwijzer",
                        "bewegwijzering"
                    ],
                    "common_phrases": [
                        "duidelijke wegaanduiding",
                        "nieuwe wegaanduiding"
                    ],
                    "target_lang_definitions": {
                        "en": "A sign or mark indicating the direction of a road or route to follow.",
                        "ru": "Знак или символ, указывающий направление дороги или маршрута."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "signage",
                                "target_lang_sense_clarification": "general system of signs"
                            },
                            {

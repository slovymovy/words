    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c9aee4c-4ace-3109-b4a8-41b9cf2914c3",
                    "sense_definition": "An alcoholic drink made from fermented grape juice.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We ordered a bottle of red <w>wine</w> to go with our dinner.",
                            "target_lang_translations": {
                                "ru": "Мы заказали бутылку красного <w>вина</w> к ужину."
                            }
                        },
                        {
                            "text": "France is famous for producing excellent <w>wine</w>.",
                            "target_lang_translations": {
                                "ru": "Франция славится производством превосходного <w>вина</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vino"
                    ],
                    "common_phrases": [
                        "red wine",
                        "white wine",
                        "sparkling wine",
                        "dry wine",
                        "glass of wine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Алкогольный напиток, приготовленный из перебродившего виноградного сока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вино",
                                "target_lang_sense_clarification": "напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1bf47b7-d5d5-30da-bc13-d19a05b3fcf1",
                    "sense_definition": "A specific serving or glass of wine.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The waiter brought two <w>wines</w> to our table immediately.",
                            "target_lang_translations": {
                                "ru": "Официант сразу принес два <w>вина</w> к нашему столику."
                            }
                        },
                        {
                            "text": "I will just have a house <w>wine</w>, please.",
                            "target_lang_translations": {
                                "ru": "Я буду просто домашнее <w>вино</w>, пожалуйста."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Порция или бокал вина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вино",
                                "target_lang_sense_clarification": "порция"
                            },
                            {

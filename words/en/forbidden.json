    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "48af50ee-597c-30e7-bf3b-b4273dfeb08f",
                    "sense_definition": "Not allowed; specifically disallowed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smoking is strictly <w>forbidden</w> inside the hospital building.",
                            "target_lang_translations": {
                                "ru": "Курение внутри здания больницы строго <w>запрещено</w>."
                            }
                        },
                        {
                            "text": "They knew that entering the old forest was <w>forbidden</w> by the village elders.",
                            "target_lang_translations": {
                                "ru": "Они знали, что вход в старый лес был <w>запрещён</w> старейшинами деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "prohibited",
                        "verboten"
                    ],
                    "antonyms": [
                        "allowed",
                        "permitted"
                    ],
                    "common_phrases": [
                        "strictly forbidden",
                        "forbidden fruit",
                        "forbidden love",
                        "forbidden city"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не разрешённый; находящийся под запретом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запрещённый",
                                "target_lang_sense_clarification": "официально не разрешённый законом или правилами"
                            },
                            {
                                "target_lang_word": "запретный",
                                "target_lang_sense_clarification": "недозволенный, табуированный (часто о желаниях или темах)"
                            },
                            {
                                "target_lang_word": "недозволенный",
                                "target_lang_sense_clarification": "непозволительный, предосудительный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8cb92e37-23ef-3032-9a3f-e7cd34b25e95",
                    "sense_definition": "past participle of <w>forbid</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents have <w>forbidden</w> me from going out tonight.",
                            "target_lang_translations": {
                                "ru": "Родители <w>запретили</w> мне выходить из дома сегодня вечером."
                            }
                        },
                        {
                            "text": "He was <w>forbidden</w> to leave the country during the investigation.",
                            "target_lang_translations": {
                                "ru": "Ему было <w>запрещено</w> покидать страну во время следствия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {

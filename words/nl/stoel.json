                {
                    "sense_id": "3497f9db-e5ff-3f86-9d11-fef917d7f201",
                    "sense_definition": "Een onderstel of constructie waarop iets zwaars rust, zoals bij een dak of machine.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware klok hangt stevig in de houten <w>stoel</w>.",
                            "target_lang_translations": {
                                "en": "The heavy clock hangs firmly in the wooden <w>frame</w>.",
                                "ru": "Тяжелый колокол прочно висит на деревянной <w>опоре</w>."
                            }
                        },
                        {
                            "text": "De timmerman controleerde of de <w>stoel</w> van het dak nog stabiel was.",
                            "target_lang_translations": {
                                "en": "The carpenter checked if the roof <w>truss</w> was still stable.",
                                "ru": "Плотник проверил, устойчив ли еще <w>каркас</w> крыши."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dakstoel",
                        "klokkenstoel"
                    ],
                    "target_lang_definitions": {
                        "en": "A support or structure on which something heavy rests, such as in a roof or machine.",
                        "ru": "Основание или конструкция, на которой покоится что-то тяжелое, например, крыша или механизм."

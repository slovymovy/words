                    ],
                    "common_phrases": [
                        "literary device",
                        "literary style",
                        "literary language"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойственный литературе, а не повседневной речи; формальный, художественный или поэтический по стилю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "книжный",
                                "target_lang_sense_clarification": "о стиле: свойственный письменной речи, не разговорный"
                            },
                            {
                                "target_lang_word": "литературный",
                                "target_lang_sense_clarification": "о языке: нормированный, художественный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38a8e794-0694-3ee1-8e14-d9d55227185c",
                    "sense_definition": "Knowledgeable about literature or fond of reading and writing.",

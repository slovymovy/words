                    }
                },
                {
                    "sense_id": "8bedf827-bfce-3f9e-af37-b89c0038113e",
                    "sense_definition": "W sposób niewidoczny, nieprzyciągający uwagi lub nierzucający się w oczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

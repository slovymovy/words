            "senses": [
                {
                    "sense_id": "a3e0a163-3d24-31d7-833b-2e7c0acacc9e",
                    "sense_definition": "Dźwięk naśladujący śmiech; w pisowni często powtarzany dla oddania długości śmiechu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "laughter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ha</w>, <w>ha</w>, <w>ha</w>, to był naprawdę zabawny dowcip!",
                            "target_lang_translations": {
                                "ru": "<w>Ха</w>-<w>ха</w>-<w>ха</w>, это была действительно смешная шутка!"
                            }
                        },
                        {
                            "text": "Z pokoju obok dobiegło nas głośne <w>ha</w>, <w>ha</w>.",
                            "target_lang_translations": {
                                "ru": "Из соседней комнаты до нас донеслось громкое <w>ха</w>-<w>ха</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cha"
                    ],
                    "common_phrases": [
                        "ha ha ha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, имитирующий смех; на письме часто повторяется для передачи продолжительности смеха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ха",
                                "target_lang_sense_clarification": "звук смеха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e60a55-69b0-37f3-adf1-f664af375a63",
                    "sense_definition": "Okrzyk wyrażający triumf, nagłe odkrycie czegoś, zaskoczenie lub satysfakcję z posiadania racji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ha</w>! Wiedziałem, że w końcu cię tu znajdę.",
                            "target_lang_translations": {
                                "ru": "<w>Ха</w>! Я знал, что в конце концов найду тебя здесь."
                            }
                        },
                        {
                            "text": "<w>Ha</w>! A nie mówiłem, że mam rację?",
                            "target_lang_translations": {
                                "ru": "<w>Ха</w>! Разве я не говорил, что я прав?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ha! mam cię",
                        "ha! wiedziałem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, выражающий триумф, внезапное открытие чего-либо, удивление или удовлетворение от собственной правоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ха",
                                "target_lang_sense_clarification": "возглас триумфа или удивления"
                            },
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "возглас догадки или обнаружения"
                            }
                        ]
                    }
                }
            ]
        },
        {

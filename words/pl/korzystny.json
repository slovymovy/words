            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a15ea44f-d5d2-3494-861c-adda67c89cc0",
                    "sense_definition": "Taki, który ma niską cenę lub oferuje dobre warunki finansowe, pozwalając zaoszczędzić pieniądze.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "financial_benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Udało mi się kupić ten komputer w bardzo <w>korzystnej</w> cenie.",
                            "target_lang_translations": {
                                "ru": "Мне удалось купить этот компьютер по очень <w>выгодной</w> цене."
                            }
                        },
                        {
                            "text": "Sklep oferuje <w>korzystne</w> warunki zakupu sprzętu na raty.",
                            "target_lang_translations": {
                                "ru": "Магазин предлагает <w>выгодные</w> условия покупки техники в рассрочку."
                            }
                        }
                    ],
                    "synonyms": [
                        "atrakcyjny",
                        "tani",
                        "opłacalny"
                    ],
                    "antonyms": [
                        "niekorzystny"
                    ],
                    "common_phrases": [
                        "w korzystnej cenie",
                        "korzystna oferta",
                        "korzystne warunki",
                        "korzystny kurs walut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дешёвый или предлагающий хорошие финансовые условия, позволяющий сэкономить деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгодный",
                                "target_lang_sense_clarification": "финансово оправданный, прибыльный"
                            },
                            {
                                "target_lang_word": "привлекательный",
                                "target_lang_sense_clarification": "о цене или предложении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22314507-4e42-38bf-b18d-648e5e0e1940",
                    "sense_definition": "Przynoszący zysk, pożytek lub pozytywne rezultaty; taki, który daje dobre efekty.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularne uprawianie sportu ma <w>korzystny</w> wpływ na nasze zdrowie.",
                            "target_lang_translations": {
                                "ru": "Регулярные занятия спортом оказывают <w>благотворное</w> влияние на наше здоровье."
                            }
                        },
                        {
                            "text": "Podpisaliśmy umowę, która będzie <w>korzystna</w> dla obu stron.",
                            "target_lang_translations": {
                                "ru": "Мы подписали договор, который будет <w>выгодным</w> для обеих сторон."
                            }
                        }
                    ],
                    "synonyms": [
                        "pożyteczny",
                        "owocny",
                        "zyskowny",
                        "dobroczynny",
                        "zbawienny"
                    ],
                    "antonyms": [
                        "niekorzystny",
                        "szkodliwy"
                    ],
                    "common_phrases": [
                        "korzystny wpływ",
                        "korzystna zmiana",
                        "korzystny bilans",
                        "korzystne rozwiązanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приносящий пользу, выгоду или положительные результаты; дающий хороший эффект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полезный",
                                "target_lang_sense_clarification": "приносящий пользу"
                            },
                            {
                                "target_lang_word": "благотворный",
                                "target_lang_sense_clarification": "о положительном влиянии (напр. на здоровье)"
                            },
                            {
                                "target_lang_word": "выгодный",
                                "target_lang_sense_clarification": "приносящий преимущество обеим сторонам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8130533a-f22e-35b8-89e4-fd6e4c67f486",
                    "sense_definition": "Sprzyjający czemuś, ułatwiający osiągnięcie celu lub rozwój sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekamy na bardziej <w>korzystne</w> warunki pogodowe, aby wypłynąć w morze.",
                            "target_lang_translations": {
                                "ru": "Мы ждем более <w>благоприятных</w> погодных условий, чтобы выйти в море."
                            }
                        },
                        {
                            "text": "To nie jest <w>korzystny</w> moment na podejmowanie tak ryzykownych decyzji.",
                            "target_lang_translations": {
                                "ru": "Это не самый <w>удачный</w> момент для принятия столь рискованных решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprzyjający",
                        "dogodny",
                        "odpowiedni",
                        "pomyślny"
                    ],
                    "antonyms": [
                        "niekorzystny",
                        "nieprzychylny"
                    ],
                    "common_phrases": [
                        "korzystne warunki",
                        "korzystny moment",
                        "korzystna sytuacja",
                        "korzystny wiatr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способствующий чему-либо, облегчающий достижение цели или развитие ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благоприятный",
                                "target_lang_sense_clarification": "об условиях, погоде или обстоятельствах"
                            },
                            {
                                "target_lang_word": "удачный",
                                "target_lang_sense_clarification": "о моменте или времени"
                            },
                            {
                                "target_lang_word": "подходящий",
                                "target_lang_sense_clarification": "соответствующий ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8705549-118d-3a7c-91e8-3cdae97c234c",
                    "sense_definition": "Przedstawiający kogoś lub coś w dobrym świetle, pełen uznania; także sprawiający, że ktoś wygląda lepiej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Recenzja spektaklu była bardzo <w>korzystna</w> i zachęciła widzów do przyjścia.",
                            "target_lang_translations": {
                                "ru": "Рецензия на спектакль была очень <w>положительной</w>, что побудило зрителей прийти."
                            }
                        },
                        {
                            "text": "W tym oświetleniu wyglądasz wyjątkowo <w>korzystnie</w> i młodo.",
                            "target_lang_translations": {
                                "ru": "При этом освещении ты выглядишь исключительно <w>выигрышно</w> и молодо."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochlebny",
                        "przychylny",
                        "twarzowy"
                    ],
                    "antonyms": [
                        "niekorzystny",
                        "niepochlebny",
                        "nieprzychylny"
                    ],
                    "common_phrases": [
                        "w korzystnym świetle",
                        "korzystna opinia",
                        "korzystny wygląd",
                        "korzystna recenzja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представляющий кого-то или что-то в хорошем свете; выражающий одобрение или улучшающий внешний вид."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительный",
                                "target_lang_sense_clarification": "об отзыве, рецензии или мнении"
                            },
                            {
                                "target_lang_word": "выигрышный",
                                "target_lang_sense_clarification": "о внешнем виде или освещении"
                            },
                            {
                                "target_lang_word": "лестный",
                                "target_lang_sense_clarification": "содержащий похвалу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "нулевая видимость",
                        "нулевая реакция",
                        "свести к нулевому уровню"
                    ],
                    "target_lang_definitions": {
                        "en": "Very low, negligible, practically absent, or having produced no result."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "zero",
                                "target_lang_sense_clarification": "indicating complete absence"
                            },
                            {
                                "target_lang_word": "nil",
                                "target_lang_sense_clarification": "non-existent or yielding nothing"
                            }
                        ]

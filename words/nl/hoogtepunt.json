            "senses": [
                {
                    "sense_id": "f0031507-3a96-3cb7-8d47-51dbf689e3d5",
                    "sense_definition": "Een bijzonder belangrijk, mooi of succesvol moment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De prachtige zonsopgang was het <w>hoogtepunt</w> van onze lange wandeling.",
                            "target_lang_translations": {
                                "ru": "Прекрасный рассвет стал <w>кульминацией</w> нашей долгой прогулки."
                            }
                        },
                        {
                            "text": "Zijn carrière bereikte een <w>hoogtepunt</w> toen hij die internationale prijs won.",
                            "target_lang_translations": {
                                "ru": "Его карьера достигла <w>пика</w>, когда он выиграл эту международную премию."
                            }
                        }
                    ],
                    "synonyms": [
                        "top",
                        "climax",
                        "apotheose"
                    ],
                    "antonyms": [
                        "dieptepunt"
                    ],
                    "common_phrases": [
                        "op het hoogtepunt",
                        "het absolute hoogtepunt",
                        "bereikte een hoogtepunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особо важный, прекрасный или успешный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кульминация",
                                "target_lang_sense_clarification": "высшая точка развития, самый яркий момент"
                            },
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "высшая степень, максимум (например, карьеры)"
                            },
                            {
                                "target_lang_word": "вершина",
                                "target_lang_sense_clarification": "высшее достижение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02bf836a-d7f6-3b08-9a92-54b08e19b1d3",
                    "sense_definition": "Het moment van de grootste seksuele opwinding; een orgasme.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de romantische film komen de hoofdpersonen samen tot een <w>hoogtepunt</w>.",
                            "target_lang_translations": {
                                "ru": "В романтическом фильме главные герои вместе достигают <w>оргазма</w>."
                            }
                        },
                        {
                            "text": "Het boek beschrijft het seksuele <w>hoogtepunt</w> op een zeer gedetailleerde manier.",
                            "target_lang_translations": {
                                "ru": "В книге очень подробно описывается сексуальный <w>оргазм</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "orgasme",
                        "climax"
                    ],
                    "common_phrases": [
                        "tot een hoogtepunt komen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент наивысшего сексуального возбуждения; оргазм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оргазм",
                                "target_lang_sense_clarification": "физиологический пик"
                            },
                            {
                                "target_lang_word": "пик наслаждения",
                                "target_lang_sense_clarification": "описательное выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b925dac0-bd9d-34b7-ab80-c37abda17475",
                    "sense_definition": "Het snijpunt van de drie hoogtelijnen in een driehoek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij een stompe driehoek ligt het <w>hoogtepunt</w> buiten de figuur zelf.",
                            "target_lang_translations": {
                                "ru": "В тупоугольном треугольнике <w>ортоцентр</w> находится за пределами самой фигуры."
                            }
                        },
                        {
                            "text": "De leraar wiskunde legde uit hoe je het <w>hoogtepunt</w> kunt construeren.",
                            "target_lang_translations": {
                                "ru": "Учитель математики объяснил, как построить <w>ортоцентр</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {

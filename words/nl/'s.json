    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "216b41da-e861-3628-9ae9-90403765580e",
                    "sense_definition": "Verkorte vorm van het lidwoord <w>des</w> (van de of van het), tegenwoordig voornamelijk gebruikt in vaste tijdsaanduidingen en staande uitdrukkingen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga <w>'s</w> ochtends altijd met de fiets naar mijn werk.",
                            "target_lang_translations": {
                                "en": "I always go to work by bike <w>in the</w> morning.",
                                "ru": "Я всегда езжу на работу на велосипеде <w>по</w> утрам."
                            }
                        },
                        {
                            "text": "Dit is <w>'s</w> werelds grootste collectie van antieke munten.",
                            "target_lang_translations": {
                                "en": "This is the world<w>'s</w> largest collection of antique coins.",
                                "ru": "Это крупнейшая <w>в</w> мире коллекция античных монет."
                            }
                        }
                    ],
                    "synonyms": [
                        "des"
                    ],
                    "common_phrases": [
                        "'s ochtends",
                        "'s middags",
                        "'s avonds",
                        "'s nachts",
                        "'s winters",
                        "'s zomers"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "contraction"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Shortened form of the article 'des' (of the), mainly used in fixed time indications and set expressions.",
                        "ru": "Сокращенная форма артикля 'des' (родительный падеж), используемая в основном в обозначениях времени и устойчивых выражениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of the",
                                "target_lang_sense_clarification": "historical/genitive meaning"
                            },
                            {
                                "target_lang_word": "in the",
                                "target_lang_sense_clarification": "used in time expressions like 'in the morning'"
                            },
                            {
                                "target_lang_word": "'s",
                                "target_lang_sense_clarification": "possessive marker"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "предлог, используемый в выражениях времени (например, 's ochtends - по утрам)"
                            },
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "предлог, используемый в некоторых выражениях (например, 's werelds - в мире)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3e6f8632-2548-3997-a002-c513f4542cac",
                    "sense_definition": "Verkorte, spreektaalachtige vorm van het bijwoord <w>eens</w>, vaak gebruikt om een zin vriendelijker of minder dwingend te maken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kom <w>'s</w> hier en laat me zien wat je hebt gemaakt.",
                            "target_lang_translations": {
                                "en": "Come here <w>for a second</w> and show me what you have made.",
                                "ru": "Иди<w>-ка</w> сюда и покажи мне, что ты сделал."
                            }
                        },
                        {
                            "text": "Kijk <w>'s</w> of de postbode al is geweest vandaag.",
                            "target_lang_translations": {
                                "en": "<w>Just</w> check if the mailman has been today.",
                                "ru": "Посмотри<w>-ка</w>, приходил ли сегодня почтальон."
                            }
                        }
                    ],
                    "synonyms": [
                        "eens"
                    ],
                    "common_phrases": [
                        "kom 's",
                        "kijk 's",
                        "luister 's",
                        "zeg 's"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Shortened, colloquial form of the adverb 'eens', often used to make a sentence friendlier or less coercive.",
                        "ru": "Сокращенная разговорная форма наречия 'eens', часто используемая для смягчения тона предложения или просьбы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "used to soften requests or commands"
                            },
                            {
                                "target_lang_word": "for a second",
                                "target_lang_sense_clarification": "idiomatic softening of imperatives"
                            },
                            {
                                "target_lang_word": "once",
                                "target_lang_sense_clarification": "literal meaning of 'eens'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "-ка",
                                "target_lang_sense_clarification": "частица, добавляемая к глаголам в повелительном наклонении (смотри-ка)"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "используется для побуждения или смягчения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "lewkoniowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4cb75992-18d2-3c0b-b94f-a789634ad720",
                    "sense_definition": "Roślina ozdobna o pachnących kwiatach, często uprawiana w ogrodach i na balkonach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "roslina_kwiat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na moim balkonie zakwitła piękna, fioletowa <w>lewkonia</w>.",
                            "target_lang_translations": {
                                "ru": "На моем балконе расцвел красивый фиолетовый <w>левкой</w>."
                            }
                        },
                        {
                            "text": "Każda <w>lewkonia</w> wymaga żyznej gleby i dużej ilości słońca.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>левкой</w> требует плодородной почвы и большого количества солнца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lewkonia letnia",
                        "pachnąca lewkonia",
                        "sadzić lewkonie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративное растение с ароматными цветами, часто выращиваемое в садах и на балконах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левкой",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "маттиола",
                                "target_lang_sense_clarification": "садовое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffeaf386-2f8d-310d-8c37-0a72ec5f6ed9",
                    "sense_definition": "Kwiat tej rośliny, często wykorzystywany w bukietach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "roslina_kwiat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostałam od męża wielki bukiet kolorowych <w>lewkonii</w>.",
                            "target_lang_translations": {
                                "ru": "Я получила от мужа большой букет разноцветных <w>левкоев</w>."
                            }
                        },
                        {
                            "text": "W wazonie na stole stała samotna, biała <w>lewkonia</w>.",
                            "target_lang_translations": {
                                "ru": "В вазе на столе стоял одинокий белый <w>левкой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bukiet lewkonii",
                        "zapach lewkonii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цветок этого растения, часто используемый в букетах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левкой",
                                "target_lang_sense_clarification": "цветок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0f16197-83ea-3631-9779-12d658367fd7",
                    "sense_definition": "Rodzaj roślin z rodziny kapustowatych, obejmujący wiele gatunków.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "roslina_kwiat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Lewkonia</w> występuje naturalnie w Europie, Azji oraz Afryce.",
                            "target_lang_translations": {
                                "ru": "<w>Левкой</w> встречается в природе в Европе, Азии и Африке."
                            }
                        },
                        {
                            "text": "Botanicy zaliczają rodzaj <w>lewkonia</w> do rodziny kapustowatych.",
                            "target_lang_translations": {
                                "ru": "Ботаники относят род <w>левкой</w> к семейству капустных."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Род растений семейства капустных, включающий множество видов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левкой",
                                "target_lang_sense_clarification": "род растений"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
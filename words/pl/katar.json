    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb56dae0-6c72-3086-a0ad-98bb1207b31e",
                    "sense_definition": "dolegliwość zdrowotna polegająca na zapaleniu błony śluzowej nosa, objawiająca się wyciekiem wydzieliny i zatkaniem nosa",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "health_katar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez silny <w>katar</w> nie mogłem spać całą noc.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного <w>насморка</w> я не мог спать всю ночь."
                            }
                        },
                        {
                            "text": "Muszę kupić krople do nosa, bo męczy mnie <w>katar</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить капли в нос, потому что меня мучает <w>насморк</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieżyt"
                    ],
                    "common_phrases": [
                        "mieć katar",
                        "katar sienny",
                        "złapać katar",
                        "leczyć katar"
                    ],
                    "target_lang_definitions": {
                        "ru": "болезненное состояние, воспаление слизистой оболочки носа, проявляющееся выделениями и заложенностью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насморк",
                                "target_lang_sense_clarification": "воспаление слизистой носа"
                            },
                            {
                                "target_lang_word": "ринит",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b7af787-abb9-3b22-bc65-217afe06e5ae",
                    "sense_definition": "wydzielina, śluz wypływający z nosa podczas infekcji lub alergii",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "health_katar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko wytarło <w>katar</w> rękawem swetra.",
                            "target_lang_translations": {
                                "ru": "Ребёнок вытер <w>сопли</w> рукавом свитера."
                            }
                        },
                        {
                            "text": "Gęsty <w>katar</w> utrudnia swobodne oddychanie.",
                            "target_lang_translations": {
                                "ru": "Густые <w>сопли</w> затрудняют свободное дыхание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wycierać katar"
                    ],
                    "target_lang_definitions": {
                        "ru": "выделения, слизь, вытекающая из носа при инфекции или аллергии"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопли",
                                "target_lang_sense_clarification": "разг. слизистые выделения"
                            },
                            {
                                "target_lang_word": "слизь",
                                "target_lang_sense_clarification": "выделения из носа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "160e781c-4576-3ed3-b722-01499c7aa978",
                    "sense_definition": "członek ruchu religijnego rozwijającego się w średniowieczu w zachodniej Europie, uznanego przez Kościół katolicki za heretycki",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>katar</w> wierzył w dualistyczną naturę świata.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>катар</w> верил в дуалистическую природу мира."
                            }
                        },
                        {
                            "text": "W XIII wieku inkwizycja surowo karała każdego, kto był <w>katarem</w>.",
                            "target_lang_translations": {
                                "ru": "В XIII веке инквизиция сурово карала каждого, кто был <w>катаром</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "kontekst historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "последователь религиозного движения, развивавшегося в Средние века в Западной Европе, признанного католической церковью еретическим"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катар",
                                "target_lang_sense_clarification": "последователь ереси"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63db5af1-215a-3e87-8ad9-8fac0308f951",
                    "sense_definition": "rodzaj indyjskiej broni białej w formie sztyletu z poprzecznym uchwytem",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum broni wschodniej podziwialiśmy zabytkowy <w>katar</w>.",
                            "target_lang_translations": {
                                "ru": "В музее восточного оружия мы любовались старинным <w>катаром</w>."
                            }
                        },
                        {
                            "text": "Wojownik zadał szybki cios, trzymając w dłoni ostry <w>katar</w>.",
                            "target_lang_translations": {
                                "ru": "Воин нанёс быстрый удар, держа в руке острый <w>катар</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "broń historyczna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "вид индийского холодного оружия в форме кинжала с поперечной рукоятью"
                    },
                    "translations": {

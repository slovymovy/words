    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a8d942cc-fde8-3765-8dff-cacc9a66fd4a",
                    "sense_definition": "Mężczyzna, który nosi bujne, widoczne wąsy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "human_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>wąsacz</w> na zdjęciu to mój pradziadek.",
                            "target_lang_translations": {
                                "ru": "Этот <w>усач</w> на фотографии — мой прадедушка."
                            }
                        },
                        {
                            "text": "Do sklepu wszedł ponury <w>wąsacz</w> w kapeluszu.",
                            "target_lang_translations": {
                                "ru": "В магазин вошел угрюмый <w>усач</w> в шляпе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wąsal"
                    ],
                    "antonyms": [
                        "gołowąs"
                    ],
                    "common_phrases": [
                        "stary wąsacz",
                        "srogi wąsacz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина с пышными, заметными усами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усач",
                                "target_lang_sense_clarification": "мужчина с усами"
                            },
                            {
                                "target_lang_word": "усатый",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27e5ee7c-12bf-3f87-8724-e70215761e4b",
                    "sense_definition": "Gatunek ryby z rodziny zbrojnikowatych lub sumowatych, posiadający charakterystyczne wyrostki przypominające wąsy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fish_species",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim akwarium pływa duży <w>wąsacz</w> niebieski.",
                            "target_lang_translations": {
                                "ru": "В моем аквариуме плавает большой голубой <w>анциструс</w>."
                            }
                        },
                        {
                            "text": "Ten <w>wąsacz</w> żywi się głównie glonami.",
                            "target_lang_translations": {
                                "ru": "Этот <w>анциструс</w> питается в основном водорослями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wąsacz niebieski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид рыбы из семейства кольчужных сомов или сомовых, имеющий характерные выросты, напоминающие усы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анциструс",
                                "target_lang_sense_clarification": "аквариумная рыбка"
                            },
                            {
                                "target_lang_word": "сом-усач",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a4e0dec-3d17-3def-a56b-1f4694662089",
                    "sense_definition": "Rasa kury ozdobnej charakteryzująca się upierzeniem na głowie tworzącym brodę i wąsy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bird_breeds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wystawie drobiu zaprezentowano rzadkiego <w>wąsacza</w>.",
                            "target_lang_translations": {
                                "ru": "На выставке птиц представили редкую <w>бородатую курицу</w>."
                            }
                        },
                        {
                            "text": "Hodowla <w>wąsaczy</w> wymaga dużej wiedzy i doświadczenia.",
                            "target_lang_translations": {
                                "ru": "Разведение <w>бородатых кур</w> требует больших знаний и опыта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wąsacz antwerpski",
                        "wąsacz watermalski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порода декоративных кур, характеризующаяся оперением на голове, образующим бороду и усы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бородатая курица",
                                "target_lang_sense_clarification": "порода кур"
                            },
                            {
                                "target_lang_word": "бородатая бентамка",
                                "target_lang_sense_clarification": "разновидность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68133139-0828-3878-a35d-b5e72f1e7096",
                    "sense_definition": "Choroba bydła i koni objawiająca się zmianami w okolicach pyska.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_diseases",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weterynarz stwierdził u krowy objawy <w>wąsacza</w>.",
                            "target_lang_translations": {
                                "ru": "Ветеринар обнаружил у коровы симптомы <w>сикоза</w>."
                            }
                        },
                        {
                            "text": "Dawniej <w>wąsacz</w> był częstym problemem w gospodarstwach wiejskich.",
                            "target_lang_translations": {
                                "ru": "Раньше <w>сикоз</w> был частой проблемой в сельских хозяйствах."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезнь крупного рогатого скота и лошадей, проявляющаяся изменениями в области морды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сикоз",
                                "target_lang_sense_clarification": "ветеринария"
                            },
                            {
                                "target_lang_word": "воспаление волосяных луковиц",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },

                            },
                            {
                                "target_lang_word": "gravitational pull",
                                "target_lang_sense_clarification": "force of gravity"
                            },
                            {
                                "target_lang_word": "attraction",
                                "target_lang_sense_clarification": "magnetic or physical pull"
                            }

                        "walczyć o srebro",
                        "mieć srebro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серебряная медаль, присуждаемая за второе место в спортивных соревнованиях"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серебро",

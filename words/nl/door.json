                    "examples": [
                        {
                            "text": "Je leert een taal het beste <w>door</w> veel te oefenen.",
                            "target_lang_translations": {
                                "en": "You learn a language best <w>by</w> practicing a lot."
                            }
                        },
                        {
                            "text": "Hij opende de gesloten kist <w>door</w> middel van een hamer.",
                            "target_lang_translations": {
                                "en": "He opened the locked chest <w>by</w> means of a hammer."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5a34048a-8807-3af4-a674-2e1dc333f87a",
                    "sense_definition": "Река или ручей, впадающие в более крупную реку или другой водоём.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ока — это крупный правый <w>приток</w> реки Волги.",
                            "target_lang_translations": {
                                "en": "The Oka is a large right <w>tributary</w> of the Volga River."
                            }
                        },
                        {
                            "text": "На карте мы нашли маленький <w>приток</w>, который впадает в озеро.",
                            "target_lang_translations": {
                                "en": "On the map, we found a small <w>tributary</w> that flows into the lake."
                            }
                        }
                    ],
                    "common_phrases": [
                        "правый приток",
                        "левый приток",
                        "крупный приток"
                    ],
                    "target_lang_definitions": {
                        "en": "A river or stream that flows into a larger river or other body of water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tributary",
                                "target_lang_sense_clarification": "geography"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8924f3e4-f976-3592-be3b-68cdc251b48e",
                    "sense_definition": "Поступление, наплыв или скопление чего-либо движущегося в одном месте (воздуха, жидкости, людей, денег и т. п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнату необходимо обеспечить постоянный <w>приток</w> свежего воздуха.",
                            "target_lang_translations": {
                                "en": "It is necessary to ensure a constant <w>inflow</w> of fresh air into the room."
                            }
                        },
                        {
                            "text": "Новый закон стимулировал <w>приток</w> иностранных инвестиций в экономику.",
                            "target_lang_translations": {
                                "en": "The new law stimulated an <w>inflow</w> of foreign investment into the economy."
                            }
                        }
                    ],
                    "synonyms": [
                        "прилив",
                        "приход"
                    ],
                    "antonyms": [
                        "отток"
                    ],
                    "common_phrases": [
                        "приток воздуха",
                        "приток капитала",
                        "приток крови",
                        "приток сил"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of flowing in, or the arrival of large amounts of something (air, liquid, people, money, etc.) in one place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inflow",
                                "target_lang_sense_clarification": "of air, liquid, capital"
                            },
                            {
                                "target_lang_word": "influx",
                                "target_lang_sense_clarification": "of people, investments"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
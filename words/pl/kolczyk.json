            "senses": [
                {
                    "sense_id": "31acc42d-a2c0-32cc-adbb-a66ba9f4d2a8",
                    "sense_definition": "Ozdoba zawieszana w przekłutej skórze, najczęściej na płatkach uszu",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na urodziny dostałam od męża piękne złote <w>kolczyki</w>.",
                            "target_lang_translations": {
                                "ru": "На день рождения я получила от мужа красивые золотые <w>серьги</w>."
                            }
                        },
                        {
                            "text": "Musisz zdjąć <w>kolczyki</w> przed wejściem na salę operacyjną.",
                            "target_lang_translations": {
                                "ru": "Ты должна снять <w>серьги</w> перед входом в операционную."
                            }
                        }
                    ],
                    "synonyms": [
                        "zausznica"
                    ],
                    "common_phrases": [
                        "złote kolczyki",
                        "nosić kolczyki",
                        "para kolczyków",
                        "przekłuć uszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшение, которое носят в проколе кожи, чаще всего в мочке уха"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серьга",
                                "target_lang_sense_clarification": "украшение"
                            },
                            {
                                "target_lang_word": "серёжка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5d5ec25-c92a-32eb-aabc-be60a405de76",
                    "sense_definition": "Znakowana blaszka umieszczana w uchu zwierzęcia hodowlanego w celu identyfikacji",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda krowa w stadzie musi posiadać żółty <w>kolczyk</w> z numerem.",
                            "target_lang_translations": {
                                "ru": "У каждой коровы в стаде должна быть желтая <w>бирка</w> с номером."
                            }
                        },
                        {
                            "text": "Weterynarz sprawnie założył <w>kolczyk</w> młodemu cielakowi.",
                            "target_lang_translations": {
                                "ru": "Ветеринар ловко прикрепил <w>бирку</w> молодому теленку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kolczyk dla bydła",
                        "numer kolczyka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маркировочная табличка, прикрепляемая к уху животного для идентификации"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бирка",

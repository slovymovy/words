                    "sense_id": "f398643c-a18f-34c8-af1a-8450a10b0238",
                    "sense_definition": "Zonder dat er iets aan ontbreekt; geheel en al compleet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "complete_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er heerste een <w>volkomen</w> stilte in de grote bibliotheek.",
                            "target_lang_translations": {
                                "ru": "В большой библиотеке царила <w>полная</w> тишина.",
                                "en": "There was <w>complete</w> silence in the large library."
                            }
                        },
                        {
                            "text": "Het was voor ons een <w>volkomen</w> verrassing dat hij langskwam.",
                            "target_lang_translations": {
                                "ru": "Его визит стал для нас <w>полной</w> неожиданностью.",
                                "en": "It was a <w>complete</w> surprise to us that he came by."
                            }

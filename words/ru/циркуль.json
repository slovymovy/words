    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba177a45-bb18-3890-bfba-ad20f8dc0a83",
                    "sense_definition": "Инструмент для черчения окружностей и измерения расстояний, состоящий из двух раздвигающихся ножек.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "drawing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На уроке геометрии ученик достал <w>циркуль</w>, чтобы начертить ровный круг.",
                            "target_lang_translations": {
                                "en": "In geometry class, the student took out a <w>compass</w> to draw a perfect circle."
                            }
                        },
                        {
                            "text": "Осторожно пользуйтесь <w>циркулем</w>, так как его игла очень острая.",
                            "target_lang_translations": {
                                "en": "Use the <w>compass</w> carefully, as its needle is very sharp."
                            }
                        }
                    ],
                    "synonyms": [
                        "козья ножка"
                    ],
                    "common_phrases": [
                        "чертить циркулем",
                        "ножка циркуля",
                        "игла циркуля"
                    ],
                    "target_lang_definitions": {
                        "en": "A tool for drawing circles and measuring distances, consisting of two hinged legs."
                    },
                    "translations": {
                        "en": [
                            {

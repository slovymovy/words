    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e0dd3b1-1a46-376b-aaf7-13845fd8f22e",
                    "sense_definition": "Специальная панель на стене, на которой пишут мелом или маркером, обычно используемая в учебных заведениях.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "info_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель попросил ученика выйти к <w>доске</w> и решить задачу.",
                            "target_lang_translations": {
                                "en": "The teacher asked the student to go to the <w>board</w> and solve the problem."
                            }
                        },
                        {
                            "text": "На школьной <w>доске</w> было написано расписание уроков на завтра.",
                            "target_lang_translations": {
                                "en": "The lesson schedule for tomorrow was written on the school <w>board</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "школьная доска",
                        "писать на доске",
                        "выйти к доске",
                        "интерактивная доска"
                    ],
                    "target_lang_definitions": {
                        "en": "A special panel on a wall where one writes with chalk or a marker, usually used in educational institutions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "board",
                                "target_lang_sense_clarification": "classroom"
                            },
                            {
                                "target_lang_word": "blackboard",
                                "target_lang_sense_clarification": "for chalk"
                            },
                            {
                                "target_lang_word": "whiteboard",
                                "target_lang_sense_clarification": "for markers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e72ad76-ab6f-3d73-9e3e-dc46ca34b9ef",
                    "sense_definition": "Плоский и длинный кусок дерева, используемый в строительстве или для изготовления предметов быта.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для нарезки овощей нам нужна чистая разделочная <w>доска</w>.",
                            "target_lang_translations": {
                                "en": "We need a clean cutting <w>board</w> to slice the vegetables."
                            }
                        },
                        {
                            "text": "Строители заменили старую гнилую <w>доску</w> в заборе на новую.",
                            "target_lang_translations": {
                                "en": "The builders replaced the old rotten <w>board</w> in the fence with a new one."
                            }
                        }
                    ],
                    "common_phrases": [
                        "разделочная доска",
                        "гладильная доска",
                        "шахматная доска",
                        "половая доска"
                    ],
                    "target_lang_definitions": {
                        "en": "A flat and long piece of wood used in construction or for making household items."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "board",
                                "target_lang_sense_clarification": "piece of wood"
                            },
                            {
                                "target_lang_word": "plank",
                                "target_lang_sense_clarification": "thick board"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13efbdb1-0f0c-30de-a55c-189b5f169218",
                    "sense_definition": "Спортивный снаряд в виде плоской платформы для катания по снегу, воде или асфальту.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой мой брат любит кататься на <w>доске</w> по заснеженным склонам.",
                            "target_lang_translations": {
                                "en": "In winter, my brother loves riding a <w>board</w> on snowy slopes."
                            }
                        },
                        {
                            "text": "Серфингист взял свою <w>доску</w> и побежал к океану.",
                            "target_lang_translations": {
                                "en": "The surfer took his <w>board</w> and ran toward the ocean."
                            }
                        }
                    ],
                    "synonyms": [
                        "борд"
                    ],
                    "common_phrases": [
                        "кататься на доске",
                        "встать на доску",
                        "сноубордическая доска"
                    ],
                    "target_lang_definitions": {
                        "en": "Sports equipment in the form of a flat platform for riding on snow, water, or asphalt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "board",
                                "target_lang_sense_clarification": "sports equipment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1566903-461f-3d66-9085-f61754d2f4f3",
                    "sense_definition": "Плита из камня или металла с надписью или изображением, установленная в память о ком-либо или для информации.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "info_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На стене дома висит мемориальная <w>доска</w> в честь известного писателя.",
                            "target_lang_translations": {
                                "en": "A memorial <w>plaque</w> hangs on the wall of the house in honor of the famous writer."
                            }
                        },
                        {
                            "text": "Фотографию лучшего сотрудника повесили на <w>доску</w> почета.",
                            "target_lang_translations": {
                                "en": "The photo of the best employee was hung on the <w>board</w> of honor."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мемориальная доска",
                        "доска почета"
                    ],
                    "target_lang_definitions": {
                        "en": "A slab of stone or metal with an inscription or image, installed in memory of someone or for information."
                    },
                    "translations": {

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c3e9e8b-e5cd-30f8-88ff-3a44dd91621f",
                    "sense_definition": "Een spoorwegsysteem voor openbaar vervoer dat grotendeels door tunnels onder de grond loopt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We namen de <w>ondergrondse</w> om snel naar het centrum van de stad te reizen.",
                            "target_lang_translations": {
                                "ru": "Мы поехали на <w>метро</w>, чтобы быстро добраться до центра города."
                            }
                        },
                        {
                            "text": "Het station van de <w>ondergrondse</w> bevindt zich vlakbij het museum.",
                            "target_lang_translations": {
                                "ru": "Станция <w>метро</w> находится совсем рядом с музеем."
                            }
                        }
                    ],
                    "synonyms": [
                        "metro"
                    ],
                    "common_phrases": [
                        "met de ondergrondse",
                        "de Londense ondergrondse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Железнодорожная система общественного транспорта, проходящая в основном по туннелям под землей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метро",
                                "target_lang_sense_clarification": "вид городского транспорта"
                            },
                            {
                                "target_lang_word": "подземка",
                                "target_lang_sense_clarification": "разговорное название метрополитена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d44da40c-b82b-3aa8-b926-7b76e7e8b744",
                    "sense_definition": "Een geheime organisatie die verzet pleegt tegen een bezetter of een dictatoriaal regime.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de oorlog zat haar grootvader bij de <w>ondergrondse</w> om mensen te helpen onderduiken.",
                            "target_lang_translations": {
                                "ru": "Во время войны её дедушка был в <w>подполье</w>, помогая людям скрываться."
                            }
                        },
                        {
                            "text": "De leden van de <w>ondergrondse</w> verspreidden in het geheim illegale kranten.",
                            "target_lang_translations": {
                                "ru": "Члены <w>подполья</w> тайно распространяли нелегальные газеты."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "42166acb-4622-3da5-a3d7-abfd380fb46d",
                    "sense_definition": "Budynek lub duże pomieszczenie przeznaczone do przechowywania towarów, materiałów lub sprzętu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "storage_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie nowe produkty trafiły wczoraj na <w>magazyn</w>.",
                            "target_lang_translations": {
                                "ru": "Все новые товары вчера поступили на <w>склад</w>."
                            }
                        },
                        {
                            "text": "Kierownik <w>magazynu</w> odpowiada za porządek w dokumentach.",
                            "target_lang_translations": {
                                "ru": "Заведующий <w>складом</w> отвечает за порядок в документах."
                            }
                        }
                    ],
                    "synonyms": [
                        "skład"
                    ],
                    "common_phrases": [
                        "pracownik magazynu",
                        "magazyn centralny",
                        "stan magazynu",
                        "przyjąć na magazyn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или большое помещение, предназначенное для хранения товаров, материалов или оборудования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склад",
                                "target_lang_sense_clarification": "помещение для хранения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49f0c510-0565-397f-8fc4-5bd904b89dc6",
                    "sense_definition": "Kolorowe czasopismo, zwykle ilustrowane, wydawane w regularnych odstępach czasu, zawierające artykuły o różnorodnej tematyce.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "media_publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam w kiosku najnowszy <w>magazyn</w> o modzie.",
                            "target_lang_translations": {
                                "ru": "Я купила в киоске свежий <w>журнал</w> о моде."
                            }
                        },
                        {
                            "text": "W poczekalni u dentysty leżały stare <w>magazyny</w>.",
                            "target_lang_translations": {
                                "ru": "В приемной у стоматолога лежали старые <w>журналы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "magazyn ilustrowany",
                        "czytać magazyn",
                        "magazyn modowy",
                        "numer magazynu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иллюстрированное периодическое издание, выходящее через регулярные промежутки времени и содержащее статьи на различные темы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "периодическое издание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4279671d-7898-34b4-b71a-eec67db086a7",
                    "sense_definition": "Cykliczna audycja radiowa lub telewizyjna poświęcona określonej dziedzinie, składająca się z różnych materiałów i felietonów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "media_program",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy niedzielny poranek słucham <w>magazynu</w> kulturalnego.",
                            "target_lang_translations": {
                                "ru": "Каждое воскресное утро я слушаю культурное <w>обозрение</w>."
                            }
                        },
                        {
                            "text": "Telewizja wyemitowała ciekawy <w>magazyn</w> sportowy podsumowujący tydzień.",
                            "target_lang_translations": {
                                "ru": "По телевидению показали интересное спортивное <w>обозрение</w>, подытоживающее неделю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "magazyn sportowy",
                        "magazyn reporterów",
                        "magazyn poranny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Циклическая радио- или телепередача, посвященная определенной области и состоящая из различных материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обозрение",
                                "target_lang_sense_clarification": "обзорная передача"
                            },
                            {
                                "target_lang_word": "тележурнал",
                                "target_lang_sense_clarification": "тип телепередачи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b102a00-33f0-353b-aef7-619d37b43f55",
                    "sense_definition": "Element broni palnej służący do przechowywania i podawania amunicji do komory nabojowej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weaponry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz szybko wymienił pusty <w>magazyn</w> w karabinie.",
                            "target_lang_translations": {
                                "ru": "Солдат быстро сменил пустой <w>магазин</w> в винтовке."
                            }
                        },
                        {
                            "text": "Zacięcie broni było spowodowane uszkodzonym <w>magazynem</w>.",
                            "target_lang_translations": {
                                "ru": "Заклинивание оружия было вызвано поврежденным <w>магазином</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "magazynek"
                    ],
                    "common_phrases": [
                        "ładować magazyn",
                        "magazyn amunicji",
                        "magazyn rurowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент огнестрельного оружия, служащий для хранения и подачи патронов в патронник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магазин",
                                "target_lang_sense_clarification": "часть оружия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b720d295-5424-3d98-8621-9f12944fe0f9",
                    "sense_definition": "Duży, elegancki sklep oferujący różnorodne towary, często wielodziałowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "retail_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wojną był to najbardziej luksusowy <w>magazyn</w> w mieście.",
                            "target_lang_translations": {
                                "ru": "До войны это был самый роскошный <w>универмаг</w> в городе."
                            }
                        },
                        {
                            "text": "Wstąpiliśmy do <w>magazynu</w> z kapeluszami, aby przymierzyć nową kolekcję.",
                            "target_lang_translations": {
                                "ru": "Мы зашли в шляпный <w>магазин</w>, чтобы примерить новую коллекцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "magazyn mód",
                        "magazyn obuwia"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie używa się słów 'sklep', 'dom towarowy' lub 'galeria'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой, элегантный магазин, предлагающий разнообразные товары (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "универмаг",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17bf8c53-5f67-3035-bfca-b5a3ebad804b",
                    "sense_definition": "Spowodować, że coś się wydarzy lub powstanie; stać się przyczyną jakiegoś zjawiska, reakcji lub stanu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nieprzemyślane słowa mogą <wywołać> poważny konflikt w zespole.",
                            "target_lang_translations": {
                                "ru": "Его необдуманные слова могут <вызвать> серьезный конфликт в коллективе."
                            }
                        },
                        {
                            "text": "Nowe leki niestety <wywołały> u pacjenta silną reakcję alergiczną.",
                            "target_lang_translations": {
                                "ru": "Новые лекарства, к сожалению, <вызвали> у пациента сильную аллергическую реакцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "spowodować",
                        "sprowokować",
                        "wzbudzić"
                    ],
                    "antonyms": [
                        "zapobiec",
                        "powstrzymać",
                        "zażegnać"
                    ],
                    "common_phrases": [
                        "wywołać skandal",
                        "wywołać reakcję",
                        "wywołać wilka z lasu",
                        "wywołać wojnę",
                        "wywołać uśmiech"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы что-то произошло или возникло; стать причиной какого-либо явления, реакции или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызвать",
                                "target_lang_sense_clarification": "стать причиной, породить (реакцию, болезнь, чувство)"
                            },
                            {
                                "target_lang_word": "спровоцировать",
                                "target_lang_sense_clarification": "подстрекнуть, вызвать (конфликт, скандал)"
                            },
                            {
                                "target_lang_word": "повлечь",
                                "target_lang_sense_clarification": "привести к последствиям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fccb5832-a8dc-3c7e-831f-4a10b861b38f",
                    "sense_definition": "Głośno wypowiedzieć czyjeś imię lub nazwisko, aby ta osoba podeszła, odezwała się lub wyszła z pomieszczenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Boję się, że nauczyciel może mnie dzisiaj <wywołać> do tablicy.",
                            "target_lang_translations": {
                                "ru": "Я боюсь, что учитель может сегодня <вызвать> меня к доске."
                            }
                        },
                        {
                            "text": "Pielęgniarka wyszła na korytarz, aby <wywołać> kolejnego pacjenta.",
                            "target_lang_translations": {
                                "ru": "Медсестра вышла в коридор, чтобы <вызвать> следующего пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "wezwać",
                        "przywołać"
                    ],
                    "common_phrases": [
                        "wywołać do tablicy",
                        "wywołać po nazwisku",
                        "wywołać z sali"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко произнести чье-то имя или фамилию, чтобы этот человек подошел, отозвался или вышел из помещения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызвать",
                                "target_lang_sense_clarification": "попросить выйти или подойти (к доске, из кабинета)"
                            },
                            {
                                "target_lang_word": "позвать",
                                "target_lang_sense_clarification": "пригласить голосом"
                            },
                            {
                                "target_lang_word": "выкликнуть",
                                "target_lang_sense_clarification": "громко назвать по имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0450619-41ff-3d79-8602-113e027ccad6",
                    "sense_definition": "Sprawić, że coś pojawi się w pamięci lub wyobraźni; w kontekście spirytystycznym: zmusić ducha do pojawienia się.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "evocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta stara piosenka potrafi <wywołać> najpiękniejsze wspomnienia z dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Эта старая песня способна <вызвать> самые прекрасные воспоминания из детства."
                            }
                        },
                        {
                            "text": "Podczas seansu spirytystycznego medium próbowało <wywołać> ducha przodka.",
                            "target_lang_translations": {
                                "ru": "Во время спиритического сеанса медиум пытался <вызвать> дух предка."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywołać",
                        "wskrzesić"
                    ],
                    "common_phrases": [
                        "wywołać wspomnienia",
                        "wywołać ducha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то появиться в памяти или воображении; в спиритическом контексте: заставить духа появиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызвать",
                                "target_lang_sense_clarification": "духа, воспоминания"
                            },
                            {
                                "target_lang_word": "пробудить",
                                "target_lang_sense_clarification": "воспоминания, чувства"
                            },
                            {
                                "target_lang_word": "навеять",
                                "target_lang_sense_clarification": "воспоминания, мысли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "302aa778-8363-35eb-9eb8-b63091fdce5b",
                    "sense_definition": "Poddawać materiał fotograficzny (kliszę, film) działaniu substancji chemicznych w celu uzyskania widocznego obrazu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy iść do fotografa, aby <wywołać> zdjęcia z naszych wakacji.",
                            "target_lang_translations": {
                                "ru": "Мы должны пойти к фотографу, чтобы <проявить> фотографии с нашего отпуска."
                            }
                        },
                        {
                            "text": "W ciemni fotograficznej można samodzielnie <wywołać> czarno-biały film.",
                            "target_lang_translations": {
                                "ru": "В фотолаборатории можно самостоятельно <проявить> черно-белую пленку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wywołać zdjęcia",
                        "wywołać film",
                        "wywołać kliszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать фотоматериал (пленку) химической обработке для получения видимого изображения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявить",
                                "target_lang_sense_clarification": "пленку, фотографии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

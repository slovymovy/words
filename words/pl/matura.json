            "pos": "noun",
            "senses": [
                {
                    "sense_id": "943f9e8b-c56c-30f9-9d1e-f808daa01916",
                    "sense_definition": "Egzamin państwowy zdawany na zakończenie szkoły średniej, będący warunkiem przyjęcia na studia wyższe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exam_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku <w>matura</w> z matematyki była wyjątkowo trudna dla uczniów.",
                            "target_lang_translations": {
                                "ru": "В этом году <w>выпускной экзамен</w> по математике был исключительно трудным для учеников."
                            }
                        },
                        {
                            "text": "Mój starszy brat intensywnie uczy się, ponieważ w maju czeka go <w>matura</w>.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат усиленно учится, так как в мае его ждет <w>выпускной экзамен</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "egzamin dojrzałości"
                    ],
                    "common_phrases": [
                        "zdać maturę",
                        "obleć maturę",
                        "matura ustna",
                        "matura pisemna",
                        "matura próbna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственный экзамен, сдаваемый по окончании средней школы, являющийся условием для поступления в высшие учебные заведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпускной экзамен",
                                "target_lang_sense_clarification": "экзамен"
                            },
                            {
                                "target_lang_word": "экзамен на аттестат зрелости",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b156a51-7638-3d23-b89e-bab9c00a185d",
                    "sense_definition": "Dokument urzędowy potwierdzający zdanie egzaminu kończącego szkołę średnią.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exam_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz złożyć kopię swojej <w>matury</w> w dziekanacie, aby dopełnić formalności.",
                            "target_lang_translations": {
                                "ru": "Ты должен подать копию своего <w>аттестата зрелости</w> в деканат, чтобы уладить формальности."
                            }
                        },
                        {
                            "text": "Dyrektor szkoły uroczyście wręczył absolwentom ich <w>matury</w>.",
                            "target_lang_translations": {
                                "ru": "Директор школы торжественно вручил выпускникам их <w>аттестаты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "świadectwo dojrzałości"
                    ],
                    "common_phrases": [
                        "odebrać maturę",
                        "odpis matury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ, подтверждающий сдачу экзамена по окончании средней школы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аттестат зрелости",
                                "target_lang_sense_clarification": "документ"
                            },
                            {
                                "target_lang_word": "аттестат",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd1f8bbb-f7fb-35f1-9cd5-820359639ccb",
                    "sense_definition": "Dawna lub gwarowa forma słowa natura, oznaczająca przyrodę lub charakter.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Taka już jego <w>matura</w>, że nigdy nie potrafi usiedzieć w jednym miejscu.",
                            "target_lang_translations": {
                                "ru": "Такая уж его <w>натура</w>, что он никогда не может усидеть на одном месте."
                            }
                        },
                        {
                            "text": "Dawniejsza <w>matura</w> ludzi była inna niż ta, którą widzimy dzisiaj.",
                            "target_lang_translations": {
                                "ru": "Прежняя <w>натура</w> людей была иной, чем та, которую мы видим сегодня."
                            }
                        }
                    ],

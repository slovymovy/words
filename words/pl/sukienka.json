{
    "word_family": [
        "suknia",
        "sukieneczka",
        "sukno",
        "sukiennik",
        "sukienkowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51a8bb67-4987-3caa-a991-2ab9dea9da12",
                    "sense_definition": "Jednoczęściowe ubranie kobiece okrywające tułów i nogi, noszone na co dzień lub od święta.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta czerwona <w>sukienka</w> bardzo ładnie na tobie leży.",
                            "target_lang_translations": {
                                "ru": "Это красное <w>платье</w> очень хорошо на тебе сидит."
                            }
                        },
                        {
                            "text": "Muszę kupić nową, elegancką <w>sukienkę</w> na wesele siostry.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новое элегантное <w>платье</w> на свадьбу сестры."
                            }
                        }
                    ],
                    "synonyms": [
                        "kiecka"
                    ],
                    "common_phrases": [
                        "letnia sukienka",
                        "ślubna sukienka",
                        "krótka sukienka",
                        "nosić sukienkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская одежда, закрывающая туловище и ноги, которую носят в повседневной жизни или по праздникам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платье",
                                "target_lang_sense_clarification": "женская одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "370ed829-279b-3db0-9592-a49fa959a451",
                    "sense_definition": "Potoczne określenie długiego stroju noszonego przez duchownych, takiego jak sutanna czy habit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clergy_attire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ulicy minął nas zakonnik ubrany w brązową <w>sukienkę</w>.",
                            "target_lang_translations": {
                                "ru": "На улице мимо нас прошел монах, одетый в коричневую <w>рясу</w>."
                            }
                        },
                        {
                            "text": "Wielu księży na co dzień nie nosi już <w>sukienek</w>, lecz zwykłe ubrania.",
                            "target_lang_translations": {
                                "ru": "Многие священники в повседневной жизни уже не носят <w>сутан</w>, а обычную одежду."
                            }
                        }
                    ],
                    "synonyms": [
                        "sutanna",
                        "habit"
                    ],
                    "common_phrases": [
                        "facet w sukience"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "o stroju duchownego"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название длинного одеяния, которое носят священнослужители (ряса, сутана)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сутана",
                                "target_lang_sense_clarification": "одежда католического духовенства"
                            },
                            {
                                "target_lang_word": "ряса",
                                "target_lang_sense_clarification": "одежда православного духовенства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d01ddc2e-4249-3766-8583-6f6ef03d0487",
                    "sense_definition": "Ozdobna nakładka z metalu szlachetnego lub tkaniny umieszczana na ikonie lub świętym obrazie, odsłaniająca zazwyczaj tylko twarze i dłonie postaci.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złota <w>sukienka</w> na obrazie Matki Boskiej lśniła w blasku świec.",
                            "target_lang_translations": {
                                "ru": "Золотой <w>оклад</w> на иконе Божьей Матери сиял в свете свечей."
                            }
                        },
                        {
                            "text": "Wierni ufundowali nową <w>sukienkę</w> jako wotum dziękczynne.",
                            "target_lang_translations": {
                                "ru": "Верующие пожертвовали новый <w>оклад</w> в знак благодарности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "srebrna sukienka",
                        "sukienka wotywna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративное покрытие из металла или ткани на иконе, оставляющее открытыми только лик и руки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оклад",
                                "target_lang_sense_clarification": "украшение на иконе"
                            },
                            {
                                "target_lang_word": "риза",
                                "target_lang_sense_clarification": "металлическая накладка на иконе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
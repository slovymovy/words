    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc7c503f-107f-3f96-b145-ed8d780192e7",
                    "sense_definition": "Jedenastocyfrowy numer identyfikacyjny przypisany do konkretnej osoby w polskim systemie ewidencji ludności, używany w sprawach urzędowych i medycznych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę wpisać swój numer <w>PESEL</w> w odpowiednim polu formularza.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, впишите свой <w>PESEL</w> в соответствующее поле формы."
                            }
                        },
                        {
                            "text": "Lekarz nie mógł wystawić recepty, ponieważ pacjent zapomniał podać <w>PESEL</w>.",
                            "target_lang_translations": {
                                "ru": "Врач не мог выписать рецепт, так как пациент забыл предоставить <w>PESEL</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "numer ewidencyjny"
                    ],
                    "common_phrases": [
                        "numer PESEL",
                        "podać PESEL",
                        "wpisać PESEL",
                        "swój PESEL"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одиннадцатизначный идентификационный номер, присваиваемый конкретному лицу в польской системе учета населения, используемый в официальных и медицинских вопросах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "номер PESEL",
                                "target_lang_sense_clarification": "идентификационный номер"
                            },
                            {
                                "target_lang_word": "PESEL",
                                "target_lang_sense_clarification": "аббревиатура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "338a6520-84bd-38e5-8632-aedb025dd5d7",
                    "sense_definition": "Powszechny Elektroniczny System Ewidencji Ludności – oficjalny rejestr państwowy gromadzący dane o obywatelach Polski.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie zmiany stanu cywilnego są natychmiast odnotowywane w systemie <w>PESEL</w>.",
                            "target_lang_translations": {
                                "ru": "Все изменения гражданского состояния немедленно фиксируются в системе <w>PESEL</w>."
                            }
                        },
                        {
                            "text": "Dostęp do bazy <w>PESEL</w> mają tylko uprawnione instytucje państwowe.",
                            "target_lang_translations": {
                                "ru": "Доступ к базе <w>PESEL</w> имеют только уполномоченные государственные учреждения."
                            }
                        }
                    ],
                    "synonyms": [
                        "ewidencja ludności"
                    ],
                    "common_phrases": [
                        "system PESEL",
                        "baza PESEL",
                        "rejestr PESEL"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всеобщая электронная система учета населения — официальный государственный реестр, собирающий данные о гражданах Польши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система PESEL",
                                "target_lang_sense_clarification": "государственный реестр"
                            },
                            {
                                "target_lang_word": "база PESEL",
                                "target_lang_sense_clarification": "база данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5bf258d-b712-324d-8fa7-8a2b94065ce6",
                    "sense_definition": "Wiek danej osoby, wspominany zazwyczaj żartobliwie w kontekście starzenia się, upływu czasu lub ograniczeń fizycznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym biegać tak szybko jak kiedyś, ale niestety <w>pesel</w> mnie ogranicza.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы бегать так же быстро, как раньше, но, к сожалению, <w>возраст</w> меня ограничивает."
                            }
                        },
                        {
                            "text": "Z <w>peselem</w> nie wygrasz, pewne dolegliwości przychodzą po prostu z wiekiem.",
                            "target_lang_translations": {
                                "ru": "С <w>возрастом</w> не поспоришь, определенные недуги приходят просто со временем."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiek",
                        "metryka"
                    ],
                    "common_phrases": [
                        "z peselem nie wygrasz",
                        "pesel nie kłamie",
                        "patrzeć w pesel"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane żartobliwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возраст человека, упоминаемый обычно в шутливом контексте старения, течения времени или физических ограничений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возраст",
                                "target_lang_sense_clarification": "количество лет"
                            },
                            {
                                "target_lang_word": "годы",
                                "target_lang_sense_clarification": "старение"
                            }
                        ]
                    }
                }
            ]
        }

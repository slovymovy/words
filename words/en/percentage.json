    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8bfcc87-c27e-32bc-b945-8ea0ff05b42c",
                    "sense_definition": "The amount, number, or rate of something, regarded as part of a total of 100.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "math_ratio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A high <w>percentage</w> of students passed the final exam.",
                            "target_lang_translations": {
                                "ru": "Высокий <w>процент</w> студентов сдал выпускной экзамен."
                            }
                        },
                        {
                            "text": "Only a small <w>percentage</w> of the population voted in the election.",
                            "target_lang_translations": {
                                "ru": "Лишь небольшой <w>процент</w> населения проголосовал на выборах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "high percentage",
                        "small percentage",
                        "large percentage",
                        "percentage of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество или доля чего-либо, выраженная как часть от ста"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "доля, часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eaa511b3-3d25-3b5a-98de-b276c1f1bc19",
                    "sense_definition": "A share of sales, profits, or earnings.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math_ratio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gets a <w>percentage</w> for every vacuum cleaner she sells.",
                            "target_lang_translations": {
                                "ru": "Она получает <w>процент</w> за каждый проданный ею пылесос."
                            }
                        },
                        {
                            "text": "The agent takes a <w>percentage</w> of the actor's earnings.",
                            "target_lang_translations": {
                                "ru": "Агент берет <w>процент</w> от заработка актера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get a percentage",
                        "take a percentage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доля от продаж, прибыли или заработка"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "комиссионные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3107a7f-4d5b-3b22-a6b7-0714d1fdb0db",
                    "sense_definition": "A benefit or advantage gained from an action.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no <w>percentage</w> in arguing with him about the rules.",
                            "target_lang_translations": {
                                "ru": "Нет никакой <w>выгоды</w> спорить с ним о правилах."
                            }
                        },
                        {
                            "text": "He realized there was no <w>percentage</w> in staying at home all day.",
                            "target_lang_translations": {
                                "ru": "Он понял, что нет никакой <w>выгоды</w> сидеть дома весь день."
                            }

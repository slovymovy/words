    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "61574238-91cd-3a60-806a-68a88c5f5408",
                    "sense_definition": "Количество и состав пищи или корма на определённый срок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач посоветовал мне пересмотреть свой ежедневный <w>рацион</w>.",
                            "target_lang_translations": {
                                "en": "The doctor advised me to review my daily <w>diet</w>."
                            }
                        },
                        {
                            "text": "В <w>рацион</w> питания ребёнка обязательно должны входить молочные продукты.",
                            "target_lang_translations": {
                                "en": "Dairy products must be included in a child's <w>diet</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дневной рацион",
                        "сбалансированный рацион",
                        "рацион питания",
                        "входить в рацион"
                    ],
                    "target_lang_definitions": {
                        "en": "The quantity and composition of food consumed over a specific period."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "diet",
                                "target_lang_sense_clarification": "habitual nourishment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adc32329-f0ad-3eb6-8244-6590a9c21117",
                    "sense_definition": "Установленная норма продуктов питания, выдаваемая на определённый срок (обычно сутки).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдатам выдали сухой <w>рацион</w> на три дня перед выходом на задание.",
                            "target_lang_translations": {
                                "en": "The soldiers were issued a dry <w>ration</w> for three days before going on the mission."
                            }
                        },
                        {
                            "text": "В голодные годы <w>рацион</w> был очень скудным и однообразным.",
                            "target_lang_translations": {
                                "en": "In the hungry years, the <w>ration</w> was very meager and monotonous."
                            }
                        }
                    ],
                    "synonyms": [
                        "паёк",
                        "порция"
                    ],
                    "common_phrases": [
                        "сухой рацион",
                        "скудный рацион",
                        "армейский рацион"
                    ],
                    "target_lang_definitions": {
                        "en": "A fixed amount of food officially allowed to a person for a specific time (usually a day)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ration",
                                "target_lang_sense_clarification": "fixed allowance, often military"
                            },
                            {
                                "target_lang_word": "food allowance",
                                "target_lang_sense_clarification": "allotted portion"
                            }
                        ]
                    }
                }
            ]
        }
    ]

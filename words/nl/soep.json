                    "sense_id": "b3f43321-e389-3f99-896b-215868be5f8a",
                    "sense_definition": "Vloeibaar gerecht dat bereid wordt door ingrediënten zoals groenten en vlees in water of bouillon te koken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik eet graag een kom warme <w>soep</w> als lunch.",
                            "target_lang_translations": {
                                "en": "I like to eat a bowl of hot <w>soup</w> for lunch.",
                                "ru": "Я люблю съесть тарелку горячего <w>супа</w> на обед."
                            }
                        },
                        {
                            "text": "Mijn moeder maakt elke zondag verse <w>soep</w>.",
                            "target_lang_translations": {
                                "en": "My mother makes fresh <w>soup</w> every Sunday.",
                                "ru": "Моя мама каждое воскресенье готовит свежий <w>суп</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kom soep",
                        "soep eten",
                        "pan soep",
                        "verse soep"
                    ],
                    "target_lang_definitions": {
                        "en": "A liquid dish prepared by boiling ingredients such as vegetables and meat in water or broth.",
                        "ru": "Жидкое блюдо, приготовленное путем варки ингредиентов, таких как овощи и мясо, в воде или бульоне."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "soup",
                                "target_lang_sense_clarification": "liquid food"
                            }

{
    "word_family": [
        "troepen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ef150aa-20f7-3ad3-9bd6-393fb82b519c",
                    "sense_definition": "Een toestand van wanorde of een verzameling spullen die niet opgeruimd zijn.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "disorder_waste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je kamer opruimen, want het is daar een enorme <w>troep</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен убраться в своей комнате, потому что там ужасный <w>бардак</w>."
                            }
                        },
                        {
                            "text": "De kinderen hebben er in de woonkamer een <w>troep</w> van gemaakt.",
                            "target_lang_translations": {
                                "ru": "Дети устроили в гостиной <w>беспорядок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rommel",
                        "wanorde",
                        "puinhoop",
                        "zooi",
                        "rotzooi"
                    ],
                    "common_phrases": [
                        "wat een troep",
                        "er een troep van maken",
                        "in de troep zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние беспорядка или куча неубранных вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспорядок",
                                "target_lang_sense_clarification": "отсутствие порядка"
                            },
                            {
                                "target_lang_word": "бардак",
                                "target_lang_sense_clarification": "разговорное: сильный беспорядок"
                            },
                            {
                                "target_lang_word": "свалка",
                                "target_lang_sense_clarification": "переносное: очень грязное место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66a39f77-d352-3e25-a3a2-fb2583cc6cac",
                    "sense_definition": "Spullen die waardeloos, vies of van slechte kwaliteit zijn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "disorder_waste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik koop die goedkope <w>troep</w> uit de supermarkt niet meer.",
                            "target_lang_translations": {
                                "ru": "Я больше не покупаю эту дешевую <w>дрянь</w> в супермаркете."
                            }
                        },
                        {
                            "text": "Gooi die oude <w>troep</w> toch gewoon in de vuilnisbak.",
                            "target_lang_translations": {
                                "ru": "Просто выброси этот старый <w>хлам</w> в мусорное ведро."
                            }
                        }
                    ],
                    "synonyms": [
                        "rommel",
                        "prul",
                        "zooi",
                        "brol"
                    ],
                    "common_phrases": [
                        "oude troep",
                        "waardeloze troep",
                        "chemische troep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещи, которые являются бесполезными, грязными или плохого качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлам",
                                "target_lang_sense_clarification": "старые или ненужные вещи"
                            },
                            {
                                "target_lang_word": "дрянь",
                                "target_lang_sense_clarification": "вещи плохого качества"
                            },
                            {
                                "target_lang_word": "барахло",
                                "target_lang_sense_clarification": "разговорное: всякая всячина"
                            },
                            {
                                "target_lang_word": "мусор",
                                "target_lang_sense_clarification": "отходы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ab4a64b-0dc1-30db-94bf-32a222020ed8",
                    "sense_definition": "Een militaire eenheid of een verzameling soldaten (vaak in het meervoud gebruikt).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "collection_beings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De generaal stuurde zijn <w>troepen</w> naar de grens om het land te verdedigen.",
                            "target_lang_translations": {
                                "ru": "Генерал отправил свои <w>войска</w> к границе для защиты страны."
                            }
                        },
                        {
                            "text": "De vijandelijke <w>troepen</w> trokken zich na twee weken terug.",
                            "target_lang_translations": {
                                "ru": "Вражеские <w>войска</w> отступили через две недели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Amerikaanse troepen",
                        "eigen troepen",
                        "troepen sturen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военное подразделение или группа солдат (часто используется во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "войска",
                                "target_lang_sense_clarification": "вооруженные силы"
                            },
                            {
                                "target_lang_word": "отряд",
                                "target_lang_sense_clarification": "группа военных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28498fd5-92ff-3646-8531-49d3747cd517",
                    "sense_definition": "Een groep dieren of mensen die bij elkaar horen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "collection_beings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zagen een <w>troep</w> wolven door het besneeuwde bos rennen.",
                            "target_lang_translations": {
                                "ru": "Мы видели, как <w>стая</w> волков бежала по заснеженному лесу."
                            }
                        },
                        {
                            "text": "Een luidruchtige <w>troep</w> jongeren liep midden in de nacht door de straat.",
                            "target_lang_translations": {
                                "ru": "Шумная <w>толпа</w> подростков шла по улице посреди ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "roedel",
                        "kudde"
                    ],
                    "common_phrases": [
                        "een troep wolven",
                        "een troep ganzen",
                        "de hele troep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа животных или людей, держащихся вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стая",
                                "target_lang_sense_clarification": "группа животных"
                            },
                            {
                                "target_lang_word": "толпа",
                                "target_lang_sense_clarification": "группа людей"
                            },
                            {
                                "target_lang_word": "ватага",
                                "target_lang_sense_clarification": "шумная группа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
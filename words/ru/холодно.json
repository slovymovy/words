            "senses": [
                {
                    "sense_id": "1ef131ed-e2aa-3727-9c1e-acb56b186c10",
                    "sense_definition": "О низкой температуре воздуха или окружающей среды",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "weather_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой в этом регионе очень <w>холодно</w> и ветрено.",
                            "target_lang_translations": {
                                "en": "In winter, it is very <w>cold</w> and windy in this region."
                            }
                        },
                        {
                            "text": "Сегодня на улице <w>холодно</w>, поэтому мы остались дома.",
                            "target_lang_translations": {
                                "en": "It is <w>cold</w> outside today, so we stayed home."
                            }
                        }
                    ],
                    "synonyms": [
                        "зябко",
                        "бесстрастно"
                    ],
                    "antonyms": [
                        "тепло",
                        "жарко"
                    ],
                    "common_phrases": [
                        "на улице холодно",
                        "зимой холодно",
                        "сегодня холодно",
                        "очень холодно"
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding low air or environmental temperature"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cold",
                                "target_lang_sense_clarification": "weather condition"
                            },
                            {
                                "target_lang_word": "it is cold",
                                "target_lang_sense_clarification": "impersonal phrase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ca5d56e-3dfb-3c8f-a48e-52dd690021d6",
                    "sense_definition": "Об ощущении холода, которое испытывает живое существо",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <w>холодно</w>, пожалуйста, закрой окно.",
                            "target_lang_translations": {
                                "en": "I am <w>cold</w>, please close the window."
                            }
                        },
                        {
                            "text": "Мальчику стало <w>холодно</w> без куртки, и он побежал домой.",
                            "target_lang_translations": {
                                "en": "The boy got <w>cold</w> without a jacket, and he ran home."
                            }
                        }
                    ],
                    "synonyms": [
                        "зябко"
                    ],
                    "antonyms": [
                        "тепло",
                        "жарко"
                    ],
                    "common_phrases": [
                        "мне холодно",
                        "рукам холодно",
                        "стало холодно"
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding the sensation of cold felt by a living being"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cold",
                                "target_lang_sense_clarification": "physical sensation"
                            },
                            {
                                "target_lang_word": "feeling cold",
                                "target_lang_sense_clarification": "state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f22d368-b966-30bb-956e-9e57573fc94f",
                    "sense_definition": "Без проявления чувств, равнодушно или неприветливо",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>холодно</w> посмотрел на собеседника и ничего не ответил.",
                            "target_lang_translations": {
                                "en": "He looked <w>coldly</w> at his interlocutor and answered nothing."
                            }
                        },
                        {
                            "text": "Гостей встретили довольно <w>холодно</w>, даже не предложив чая.",
                            "target_lang_translations": {
                                "en": "The guests were greeted rather <w>coldly</w>, without even being offered tea."
                            }
                        }
                    ],
                    "synonyms": [
                        "бесстрастно",
                        "невозмутимо",
                        "бесстрастно",
                        "бесстрастно"
                    ],
                    "antonyms": [

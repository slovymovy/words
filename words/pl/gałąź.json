{
    "word_family": [
        "gałązka",
        "rozgałęzienie",
        "gałęziasty"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7136c431-9d5c-3152-9c3d-9e8bc4a094f1",
                    "sense_definition": "Boczny pęd drzewa lub krzewu wyrastający z pnia lub innej, grubszej odnogi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr złamał dużą <gałąź> starego dębu w naszym ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер сломал большую <ветку> старого дуба в нашем саду."
                            }
                        },
                        {
                            "text": "Ptaki często siadają na tej <gałęzi>, aby obserwować okolicę.",
                            "target_lang_translations": {
                                "ru": "Птицы часто садятся на эту <ветку>, чтобы осмотреть окрестности."
                            }
                        }
                    ],
                    "synonyms": [
                        "konar",
                        "odrośl",
                        "gw. (Górny Śląsk) asta"
                    ],
                    "antonyms": [
                        "pień",
                        "korzenie"
                    ],
                    "common_phrases": [
                        "sucha gałąź",
                        "siedzieć na gałęzi",
                        "obciąć gałąź",
                        "gruba gałąź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Боковой побег дерева или кустарника, растущий от ствола или другого, более толстого ответвления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветка",
                                "target_lang_sense_clarification": "общее значение; обычно более тонкая"
                            },
                            {
                                "target_lang_word": "ветвь",
                                "target_lang_sense_clarification": "крупная ветка; ботанический или поэтический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d15ef665-4ac7-36de-af44-1209c4c9f500",
                    "sense_definition": "Wyodrębniona część jakiejś dziedziny, zwłaszcza gospodarki, przemysłu lub nauki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turystyka to obecnie najważniejsza <gałąź> gospodarki w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Туризм — это в настоящее время важнейшая <отрасль> экономики в этом регионе."
                            }
                        },
                        {
                            "text": "Medycyna estetyczna jest dynamicznie rozwijającą się <gałęzią> nauki.",
                            "target_lang_translations": {
                                "ru": "Эстетическая медицина является динамично развивающейся <отраслью> науки."
                            }
                        }
                    ],
                    "synonyms": [
                        "dział"
                    ],
                    "common_phrases": [
                        "gałąź przemysłu",
                        "gałąź gospodarki",
                        "gałąź nauki",
                        "nowa gałąź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная часть какой-либо сферы деятельности, особенно экономики, промышленности или науки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрасль",
                                "target_lang_sense_clarification": "экономики, науки, промышленности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21366929-8cc8-3d62-add7-378b296c6fdf",
                    "sense_definition": "Odnoga lub rozwidlenie czegoś, co tworzy strukturę, np. rodu, rzeki lub układu nerwowego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas badań genealogicznych odkrył zapomnianą <gałąź> swojej rodziny.",
                            "target_lang_translations": {
                                "ru": "В ходе генеалогических исследований он обнаружил забытую <ветвь> своей семьи."
                            }
                        },
                        {
                            "text": "Lekarz wyjaśnił pacjentowi, która <gałąź> nerwu została uszkodzona.",
                            "target_lang_translations": {
                                "ru": "Врач объяснил пациенту, какая <ветвь> нерва была повреждена."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnoga",
                        "linia boczna"
                    ],
                    "common_phrases": [
                        "gałąź rodu",
                        "boczna gałąź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ответвление или разветвление структуры, например, рода, реки или нервной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветвь",
                                "target_lang_sense_clarification": "рода, генеалогическая, нервная"
                            },
                            {
                                "target_lang_word": "ответвление",
                                "target_lang_sense_clarification": "общая структура"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
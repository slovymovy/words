                {
                    "sense_id": "e1306318-699f-32fb-abf3-f3eb8caa05d9",
                    "sense_definition": "Przyprawa o korzennym zapachu i gorzkawym smaku, uzyskiwana z nasion kozieradki pospolitej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dodawana do potraw <w>kozieradka</w> wzbogaca ich smak i pobudza trawienie.",
                            "target_lang_translations": {
                                "ru": "Добавляемый в блюда <w>пажитник</w> обогащает их вкус и стимулирует пищеварение."
                            }
                        },
                        {
                            "text": "Do przygotowania domowego curry niezbędna będzie mielona <w>kozieradka</w>.",
                            "target_lang_translations": {
                                "ru": "Для приготовления домашнего карри понадобится молотый <w>пажитник</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kozierodka"
                    ],
                    "common_phrases": [
                        "mielona kozieradka",
                        "napar z kozieradki",
                        "łyżeczka kozieradki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пряность с пряным запахом и горьковатым вкусом, получаемая из семян пажитника сенного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пажитник",
                                "target_lang_sense_clarification": "пряность"
                            },
                            {
                                "target_lang_word": "шамбала",
                                "target_lang_sense_clarification": "кулинарное название"
                            },
                            {
                                "target_lang_word": "фенугрек",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b3c4137-00c2-3f3f-adba-f204f79bb198",
                    "sense_definition": "Roślina zielna z rodziny bobowatych, uprawiana dla nasion wykorzystywanych w lecznictwie i kuchni.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na polu uprawnym rosła bujna <w>kozieradka</w> przeznaczona na paszę.",
                            "target_lang_translations": {
                                "ru": "На возделываемом поле рос пышный <w>пажитник</w>, предназначенный на корм."
                            }
                        },
                        {
                            "text": "W medycynie ludowej <w>kozieradka</w> była stosowana jako środek wzmacniający organizm.",
                            "target_lang_translations": {
                                "ru": "В народной медицине <w>пажитник</w> использовался как средство для укрепления организма."
                            }
                        }
                    ],
                    "synonyms": [
                        "kozierodka"
                    ],
                    "common_phrases": [
                        "nasiona kozieradki",
                        "uprawa kozieradki",
                        "ekstrakt z kozieradki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Травянистое растение семейства бобовых, выращиваемое ради семян, используемых в медицине и кулинарии."
                    },

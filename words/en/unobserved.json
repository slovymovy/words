{
    "word_family": [
        "observe",
        "observation",
        "observer",
        "observable",
        "observant",
        "observance"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5db1d583-03b8-3d4c-9470-06fd8b93375a",
                    "sense_definition": "Not seen or noticed by anyone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "not_seen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small error in the calculation went <w>unobserved</w> for several days.",
                            "target_lang_translations": {
                                "ru": "Небольшая ошибка в расчетах оставалась <w>незамеченной</w> в течение нескольких дней."
                            }
                        },
                        {
                            "text": "He preferred to remain <w>unobserved</w> at the back of the crowded room.",
                            "target_lang_translations": {
                                "ru": "Он предпочел оставаться <w>незамеченным</w> в глубине переполненной комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "unseen",
                        "unnoticed",
                        "undetected"
                    ],
                    "antonyms": [
                        "seen",
                        "noticed",
                        "observed"
                    ],
                    "common_phrases": [
                        "go unobserved",
                        "remain unobserved"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не увиденный или не замеченный никем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незамеченный",
                                "target_lang_sense_clarification": "который не заметили; упущенный из виду"
                            },
                            {
                                "target_lang_word": "не наблюдаемый",
                                "target_lang_sense_clarification": "за которым не ведется наблюдение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "11056d70-5dbc-399a-84ee-5806c2228bf4",
                    "sense_definition": "Without being seen or noticed.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "not_seen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spy managed to slip into the building <w>unobserved</w>.",
                            "target_lang_translations": {
                                "ru": "Шпиону удалось проскользнуть в здание <w>незамеченным</w>."
                            }
                        },
                        {
                            "text": "She walked <w>unobserved</w> through the busy market.",
                            "target_lang_translations": {
                                "ru": "Она прошла <w>незамеченной</w> через оживленный рынок."
                            }
                        }
                    ],
                    "synonyms": [
                        "unnoticed",
                        "secretly"
                    ],
                    "antonyms": [
                        "openly",
                        "visibly"
                    ],
                    "common_phrases": [
                        "pass unobserved"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без того, чтобы быть увиденным или замеченным"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незамеченным",
                                "target_lang_sense_clarification": "в состоянии, когда никто не видит (отвечает на вопрос «как?» или «каким?»)"
                            },
                            {
                                "target_lang_word": "незаметно",
                                "target_lang_sense_clarification": "скрытно, без привлечения внимания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
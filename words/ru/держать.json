    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c4d72612-338b-3ae5-9577-79a373544aa5",
                    "sense_definition": "Взять и удерживать что-либо в руках, зубах или иным способом, не отпуская.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_grasp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама крепко <w>держала</w> маленького сына за руку, чтобы он не убежал на дорогу.",
                            "target_lang_translations": {
                                "en": "Mom was tightly <w>holding</w> her little son's hand so he wouldn't run onto the road."
                            }
                        },
                        {
                            "text": "Пожалуйста, <w>держи</w> эту тяжёлую сумку, пока я достаю ключи от квартиры.",
                            "target_lang_translations": {
                                "en": "Please <w>hold</w> this heavy bag while I get the apartment keys."
                            }
                        }
                    ],
                    "synonyms": [
                        "удерживать"
                    ],
                    "antonyms": [
                        "отпускать",
                        "бросать",
                        "ронять"
                    ],
                    "common_phrases": [
                        "держать за руку",
                        "держать в руках",
                        "крепко держать",
                        "держать вилку"
                    ],
                    "target_lang_definitions": {
                        "en": "To take and keep something in one's hands, teeth, or otherwise, without letting go."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "physical grasp"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0efe795-b9ab-3ff0-994e-e1c792104dc8",
                    "sense_definition": "Сохранять кого-либо или что-либо в определённом положении, состоянии или виде.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "maintain_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач посоветовал больному несколько дней <w>держать</w> ноги в тепле и не выходить на улицу.",
                            "target_lang_translations": {
                                "en": "The doctor advised the patient to <w>keep</w> his feet warm for a few days and not go outside."
                            }
                        },
                        {
                            "text": "Ты должен всегда <w>держать</w> спину прямо, когда сидишь за рабочим столом.",
                            "target_lang_translations": {
                                "en": "You must always <w>keep</w> your back straight when sitting at a desk."
                            }
                        },
                        {
                            "text": "Пожалуйста, <w>держи</w> дверь открытой, пока мы заносим мебель в комнату.",
                            "target_lang_translations": {
                                "en": "Please <w>hold</w> the door open while we carry the furniture into the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "сохранять",
                        "оставлять"
                    ],
                    "antonyms": [
                        "менять"
                    ],
                    "common_phrases": [
                        "держать в секрете",
                        "держать в курсе",
                        "держать себя в руках",
                        "держать дверь открытой",
                        "держать в чистоте"
                    ],
                    "target_lang_definitions": {
                        "en": "To preserve someone or something in a certain position, state, or form."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "maintain a state or condition"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "maintain a position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d837b245-d577-3631-bce9-ca58f80f2c35",
                    "sense_definition": "Хранить что-либо в определённом месте или иметь у себя в собственности (животных, вещи).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "store_own",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили не <w>держать</w> дома кошку, потому что у папы сильная аллергия.",
                            "target_lang_translations": {
                                "en": "We decided not to <w>keep</w> a cat at home because Dad has a severe allergy."
                            }
                        },
                        {
                            "text": "Где вы обычно <w>держите</w> свежие овощи зимой: в холодильнике или на балконе?",
                            "target_lang_translations": {
                                "en": "Where do you usually <w>keep</w> fresh vegetables in winter: in the fridge or on the balcony?"
                            }
                        }
                    ],
                    "synonyms": [
                        "хранить",
                        "иметь",
                        "содержать"
                    ],
                    "common_phrases": [
                        "держать собаку",
                        "держать деньги в банке",
                        "держать дома",
                        "держать в голове"
                    ],
                    "target_lang_definitions": {
                        "en": "To store something in a certain place or to have in one's ownership (animals, things)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "store or raise animals"
                            },
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "own or possess"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7ea95f6-6683-3bd4-85df-1cf5c91e2f26",
                    "sense_definition": "Не отпускать от себя, заставлять оставаться на месте, ограничивать свободу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "restrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция <w>держала</w> подозреваемого в камере предварительного заключения три дня.",
                            "target_lang_translations": {
                                "en": "The police <w>held</w> the suspect in a pre-trial detention cell for three days."
                            }
                        },
                        {
                            "text": "Никто тебя здесь силой не <w>держит</w>, ты можешь уйти в любой момент.",
                            "target_lang_translations": {
                                "en": "No one is <w>keeping</w> you here by force; you can leave at any moment."
                            }
                        }
                    ],
                    "synonyms": [
                        "удерживать",
                        "задерживать",
                        "не пускать"
                    ],
                    "antonyms": [
                        "отпускать",
                        "освобождать"
                    ],
                    "common_phrases": [
                        "держать в тюрьме",
                        "держать силой",
                        "держать взаперти"
                    ],
                    "target_lang_definitions": {
                        "en": "Not to let go from oneself, to force to stay in place, to limit freedom."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "detain or keep in custody"
                            },
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "restrain or prevent from leaving"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ac38658-d935-34d3-b561-5f75c9d6b182",
                    "sense_definition": "Служить физической опорой чему-либо, не давать упасть или разрушиться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти старые деревянные балки всё ещё надёжно <w>держат</w> тяжёлую крышу дома.",
                            "target_lang_translations": {
                                "en": "These old wooden beams still securely <w>hold</w> the heavy roof of the house."
                            }
                        },
                        {
                            "text": "Этот стул сломан и вряд ли <w>выдержит</w> вес взрослого человека.",
                            "target_lang_translations": {
                                "en": "This chair is broken and is unlikely to <w>hold</w> the weight of an adult."
                            }
                        }
                    ],
                    "synonyms": [
                        "поддерживать",
                        "нести"
                    ],
                    "common_phrases": [
                        "держать вес",
                        "держать удар",
                        "ноги не держат"
                    ],
                    "target_lang_definitions": {
                        "en": "To serve as a physical support for something, not letting it fall or collapse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "support weight"
                            },
                            {
                                "target_lang_word": "support",
                                "target_lang_sense_clarification": "bear the weight of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86f05ca8-0d85-3df0-9b82-862bedf27a74",
                    "sense_definition": "Выполнять обязательство или совершать действие (в устойчивых выражениях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idiomatic_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Настоящий мужчина всегда должен <w>держать</w> своё слово и выполнять данные обещания.",
                            "target_lang_translations": {
                                "en": "A real man must always <w>keep</w> his word and fulfill promises made."
                            }
                        },
                        {
                            "text": "Путники устали, но продолжали <w>держать</w> путь к далёким горам на горизонте.",
                            "target_lang_translations": {
                                "en": "The travelers were tired but continued to <w>make</w> their way toward the distant mountains on the horizon."
                            }
                        }
                    ],
                    "synonyms": [
                        "выполнять",
                        "соблюдать"
                    ],
                    "antonyms": [
                        "нарушать"
                    ],
                    "common_phrases": [
                        "держать слово",
                        "держать обещание",
                        "держать путь",
                        "держать совет"
                    ],
                    "target_lang_definitions": {
                        "en": "To fulfill an obligation or perform an action (in set expressions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "fulfill a promise or word"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93e48d80-906d-31bd-a6e4-2ae966c4f4e8",
                    "sense_definition": "Владеть предприятием, управлять бизнесом или заведением.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "business",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дядя уже много лет <w>держит</w> небольшую пекарню в центре нашего города.",
                            "target_lang_translations": {
                                "en": "My uncle has been <w>running</w> a small bakery in the center of our town for many years."
                            }
                        },
                        {
                            "text": "В прошлом веке купец <w>держал</w> здесь трактир для проезжающих путешественников.",
                            "target_lang_translations": {
                                "en": "In the last century, a merchant <w>kept</w> an inn here for passing travelers."
                            }
                        }
                    ],
                    "synonyms": [
                        "владеть",
                        "управлять",
                        "вести"
                    ],
                    "common_phrases": [
                        "держать магазин",
                        "держать лавку",
                        "держать ресторан"
                    ],
                    "target_lang_definitions": {
                        "en": "To own an enterprise, to manage a business or establishment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "manage a business"
                            },
                            {
                                "target_lang_word": "own",
                                "target_lang_sense_clarification": "possess a business"
                            },
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "manage an establishment (dated)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6047b072-2306-32cf-b283-72ecc58baecc",
                    "sense_definition": "Направляться, двигаться в определённую сторону (обычно о транспорте или движении).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "steer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель автобуса старался <w>держать</w> правее, чтобы пропустить скорую помощь.",
                            "target_lang_translations": {
                                "en": "The bus driver tried to <w>keep</w> to the right to let the ambulance pass."
                            }
                        },
                        {
                            "text": "Капитан приказал рулевому <w>держать</w> курс строго на север, несмотря на ветер.",
                            "target_lang_translations": {
                                "en": "The captain ordered the helmsman to <w>hold</w> the course strictly to the north, despite the wind."
                            }
                        }
                    ],
                    "synonyms": [
                        "направляться",
                        "двигаться"
                    ],
                    "common_phrases": [
                        "держать курс",
                        "держать правее",
                        "держать левее"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in transport or navigation contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To head, to move in a certain direction (usually about transport or movement)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "maintain direction (e.g., keep right)"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "maintain a course"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
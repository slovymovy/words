                    "sense_definition": "Third-person singular simple present indicative of <w>trouble</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>troubles</w> me that he has not replied to my message yet.",
                            "target_lang_translations": {
                                "ru": "Меня <w>беспокоит</w>, что он до сих пор не ответил на мое сообщение."
                            }
                        },
                        {
                            "text": "The noise from the street <w>troubles</w> her when she tries to sleep.",
                            "target_lang_translations": {
                                "ru": "Шум с улицы <w>мешает</w> ей, когда она пытается уснуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "it troubles me",
                        "what troubles you"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "third-person singular"
                        }
                    ],
                    "target_lang_definitions": {

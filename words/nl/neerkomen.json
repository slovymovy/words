    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "04a5ed92-7df3-34c0-aecf-6c6d897a0a03",
                    "sense_definition": "Omlaag vallen of op de grond terechtkomen na een val of sprong.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De parachutist <kwam> veilig <neer> in het grote weiland.",
                            "target_lang_translations": {
                                "en": "The parachutist <landed> safely in the large meadow.",
                                "ru": "Парашютист благополучно <приземлился> на большом лугу."
                            }
                        },
                        {
                            "text": "Hij liet de zware hamer met een harde klap op de tafel <neerkomen>.",
                            "target_lang_translations": {
                                "en": "He let the heavy hammer <come down> with a loud bang on the table.",
                                "ru": "Он с громким стуком <опустил> тяжелый молоток на стол."
                            }
                        }
                    ],
                    "synonyms": [
                        "landen",
                        "terechtkomen",
                        "vallen"
                    ],
                    "antonyms": [
                        "opstijgen",
                        "omhooggaan"
                    ],
                    "common_phrases": [
                        "hard neerkomen",
                        "zacht neerkomen",
                        "op zijn pootjes neerkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fall down or land on the ground after a fall or jump.",
                        "ru": "Падать вниз или приземляться на землю после падения или прыжка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "land",
                                "target_lang_sense_clarification": "to arrive on the ground safely"
                            },
                            {
                                "target_lang_word": "come down",
                                "target_lang_sense_clarification": "to fall or descend forcefully"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приземляться",
                                "target_lang_sense_clarification": "опускаться на землю после полета или прыжка"
                            },
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "падать или снижаться на поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "369805d6-1454-32a0-9020-99917b6df85f",
                    "sense_definition": "Uiteindelijk betekenen of als conclusie hebben; in de kern hetzelfde zijn als.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <komt> erop <neer> dat we morgen vroeger moeten beginnen.",
                            "target_lang_translations": {
                                "en": "It <comes down> to the fact that we have to start earlier tomorrow.",
                                "ru": "Это <сводится> к тому, что завтра нам нужно начать раньше."
                            }
                        },
                        {
                            "text": "De extra kosten zullen <neerkomen> op ongeveer vijftig euro per persoon.",
                            "target_lang_translations": {
                                "en": "The extra costs will <amount> to about fifty euros per person.",
                                "ru": "Дополнительные расходы будут <составлять> около пятидесяти евро на человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "betekenen",
                        "inhouden",
                        "resulteren in"
                    ],
                    "common_phrases": [
                        "neerkomen op",
                        "het komt erop neer"
                    ],
                    "target_lang_definitions": {
                        "en": "To ultimately mean or result in; to essentially be the same as.",
                        "ru": "В конечном итоге означать или приводить к выводу; по сути быть тем же самым."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come down to",
                                "target_lang_sense_clarification": "to depend on or mean ultimately"
                            },
                            {
                                "target_lang_word": "boil down to",
                                "target_lang_sense_clarification": "to be the essential outcome or summary"
                            },
                            {
                                "target_lang_word": "amount to",
                                "target_lang_sense_clarification": "regarding costs or totals"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сводиться к",
                                "target_lang_sense_clarification": "зависеть от чего-либо или означать в итоге"
                            },
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "о денежных суммах или итогах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "сход",
        "схождение",
        "сошествие"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d1e2ff32-08bb-3b4e-91b4-b3e95bd875ae",
                    "sense_definition": "Спуститься вниз (по лестнице, с горы, с высоты).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы осторожно <сошли> с крутого холма к реке.",
                            "target_lang_translations": {
                                "en": "We carefully <went down> the steep hill to the river."
                            }
                        },
                        {
                            "text": "Гости медленно <сошли> вниз по широкой лестнице.",
                            "target_lang_translations": {
                                "en": "The guests slowly <came down> the wide staircase."
                            }
                        }
                    ],
                    "synonyms": [
                        "спуститься"
                    ],
                    "antonyms": [
                        "подняться",
                        "взойти́"
                    ],
                    "common_phrases": [
                        "сойти вниз",
                        "сойти по лестнице"
                    ],
                    "target_lang_definitions": {
                        "en": "To move from a higher place to a lower one (down stairs, from a hill, from a height)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "general movement to a lower place"
                            },
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "come down",
                                "target_lang_sense_clarification": "movement towards the speaker"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81177420-823e-3012-a541-cb0e5d4fbaef",
                    "sense_definition": "Выйти из транспортного средства (автобуса, поезда, вагона).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно <сойти> на следующей остановке.",
                            "target_lang_translations": {
                                "en": "We need to <get off> at the next stop."
                            }
                        },
                        {
                            "text": "Пассажиры быстро <сошли> с поезда на перрон.",
                            "target_lang_translations": {
                                "en": "The passengers quickly <got off> the train onto the platform."
                            }
                        }
                    ],
                    "synonyms": [
                        "выйти",
                        "вылезти"
                    ],
                    "antonyms": [
                        "сесть"
                    ],
                    "common_phrases": [
                        "сойти на остановке",
                        "сойти с поезда",
                        "сойти на берег"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a vehicle (bus, train, carriage)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get off",
                                "target_lang_sense_clarification": "standard for public transport"
                            },
                            {
                                "target_lang_word": "step off",
                                "target_lang_sense_clarification": "action of stepping down from a vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2e7c81a-d1b3-384b-add6-1dc63f4e3ffb",
                    "sense_definition": "Оказаться удовлетворительным, приемлемым, подходящим (обычно в разговорной речи).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "acceptability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не старайся сделать идеально, и так <сойдёт>.",
                            "target_lang_translations": {
                                "en": "Don't try to make it perfect, it <will do> as it is."
                            }
                        },
                        {
                            "text": "Для домашней одежды эта старая футболка вполне <сойдёт>.",
                            "target_lang_translations": {
                                "en": "For home clothes, this old T-shirt <will do> just fine."
                            }
                        }
                    ],
                    "common_phrases": [
                        "и так сойдёт",
                        "сойдёт для начала"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'и так сойдёт'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be satisfactory, acceptable, or suitable (usually colloquial)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "be sufficient or good enough"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "be accepted as adequate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7023bf3f-f647-38c3-8a90-0212981f96c6",
                    "sense_definition": "Исчезнуть, удалиться с поверхности (о снеге, краске, пятнах, загаре).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "disappearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весной снег быстро <сошёл> с полей.",
                            "target_lang_translations": {
                                "en": "In spring, the snow quickly <disappeared> from the fields."
                            }
                        },
                        {
                            "text": "Пятно от вина не <сошло> даже после стирки.",
                            "target_lang_translations": {
                                "en": "The wine stain did not <come out> even after washing."
                            }
                        }
                    ],
                    "synonyms": [
                        "исчезнуть",
                        "слезть"
                    ],
                    "antonyms": [
                        "появиться"
                    ],
                    "common_phrases": [
                        "снег сошёл",
                        "краска сошла",
                        "загар сошёл",
                        "улыбка сошла"
                    ],
                    "target_lang_definitions": {
                        "en": "To disappear or be removed from a surface (of snow, paint, stains, tan)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come off",
                                "target_lang_sense_clarification": "detach from a surface (paint, skin)"
                            },
                            {
                                "target_lang_word": "come out",
                                "target_lang_sense_clarification": "be removed by washing (stains)"
                            },
                            {
                                "target_lang_word": "disappear",
                                "target_lang_sense_clarification": "vanish (snow)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7707dd0d-5399-36db-a63e-51b98822c18c",
                    "sense_definition": "Сбиться с пути, соскочить с колеи или рельсов (прямо и перен.); потерять рассудок (в идиомах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за аварии трамвай <сошёл> с рельсов.",
                            "target_lang_translations": {
                                "en": "Due to the accident, the tram <went off> the rails."
                            }
                        },
                        {
                            "text": "От такого шума можно просто <сойти> с ума.",
                            "target_lang_translations": {
                                "en": "From such noise one could simply <go> crazy."
                            }
                        },
                        {
                            "text": "Спортсмен был вынужден <сойти> с дистанции из-за травмы.",
                            "target_lang_translations": {
                                "en": "The athlete was forced to <drop out> of the race due to an injury."
                            }
                        }
                    ],
                    "synonyms": [
                        "соскочить"
                    ],
                    "antonyms": [
                        "устоять"
                    ],
                    "common_phrases": [
                        "сойти с ума",
                        "сойти с рельсов",
                        "сойти с дистанции",
                        "сойти с пути"
                    ],
                    "target_lang_definitions": {
                        "en": "To go off track/rails; to lose one's mind; to withdraw from a competition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go off",
                                "target_lang_sense_clarification": "derail (the rails)"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "become (crazy/mad)"
                            },
                            {
                                "target_lang_word": "drop out",
                                "target_lang_sense_clarification": "withdraw from a race or competition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fef64109-9f66-3d17-819e-50dec6fe80ea",
                    "sense_definition": "Показаться кем-то или чем-то, быть принятым за кого-то.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если ты будешь молчать, то вполне <сойдёшь> за умного.",
                            "target_lang_translations": {
                                "en": "If you keep silent, you might well <pass> for a smart person."
                            }
                        },
                        {
                            "text": "В темноте этот странный предмет мог <сойти> за оружие.",
                            "target_lang_translations": {
                                "en": "In the dark, this strange object could <pass> for a weapon."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сойти за своего",
                        "сойти за умного"
                    ],
                    "target_lang_definitions": {
                        "en": "To appear to be or be mistaken for someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass for",
                                "target_lang_sense_clarification": "be accepted or regarded as"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
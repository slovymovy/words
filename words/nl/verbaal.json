    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bcd45fac-aaa8-360f-aba3-e68ed420a165",
                    "sense_definition": "Uitgedrukt met woorden; door middel van taal (in tegenstelling tot gebaren of fysieke acties).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is <w>verbaal</w> erg sterk en wint bijna elke discussie.",
                            "target_lang_translations": {
                                "en": "He is <w>verbally</w> very strong and wins almost every discussion.",
                                "ru": "Он очень силен <w>на словах</w> и выигрывает почти каждый спор."
                            }
                        },
                        {
                            "text": "De ruzie bleef gelukkig beperkt tot <w>verbaal</w> geweld en werd niet fysiek.",
                            "target_lang_translations": {
                                "en": "Fortunately, the argument was limited to <w>verbal</w> abuse and did not become physical.",
                                "ru": "К счастью, ссора ограничилась <w>словесной</w> перепалкой и не переросла в физическую."
                            }
                        }
                    ],
                    "synonyms": [
                        "woordelijk"
                    ],
                    "antonyms": [
                        "non-verbaal"
                    ],
                    "common_phrases": [
                        "verbaal geweld",
                        "verbaal begaafd",
                        "verbale communicatie",
                        "verbaal sterk"
                    ],
                    "target_lang_definitions": {
                        "en": "Expressed with words; by means of language (as opposed to gestures or physical actions).",
                        "ru": "Выраженный словами; осуществляемый с помощью языка (в отличие от жестов или физических действий)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "verbal",
                                "target_lang_sense_clarification": "relating to words or speech"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "словесный",
                                "target_lang_sense_clarification": "выраженный словами"
                            },
                            {
                                "target_lang_word": "вербальный",
                                "target_lang_sense_clarification": "относящийся к речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a2f25d7-095c-3de9-82d5-9644bcdbd12a",
                    "sense_definition": "Betrekking hebbend op een werkwoord (grammaticale term).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>verbale</w> gezegde in deze zin bestaat uit drie woorden.",
                            "target_lang_translations": {
                                "en": "The <w>verbal</w> predicate in this sentence consists of three words.",
                                "ru": "<w>Глагольное</w> сказуемое в этом предложении состоит из трех слов."
                            }
                        },
                        {
                            "text": "In de taalkunde bestuderen we de <w>verbale</w> aspecten van de zin.",
                            "target_lang_translations": {
                                "en": "In linguistics, we study the <w>verbal</w> aspects of the sentence.",
                                "ru": "В лингвистике мы изучаем <w>глагольные</w> аспекты предложения."
                            }
                        }
                    ],
                    "synonyms": [
                        "werkwoordelijk"
                    ],
                    "common_phrases": [
                        "verbaal gezegde",
                        "verbaal adjectief"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a verb (grammatical term).",
                        "ru": "Относящийся к глаголу (грамматический термин)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "verbal",
                                "target_lang_sense_clarification": "grammatical term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "глагольный",
                                "target_lang_sense_clarification": "грамматика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4eafcf82-654c-3510-8d52-2705c55debdc",
                    "sense_definition": "Een officieel schriftelijk verslag van de politie van een overtreding; vaak gebruikt als synoniem voor een bekeuring.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De agent maakte <w>verbaal</w> op tegen de automobilist die door rood reed.",
                            "target_lang_translations": {
                                "en": "The officer drew up an <w>official report</w> against the motorist who drove through a red light.",
                                "ru": "Полицейский составил <w>протокол</w> на водителя, проехавшего на красный свет."
                            }
                        },
                        {
                            "text": "Hij kreeg een <w>verbaal</w> omdat hij zijn hond niet had aangelijnd.",
                            "target_lang_translations": {
                                "en": "He received a <w>ticket</w> because he had not leashed his dog.",
                                "ru": "Он получил <w>штраф</w>, потому что его собака была без поводка."
                            }
                        }
                    ],
                    "synonyms": [
                        "proces-verbaal",
                        "bekeuring"
                    ],
                    "common_phrases": [
                        "verbaal opmaken",
                        "een verbaal krijgen"
                    ],
                    "target_lang_definitions": {
                        "en": "An official written police report of an offense; often used as a synonym for a fine.",
                        "ru": "Официальный письменный отчет полиции о нарушении; часто используется как синоним штрафа."
                    },
                    "translations": {
                        "en": [
                            {

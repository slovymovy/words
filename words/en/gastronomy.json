    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9701f1e4-a89c-35bd-a141-493e877e8464",
                    "sense_definition": "The art and practice of choosing, preparing, and eating good food.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "art_of_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "French <w>gastronomy</w> is celebrated around the world for its sophistication.",
                            "target_lang_translations": {
                                "ru": "Французская <w>гастрономия</w> славится во всем мире своей изысканностью."
                            }
                        },
                        {
                            "text": "Modern <w>gastronomy</w> often combines science with traditional cooking techniques.",
                            "target_lang_translations": {
                                "ru": "Современная <w>гастрономия</w> часто сочетает науку с традиционными методами приготовления."
                            }
                        }
                    ],
                    "synonyms": [
                        "culinary art",
                        "cookery"
                    ],
                    "common_phrases": [
                        "molecular gastronomy",
                        "fine gastronomy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусство и практика выбора, приготовления и наслаждения качественной едой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гастрономия",
                                "target_lang_sense_clarification": "искусство изысканной еды"
                            },
                            {
                                "target_lang_word": "кулинария",
                                "target_lang_sense_clarification": "искусство приготовления пищи"
                            },
                            {
                                "target_lang_word": "кулинарное искусство",
                                "target_lang_sense_clarification": "мастерство готовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad89e72c-f049-3713-a5ea-ed5315974622",
                    "sense_definition": "The style of cooking and food characteristic of a particular region.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "regional_cuisine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>gastronomy</w> of Mexico is world-famous for its spices and flavors.",
                            "target_lang_translations": {
                                "ru": "<w>Гастрономия</w> Мексики известна во всем мире своими специями и вкусами."
                            }
                        },
                        {
                            "text": "Tourists often travel specifically to experience the local <w>gastronomy</w>.",
                            "target_lang_translations": {
                                "ru": "Туристы часто путешествуют специально, чтобы познакомиться с местной <w>гастрономией</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cuisine",
                        "cooking"
                    ],
                    "common_phrases": [
                        "local gastronomy",
                        "regional gastronomy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стиль приготовления пищи и продукты, характерные для определенного региона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухня",
                                "target_lang_sense_clarification": "кулинарные традиции региона"
                            },
                            {
                                "target_lang_word": "гастрономия",
                                "target_lang_sense_clarification": "совокупность региональных блюд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eaba4ac0-f402-37ba-855b-08f5eec43b81",
                    "sense_definition": "The academic study of the relationship between food and culture.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "art_of_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her degree in <w>gastronomy</w> explored how history shapes our diet.",
                            "target_lang_translations": {
                                "ru": "Ее дипломная работа по <w>гастрономии</w> исследовала, как история формирует наш рацион."
                            }
                        },
                        {
                            "text": "The university offers a specialized course in <w>gastronomy</w> and food science.",
                            "target_lang_translations": {
                                "ru": "Университет предлагает специализированный курс по <w>гастрономии</w> и науке о питании."
                            }
                        }
                    ],
                    "synonyms": [
                        "food studies"
                    ],
                    "common_phrases": [
                        "history of gastronomy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Научная дисциплина, изучающая связь между едой и культурой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гастрономия",
                                "target_lang_sense_clarification": "наука о культуре питания"
                            }
                        ]
                    }
                }
            ]
        }
    ]

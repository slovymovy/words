{
    "word_family": [
        "merchandise",
        "mercantile",
        "merchantable",
        "merchantman"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d58f4162-856d-3d03-90eb-bbce2819d0ad",
                    "sense_definition": "A person or business involved in the buying and selling of goods for profit, especially on a large scale or with foreign countries.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "business_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy <w>merchant</w> imported fine silks and spices from the East.",
                            "target_lang_translations": {
                                "ru": "Богатый <w>купец</w> ввозил прекрасные шелка и специи с Востока."
                            }
                        },
                        {
                            "text": "My grandfather worked as a coal <w>merchant</w> for forty years.",
                            "target_lang_translations": {
                                "ru": "Мой дед сорок лет работал угольным <w>торговцем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trader",
                        "dealer",
                        "seller",
                        "vendor"
                    ],
                    "antonyms": [
                        "purchaser"
                    ],
                    "common_phrases": [
                        "wine merchant",
                        "timber merchant",
                        "wholesale merchant",
                        "merchant bank",
                        "merchant navy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или компания, занимающиеся куплей-продажей товаров с целью получения прибыли, особенно в крупных масштабах или с зарубежными странами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговец",
                                "target_lang_sense_clarification": "общее название для человека, занимающегося торговлей"
                            },
                            {
                                "target_lang_word": "купец",
                                "target_lang_sense_clarification": "исторический термин или крупный торговец"
                            },
                            {
                                "target_lang_word": "оптовик",
                                "target_lang_sense_clarification": "торговец, занимающийся оптовыми продажами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b9d2e4b-57d6-32c7-b356-ee5d8f4aca16",
                    "sense_definition": "The owner or operator of a retail business or shop.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every local <w>merchant</w> decorated their shop window for the holiday season.",
                            "target_lang_translations": {
                                "ru": "Каждый местный <w>торговец</w> украсил витрину своего магазина к праздникам."
                            }
                        },
                        {
                            "text": "You can pay by credit card at any participating <w>merchant</w> in the city.",
                            "target_lang_translations": {
                                "ru": "Вы можете расплатиться кредитной картой у любого участвующего в программе <w>продавца</w> в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "shopkeeper",
                        "retailer",
                        "storekeep"
                    ],
                    "common_phrases": [
                        "local merchant",
                        "retail merchant",
                        "authorized merchant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владелец или управляющий розничным торговым предприятием или магазином."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговец",
                                "target_lang_sense_clarification": "владелец торговой точки"
                            },
                            {
                                "target_lang_word": "лавочник",
                                "target_lang_sense_clarification": "владелец небольшой лавки или магазина"
                            },
                            {
                                "target_lang_word": "владелец магазина",
                                "target_lang_sense_clarification": "собственник розничного бизнеса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec0c6290-6e5b-34da-92b8-740a13a9eac3",
                    "sense_definition": "Someone who is known for a specific type of activity or behavior, often used in negative or humorous descriptions.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He drives far too fast and is known as a speed <w>merchant</w>.",
                            "target_lang_translations": {
                                "ru": "Он ездит слишком быстро, и его называют <w>лихачом</w>."
                            }
                        },
                        {
                            "text": "Don't listen to him because he is just a doom <w>merchant</w>.",
                            "target_lang_translations": {
                                "ru": "Не слушай его, он просто <w>паникёр</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speed merchant",
                        "doom merchant",
                        "gossip merchant"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe someone by their habit"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, известный определенным видом деятельности или поведения (часто используется в негативном или шутливом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любитель",
                                "target_lang_sense_clarification": "человек, увлекающийся чем-либо (например, скоростью)"
                            },
                            {
                                "target_lang_word": "распространитель",
                                "target_lang_sense_clarification": "тот, кто распространяет что-либо (например, слухи или панику)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3df5af03-1d46-3614-903e-34c127c88676",
                    "sense_definition": "A ship used for commerce and transporting goods.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The armed <w>merchant</w> sailed across the ocean with a heavy cargo.",
                            "target_lang_translations": {
                                "ru": "Вооружённое <w>торговое судно</w> плыло через океан с тяжёлым грузом."
                            }
                        },
                        {
                            "text": "Pirates attacked the lone <w>merchant</w> near the coast.",
                            "target_lang_translations": {
                                "ru": "Пираты напали на одинокое <w>торговое судно</w> у побережья."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'merchant ship' or 'merchantman'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Судно, используемое для коммерции и перевозки грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговое судно",
                                "target_lang_sense_clarification": "корабль торгового флота"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "559ce8f5-2b3e-3fcd-a800-37345753e3ee",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We invited Mr. <w>Merchant</w> to speak at the conference.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили мистера <w>Мерчанта</w> выступить на конференции."
                            }
                        },
                        {
                            "text": "The <w>Merchant</w> family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Мерчант</w> живет в этом городе уже несколько поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мерчант",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ff463b46-17a6-319e-ac33-37f34e5a7251",
                    "sense_definition": "To buy and sell goods, specifically involving trade between non-residents.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company specializes in <w>merchanting</w> steel to international buyers.",
                            "target_lang_translations": {
                                "ru": "Компания специализируется на <w>торговле</w> сталью с международными покупателями."
                            }
                        },
                        {
                            "text": "They <w>merchanted</w> the grain efficiently across the border.",
                            "target_lang_translations": {
                                "ru": "Они эффективно <w>продавали</w> зерно за границу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покупать и продавать товары, особенно подразумевая торговлю между нерезидентами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговать",
                                "target_lang_sense_clarification": "заниматься куплей-продажей"
                            },
                            {
                                "target_lang_word": "продавать",
                                "target_lang_sense_clarification": "реализовывать товар"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
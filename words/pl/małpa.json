                {
                    "sense_id": "59430244-c164-333d-bc44-de48af2ee3f2",
                    "sense_definition": "Znak typograficzny @ używany powszechnie w adresach poczty elektronicznej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój adres e-mail to jan kropka kowalski <w>małpa</w> gmail kropka com.",
                            "target_lang_translations": {
                                "ru": "Мой адрес электронной почты: ян точка ковальский <w>собака</w> джимэйл точка ком."
                            }
                        },
                        {
                            "text": "Pamiętaj, żeby w każdym adresie mailowym wpisać znak <w>małpy</w>.",
                            "target_lang_translations": {
                                "ru": "Помни, что в каждом адресе электронной почты нужно писать знак <w>собаки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "at"

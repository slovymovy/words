    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e41c2e6e-52a2-3748-ac05-91b8e9b62801",
                    "sense_definition": "Wziąć coś do ręki, chwycić lub przemieścić coś w inne miejsce; wziąć coś ze sobą.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <zabrać> parasol, ponieważ na zewnątrz pada deszcz.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>взять</w> зонт, так как на улице идет дождь."
                            }
                        },
                        {
                            "text": "Proszę <zabrać> swoje rzeczy ze stołu i położyć je na półce.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>заберите</w> свои вещи со стола и положите их на полку."
                            }
                        }
                    ],
                    "synonyms": [
                        "wziąć"
                    ],
                    "antonyms": [
                        "zostawić",
                        "odłożyć"
                    ],
                    "common_phrases": [
                        "zabrać ze sobą",
                        "zabrać głos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять что-то в руки, схватить или переместить что-то в другое место; взять что-то с собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "взять себе, унести или переместить"
                            },
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "взять с собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b11b7b63-4b97-3346-9f5a-c733ff02588e",
                    "sense_definition": "Pozwolić komuś pójść lub pojechać ze sobą; podwieźć kogoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ojciec obiecał <zabrać> dzieci w niedzielę do zoo.",
                            "target_lang_translations": {
                                "ru": "Отец обещал <w>взять</w> детей в воскресенье в зоопарк."
                            }
                        },
                        {
                            "text": "Czy możesz mnie <zabrać> samochodem do centrum miasta?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>подвезти</w> меня на машине в центр города?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wziąć"
                    ],
                    "antonyms": [
                        "zostawić"
                    ],
                    "common_phrases": [
                        "zabrać kogoś na stopa",
                        "zabrać pasażera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволить кому-то пойти или поехать с собой; подвезти кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "позволить пойти/поехать с собой"
                            },
                            {
                                "target_lang_word": "подвезти",
                                "target_lang_sense_clarification": "подвезти на транспорте"
                            },
                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "заехать за кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49e1ddd7-6361-38ce-8009-8dd6d67dc84c",
                    "sense_definition": "Pozbawić kogoś czegoś, przywłaszczyć sobie coś lub odebrać coś komuś (siłą lub prawem).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <zabrał> uczniowi telefon, ponieważ używał go na lekcji.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>забрал</w> у ученика телефон, так как он пользовался им на уроке."
                            }
                        },
                        {
                            "text": "Policja musiała mu <zabrać> prawo jazdy za jazdę pod wpływem alkoholu.",
                            "target_lang_translations": {
                                "ru": "Полиция была вынуждена <w>отобрать</w> у него водительские права за вождение в нетрезвом виде."
                            }
                        }
                    ],
                    "synonyms": [
                        "odebrać",
                        "pozbawić",
                        "skonfiskować"
                    ],
                    "antonyms": [
                        "oddać",
                        "zwrócić"
                    ],
                    "common_phrases": [
                        "zabrać siłą",
                        "zabrać wszystko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить кого-то чего-то, присвоить себе что-то или отобрать что-то у кого-то (силой или по закону)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отобрать",
                                "target_lang_sense_clarification": "отнять силой или принудительно"
                            },
                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "присвоить или конфисковать"
                            },
                            {
                                "target_lang_word": "лишить",
                                "target_lang_sense_clarification": "оставить без чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9081f23f-ca3f-3df7-b30e-25b6319a1b33",
                    "sense_definition": "Zająć określoną ilość czasu; trwać.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprawa twojego samochodu <zabierze> mechanikowi co najmniej dwa dni.",
                            "target_lang_translations": {
                                "ru": "Ремонт твоей машины <w>займет</w> у механика не менее двух дней."
                            }
                        },
                        {
                            "text": "To zadanie <zabrało> nam znacznie więcej czasu, niż planowaliśmy.",
                            "target_lang_translations": {
                                "ru": "Это задание <w>отняло</w> у нас гораздо больше времени, чем мы планировали."
                            }
                        }
                    ],
                    "synonyms": [
                        "zająć"
                    ],
                    "antonyms": [
                        "zaoszczędzić"
                    ],
                    "common_phrases": [
                        "zabrać chwilę",
                        "zabrać dużo czasu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занять определенное количество времени; длиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занять",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "отнять",
                                "target_lang_sense_clarification": "потратить время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec725da4-4584-3f12-a078-9fc2c50aa9e0",
                    "sense_definition": "Spowodować czyjąś śmierć lub zniszczenie (zwykle o chorobie, wojnie lub żywiole).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Straszna powódź <zabrała> mieszkańcom wioski cały ich dobytek.",
                            "target_lang_translations": {
                                "ru": "Страшное наводнение <w>унесло</w> у жителей деревни все их имущество."
                            }
                        },
                        {
                            "text": "Druga wojna światowa <zabrała> miliony istnień ludzkich.",
                            "target_lang_translations": {
                                "ru": "Вторая мировая война <w>унесла</w> миллионы человеческих жизней."
                            }
                        }
                    ],
                    "synonyms": [
                        "uśmiercić",
                        "zabić"
                    ],
                    "antonyms": [
                        "ocalić"
                    ],
                    "common_phrases": [
                        "zabrać życie",
                        "zabrać nadzieję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести к чьей-то смерти или разрушению (обычно о болезни, войне или стихии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "унести",
                                "target_lang_sense_clarification": "о жизни или имуществе (в результате бедствия)"
                            },
                            {
                                "target_lang_word": "погубить",
                                "target_lang_sense_clarification": "привести к смерти"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
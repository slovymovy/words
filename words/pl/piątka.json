    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ef2a27b-1488-3bd0-af7f-e980e28c72c8",
                    "sense_definition": "Nazwa cyfry 5 lub jej zapis graficzny.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "digits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mała dziewczynka narysowała w zeszycie dużą i kolorową <w>piątkę</w>.",
                            "target_lang_translations": {
                                "ru": "Маленькая девочка нарисовала в тетради большую и цветную <w>пятёрку</w>."
                            }
                        },
                        {
                            "text": "Mój numer domu to <w>piątka</w>, więc łatwo do mnie trafisz.",
                            "target_lang_translations": {
                                "ru": "Мой номер дома — <w>пятёрка</w>, так что ты легко меня найдёшь."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название цифры 5 или её графическое изображение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятёрка",
                                "target_lang_sense_clarification": "цифра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80028176-cf69-32cd-9610-def3fa62e160",
                    "sense_definition": "Ocena szkolna oznaczająca wynik bardzo dobry.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grades",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z ostatniego sprawdzianu z matematyki dostałem zasłużoną <w>piątkę</w>.",
                            "target_lang_translations": {
                                "ru": "За последнюю контрольную по математике я получил заслуженную <w>пятёрку</w>."
                            }
                        },
                        {
                            "text": "Rodzice obiecali mi nagrodę, jeśli na koniec roku będę miał same <w>piątki</w>.",
                            "target_lang_translations": {
                                "ru": "Родители обещали мне награду, если в конце года у меня будут одни <w>пятёрки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bardzo dobry",
                        "bdb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Школьная оценка, означающая «отлично» (высший балл)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятёрка",
                                "target_lang_sense_clarification": "оценка"
                            },
                            {
                                "target_lang_word": "пять",
                                "target_lang_sense_clarification": "балл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e467feb-a0f2-33f6-9d03-42752e1d30c6",
                    "sense_definition": "Rzecz oznaczona numerem 5, na przykład autobus, tramwaj lub bieg w samochodzie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "numbered_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano jeżdżę do pracy tramwajem numer <w>piątka</w>.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я езжу на работу на <w>пятёрке</w> (трамвае номер пять)."
                            }
                        },
                        {
                            "text": "Kierowca wrzucił <w>piątkę</w> i samochód zaczął jechać znacznie szybciej.",
                            "target_lang_translations": {
                                "ru": "Водитель включил <w>пятую</w> передачу, и машина поехала значительно быстрее."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или объект под номером 5, например автобус, трамвай или передача в автомобиле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятёрка",
                                "target_lang_sense_clarification": "номер маршрута, транспорт"
                            },
                            {
                                "target_lang_word": "пятая",
                                "target_lang_sense_clarification": "передача (в автомобиле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36da829f-b5ca-332f-832c-e504bcf74d22",
                    "sense_definition": "Grupa składająca się z pięciu osób lub elementów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "groups",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza zgrana <w>piątka</w> przyjaciół zawsze spędza wakacje razem.",
                            "target_lang_translations": {
                                "ru": "Наша дружная <w>пятёрка</w> друзей всегда проводит каникулы вместе."
                            }
                        },
                        {
                            "text": "Trener wybrał startową <w>piątkę</w> zawodników na dzisiejszy mecz koszykówki.",
                            "target_lang_translations": {
                                "ru": "Тренер выбрал стартовую <w>пятёрку</w> игроков на сегодняшний баскетбольный матч."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа, состоящая из пяти человек или элементов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятёрка",
                                "target_lang_sense_clarification": "группа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "229c2957-c0bc-3168-ac8e-41334305cb18",
                    "sense_definition": "Gest dłonią oznaczający powitanie, gratulacje lub radość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gestures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy zdałem egzamin, kolega z radością przybił mi <w>piątkę</w>.",
                            "target_lang_translations": {
                                "ru": "Когда я сдал экзамен, друг радостно дал мне <w>пять</w>."
                            }
                        },
                        {
                            "text": "Daj <w>piątkę</w> na pożegnanie, bo muszę już uciekać do domu.",
                            "target_lang_translations": {
                                "ru": "Дай <w>пять</w> на прощание, потому что мне уже пора бежать домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przybić piątkę",
                        "dać piątkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal gesture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жест ладонью, выражающий приветствие, поздравление или радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пять",
                                "target_lang_sense_clarification": "в выражении «дать пять»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9814a1fe-57e1-3bcc-9ad1-2611ffbad71d",
                    "sense_definition": "W piłce nożnej: pole bramkowe, obszar wyznaczony linią w odległości 5,5 metra od bramki.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz ustawił piłkę na <w>piątce</w> i mocno wykopał ją w stronę połowy boiska.",
                            "target_lang_translations": {
                                "ru": "Вратарь установил мяч во <w>вратарской</w> и сильно выбил его в сторону половины поля."
                            }
                        },
                        {
                            "text": "Obrońcy drużyny przeciwnej zgromadzili się w obrębie <w>piątki</w>, blokując dostęp do bramki.",
                            "target_lang_translations": {
                                "ru": "Защитники команды соперника столпились во <w>вратарской площадке</w>, блокируя доступ к воротам."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedpole"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "sports jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В футболе: вратарская площадка, зона перед воротами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вратарская площадка",
                                "target_lang_sense_clarification": "футбольный термин"
                            },
                            {
                                "target_lang_word": "вратарская",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

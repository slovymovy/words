                {
                    "sense_id": "1dac74ca-c92e-3850-a9a8-ae41bf575311",
                    "sense_definition": "The layer of gases surrounding the Earth or any other planet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rocket burned up as it entered the Earth's <w>atmosphere</w>.",
                            "target_lang_translations": {
                                "ru": "Ракета сгорела при входе в <w>атмосферу</w> Земли."
                            }
                        },
                        {
                            "text": "Scientists are studying the <w>atmosphere</w> of Mars to see if it can support life.",
                            "target_lang_translations": {
                                "ru": "Ученые изучают <w>атмосферу</w> Марса, чтобы понять, может ли она поддерживать жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "air"
                    ],
                    "common_phrases": [

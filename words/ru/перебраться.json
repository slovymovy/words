            "senses": [
                {
                    "sense_id": "b7a82c2c-5903-3c84-8ebe-dc1b5246291f",
                    "sense_definition": "Переехать на новое место жительства или пребывания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "move_residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Семья решила <перебраться> в деревню, чтобы жить ближе к природе.",
                            "target_lang_translations": {
                                "en": "The family decided to <w>move</w> to the village to live closer to nature."
                            }
                        },
                        {
                            "text": "Нам пришлось <перебраться> на новую квартиру из-за ремонта.",
                            "target_lang_translations": {
                                "en": "We had to <w>move</w> to a new apartment because of the renovations."
                            }
                        }
                    ],
                    "synonyms": [
                        "переехать",
                        "переселиться"
                    ],
                    "antonyms": [
                        "остаться"
                    ],
                    "common_phrases": [
                        "перебраться в город",
                        "перебраться на квартиру",
                        "перебраться в дом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in spoken language instead of 'переехать'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move to a new place of residence or stay."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to a new home"
                            },
                            {
                                "target_lang_word": "relocate",
                                "target_lang_sense_clarification": "formal, change residence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb27943c-95f7-344a-9f19-c2e659bd6662",
                    "sense_definition": "Переместиться на другую сторону чего-либо, преодолевая препятствие (реку, забор, гору и т.п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cross_obstacle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристам удалось <перебраться> через бурную реку только к вечеру.",
                            "target_lang_translations": {
                                "en": "The tourists managed to <w>get across</w> the turbulent river only by evening."
                            }
                        },
                        {
                            "text": "Кот ловко сумел <перебраться> через высокий забор в соседний сад.",
                            "target_lang_translations": {
                                "en": "The cat managed to deftly <w>climb over</w> the high fence into the neighbor's garden."
                            }
                        }
                    ],
                    "synonyms": [
                        "переправиться",
                        "перейти",
                        "перелезть"
                    ],
                    "common_phrases": [
                        "перебраться через реку",
                        "перебраться на берег",
                        "перебраться через забор"
                    ],
                    "target_lang_definitions": {
                        "en": "To move to the other side of something, often overcoming an obstacle (river, fence, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get across",
                                "target_lang_sense_clarification": "reach the other side"
                            },
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "traverse a river or area"
                            },
                            {
                                "target_lang_word": "climb over",
                                "target_lang_sense_clarification": "surmount a fence or wall"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "434940b3-93ba-3f37-8a52-7ff4af2cf004",
                    "sense_definition": "Переместиться с одного места на другое (обычно недалеко, в пределах одного пространства).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "move_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости решили <перебраться> из душной комнаты на свежий воздух в сад.",
                            "target_lang_translations": {
                                "en": "The guests decided to <w>move</w> from the stuffy room to the fresh air in the garden."
                            }
                        },
                        {
                            "text": "Бабушка попросила помочь ей <перебраться> в удобное кресло.",
                            "target_lang_translations": {
                                "en": "Grandma asked for help to <w>move</w> into a comfortable armchair."
                            }
                        }
                    ],
                    "synonyms": [
                        "переместиться",
                        "пересесть"
                    ],
                    "common_phrases": [
                        "перебраться поближе",
                        "перебраться в кресло"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move from one place to another nearby, usually within the same space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "change position"
                            },
                            {
                                "target_lang_word": "shift",
                                "target_lang_sense_clarification": "change place slightly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36b01589-af71-3621-9eff-db08427a7ac2",
                    "sense_definition": "Сменить шерсть или оперение (о животных и птицах); полинять.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "molt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К зиме заяц должен полностью <перебраться> и стать белым.",
                            "target_lang_translations": {
                                "en": "By winter, the hare must fully <w>molt</w> and turn white."
                            }
                        },
                        {
                            "text": "Молодые птицы успели <перебраться> до начала осеннего перелёта.",
                            "target_lang_translations": {
                                "en": "The young birds managed to <w>molt</w> before the start of the autumn migration."
                            }
                        }
                    ],
                    "synonyms": [
                        "полинять"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "hunting jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To shed fur or feathers (regarding animals and birds)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "molt",
                                "target_lang_sense_clarification": "shed feathers or fur"
                            },
                            {
                                "target_lang_word": "shed",
                                "target_lang_sense_clarification": "lose hair or fur"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "25891337-b37b-3e99-bfe3-15d4d0b74b9e",
                    "sense_definition": "A person who opposes or fights against people in authority or the government.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_resister",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A group of <w>rebels</w> defied the general's orders and split off from the main army.",
                            "target_lang_translations": {
                                "ru": "Группа <w>мятежников</w> нарушила приказ генерала и отделилась от основной армии."
                            }
                        },
                        {
                            "text": "My little sister is such a <w>rebel</w>, always refusing to do her chores and coming home late.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра — настоящая <w>бунтарка</w>, она вечно отказывается делать работу по дому и поздно приходит."
                            }
                        }
                    ],
                    "synonyms": [
                        "renegade"
                    ],
                    "common_phrases": [
                        "rebel forces",
                        "rebel leader",
                        "teenage rebel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который сопротивляется власти или установленному порядку; участник восстания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бунтарь",
                                "target_lang_sense_clarification": "человек с непокорным характером или подстрекатель"
                            },
                            {
                                "target_lang_word": "повстанец",
                                "target_lang_sense_clarification": "участник вооруженного восстания"
                            },
                            {
                                "target_lang_word": "мятежник",
                                "target_lang_sense_clarification": "участник мятежа, бунтовщик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7086652-5e9a-3bb7-a26d-40727adb6617",
                    "sense_definition": "A soldier or loyal citizen of the Confederate States of America during the American Civil War.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_confederate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The history book described how the <w>Rebels</w> defended their territory during the conflict.",
                            "target_lang_translations": {
                                "ru": "В учебнике истории описывалось, как <w>конфедераты</w> защищали свою территорию во время конфликта."
                            }
                        },
                        {
                            "text": "He found an old letter written by a <w>Rebel</w> soldier to his family.",
                            "target_lang_translations": {
                                "ru": "Он нашел старое письмо, написанное солдатом-<w>конфедератом</w> своей семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "Confederate",
                        "Johnny Reb",
                        "Reb"
                    ],
                    "common_phrases": [
                        "Rebel yell",
                        "Rebel flag"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "United States"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Солдат или сторонник Конфедерации южных штатов во время Гражданской войны в США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конфедерат",
                                "target_lang_sense_clarification": "исторический термин"
                            },
                            {
                                "target_lang_word": "мятежник",
                                "target_lang_sense_clarification": "прозвище южан, использовавшееся северянами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "faead915-81f9-3530-b428-9d3e8e18008a",
                    "sense_definition": "To oppose, fight against, or refuse to obey someone in a position of authority.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verb_resist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens began to <w>rebel</w> against the unfair tax laws imposed by the government.",
                            "target_lang_translations": {
                                "ru": "Граждане начали <w>восставать</w> против несправедливых налоговых законов, введенных правительством."
                            }
                        },
                        {
                            "text": "Teenagers often <w>rebel</w> against their parents' strict rules as they seek more independence.",
                            "target_lang_translations": {
                                "ru": "Подростки часто <w>бунтуют</w> против строгих родительских правил в поисках большей независимости."
                            }
                        }
                    ],
                    "synonyms": [
                        "defy"
                    ],
                    "antonyms": [
                        "obey",
                        "submit"
                    ],
                    "common_phrases": [
                        "rebel against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Противостоять власти, поднимать восстание или отказываться подчиняться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бунтовать",
                                "target_lang_sense_clarification": "выражать недовольство, протестовать"
                            },
                            {
                                "target_lang_word": "восставать",
                                "target_lang_sense_clarification": "поднимать восстание, бороться против чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8dcf09f6-2a1f-3160-8b3a-891c24a2b57f",
                    "sense_definition": "A surname of German origin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Rebel</w> signed the guestbook at the hotel reception.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Ребел</w> расписался в гостевой книге на стойке регистрации отеля."
                            }
                        },
                        {
                            "text": "We are looking for the family history of the <w>Rebel</w> lineage in this region.",
                            "target_lang_translations": {
                                "ru": "Мы ищем семейную историю рода <w>Ребел</w> в этом регионе."
                            }
                        }
                    ],

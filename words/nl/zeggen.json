                },
                {
                    "sense_id": "cbe298b2-efa6-3e71-b5bb-0c870df71f71",
                    "sense_definition": "Iemand dwingend opdragen iets te doen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "command",
                    "name_type": "no",

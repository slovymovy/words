                    "target_lang_definitions": {
                        "ru": "Взаимно уничтожиться или перестать действовать (о силах, величинах); используется с местоимением 'się'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уравновеситься",
                                "target_lang_sense_clarification": "о силах"
                            },
                            {
                                "target_lang_word": "компенсироваться",
                                "target_lang_sense_clarification": "взаимно погаситься"
                            }
                        ]
                    }
                }
            ]

                        "ru": [
                            {
                                "target_lang_word": "олух",
                                "target_lang_sense_clarification": "неуклюжий человек"
                            },
                            {
                                "target_lang_word": "хам",
                                "target_lang_sense_clarification": "грубиян"
                            }
                        ]
                    }
                }
            ]

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a9f622f-2ee0-3f70-9a37-7ddd6dba6617",
                    "sense_definition": "A mythical bird that lives for hundreds of years, burns itself to ashes, and is born again from them.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mythical_bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The legend says that the <w>phoenix</w> rises from its own ashes to live again.",
                            "target_lang_translations": {
                                "ru": "Легенда гласит, что <w>феникс</w> восстает из собственного пепла, чтобы жить снова."
                            }
                        },
                        {
                            "text": "In the story, the magical <w>phoenix</w> healed the hero with its tears.",
                            "target_lang_translations": {
                                "ru": "В этой истории волшебный <w>феникс</w> исцелил героя своими слезами."
                            }
                        }
                    ],
                    "synonyms": [
                        "firebird"
                    ],
                    "common_phrases": [
                        "rise like a phoenix",
                        "phoenix from the ashes"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mythological"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мифическая птица, которая живет сотни лет, сжигает себя и возрождается из пепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "феникс",
                                "target_lang_sense_clarification": "мифологическая птица"
                            },
                            {
                                "target_lang_word": "жар-птица",
                                "target_lang_sense_clarification": "аналог в русском фольклоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45f601a9-00cb-3cb1-96e4-8bb0519eb211",
                    "sense_definition": "A person or thing that has been restored to a new existence or status after apparent destruction.",

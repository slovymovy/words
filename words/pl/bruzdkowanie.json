    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e4b74d1c-682a-3378-b414-0c1f8e6af7bc",
                    "sense_definition": "Proces polegający na podziale zapłodnionej komórki jajowej (zygoty) na szereg mniejszych komórek potomnych (blastomerów).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biologia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczesne <bruzdkowanie> zarodka przebiega bardzo dynamicznie.",
                            "target_lang_translations": {
                                "ru": "Раннее <дробление> зародыша происходит очень динамично."
                            }
                        },
                        {
                            "text": "U ssaków występuje <bruzdkowanie> całkowite, które obejmuje całą komórkę jajową.",
                            "target_lang_translations": {
                                "ru": "У млекопитающих происходит полное <дробление>, охватывающее всю яйцеклетку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bruzdkowanie całkowite",
                        "bruzdkowanie częściowe",
                        "bruzdkowanie spiralne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс деления оплодотворенной яйцеклетки (зиготы) на ряд более мелких дочерних клеток (бластомеров)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дробление",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89785be9-85ce-3912-b23f-4bff4a22e106",
                    "sense_definition": "Ciąg małych rowków lub zagłębień widocznych na powierzchni przedmiotu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "struktura",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ozdobne <bruzdkowanie> na krawędzi monety zapobiegało jej opiłowywaniu.",
                            "target_lang_translations": {
                                "ru": "Декоративная <насечка> на ребре монеты предотвращала её опиливание."
                            }
                        },
                        {
                            "text": "Ceramiczna waza posiadała delikatne <bruzdkowanie>, które nadawało jej antyczny charakter.",
                            "target_lang_translations": {
                                "ru": "Керамическая ваза имела тонкое <рифление>, придававшее ей античный характер."
                            }
                        }
                    ],
                    "synonyms": [
                        "żłobkowanie"
                    ],
                    "common_phrases": [
                        "bruzdkowanie powierzchni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд мелких канавок или углублений, видимых на поверхности предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рифление",
                                "target_lang_sense_clarification": "узор на поверхности"
                            },
                            {
                                "target_lang_word": "насечка",
                                "target_lang_sense_clarification": "на монетах или инструментах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1a77aaa-57a5-3499-bc7c-10e8c484cc6e",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>bruzdkować</w>: czynność wykonywania rowków lub żłobień.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "czynnosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechaniczne <bruzdkowanie> metalu wymaga użycia precyzyjnych narzędzi.",
                            "target_lang_translations": {
                                "ru": "Механическое <рифление> металла требует использования точных инструментов."
                            }
                        },
                        {
                            "text": "Przez <bruzdkowanie> gleby rolnik przygotował pole pod zasiew.",
                            "target_lang_translations": {
                                "ru": "Путем <нарезки борозд> в почве фермер подготовил поле к посеву."
                            }
                        }
                    ],
                    "synonyms": [
                        "żłobkowanie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzeczownik odczasownikowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по созданию канавок или углублений; отглагольное существительное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарезка борозд",
                                "target_lang_sense_clarification": "в почве"
                            },
                            {
                                "target_lang_word": "рифление",
                                "target_lang_sense_clarification": "процесс обработки материала"
                            }
                        ]
                    }
                }
            ]
        }

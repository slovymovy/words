{
    "word_family": [
        "dyspozycyjny",
        "dyspozycyjność",
        "dysponować",
        "dysponent"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b3283c65-0b1f-333f-a7f3-b33b24da87af",
                    "sense_definition": "Możliwość swobodnego korzystania z kogoś lub czegoś; stan bycia dostępnym do użycia lub pomocy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód służbowy jest do twojej <dyspozycji> przez całą dobę.",
                            "target_lang_translations": {
                                "ru": "Служебный автомобиль в твоем <w>распоряжении</w> круглосуточно."
                            }
                        },
                        {
                            "text": "Jestem do państwa <dyspozycji> w razie jakichkolwiek pytań dotyczących projektu.",
                            "target_lang_translations": {
                                "ru": "Я в вашем <w>распоряжении</w> в случае каких-либо вопросов по проекту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do dyspozycji",
                        "mieć do dyspozycji",
                        "oddać do dyspozycji",
                        "pozostawać do dyspozycji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность свободно пользоваться кем-либо или чем-либо; состояние готовности к использованию или помощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "доступность для использования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f741104a-5c1e-378e-b997-8a91fb7781a7",
                    "sense_definition": "Oficjalne polecenie wykonania czegoś, wydane przez osobę lub instytucję posiadającą władzę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor wydał <dyspozycję> przelania środków na nowe konto bankowe.",
                            "target_lang_translations": {
                                "ru": "Директор отдал <w>распоряжение</w> перевести средства на новый банковский счет."
                            }
                        },
                        {
                            "text": "Pracownicy czekają na dalsze <dyspozycje> od kierownika zmiany.",
                            "target_lang_translations": {
                                "ru": "Сотрудники ждут дальнейших <w>указаний</w> от начальника смены."
                            }
                        }
                    ],
                    "synonyms": [
                        "polecenie",
                        "nakaz",
                        "zarządzenie",
                        "rozkaz",
                        "rozporządzenie"
                    ],
                    "common_phrases": [
                        "wydać dyspozycję",
                        "otrzymać dyspozycję",
                        "zgodnie z dyspozycją"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное указание выполнить что-либо, отданное лицом или учреждением, обладающим властью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "приказ, поручение"
                            },
                            {
                                "target_lang_word": "указание",
                                "target_lang_sense_clarification": "инструкция, наставление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e909685b-9538-3092-8d11-834e8b787239",
                    "sense_definition": "Aktualny stan fizyczny lub psychiczny kogoś, wpływający na sprawność działania w danym momencie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz był tego dnia w świetnej <dyspozycji> i obronił rzut karny.",
                            "target_lang_translations": {
                                "ru": "Вратарь был в тот день в отличной <w>форме</w> и отбил пенальти."
                            }
                        },
                        {
                            "text": "Jej gorsza <dyspozycja> w pracy wynikała z niewyleczonej grypy.",
                            "target_lang_translations": {
                                "ru": "Ее плохая <w>форма</w> на работе была следствием недолеченного гриппа."
                            }
                        }
                    ],
                    "antonyms": [
                        "niedyspozycja"
                    ],
                    "common_phrases": [
                        "dobra dyspozycja",
                        "wysoka dyspozycja",
                        "dyspozycja dnia",
                        "być w dyspozycji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Текущее физическое или психическое состояние, влияющее на работоспособность в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форма",
                                "target_lang_sense_clarification": "физическая или спортивная кондиция"
                            },
                            {
                                "target_lang_word": "состояние",
                                "target_lang_sense_clarification": "самочувствие"
                            },
                            {
                                "target_lang_word": "настрой",
                                "target_lang_sense_clarification": "психологическая готовность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5851b806-dc6d-3078-b9b3-c962f7e8a5c1",
                    "sense_definition": "Wrodzona skłonność organizmu do czegoś lub naturalna umiejętność w jakiejś dziedzinie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko wykazuje dużą <dyspozycję> do nauki języków obcych i muzyki.",
                            "target_lang_translations": {
                                "ru": "Ребенок проявляет большую <w>способность</w> к изучению иностранных языков и музыки."
                            }
                        },
                        {
                            "text": "Lekarz stwierdził u pacjenta genetyczną <dyspozycję> do chorób serca.",
                            "target_lang_translations": {
                                "ru": "Врач обнаружил у пациента генетическую <w>предрасположенность</w> к болезням сердца."
                            }
                        }
                    ],
                    "synonyms": [
                        "predyspozycja",
                        "talent"
                    ],
                    "common_phrases": [
                        "dyspozycja genetyczna",
                        "dyspozycja do czegoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Врожденная склонность организма к чему-либо или природная способность в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предрасположенность",
                                "target_lang_sense_clarification": "склонность к чему-либо (напр. к болезни)"
                            },
                            {
                                "target_lang_word": "способность",
                                "target_lang_sense_clarification": "талант, задатки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "521fb58e-78ac-37fd-a39b-9d4efb4eaed2",
                    "sense_definition": "Ułożony plan rozmieszczenia elementów, struktura lub układ czegoś (np. wnętrza, tekstu).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Architekt zaproponował nową <dyspozycję> pomieszczeń w remontowanym biurowcu.",
                            "target_lang_translations": {
                                "ru": "Архитектор предложил новую <w>планировку</w> помещений в ремонтируемом офисе."
                            }
                        },
                        {
                            "text": "Przejrzysta <dyspozycja> artykułu ułatwia czytelnikom zrozumienie trudnej treści.",
                            "target_lang_translations": {
                                "ru": "Понятная <w>структура</w> статьи облегчает читателям понимание сложного содержания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dyspozycja wnętrza",
                        "dyspozycja tekstu"
                    ],
                    "target_lang_definitions": {
                        "ru": "План размещения элементов, структура или устройство чего-либо (например, интерьера, текста)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планировка",
                                "target_lang_sense_clarification": "расположение помещений"
                            },
                            {
                                "target_lang_word": "структура",
                                "target_lang_sense_clarification": "строение, композиция"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
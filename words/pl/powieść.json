    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97bcda10-0cdb-3237-8053-ea759b495b25",
                    "sense_definition": "Długi utwór literacki pisany prozą, opisujący rozbudowany ciąg zdarzeń i losy bohaterów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj skończyłem czytać fascynującą <w>powieść</w> kryminalną.",
                            "target_lang_translations": {
                                "ru": "Вчера я закончил читать увлекательный детективный <w>роман</w>."
                            }
                        },
                        {
                            "text": "Jego pierwsza <w>powieść</w> została przetłumaczona na wiele języków.",
                            "target_lang_translations": {
                                "ru": "Его первый <w>роман</w> был переведен на многие языки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "powieść historyczna",
                        "powieść kryminalna",
                        "bohater powieści",
                        "pisać powieść",
                        "adaptacja powieści"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное литературное произведение в прозе, описывающее развернутую последовательность событий и судьбы героев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роман",
                                "target_lang_sense_clarification": "крупная литературная форма"
                            },
                            {
                                "target_lang_word": "повесть",
                                "target_lang_sense_clarification": "средняя литературная форма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b31ca531-d71b-3876-b144-4512fce5da42",
                    "sense_definition": "Zakończyć się sukcesem lub pomyślnie ułożyć (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "success",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że ten ryzykowny plan nam się <w>powiedzie</w>.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, что этот рискованный план нам <w>удастся</w>."
                            }
                        },
                        {
                            "text": "Niestety, eksperyment nie <w>powiódł</w> się zgodnie z naszymi oczekiwaniami.",
                            "target_lang_translations": {
                                "ru": "К сожалению, эксперимент не <w>удался</w> в соответствии с нашими ожиданиями."
                            }
                        }
                    ],
                    "synonyms": [
                        "udać"
                    ],
                    "common_phrases": [
                        "powieść się",
                        "wszystko się powiedzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завершиться успехом или сложиться удачно (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удаться",
                                "target_lang_sense_clarification": "завершиться успешно"
                            },
                            {
                                "target_lang_word": "получиться",
                                "target_lang_sense_clarification": "выйти, сложиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ee02f8f-8c55-3823-a488-21a78754bec0",
                    "sense_definition": "Przesunąć czymś, na przykład wzrokiem lub dłonią, po powierzchni czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <w>powiódł</w> surowym wzrokiem po całej klasie.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>повёл</w> суровым взглядом по всему классу."
                            }
                        },
                        {
                            "text": "Dziewczyna <w>powiodła</w> palcem po mapie, planując trasę wycieczki.",
                            "target_lang_translations": {
                                "ru": "Девушка <w>повела</w> пальцем по карте, планируя маршрут экскурсии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "powieść wzrokiem",
                        "powieść oczami",
                        "powieść ręką"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Провести чем-либо, например взглядом или рукой, по поверхности чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повести",
                                "target_lang_sense_clarification": "двинуть, направить (взгляд, руку)"
                            },
                            {
                                "target_lang_word": "провести",
                                "target_lang_sense_clarification": "выполнить движение по поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74329ab1-a29b-3569-92d2-603704139ba0",
                    "sense_definition": "Sprawić, że ktoś pójdzie w określonym kierunku; stanąć na czele grupy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "leading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony przewodnik <w>powiódł</w> turystów bezpieczną ścieżką przez góry.",
                            "target_lang_translations": {
                                "ru": "Опытный гид <w>повёл</w> туристов по безопасной тропе через горы."
                            }
                        },
                        {
                            "text": "Generał <w>powiódł</w> swoje wojska do decydującej bitwy.",
                            "target_lang_translations": {
                                "ru": "Генерал <w>повёл</w> свои войска в решающую битву."
                            }
                        }
                    ],
                    "synonyms": [
                        "poprowadzić"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то пойти в определенном направлении; возглавить группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повести",
                                "target_lang_sense_clarification": "возглавить, направить"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "f6ea70e8-1926-36ac-b327-2d6ec4b524e2",
                    "sense_definition": "Tradycyjna polska zupa przygotowywana z pokrojonych w paski fragmentów żołądka wołowego, gotowana z warzywami i przyprawami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food_dish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej restauracji podają najlepsze <w>flaki</w> w całym mieście.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают лучшие <w>фляки</w> во всём городе."
                            }
                        },
                        {
                            "text": "Na pierwsze danie zjedliśmy gorące <w>flaki</w> wołowe z bułką.",
                            "target_lang_translations": {
                                "ru": "На первое мы съели горячие говяжье <w>фляки</w> с булкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "flaczki"
                    ],
                    "common_phrases": [
                        "flaki wołowe",
                        "flaki po zamojsku",
                        "gorące flaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционный польский суп из нарезанного полосками говяжьего рубца, сваренный с овощами и специями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фляки",
                                "target_lang_sense_clarification": "название блюда"
                            },
                            {
                                "target_lang_word": "суп из рубца",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {

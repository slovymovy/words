{
    "word_family": [
        "plecy",
        "plecak",
        "zapleczowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "586c4574-1bbf-33bc-84dc-48883ac49f5a",
                    "sense_definition": "Pomieszczenie lub wydzielona część budynku znajdująca się na tyłach sklepu, biura lub lokalu, zazwyczaj niedostępna dla klientów, służąca do przechowywania towarów lub prac pomocniczych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "space_rear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownicy sklepu spożywają posiłki na <w>zapleczu</w>.",
                            "target_lang_translations": {
                                "ru": "Сотрудники магазина едят в <w>подсобке</w>."
                            }
                        },
                        {
                            "text": "Dostawca wniósł wszystkie kartony z towarem bezpośrednio na <w>zaplecze</w>.",
                            "target_lang_translations": {
                                "ru": "Поставщик занес все коробки с товаром прямо в <w>подсобное помещение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomieszczenie gospodarcze"
                    ],
                    "common_phrases": [
                        "wejście od zaplecza",
                        "praca na zapleczu",
                        "drzwi na zaplecze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение или отделенная часть здания в задней части магазина, офиса или заведения, обычно недоступная для клиентов, используемая для хранения товаров или служебных целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсобка",
                                "target_lang_sense_clarification": "разговорное, сокращение от подсобное помещение"
                            },
                            {
                                "target_lang_word": "подсобное помещение",
                                "target_lang_sense_clarification": "официальное название"
                            },
                            {
                                "target_lang_word": "служебное помещение",
                                "target_lang_sense_clarification": "помещение для персонала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f626b9d8-62c9-313e-92a8-0048352d9878",
                    "sense_definition": "Zasoby ludzkie, finansowe, techniczne lub organizacyjne, które stanowią podstawę i wsparcie dla funkcjonowania jakiegoś przedsięwzięcia lub instytucji.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "support_base",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Partia rządząca posiada silne <w>zaplecze</w> polityczne w parlamencie.",
                            "target_lang_translations": {
                                "ru": "Правящая партия имеет сильную политическую <w>базу</w> в парламенте."
                            }
                        },
                        {
                            "text": "Firma inwestuje w nowoczesne <w>zaplecze</w> techniczne, aby zwiększyć produkcję.",
                            "target_lang_translations": {
                                "ru": "Фирма инвестирует в современную техническую <w>базу</w>, чтобы увеличить производство."
                            }
                        }
                    ],
                    "synonyms": [
                        "baza"
                    ],
                    "common_phrases": [
                        "zaplecze polityczne",
                        "zaplecze techniczne",
                        "zaplecze finansowe",
                        "zaplecze socjalne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Людские, финансовые, технические или организационные ресурсы, составляющие основу и поддержку для функционирования предприятия или учреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "материально-техническая или ресурсная основа"
                            },
                            {
                                "target_lang_word": "тыл",
                                "target_lang_sense_clarification": "переносное значение: надежная поддержка"
                            },
                            {
                                "target_lang_word": "инфраструктура",
                                "target_lang_sense_clarification": "техническое оснащение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99fc4d1d-1ac0-3bbe-b180-6778cbc9f7e5",
                    "sense_definition": "Obszar przylegający do dużego ośrodka miejskiego, portu lub okręgu przemysłowego, stanowiący jego strefę wpływów lub bazę zaopatrzeniową.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geo_hinterland",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rozwój portu morskiego zależy od dobrego skomunikowania z jego <w>zapleczem</w> lądowym.",
                            "target_lang_translations": {
                                "ru": "Развитие морского порта зависит от хорошего сообщения с его сухопутным <w>тылом</w>."
                            }
                        },
                        {
                            "text": "To województwo stanowi doskonałe <w>zaplecze</w> surowcowe dla przemysłu ciężkiego.",
                            "target_lang_translations": {
                                "ru": "Это воеводство представляет собой отличную сырьевую <w>базу</w> для тяжелой промышленности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zaplecze portu",
                        "zaplecze surowcowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, прилегающая к крупному городскому центру, порту или промышленному округу, являющаяся зоной его влияния или базой снабжения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыл",
                                "target_lang_sense_clarification": "в логистике и географии: территория, тяготеющая к порту или центру"
                            },
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "источник ресурсов (напр. сырьевая база)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3430842b-cc27-3571-9f50-b3da60ebce7e",
                    "sense_definition": "W anatomii owadów: trzeci, tylny segment tułowia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology_anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod mikroskopem wyraźnie widać budowę <w>zaplecza</w> tego chrząszcza.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом отчетливо видно строение <w>заднегруди</w> этого жука."
                            }
                        },
                        {
                            "text": "Na <w>zapleczu</w> owada osadzona jest druga para skrzydeł.",
                            "target_lang_translations": {
                                "ru": "На <w>заднегруди</w> насекомого расположена вторая пара крыльев."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В анатомии насекомых: третий, задний сегмент груди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заднегрудь",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
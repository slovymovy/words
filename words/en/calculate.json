{
    "word_family": [
        "calculation",
        "calculator",
        "calculable",
        "incalculable",
        "miscalculate",
        "calculating"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9853438-ecc6-36ff-9d69-0bc0ae197a2b",
                    "sense_definition": "To discover a number, amount, or solution using mathematics.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "math_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>calculate</w> the total cost of the trip before booking.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>рассчитать</w> общую стоимость поездки перед бронированием."
                            }
                        },
                        {
                            "text": "Scientists can <w>calculate</w> the distance between the Earth and the Moon.",
                            "target_lang_translations": {
                                "ru": "Ученые могут <w>вычислить</w> расстояние между Землей и Луной."
                            }
                        }
                    ],
                    "synonyms": [
                        "compute",
                        "work out",
                        "reckon"
                    ],
                    "antonyms": [
                        "guess",
                        "estimate"
                    ],
                    "common_phrases": [
                        "calculate the cost",
                        "calculate the value",
                        "calculate the risk",
                        "calculate the distance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находить число, сумму или решение, используя математические вычисления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вычислять",
                                "target_lang_sense_clarification": "производить вычисления по формулам"
                            },
                            {
                                "target_lang_word": "рассчитывать",
                                "target_lang_sense_clarification": "определять размер, стоимость или время"
                            },
                            {
                                "target_lang_word": "подсчитывать",
                                "target_lang_sense_clarification": "подводить итог, считать количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc30237d-4c85-3595-9b56-7a7821762945",
                    "sense_definition": "To plan or design something specifically to achieve a particular effect or purpose.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "plan_intent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The advertisement was <w>calculated</w> to appeal to young children.",
                            "target_lang_translations": {
                                "ru": "Реклама была <w>рассчитана</w> на то, чтобы привлечь маленьких детей."
                            }
                        },
                        {
                            "text": "His comments were <w>calculated</w> to mislead the public about the incident.",
                            "target_lang_translations": {
                                "ru": "Его комментарии были <w>рассчитаны</w> на то, чтобы ввести общественность в заблуждение относительно инцидента."
                            }
                        }
                    ],
                    "synonyms": [
                        "compute",
                        "reckon",
                        "work out"
                    ],
                    "common_phrases": [
                        "calculated to succeed",
                        "calculated to deceive",
                        "calculated to impress"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often passive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Планировать или создавать что-либо специально для достижения определенного эффекта или цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассчитывать",
                                "target_lang_sense_clarification": "планировать с определенным умыслом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c7646de-2292-30c2-aee6-d6705eaae7bd",
                    "sense_definition": "To think, suppose, or expect something to happen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "opinion_dialect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>calculate</w> that we will arrive at the station by noon.",
                            "target_lang_translations": {
                                "ru": "Я <w>считаю</w>, что мы прибудем на станцию к полудню."
                            }
                        },
                        {
                            "text": "He <w>calculated</w> it was time to leave before the rain started.",
                            "target_lang_translations": {
                                "ru": "Он <w>посчитал</w>, что пора уходить, пока не начался дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "reckon"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "US"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "regional"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Думать, полагать или ожидать, что что-то произойдет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение"
                            },
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "предполагать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
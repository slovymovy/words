    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9d4eec14-11c2-3507-a014-65b85ab47c80",
                    "sense_definition": "Showing too much curiosity about other people's private affairs or business.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>snoopy</w> neighbor is always watching us from behind her curtains.",
                            "target_lang_translations": {
                                "ru": "Эта <w>любопытная</w> соседка вечно следит за нами из-за своих штор."
                            }
                        },
                        {
                            "text": "I do not want to be <w>snoopy</w>, but I noticed you were crying earlier.",
                            "target_lang_translations": {
                                "ru": "Не хочу показаться <w>назойливым</w>, но я заметил, что ты плакала."
                            }
                        }
                    ],
                    "synonyms": [
                        "nosy",
                        "prying",
                        "inquisitive",
                        "intrusive"
                    ],
                    "antonyms": [
                        "indifferent",
                        "uninterested"
                    ],
                    "common_phrases": [
                        "snoopy neighbor",
                        "snoopy reporter",
                        "snoopy question"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявляющий чрезмерное любопытство к личным делам других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любопытный",
                                "target_lang_sense_clarification": "проявляющий излишний интерес"
                            },
                            {
                                "target_lang_word": "назойливый",
                                "target_lang_sense_clarification": "бесцеремонно вмешивающийся"
                            },
                            {
                                "target_lang_word": "пронырливый",
                                "target_lang_sense_clarification": "высматривающий что-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e11bdde7-5218-3825-a2ce-9ff12b98852a",
                    "sense_definition": "A famous fictional beagle dog from the comic strip Peanuts created by Charles M. Schulz.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "My daughter loves the cartoon character <w>Snoopy</w> because he is funny and imaginative.",
                            "target_lang_translations": {
                                "ru": "Моя дочь обожает мультяшного персонажа <w>Снупи</w>, потому что он забавный и обладает богатым воображением."
                            }
                        },
                        {
                            "text": "The child hugged a stuffed toy that looked just like <w>Snoopy</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок обнял мягкую игрушку, которая выглядела точь-в-точь как <w>Снупи</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {

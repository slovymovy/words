    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7264026b-39c5-3ac5-ba7a-e0172df365ce",
                    "sense_definition": "Жидкий или кашицеобразный раствор извести, используемый для побелки поверхностей или скрепления строительных материалов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие принесли большое ведро, в котором была разведена белая <w>извёстка</w>.",
                            "target_lang_translations": {
                                "en": "The workers brought a large bucket in which white <w>lime</w> had been mixed."
                            }
                        },
                        {
                            "text": "Бабушка всегда белила печку свежей <w>извёсткой</w> перед праздниками.",
                            "target_lang_translations": {
                                "en": "Grandma always whitewashed the stove with fresh <w>whitewash</w> before the holidays."
                            }
                        }
                    ],
                    "common_phrases": [
                        "развести извёстку",
                        "побелить извёсткой",
                        "засохшая извёстка",
                        "пятно от извёстки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное название раствора"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A liquid or paste-like lime solution used for whitewashing surfaces or binding building materials."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whitewash",
                                "target_lang_sense_clarification": "liquid for painting walls"
                            },
                            {
                                "target_lang_word": "lime",
                                "target_lang_sense_clarification": "solution or mixture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15cea2ce-9047-3545-b8c7-ca1be4da8c4a",
                    "sense_definition": "То же, что известь; вяжущий материал, получаемый путём обжига карбонатных горных пород.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В углу сарая лежал старый мешок, в котором хранилась сухая <w>извёстка</w>.",
                            "target_lang_translations": {
                                "en": "In the corner of the shed lay an old sack in which dry <w>lime</w> was stored."
                            }
                        },
                        {
                            "text": "При попадании воды негашёная <w>извёстка</w> начинает сильно нагреваться.",
                            "target_lang_translations": {
                                "en": "When water hits it, quick<w>lime</w> starts to heat up intensely."
                            }
                        }
                    ],
                    "synonyms": [
                        "и́звесть"
                    ],
                    "common_phrases": [
                        "гашёная извёстка",
                        "негашёная извёстка",
                        "мешок извёстки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное название материала"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The same as lime; a binding material obtained by burning carbonate rocks."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lime",
                                "target_lang_sense_clarification": "material"
                            }
                        ]
                    }
                }
            ]
        }

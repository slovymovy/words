            "pos": "noun",
            "senses": [
                {
                    "sense_id": "198e0025-1d62-3252-be9f-97f35f4a0fb1",
                    "sense_definition": "Форма местного (второго предложного) падежа единственного числа существительного <w>ад</w> (используется с предлогами «в» и «на»)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ad_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грешники, согласно легендам, будут вечно гореть в <w>аду</w>.",
                            "target_lang_translations": {
                                "en": "Sinners, according to legends, will burn eternally in <w>hell</w>."
                            }
                        },
                        {
                            "text": "После всех испытаний его жизнь словно превратилась в пребывание в <w>аду</w>.",
                            "target_lang_translations": {
                                "en": "After all the trials, his life seemed to turn into living in <w>hell</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "гореть в аду",
                        "лучше править в аду",
                        "встретимся в аду"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "locative singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Locative (second prepositional) singular form of the noun \"hell\" (used with prepositions \"in\" and \"on\")"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hell",
                                "target_lang_sense_clarification": "place of torment/afterlife"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d646f69e-2028-3ebe-8ac3-4e26eabf9683",
                    "sense_definition": "Форма дательного падежа единственного числа существительного <w>ад</w>",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "ad_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы медленно подошли к самому <w>аду</w> сражения.",
                            "target_lang_translations": {
                                "en": "We slowly approached the very <w>hell</w> of the battle."
                            }
                        },
                        {
                            "text": "Подобно <w>аду</w>, эта пустыня была невыносимо жаркой.",
                            "target_lang_translations": {
                                "en": "Like <w>hell</w>, this desert was unbearably hot."
                            }
                        }
                    ],
                    "common_phrases": [
                        "предать аду",
                        "к аду"

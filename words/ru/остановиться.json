    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "101e6602-23ad-32ab-b90c-6586ce64dd4e",
                    "sense_definition": "Прекратить двигаться, перестать идти или ехать",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автобус <остановился> прямо перед нами.",
                            "target_lang_translations": {
                                "en": "The bus <stopped> right in front of us."
                            }
                        },
                        {
                            "text": "Ты должен <остановиться> у знака стоп.",
                            "target_lang_translations": {
                                "en": "You must <stop> at the stop sign."
                            }
                        }
                    ],
                    "synonyms": [
                        "встать"
                    ],
                    "antonyms": [
                        "пойти",
                        "двинуться"
                    ],
                    "common_phrases": [
                        "резко остановиться",
                        "остановиться на светофоре",
                        "машина остановилась"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop moving, to cease walking or driving"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "cease moving"
                            },
                            {
                                "target_lang_word": "come to a halt",
                                "target_lang_sense_clarification": "stop completely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "091e1bd3-4151-3eb7-8d03-6adf6bf14907",
                    "sense_definition": "Временно поселиться где-либо, например в гостинице или у знакомых",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lodging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <остановиться> в недорогой гостинице.",
                            "target_lang_translations": {
                                "en": "We decided to <stay> at an inexpensive hotel."
                            }
                        },
                        {
                            "text": "В Москве они <остановились> у родственников.",
                            "target_lang_translations": {
                                "en": "In Moscow, they <stayed> with relatives."
                            }
                        }
                    ],
                    "common_phrases": [
                        "остановиться в отеле",
                        "остановиться у друзей",
                        "остановиться на ночлег"
                    ],
                    "target_lang_definitions": {
                        "en": "To stay temporarily somewhere, such as at a hotel or with friends"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stay",
                                "target_lang_sense_clarification": "lodge temporarily"
                            },
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "stay briefly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b7637ed-f0d1-3e4b-89c2-39246f16bac2",
                    "sense_definition": "Перестать делать что-либо, прекратить действие",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он хотел что-то сказать, но вдруг <остановился>.",
                            "target_lang_translations": {
                                "en": "He wanted to say something, but suddenly <stopped>."
                            }
                        },
                        {
                            "text": "Пожалуйста, <остановись> и послушай меня.",
                            "target_lang_translations": {
                                "en": "Please <stop> and listen to me."
                            }
                        }
                    ],
                    "antonyms": [
                        "продолжить"
                    ],
                    "common_phrases": [
                        "вовремя остановиться",
                        "не могу остановиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop doing something, to cease an action"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "cease an activity"
                            },
                            {
                                "target_lang_word": "pause",
                                "target_lang_sense_clarification": "stop temporarily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef78f358-991d-3a8f-a882-68b1479ed16b",
                    "sense_definition": "Уделить особое внимание чему-либо в разговоре или исследовании",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "focus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лектор предложил <остановиться> на этой проблеме подробнее.",
                            "target_lang_translations": {
                                "en": "The lecturer suggested <dwelling> on this problem in more detail."
                            }
                        },
                        {
                            "text": "Я хочу отдельно <остановиться> на результатах работы.",
                            "target_lang_translations": {
                                "en": "I want to specifically <focus> on the results of the work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подробно остановиться",
                        "остановиться на вопросе",
                        "остановиться на деталях"
                    ],
                    "target_lang_definitions": {
                        "en": "To dwell on or give special attention to a topic"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dwell on",
                                "target_lang_sense_clarification": "discuss in detail"
                            },
                            {
                                "target_lang_word": "focus on",
                                "target_lang_sense_clarification": "pay attention to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77720f5a-c69f-3d67-9e44-e8efbbc6350f",
                    "sense_definition": "Сделать выбор в пользу кого-либо или чего-либо",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго спорили и <остановились> на компромиссе.",
                            "target_lang_translations": {
                                "en": "We argued for a long time and <settled> on a compromise."
                            }
                        },
                        {
                            "text": "Почему вы <остановились> именно на этой модели?",
                            "target_lang_translations": {
                                "en": "Why did you <settle> on this particular model?"
                            }

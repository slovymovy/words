                    }
                },
                {
                    "sense_id": "db99d676-e10e-3b24-b139-78f4c8ab8b56",
                    "sense_definition": "An insulting term used to describe a person considered low, contemptible, or treacherous.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He shouted at his rival, calling him a vile <w>ratcatcher</w> during their heated argument.",
                            "target_lang_translations": {
                                "ru": "Он кричал на своего соперника, называя его гнусным <w>мерзавцем</w> во время их жаркого спора."
                            }
                        },
                        {
                            "text": "The character in the play mocks his enemy by addressing him as a <w>ratcatcher</w>.",
                            "target_lang_translations": {

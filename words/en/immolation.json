            "senses": [
                {
                    "sense_id": "f3665231-05e1-3620-8bd8-69b4ad63bf4f",
                    "sense_definition": "The act of killing or destroying something, especially by fire, often as a religious sacrifice.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The protest involved a tragic act of self-<w>immolation</w> in the city square.",
                            "target_lang_translations": {
                                "ru": "Протест включал в себя трагический акт само<w>сожжения</w> на городской площади."
                            }
                        },
                        {
                            "text": "Ancient texts describe the ritual <w>immolation</w> of animals to appease the gods.",
                            "target_lang_translations": {
                                "ru": "Древние тексты описывают ритуальное <w>жертвоприношение</w> животных, чтобы умилостивить богов."
                            }
                        }
                    ],
                    "synonyms": [

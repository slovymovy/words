    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3d884e5-e83b-3d5f-99a5-0e388b5729bb",
                    "sense_definition": "Człowiek lub istota o nienaturalnie dużym wzroście i ogromnej sile.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_creature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten koszykarz to prawdziwy <gigant> w porównaniu do reszty drużyny.",
                            "target_lang_translations": {
                                "ru": "Этот баскетболист — настоящий <гигант> по сравнению с остальной командой."
                            }
                        },
                        {
                            "text": "W starej baśni zły <gigant> pilnował wejścia do jaskini ze skarbami.",
                            "target_lang_translations": {
                                "ru": "В старой сказке злой <великан> охранял вход в пещеру с сокровищами."
                            }
                        }
                    ],
                    "synonyms": [
                        "olbrzym",
                        "kolos"
                    ],
                    "antonyms": [
                        "karzeł",
                        "liliput",
                        "krasnal",
                        "kurdupel"
                    ],
                    "common_phrases": [
                        "gigant intelektu",
                        "prawdziwy gigant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или существо неестественно высокого роста и огромной силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "великан",
                                "target_lang_sense_clarification": "человек или сказочное существо огромного роста"
                            },
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "человек очень высокого роста"
                            },
                            {
                                "target_lang_word": "исполин",
                                "target_lang_sense_clarification": "человек богатырского сложения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63612931-77b6-37c4-aaec-d23356f2e1e1",
                    "sense_definition": "Organizacja, firma lub obiekt o potężnych rozmiarach i bardzo dużym znaczeniu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "large_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta firma to technologiczny <gigant>, który dominuje na światowym rynku.",
                            "target_lang_translations": {
                                "ru": "Эта фирма — технологический <гигант>, который доминирует на мировом рынке."
                            }
                        },
                        {
                            "text": "Nowy statek pasażerski jest stalowym <gigantem> pływającym po oceanach.",
                            "target_lang_translations": {
                                "ru": "Новый пассажирский лайнер является стальным <гигантом>, плавающим по океанам."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolos",
                        "olbrzym"
                    ],
                    "common_phrases": [
                        "gigant na glinianych nogach",
                        "gigant technologiczny",
                        "finansowy gigant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация, фирма или объект огромных размеров и очень большого значения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "крупная организация или объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61e9cd92-642f-33a7-a9ad-df62e21ec15a",
                    "sense_definition": "W mitologii greckiej: potężna istota walcząca z bogami olimpijskimi, syn Uranosa i Gai.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mythology",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Zeus użył swoich piorunów, aby pokonać ostatniego <giganta>.",
                            "target_lang_translations": {
                                "ru": "Зевс использовал свои молнии, чтобы победить последнего <гиганта>."
                            }
                        },
                        {
                            "text": "Starożytne mity opowiadają o wojnie bogów z potężnymi <gigantami>.",
                            "target_lang_translations": {
                                "ru": "Древние мифы рассказывают о войне богов с могучими <гигантами>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tytan"
                    ],
                    "common_phrases": [
                        "walka z gigantami"
                    ],
                    "target_lang_definitions": {
                        "ru": "В греческой мифологии: могущественное существо, сражавшееся с олимпийскими богами, сын Урана и Геи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "мифологическое существо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a7458c2-b201-3830-a44e-538173bb6b64",
                    "sense_definition": "Ucieczka z domu lub placówki wychowawczej połączona z włóczęgą.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_escape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody chłopak poszedł na <gigant> po ostrej kłótni z rodzicami.",
                            "target_lang_translations": {
                                "ru": "Молодой парень ударился в <бега> после сильной ссоры с родителями."
                            }
                        },
                        {
                            "text": "Policja odnalazła nastolatkę, która była na <gigancie> od dwóch tygodni.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла девушку-подростка, которая была в <бегах> уже две недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "ucieczka",
                        "zrywka"
                    ],
                    "common_phrases": [
                        "iść na gigant",
                        "być na gigancie"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane głównie w kontekście młodzieżowym lub więziennym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Побег из дома или воспитательного учреждения, сопряженный с бродяжничеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бега",
                                "target_lang_sense_clarification": "состояние того, кто скрывается; побег"
                            },
                            {
                                "target_lang_word": "побег",
                                "target_lang_sense_clarification": "самовольный уход"
                            }
                        ]
                    }
                }
            ]
        }
    ]

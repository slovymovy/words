    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "122cb258-e0df-3ca9-a14e-6e080ba50eeb",
                    "sense_definition": "Очень хорошо, превосходно, восхитительно (о действии или качестве).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты сегодня просто <чудесно> выглядишь в этом новом платье.",
                            "target_lang_translations": {
                                "en": "You look simply <w>wonderful</w> today in this new dress."
                            }
                        },
                        {
                            "text": "Эти весенние цветы <чудесно> пахнут и радуют глаз.",
                            "target_lang_translations": {
                                "en": "These spring flowers smell <w>wonderful</w> and delight the eye."
                            }
                        }
                    ],
                    "synonyms": [
                        "прекрасно",
                        "замечательно",
                        "великолепно",
                        "отлично"
                    ],
                    "antonyms": [
                        "ужасно",
                        "отвратительно",
                        "плохо"
                    ],
                    "common_phrases": [
                        "чудесно провести время",
                        "чудесно выглядеть",
                        "чудесно пахнуть"
                    ],
                    "target_lang_definitions": {
                        "en": "Very well, excellently, delightfully (referring to an action or quality)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wonderfully",
                                "target_lang_sense_clarification": "general adverb"
                            },
                            {

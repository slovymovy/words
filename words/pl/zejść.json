    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0aa6a65f-46a5-3708-8914-68fba0812360",
                    "sense_definition": "Przemieścić się z góry na dół, zazwyczaj stawiając kroki (np. po schodach, z drabiny).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę ostrożnie <zejść> po stromych schodach do piwnicy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно осторожно <спуститься> по крутой лестнице в подвал."
                            }
                        },
                        {
                            "text": "Czy możesz już <zejść> na dół na śniadanie?",
                            "target_lang_translations": {
                                "ru": "Ты можешь уже <спуститься> вниз на завтрак?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zleźć"
                    ],
                    "antonyms": [
                        "wejść"
                    ],
                    "common_phrases": [
                        "zejść na dół",
                        "zejść po schodach",
                        "zejść z góry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместиться сверху вниз, обычно шагая (например, по лестнице)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спуститься",
                                "target_lang_sense_clarification": "движение вниз"
                            },
                            {
                                "target_lang_word": "сойти",
                                "target_lang_sense_clarification": "покинуть возвышенность или транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bd4982e-ad4d-36de-bc2d-3c89dc59339f",
                    "sense_definition": "Opuścić zajmowane miejsce, powierzchnię lub obszar; usunąć się.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "leave_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę natychmiast <zejść> z trawnika, tu nie wolno deptać trawy.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, немедленно <сойдите> с газона, здесь нельзя топтать траву."
                            }
                        },
                        {
                            "text": "Musisz mi <zejść> z drogi, bo niosę ciężkie pudła.",
                            "target_lang_translations": {
                                "ru": "Ты должен <уйти> с моей дороги, потому что я несу тяжелые коробки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zleźć",
                        "zleźć"
                    ],
                    "antonyms": [
                        "wejść"
                    ],
                    "common_phrases": [
                        "zejść z drogi",
                        "zejść z oczu",
                        "zejść z boiska",
                        "zejść ze sceny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покинуть занимаемое место, поверхность или область; отойти в сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сойти",
                                "target_lang_sense_clarification": "покинуть поверхность (газон, сцену)"
                            },
                            {
                                "target_lang_word": "уйти",
                                "target_lang_sense_clarification": "освободить место, уйти с дороги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3006a854-e685-36fb-bb73-89f6e66fb7db",
                    "sense_definition": "Zniknąć, zostać usuniętym lub odpaść (używane w kontekście plam, farby, skóry lub paznokci).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "disappear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta tłusta plama nie chce <zejść> z mojej ulubionej koszuli.",
                            "target_lang_translations": {
                                "ru": "Это жирное пятно не хочет <сходить> с моей любимой рубашки."
                            }
                        },
                        {
                            "text": "Po intensywnym opalaniu <zeszła> mi skóra z pleców.",
                            "target_lang_translations": {
                                "ru": "После интенсивного загара у меня <слезла> кожа со спины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skóra zeszła",
                        "plama zeszła",
                        "paznokieć zszedł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исчезнуть, удалиться (о пятнах) или отделиться слоями (о коже, ногтях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сойти",
                                "target_lang_sense_clarification": "исчезнуть (о пятне, краске)"
                            },
                            {
                                "target_lang_word": "слезть",
                                "target_lang_sense_clarification": "отделиться (о коже, ногте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b1e2045-91ba-3e4b-8e05-fc7054e35735",
                    "sense_definition": "Obniżyć cenę, wartość lub wymagania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprzedawca nie chciał <zejść> z ceny ani o złotówkę.",
                            "target_lang_translations": {
                                "ru": "Продавец не хотел <сбросить> цену ни на злотый."
                            }
                        },
                        {
                            "text": "Musimy trochę <zejść> z kosztów, żeby zmieścić się w budżecie.",
                            "target_lang_translations": {
                                "ru": "Мы должны немного <снизить> расходы, чтобы уложиться в бюджет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zejść z ceny",
                        "zejść z kosztów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снизить цену, стоимость или требования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снизить",
                                "target_lang_sense_clarification": "уменьшить (расходы)"
                            },
                            {
                                "target_lang_word": "сбросить",
                                "target_lang_sense_clarification": "разг. уменьшить цену"
                            },
                            {
                                "target_lang_word": "уступить",
                                "target_lang_sense_clarification": "согласиться на меньшую цену"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f7da8a4-bf23-35bb-904f-b789d5ff8be1",
                    "sense_definition": "Umrzeć (często używane w kontekście nagłej śmierci).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "die",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słyszałem, że stary sąsiad <zszedł> na zawał w zeszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Я слышал, что старый сосед <умер> от инфаркта на прошлой неделе."
                            }
                        },
                        {
                            "text": "Pacjent nagle <zszedł> podczas operacji mimo wysiłków lekarzy.",
                            "target_lang_translations": {
                                "ru": "Пациент внезапно <скончался> во время операции, несмотря на усилия врачей."
                            }
                        }
                    ],
                    "synonyms": [
                        "zleźć"
                    ],
                    "common_phrases": [
                        "zejść na zawał",
                        "zejść z tego świata"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умереть (часто используется в контексте внезапной смерти)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умереть",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "скончаться",
                                "target_lang_sense_clarification": "официальный или уважительный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f070f6b-85ca-3e4a-84a8-cce789f12166",
                    "sense_definition": "Spotkać się w jednym miejscu lub zbiec się w jednym punkcie (wymaga użycia zaimka 'się').",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "converge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie drogi w tym mieście zdają się <zejść> w rynku.",
                            "target_lang_translations": {
                                "ru": "Все дороги в этом городе, кажется, <сходятся> на рыночной площади."
                            }
                        },
                        {
                            "text": "Musimy się <zejść> w połowie drogi, żeby przekazać paczkę.",
                            "target_lang_translations": {
                                "ru": "Мы должны <встретиться> на полпути, чтобы передать посылку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zejść się",
                        "drogi się zeszły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встретиться в одном месте или соединиться в одной точке (требует возвратной частицы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сойтись",
                                "target_lang_sense_clarification": "соединиться в одной точке"
                            },
                            {
                                "target_lang_word": "встретиться",
                                "target_lang_sense_clarification": "о людях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64392334-7302-3966-9891-fe598921f9e1",
                    "sense_definition": "Zostać szybko sprzedanym (o towarze).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały towar <zszedł> nam w ciągu jednej godziny.",
                            "target_lang_translations": {
                                "ru": "Весь товар <разошелся> у нас в течение одного часа."
                            }
                        },
                        {
                            "text": "Świeże bułki <zeszły> na pniu zaraz po otwarciu piekarni.",
                            "target_lang_translations": {
                                "ru": "Свежие булочки <разошлись> мгновенно сразу после открытия пекарни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zejść na pniu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть быстро распроданным (о товаре)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разойтись",
                                "target_lang_sense_clarification": "быть раскупленным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec128a12-350d-3195-b539-67d065299f36",
                    "sense_definition": "Wzejść, wykiełkować (o roślinach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sprout",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zboże na polu już dawno <zeszło> po wiosennych deszczach.",
                            "target_lang_translations": {
                                "ru": "Зерно на поле уже давно <взошло> после весенних дождей."
                            }
                        },
                        {
                            "text": "Czekamy, aż posadzona marchewka w końcu <zejdzie>.",
                            "target_lang_translations": {
                                "ru": "Мы ждем, когда посаженная морковь наконец <взойдет>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "gwara, np. Bukowina"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прорасти, дать всходы (о растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взойти",
                                "target_lang_sense_clarification": "о посевах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e12e29e-e11e-36c3-aa78-369b151ca352",
                    "sense_definition": "Forma słowa <w>zejście</w> (rzeczownik, dopełniacz liczby mnogiej).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym projekcie nie przewidziano dwóch osobnych <zejść> do garażu.",
                            "target_lang_translations": {
                                "ru": "В этом проекте не предусмотрено двух отдельных <спусков> в гараж."
                            }
                        },
                        {
                            "text": "Unikaj gwałtownych <zejść> pod wodę bez wyrównania ciśnienia.",
                            "target_lang_translations": {
                                "ru": "Избегай резких <погружений> под воду без выравнивания давления."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма существительного 'zejście' (спуск/выход) в родительном падеже множественного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спусков",
                                "target_lang_sense_clarification": "форма слова спуск"
                            },
                            {
                                "target_lang_word": "сходов",
                                "target_lang_sense_clarification": "форма слова сход"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            },
                            {
                                "target_lang_word": "измена",
                                "target_lang_sense_clarification": "государственная или супружеская"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "betrayal",
                                "target_lang_sense_clarification": "violation of trust"
                            },
                            {
                                "target_lang_word": "treason",
                                "target_lang_sense_clarification": "against one's country"
                            },
                            {
                                "target_lang_word": "treachery",

                    "synonyms": [
                        "месиво",
                        "жижа",
                        "шуга"
                    ],
                    "antonyms": [
                        "твердь"
                    ],
                    "common_phrases": [
                        "снежная каша",
                        "грязная каша"
                    ],
                    "target_lang_definitions": {
                        "en": "A dirty, viscous mass, usually of melting snow or mud."
                    },
                    "translations": {

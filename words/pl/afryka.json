                        "kraje Afryki",
                        "wyjazd do Afryki",
                        "mapa Afryki"
                    ],
                    "target_lang_definitions": {
                        "ru": "большой континент, расположенный к югу от Европы и отделенный от нее Средиземным морем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Африка",
                                "target_lang_sense_clarification": "континент"

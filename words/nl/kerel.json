                            {
                                "target_lang_word": "guy",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "fellow",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "bloke",
                                "target_lang_sense_clarification": "British English"
                            },
                            {
                                "target_lang_word": "dude",
                                "target_lang_sense_clarification": "very informal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7cf6261-d8db-3a46-aed9-5c4c47319930",
                    "sense_definition": "Een grote, sterke en stoere man.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "man_strong",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet een sterke <kerel> zijn om die zware kist alleen te tillen.",
                            "target_lang_translations": {
                                "en": "You have to be a strong <fellow> to lift that heavy box alone."
                            }
                        },
                        {
                            "text": "De uitsmijter was een brede <kerel> die iedereen bij de deur tegenhield.",
                            "target_lang_translations": {
                                "en": "The bouncer was a broad <guy> who stopped everyone at the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "vent"
                    ],
                    "common_phrases": [
                        "flinke kerel",
                        "stoere kerel",
                        "grote kerel"
                    ],
                    "target_lang_definitions": {
                        "en": "A big, strong, and tough man."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fellow",
                                "target_lang_sense_clarification": "emphasizing robustness"
                            },
                            {
                                "target_lang_word": "guy",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08ef95a8-cf66-34d3-b640-8abeb6c201d5",
                    "sense_definition": "De mannelijke partner in een huwelijk of relatie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "man_partner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze is gisteren samen met haar <kerel> naar de bioscoop geweest.",
                            "target_lang_translations": {
                                "en": "She went to the cinema with her <man> yesterday."
                            }
                        },
                        {
                            "text": "Mijn <kerel> kookt vanavond, dus ik heb lekker vrij.",
                            "target_lang_translations": {
                                "en": "My <man> is cooking tonight, so I have the night off."
                            }
                        }
                    ],

{
    "word_family": [
        "herinnering"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af1fb8d2-c874-32ef-ab17-42835e806f6f",
                    "sense_definition": "Iets uit het verleden weer in je gedachten krijgen of nog weten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "memory_cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan me zijn naam niet meer <w>herinneren</w>.",
                            "target_lang_translations": {
                                "en": "I can no longer <w>remember</w> his name.",
                                "ru": "Я больше не могу <w>вспомнить</w> его имя."
                            }
                        },
                        {
                            "text": "Hij kon zich die gebeurtenis niet <w>herinneren</w>.",
                            "target_lang_translations": {
                                "en": "He could not <w>remember</w> that event.",
                                "ru": "Он не мог <w>вспомнить</w> это событие."
                            }
                        },
                        {
                            "text": "Voor zover ik mij kan <w>herinneren</w>, was het die dag erg koud.",
                            "target_lang_translations": {
                                "en": "As far as I can <w>remember</w>, it was very cold that day.",
                                "ru": "Насколько я <w>помню</w>, в тот день было очень холодно."
                            }
                        }
                    ],
                    "synonyms": [
                        "voor de geest halen"
                    ],
                    "common_phrases": [
                        "zich iets herinneren",
                        "zich niets herinneren",
                        "voor zover ik mij herinner"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring something from the past back into your mind or to still know it.",
                        "ru": "Восстанавливать что-либо в памяти или сохранять информацию о прошлом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remember",
                                "target_lang_sense_clarification": "to recall from memory"
                            },
                            {
                                "target_lang_word": "recall",
                                "target_lang_sense_clarification": "to bring back to mind"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "хранить в памяти"
                            },
                            {
                                "target_lang_word": "вспоминать",
                                "target_lang_sense_clarification": "восстанавливать в памяти"
                            },
                            {
                                "target_lang_word": "припоминать",
                                "target_lang_sense_clarification": "с трудом вспоминать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0af97ac-3da2-3236-8f31-0e43d45ab0f6",
                    "sense_definition": "Iemand anders weer aan iets laten denken of zorgen dat iets niet vergeten wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "memory_cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>herinnerde</w> me eraan dat het zondag was.",
                            "target_lang_translations": {
                                "en": "She <w>reminded</w> me that it was Sunday.",
                                "ru": "Она <w>напомнила</w> мне, что было воскресенье."
                            }
                        },
                        {
                            "text": "Dat liedje <w>herinnert</w> mij aan de goede oude tijd.",
                            "target_lang_translations": {
                                "en": "That song <w>reminds</w> me of the good old days.",
                                "ru": "Эта песня <w>напоминает</w> мне о старых добрых временах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand eraan herinneren",
                        "doen herinneren aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone else think of something again or ensure something is not forgotten.",
                        "ru": "Заставлять кого-то вспомнить о чем-то или указывать на сходство с чем-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remind",
                                "target_lang_sense_clarification": "to cause someone to remember something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "напоминать",
                                "target_lang_sense_clarification": "не дать забыть или быть похожим"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
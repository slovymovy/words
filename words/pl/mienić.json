            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d2a64a9d-2e05-33c0-95ad-1d8e53e4fb12",
                    "sense_definition": "Błyszczeć światłem odbitym, często zmieniając przy tym barwy lub odcienie w zależności od kąta patrzenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej wieczorowa suknia <mieniła> się wszystkimi kolorami tęczy.",
                            "target_lang_translations": {
                                "ru": "Её вечернее платье <переливалось> всеми цветами радуги."
                            }
                        },
                        {
                            "text": "Powierzchnia jeziora <mieni> się w promieniach zachodzącego słońca.",
                            "target_lang_translations": {
                                "ru": "Поверхность озера <сверкает> в лучах заходящего солнца."
                            }
                        }
                    ],
                    "synonyms": [
                        "błyszczeć",
                        "połyskiwać",
                        "migotać"
                    ],
                    "common_phrases": [
                        "mienić się kolorami",
                        "mienić się w oczach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сверкать отраженным светом, часто меняя при этом цвета или оттенки в зависимости от угла зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переливаться",
                                "target_lang_sense_clarification": "менять оттенки, сверкать разными цветами"
                            },
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "ярко блестеть"
                            },
                            {
                                "target_lang_word": "искриться",
                                "target_lang_sense_clarification": "излучать искры света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffaa1fc5-009c-35f4-bdc8-f1411480f1f4",
                    "sense_definition": "Uważać się za kogoś, przypisywać sobie jakąś rolę, tytuł lub cechę (często niesłusznie lub na wyrost).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "identity_claim",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna ten bezpodstawnie <mieni> się właścicielem całej posiadłości.",
                            "target_lang_translations": {
                                "ru": "Этот мужчина безосновательно <называет> себя владельцем всего поместья."
                            }
                        },
                        {
                            "text": "Chociaż <mienił> się ekspertem, nie potrafił odpowiedzieć na proste pytanie.",
                            "target_lang_translations": {
                                "ru": "Хотя он <называл> себя экспертом, он не смог ответить на простой вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "uważać się",
                        "poczytywać się"
                    ],
                    "common_phrases": [
                        "mienić się przyjacielem",
                        "mienić się znawcą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать себя кем-либо, приписывать себе какую-либо роль, титул или качество (часто незаслуженно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть себя",
                                "target_lang_sense_clarification": "давать себе звание или имя"
                            },
                            {
                                "target_lang_word": "считать себя",
                                "target_lang_sense_clarification": "быть мнения о себе как о ком-то"
                            },
                            {
                                "target_lang_word": "выдавать себя",
                                "target_lang_sense_clarification": "притворяться кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04d49bdd-4e61-3831-bcd8-4a223b94f738",
                    "sense_definition": "Nazywać kogoś w określony sposób, nadawać komuś miano lub tytuł.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych pieśniach <mieniono> go najdzielniejszym z rycerzy.",
                            "target_lang_translations": {
                                "ru": "В старинных песнях его <называли> храбрейшим из рыцарей."
                            }
                        },
                        {
                            "text": "Jak <mienisz> ten przedmiot w swoim ojczystym języku?",
                            "target_lang_translations": {
                                "ru": "Как ты <называешь> этот предмет на своем родном языке?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nazywać",
                        "zwać",
                        "tytułować"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie używa się słowa 'nazywać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Называть кого-либо определенным образом, давать кому-либо имя или титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть",
                                "target_lang_sense_clarification": "давать имя или определение"
                            },
                            {
                                "target_lang_word": "именовать",
                                "target_lang_sense_clarification": "называть официально или книжно"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "обращаться по имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de1f5b06-4173-312e-a076-e92fc52caabd",
                    "sense_definition": "Dokonywać zmiany, czynić coś innym niż było.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czas nieustannie <mieni> oblicze tego starego miasta.",
                            "target_lang_translations": {
                                "ru": "Время непрестанно <меняет> облик этого старого города."
                            }
                        },
                        {
                            "text": "Mimo wielu lat rozłąki, nic nie <mieniło> jego uczuć do niej.",
                            "target_lang_translations": {
                                "ru": "Несмотря на долгие годы разлуки, ничто не <изменило> его чувств к ней."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmieniać"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie używa się słowa 'zmieniać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить изменения, делать что-либо иным, чем было."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "менять",
                                "target_lang_sense_clarification": "делать иным"
                            },
                            {
                                "target_lang_word": "изменять",
                                "target_lang_sense_clarification": "вносить перемены"
                            }
                        ]
                    }
                }
            ]
        }

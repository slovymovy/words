{
    "word_family": [
        "весь",
        "всё",
        "всегда",
        "всюду",
        "всякий",
        "вовсе",
        "всецело",
        "всеобщий"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a83a0d42-4e54-3e57-82e0-579b30e8f013",
                    "sense_definition": "Употребляется для ограничения количества, меры или времени; то же, что «только», «лишь».",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "До начала фильма осталось <w>всего</w> пять минут.",
                            "target_lang_translations": {
                                "en": "There are <w>only</w> five minutes left until the movie starts."
                            }
                        },
                        {
                            "text": "Это стоит <w>всего</w> сто рублей.",
                            "target_lang_translations": {
                                "en": "It costs <w>just</w> one hundred rubles."
                            }
                        }
                    ],
                    "synonyms": [
                        "только",
                        "лишь",
                        "всего́ лишь"
                    ],
                    "antonyms": [
                        "аж"
                    ],
                    "common_phrases": [
                        "всего лишь",
                        "всего-навсего"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to limit quantity, measure, or time; same as \"only\", \"merely\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "only",
                                "target_lang_sense_clarification": "no more than; exclusively"
                            },
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "simply; merely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27ef6a54-6904-354c-adfe-02ec908dda34",
                    "sense_definition": "В сумме, в конечном счёте; употребляется при подведении итогов.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "total",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На собрании присутствовало <w>всего</w> десять человек.",
                            "target_lang_translations": {
                                "en": "There were ten people <w>in all</w> at the meeting."
                            }
                        },
                        {
                            "text": "Сколько <w>всего</w> книг ты прочитал за лето?",
                            "target_lang_translations": {
                                "en": "How many books <w>in total</w> did you read over the summer?"
                            }
                        }
                    ],
                    "synonyms": [
                        "итого́",
                        "в общей сложности"
                    ],
                    "target_lang_definitions": {
                        "en": "In sum, ultimately; used when summarizing or counting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in all",
                                "target_lang_sense_clarification": "with everything included"
                            },
                            {
                                "target_lang_word": "altogether",
                                "target_lang_sense_clarification": "on the whole; with everything considered"
                            },
                            {
                                "target_lang_word": "in total",
                                "target_lang_sense_clarification": "the sum of parts"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "77154a07-317e-348d-8b43-c74d94d5ca7d",
                    "sense_definition": "Форма родительного падежа местоимения <w>всё</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "form_vse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я желаю тебе <w>всего</w> самого лучшего.",
                            "target_lang_translations": {
                                "en": "I wish you <w>all</w> the best."
                            }
                        },
                        {
                            "text": "У меня нет времени на обсуждение <w>всего</w> этого.",
                            "target_lang_translations": {
                                "en": "I don't have time to discuss <w>all</w> of this."
                            }
                        }
                    ],
                    "common_phrases": [
                        "всего хорошего",
                        "всего доброго",
                        "прежде всего"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive case"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive case form of the pronoun <w>everything</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of everything",
                                "target_lang_sense_clarification": "genitive form"
                            },
                            {
                                "target_lang_word": "all",
                                "target_lang_sense_clarification": "referring to the entirety of things"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "b7f68835-2d25-36d4-9628-641f4c4bb545",
                    "sense_definition": "Форма родительного или винительного падежа местоимения <w>весь</w> (мужской и средний род).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "form_ves",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы обошли вокруг <w>всего</w> здания.",
                            "target_lang_translations": {
                                "en": "We walked around the <w>whole</w> building."
                            }
                        },
                        {
                            "text": "Я вижу его <w>всего</w> насквозь.",
                            "target_lang_translations": {
                                "en": "I see right through <w>all</w> of him."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive/accusative case"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive or accusative case form of the pronoun <w>all</w> or <w>whole</w> (masculine and neuter)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whole",
                                "target_lang_sense_clarification": "entire; complete"
                            },
                            {
                                "target_lang_word": "entire",
                                "target_lang_sense_clarification": "whole; with no part left out"
                            },
                            {
                                "target_lang_word": "all",
                                "target_lang_sense_clarification": "the whole quantity or extent of"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "5e68bc2f-9153-306c-84de-e953635fcfca",
                    "sense_definition": "Разговорная форма прощания; сокращение от «всего хорошего» или «всего доброго».",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "farewell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ну, я пошёл, <w>всего</w>!",
                            "target_lang_translations": {
                                "en": "Well, I'm off, <w>all the best</w>!"
                            }
                        },
                        {
                            "text": "Звони, если что, <w>всего</w>!",
                            "target_lang_translations": {
                                "en": "Call if anything comes up, <w>take care</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "всего хорошего",
                        "всего доброго",
                        "до свидания"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial form of farewell; short for \"all the best\" or \"good luck\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "all the best",
                                "target_lang_sense_clarification": "farewell greeting"
                            },
                            {
                                "target_lang_word": "take care",
                                "target_lang_sense_clarification": "informal farewell"
                            },
                            {
                                "target_lang_word": "bye",
                                "target_lang_sense_clarification": "short informal farewell"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "наводнение",
        "наводнять"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad150e9a-cc59-34ce-835b-6ecc1444fe6d",
                    "sense_definition": "Заполнить какое-либо пространство или место огромным количеством людей или предметов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом туристы буквально <наводнили> узкие улочки старого города.",
                            "target_lang_translations": {
                                "en": "In summer, tourists literally <flooded> the narrow streets of the old city."
                            }
                        },
                        {
                            "text": "Дешёвые товары из-за границы быстро <наводнили> местный рынок.",
                            "target_lang_translations": {
                                "en": "Cheap goods from abroad quickly <flooded> the local market."
                            }
                        }
                    ],
                    "synonyms": [
                        "заполнить"
                    ],
                    "common_phrases": [
                        "наводнить рынок",
                        "наводнить город",
                        "наводнить страну",
                        "наводнить магазины"
                    ],
                    "target_lang_definitions": {
                        "en": "To fill a space or place with a huge number of people or objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "fill in large numbers"
                            },
                            {
                                "target_lang_word": "overrun",
                                "target_lang_sense_clarification": "occupy in large numbers"
                            },
                            {
                                "target_lang_word": "inundate",
                                "target_lang_sense_clarification": "overwhelm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66ee6070-a08c-37af-93cd-f52a776f23bd",
                    "sense_definition": "Затопить водой какую-либо территорию или пространство.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вышедшая из берегов река <наводнила> прибрежные деревни.",
                            "target_lang_translations": {
                                "en": "The river, having burst its banks, <flooded> the coastal villages."
                            }
                        },
                        {
                            "text": "Весенние дожди <наводнили> низину, превратив её в болото.",
                            "target_lang_translations": {
                                "en": "Spring rains <flooded> the lowland, turning it into a swamp."
                            }
                        }
                    ],
                    "synonyms": [
                        "затопить"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "в современном языке чаще используется глагол «затопить»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a territory or space with water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "cover with water"
                            }
                        ]
                    }
                }
            ]
        }
    ]

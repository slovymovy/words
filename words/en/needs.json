            "senses": [
                {
                    "sense_id": "09b1284e-0aa1-383e-8b6f-15fd411d29ee",
                    "sense_definition": "The plural form of the noun <need>, referring to things that are necessary for life, health, or a specific purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

            "senses": [
                {
                    "sense_id": "065e6f45-eb58-3887-b6fe-50a3ae57ea7c",
                    "sense_definition": "Matka Jezusa Chrystusa, Maria z Nazaretu",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_figure",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Wierni z całego kraju pielgrzymują, aby pokłonić się Czarnej <w>Madonnie</w>.",
                            "target_lang_translations": {
                                "ru": "Верующие со всей страны совершают паломничество, чтобы поклониться Чёрной <w>Мадонне</w>."
                            }
                        },
                        {
                            "text": "W ołtarzu głównym znajduje się cudowny wizerunek <w>Madonny</w>.",
                            "target_lang_translations": {
                                "ru": "В главном алтаре находится чудотворный образ <w>Мадонны</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Matka Boska",
                        "Maryja",
                        "Bogurodzica",
                        "Dziewica Maryja"
                    ],
                    "common_phrases": [
                        "Czarna Madonna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мать Иисуса Христа, Дева Мария (преимущественно в западной христианской традиции)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мадонна",
                                "target_lang_sense_clarification": "Богоматерь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8541d886-85db-3d1b-a8a7-0ecbf279e6f6",
                    "sense_definition": "imię żeńskie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja znajoma <w>Madonna</w> obchodzi imieniny w połowie sierpnia.",
                            "target_lang_translations": {
                                "ru": "Моя знакомая <w>Мадонна</w> празднует именины в середине августа."
                            }
                        },
                        {
                            "text": "To rzadkie imię, ale <w>Madonna</w> nosi je z wielką dumą.",
                            "target_lang_translations": {
                                "ru": "Это редкое имя, но <w>Мадонна</w> носит его с большой гордостью."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мадонна",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a25e4d40-935b-3635-86b9-70c2846eb889",
                    "sense_definition": "artystyczne przedstawienie Matki Boskiej, zazwyczaj w malarstwie lub rzeźbie, często z Dzieciątkiem Jezus",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "art_depiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W galerii sztuki dawnej wisi piękna renesansowa <w>madonna</w>.",
                            "target_lang_translations": {
                                "ru": "В галерее старинного искусства висит прекрасная ренессансная <w>мадонна</w>."
                            }
                        },
                        {
                            "text": "Artysta wyrzeźbił drewnianą <w>madonnę</w> do przydrożnej kapliczki.",
                            "target_lang_translations": {
                                "ru": "Художник вырезал деревянную <w>мадонну</w> для придорожной часовни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "madonna z dzieciątkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Художественное изображение Богоматери (в живописи или скульптуре)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мадонна",
                                "target_lang_sense_clarification": "произведение искусства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae94347b-7fa1-348b-aba0-df746b7d0124",
                    "sense_definition": "kobieta o subtelnej, uduchowionej urodzie i łagodnym wyrazie twarzy",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej delikatne rysy sprawiały, że wyglądała jak prerafaelicka <w>madonna</w>.",
                            "target_lang_translations": {
                                "ru": "Её тонкие черты лица придавали ей сходство с <w>мадонной</w> прерафаэлитов."
                            }
                        },
                        {
                            "text": "Pośród tłumu wyróżniała się spokojem niczym <w>madonna</w>.",
                            "target_lang_translations": {
                                "ru": "В толпе она выделялась спокойствием, словно <w>мадонна</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина с одухотворённой, кроткой красотой и мягким выражением лица"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мадонна",
                                "target_lang_sense_clarification": "тип красоты"
                            }
                        ]
                    }
                }
            ]
        }

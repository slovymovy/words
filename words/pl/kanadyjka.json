            "senses": [
                {
                    "sense_id": "96cbadd9-7d0a-3e67-ab40-ef9fbf2029eb",
                    "sense_definition": "Mieszkanka Kanady; żeńska forma słowa <w>Kanadyjczyk</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Pani Cartier pochodzi z Quebecu, więc jest <w>Kanadyjką</w>.",
                            "target_lang_translations": {
                                "ru": "Госпожа Картье родом из Квебека, поэтому она <w>канадка</w>."
                            }
                        },
                        {
                            "text": "Moja sąsiadka to rodowita <w>Kanadyjka</w>, która przeprowadziła się do Polski rok temu.",
                            "target_lang_translations": {
                                "ru": "Моя соседка — коренная <w>канадка</w>, которая переехала в Польшу год назад."
                            }
                        }
                    ],
                    "antonyms": [
                        "Kanadyjczyk"
                    ],
                    "common_phrases": [
                        "rdzenna Kanadyjka",
                        "znana Kanadyjka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жительница Канады; женская форма слова «канадец»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канадка",
                                "target_lang_sense_clarification": "жительница Канады"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f6d980c-e4c9-33bd-b9e0-bf7cad05ba87",
                    "sense_definition": "Lekka łódź wiosłowa, w której wiosłuje się wiosłem o jednym piórze, klęcząc lub siedząc twarzą do kierunku płynięcia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bogdan nie powinien pływać <w>kanadyjką</w>, bo zawsze wywraca łódź na środku jeziora.",
                            "target_lang_translations": {
                                "ru": "Богдан не должен плавать на <w>каноэ</w>, потому что всегда переворачивает лодку посреди озера."
                            }
                        },
                        {
                            "text": "Podczas wakacji na Mazurach wypożyczyliśmy dwuosobową <w>kanadyjkę</w> na cały dzień.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска на Мазурах мы взяли напрокат двухместное <w>каноэ</w> на весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "canoe",
                        "kanu"
                    ],
                    "common_phrases": [
                        "pływać kanadyjką",
                        "wiosło do kanadyjki",
                        "wyścigi kanadyjek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая гребная лодка, в которой гребут однолопастным веслом, стоя на колене или сидя лицом вперед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каноэ",
                                "target_lang_sense_clarification": "лодка"
                            },
                            {
                                "target_lang_word": "канадка",
                                "target_lang_sense_clarification": "разг. вид лодки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a928f7d0-d738-3ded-8657-baba5e2b31aa",
                    "sense_definition": "Składane łóżko polowe z drewnianym lub metalowym stelażem i naciągniętym na niego materiałem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na biwaku spaliśmy w wojskowych namiotach na niewygodnych <w>kanadyjkach</w>.",
                            "target_lang_translations": {
                                "ru": "В походе мы спали в военных палатках на неудобных <w>раскладушках</w>."
                            }
                        },
                        {
                            "text": "Muszę rozłożyć starą <w>kanadyjkę</w> w salonie, bo przyjeżdżają do nas goście.",
                            "target_lang_translations": {
                                "ru": "Мне нужно разложить старую <w>раскладушку</w> в гостиной, так как к нам приезжают гости."
                            }
                        }
                    ],
                    "synonyms": [
                        "kanu"
                    ],
                    "common_phrases": [
                        "spać na kanadyjce",
                        "rozkładać kanadyjkę",
                        "stara kanadyjka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Складная походная кровать с деревянным или металлическим каркасом и натянутой на него тканью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскладушка",
                                "target_lang_sense_clarification": "кровать"
                            },
                            {
                                "target_lang_word": "походная кровать",
                                "target_lang_sense_clarification": "мебель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "094c9366-f74d-3b82-af8a-b5f5be1a88a8",
                    "sense_definition": "Krótka kurtka, często skórzana, zmarszczona w pasie i ściągnięta paskiem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W szafie dziadka wisiała skórzana <w>kanadyjka</w>, którą nosił w młodości.",
                            "target_lang_translations": {
                                "ru": "В шкафу дедушки висела кожаная <w>канадка</w>, которую он носил в молодости."
                            }
                        },
                        {
                            "text": "Moda na ten rodzaj odzieży powróciła i teraz wiele osób szuka stylowej <w>kanadyjki</w>.",
                            "target_lang_translations": {
                                "ru": "Мода на этот вид одежды вернулась, и теперь многие ищут стильную <w>канадку</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "fashion style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая куртка, часто кожаная, приталенная или стянутая поясом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канадка",
                                "target_lang_sense_clarification": "куртка"
                            }

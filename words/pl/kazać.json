                {
                    "sense_id": "c2334bb7-d53a-3cc1-b58c-d60349ca8dd1",
                    "sense_definition": "Wydać komuś polecenie lub rozkaz wykonania jakiejś czynności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice <kazali> mi posprzątać pokój przed wyjściem do kina.",
                            "target_lang_translations": {
                                "ru": "Родители <велели> мне убрать в комнате перед уходом в кино."
                            }
                        },
                        {
                            "text": "Szef <kazał> pracownikom przygotować raport na poniedziałek rano.",
                            "target_lang_translations": {
                                "ru": "Начальник <приказал> сотрудникам подготовить отчет к утру понедельника."
                            }
                        },
                        {
                            "text": "Nauczycielka <kazała> dzieciom otworzyć podręczniki na stronie dziesiątej.",
                            "target_lang_translations": {
                                "ru": "Учительница <велела> детям открыть учебники на десятой странице."
                            }
                        }
                    ],
                    "synonyms": [
                        "nakazywać",
                        "wskazywać"
                    ],
                    "antonyms": [
                        "prosić",
                        "zabraniać",
                        "zakazywać"
                    ],
                    "common_phrases": [
                        "kazać czekać",
                        "kazać wyjść",
                        "robić co każą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать кому-либо распоряжение или приказ выполнить какое-либо действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "сказать сделать что-то; дать распоряжение"
                            },
                            {
                                "target_lang_word": "приказывать",
                                "target_lang_sense_clarification": "отдавать строгий приказ (официально или властно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71c64fab-242e-38ea-89a8-519c1cd30359",
                    "sense_definition": "Sprawiać, że coś jest konieczne lub sugerować określony wniosek (zwykle o okolicznościach, logice lub tradycji).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dictate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary obyczaj <każe> dzielić się opłatkiem przy wigilijnym stole.",
                            "target_lang_translations": {
                                "ru": "Старинный обычай <велит> делиться облаткой за рождественским столом."
                            }
                        },
                        {
                            "text": "Zdrowy rozsądek <każe> nam zachować ostrożność w tej niebezpiecznej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Здравый смысл <диктует> нам соблюдать осторожность в этой опасной ситуации."
                            }
                        },
                        {
                            "text": "Jego dziwne zachowanie <każe> przypuszczać, że coś przed nami ukrywa.",
                            "target_lang_translations": {
                                "ru": "Его странное поведение <заставляет> предполагать, что он что-то от нас скрывает."
                            }
                        }
                    ],
                    "synonyms": [
                        "nakazywać",
                        "wskazywać"
                    ],
                    "common_phrases": [
                        "jak obyczaj każe",
                        "rozsądek każe",
                        "sytuacja każe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо необходимым или наводить на определенный вывод (обычно об обстоятельствах, логике или традиции)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "перен. требовать, предписывать (о долге, сердце, обычае)"
                            },
                            {
                                "target_lang_word": "диктовать",
                                "target_lang_sense_clarification": "навязывать условия; делать обязательным"
                            },
                            {
                                "target_lang_word": "заставлять",
                                "target_lang_sense_clarification": "вынуждать к действию или наводить на мысль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4646bec-a9a9-31f3-9992-999586718d3f",
                    "sense_definition": "Wygłaszać mowę, zwłaszcza kazanie w kościele.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "preach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wiekach wędrowni kaznodzieje <kazali> na rynkach miast.",
                            "target_lang_translations": {
                                "ru": "В былые времена странствующие проповедники <проповедовали> на городских площадях."
                            }
                        },
                        {
                            "text": "Ksiądz wszedł na ambonę i <kazał> wiernym o miłości bliźniego.",
                            "target_lang_translations": {
                                "ru": "Священник взошел на амвон и <проповедовал> верующим о любви к ближнему."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemawiać",
                        "mówić"
                    ],

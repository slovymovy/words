            "senses": [
                {
                    "sense_id": "1e134eb8-28ba-3da8-b927-17b67a9869d5",
                    "sense_definition": "Meervoud van <w>balk</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het plafond van de oude boerderij wordt gedragen door zware houten <w>balken</w>.",
                            "target_lang_translations": {
                                "en": "The ceiling of the old farmhouse is supported by heavy wooden <w>beams</w>.",
                                "ru": "Потолок старой фермы поддерживается тяжелыми деревянными <w>балками</w>."
                            }
                        },
                        {
                            "text": "Op het televisiescherm verschenen gekleurde <w>balken</w> toen het signaal wegviel.",
                            "target_lang_translations": {
                                "en": "Colored <w>bars</w> appeared on the television screen when the signal was lost.",
                                "ru": "На экране телевизора появились цветные <w>полосы</w>, когда пропал сигнал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "houten balken",
                        "zware balken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of <w>balk</w>.",
                        "ru": "Множественное число от слова <w>balk</w> (балка, брус)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beams",
                                "target_lang_sense_clarification": "construction/wood"
                            },
                            {
                                "target_lang_word": "bars",
                                "target_lang_sense_clarification": "graphical/shape"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "балки",
                                "target_lang_sense_clarification": "строительный элемент"
                            },
                            {
                                "target_lang_word": "брусья",
                                "target_lang_sense_clarification": "пиломатериал"
                            },
                            {
                                "target_lang_word": "полосы",
                                "target_lang_sense_clarification": "на экране"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02d58433-24f5-3de5-9a05-f0b6612e87e4",
                    "sense_definition": "Het typische, luide 'ia'-geluid maken dat ezels maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ezel stond in de wei te <w>balken</w> toen we langs liepen.",
                            "target_lang_translations": {
                                "en": "The donkey was <w>braying</w> in the meadow when we walked past.",
                                "ru": "Осел стоял на лугу и <w>ревел</w>, когда мы проходили мимо."
                            }
                        },
                        {
                            "text": "Ik werd vanochtend vroeg wakker omdat de ezel van de buren begon te <w>balken</w>.",
                            "target_lang_translations": {
                                "en": "I woke up early this morning because the neighbors' donkey started <w>braying</w>.",
                                "ru": "Я проснулся сегодня рано утром, потому что соседский осел начал <w>реветь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "iaën"
                    ],
                    "common_phrases": [
                        "staan te balken"
                    ],
                    "target_lang_definitions": {
                        "en": "To make the typical, loud 'hee-haw' sound that donkeys make.",
                        "ru": "Издавать характерный громкий звук «иа», свойственный ослам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bray",
                                "target_lang_sense_clarification": "donkey sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реветь",
                                "target_lang_sense_clarification": "об осле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87da3852-0bce-3e23-b88f-6fb81cd1b940",
                    "sense_definition": "Luidkeels en vaak onwelluidend schreeuwen, zingen of huilen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stond luidkeels een lied te <w>balken</w> onder de douche.",
                            "target_lang_translations": {
                                "en": "He was loudly <w>bellowing</w> a song in the shower.",
                                "ru": "Он стоял и во все горло <w>горланил</w> песню в душе."
                            }
                        },
                        {
                            "text": "Hou op met <w>balken</w> en vertel rustig wat er aan de hand is.",
                            "target_lang_translations": {
                                "en": "Stop <w>bawling</w> and calmly tell me what is going on.",
                                "ru": "Перестань <w>реветь</w> и расскажи спокойно, что случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "brullen",
                        "schreeuwen",
                        "loeien"
                    ],
                    "antonyms": [
                        "fluisteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To scream, sing, or cry loudly and often unpleasantly.",
                        "ru": "Громко и часто неприятно кричать, петь или плакать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bellow",
                                "target_lang_sense_clarification": "shout or sing loudly"
                            },
                            {
                                "target_lang_word": "bawl",
                                "target_lang_sense_clarification": "cry or shout loudly"
                            },
                            {
                                "target_lang_word": "bray",
                                "target_lang_sense_clarification": "make a harsh sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "горланить",
                                "target_lang_sense_clarification": "громко петь"
                            },
                            {
                                "target_lang_word": "орать",
                                "target_lang_sense_clarification": "кричать"
                            },
                            {
                                "target_lang_word": "реветь",
                                "target_lang_sense_clarification": "громко плакать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

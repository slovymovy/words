{
    "word_family": [
        "buiteling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2bfd6eb0-3ced-3613-ad87-674796bc9d9b",
                    "sense_definition": "Over de kop gaan, rollen of vallen, vaak op een onhandige of speelse manier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is gisteren per ongeluk van de trap <w>gebuiteld</w>.",
                            "target_lang_translations": {
                                "en": "He accidentally <w>tumbled</w> down the stairs yesterday.",
                                "ru": "Вчера он случайно <w>скатился</w> с лестницы."
                            }
                        },
                        {
                            "text": "De jonge honden <w>buitelden</w> vrolijk over elkaar heen in het gras.",
                            "target_lang_translations": {
                                "en": "The young dogs happily <w>tumbled</w> over each other in the grass.",
                                "ru": "Молодые собаки весело <w>кувыркались</w> друг через друга в траве."
                            }
                        }
                    ],
                    "synonyms": [
                        "duikelen"
                    ],
                    "common_phrases": [
                        "van de trap buitelen",
                        "in het gras buitelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go head over heels, roll, or fall, often in a clumsy or playful way.",
                        "ru": "Кувыркаться, падать или катиться, часто неуклюже или игриво."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tumble",
                                "target_lang_sense_clarification": "general movement of rolling or falling"
                            },
                            {
                                "target_lang_word": "somersault",
                                "target_lang_sense_clarification": "acrobatic or playful rolling movement"
                            },
                            {
                                "target_lang_word": "topple",
                                "target_lang_sense_clarification": "to fall over, often from a height"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кувыркаться",
                                "target_lang_sense_clarification": "делать кувырки, переворачиваться через голову (часто в игре)"
                            },
                            {
                                "target_lang_word": "лететь кубарем",
                                "target_lang_sense_clarification": "падать, вращаясь (обычно неуклюже)"
                            },
                            {
                                "target_lang_word": "скатываться",
                                "target_lang_sense_clarification": "падать вниз, вращаясь (например, с лестницы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c30da93-c4b8-3af2-98d7-711c253901ed",
                    "sense_definition": "Elkaar op een chaotische manier verdringen of heel snel opvolgen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De journalisten <w>buitelden</w> over elkaar heen om de beroemde filmster te spreken.",
                            "target_lang_translations": {
                                "en": "The journalists <w>tumbled</w> over each other to speak to the famous movie star.",
                                "ru": "Журналисты <w>лезли наперебой</w>, чтобы поговорить с известной кинозвездой."
                            }
                        },
                        {
                            "text": "Tijdens de vergadering <w>buitelden</w> de nieuwe plannen en ideeën over elkaar heen.",
                            "target_lang_translations": {
                                "en": "During the meeting, the new plans and ideas <w>tumbled</w> over one another.",
                                "ru": "Во время собрания новые планы и идеи <w>опережали</w> друг друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "duikelen"
                    ],
                    "common_phrases": [
                        "over elkaar heen buitelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To crowd one another chaotically or follow in quick succession.",
                        "ru": "Хаотично теснить друг друга или следовать друг за другом в быстрой последовательности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tumble over each other",
                                "target_lang_sense_clarification": "figurative, to crowd or rush chaotically"
                            },
                            {
                                "target_lang_word": "trip over each other",
                                "target_lang_sense_clarification": "idiomatic, to be overly eager or chaotic"
                            },
                            {
                                "target_lang_word": "scramble",
                                "target_lang_sense_clarification": "to move or act quickly and chaotically"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лезть наперебой",
                                "target_lang_sense_clarification": "стараться опередить друг друга в спешке"
                            },
                            {
                                "target_lang_word": "теснить друг друга",
                                "target_lang_sense_clarification": "толкаться, мешать друг другу"
                            },
                            {
                                "target_lang_word": "опережать друг друга",
                                "target_lang_sense_clarification": "быстро сменять друг друга (об идеях, мыслях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
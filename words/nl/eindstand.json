                {
                    "sense_id": "73077e00-5dbf-3080-a963-da8db023bb2d",
                    "sense_definition": "De definitieve score of het resultaat aan het einde van een sportwedstrijd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>eindstand</w> van de spannende voetbalwedstrijd was uiteindelijk drie tegen twee.",
                            "target_lang_translations": {
                                "ru": "<w>Итоговый счет</w> захватывающего футбольного матча в итоге составил три-два."
                            }
                        },
                        {
                            "text": "Na het laatste fluitsignaal verscheen de definitieve <w>eindstand</w> groot op het scorebord.",
                            "target_lang_translations": {
                                "ru": "После финального свистка на табло крупно высветился окончательный <w>счет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "einduitslag",
                        "eindscore"
                    ],
                    "antonyms": [
                        "tussenstand"
                    ],
                    "common_phrases": [
                        "de definitieve eindstand",
                        "eindstand opmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательный счет или результат в конце спортивного матча."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "итоговый счет",
                                "target_lang_sense_clarification": "результат матча"
                            },
                            {
                                "target_lang_word": "окончательный результат",
                                "target_lang_sense_clarification": "общий итог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9be609e0-adf2-3d5f-9d68-1f98fa360ec9",
                    "sense_definition": "De waarde die een meetinstrument aangeeft aan het einde van een bepaalde periode.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor de jaarafrekening moet u de <w>eindstand</w> van de gasmeter doorgeven aan het energiebedrijf.",
                            "target_lang_translations": {
                                "ru": "Для годового расчета вы должны передать <w>конечное показание</w> газового счетчика в энергетическую компанию."
                            }
                        },
                        {
                            "text": "Het verschil tussen de beginstand en de <w>eindstand</w> bepaalt hoeveel water u heeft verbruikt.",
                            "target_lang_translations": {
                                "ru": "Разница между начальным и <w>конечным показанием</w> определяет, сколько воды вы потребили."
                            }
                        }
                    ],
                    "antonyms": [
                        "beginstand"
                    ],
                    "common_phrases": [
                        "eindstand van de meter",
                        "eindstand doorgeven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Значение, которое показывает измерительный прибор в конце определенного периода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечное показание",
                                "target_lang_sense_clarification": "счетчика или прибора"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        {
                            "text": "Wilk rozszarpał ofiarę i wyjadł jej <w>flaki</w>.",
                            "target_lang_translations": {
                                "ru": "Волк растерзал жертву и выел её <w>кишки</w>."
                            }
                        }
                    ],
                    "common_phrases": [

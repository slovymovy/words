{
    "word_family": [
        "exhale",
        "exhalation"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6895b8a5-5efd-336c-aa61-0b1f1e771ccf",
                    "sense_definition": "Describing a structure or process that breathes out or emits fluid, air, or vapor.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>exhalant</w> siphon pushes water away from the animal's body.",
                            "target_lang_translations": {
                                "ru": "<w>Выводной</w> сифон выталкивает воду прочь от тела животного."
                            }
                        },
                        {
                            "text": "Biologists measured the speed of the <w>exhalant</w> current produced by the sponge.",
                            "target_lang_translations": {
                                "ru": "Биологи измерили скорость <w>выводного</w> течения, создаваемого губкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "emitting",
                        "expelling",
                        "discharging"
                    ],
                    "antonyms": [
                        "inhalant"
                    ],
                    "common_phrases": [
                        "exhalant siphon",
                        "exhalant current"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывающий структуру или процесс, который выводит или выбрасывает жидкость, воздух или пар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выводной",
                                "target_lang_sense_clarification": "о сифоне или потоке жидкости (биол.)"
                            },
                            {
                                "target_lang_word": "выдыхательный",
                                "target_lang_sense_clarification": "относящийся к процессу выдоха"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e8cae8cf-1e1d-3616-89a1-8d630fa9d373",
                    "sense_definition": "A structure, duct, or substance that emits a fluid or vapor.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This small duct serves as an <w>exhalant</w> for the waste products.",
                            "target_lang_translations": {
                                "ru": "Этот маленький проток служит <w>выводным каналом</w> для продуктов жизнедеятельности."
                            }
                        },
                        {
                            "text": "The scientist identified the pore as an <w>exhalant</w> rather than an intake.",
                            "target_lang_translations": {
                                "ru": "Ученый определил эту пору как <w>выводное отверстие</w>, а не как впускное."
                            }
                        }
                    ],
                    "synonyms": [
                        "emitter",
                        "vent",
                        "outlet"
                    ],
                    "antonyms": [
                        "inhalant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Структура, проток или вещество, выделяющее жидкость или пар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выводной канал",
                                "target_lang_sense_clarification": "анатомическая структура"
                            },
                            {
                                "target_lang_word": "выводное отверстие",
                                "target_lang_sense_clarification": "место выхода жидкости или газа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
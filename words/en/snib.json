            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "51980bb6-4d46-3722-b325-c741cbcc8767",
                    "sense_definition": "To fasten or lock a door or window using a small latch.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fastening_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>snib</w> the window to stop it from banging in the wind.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>защёлкни</w> окно, чтобы оно не хлопало на ветру."
                            }
                        },
                        {
                            "text": "He <w>snibbed</w> the door shut to ensure they would not be disturbed.",
                            "target_lang_translations": {
                                "ru": "Он <w>запер на защёлку</w> дверь, чтобы их не беспокоили."
                            }
                        }
                    ],
                    "synonyms": [
                        "latch",
                        "bolt",
                        "fasten",
                        "lock"
                    ],
                    "antonyms": [
                        "unlatch",
                        "unlock",
                        "open"
                    ],
                    "common_phrases": [
                        "snib the door",
                        "snib the window"
                    ],

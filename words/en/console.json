    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "418f0d04-9154-3f23-9b7c-60d5c9347c7c",
                    "sense_definition": "An electronic device designed for playing video games on a television or display screen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "electronic_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother wants a new <w>console</w> for his birthday so he can play the latest games.",
                            "target_lang_translations": {
                                "ru": "Мой брат хочет новую <w>игровую приставку</w> на день рождения, чтобы играть в новейшие игры."
                            }
                        },
                        {
                            "text": "Most modern <w>consoles</w> allow you to connect to the internet and play with friends.",
                            "target_lang_translations": {
                                "ru": "Большинство современных <w>консолей</w> позволяют подключаться к интернету и играть с друзьями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "game console",
                        "video game console",
                        "gaming console"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство, предназначенное для видеоигр на телевизоре или экране."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игровая приставка",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "консоль",
                                "target_lang_sense_clarification": "видеоигры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dc79659-b5fa-3e15-a122-40c31206b12f",
                    "sense_definition": "A panel or unit containing controls, switches, and displays used to operate a computer or electronic system.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "control_panel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer sat at the main <w>console</w> to monitor the power plant's performance.",
                            "target_lang_translations": {
                                "ru": "Инженер сидел за главным <w>пультом управления</w>, чтобы следить за работой электростанции."
                            }
                        },
                        {
                            "text": "You can enter commands directly into the system <w>console</w> to fix the error.",
                            "target_lang_translations": {
                                "ru": "Вы можете вводить команды прямо в системную <w>консоль</w>, чтобы исправить ошибку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "control console",
                        "operator console",
                        "admin console"
                    ],
                    "target_lang_definitions": {
                        "ru": "Панель или блок с элементами управления, переключателями и дисплеями для работы с компьютером или технической системой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульт управления",
                                "target_lang_sense_clarification": "панель приборов"
                            },
                            {
                                "target_lang_word": "консоль",
                                "target_lang_sense_clarification": "интерфейс ввода команд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d30f7e1-7569-3590-b0e2-89e4c76ea602",
                    "sense_definition": "A storage compartment or control panel located between the front seats of a vehicle.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "control_panel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She put her coffee cup in the holder on the center <w>console</w>.",
                            "target_lang_translations": {
                                "ru": "Она поставила чашку с кофе в держатель на центральной <w>консоли</w>."
                            }
                        },
                        {
                            "text": "I keep my sunglasses and spare change in the car's <w>console</w>.",
                            "target_lang_translations": {
                                "ru": "Я держу солнечные очки и мелочь в автомобильной <w>консоли</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "center console"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отделение для хранения вещей или панель управления, расположенная между передними сиденьями автомобиля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консоль",
                                "target_lang_sense_clarification": "в автомобиле"
                            },
                            {
                                "target_lang_word": "центральная консоль",
                                "target_lang_sense_clarification": "часть салона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c20eb349-ff73-3b5c-b061-0e0c7d6f83a7",
                    "sense_definition": "An ornamental bracket, often curved in an S-shape, used in architecture to support a shelf, cornice, or other structure.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy stone balcony was supported by a decorative <w>console</w> on each side.",
                            "target_lang_translations": {
                                "ru": "Тяжелый каменный балкон поддерживался декоративной <w>консолью</w> с каждой стороны."
                            }
                        },
                        {
                            "text": "Restorers carefully cleaned the carved <w>console</w> under the window ledge.",
                            "target_lang_translations": {
                                "ru": "Реставраторы осторожно очистили резную <w>консоль</w> под подоконником."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "architecture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный кронштейн, часто S-образной формы, используемый в архитектуре для поддержки карниза, балкона или полки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консоль",
                                "target_lang_sense_clarification": "архитектура"
                            }
                        ]

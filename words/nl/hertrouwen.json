                    "sense_id": "76a1c175-85c7-3212-99a8-27a941ae82b0",
                    "sense_definition": "Opnieuw in het huwelijk treden nadat een eerder huwelijk is beëindigd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twee jaar na het overlijden van zijn eerste vrouw is hij <w>hertrouwd</w>.",
                            "target_lang_translations": {
                                "en": "Two years after the death of his first wife, he <w>remarried</w>.",
                                "ru": "Через два года после смерти первой жены он <w>женился снова</w>."
                            }
                        },
                        {
                            "text": "Na haar scheiding besloot ze om niet meer te <w>hertrouwen</w>.",
                            "target_lang_translations": {
                                "en": "After her divorce, she decided not to <w>remarry</w>.",
                                "ru": "После развода она решила больше не <w>выходить замуж повторно</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hertrouwen met"
                    ],
                    "target_lang_definitions": {
                        "en": "To marry again after a previous marriage has ended.",
                        "ru": "Вступить в брак еще раз после того, как предыдущий брак был расторгнут."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remarry",
                                "target_lang_sense_clarification": "to marry again"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вступить в повторный брак",
                                "target_lang_sense_clarification": "официально оформить отношения снова"
                            },
                            {
                                "target_lang_word": "снова пожениться",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                }
            ]
        },

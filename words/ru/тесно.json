    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e3f585e7-69ac-305a-9c21-33fc9f853eab",
                    "sense_definition": "В тесной связи, в непосредственном общении или взаимодействии.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наши компании <тесно> сотрудничают уже много лет.",
                            "target_lang_translations": {
                                "en": "Our companies have been cooperating <closely> for many years."
                            }
                        },
                        {
                            "text": "Их судьбы <тесно> переплелись в этом странном городе.",
                            "target_lang_translations": {
                                "en": "Their fates were <closely> intertwined in this strange city."
                            }
                        }
                    ],
                    "synonyms": [
                        "неразрывно",
                        "близко"
                    ],
                    "common_phrases": [
                        "тесно сотрудничать",
                        "тесно связаны"
                    ],
                    "target_lang_definitions": {
                        "en": "In a close relationship, direct communication, or interaction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "closely",
                                "target_lang_sense_clarification": "regarding cooperation or connection"
                            },
                            {
                                "target_lang_word": "intimately",
                                "target_lang_sense_clarification": "deeply connected or acquainted"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffc2e666-c2af-34c7-8887-148f1dec2f2b",
                    "sense_definition": "Располагаясь очень близко друг к другу, вплотную; без свободного пространства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_tightness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старые дома на этой улице стоят очень <тесно>.",
                            "target_lang_translations": {
                                "en": "The old houses on this street stand very <close together>."
                            }
                        },
                        {
                            "text": "Пассажиры в автобусе сидели <тесно>, плечом к плечу.",
                            "target_lang_translations": {
                                "en": "The passengers in the bus sat <tightly packed>, shoulder to shoulder."
                            }
                        }
                    ],
                    "synonyms": [
                        "плотно",
                        "вплотную"
                    ],
                    "antonyms": [
                        "просторно"
                    ],
                    "common_phrases": [
                        "тесно прижаться",
                        "стоять тесно"
                    ],
                    "target_lang_definitions": {
                        "en": "Positioned very near each other; without free space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tightly",
                                "target_lang_sense_clarification": "packed firmly together"
                            },
                            {
                                "target_lang_word": "close together",
                                "target_lang_sense_clarification": "regarding proximity of objects"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3ae03869-af29-3423-81f4-ea760207a321",
                    "sense_definition": "Ощущение нехватки места, пространства; когда мало места.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_tightness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашей маленькой квартире нам стало слишком <тесно>.",
                            "target_lang_translations": {
                                "en": "It has become too <cramped> for us in our small apartment."
                            }
                        },
                        {
                            "text": "В переполненном вагоне метро было душно и <тесно>.",
                            "target_lang_translations": {
                                "en": "It was stuffy and <crowded> in the packed subway car."
                            }
                        }
                    ],
                    "antonyms": [
                        "просторно"
                    ],
                    "common_phrases": [
                        "здесь тесно",
                        "мне тесно"
                    ],
                    "target_lang_definitions": {
                        "en": "A sensation of lack of space; when there is little room."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cramped",
                                "target_lang_sense_clarification": "lacking physical space"
                            },
                            {
                                "target_lang_word": "crowded",
                                "target_lang_sense_clarification": "full of people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f322e34-22d1-3274-8742-78e9dc059a2c",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>тесный</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_tightness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это красивое платье мне, к сожалению, уже <тесно>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, this beautiful dress is already too <tight> for me."
                            }
                        },
                        {
                            "text": "Кольцо стало <тесно>, и я никак не могу его снять.",
                            "target_lang_translations": {
                                "en": "The ring has become too <tight>, and I can't take it off at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "узко"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Fitting too closely to the body; not big enough (of clothing/items)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tight",
                                "target_lang_sense_clarification": "clothing or accessories"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
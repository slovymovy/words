        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aee3cc92-9d2b-36e5-bd0d-22dc763b10e2",
                    "sense_definition": "To remove seeds from cotton using a machine.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "process_cotton",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers <w>ginned</w> the cotton before baling it for transport.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>джинировали</w> хлопок перед прессованием в кипы для транспортировки."
                            }
                        },
                        {
                            "text": "It is much faster to <w>gin</w> the crop mechanically than by hand.",
                            "target_lang_translations": {
                                "ru": "Гораздо быстрее <w>джинировать</w> урожай механически, чем вручную."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отделять семена от хлопка с помощью специальной машины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "джинировать",
                                "target_lang_sense_clarification": "обрабатывать хлопок"
                            },
                            {
                                "target_lang_word": "очищать от семян",
                                "target_lang_sense_clarification": "хлопок"
                            }
                        ]
                    }
                },

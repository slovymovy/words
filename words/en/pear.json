    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3d8e05f8-8e8e-324e-b48f-880f4a9f7ed0",
                    "sense_definition": "An edible fruit produced by the pear tree, similar to an apple but typically elongated towards the stem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She packed a juicy <w>pear</w> in her lunchbox.",
                            "target_lang_translations": {
                                "ru": "Она положила сочную <w>грушу</w> в свой ланчбокс."
                            }
                        },
                        {
                            "text": "The <w>pear</w> was sweet and soft to the bite.",
                            "target_lang_translations": {
                                "ru": "<w>Груша</w> была сладкой и мягкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ripe pear",
                        "prickly pear",
                        "pear juice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный плод грушевого дерева, похожий на яблоко, но обычно вытянутый к черенку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "груша",
                                "target_lang_sense_clarification": "фрукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97fc9260-a41d-3b1c-a084-3e2b222eaa60",
                    "sense_definition": "A type of fruit tree (Pyrus communis).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We planted a young <w>pear</w> in the backyard.",
                            "target_lang_translations": {
                                "ru": "Мы посадили молодую <w>грушу</w> на заднем дворе."
                            }
                        },
                        {
                            "text": "The old <w>pear</w> blooms with white flowers every spring.",
                            "target_lang_translations": {
                                "ru": "Старая <w>груша</w> цветет белыми цветами каждую весну."
                            }
                        }
                    ],
                    "synonyms": [
                        "pear tree"
                    ],
                    "common_phrases": [
                        "flowering pear"
                    ],
                    "target_lang_definitions": {

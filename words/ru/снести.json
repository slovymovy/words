                    ],
                    "synonyms": [
                        "отнести",
                        "доставить"
                    ],
                    "common_phrases": [
                        "снести в ремонт",
                        "снести письмо",
                        "снести багаж"
                    ],
                    "target_lang_definitions": {
                        "en": "To take or deliver something to another place or a specific person."
                    },
                    "translations": {

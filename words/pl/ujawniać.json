                            },
                            {
                                "target_lang_word": "сдаваться",
                                "target_lang_sense_clarification": "властям или полиции"
                            },
                            {
                                "target_lang_word": "легализоваться",
                                "target_lang_sense_clarification": "официально объявлять о своем статусе"
                            }
                        ]
                    }
                }
            ]
        }

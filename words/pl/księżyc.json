    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83dbc6d9-7f63-342f-ac78-97d601c25845",
                    "sense_definition": "Księżyc (naturalny satelita Ziemi) widziany jako świecąca tarcza lub sierp na niebie; obiekt ten postrzegany z perspektywy obserwatora na Ziemi",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "celestial_body_view",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś w nocy <księżyc> świeci wyjątkowo jasno.",
                            "target_lang_translations": {
                                "ru": "Сегодня ночью <луна> светит особенно ярко."
                            }
                        },
                        {
                            "text": "Wilki w lesie zaczęły głośno wyć do <księżyca>.",
                            "target_lang_translations": {
                                "ru": "Волки в лесу начали громко выть на <луну>."
                            }
                        },
                        {
                            "text": "Ponad lasem widać było wschodzący <księżyc>.",
                            "target_lang_translations": {
                                "ru": "Над лесом было видно восходящую <луну>."
                            }
                        }
                    ],
                    "synonyms": [
                        "miesiąc",
                        "łysy"
                    ],
                    "common_phrases": [
                        "pełnia księżyca",
                        "nów księżyca",
                        "w blasku księżyca",
                        "wschód księżyca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Луна (естественный спутник Земли), видимая как светящийся диск или серп на небе; объект с точки зрения наблюдателя на Земле"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "небесное тело"
                            },
                            {
                                "target_lang_word": "месяц",
                                "target_lang_sense_clarification": "серп луны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0df4963e-7d9e-358d-8240-b1bf3acca3c6",
                    "sense_definition": "ciało niebieskie obiegające planetę, planetę karłowatą lub planetoidę; naturalny satelita",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "astronomy_satellite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jowisz posiada wiele naturalnych <księżyców> o różnej wielkości.",
                            "target_lang_translations": {
                                "ru": "У Юпитера много естественных <спутников> разной величины."
                            }
                        },
                        {
                            "text": "Tytan jest największym <księżycem> krążącym wokół Saturna.",
                            "target_lang_translations": {
                                "ru": "Титан является самым большим <спутником>, вращающимся вокруг Сатурна."
                            }
                        }
                    ],
                    "synonyms": [
                        "satelita",
                        "naturalny satelita"
                    ],
                    "common_phrases": [
                        "księżyce Jowisza",
                        "odkryć nowy księżyc"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical/astronomical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, вращающееся вокруг планеты, карликовой планеты или астероида; естественный спутник"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спутник",
                                "target_lang_sense_clarification": "астрономический объект"
                            },
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "спутник планеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff69344f-efc3-3afc-ac3a-9a70ff1b0876",
                    "sense_definition": "przedmiot, rysunek, wypiek lub figura w kształcie sierpa, przypominająca Księżyc w jednej z jego faz",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na choince wisiały piernikowe gwiazdki i lukrowane <księżyce>.",
                            "target_lang_translations": {
                                "ru": "На ёлке висели пряничные звёздочки и глазированные <полумесяцы>."
                            }
                        },
                        {
                            "text": "Dziecko narysowało na kartce żółty <księżyc> i słońce.",
                            "target_lang_translations": {
                                "ru": "Ребёнок нарисовал на листе жёлтый <месяц> и солнце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rogalik w kształcie księżyca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, рисунок, выпечка или фигура в форме серпа, напоминающая Луну в одной из её фаз"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полумесяц",
                                "target_lang_sense_clarification": "форма"
                            },
                            {
                                "target_lang_word": "месяц",
                                "target_lang_sense_clarification": "фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "634f5389-d740-3f43-9196-627e104a0c5d",
                    "sense_definition": "światło odbite od Księżyca; blask księżycowy oświetlający otoczenie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały ogród był zalany srebrnym <księżycem>.",
                            "target_lang_translations": {
                                "ru": "Весь сад был залит серебряным <лунным светом>."
                            }
                        },
                        {
                            "text": "Śnieg iskrzył się w jasnym <księżycu> podczas mroźnej nocy.",
                            "target_lang_translations": {
                                "ru": "Снег искрился при яркой <луне> в морозную ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "luna",
                        "srebrny glob"
                    ],
                    "common_phrases": [
                        "zalany księżycem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or colloquial usage referring to light"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свет, отражённый от Луны; лунное сияние, освещающее окрестности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лунный свет",
                                "target_lang_sense_clarification": "освещение"
                            },
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "свет (метонимия)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4e46836-68f3-3059-8e0d-cf307aa51084",
                    "sense_definition": "dawne określenie miesiąca jako jednostki czasu, oparte na cyklu faz Księżyca",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz obiecał wrócić do zamku, zanim minie kolejny <księżyc>.",
                            "target_lang_translations": {
                                "ru": "Рыцарь обещал вернуться в замок, прежде чем пройдёт ещё одна <луна>."
                            }
                        },
                        {
                            "text": "Czekał na nią przez wiele długich <księżyców>.",
                            "target_lang_translations": {
                                "ru": "Он ждал её много долгих <лун>."
                            }
                        }
                    ],
                    "synonyms": [
                        "miesiąc"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used mainly in historical or fantasy contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное обозначение месяца как единицы времени, основанное на цикле фаз Луны"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "единица времени (архаичное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [

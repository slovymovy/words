    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f5c97119-8398-3be3-a701-fbf87465b777",
                    "sense_definition": "Rzemieślnik zajmujący się wyrobem naczyń i innych przedmiotów z gliny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "professions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wyschnięciu <w>garncarz</w> wstawia garnki do pieca.",
                            "target_lang_translations": {
                                "ru": "После высыхания <w>гончар</w> ставит горшки в печь."
                            }
                        },
                        {
                            "text": "Na jarmarku stary <w>garncarz</w> sprzedawał ręcznie robione misy.",
                            "target_lang_translations": {
                                "ru": "На ярмарке старый <w>гончар</w> продавал миски ручной работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. garcorz"
                    ],
                    "common_phrases": [
                        "koło garncarza",
                        "warsztat garncarza",
                        "zawód garncarza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремесленник, занимающийся изготовлением посуды и других предметов из глины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гончар",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3ec905b-8ec3-35d2-8b89-33bee0d4c0c1",
                    "sense_definition": "Ptak z Ameryki Południowej budujący gniazda z gliny, które przypominają garnki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samiczka <w>garncarza</w> wyściela gniazdo trawą i pierzem.",
                            "target_lang_translations": {
                                "ru": "Самка <w>печника</w> выстилает гнездо травой и перьями."
                            }
                        },
                        {
                            "text": "Mały <w>garncarz</w> siedział na płocie i śpiewał głośno.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>печник</w> сидел на заборе и громко пел."
                            }
                        }
                    ],
                    "synonyms": [
                        "garncarz rdzawy"
                    ],
                    "common_phrases": [
                        "gniazdo garncarza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Птица из Южной Америки, строящая гнезда из глины, напоминающие горшки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печник",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "639efe1b-1cf6-3718-b669-4a1ea66bcac5",
                    "sense_definition": "Rzemieślnik budujący i naprawiający piece; zdun.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "professions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>garncarz</w> postawił u nas solidny piec kaflowy.",
                            "target_lang_translations": {
                                "ru": "Этот <w>печник</w> сложил у нас добротную изразцовую печь."
                            }
                        },
                        {
                            "text": "W starej kamienicy <w>garncarz</w> naprawiał przewody kominowe.",
                            "target_lang_translations": {
                                "ru": "В старом каменном доме <w>печник</w> ремонтировал дымоходы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Poznań"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "gwara poznańska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мастер, занимающийся кладкой и ремонтом печей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печник",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9b0f27d5-4eee-3c3f-b92b-706aefbd765e",
                    "sense_definition": "Przełożona żeńskiego klasztoru w niektórych zakonach chrześcijańskich.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa <w>ksieni</w> została uroczyście wprowadzona do klasztoru przez biskupa.",
                            "target_lang_translations": {
                                "ru": "Новая <w>аббатиса</w> была торжественно введена в монастырь епископом."
                            }
                        },
                        {
                            "text": "Siostry zakonne poprosiły o radę, którą mogła im dać tylko doświadczona <w>ksieni</w>.",
                            "target_lang_translations": {
                                "ru": "Монахини попросили совета, который могла им дать только опытная <w>аббатиса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeorysza",
                        "opatka",
                        "abatysa"
                    ],
                    "common_phrases": [
                        "ksieni klasztoru",
                        "wybór ksieni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настоятельница женского монастыря в некоторых христианских орденах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аббатиса",
                                "target_lang_sense_clarification": "настоятельница католического монастыря"
                            },
                            {
                                "target_lang_word": "игуменья",
                                "target_lang_sense_clarification": "настоятельница православного монастыря"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3177abe8-7178-319d-9d18-eec32f85f0fb",
                    "sense_definition": "Dawne określenie księżnej lub żony władcy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noble_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych kronikach zapisano, że <w>ksieni</w> rządziła tymi ziemiami sprawiedliwie.",
                            "target_lang_translations": {
                                "ru": "В старых хрониках записано, что <w>княгиня</w> справедливо правила этими землями."
                            }
                        },
                        {
                            "text": "Młoda <w>ksieni</w> przybyła na zamek w towarzystwie licznej świty.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>княгиня</w> прибыла в замок в сопровождении многочисленной свиты."
                            }
                        }
                    ],
                    "synonyms": [
                        "księżna"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo wyszło z powszechnego użycia w tym znaczeniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название княгини или жены правителя"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "княгиня",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e2b0a07-228b-3dec-a994-dc216f30b2cf",
                    "sense_definition": "Dawne określenie kapłanki, szczególnie w kontekście pogańskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara <w>ksieni</w> odprawiała tajemnicze rytuały w świętym gaju.",
                            "target_lang_translations": {
                                "ru": "Старая <w>жрица</w> совершала таинственные ритуалы в священной роще."
                            }
                        },
                        {
                            "text": "Ludzie wierzyli, że <w>ksieni</w> potrafi rozmawiać z duchami przodków.",
                            "target_lang_translations": {
                                "ru": "Люди верили, что <w>жрица</w> может разговаривать с духами предков."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapłanka"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w kontekście historycznym lub literackim"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название жрицы, особенно в языческом контексте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жрица",
                                "target_lang_sense_clarification": "языческая служительница культа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

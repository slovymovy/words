            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eabb9652-d708-3754-af4c-e13b08406af2",
                    "sense_definition": "Cienka warstwa substancji gromadząca się na powierzchni przedmiotu, często trudna do usunięcia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "coating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ściankach czajnika zebrał się biały <w>osad</w> z twardej wody.",
                            "target_lang_translations": {
                                "ru": "На стенках чайника собралась белая <w>накипь</w> от жесткой воды."
                            }
                        },
                        {
                            "text": "Regularne mycie zębów zapobiega powstawaniu nieestetycznego <w>osadu</w>.",
                            "target_lang_translations": {
                                "ru": "Регулярная чистка зубов предотвращает появление неэстетичного <w>налёта</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nalot",
                        "warstewka"
                    ],
                    "common_phrases": [
                        "osad na zębach",
                        "osad z kamienia",
                        "biały osad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий слой вещества, скапливающийся на поверхности предмета, часто трудноудаляемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налёт",
                                "target_lang_sense_clarification": "на зубах, на поверхности"
                            },
                            {
                                "target_lang_word": "накипь",
                                "target_lang_sense_clarification": "известковый, в чайнике"
                            },
                            {
                                "target_lang_word": "отложения",
                                "target_lang_sense_clarification": "накопления вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4801be5e-7bb0-361f-a7ab-9539c2de230c",
                    "sense_definition": "Substancja stała, która wydziela się z cieczy i opada na dno naczynia lub zbiornika.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sediment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dnie butelki ze starym winem widoczny był ciemny <w>osad</w>.",
                            "target_lang_translations": {
                                "ru": "На дне бутылки со старым вином был виден темный <w>осадок</w>."
                            }
                        },
                        {
                            "text": "W procesie oczyszczania ścieków na dnie zbiornika gromadzi się gęsty <w>osad</w>.",
                            "target_lang_translations": {
                                "ru": "В процессе очистки сточных вод на дне резервуара скапливается густой <w>осадок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "szlam",
                        "ustoiny",
                        "strąt"
                    ],
                    "common_phrases": [
                        "osad na dnie",
                        "osad z kawy",
                        "osad rzeczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое вещество, которое выделяется из жидкости и оседает на дно сосуда или резервуара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осадок",
                                "target_lang_sense_clarification": "на дне, химический"
                            },
                            {
                                "target_lang_word": "гуща",
                                "target_lang_sense_clarification": "кофейная, густая масса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28fce798-51c1-3542-ab42-542c97a33d55",
                    "sense_definition": "*Dopełniacz* liczby mnogiej od słowa <w>osada</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele małych <w>osad</w> zostało opuszczonych przez mieszkańców w poszukiwaniu pracy.",
                            "target_lang_translations": {
                                "ru": "Много малых <w>поселений</w> было покинуто жителями в поисках работы."
                            }
                        },
                        {
                            "text": "Historia tych średniowiecznych <w>osad</w> jest niezwykle interesująca dla archeologów.",
                            "target_lang_translations": {
                                "ru": "История этих средневековых <w>поселений</w> чрезвычайно интересна для археологов."
                            }
                        }
                    ],

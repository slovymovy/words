                {
                    "sense_id": "fcc25990-720d-38a9-a9ca-b013bdf602e5",
                    "sense_definition": "Zdrobnienie od słowa <w>buk</w>; małe lub młode drzewo tego gatunku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tree_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na skraju lasu rośnie samotny, młody <w>buczek</w>.",
                            "target_lang_translations": {
                                "ru": "На опушке леса растет одинокий молодой <w>бучок</w>."
                            }
                        },
                        {
                            "text": "Ogrodnik posadził mały <w>buczek</w> w cieniu wielkiego dębu.",
                            "target_lang_translations": {
                                "ru": "Садовник посадил маленький <w>бучок</w> в тени огромного дуба."
                            }
                        }
                    ],
                    "common_phrases": [
                        "młody buczek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «бук»; маленькое или молодое дерево этого вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бучок",
                                "target_lang_sense_clarification": "молодое дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1557219-2737-3859-ad2a-0195ed9ccf07",
                    "sense_definition": "Urządzenie sygnalizacyjne wytwarzające niski, buczący dźwięk, służące do ostrzegania (np. we mgle) lub informowania (np. w fabryce).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "device_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy mgła zgęstniała, kapitan nakazał włączyć <w>buczek</w> mgłowy.",
                            "target_lang_translations": {
                                "ru": "Когда туман сгустился, капитан приказал включить туманный <w>ревун</w>."
                            }
                        },
                        {
                            "text": "Głośny <w>buczek</w> oznajmił pracownikom koniec przerwy śniadaniowej.",
                            "target_lang_translations": {
                                "ru": "Громкий <w>гудок</w> возвестил работникам об окончании перерыва на завтрак."
                            }
                        }
                    ],
                    "synonyms": [
                        "syrena",
                        "sygnalizator"
                    ],
                    "common_phrases": [
                        "buczek mgłowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сигнальное устройство, издающее низкий, гудящий звук, служащее для предупреждения (например, в тумане) или информирования (например, на заводе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ревун",
                                "target_lang_sense_clarification": "сирена, особенно судовая"
                            },
                            {
                                "target_lang_word": "гудок",
                                "target_lang_sense_clarification": "звуковой сигнал"
                            },
                            {
                                "target_lang_word": "зуммер",
                                "target_lang_sense_clarification": "электрический сигнал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6574504e-dbf3-32f8-9e02-db407d3ba139",
                    "sense_definition": "Laska lub kij wykonany z drewna bukowego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object_wood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starszy pan podpierał się sękatym <w>buczkiem</w> podczas spaceru po górach.",
                            "target_lang_translations": {
                                "ru": "Пожилой господин опирался на сучковатую <w>буковую палку</w> во время прогулки по горам."
                            }
                        },
                        {
                            "text": "Pasterz wyciosał sobie prosty <w>buczek</w> do zaganiania owiec.",
                            "target_lang_translations": {
                                "ru": "Пастух вытесал себе простой <w>посох</w> для загона овец."
                            }
                        }
                    ],
                    "synonyms": [
                        "laska",
                        "kij"
                    ],

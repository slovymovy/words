    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8674f4fe-ed30-3247-a612-df93cd21dbac",
                    "sense_definition": "Having or marked by bad luck; not favored by fortune.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "lack_of_fortune",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>unfortunate</w> traveler lost his passport right before the flight.",
                            "target_lang_translations": {
                                "ru": "<w>Неудачливый</w> путешественник потерял паспорт прямо перед вылетом."
                            }
                        },
                        {
                            "text": "It was an <w>unfortunate</w> accident that caused a lot of damage to the car.",
                            "target_lang_translations": {
                                "ru": "Это был <w>несчастный</w> случай, который причинил машине сильный ущерб."
                            }
                        }
                    ],
                    "synonyms": [
                        "unlucky"
                    ],
                    "antonyms": [
                        "lucky",
                        "fortunate"
                    ],
                    "common_phrases": [
                        "unfortunate accident",
                        "unfortunate event",
                        "unfortunate circumstance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сопровождаемый неудачей или отмеченный невезением; преследуемый злым роком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудачливый",
                                "target_lang_sense_clarification": "о человеке, которому не везет"
                            },
                            {
                                "target_lang_word": "несчастный",
                                "target_lang_sense_clarification": "вызывающий жалость или пострадавший от беды"
                            },
                            {
                                "target_lang_word": "неудачный",
                                "target_lang_sense_clarification": "о событии или стечении обстоятельств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77927803-01f7-3762-8a66-6cd4d427cf7d",
                    "sense_definition": "Regrettable, unsuitable, or inappropriate.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made an <w>unfortunate</w> remark that upset several guests at the dinner.",
                            "target_lang_translations": {
                                "ru": "Он сделал <w>неудачное</w> замечание, которое расстроило нескольких гостей за ужином."
                            }
                        },
                        {
                            "text": "It is <w>unfortunate</w> that we cannot attend the wedding due to work.",
                            "target_lang_translations": {
                                "ru": "<w>Досадно</w>, что мы не можем пойти на свадьбу из-за работы."
                            }
                        }
                    ],
                    "antonyms": [
                        "fortunate"
                    ],
                    "common_phrases": [
                        "unfortunate mistake",
                        "unfortunate remark",
                        "unfortunate choice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий сожаление; неуместный или неловкий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудачный",
                                "target_lang_sense_clarification": "неуместный, например, о замечании"
                            },
                            {
                                "target_lang_word": "досадный",
                                "target_lang_sense_clarification": "вызывающий чувство досады или сожаления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b099a0e-c4d9-3771-8836-deadb6443561",
                    "sense_definition": "An unlucky person; someone who has fallen into bad circumstances.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lack_of_fortune",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shelter provides warm meals for the <w>unfortunates</w> of the city.",
                            "target_lang_translations": {
                                "ru": "Приют предоставляет горячее питание для городских <w>несчастных</w>."
                            }
                        },
                        {
                            "text": "She felt deep pity for the poor <w>unfortunate</w> shivering in the cold.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала глубокую жалость к бедному <w>бедолаге</w>, дрожащему на холоде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poor unfortunate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, попавший в беду или находящийся в тяжелых жизненных обстоятельствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несчастный",
                                "target_lang_sense_clarification": "сущ. человек, достойный жалости"
                            },
                            {
                                "target_lang_word": "бедолага",
                                "target_lang_sense_clarification": "разг. бедный, невезучий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bc52b93-52dc-39a2-bf8f-25c695ca2b92",
                    "sense_definition": "A prostitute.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old novel, the main character tries to save an <w>unfortunate</w> from the streets.",
                            "target_lang_translations": {
                                "ru": "В старом романе главный герой пытается спасти <w>падшую женщину</w> с улицы."
                            }
                        },
                        {
                            "text": "Victorian society often looked down upon the <w>unfortunate</w> rather than offering help.",
                            "target_lang_translations": {
                                "ru": "Викторианское общество часто смотрело свысока на <w>падших женщин</w>, вместо того чтобы предлагать помощь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "euphemistic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проститутка (устаревшее, эвфемизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падшая женщина",
                                "target_lang_sense_clarification": "устар. эвфемизм"
                            }
                        ]
                    }
                }
            ]
        }
    ]

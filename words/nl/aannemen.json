{
    "word_family": [
        "aanname",
        "aannemer",
        "aannemelijk"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b4b1c277-fd91-3a12-9476-d9d200a44f40",
                    "sense_definition": "Iets in ontvangst nemen of accepteren wat aangeboden wordt, zoals een geschenk of een uitnodiging",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "accept_receive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <nam> het geschenk dankbaar <aan>.",
                            "target_lang_translations": {
                                "ru": "Она с благодарностью <приняла> подарок."
                            }
                        },
                        {
                            "text": "Ik <neem> je uitnodiging graag <aan>.",
                            "target_lang_translations": {
                                "ru": "Я с удовольствием <принимаю> твое приглашение."
                            }
                        }
                    ],
                    "synonyms": [
                        "accepteren",
                        "aanvaarden"
                    ],
                    "common_phrases": [
                        "een pakketje aannemen",
                        "de telefoon aannemen",
                        "een uitnodiging aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать или получать то, что предлагают, например, подарок или приглашение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "получать что-либо предложенное"
                            },
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "физически брать в руки или отвечать на звонок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d3217b9-b93f-375b-89e5-2fcf99d3516a",
                    "sense_definition": "Geloven of veronderstellen dat iets waar is zonder zekerheid",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <nemen> <aan> dat je de waarheid spreekt.",
                            "target_lang_translations": {
                                "ru": "Мы <предполагаем>, что ты говоришь правду."
                            }
                        },
                        {
                            "text": "Hij <nam> zonder meer <aan> dat het verhaal klopte.",
                            "target_lang_translations": {
                                "ru": "Он безоговорочно <поверил>, что история правдива."
                            }
                        }
                    ],
                    "synonyms": [
                        "veronderstellen",
                        "assumeren"
                    ],
                    "common_phrases": [
                        "ik neem aan",
                        "zonder meer aannemen",
                        "aannemen dat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верить или допускать, что что-то является правдой, без полной уверенности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предполагать",
                                "target_lang_sense_clarification": "делать допущение"
                            },
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "считать верным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a1ff855-2734-3417-98ed-f35d2e7a58fd",
                    "sense_definition": "Iemand een baan geven of in dienst nemen bij een bedrijf",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf <nam> vorige maand nieuwe werknemers <aan>.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце компания <наняла> новых сотрудников."
                            }
                        },
                        {
                            "text": "Ze werd direct <aangenomen> na het sollicitatiegesprek.",
                            "target_lang_translations": {
                                "ru": "Её <приняли> на работу сразу после собеседования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "personeel aannemen",
                        "iemand aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-то работу или зачислять в штат компании"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанимать",
                                "target_lang_sense_clarification": "брать на работу"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "зачислять в штат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce96b6e3-1294-3db7-a25a-a31a303d83c2",
                    "sense_definition": "Een voorstel, wet of motie officieel goedkeuren door te stemmen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "approve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het voorstel is gisteren <aangenomen> door de gemeenteraad.",
                            "target_lang_translations": {
                                "ru": "Вчера предложение было <принято> городским советом."
                            }
                        },
                        {
                            "text": "De nieuwe wet werd met een grote meerderheid <aangenomen>.",
                            "target_lang_translations": {
                                "ru": "Новый закон был <принят> подавляющим большинством голосов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een wet aannemen",
                        "een motie aannemen",
                        "een voorstel aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально одобрять предложение, закон или резолюцию путем голосования"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "утверждать закон или решение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c9898ae-bb23-39d4-9664-5ddc3f22c7f8",
                    "sense_definition": "Een bepaalde houding, vorm, naam of strategie gaan gebruiken",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adopt_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze <namen> een nieuwe strategie <aan> om het probleem op te lossen.",
                            "target_lang_translations": {
                                "ru": "Они <приняли> новую стратегию для решения проблемы."
                            }
                        },
                        {
                            "text": "Hij <nam> een dreigende houding <aan> tegenover de agent.",
                            "target_lang_translations": {
                                "ru": "Он <принял> угрожающую позу по отношению к полицейскому."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een houding aannemen",
                        "een gedaante aannemen",
                        "een naam aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать использовать определенную позицию, форму, имя или стратегию"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "вид, форму или позу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28a28033-351b-37c3-974f-c154427d46ec",
                    "sense_definition": "Een kind wettelijk als eigen kind in het gezin opnemen",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "adopt_child",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kinderloze gezin had twee kinderen <aangenomen>.",
                            "target_lang_translations": {
                                "ru": "Бездетная семья <усыновила> двоих детей."
                            }
                        },
                        {
                            "text": "Zij willen graag een kind uit het buitenland <aannemen>.",
                            "target_lang_translations": {
                                "ru": "Они хотят <усыновить> ребенка из-за границы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een kind aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридически принимать ребенка в семью как своего собственного"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усыновлять",
                                "target_lang_sense_clarification": "мальчика или ребенка в общем"
                            },
                            {
                                "target_lang_word": "удочерять",
                                "target_lang_sense_clarification": "девочку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15d49462-5803-3312-881d-b6c1fa254b32",
                    "sense_definition": "Een karwei of opdracht accepteren om uit te voeren tegen betaling",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "contract_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aannemer <neemt> werk <aan> voor een vaste prijs.",
                            "target_lang_translations": {
                                "ru": "Подрядчик <берет> работу за фиксированную цену."
                            }
                        },
                        {
                            "text": "Hij heeft de klus <aangenomen> maar kreeg het niet op tijd af.",
                            "target_lang_translations": {
                                "ru": "Он <взялся> за эту работу, но не закончил её вовремя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "werk aannemen",
                        "een klus aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашаться выполнить работу или задание за плату"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браться",
                                "target_lang_sense_clarification": "за работу или дело"
                            },
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "заказ или подряд"
                            }
                        ]
                    }
                }
            ]
        }
    ]

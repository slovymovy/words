                {
                    "sense_id": "828c0000-9375-331a-a406-8a1ceb0d2f3c",
                    "sense_definition": "uruchamiać urządzenie grające lub odtwarzać nagranie (muzykę, film)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad głośno <w>puszcza</w> muzykę w nocy.",
                            "target_lang_translations": {
                                "ru": "Мой сосед громко <w>включает</w> музыку по ночам."
                            }
                        },
                        {
                            "text": "Często <w>puszczamy</w> dzieciom bajki w telewizji.",
                            "target_lang_translations": {
                                "ru": "Мы часто <w>ставим</w> детям мультики по телевизору."
                            }
                        }
                    ],
                    "synonyms": [

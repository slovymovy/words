    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "57d77fe0-6847-3ce8-9f1d-55eae9424e21",
                    "sense_definition": "Mały kartonik z zapisanymi na nim danymi osobowymi lub firmowymi, wręczany przy powitaniu lub pożegnaniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_card",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wymiana <w>wizytówek</w> to częsty element spotkań handlowych.",
                            "target_lang_translations": {
                                "ru": "Обмен <w>визитками</w> — это частый элемент деловых встреч."
                            }
                        },
                        {
                            "text": "Czy mogę prosić o twoją <w>wizytówkę</w> z numerem telefonu?",
                            "target_lang_translations": {
                                "ru": "Можно попросить твою <w>визитку</w> с номером телефона?"
                            }
                        }
                    ],
                    "synonyms": [
                        "bilecik",
                        "bilet wizytowy",
                        "karta wizytowa",
                        "gw. (Górny Śląsk) wizytkarta"
                    ],
                    "common_phrases": [
                        "wymiana wizytówek",
                        "dać wizytówkę",
                        "wydrukować wizytówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая карточка с контактными данными человека или организации, вручаемая при знакомстве или прощании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "визитка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "визитная карточка",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ead0667b-46da-36b5-a127-76ff2b75d182",
                    "sense_definition": "Dołączana do wiadomości e-mail informacja o danych osobowych lub firmowych nadawcy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele programów pocztowych umożliwia dołączanie <w>wizytówek</w> do wysyłanej korespondencji.",
                            "target_lang_translations": {
                                "ru": "Многие почтовые программы позволяют прикреплять <w>визитные карточки</w> к отправляемой корреспонденции."
                            }
                        },
                        {
                            "text": "Moja elektroniczna <w>wizytówka</w> zawiera adres biura i link do strony.",
                            "target_lang_translations": {
                                "ru": "Моя электронная <w>визитка</w> содержит адрес офиса и ссылку на сайт."
                            }
                        }
                    ],
                    "synonyms": [
                        "sygnaturka"
                    ],
                    "common_phrases": [
                        "wizytówka w mailu",
                        "plik wizytówki",
                        "załączyć wizytówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контактная информация, прикрепляемая к электронному письму (например, файл vCard)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электронная визитка",
                                "target_lang_sense_clarification": "цифровой контакт"
                            },
                            {
                                "target_lang_word": "визитная карточка",
                                "target_lang_sense_clarification": "вложения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6f957b6-e424-38fe-be87-76a7acfb5a3d",
                    "sense_definition": "Coś, czym się można pochwalić, co stanowi o dobrej opinii i reprezentuje kogoś lub coś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_showcase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tężnie i park stały się <w>wizytówką</w> nadodrzańskiego grodu.",
                            "target_lang_translations": {
                                "ru": "Градирни и парк стали <w>визитной карточкой</w> города на Одере."
                            }
                        },
                        {
                            "text": "Czyste i zadbane ulice są najlepszą <w>wizytówką</w> tej dzielnicy.",
                            "target_lang_translations": {
                                "ru": "Чистые и ухоженные улицы являются лучшей <w>визитной карточкой</w> этого района."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wizytówka miasta",
                        "najlepsza wizytówka",
                        "stanowić wizytówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что служит предметом гордости, создает хорошее впечатление и является отличительным знаком кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "визитная карточка",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "репутация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08e9fca9-bd7f-3ea3-a0fb-75d0ead9bdf0",
                    "sense_definition": "Tabliczka na drzwiach z informacją o lokatorze.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "door_plate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaraz zauważył widniejące na drewnianej <w>wizytówce</w> imię i nazwisko.",
                            "target_lang_translations": {
                                "ru": "Он сразу заметил имя и фамилию на деревянной <w>табличке</w>."
                            }
                        },
                        {
                            "text": "Na mosiężnej <w>wizytówce</w> wygrawerowano nazwisko lekarza.",
                            "target_lang_translations": {
                                "ru": "На латунной <w>табличке</w> была выгравирована фамилия врача."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wizytówka na drzwiach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Табличка на двери с информацией о жильце или владельце кабинета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "табличка",
                                "target_lang_sense_clarification": "на двери"
                            },
                            {
                                "target_lang_word": "табличка с именем",
                                "target_lang_sense_clarification": "именная"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "05fb5eb7-1f5a-364f-99ef-fce37a26f5c0",
                    "sense_definition": "Miasto powiatowe w południowo-zachodniej Polsce, w województwie dolnośląskim, znane z zabytkowego zamku książęcego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "towns",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W ten weekend planujemy zwiedzić renesansowy zamek w <w>Oleśnicy</w>.",
                            "target_lang_translations": {
                                "ru": "В эти выходные мы планируем посетить ренессансный замок в <w>Олеснице</w>."
                            }
                        },
                        {
                            "text": "<w>Oleśnica</w> leży w niewielkiej odległości od Wrocławia.",
                            "target_lang_translations": {
                                "ru": "<w>Олесница</w> находится недалеко от Вроцлава."
                            }
                        }
                    ],
                    "common_phrases": [
                        "miasto Oleśnica",
                        "zamek w Oleśnicy",
                        "powiat oleśnicki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Районный город на юго-западе Польши, в Нижнесилезском воеводстве, известный своим историческим княжеским замком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Олесница",
                                "target_lang_sense_clarification": "город в Нижнесилезском воеводстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e669ddd-9403-359d-be62-a6873fea3e84",
                    "sense_definition": "Mniejsze miasto w południowo-wschodniej Polsce, położone w województwie świętokrzyskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "towns",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Miasteczko <w>Oleśnica</w> posiada długą historię sięgającą średniowiecza.",
                            "target_lang_translations": {
                                "ru": "Городок <w>Олесница</w> имеет долгую историю, восходящую к средневековью."
                            }
                        },
                        {
                            "text": "Przez <w>Oleśnicę</w> przebiega ważna droga wojewódzka.",
                            "target_lang_translations": {
                                "ru": "Через <w>Олесницу</w> проходит важная воеводская дорога."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gmina Oleśnica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой город на юго-востоке Польши, расположенный в Свентокшиском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Олесница",
                                "target_lang_sense_clarification": "город в Свентокшиском воеводстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ecc6c4d-58ce-38cd-b8d1-791e30162d62",
                    "sense_definition": "Nazwa kilku rzek w Polsce, w tym lewego dopływu Warty oraz dopływu Widawy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rivers",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Rzeka <w>Oleśnica</w> jest prawym dopływem Widawy i przepływa przez miasto o tej samej nazwie.",
                            "target_lang_translations": {
                                "ru": "Река <w>Олесница</w> является правым притоком Видавы и протекает через одноименный город."
                            }
                        },
                        {
                            "text": "Wody <w>Oleśnicy</w> są popularnym miejscem dla lokalnych wędkarzy.",
                            "target_lang_translations": {
                                "ru": "Воды <w>Олесницы</w> — популярное место для местных рыбаков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzeka Oleśnica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Название нескольких рек в Польше, в том числе левого притока Варты и притока Видавы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Олесница",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                }
            ]
        }

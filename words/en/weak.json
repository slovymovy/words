    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "34aa34a3-cf83-3220-a514-ff967a87a35a",
                    "sense_definition": "Lacking physical strength, energy, or vigor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lack_of_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient was too <w>weak</w> to stand up without help.",
                            "target_lang_translations": {
                                "ru": "Пациент был слишком <w>слаб</w>, чтобы встать без посторонней помощи."
                            }
                        },
                        {
                            "text": "I felt <w>weak</w> and dizzy after not eating all day.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал <w>слабость</w> и головокружение, так как не ел весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "feeble",
                        "frail",
                        "infirm",
                        "powerless"
                    ],
                    "antonyms": [
                        "strong",
                        "robust",
                        "powerful",
                        "healthy"
                    ],
                    "common_phrases": [
                        "feel weak",
                        "weak muscles",
                        "weak with hunger"
                    ],
                    "target_lang_definitions": {

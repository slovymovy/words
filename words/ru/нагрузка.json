    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "099de987-98a1-3398-bb90-4efeaecc254e",
                    "sense_definition": "Количество работы, занятий или физических упражнений, которые необходимо выполнить; степень занятости.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "work_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У учителя сейчас очень большая учебная <нагрузка> в школе.",
                            "target_lang_translations": {
                                "en": "The teacher has a very heavy teaching <workload> at school right now."
                            }
                        },
                        {
                            "text": "Врач рекомендовал снизить физическую <нагрузку> на спину.",
                            "target_lang_translations": {
                                "en": "The doctor recommended reducing the physical <load> on the back."
                            }
                        }
                    ],
                    "synonyms": [
                        "загруженность",
                        "занятость"
                    ],
                    "common_phrases": [
                        "физическая нагрузка",
                        "учебная нагрузка",
                        "большая нагрузка",
                        "двойная нагрузка"
                    ],
                    "target_lang_definitions": {
                        "en": "The amount of work, study, or physical activity that needs to be performed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "workload",
                                "target_lang_sense_clarification": "amount of work or study"
                            },
                            {
                                "target_lang_word": "load",
                                "target_lang_sense_clarification": "physical exertion or general burden"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bbcce4f-381a-3ee6-98f5-223118f0dded",
                    "sense_definition": "Сила, вес или мощность, действующие на механизм, конструкцию или электрическую сеть.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой <нагрузка> на городскую электросеть значительно возрастает.",
                            "target_lang_translations": {
                                "en": "In winter, the <load> on the city's electrical grid increases significantly."
                            }
                        },
                        {
                            "text": "Старый мост не выдержал такой огромной <нагрузки> и начал разрушаться.",
                            "target_lang_translations": {
                                "en": "The old bridge could not withstand such a huge <load> and began to collapse."
                            }
                        }
                    ],
                    "synonyms": [
                        "тяжесть",
                        "давление"
                    ],
                    "common_phrases": [
                        "нагрузка на ось",
                        "пиковая нагрузка",
                        "допустимая нагрузка",
                        "полезная нагрузка"
                    ],
                    "target_lang_definitions": {
                        "en": "Force, weight, or power acting on a mechanism, structure, or electrical network."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "load",
                                "target_lang_sense_clarification": "mechanical or electrical force"
                            },
                            {
                                "target_lang_word": "stress",
                                "target_lang_sense_clarification": "pressure on a structure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57db5663-3450-3ceb-8897-eabaec85799e",
                    "sense_definition": "Процесс наполнения чего-либо грузом.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action_loading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Нагрузка> угля на корабль заняла у рабочих весь день.",
                            "target_lang_translations": {
                                "en": "<Loading> coal onto the ship took the workers the whole day."
                            }
                        },
                        {
                            "text": "Мы начали <нагрузку> вещей в машину рано утром, чтобы успеть до пробок.",
                            "target_lang_translations": {
                                "en": "We started <loading> things into the car early in the morning to beat the traffic."
                            }
                        }
                    ],
                    "synonyms": [
                        "погрузка",
                        "загрузка"
                    ],
                    "antonyms": [
                        "разгрузка",
                        "выгрузка"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of putting cargo or items into or onto something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loading",
                                "target_lang_sense_clarification": "action of filling with cargo"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79d50007-1104-37e9-a01b-88cc7a4ff61a",
                    "sense_definition": "Внутреннее содержание, смысл или эмоциональная окраска чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это простое слово в данном контексте несет негативную эмоциональную <нагрузку>.",
                            "target_lang_translations": {
                                "en": "This simple word carries a negative emotional <weight> in this context."
                            }
                        },
                        {
                            "text": "Картина имеет глубокую смысловую <нагрузку>, которую поймет не каждый зритель.",
                            "target_lang_translations": {
                                "en": "The painting has a deep semantic <content> that not every viewer will understand."
                            }
                        }
                    ],
                    "synonyms": [
                        "содержание",
                        "смысл"
                    ],
                    "common_phrases": [
                        "смысловая нагрузка",
                        "эмоциональная нагрузка"
                    ],
                    "target_lang_definitions": {
                        "en": "The internal meaning, significance, or emotional weight of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weight",
                                "target_lang_sense_clarification": "emotional or figurative significance"
                            },
                            {
                                "target_lang_word": "content",
                                "target_lang_sense_clarification": "meaning or substance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f34e6bea-3cad-399f-a1e9-2eff193f2b14",
                    "sense_definition": "Общественное поручение или обязанность, выполняемая добровольно и бесплатно.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_duty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Староста группы получил новую общественную <нагрузку> — организовать праздник.",
                            "target_lang_translations": {
                                "en": "The group monitor received a new social <duty> — to organize the party."
                            }
                        },
                        {
                            "text": "Для него это не работа, а просто дополнительная <нагрузка> в свободное время.",
                            "target_lang_translations": {
                                "en": "For him, this is not work, but just an additional <responsibility> in his free time."
                            }
                        }
                    ],
                    "synonyms": [
                        "поручение",
                        "обязанность"
                    ],
                    "common_phrases": [
                        "общественная нагрузка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A social task or responsibility performed voluntarily."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duty",
                                "target_lang_sense_clarification": "assigned task or role"
                            },
                            {
                                "target_lang_word": "responsibility",
                                "target_lang_sense_clarification": "obligation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e8034ce-164f-3220-9f09-a63221bcbf1d",
                    "sense_definition": "Ненужный товар, который заставляют купить вместе с дефицитным товаром (историческое явление).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "forced_purchase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К билетам в театр нам дали в <нагрузку> несколько ненужных брошюр.",
                            "target_lang_translations": {
                                "en": "Along with the theater tickets, we were given some useless brochures <as a tie-in>."
                            }
                        },
                        {
                            "text": "Раньше хорошие книги часто продавали в <нагрузку> с теми, которые никто не читал.",
                            "target_lang_translations": {
                                "en": "Previously, good books were often sold <bundled> with those that no one read."
                            }
                        }
                    ],
                    "common_phrases": [
                        "в нагрузку",
                        "дать в нагрузку"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to Soviet trade practices"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Unwanted goods forced upon a buyer along with a desired purchase."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tie-in",
                                "target_lang_sense_clarification": "item sold conditionally with another"
                            },
                            {
                                "target_lang_word": "forced addition",
                                "target_lang_sense_clarification": "unwanted extra item"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "6970d5e6-ff7b-330e-b71b-514ae889e5ef",
                    "sense_definition": "Aandacht geven aan iemand of iets om het in goede staat te houden of te helpen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "care_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verpleegkundige <w>zorgt</w> dag en nacht voor de patiënten in het ziekenhuis.",
                            "target_lang_translations": {
                                "en": "The nurse <w>cares</w> for the patients in the hospital day and night.",
                                "ru": "Медсестра день и ночь <w>ухаживает</w> за пациентами в больнице."
                            }
                        },
                        {
                            "text": "Wie <w>zorgt</w> er voor de kat als jullie op vakantie zijn?",
                            "target_lang_translations": {
                                "en": "Who <w>looks after</w> the cat when you are on vacation?",
                                "ru": "Кто <w>присмотрит</w> за кошкой, когда вы будете в отпуске?"
                            }
                        }
                    ],
                    "synonyms": [
                        "verzorgen",
                        "passen op"
                    ],
                    "antonyms": [
                        "verwaarlozen"
                    ],
                    "common_phrases": [
                        "zorgen voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To give attention to someone or something to keep them in good condition or to help.",
                        "ru": "Уделять внимание кому-либо или чему-либо, чтобы поддерживать в хорошем состоянии или помогать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take care of",
                                "target_lang_sense_clarification": "to be responsible for or attend to"
                            },
                            {
                                "target_lang_word": "care for",
                                "target_lang_sense_clarification": "to look after someone or something"
                            },
                            {
                                "target_lang_word": "look after",
                                "target_lang_sense_clarification": "to watch over or tend to"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заботиться",
                                "target_lang_sense_clarification": "проявлять заботу и внимание"
                            },
                            {
                                "target_lang_word": "ухаживать",
                                "target_lang_sense_clarification": "следить за состоянием кого-либо (например, больного)"
                            },
                            {
                                "target_lang_word": "присматривать",
                                "target_lang_sense_clarification": "наблюдать или следить за кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3de560d4-dc93-3f1d-8ebb-6e34d3131170",
                    "sense_definition": "Ervoor zorgen dat iets gebeurt of gedaan wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ensure_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal <w>zorgen</w> dat alles op tijd klaar is voor het feest.",
                            "target_lang_translations": {
                                "en": "I will <w>ensure</w> that everything is ready in time for the party.",
                                "ru": "Я <w>позабочусь</w> о том, чтобы всё было готово к празднику вовремя."
                            }
                        },
                        {
                            "text": "Zorg jij dat de deur goed op slot zit voordat we weggaan?",
                            "target_lang_translations": {
                                "en": "Will you <w>make sure</w> the door is locked securely before we leave?",
                                "ru": "Ты <w>проследишь</w> за тем, чтобы дверь была надежно заперта, прежде чем мы уйдем?"
                            }
                        }
                    ],
                    "synonyms": [
                        "regelen",
                        "garanderen",
                        "verzekeren"
                    ],
                    "common_phrases": [
                        "zorgen dat",
                        "ervoor zorgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make sure that something happens or is done.",
                        "ru": "Делать так, чтобы что-то обязательно произошло или было выполнено."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ensure",
                                "target_lang_sense_clarification": "to guarantee an outcome"

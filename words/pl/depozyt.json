    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cf26a8b7-e410-3b6d-bed0-ab7a515c7085",
                    "sense_definition": "Środki pieniężne przechowywane w banku lub innej instytucji finansowej.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klient zdecydował się wpłacić dużą sumę na <w>depozyt</w> terminowy.",
                            "target_lang_translations": {
                                "ru": "Клиент решил внести крупную сумму на срочный <w>депозит</w>."
                            }
                        },
                        {
                            "text": "Bank oferuje wysokie oprocentowanie dla każdego nowego <w>depozytu</w>.",
                            "target_lang_translations": {
                                "ru": "Банк предлагает высокие проценты по каждому новому <w>вкладу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lokata"
                    ],
                    "common_phrases": [
                        "depozyt bankowy",
                        "złożyć depozyt",
                        "gwarancja depozytów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежные средства, хранящиеся в банке или другом финансовом учреждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "депозит",
                                "target_lang_sense_clarification": "финансовый термин"
                            },
                            {
                                "target_lang_word": "вклад",
                                "target_lang_sense_clarification": "банковский счет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "598c639e-d9d9-3a70-98b9-6932a446d285",
                    "sense_definition": "Rzecz oddana na przechowanie lub formalne przekazanie własności pod czyjąś opiekę.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "custody",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zabezpieczyła dowody i przekazała je do sądowego <w>depozytu</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция изъяла улики и передала их на судебное <w>хранение</w>."
                            }
                        },
                        {
                            "text": "Muzeum przyjęło cenny obraz w <w>depozyt</w> od prywatnego kolekcjonera.",
                            "target_lang_translations": {
                                "ru": "Музей принял ценную картину на <w>хранение</w> от частного коллекционера."
                            }
                        }
                    ],
                    "synonyms": [
                        "przechowanie"
                    ],
                    "common_phrases": [
                        "oddać w depozyt",
                        "przyjąć w depozyt",
                        "depozyt sądowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, переданная на хранение, или формальная передача собственности под чью-либо опеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранение",
                                "target_lang_sense_clarification": "передача под опеку"
                            },
                            {
                                "target_lang_word": "депонирование",
                                "target_lang_sense_clarification": "процесс передачи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e85ce8f-0b07-3b89-89ee-cf63496cd736",
                    "sense_definition": "Miejsce lub pomieszczenie służące do bezpiecznego przechowywania wartościowych rzeczy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed operacją pacjent musi zostawić wszystkie kosztowności w szpitalnym <w>depozycie</w>.",
                            "target_lang_translations": {
                                "ru": "Перед операцией пациент должен оставить все ценности в больничной <w>камере хранения</w>."
                            }
                        },
                        {
                            "text": "Złodzieje bezskutecznie próbowali dostać się do bankowego <w>depozytu</w>.",
                            "target_lang_translations": {
                                "ru": "Воры безуспешно пытались проникнуть в банковское <w>хранилище</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "depozytorium",
                        "skarbiec",
                        "skrytka",
                        "trezor"
                    ],
                    "common_phrases": [
                        "depozyt szpitalny",
                        "klucz do depozytu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или помещение для безопасного хранения ценных вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранилище",
                                "target_lang_sense_clarification": "помещение для ценностей"
                            },
                            {
                                "target_lang_word": "камера хранения",
                                "target_lang_sense_clarification": "в общественных местах"
                            },
                            {
                                "target_lang_word": "депозитарий",
                                "target_lang_sense_clarification": "в банке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

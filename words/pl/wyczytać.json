                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высказать",
                                "target_lang_sense_clarification": "предъявить претензии"
                            },
                            {
                                "target_lang_word": "попрекнуть",
                                "target_lang_sense_clarification": "укорить прошлым"
                            },
                            {
                                "target_lang_word": "припомнить",
                                "target_lang_sense_clarification": "напомнить о плохом"
                            }
                        ]
                    }
                }

            "senses": [
                {
                    "sense_id": "45d075ed-862c-3118-a461-fbc8fad3ffb8",
                    "sense_definition": "Zespół cech charakterystycznych dla sztuki, architektury, mody lub kultury danego regionu albo epoki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary kościół został zbudowany w <w>stylu</w> gotyckim.",
                            "target_lang_translations": {
                                "ru": "Эта старая церковь была построена в готическом <w>стиле</w>."
                            }
                        },
                        {
                            "text": "Bardzo podoba mi się wystrój wnętrz w <w>stylu</w> skandynawskim.",
                            "target_lang_translations": {
                                "ru": "Мне очень нравится интерьер в скандинавском <w>стиле</w>."
                            }
                        }
                    ],
                    "common_phrases": [

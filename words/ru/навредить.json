    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8c943ec1-f782-3585-8dbd-e57c2a0e8616",
                    "sense_definition": "Причинить вред, сделать что-то плохое, нанести ущерб кому-либо или чему-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "harm_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не хотел никому <w>навредить</w> своими словами.",
                            "target_lang_translations": {
                                "en": "I didn't want to <w>hurt</w> anyone with my words."
                            }
                        },
                        {
                            "text": "Курение может серьезно <w>навредить</w> вашему здоровью.",
                            "target_lang_translations": {
                                "en": "Smoking can seriously <w>harm</w> your health."
                            }
                        }
                    ],
                    "synonyms": [
                        "насолить",
                        "помешать"
                    ],
                    "antonyms": [
                        "помочь",
                        "посодействовать"
                    ],
                    "common_phrases": [
                        "навредить здоровью",
                        "навредить себе",
                        "навредить репутации",
                        "бояться навредить"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause harm, do something bad, or inflict damage on someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "harm",
                                "target_lang_sense_clarification": "general term for causing injury or damage"
                            },
                            {
                                "target_lang_word": "hurt",
                                "target_lang_sense_clarification": "to cause pain or distress to a person"
                            },
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "to cause physical or abstract harm (e.g., to health or reputation)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f24c1d6d-eaf2-3c9c-88ce-7a39da07234c",
                    "sense_definition": "Испортить или сломать какой-либо предмет (употребляется в некоторых диалектах).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "harm_physical_dialect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный град <w>навредил</w> посевы в поле.",
                            "target_lang_translations": {
                                "en": "Strong hail <w>damaged</w> the crops in the field."
                            }
                        },
                        {
                            "text": "Он случайно <w>навредил</w> забор, когда выезжал со двора.",
                            "target_lang_translations": {
                                "en": "He accidentally <w>damaged</w> the fence when driving out of the yard."
                            }
                        }
                    ],
                    "synonyms": [
                        "повредить",
                        "испортить",
                        "сломать"
                    ],
                    "antonyms": [
                        "починить",
                        "исправить"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "встречается в региональной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spoil or break an object (used in some dialects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "to physically harm an object"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "to spoil or destroy something"
                            }
                        ]
                    }
                }
            ]
        }
    ]

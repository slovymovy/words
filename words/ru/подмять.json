    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5bd7b364-632b-3854-9ffc-7a0b353375c6",
                    "sense_definition": "Навалившись всем весом, придавить кого-либо или что-либо под собой; смять, пригнуть к земле.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_crush",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный медведь с рычанием бросился на человека и <подмял> его под себя.",
                            "target_lang_translations": {
                                "en": "The huge bear rushed at the man with a roar and <crushed> him underneath itself."
                            }
                        },
                        {
                            "text": "Упавшее старое дерево <подмяло> кусты и сломало забор.",
                            "target_lang_translations": {
                                "en": "The fallen old tree <flattened> the bushes and broke the fence."
                            }
                        }
                    ],
                    "synonyms": [
                        "придавить",
                        "примять"
                    ],
                    "common_phrases": [
                        "подмять траву",
                        "подмять кусты",
                        "подмять под себя"
                    ],
                    "target_lang_definitions": {
                        "en": "To press down or crush someone or something underneath oneself with one's weight; to flatten against the ground."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crush underneath",
                                "target_lang_sense_clarification": "to press something down with body weight"
                            },
                            {
                                "target_lang_word": "pin down",
                                "target_lang_sense_clarification": "to hold someone down by lying on them"
                            },
                            {
                                "target_lang_word": "flatten",
                                "target_lang_sense_clarification": "to press vegetation or objects flat against the ground"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a79d33cc-c84d-331d-aa67-1070ec47249c",
                    "sense_definition": "Полностью подчинить кого-либо или что-либо своей воле, власти или влиянию; захватить контроль над чем-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "subjugate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крупная корпорация пытается <подмять> под себя весь региональный рынок.",
                            "target_lang_translations": {
                                "en": "A large corporation is trying to <take over> the entire regional market."
                            }
                        },
                        {
                            "text": "Новый руководитель быстро <подмял> коллектив и установил свои порядки.",
                            "target_lang_translations": {
                                "en": "The new manager quickly <subjugated> the team and established his own rules."
                            }
                        }
                    ],
                    "synonyms": [
                        "подчинить",
                        "подавить",
                        "поработить"
                    ],
                    "antonyms": [
                        "освободить"
                    ],
                    "common_phrases": [
                        "подмять под себя рынок",
                        "подмять конкурентов",
                        "подмять бизнес",
                        "подмять власть"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely subject someone or something to one's will, power, or influence; to seize control."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "subjugate",
                                "target_lang_sense_clarification": "to bring under domination or control"
                            },
                            {
                                "target_lang_word": "take over",
                                "target_lang_sense_clarification": "to assume control or possession of (e.g., a market)"
                            },
                            {
                                "target_lang_word": "bend to one's will",
                                "target_lang_sense_clarification": "to force someone to submit"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
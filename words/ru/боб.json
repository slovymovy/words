            "senses": [
                {
                    "sense_id": "b90ef1e9-8305-30c6-adb3-24b9e0433ad8",
                    "sense_definition": "Плод зернобобовых растений, а также семя этого плода, обычно округлой или овальной формы.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ужин мама приготовила вкусное рагу, в которое добавила красные <w>бобы</w>."
                        },
                        {
                            "text": "Этот <w>боб</w> оказался слишком твёрдым, поэтому его пришлось варить дольше обычного."
                        }
                    ],
                    "synonyms": [
                        "фасоль"
                    ],
                    "common_phrases": [
                        "жареные бобы",
                        "салат из бобов",
                        "стебель боба"
                    ]
                },
                {

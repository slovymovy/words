                {
                    "sense_id": "c695c9d1-51e6-3ee7-9042-0215648a1a63",
                    "sense_definition": "Państwo w Azji Wschodniej, położone na wyspie o tej samej nazwie, oficjalnie nazywające się Republiką Chińską.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mój brat mieszka na <w>Tajwanie</w> i uczy tam języka angielskiego.",
                            "target_lang_translations": {
                                "ru": "Мой брат живет на <w>Тайване</w> и преподает там английский язык."
                            }
                        },
                        {
                            "text": "Stolica <w>Tajwanu</w> tętni życiem przez całą dobę.",
                            "target_lang_translations": {
                                "ru": "Столица <w>Тайваня</w> кипит жизнью круглосуточно."
                            }
                        }
                    ],
                    "synonyms": [
                        "Republika Chińska",
                        "Chińskie Tajpej"
                    ],
                    "common_phrases": [
                        "rząd Tajwanu",
                        "stolica Tajwanu",
                        "flaga Tajwanu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство в Восточной Азии, расположенное на одноименном острове, официально именуемое Китайской Республикой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тайвань",
                                "target_lang_sense_clarification": "государство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "225ccfd0-a0e0-380a-aba3-4e2f7ccbef5d",
                    "sense_definition": "Duża wyspa na Oceanie Spokojnym, oddzielona od kontynentu azjatyckiego Cieśniną Tajwańską.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Wyspa <w>Tajwan</w> leży w strefie aktywnej sejsmicznie.",
                            "target_lang_translations": {
                                "ru": "Остров <w>Тайвань</w> находится в сейсмически активной зоне."
                            }
                        },
                        {
                            "text": "Wschodnie wybrzeże <w>Tajwanu</w> słynie z pięknych klifów.",
                            "target_lang_translations": {
                                "ru": "Восточное побережье <w>Тайваня</w> славится красивыми утесами."
                            }
                        }
                    ],
                    "synonyms": [
                        "Formoza"
                    ],
                    "common_phrases": [
                        "klimat Tajwanu",
                        "mapa Tajwanu",
                        "wyspa Tajwan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой остров в Тихом океане, отделенный от азиатского континента Тайваньским проливом."
                    },
                    "translations": {
                        "ru": [

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ебашить",
                                "target_lang_sense_clarification": "груб. работать очень много и тяжело"
                            },
                            {
                                "target_lang_word": "пахать",
                                "target_lang_sense_clarification": "разг. усердно трудиться"
                            },
                            {
                                "target_lang_word": "вкалывать",
                                "target_lang_sense_clarification": "разг. интенсивно работать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b7d5b1c-bbee-3f64-a6ed-6e34ed5fdc27",
                    "sense_definition": "Przemieszczać się z bardzo dużą prędkością; biec lub jechać bardzo szybko",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy samochód potrafi <w>zapierdalać</w> ponad dwieście na godzinę.",
                            "target_lang_translations": {
                                "ru": "Эта новая тачка может <w>хуярить</w> больше двухсот километров в час."
                            }
                        },
                        {
                            "text": "Musieliśmy równo <w>zapierdalać</w> na dworzec, żeby zdążyć na ostatni pociąg.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось конкретно <w>гнать</w> на вокзал, чтобы успеть на последний поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "pędzić",
                        "gnać",
                        "zapieprzać",
                        "zasuwać"
                    ],
                    "antonyms": [
                        "wlec się",
                        "guzdrać się"
                    ],
                    "common_phrases": [

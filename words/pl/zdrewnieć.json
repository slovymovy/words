    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e42f820e-5fe3-3630-bd75-f34bc53d6d94",
                    "sense_definition": "Stać się sztywnym, odrętwiałym i niezdolnym do ruchu, zazwyczaj z powodu zimna, przebywania w niewygodnej pozycji lub pod wpływem silnych emocji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stiffness_numbness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedziałem tak długo w jednej pozycji, że aż <w>zdrewniały</w> mi nogi.",
                            "target_lang_translations": {
                                "ru": "Я так долго сидел в одной позе, что у меня <w>одеревенели</w> ноги."
                            }
                        },
                        {
                            "text": "Na widok ogromnego psa dziewczynka <w>zdrewniała</w> ze strachu.",
                            "target_lang_translations": {
                                "ru": "При виде огромной собаки девочка <w>оцепенела</w> от страха."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdrętwieć",
                        "zesztywnieć",
                        "skostnieć"
                    ],
                    "antonyms": [
                        "rozluźnić się"
                    ],
                    "common_phrases": [
                        "zdrewnieć ze strachu",
                        "nogi zdrewniały",
                        "ręce zdrewniały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать жестким, неподвижным или потерять чувствительность (от холода, неудобной позы или эмоций)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеревенеть",
                                "target_lang_sense_clarification": "стать твердым и неподвижным"
                            },
                            {
                                "target_lang_word": "оцепенеть",
                                "target_lang_sense_clarification": "застыть от страха или удивления"
                            },
                            {
                                "target_lang_word": "онеметь",
                                "target_lang_sense_clarification": "потерять чувствительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cd8f573-9a26-3a70-a9bb-95586708c340",
                    "sense_definition": "Ulec procesowi drewnienia, zmieniając się w twardą tkankę przypominającą drewno (o roślinach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młode pędy krzewu należy przycinać, zanim całkowicie <w>zdrewnieją</w>.",
                            "target_lang_translations": {
                                "ru": "Молодые побеги кустарника нужно обрезать, прежде чем они полностью <w>одеревенеют</w>."
                            }
                        },
                        {
                            "text": "Dolna część łodygi tej rośliny szybko <w>zdrewniała</w> przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Нижняя часть стебля этого растения быстро <w>одеревенела</w> до наступления зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "stwardnieć"
                    ],
                    "common_phrases": [
                        "pędy zdrewniały",
                        "łodyga zdrewniała"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрыться корой, стать твердым, как дерево (о растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеревенеть",
                                "target_lang_sense_clarification": "превратиться в древесину"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
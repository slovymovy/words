                            {
                                "target_lang_word": "to sit down",
                                "target_lang_sense_clarification": "general action of taking a seat"
                            },
                            {
                                "target_lang_word": "to take a seat",
                                "target_lang_sense_clarification": "more formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41c6a501-b334-3671-b57e-d59f38b20b50",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "555cb48d-a905-345f-aeb1-66cafe127560",
                    "sense_definition": "A covering for the inside surface of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "material_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The winter coat has a thick wool <w>lining</w> to keep you warm.",
                            "target_lang_translations": {
                                "ru": "У этого зимнего пальто толстая шерстяная <w>подкладка</w>, чтобы тебе было тепло."
                            }
                        },
                        {
                            "text": "Every cloud has a silver <w>lining</w>.",
                            "target_lang_translations": {
                                "ru": "У каждого облака есть серебряная <w>кайма</w> (нет худа без добра)."
                            }
                        },
                        {
                            "text": "Spicy food can sometimes irritate the stomach <w>lining</w>.",
                            "target_lang_translations": {
                                "ru": "Острая пища может иногда раздражать <w>слизистую оболочку</w> желудка."
                            }
                        }
                    ],
                    "synonyms": [
                        "inner layer",
                        "facing",
                        "liner"
                    ],
                    "antonyms": [
                        "outer layer",
                        "shell"
                    ],
                    "common_phrases": [
                        "silver lining",
                        "stomach lining",
                        "fur lining",
                        "silk lining"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутреннее покрытие или слой материала, покрывающий внутреннюю поверхность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкладка",
                                "target_lang_sense_clarification": "одежда, сумки"
                            },
                            {
                                "target_lang_word": "оболочка",
                                "target_lang_sense_clarification": "анатомия, техника"
                            },
                            {
                                "target_lang_word": "внутреннее покрытие",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "299eb900-c667-3d43-b1d4-5a69eda37a1e",
                    "sense_definition": "The material used for covering the inside surface of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "material_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought three meters of blue <w>lining</w> to finish the dress.",
                            "target_lang_translations": {
                                "ru": "Она купила три метра синей <w>подкладочной ткани</w>, чтобы закончить платье."
                            }
                        },
                        {
                            "text": "This synthetic fabric is too heavy to be used as <w>lining</w>.",
                            "target_lang_translations": {
                                "ru": "Эта синтетическая ткань слишком тяжелая, чтобы использовать её в качестве <w>подкладки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "synthetic lining",
                        "cotton lining"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, предназначенный для внутренней отделки швейных изделий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкладочная ткань",
                                "target_lang_sense_clarification": "материал"

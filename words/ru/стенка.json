    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "327c2678-e020-30f5-8d1a-e15694fc6c99",
                    "sense_definition": "Уменьшительная форма от слова <w>стена</w>; тонкая перегородка.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wall_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За тонкой <стенкой> было слышно всё, о чём говорили соседи.",
                            "target_lang_translations": {
                                "en": "Behind the thin <w>wall</w>, everything the neighbors were saying could be heard."
                            }
                        },
                        {
                            "text": "Ребёнок нарисовал красивый цветок прямо на <стенке>.",
                            "target_lang_translations": {
                                "en": "The child drew a beautiful flower right on the <w>wall</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "перегородка"
                    ],
                    "common_phrases": [
                        "шведская стенка",
                        "прижать к стенке",
                        "в четырёх стенках",
                        "как об стенку горох"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A thin wall or partition, often used to refer to non-load-bearing walls inside a home."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wall",
                                "target_lang_sense_clarification": "general meaning, often diminutive"
                            },
                            {
                                "target_lang_word": "partition",
                                "target_lang_sense_clarification": "thin dividing wall"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41ee84ba-e1aa-3c06-b20e-ca53a7ffe989",
                    "sense_definition": "Боковая поверхность или оболочка сосуда, органа или полого предмета.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "side_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чай был очень горячим, и <стенки> кружки быстро нагрелись.",
                            "target_lang_translations": {
                                "en": "The tea was very hot, and the <w>sides</w> of the mug heated up quickly."
                            }
                        },
                        {
                            "text": "Врач сказал, что <стенки> сосудов должны быть эластичными.",
                            "target_lang_translations": {
                                "en": "The doctor said that the blood vessel <w>walls</w> should be elastic."
                            }
                        }
                    ],
                    "synonyms": [
                        "оболочка",
                        "бок",
                        "сторона"
                    ],
                    "common_phrases": [
                        "стенки сосудов",
                        "толстые стенки",
                        "клеточная стенка"
                    ],
                    "target_lang_definitions": {
                        "en": "The side surface or enclosing layer of a container, organ, or tube."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wall",
                                "target_lang_sense_clarification": "of an organ, cell, or vessel"
                            },
                            {
                                "target_lang_word": "side",
                                "target_lang_sense_clarification": "surface of an object like a cup or box"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c35ea915-effc-3c71-9f57-d06c2423a67c",
                    "sense_definition": "Предмет мебели, состоящий из нескольких шкафов и секций, поставленных в ряд вдоль стены.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В гостиной стояла старая советская <стенка> с книгами и посудой.",
                            "target_lang_translations": {
                                "en": "An old Soviet <w>wall unit</w> with books and dishes stood in the living room."
                            }
                        },
                        {
                            "text": "Мы решили купить новую <стенку>, чтобы сложить туда все вещи.",
                            "target_lang_translations": {
                                "en": "We decided to buy a new <w>wall unit</w> to put all our things in."
                            }
                        }
                    ],
                    "synonyms": [
                        "гарнитур",
                        "шкаф"
                    ],
                    "common_phrases": [
                        "мебельная стенка",
                        "купить стенку",
                        "собрать стенку"
                    ],
                    "target_lang_definitions": {
                        "en": "A large furniture set consisting of cabinets and shelves arranged along a wall."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wall unit",
                                "target_lang_sense_clarification": "furniture set"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdfe879c-89a0-3147-9f9c-9556458fd2c8",
                    "sense_definition": "В футболе: ряд игроков, выстроившихся в линию для защиты ворот при штрафном ударе.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sport_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вратарь громко командовал, выстраивая <стенку> перед воротами.",
                            "target_lang_translations": {
                                "en": "The goalkeeper commanded loudly, lining up a <w>wall</w> in front of the goal."
                            }
                        },
                        {
                            "text": "Мяч попал в <стенку> из игроков и отскочил в поле.",
                            "target_lang_translations": {
                                "en": "The ball hit the <w>wall</w> of players and bounced back into the field."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поставить стенку",
                        "ударить в стенку",
                        "обвести стенку"
                    ],
                    "target_lang_definitions": {
                        "en": "A line of players formed to block a free kick in soccer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wall",
                                "target_lang_sense_clarification": "defensive line in sports"
                            }
                        ]
                    }
                }
            ]
        }
    ]

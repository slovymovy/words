                {
                    "sense_id": "cbf3e544-16d1-3ab9-b95b-063619c48b05",
                    "sense_definition": "A city in Kentucky, United States.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They traveled to <w>Hazard</w> to visit relatives.",
                            "target_lang_translations": {
                                "ru": "Они поехали в <w>Хазард</w>, чтобы навестить родственников."
                            }
                        },
                        {
                            "text": "<w>Hazard</w> is the county seat of Perry County.",
                            "target_lang_translations": {
                                "ru": "<w>Хазард</w> — административный центр округа Перри."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в штате Кентукки, США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хазард",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]

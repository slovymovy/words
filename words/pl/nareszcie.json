    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "edf926a4-bfb1-346a-b7c8-1298e88679f1",
                    "sense_definition": "słowo oznaczające, że oczekiwane zdarzenie w końcu nastąpiło po długim czasie, zazwyczaj wyrażające ulgę lub zadowolenie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "finality_relief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nareszcie doczekałam się odpowiedzi z urzędu.",
                            "target_lang_translations": {
                                "ru": "<w>Наконец-то</w> я дождалась ответа из учреждения."
                            }
                        },
                        {
                            "text": "Po długiej i męczącej podróży <w>nareszcie</w> dotarliśmy do domu.",
                            "target_lang_translations": {
                                "ru": "После долгой и утомительной поездки мы <w>наконец</w> добрались до дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "w końcu",
                        "wreszcie"
                    ],
                    "common_phrases": [
                        "nareszcie w domu",
                        "nareszcie wolny",
                        "nareszcie razem"
                    ],
                    "target_lang_definitions": {
                        "ru": "слово, указывающее на то, что ожидаемое событие произошло после долгого времени, обычно выражает облегчение или удовлетворение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наконец",
                                "target_lang_sense_clarification": "наречие времени"
                            },
                            {
                                "target_lang_word": "наконец-то",
                                "target_lang_sense_clarification": "с оттенком облегчения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c17dee15-f8ff-3ccf-a809-a5634b673ed4",
                    "sense_definition": "okrzyk wyrażający silne emocje, takie jak radość lub ulga, z powodu pomyślnego zakończenia czegoś lub nadejścia oczekiwanej chwili",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "finality_relief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nareszcie</w>! Już myślałem, że nigdy nie przyjdziesz.",
                            "target_lang_translations": {
                                "ru": "<w>Наконец-то</w>! Я уже думал, что ты никогда не придешь."
                            }
                        },
                        {
                            "text": "No <w>nareszcie</w>, ile można na ciebie czekać?",
                            "target_lang_translations": {
                                "ru": "Ну <w>наконец-то</w>, сколько можно тебя ждать?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wreszcie",
                        "w końcu"
                    ],
                    "common_phrases": [
                        "no nareszcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "возглас, выражающий радость или облегчение в связи с наступлением долгожданного момента"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наконец-то",
                                "target_lang_sense_clarification": "эмоциональный возглас"
                            },
                            {
                                "target_lang_word": "ну наконец-то",
                                "target_lang_sense_clarification": "усиленный возглас"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "promote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He did everything he could to <w>further</w> his own career.",
                            "target_lang_translations": {
                                "ru": "Он делал всё возможное, чтобы <w>продвигать</w> свою карьеру."
                            }
                        },
                        {
                            "text": "This new grant will help <w>further</w> our research significantly.",
                            "target_lang_translations": {
                                "ru": "Этот новый грант поможет значительно <w>продвинуть</w> наши исследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "promote",
                        "advance",
                        "foster",
                        "encourage"
                    ],
                    "antonyms": [
                        "hinder",
                        "impede"
                    ],
                    "common_phrases": [
                        "further one's career",
                        "further the cause",
                        "further the interests"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способствовать развитию; продвигать; содействовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвигать",
                                "target_lang_sense_clarification": "двигать вперед, развивать"
                            },
                            {
                                "target_lang_word": "способствовать",
                                "target_lang_sense_clarification": "оказывать помощь, содействовать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "a83e18a6-b646-36b2-8192-17618394d0b1",
                    "sense_definition": "Vastzittend zodat beweging niet meer mogelijk is",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn fiets staat <w>klem</w> tussen de andere fietsen in het rek.",
                            "target_lang_translations": {
                                "en": "My bike is <w>stuck</w> between the other bikes in the rack.",
                                "ru": "Мой велосипед <w>зажат</w> между другими велосипедами на стойке."
                            }
                        },
                        {
                            "text": "De lade van het bureau zit <w>klem</w> en wil niet meer open.",
                            "target_lang_translations": {

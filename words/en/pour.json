{
    "word_family": [
        "downpour",
        "outpouring",
        "pourer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a74a6094-9d44-3208-8feb-cd54aa5b19d0",
                    "sense_definition": "To cause a liquid or loose substance to flow in a steady stream from a container.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cause_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>pour</w> some water into my glass.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>налей</w> немного воды в мой стакан."
                            }
                        },
                        {
                            "text": "She <w>poured</w> the hot tea carefully to avoid spilling it.",
                            "target_lang_translations": {
                                "ru": "Она осторожно <w>наливала</w> горячий чай, чтобы не пролить его."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pour a drink",
                        "pour water",
                        "pour coffee",
                        "pour into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять жидкость или сыпучее вещество течь непрерывной струей из емкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наливать",
                                "target_lang_sense_clarification": "наполнять емкость жидкостью"
                            },
                            {
                                "target_lang_word": "лить",
                                "target_lang_sense_clarification": "заставлять течь"
                            },
                            {
                                "target_lang_word": "сыпать",
                                "target_lang_sense_clarification": "о сыпучих веществах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e15d369-435b-3a42-8a1f-5d5df8c72bcb",
                    "sense_definition": "To rain very heavily.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It started to <w>pour</w> just as we left the house without umbrellas.",
                            "target_lang_translations": {
                                "ru": "Дождь начал <w>лить</w> как раз когда мы вышли из дома без зонтов."
                            }
                        },
                        {
                            "text": "The rain <w>poured</w> down all afternoon, flooding the streets.",
                            "target_lang_translations": {
                                "ru": "Дождь <w>лил</w> весь день, затапливая улицы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pour down",
                        "it is pouring",
                        "pouring rain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильно идти (о дожде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лить",
                                "target_lang_sense_clarification": "о сильном дожде"
                            },
                            {
                                "target_lang_word": "ливать",
                                "target_lang_sense_clarification": "разг. сильно идти (о дожде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b4f1516-3f34-3fdd-beba-05d997e43620",
                    "sense_definition": "To flow rapidly and in large amounts in a steady stream.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intransitive_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thick black smoke <w>poured</w> from the windows of the burning building.",
                            "target_lang_translations": {
                                "ru": "Густой черный дым <w>валил</w> из окон горящего здания."
                            }
                        },
                        {
                            "text": "Blood <w>poured</w> from the wound before the doctor could bandage it.",
                            "target_lang_translations": {
                                "ru": "Кровь <w>лилась</w> из раны, пока врач не успел перевязать её."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pour from",
                        "pour out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро течь или выходить в больших количествах непрерывным потоком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "валить",
                                "target_lang_sense_clarification": "о дыме или паре"
                            },
                            {
                                "target_lang_word": "литься",
                                "target_lang_sense_clarification": "течь струей"
                            },
                            {
                                "target_lang_word": "хлестать",
                                "target_lang_sense_clarification": "течь с силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50c77fd3-b682-31f6-85fa-70ea58a423f1",
                    "sense_definition": "To move in large numbers, similar to a flowing stream.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mass_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thousands of excited fans <w>poured</w> into the stadium for the concert.",
                            "target_lang_translations": {
                                "ru": "Тысячи взволнованных фанатов <w>хлынули</w> на стадион на концерт."
                            }
                        },
                        {
                            "text": "Commuters <w>poured</w> out of the train station during rush hour.",
                            "target_lang_translations": {
                                "ru": "Пассажиры <w>валили</w> с вокзала в час пик."
                            }
                        }
                    ],
                    "synonyms": [
                        "crowd",
                        "throng"
                    ],
                    "common_phrases": [
                        "pour into",
                        "pour out of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться в больших количествах, подобно потоку (о людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлынуть",
                                "target_lang_sense_clarification": "устремиться потоком"
                            },
                            {
                                "target_lang_word": "валить",
                                "target_lang_sense_clarification": "идти массой"
                            },
                            {
                                "target_lang_word": "высыпать",
                                "target_lang_sense_clarification": "выйти толпой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73cc7eeb-c839-32ab-8148-223f3dfff25d",
                    "sense_definition": "To provide, express, or distribute something freely and in large quantities.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government <w>poured</w> millions of dollars into the new infrastructure project.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>влило</w> миллионы долларов в новый инфраструктурный проект."
                            }
                        },
                        {
                            "text": "He <w>poured</w> out his feelings in a long letter to his friend.",
                            "target_lang_translations": {
                                "ru": "Он <w>излил</w> свои чувства в длинном письме другу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pour money into",
                        "pour one's heart out",
                        "pour scorn on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять, выражать или распределять что-либо свободно и в больших количествах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкладывать",
                                "target_lang_sense_clarification": "деньги, средства"
                            },
                            {
                                "target_lang_word": "изливать",
                                "target_lang_sense_clarification": "чувства, душу"
                            },
                            {
                                "target_lang_word": "вливать",
                                "target_lang_sense_clarification": "средства, капитал"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a9a534d0-2bae-348d-a3ca-b6d6e711b5a3",
                    "sense_definition": "The action of pouring a liquid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cause_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bartender has a very precise <w>pour</w> when serving draft beer.",
                            "target_lang_translations": {
                                "ru": "У бармена очень точный <w>розлив</w> при подаче разливного пива."
                            }
                        },
                        {
                            "text": "Give the sauce a slow <w>pour</w> over the pasta to coat it evenly.",
                            "target_lang_translations": {
                                "ru": "Медленно <w>полей</w> соусом пасту, чтобы покрыть её равномерно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a steady pour"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по наливанию жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розлив",
                                "target_lang_sense_clarification": "процесс разливания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fab5360-cd4e-3ce7-86c0-8d13054d2f23",
                    "sense_definition": "An amount of material, especially concrete, that is poured.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction team scheduled the concrete <w>pour</w> for early morning.",
                            "target_lang_translations": {
                                "ru": "Строительная бригада запланировала <w>заливку</w> бетона на раннее утро."
                            }
                        },
                        {
                            "text": "We need to finish the <w>pour</w> before the weather turns bad.",
                            "target_lang_translations": {
                                "ru": "Нам нужно закончить <w>заливку</w> до того, как погода испортится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "concrete pour"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or construction context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество материала, особенно бетона, которое заливается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заливка",
                                "target_lang_sense_clarification": "бетона"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
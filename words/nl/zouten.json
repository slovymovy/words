                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число слова \"соль\"; химические соединения, состоящие из ионов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соли",
                                "target_lang_sense_clarification": "химические соединения"

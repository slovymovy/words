    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e19df06d-20ed-3e2a-bc90-53869822c6cc",
                    "sense_definition": "Постигать что-либо в процессе обучения; учить какой-либо предмет, науку или дисциплину глубоко и основательно.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "learning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В университете студенты <изучают> экономику и право.",
                            "target_lang_translations": {
                                "en": "At the university, students <study> economics and law."
                            }
                        },
                        {
                            "text": "Я начал <изучать> русский язык два года назад.",
                            "target_lang_translations": {
                                "en": "I started <studying> the Russian language two years ago."
                            }
                        }
                    ],
                    "synonyms": [
                        "учить",
                        "осваивать"
                    ],
                    "common_phrases": [
                        "изучать иностранный язык",
                        "изучать историю",
                        "изучать математику",
                        "изучать литературу"
                    ],
                    "target_lang_definitions": {
                        "en": "To grasp something through the process of learning; to learn a subject, science, or discipline deeply and thoroughly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "study",
                                "target_lang_sense_clarification": "to take a course or learn a subject academically"
                            },
                            {
                                "target_lang_word": "learn",
                                "target_lang_sense_clarification": "to acquire knowledge or skills"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ca00b9d-2e80-314b-ad5a-c29d4e4e999f",
                    "sense_definition": "Проводить научное исследование, анализировать что-либо для выяснения фактов, закономерностей или свойств.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "research",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные <изучают> влияние климата на природу.",
                            "target_lang_translations": {
                                "en": "Scientists are <studying> the influence of climate on nature."
                            }
                        },
                        {
                            "text": "Специальная комиссия сейчас <изучает> причины аварии.",
                            "target_lang_translations": {
                                "en": "A special commission is currently <investigating> the causes of the accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "исследовать",
                        "анализировать",
                        "обследовать"
                    ],
                    "common_phrases": [
                        "изучать проблему",
                        "изучать вопрос",
                        "изучать рынок",
                        "изучать документы"
                    ],
                    "target_lang_definitions": {
                        "en": "To conduct scientific research or analyze something to ascertain facts, patterns, or properties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "research",
                                "target_lang_sense_clarification": "to conduct scientific investigation"
                            },
                            {
                                "target_lang_word": "investigate",
                                "target_lang_sense_clarification": "to examine the facts or causes of something"
                            },
                            {
                                "target_lang_word": "study",
                                "target_lang_sense_clarification": "to analyze a specific topic or phenomenon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "385df727-812d-367b-afcb-890a4dc2d529",
                    "sense_definition": "Внимательно рассматривать кого-либо или что-либо с целью ознакомления или понимания деталей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Турист долго <изучал> карту города, чтобы найти отель.",
                            "target_lang_translations": {
                                "en": "The tourist <studied> the city map for a long time to find the hotel."
                            }
                        },
                        {
                            "text": "Покупатель внимательно <изучал> состав продукта на этикетке.",
                            "target_lang_translations": {
                                "en": "The customer carefully <examined> the product's ingredients on the label."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассматривать",
                        "разглядывать"
                    ],
                    "common_phrases": [

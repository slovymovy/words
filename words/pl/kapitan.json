    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe97eebf-e2d7-3349-ae16-aadd2ed8eb04",
                    "sense_definition": "Dowódca cywilnego statku wodnego lub samolotu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "commander",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara morska zasada mówi, że <kapitan> schodzi ostatni z tonącego statku.",
                            "target_lang_translations": {
                                "ru": "Старое морское правило гласит, что <капитан> последним покидает тонущий корабль."
                            }
                        },
                        {
                            "text": "Nasz <kapitan> powitał wszystkich pasażerów na pokładzie samolotu.",
                            "target_lang_translations": {
                                "ru": "Наш <капитан> поприветствовал всех пассажиров на борту самолета."
                            }
                        }
                    ],
                    "synonyms": [
                        "dowódca"
                    ],
                    "common_phrases": [
                        "kapitan statku",
                        "kapitan samolotu",
                        "kapitan portu",
                        "kapitan żeglugi wielkiej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Командир гражданского судна или самолета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "судна или самолета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79d038f6-81dd-3fbd-b1e1-1425f2ca77bd",
                    "sense_definition": "Gracz reprezentujący całą swoją drużynę sportową i pełniący w niej funkcję przywódcy.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jurek był <kapitanem> szkolnej drużyny koszykówki i świetnie motywował kolegów.",
                            "target_lang_translations": {
                                "ru": "Юрек был <капитаном> школьной баскетбольной команды и отлично мотивировал товарищей."
                            }
                        },
                        {
                            "text": "Sędzia wezwał do siebie <kapitanów> obu drużyn przed rozpoczęciem meczu.",
                            "target_lang_translations": {
                                "ru": "Судья подозвал к себе <капитанов> обеих команд перед началом матча."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kapitan drużyny",
                        "opaska kapitana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок, представляющий свою спортивную команду и выполняющий в ней функции лидера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "в спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b87398a0-852e-3716-955d-012e3f04e8c8",
                    "sense_definition": "Stopień oficerski w wojsku, policji i innych służbach mundurowych, znajdujący się zwykle między porucznikiem a majorem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szeregowy bardzo bał się swojego <kapitana>, który słynął z surowości.",
                            "target_lang_translations": {
                                "ru": "Рядовой очень боялся своего <капитана>, который славился строгостью."
                            }
                        },
                        {
                            "text": "Za zasługi na polu walki młody oficer otrzymał awans na <kapitana>.",
                            "target_lang_translations": {
                                "ru": "За боевые заслуги молодой офицер получил повышение до <капитана>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "awans na kapitana",
                        "panie kapitanie",
                        "stopień kapitana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Офицерское звание в армии, полиции и других службах, обычно между старшим лейтенантом и майором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21853783-1232-32ed-bbe9-6ce357b554d6",
                    "sense_definition": "Trzeci, najmłodszy wiek gwadalupu (środkowego permu) w geologii.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Osady z okresu <kapitana> zawierają liczne skamieniałości organizmów morskich.",
                            "target_lang_translations": {
                                "ru": "Отложения периода <кептена> содержат многочисленные окаменелости морских организмов."
                            }
                        },
                        {
                            "text": "Badania stratygraficzne pozwoliły precyzyjnie określić ten poziom jako <kapitan>.",
                            "target_lang_translations": {
                                "ru": "Стратиграфические исследования позволили точно определить этот уровень как <кептен>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "termin geologiczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третий, самый поздний ярус гваделупского отдела пермской системы в геологии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кептен",
                                "target_lang_sense_clarification": "геология"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "careful",
        "carefully",
        "careless",
        "carelessly",
        "carefree",
        "caring",
        "carer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "40fb1661-d05e-3911-8f8f-757def524f68",
                    "sense_definition": "To feel concern or interest; to attach importance to something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_concern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She doesn't <w>care</w> what other people think.",
                            "target_lang_translations": {
                                "ru": "Её не <w>волнует</w>, что думают другие."
                            }
                        },
                        {
                            "text": "I really <w>care</w> about your safety.",
                            "target_lang_translations": {
                                "ru": "Я действительно <w>беспокоюсь</w> о твоей безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "worry",
                        "mind",
                        "bother",
                        "concern oneself",
                        "trouble oneself"
                    ],
                    "antonyms": [
                        "let slide"
                    ],
                    "common_phrases": [
                        "care about",
                        "who cares",
                        "I don't care",
                        "couldn't care less"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать беспокойство или интерес; придавать чему-либо значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волноваться",
                                "target_lang_sense_clarification": "испытывать эмоциональное беспокойство или интерес"
                            },
                            {
                                "target_lang_word": "беспокоиться",
                                "target_lang_sense_clarification": "проявлять озабоченность чем-либо"
                            },
                            {
                                "target_lang_word": "быть небезразличным",
                                "target_lang_sense_clarification": "проявлять интерес, не быть равнодушным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4691be94-c67e-3ba5-8bd1-a797e221701a",
                    "sense_definition": "To look after or provide for the needs of someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>cared</w> for his sick mother for many years.",
                            "target_lang_translations": {
                                "ru": "Он много лет <w>ухаживал</w> за своей больной матерью."
                            }
                        },
                        {
                            "text": "It takes a lot of time to <w>care</w> for a large garden.",
                            "target_lang_translations": {
                                "ru": "Чтобы <w>ухаживать</w> за большим садом, требуется много времени."
                            }
                        }
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "care for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присматривать или обеспечивать потребности кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухаживать",
                                "target_lang_sense_clarification": "следить за состоянием больного, ребенка или объекта"
                            },
                            {
                                "target_lang_word": "заботиться",
                                "target_lang_sense_clarification": "проявлять внимание и попечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c4a6bc9-2ccd-3e44-b9a5-5e056f0cb585",
                    "sense_definition": "To want, desire, or like something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Would you <w>care</w> for a cup of tea?",
                            "target_lang_translations": {
                                "ru": "Не <w>хотите</w> ли чашечку чая?"
                            }
                        },
                        {
                            "text": "I do not <w>care</w> to discuss this matter any further.",
                            "target_lang_translations": {
                                "ru": "Я не <w>желаю</w> больше обсуждать этот вопрос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "care for",
                        "care to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or polite"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хотеть, желать или любить что-либо (обычно в вежливых вопросах или отрицаниях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "иметь желание (часто в вопросе 'не хотите ли?')"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "испытывать стремление (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "835363d4-c3c3-3373-b665-e4dee07eb75c",
                    "sense_definition": "To mind or object to something.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_concern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you <w>care</w> if I open the window?",
                            "target_lang_translations": {
                                "ru": "Вы не <w>возражаете</w>, если я открою окно?"
                            }
                        },
                        {
                            "text": "I don't <w>care</w> if we stay or go.",
                            "target_lang_translations": {
                                "ru": "Меня не <w>волнует</w>, останемся мы или уйдем."
                            }
                        }
                    ],
                    "synonyms": [
                        "mind"
                    ],
                    "common_phrases": [
                        "if you don't care"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возражать или быть против чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возражать",
                                "target_lang_sense_clarification": "выражать несогласие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "171faab7-6596-3394-98d4-b242a697a501",
                    "sense_definition": "Serious attention or caution applied to avoid damage, risk, or error.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please handle the glass vase with <w>care</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, обращайтесь со стеклянной вазой с <w>осторожностью</w>."
                            }
                        },
                        {
                            "text": "You should take <w>care</w> when crossing the busy street.",
                            "target_lang_translations": {
                                "ru": "Тебе следует проявлять <w>осторожность</w>, переходя оживленную улицу."
                            }
                        }
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "take care",
                        "handle with care",
                        "with care"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серьезное внимание или осторожность, применяемые во избежание ущерба, риска или ошибки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осторожность",
                                "target_lang_sense_clarification": "осмотрительность в действиях"
                            },
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "сосредоточенность на безопасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6adb29fe-1a0b-37c5-bd53-c1f55f91fd12",
                    "sense_definition": "The provision of what is necessary for the health, welfare, maintenance, and protection of someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hospital provides excellent medical <w>care</w> to all patients.",
                            "target_lang_translations": {
                                "ru": "Больница предоставляет отличный медицинский <w>уход</w> всем пациентам."
                            }
                        },
                        {
                            "text": "Proper <w>care</w> of your car includes regular oil changes.",
                            "target_lang_translations": {
                                "ru": "Правильный <w>уход</w> за вашим автомобилем включает регулярную замену масла."
                            }
                        }
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "health care",
                        "medical care",
                        "skin care",
                        "child care",
                        "intensive care"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечение того, что необходимо для здоровья, благополучия, содержания и защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уход",
                                "target_lang_sense_clarification": "действия по поддержанию состояния (медицинский, за кожей)"
                            },
                            {
                                "target_lang_word": "забота",
                                "target_lang_sense_clarification": "деятельность по обеспечению благополучия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2697342b-2e61-31f3-9523-aa9663228d62",
                    "sense_definition": "A feeling of worry, anxiety, or sadness.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_concern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He smiled as if he didn't have a <w>care</w> in the world.",
                            "target_lang_translations": {
                                "ru": "Он улыбался так, словно у него не было ни одной <w>заботы</w> на свете."
                            }
                        },
                        {
                            "text": "Her face was full of <w>care</w> after the bad news.",
                            "target_lang_translations": {
                                "ru": "Её лицо было полно <w>тревоги</w> после плохих новостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "worry"
                    ],
                    "common_phrases": [
                        "care in the world",
                        "free from care"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство беспокойства, тревоги или печали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забота",
                                "target_lang_sense_clarification": "бремя ответственности или тревога"
                            },
                            {
                                "target_lang_word": "тревога",
                                "target_lang_sense_clarification": "состояние беспокойства"
                            },
                            {
                                "target_lang_word": "беспокойство",
                                "target_lang_sense_clarification": "душевное волнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c9dd463-c949-3630-98d7-fa8843432556",
                    "sense_definition": "The legal or official state of being looked after, especially by the government.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child was placed in <w>care</w> for his own safety.",
                            "target_lang_translations": {
                                "ru": "Ребёнка передали под <w>опеку</w> ради его собственной безопасности."
                            }
                        },
                        {
                            "text": "Young people leaving <w>care</w> often need extra support.",
                            "target_lang_translations": {
                                "ru": "Молодые люди, выходящие из-под <w>опеки</w>, часто нуждаются в дополнительной поддержке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in care",
                        "foster care"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическое или официальное состояние нахождения под присмотром, особенно со стороны государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опека",
                                "target_lang_sense_clarification": "форма государственной защиты и надзора"
                            },
                            {
                                "target_lang_word": "попечение",
                                "target_lang_sense_clarification": "покровительство и забота"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "fdcd6faa-e8b8-33d2-98af-edad3609a2f2",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Care</w> signed the document.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Кэр</w> подписал документ."
                            }
                        },
                        {
                            "text": "We met a family named <w>Care</w>.",
                            "target_lang_translations": {
                                "ru": "Мы познакомились с семьей по фамилии <w>Кэр</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кэр",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a34d39af-761a-363b-a448-ea6580489b2b",
                    "sense_definition": "Eerlijk en betrouwbaar van karakter; iemand die de waarheid spreekt en geen slechte bedoelingen heeft.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter is een <w>oprecht</w> man die altijd het beste met zijn patiënten voorheeft.",
                            "target_lang_translations": {
                                "ru": "Доктор — <w>искренний</w> человек, который всегда желает своим пациентам самого лучшего."
                            }
                        },
                        {
                            "text": "Zij is een <w>oprechte</w> vrouw die nooit tegen haar vrienden zou liegen.",
                            "target_lang_translations": {
                                "ru": "Она <w>искренняя</w> женщина, которая никогда бы не солгала своим друзьям."
                            }
                        }
                    ],
                    "synonyms": [
                        "eerlijk",
                        "betrouwbaar"
                    ],
                    "antonyms": [
                        "onoprecht",
                        "vals"
                    ],
                    "common_phrases": [
                        "een oprecht mens",
                        "een oprecht persoon",
                        "eerlijk en oprecht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честный и надежный по характеру; тот, кто говорит правду и не имеет дурных намерений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искренний",
                                "target_lang_sense_clarification": "о человеке: правдивый, откровенный"
                            },
                            {
                                "target_lang_word": "честный",
                                "target_lang_sense_clarification": "не лживый, добросовестный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f57cc66d-40a7-38c4-8806-907b528fbc4c",
                    "sense_definition": "Gemeend en niet gespeeld; recht uit het hart komend.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil je bedanken voor je <w>oprechte</w> steun tijdens deze lastige periode.",
                            "target_lang_translations": {
                                "ru": "Я хочу поблагодарить тебя за твою <w>искреннюю</w> поддержку в этот трудный период."
                            }
                        },
                        {
                            "text": "Hij toonde <w>oprechte</w> interesse in mijn verhaal en luisterde aandachtig naar wat ik zei.",
                            "target_lang_translations": {
                                "ru": "Он проявил <w>неподдельный</w> интерес к моему рассказу и внимательно слушал то, что я говорил."
                            }
                        },
                        {
                            "text": "Ik weet <w>oprecht</w> niet wat ik moet doen in deze ingewikkelde situatie.",
                            "target_lang_translations": {
                                "ru": "Я <w>искренне</w> не знаю, что делать в этой сложной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "betrouwbaar",
                        "eerlijk",
                        "waarheidsgetrouw"
                    ],
                    "antonyms": [
                        "onoprecht",
                        "vals"
                    ],
                    "common_phrases": [
                        "oprechte deelneming",
                        "oprechte excuses",
                        "oprechte dank",
                        "oprechte interesse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Истинный, не наигранный; идущий от чистого сердца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искренний",
                                "target_lang_sense_clarification": "выражающий подлинные чувства"
                            },
                            {
                                "target_lang_word": "неподдельный",
                                "target_lang_sense_clarification": "настоящий, не фальшивый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "2818ddca-449b-3b30-853b-3ef39b1c43cf",
                    "sense_definition": "Zonder gestraft te worden of zonder enige belemmering.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte ging <w>vrijuit</w> omdat er onvoldoende bewijs tegen hem was gevonden.",
                            "target_lang_translations": {
                                "en": "The suspect went <w>scot-free</w> because insufficient evidence was found against him.",
                                "ru": "Подозреваемый избежал <w>наказания</w>, так как против него было найдено недостаточно улик."
                            }
                        },
                        {
                            "text": "In dit natuurgebied kunnen de wilde dieren <w>vrijuit</w> rondlopen.",
                            "target_lang_translations": {
                                "en": "In this nature reserve, the wild animals can roam <w>freely</w>.",
                                "ru": "В этом заповеднике дикие животные могут <w>свободно</w> бродить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vrijuit gaan"
                    ],
                    "target_lang_definitions": {

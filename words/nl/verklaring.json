                {
                    "sense_id": "07532cd7-1fd5-3182-b6d0-f67e31ce920d",
                    "sense_definition": "Een uitleg die duidelijk maakt waarom iets is gebeurd of wat iets betekent",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "explanation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wetenschapper zocht naar een logische <w>verklaring</w> voor het vreemde fenomeen.",
                            "target_lang_translations": {
                                "en": "The scientist was looking for a logical <w>explanation</w> for the strange phenomenon.",
                                "ru": "Учёный искал логическое <w>объяснение</w> странному явлению."
                            }
                        },
                        {
                            "text": "Zijn <w>verklaring</w> voor de vertraging klonk helaas erg ongeloofwaardig.",
                            "target_lang_translations": {
                                "en": "His <w>explanation</w> for the delay unfortunately sounded very implausible.",
                                "ru": "Его <w>объяснение</w> задержки, к сожалению, звучало очень неправдоподобно."
                            }
                        }
                    ],
                    "synonyms": [
                        "toelichting"
                    ],
                    "common_phrases": [
                        "een logische verklaring",
                        "geen verklaring hebben",
                        "een verklaring zoeken"
                    ],
                    "target_lang_definitions": {
                        "en": "An explanation that clarifies why something happened or what something means",
                        "ru": "Объяснение, которое проясняет, почему что-то произошло или что что-то означает"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "explanation",
                                "target_lang_sense_clarification": "clarification, reason"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "объяснение",
                                "target_lang_sense_clarification": "толкование, причина"
                            },
                            {
                                "target_lang_word": "разъяснение",
                                "target_lang_sense_clarification": "уточнение смысла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c13957e-b4ae-3791-a804-31efb3dfff0a",
                    "sense_definition": "Een officiële mededeling of schriftelijk stuk waarin een standpunt of feit wordt vastgelegd",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "statement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur zal later vandaag een officiële <w>verklaring</w> afleggen aan de pers.",
                            "target_lang_translations": {
                                "en": "The director will make an official <w>statement</w> to the press later today.",
                                "ru": "Директор сделает официальное <w>заявление</w> для прессы сегодня позже."
                            }
                        },
                        {
                            "text": "In de schriftelijke <w>verklaring</w> stond dat het bedrijf gaat reorganiseren.",
                            "target_lang_translations": {
                                "en": "The written <w>statement</w> said that the company is going to reorganize.",
                                "ru": "В письменном <w>заявлении</w> говорилось, что компания собирается провести реорганизацию."
                            }
                        }
                    ],
                    "synonyms": [

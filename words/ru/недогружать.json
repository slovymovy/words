    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "26587eba-49b0-3e10-9330-0037637f35ce",
                    "sense_definition": "Грузить или наполнять что-либо грузом не полностью, меньше установленной нормы или вместимости.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "loading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие старались не <w>недогружать</w> вагоны, чтобы выполнить план перевозок.",
                            "target_lang_translations": {
                                "en": "The workers tried not to <w>underload</w> the wagons to meet the shipping plan."
                            }
                        },
                        {
                            "text": "Если постоянно <w>недогружать</w> стиральную машину, расход воды будет неэффективным.",
                            "target_lang_translations": {
                                "en": "If you constantly <w>underload</w> the washing machine, water usage will be inefficient."
                            }
                        }
                    ],
                    "antonyms": [
                        "перегружать"
                    ],
                    "common_phrases": [
                        "недогружать вагон",
                        "недогружать судно",
                        "недогружать машину"
                    ],
                    "target_lang_definitions": {
                        "en": "To load or fill something incompletely, with less than the established norm or capacity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "underload",
                                "target_lang_sense_clarification": "vehicle or container"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4474023a-40f8-3abc-88fb-d0493952d5e1",
                    "sense_definition": "Обеспечивать кого-либо работой или задачами в недостаточном объёме; давать меньше дел, чем человек может выполнить.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "workload",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник отдела предпочитал <w>недогружать</w> новичков в первую неделю работы.",
                            "target_lang_translations": {
                                "en": "The department head preferred to <w>underload</w> newcomers during their first week of work."
                            }
                        },
                        {
                            "text": "Умных сотрудников нельзя <w>недогружать</w>, иначе они потеряют интерес к проекту.",
                            "target_lang_translations": {
                                "en": "Smart employees must not be <w>underloaded</w>, otherwise they will lose interest in the project."
                            }
                        }
                    ],
                    "antonyms": [
                        "перегружать"
                    ],
                    "common_phrases": [
                        "недогружать сотрудников",
                        "недогружать работой"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide someone with an insufficient volume of work or tasks; to assign fewer duties than a person can perform."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "underload",
                                "target_lang_sense_clarification": "with work"
                            },
                            {
                                "target_lang_word": "underwork",
                                "target_lang_sense_clarification": "people or staff"
                            }
                        ]
                    }
                }
            ]
        }
    ]

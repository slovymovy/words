                {
                    "sense_id": "d2303a4a-b0b7-341f-9647-f450ce8040c4",
                    "sense_definition": "Een zeer mager persoon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "thin_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ondanks dat hij veel eet, blijft hij een echte <w>gratenbaal</w>.",
                            "target_lang_translations": {
                                "en": "Despite eating a lot, he remains a real <w>bag of bones</w>.",
                                "ru": "Несмотря на то, что он много ест, он остаётся настоящим <w>скелетом</w>."
                            }
                        },
                        {
                            "text": "Na zijn lange ziekbed was de man veranderd in een <w>gratenbaal</w>.",
                            "target_lang_translations": {
                                "en": "After his long illness, the man had turned into a <w>skeleton</w>.",
                                "ru": "После долгой болезни мужчина превратился в <w>мешок с костями</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "graat",
                        "lat",
                        "spriet"
                    ],
                    "antonyms": [
                        "dikkerd"
                    ],
                    "common_phrases": [
                        "een echte gratenbaal"
                    ],
                    "target_lang_definitions": {
                        "en": "A very thin person.",
                        "ru": "Очень худой человек."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bag of bones",

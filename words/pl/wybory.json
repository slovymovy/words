                    "sense_id": "ad57a795-50b3-3794-bc19-1879cefb98b2",
                    "sense_definition": "forma liczby mnogiej od słowa <w>wybór</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasze codzienne <w>wybory</w> mają wpływ na naszą przyszłość.",
                            "target_lang_translations": {
                                "ru": "Наш ежедневный <w>выбор</w> влияет на наше будущее."
                            }
                        },
                        {
                            "text": "To były bardzo trudne <w>wybory</w> dla całej naszej rodziny.",
                            "target_lang_translations": {
                                "ru": "Это был очень трудный <w>выбор</w> для всей нашей семьи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trudne wybory",
                        "dokonywać wyborów"
                    ],

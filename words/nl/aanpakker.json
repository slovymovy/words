    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db66cc8d-e3f1-3079-bf46-c29e2f449c76",
                    "sense_definition": "Iemand die hard werkt en initiatief toont om dingen gedaan te krijgen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een echte <w>aanpakker</w> en krijgt veel werk gedaan op een dag.",
                            "target_lang_translations": {
                                "en": "She is a real <w>go-getter</w> and gets a lot of work done in a day."
                            }
                        },
                        {
                            "text": "Voor deze functie zoeken we een <w>aanpakker</w> die niet bang is voor vieze handen.",
                            "target_lang_translations": {
                                "en": "For this position, we are looking for a <w>doer</w> who isn't afraid of getting their hands dirty."
                            }
                        }
                    ],
                    "antonyms": [
                        "luilak",
                        "nietsnut"
                    ],
                    "common_phrases": [
                        "een echte aanpakker",
                        "van aanpakken weten"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who works hard and shows initiative to get things done."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go-getter",
                                "target_lang_sense_clarification": "energetic and determined person"
                            },
                            {
                                "target_lang_word": "doer",
                                "target_lang_sense_clarification": "person who acts rather than just talking"
                            },
                            {
                                "target_lang_word": "hands-on person",
                                "target_lang_sense_clarification": "someone practical who gets involved"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "550c3634-f8f6-383a-98a9-3a9fdf5cdc90",
                    "sense_definition": "Een doekje of lapje waarmee men hete voorwerpen in de keuken kan vastpakken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kitchen_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gebruik een <w>aanpakker</w> om de hete schaal uit de oven te halen.",
                            "target_lang_translations": {
                                "en": "Use a <w>potholder</w> to take the hot dish out of the oven."
                            }
                        },
                        {
                            "text": "Zonder <w>aanpakker</w> of pannenlap verbrand je je vingers aan de pan.",
                            "target_lang_translations": {
                                "en": "Without a <w>potholder</w> or oven mitt, you will burn your fingers on the pan."
                            }
                        }
                    ],
                    "synonyms": [
                        "pannenlap",
                        "aanvattertje"
                    ],
                    "target_lang_definitions": {
                        "en": "A small cloth or pad used to grasp hot objects in the kitchen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "potholder",
                                "target_lang_sense_clarification": "protective pad for hot cookware"
                            },
                            {
                                "target_lang_word": "oven cloth",
                                "target_lang_sense_clarification": "cloth for handling hot items"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
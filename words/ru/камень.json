            "senses": [
                {
                    "sense_id": "586d58e6-e730-3f13-a1ff-dfe7bb387619",
                    "sense_definition": "Отдельный кусок твёрдой горной породы или минерала.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_rock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик поднял с земли тяжёлый <w>камень</w> и бросил его в воду.",
                            "target_lang_translations": {
                                "en": "The boy picked up a heavy <w>stone</w> from the ground and threw it into the water."
                            }
                        },
                        {
                            "text": "Туристы сели отдохнуть на большой плоский <w>камень</w> у дороги.",
                            "target_lang_translations": {
                                "en": "The tourists sat down to rest on a large flat <w>rock</w> by the road."
                            }
                        }
                    ],
                    "synonyms": [
                        "булыжник"
                    ],
                    "common_phrases": [
                        "бросить камень",
                        "споткнуться о камень",
                        "подводный камень"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate piece of hard rock or mineral."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stone",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "rock",
                                "target_lang_sense_clarification": "often larger or rougher"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "189cb6ab-5b91-36b0-8d67-c169caa126b2",
                    "sense_definition": "Твёрдая горная порода как строительный материал.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_rock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стены этого старинного замка были построены из серого <w>камня</w>.",
                            "target_lang_translations": {
                                "en": "The walls of this ancient castle were built of grey <w>stone</w>."
                            }
                        },
                        {
                            "text": "Для фундамента дома нам нужно заказать машину песка и <w>камня</w>.",
                            "target_lang_translations": {
                                "en": "For the house foundation, we need to order a truckload of sand and <w>stone</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "горная порода"
                    ],
                    "common_phrases": [
                        "дом из камня",
                        "холодный как камень",
                        "сердце из камня"
                    ],
                    "target_lang_definitions": {
                        "en": "Hard rock used as a building material."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stone",
                                "target_lang_sense_clarification": "material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7f86519-9131-33b7-8fbd-e6743c58a2c5",
                    "sense_definition": "Драгоценный или полудрагоценный минерал, используемый в украшениях.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_rock",
                    "name_type": "no",
                    "examples": [
                        {

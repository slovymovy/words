            "senses": [
                {
                    "sense_id": "3fcc45c6-bb8b-3bf9-b8dd-285b2be481bd",
                    "sense_definition": "gekleurd in een combinatie van blauwe en witte vlakken, strepen of patronen",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "color_pattern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze droeg een vrolijke <w>blauw-wit</w> gestreepte jurk naar het feest.",
                            "target_lang_translations": {
                                "ru": "Она надела на вечеринку веселое платье в <w>сине-белую</w> полоску."
                            }
                        },
                        {
                            "text": "De vlag van Griekenland is <w>blauw-wit</w>.",
                            "target_lang_translations": {
                                "ru": "Флаг Греции — <w>бело-синий</w>."
                            }

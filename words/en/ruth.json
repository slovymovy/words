                    "semantic_group_id": "emotion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was filled with <w>ruth</w> for the harsh words she had spoken in anger.",
                            "target_lang_translations": {
                                "ru": "Она была полна <w>раскаяния</w> за резкие слова, сказанные в гневе."
                            }

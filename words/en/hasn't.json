{
    "word_family": [
        "have-not"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "61936491-927b-3c70-823d-21d881420603",
                    "sense_definition": "Contraction of <w>has</w> not used as an auxiliary verb to form the perfect tenses.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>hasn't</w> finished his homework yet.",
                            "target_lang_translations": {
                                "ru": "Он <w>еще не</w> закончил свою домашнюю работу."
                            }
                        },
                        {
                            "text": "She <w>hasn't</w> been to the new park.",
                            "target_lang_translations": {
                                "ru": "Она <w>не</w> была в новом парке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hasn't been",
                        "hasn't done",
                        "hasn't seen",
                        "hasn't got"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "contraction"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"has not\", используемое как вспомогательный глагол для образования времен группы Perfect."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "отрицательная частица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ea9f14b-bdee-32b1-adc7-fa54c14961a8",
                    "sense_definition": "Contraction of <w>has</w> not used to indicate a lack of possession.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "lexical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poor man <w>hasn't</w> a penny to his name.",
                            "target_lang_translations": {
                                "ru": "У бедняка <w>нет</w> ни гроша за душой."
                            }
                        },
                        {
                            "text": "She <w>hasn't</w> a clue about what happened.",
                            "target_lang_translations": {
                                "ru": "У нее <w>нет</w> ни малейшего представления о том, что случилось."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hasn't a clue",
                        "hasn't time",
                        "hasn't any"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "contraction"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"has not\", используемое для указания на отсутствие обладания чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отсутствие (в конструкции \"у кого-то нет\")"
                            },
                            {
                                "target_lang_word": "не имеет",
                                "target_lang_sense_clarification": "отсутствие обладания"
                            }
                        ]
                    }
                }
            ]
        }
    ]

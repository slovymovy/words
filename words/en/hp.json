    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3832e201-96e7-383c-8f13-9ccab529996f",
                    "sense_definition": "Abbreviation of horsepower, a unit used to measure the power of engines and motors.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new sports car has an engine with over 400 <w>hp</w>.",
                            "target_lang_translations": {
                                "ru": "У нового спорткара двигатель мощностью более 400 <w>л.с.</w>"
                            }
                        },
                        {
                            "text": "You need a tractor with high <w>hp</w> to pull this heavy load.",
                            "target_lang_translations": {
                                "ru": "Вам нужен трактор с большим количеством <w>лошадиных сил</w>, чтобы потянуть этот тяжелый груз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brake hp",
                        "engine hp"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «лошадиная сила», единица измерения мощности двигателей и моторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "л.с.",
                                "target_lang_sense_clarification": "сокращение"
                            },
                            {
                                "target_lang_word": "лошадиная сила",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66518c07-ece2-353c-be54-660a9f0609ce",
                    "sense_definition": "Initialism of hit points or health points; a number representing the remaining life or health of a character in a video game.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to use a healing potion because my <w>HP</w> is very low.",
                            "target_lang_translations": {
                                "ru": "Мне нужно использовать лечебное зелье, потому что у меня очень мало <w>очков здоровья</w>."
                            }
                        },
                        {
                            "text": "The final boss has a massive amount of <w>HP</w> and is hard to defeat.",
                            "target_lang_translations": {
                                "ru": "У финального босса огромное количество <w>ХП</w>, и его трудно победить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "full HP",
                        "zero HP",
                        "lose HP"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «hit points» (очки здоровья); число, обозначающее оставшийся уровень жизни или здоровья персонажа в видеоигре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очки здоровья",
                                "target_lang_sense_clarification": "термин в играх"
                            },
                            {
                                "target_lang_word": "ХП",
                                "target_lang_sense_clarification": "сленг геймеров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41c6ae79-bc8e-3eae-ae6a-44891555df87",
                    "sense_definition": "Initialism of hire purchase; a system of paying for goods in installments while using them.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They could not afford the car outright, so they bought it on <w>HP</w>.",
                            "target_lang_translations": {
                                "ru": "Они не могли позволить себе машину сразу, поэтому купили её в <w>рассрочку</w>."
                            }
                        },
                        {
                            "text": "The <w>HP</w> agreement requires us to make monthly payments for three years.",
                            "target_lang_translations": {
                                "ru": "Договор <w>рассрочки</w> требует от нас ежемесячных платежей в течение трех лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on HP",
                        "buy on HP"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «hire purchase» (покупка в рассрочку); система оплаты товаров частями при их использовании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассрочка",
                                "target_lang_sense_clarification": "способ оплаты"
                            },
                            {
                                "target_lang_word": "покупка в рассрочку",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "26daf8c9-4f94-3a0a-ac0e-06a937715c05",
                    "sense_definition": "Initialism of Hewlett-Packard, a major American technology company known for computers and printers.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "I need to buy a new ink cartridge for my <w>HP</w> printer.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый картридж для моего принтера <w>HP</w>."
                            }
                        },
                        {
                            "text": "Our office uses <w>HP</w> laptops for all the employees.",
                            "target_lang_translations": {
                                "ru": "Наш офис использует ноутбуки <w>HP</w> для всех сотрудников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "HP printer",
                        "HP laptop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от Hewlett-Packard, крупной американской технологической компании, известной своими компьютерами и принтерами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "HP",
                                "target_lang_sense_clarification": "бренд"

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7a2afecb-b740-3c8a-a89a-2e065bc80f30",
                    "sense_definition": "Trwający bez przerwy przez dłuższy czas; nieustanny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przygnębiasz mnie swoim <w>ciągłym</w> narzekaniem na pogodę.",
                            "target_lang_translations": {
                                "ru": "Ты угнетаешь меня своим <w>постоянным</w> нытьем о погоде."
                            }
                        },
                        {
                            "text": "Jej <w>ciągła</w> praca nad nowym projektem nie pozwalała jej na odpoczynek.",
                            "target_lang_translations": {
                                "ru": "Ее <w>непрерывная</w> работа над новым проектом не позволяла ей отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieustanny",
                        "bezustanny",
                        "stały"
                    ],
                    "antonyms": [
                        "chwilowy",
                        "dorywczy"
                    ],
                    "common_phrases": [
                        "ciągły ruch",
                        "ciągły hałas",
                        "ciągły stres",
                        "ciągły rozwój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающийся без перерыва в течение длительного времени; непрестанный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непрерывный",
                                "target_lang_sense_clarification": "длящийся без пауз"
                            },
                            {
                                "target_lang_word": "постоянный",
                                "target_lang_sense_clarification": "всё время существующий"
                            },
                            {
                                "target_lang_word": "беспрерывный",
                                "target_lang_sense_clarification": "без перерывов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b108e2e-3ad2-3176-a7b7-1906419f25f0",
                    "sense_definition": "Powtarzający się bardzo często lub regularnie; uporczywy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "repetition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo <w>ciągłych</w> skarg mieszkańców okolicy, głośny klub nadal działa.",
                            "target_lang_translations": {
                                "ru": "Несмотря на <w>постоянные</w> жалобы местных жителей, шумный клуб продолжает работать."
                            }
                        },
                        {
                            "text": "Rozwiodła się z nim z powodu jego <w>ciągłych</w> kłamstw i zdrad.",
                            "target_lang_translations": {
                                "ru": "Она развелась с ним из-за его <w>постоянной</w> лжи и измен."
                            }
                        }
                    ],
                    "synonyms": [
                        "uporczywy",
                        "notoryczny",
                        "częsty"
                    ],
                    "antonyms": [
                        "rzadki",
                        "sporadyczny"
                    ],
                    "common_phrases": [
                        "ciągłe zmiany",
                        "ciągłe problemy",
                        "ciągłe pytania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторяющийся очень часто или регулярно; настойчивый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоянный",
                                "target_lang_sense_clarification": "регулярный, частый"
                            },
                            {
                                "target_lang_word": "бесконечный",
                                "target_lang_sense_clarification": "надоедливый, нескончаемый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58b8ba4a-e7d5-3906-8f0d-ba7339a9bc2c",
                    "sense_definition": "Stanowiący nieprzerwaną całość w przestrzeni; niemający przerw ani luk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "space_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podwójna <w>ciągła</w> linia na środku jezdni oznacza, że wyprzedzanie jest niedozwolone.",
                            "target_lang_translations": {
                                "ru": "Двойная <w>сплошная</w> линия посередине дороги означает, что обгон запрещен."
                            }
                        },
                        {
                            "text": "Wzdłuż granicy państwa zbudowano wysoki i <w>ciągły</w> mur.",
                            "target_lang_translations": {
                                "ru": "Вдоль государственной границы была построена высокая и <w>сплошная</w> стена."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprzerwany",
                        "lity"
                    ],
                    "antonyms": [
                        "przerywany"
                    ],
                    "common_phrases": [
                        "linia ciągła",
                        "szew ciągły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представляющий собой непрерывное целое в пространстве; не имеющий перерывов или пробелов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сплошной",
                                "target_lang_sense_clarification": "без промежутков (например, линия)"
                            },
                            {
                                "target_lang_word": "непрерывный",
                                "target_lang_sense_clarification": "целостный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "837bf058-46ac-3f7e-b251-2badfe02f01e",
                    "sense_definition": "W matematyce: taki, którego wykres nie ma przerw; zachowujący granicę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel tłumaczył nam, czym charakteryzuje się funkcja <w>ciągła</w> w danym przedziale.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснял нам, чем характеризуется <w>непрерывная</w> функция в данном интервале."
                            }
                        },
                        {
                            "text": "Zmienna losowa o rozkładzie <w>ciągłym</w> może przyjmować dowolne wartości z zakresu.",
                            "target_lang_translations": {
                                "ru": "Случайная величина с <w>непрерывным</w> распределением может принимать любые значения из диапазона."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieciągły"
                    ],
                    "common_phrases": [
                        "funkcja ciągła",
                        "rozkład ciągły"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "matematyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В математике: такой, график которого не имеет разрывов; сохраняющий предел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непрерывный",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

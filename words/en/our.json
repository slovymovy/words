    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "0da0c955-044c-350d-8642-a55622347b1f",
                    "sense_definition": "Belonging to or associated with the speaker and one or more other people.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sorry, you are not invited to <w>our</w> party.",
                            "target_lang_translations": {
                                "ru": "Извини, ты не приглашен на <w>нашу</w> вечеринку."
                            }
                        },
                        {
                            "text": "We must try to save <w>our</w> marriage for the sake of the children.",
                            "target_lang_translations": {
                                "ru": "Мы должны попытаться сохранить <w>наш</w> брак ради детей."
                            }
                        }
                    ],
                    "antonyms": [
                        "your",
                        "their"
                    ],
                    "common_phrases": [
                        "our house",
                        "our family",
                        "our country",
                        "our team",
                        "our best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий или связанный с говорящим и одним или несколькими другими людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "принадлежащий нам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6241c633-bab0-3453-ad46-81f8c68e638b",
                    "sense_definition": "Belonging to people in general.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possession_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As we get older, <w>our</w> reactions get slower.",
                            "target_lang_translations": {
                                "ru": "С возрастом <w>наши</w> реакции замедляются."
                            }
                        },
                        {
                            "text": "We need to protect <w>our</w> environment for future generations.",
                            "target_lang_translations": {
                                "ru": "Нам нужно беречь <w>нашу</w> окружающую среду для будущих поколений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "our world",
                        "our lives",
                        "our society",
                        "our planet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий людям вообще."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "общий для всех людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01385658-dd55-3b83-89ad-599f791b3282",
                    "sense_definition": "Used before a person's name to indicate that the person is in one's family or is a very close friend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going to see <w>our</w> Terry for tea this afternoon.",
                            "target_lang_translations": {
                                "ru": "Я собираюсь к <w>нашему</w> Терри на чай сегодня днем."
                            }
                        },
                        {
                            "text": "Has <w>our</w> John called you yet?",
                            "target_lang_translations": {
                                "ru": "<w>Наш</w> Джон тебе еще не звонил?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "our kid",
                        "our mum"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to indicate family connection or familiarity"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед именем человека, чтобы указать, что он является членом семьи или очень близким другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "о родственнике или близком человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a3cc7d3-72e6-30ea-8ba0-206f0ef95af8",
                    "sense_definition": "Belonging to the person being addressed; used especially by caregivers or to show empathy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Have we taken <w>our</w> tablets yet?",
                            "target_lang_translations": {
                                "ru": "Мы уже выпили <w>наши</w> таблетки?"
                            }
                        },
                        {
                            "text": "We need to finish <w>our</w> lunch before we go for a walk.",
                            "target_lang_translations": {
                                "ru": "Нам нужно доесть <w>наш</w> обед, прежде чем идти гулять."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take our medicine",
                        "eat our dinner"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in caregiving or patronizing contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий тому, к кому обращаются; используется особенно опекунами или для проявления сочувствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "в ласковом или покровительственном обращении"
                            }
                        ]
                    }
                }
            ]
        }
    ]

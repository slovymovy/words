                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного «zatrucie» (отравление)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отравлений",
                                "target_lang_sense_clarification": "форма мн. ч. родительного падежа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

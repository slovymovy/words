{
    "word_family": [
        "silky",
        "silken",
        "silkworm"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b4bf4c82-16af-3e4e-a151-a502ee0f89c3",
                    "sense_definition": "A fine, soft cloth woven from silk fibers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fabric",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a beautiful <w>silk</w> dress to the evening party.",
                            "target_lang_translations": {
                                "ru": "Она надела красивое платье из <w>шёлка</w> на вечеринку."
                            }
                        },
                        {
                            "text": "This scarf is made of pure <w>silk</w> and feels very smooth.",
                            "target_lang_translations": {
                                "ru": "Этот шарф сделан из чистого <w>шёлка</w> и очень гладкий на ощупь."
                            }
                        }
                    ],
                    "antonyms": [
                        "cotton",
                        "wool"
                    ],
                    "common_phrases": [
                        "pure silk",
                        "silk scarf",
                        "silk tie",
                        "silk dress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкая, мягкая ткань, сотканная из волокон, добываемых из коконов тутового шелкопряда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шёлк",
                                "target_lang_sense_clarification": "ткань"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30e206b7-2dd4-3867-991d-539caacbc2d9",
                    "sense_definition": "A fine fiber excreted by the silkworm or other arthropod (such as a spider).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spider spun a web using strong strands of <w>silk</w>.",
                            "target_lang_translations": {
                                "ru": "Паук сплёл паутину, используя прочные нити <w>шёлка</w>."
                            }
                        },
                        {
                            "text": "Farmers collect raw <w>silk</w> from cocoons to produce expensive fabric.",
                            "target_lang_translations": {
                                "ru": "Фермеры собирают необработанный <w>шёлк</w> из коконов для производства дорогой ткани."
                            }
                        }
                    ],
                    "synonyms": [
                        "fiber",
                        "thread"
                    ],
                    "common_phrases": [
                        "spider silk",
                        "raw silk",
                        "silk thread"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкое волокно, выделяемое тутовым шелкопрядом или другими членистоногими (например, пауком)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шёлк",
                                "target_lang_sense_clarification": "волокно, нити"
                            },
                            {
                                "target_lang_word": "паутина",
                                "target_lang_sense_clarification": "паучий шёлк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57c6bc40-926d-3ee2-a988-8d5a1ec15d49",
                    "sense_definition": "Anything which resembles silk, such as the filiform styles of the female flower of maize.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should remove the <w>silk</w> from the ear of corn before cooking it.",
                            "target_lang_translations": {
                                "ru": "Перед варкой следует удалить <w>рыльца</w> с початка кукурузы."
                            }
                        },
                        {
                            "text": "The corn <w>silk</w> turned brown, indicating that the crop was ready for harvest.",
                            "target_lang_translations": {
                                "ru": "Кукурузные <w>рыльца</w> потемнели, указывая на то, что урожай готов к сбору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "corn silk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Волокна, напоминающие шёлк, например, нитевидные столбики (рыльца) женских цветков кукурузы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыльца",
                                "target_lang_sense_clarification": "кукурузные"
                            },
                            {
                                "target_lang_word": "волокна",
                                "target_lang_sense_clarification": "ботаника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e5f0bef-4913-339c-a1e8-78a343ff1dfe",
                    "sense_definition": "A pair of long silk sheets suspended in the air on which a performer performs tricks.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "acrobatics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The acrobat climbed the aerial <w>silks</w> with incredible strength and grace.",
                            "target_lang_translations": {
                                "ru": "Акробат взбирался на воздушные <w>полотна</w> с невероятной силой и грацией."
                            }
                        },
                        {
                            "text": "She practices on the <w>silks</w> every day to perfect her routine.",
                            "target_lang_translations": {
                                "ru": "Она тренируется на <w>полотнах</w> каждый день, чтобы довести свой номер до совершенства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aerial silks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пара длинных полотен ткани, подвешенных в воздухе, на которых артист выполняет акробатические трюки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полотна",
                                "target_lang_sense_clarification": "воздушная гимнастика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53bc0f99-44c9-3d8d-8820-1ddca081d86b",
                    "sense_definition": "A Queen's Counsel, King's Counsel or Senior Counsel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After many years of successful practice, the lawyer hopes to take <w>silk</w>.",
                            "target_lang_translations": {
                                "ru": "После многих лет успешной практики адвокат надеется получить статус <w>королевского адвоката</w>."
                            }
                        },
                        {
                            "text": "The <w>silk</w> stood up to address the judge on behalf of the defendant.",
                            "target_lang_translations": {
                                "ru": "<w>Королевский адвокат</w> встал, чтобы обратиться к судье от имени подсудимого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take silk"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK, Commonwealth"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "metonym for the gown worn by senior counsel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Королевский адвокат (в Великобритании и странах Содружества); название происходит от шёлковой мантии, которую они носят."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королевский адвокат",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebac83b3-5283-3258-b39e-80ee91fa6da7",
                    "sense_definition": "The garments worn by a jockey displaying the colors of the horse's owner.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jockey wore bright blue and yellow <w>silks</w> for the big race.",
                            "target_lang_translations": {
                                "ru": "Для большой гонки жокей надел ярко-синий с желтым <w>камзол</w>."
                            }
                        },
                        {
                            "text": "Spectators could identify the horse by the pattern on the rider's <w>silks</w>.",
                            "target_lang_translations": {
                                "ru": "Зрители могли узнать лошадь по узору на <w>камзоле</w> наездника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "racing silks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма жокея (обычно камзол и кепка), окрашенная в цвета владельца лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камзол",
                                "target_lang_sense_clarification": "жокейский"
                            },
                            {
                                "target_lang_word": "форма",
                                "target_lang_sense_clarification": "спортивная"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b933e879-d8bd-3cdb-b02e-5c8e9d13bb6f",
                    "sense_definition": "To remove the silk from corn.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We sat on the porch and <w>silked</w> the corn for dinner.",
                            "target_lang_translations": {
                                "ru": "Мы сидели на крыльце и <w>очищали</w> кукурузу от рылец к ужину."
                            }
                        },
                        {
                            "text": "It takes a bit of time to properly <w>silk</w> fresh corn before boiling it.",
                            "target_lang_translations": {
                                "ru": "Требуется немного времени, чтобы правильно <w>очистить</w> свежую кукурузу перед варкой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очищать кукурузу от волокон (рылец)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очищать",
                                "target_lang_sense_clarification": "от волокон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "efa7fb08-6f8a-36f1-bc5b-80ed88adf7b3",
                    "sense_definition": "A surname originating as an occupation for a seller of silk.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Silk</w> opened a new textile shop in the city center.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Силк</w> открыл новый магазин тканей в центре города."
                            }
                        },
                        {
                            "text": "I went to school with a girl whose last name was <w>Silk</w>.",
                            "target_lang_translations": {
                                "ru": "Я учился в школе с девочкой по фамилии <w>Силк</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от профессии торговца шёлком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Силк",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
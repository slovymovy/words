            "pos": "verb",
            "senses": [
                {
                    "sense_id": "98c636f5-37f9-3b34-8e7a-ec9de29cdf6a",
                    "sense_definition": "Znajdować się w pozycji, w której ciężar ciała opiera się na pośladkach (zwykle na krześle, fotelu lub podłodze).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek lubi <w>siedzieć</w> w swoim ulubionym fotelu i czytać gazetę.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка любит <w>сидеть</w> в своем любимом кресле и читать газету."
                            }
                        },
                        {
                            "text": "Musisz <w>siedzieć</w> prosto przy stole, kiedy jesz obiad z rodziną.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>сидеть</w> прямо за столом, когда обедаешь с семьей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "siedzieć na krześle",
                        "siedzieć przy stole",
                        "siedzieć prosto",
                        "siedzieć wygodnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в положении, при котором вес тела приходится на ягодицы (обычно на стуле, кресле или полу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "поза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "317121c0-332e-3815-af20-e7a0239cc7f1",
                    "sense_definition": "Przebywać w jakimś miejscu przez pewien czas, często bezczynnie, czekając na coś lub nie wychodząc na zewnątrz.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_stay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ten deszczowy weekend zamierzam <w>siedzieć</w> w domu i oglądać filmy.",
                            "target_lang_translations": {
                                "ru": "В эти дождливые выходные я собираюсь <w>сидеть</w> дома и смотреть фильмы."
                            }
                        },
                        {
                            "text": "Nie lubię <w>siedzieć</w> w pracy po godzinach, wolę wrócić do rodziny.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <w>сидеть</w> на работе сверхурочно, предпочитаю вернуться к семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "przebywać",
                        "gnić"
                    ],
                    "common_phrases": [
                        "siedzieć w domu",
                        "siedzieć w pracy",
                        "siedzieć cicho",
                        "siedzieć bezczynnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в каком-либо месте в течение определенного времени, часто бездействуя или не выходя наружу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "находиться где-либо"
                            },
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "пребывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2aef5e7-f076-3e36-919c-358ec6cc36d5",
                    "sense_definition": "Odbywać karę pozbawienia wolności w zakładzie karnym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "prison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten znany przestępca będzie <w>siedzieć</w> w więzieniu przez najbliższe dziesięć lat.",
                            "target_lang_translations": {
                                "ru": "Этот известный преступник будет <w>сидеть</w> в тюрьме ближайшие десять лет."
                            }
                        },
                        {
                            "text": "On twierdzi, że musi <w>siedzieć</w> za niewinność, ale sąd miał inne zdanie.",
                            "target_lang_translations": {
                                "ru": "Он утверждает, что ему приходится <w>сидеть</w> без вины, но у суда было иное мнение."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsiadywać",
                        "kiblować"
                    ],
                    "common_phrases": [
                        "siedzieć w więzieniu",
                        "siedzieć za kradzież",
                        "siedzieć za niewinność"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard term is 'odbywać karę'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отбывать наказание в виде лишения свободы в исправительном учреждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "в тюрьме"
                            },
                            {
                                "target_lang_word": "отбывать срок",
                                "target_lang_sense_clarification": "о наказании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ed9b174-979d-3846-8fb7-98d0a3a49777",
                    "sense_definition": "Poświęcać czas na intensywną pracę, naukę lub zajmowanie się czymś szczegółowym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę dzisiaj <w>siedzieć</w> nad książkami, ponieważ jutro mam bardzo ważny egzamin.",
                            "target_lang_translations": {
                                "ru": "Сегодня я должен <w>сидеть</w> за книгами, так как завтра у меня очень важный экзамен."
                            }
                        },
                        {
                            "text": "Księgowa musi <w>siedzieć</w> w dokumentach przez cały tydzień, aby rozliczyć podatki.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер должна <w>сидеть</w> в документах всю неделю, чтобы рассчитать налоги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "siedzieć nad książkami",
                        "siedzieć w papierach",
                        "siedzieć nad projektem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посвящать время напряженной работе, учебе или детальному изучению чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "заниматься чем-либо"
                            },
                            {
                                "target_lang_word": "корпеть",
                                "target_lang_sense_clarification": "усердно работать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b2f49d4-5c65-3978-b1e1-b8d5640af4ca",
                    "sense_definition": "Mieszkać w danym miejscu na stałe lub przez dłuższy okres.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "location_reside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat postanowił <w>siedzieć</w> za granicą jeszcze przez kilka lat, żeby zarobić pieniądze.",
                            "target_lang_translations": {
                                "ru": "Мой брат решил <w>остаться</w> за границей еще на несколько лет, чтобы заработать денег."
                            }
                        },
                        {
                            "text": "Nie możesz wiecznie <w>siedzieć</w> u rodziców, musisz się w końcu usamodzielnić.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь вечно <w>сидеть</w> у родителей, ты должен наконец стать самостоятельным."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieszkać"
                    ],
                    "common_phrases": [
                        "siedzieć za granicą",
                        "siedzieć u rodziców"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жить в определенном месте постоянно или в течение длительного времени (разг.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "проживать"
                            },
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "пребывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81fbc6d6-fc79-3580-a787-e3d3e881c6ef",
                    "sense_definition": "Pasować rozmiarem lub krojem (o ubraniu) lub być mocno osadzonym w czymś (o przedmiocie).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "object_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy garnitur zdaje się <w>siedzieć</w> na tobie idealnie, jakby był szyty na miarę.",
                            "target_lang_translations": {
                                "ru": "Этот новый костюм, кажется, <w>сидит</w> на тебе идеально, как будто сшит на заказ."
                            }
                        },
                        {
                            "text": "Stary gwóźdź musi mocno <w>siedzieć</w> w desce, bo nie mogę go wcale wyciągnąć.",
                            "target_lang_translations": {
                                "ru": "Старый гвоздь, должно быть, крепко <w>сидит</w> в доске, потому что я совсем не могу его вытащить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobrze siedzieć",
                        "siedzieć jak ulał",
                        "siedzieć głęboko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходить по размеру (об одежде) или быть прочно закрепленным в чем-либо (о предмете)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "об одежде или предмете"
                            }
                        ]
                    }
                }
            ]
        }
    ]

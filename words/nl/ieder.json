                {
                    "sense_id": "81e125e9-e505-3291-bf18-4ee1f587b042",
                    "sense_definition": "Elk lid van een groep of geheel afzonderlijk beschouwd.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "each_individual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij gaat <w>ieder</w> jaar naar Frankrijk op vakantie.",
                            "target_lang_translations": {
                                "ru": "Она ездит во Францию в отпуск <w>каждый</w> год."
                            }
                        },
                        {
                            "text": "<w>Ieder</w> kind in de klas kreeg een klein cadeautje.",
                            "target_lang_translations": {

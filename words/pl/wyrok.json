                {
                    "sense_id": "6a7f9d90-54b5-385b-a6af-cc227cdacf5e",
                    "sense_definition": "Oficjalne orzeczenie sądu rozstrzygające sprawę karną lub cywilną, ustalające winę i karę lub uniewinnienie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia ogłosił <wyrok> skazujący oskarżonego na dwa lata więzienia.",
                            "target_lang_translations": {
                                "ru": "Судья огласил обвинительный <приговор>, осудив обвиняемого на два года тюрьмы."
                            }
                        },
                        {
                            "text": "Na sali rozpraw zapadły dzisiaj wyjątkowo surowe <wyroki>.",
                            "target_lang_translations": {
                                "ru": "В зале суда сегодня были вынесены исключительно суровые <приговоры>."
                            }
                        }
                    ],
                    "synonyms": [
                        "orzeczenie",
                        "werdykt"
                    ],
                    "common_phrases": [
                        "wydać wyrok",
                        "prawomocny wyrok",
                        "wyrok w zawieszeniu",
                        "ogłosić wyrok",
                        "wyrok śmierci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное решение суда по уголовному или гражданскому делу, устанавливающее виновность и наказание либо оправдание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приговор",
                                "target_lang_sense_clarification": "решение суда по уголовному делу"
                            },
                            {
                                "target_lang_word": "решение",
                                "target_lang_sense_clarification": "постановление суда, часто по гражданским делам"
                            },
                            {
                                "target_lang_word": "вердикт",
                                "target_lang_sense_clarification": "решение присяжных заседателей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "439960f4-6b83-3d27-92ab-47b7f0941110",
                    "sense_definition": "Przeznaczenie lub los, na który człowiek nie ma wpływu (często używane w kontekście religijnym lub historycznym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy z pokorą przyjąć <wyroki> boskie, których nie jesteśmy w stanie zrozumieć.",
                            "target_lang_translations": {
                                "ru": "Мы должны со смирением принять божью <волю>, которую мы не в силах понять."
                            }
                        },
                        {
                            "text": "Nikt nie przewidział, jakie będą <wyroki> historii dla tego małego narodu.",
                            "target_lang_translations": {
                                "ru": "Никто не предвидел, каким будет <приговор> истории для этого маленького народа."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeznaczenie",
                        "los",
                        "fatum"
                    ],
                    "common_phrases": [
                        "wyroki boskie",
                        "niezbadane wyroki",
                        "wyroki historii",
                        "wyroki losu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судьба или рок, на которые человек не может повлиять (часто используется в религиозном или историческом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приговор",
                                "target_lang_sense_clarification": "перен. неотвратимое решение судьбы или истории"
                            },
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "высшее предопределение, провидение"
                            },
                            {
                                "target_lang_word": "суд",
                                "target_lang_sense_clarification": "устар./религ. божественное решение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

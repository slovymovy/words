            "senses": [
                {
                    "sense_id": "a30c801f-389e-38b5-be26-e2417f1da1b9",
                    "sense_definition": "Określony przedział czasu, często związany z cyklem dnia lub roku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O tej <w>porze</w> roku dni są zazwyczaj bardzo krótkie i chłodne.",
                            "target_lang_translations": {
                                "ru": "В это <w>время</w> года дни обычно очень короткие и прохладные."
                            }
                        },
                        {
                            "text": "Do tej <w>pory</w> nie otrzymałem żadnej wiadomości od brata.",
                            "target_lang_translations": {
                                "ru": "До сих <w>пор</w> я не получил никаких известий от брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "czas",
                        "okres"
                    ],
                    "common_phrases": [
                        "pora roku",
                        "pora dnia",
                        "o tej porze",
                        "do tej pory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный промежуток времени, часто связанный с циклом дня или года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "период, сезон"
                            },
                            {
                                "target_lang_word": "пора",
                                "target_lang_sense_clarification": "период, эпоха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4dc1b0f-c46c-350f-9b43-75dd6f3f5807",
                    "sense_definition": "Odpowiedni moment lub właściwa chwila na wykonanie jakiejś czynności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Już najwyższa <w>pora</w>, żebyśmy zaczęli przygotowywać się do wyjścia.",
                            "target_lang_translations": {
                                "ru": "Уже давно <w>пора</w> начать собираться к выходу."
                            }
                        },
                        {
                            "text": "Przyjechałeś w samą <w>porę</w>, właśnie podajemy gorący obiad.",
                            "target_lang_translations": {
                                "ru": "Ты приехал как раз <w>вовремя</w>, мы как раз подаем горячий обед."
                            }
                        }
                    ],
                    "synonyms": [
                        "czas"
                    ],
                    "common_phrases": [
                        "najwyższa pora",
                        "w samą porę",
                        "pora na",
                        "pora spać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий момент или время для совершения действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пора",
                                "target_lang_sense_clarification": "настало время"
                            },
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "подходящий момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00a4decb-039c-36cc-bd0c-09845c31b967",
                    "sense_definition": "Forma dopełniacza lub biernika liczby pojedynczej rzeczownika <w>por</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "vegetable_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do gotującego się rosołu warto dodać opalonego <w>pora</w>.",
                            "target_lang_translations": {
                                "ru": "В кипящий бульон стоит добавить опаленный <w>лук-порей</w>."
                            }
                        },
                        {
                            "text": "W sklepie nie znalazłem świeżego <w>pora</w>, więc kupiłem cebulę.",
                            "target_lang_translations": {
                                "ru": "В магазине я не нашел свежего <w>лука-порея</w>, поэтому купил лук."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",

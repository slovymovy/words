                },
                {
                    "sense_id": "1c1b5e0e-cc08-351e-b33b-a1ddf32ddfc1",
                    "sense_definition": "Wytworzony lub wykonany przy użyciu maszyny, a nie ręcznie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tynk <w>maszynowy</w> pozwala na znacznie szybsze wykończenie ścian w budynku.",
                            "target_lang_translations": {
                                "ru": "<w>Машинная</w> штукатурка позволяет значительно быстрее отделать стены в здании."
                            }
                        },
                        {
                            "text": "Wstępne tłumaczenie <w>maszynowe</w> wymagało wielu poprawek redaktora.",
                            "target_lang_translations": {
                                "ru": "Черновой <w>машинный</w> перевод требовал множества правок редактора."
                            }
                        }
                    ],
                    "antonyms": [
                        "ręczny"
                    ],
                    "common_phrases": [
                        "szycie maszynowe",
                        "tłumaczenie maszynowe",
                        "tynk maszynowy",
                        "haft maszynowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изготовленный или выполненный с помощью машины, а не вручную."

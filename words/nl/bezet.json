                {
                    "sense_id": "a9c8a391-2b51-34ae-b043-7e95a0daf453",
                    "sense_definition": "Onder controle van een vreemde mogendheid of een vijandig leger.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de oorlog was het hele land jarenlang <w>bezet</w>.",

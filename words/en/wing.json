            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d8054f3-1349-3f13-928a-db0c731cfe79",
                    "sense_definition": "To act or speak without preparation; to improvise.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "improvise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I lost my notes, so I had to <w>wing</w> the presentation.",
                            "target_lang_translations": {
                                "ru": "Я потерял свои записи, поэтому мне пришлось <w>импровизировать</w> на презентации."
                            }
                        },
                        {
                            "text": "Don't worry about the script, just <w>wing</w> it.",
                            "target_lang_translations": {
                                "ru": "Не беспокойся о сценарии, просто <w>импровизируй</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "improvise"
                    ],
                    "antonyms": [
                        "prepare",
                        "rehearse"
                    ],
                    "common_phrases": [
                        "wing it"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "usually in the phrase 'wing it'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действовать или говорить без подготовки; импровизировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импровизировать",
                                "target_lang_sense_clarification": "делать без подготовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fcb4001-d5e1-3e65-895e-1b3c6320b96e",
                    "sense_definition": "To fly or travel through the air.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The swans <w>winged</w> their way south for the winter.",
                            "target_lang_translations": {
                                "ru": "Лебеди <w>летели</w> на юг на зимовку."
                            }
                        },
                        {
                            "text": "We saw a hawk <w>winging</w> towards the forest.",
                            "target_lang_translations": {

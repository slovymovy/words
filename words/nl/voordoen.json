                    "sense_id": "a907a8d1-bd39-3ffe-a261-f1500d9b9d71",
                    "sense_definition": "Als voorbeeld laten zien hoe men iets moet doen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar zal de moeilijke oefening eerst <w>voordoen</w>.",
                            "target_lang_translations": {
                                "en": "The teacher will first <w>demonstrate</w> the difficult exercise.",
                                "ru": "Учитель сначала <w>покажет</w> трудное упражнение."
                            }
                        },
                        {
                            "text": "Zij heeft ons precies <w>voorgedaan</w> hoe het apparaat werkt.",
                            "target_lang_translations": {
                                "en": "She <w>showed</w> us exactly how the device works.",
                                "ru": "Она в точности <w>показала</w> нам, как работает устройство."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonstreren",
                        "tonen"
                    ],
                    "antonyms": [
                        "nadoen"
                    ],
                    "common_phrases": [
                        "iets voordoen",
                        "het even voordoen"
                    ],
                    "target_lang_definitions": {
                        "en": "To show how to do something as an example.",
                        "ru": "Показать на примере, как нужно что-то делать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "show how a task is performed"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "perform an action for others to see"

                    "sense_id": "a2413e21-434b-34bf-b31e-da6dd4c8aa3a",
                    "sense_definition": "Gebiedende wijs van <w>vossen</w>.",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Vos</w> niet zo en doe gewoon mee!",
                            "target_lang_translations": {
                                "en": "Don't <w>mess around</w> like that and just join in!",
                                "ru": "Не <w>хитри</w> так и просто участвуй!"
                            }
                        },
                        {
                            "text": "<w>Vos</w> voorzichtig als je het bos in gaat.",
                            "target_lang_translations": {
                                "en": "<w>Hunt foxes</w> carefully when you go into the forest.",
                                "ru": "<w>Охоться на лис</w> осторожно, когда идешь в лес."
                            }
                        }
                    ],

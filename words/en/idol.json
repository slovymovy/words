        "idolatrous",
        "idolater"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1286905-f128-33c1-90ac-5b4e7d677f81",
                    "sense_definition": "A famous person who is greatly admired, loved, and looked up to by many people.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "admired_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young singer became a teen <w>idol</w> after his first album was released.",
                            "target_lang_translations": {
                                "ru": "Молодой певец стал <w>кумиром</w> подростков после выхода его первого альбома."
                            }
                        },
                        {
                            "text": "She has always been my <w>idol</w> because of her talent and kindness.",
                            "target_lang_translations": {
                                "ru": "Она всегда была моим <w>кумиром</w> из-за её таланта и доброты."
                            }
                        }
                    ],
                    "synonyms": [
                        "icon",
                        "star",
                        "superstar"
                    ],
                    "common_phrases": [
                        "teen idol",
                        "pop idol",
                        "my idol",
                        "matinee idol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знаменитый человек, которым восхищаются и которого любят многие люди; кумир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кумир",
                                "target_lang_sense_clarification": "объект восхищения, любимец"
                            },
                            {
                                "target_lang_word": "идол",
                                "target_lang_sense_clarification": "объект поклонения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8f17eca-610e-38e8-920b-32be0c3d0587",
                    "sense_definition": "A statue, image, or object that is worshipped as a god.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient tribe built a golden <w>idol</w> in the center of the village.",
                            "target_lang_translations": {
                                "ru": "Древнее племя воздвигло золотого <w>идола</w> в центре деревни."
                            }
                        },
                        {
                            "text": "They were forbidden to worship any <w>idol</w> made of stone or wood.",
                            "target_lang_translations": {
                                "ru": "Им было запрещено поклоняться любому <w>идолу</w>, сделанному из камня или дерева."
                            }
                        }
                    ],
                    "common_phrases": [
                        "false idol",
                        "worship an idol",
                        "golden idol",
                        "pagan idol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Статуя, изображение или предмет, которому поклоняются как божеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идол",
                                "target_lang_sense_clarification": "языческое божество"
                            },
                            {
                                "target_lang_word": "истукан",
                                "target_lang_sense_clarification": "статуя, идол"
                            },
                            {
                                "target_lang_word": "кумир",
                                "target_lang_sense_clarification": "ложный бог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b76b8035-4985-3aa4-83fb-5aa900f64f06",
                    "sense_definition": "A misleading image, false idea, or phantom.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract_image",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent his life chasing the <w>idol</w> of fame but found no satisfaction.",
                            "target_lang_translations": {
                                "ru": "Он провёл жизнь в погоне за <w>идолом</w> славы, но не нашёл удовлетворения."
                            }
                        },
                        {
                            "text": "The philosopher argued that superstition is an <w>idol</w> of the mind.",
                            "target_lang_translations": {
                                "ru": "Философ утверждал, что суеверие — это <w>идол</w> разума."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly used in literary or philosophical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ложное представление, заблуждение или фантом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идол",
                                "target_lang_sense_clarification": "ложная идея, предрассудок"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
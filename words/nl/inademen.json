            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5669b3d-2e55-36f6-a073-5b87add210ff",
                    "sense_definition": "Lucht de longen in zuigen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "breathing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet diep <w>inademen</w> om weer rustig te worden.",
                            "target_lang_translations": {
                                "en": "You need to <w>breathe in</w> deeply to calm down again.",
                                "ru": "Тебе нужно глубоко <w>вдохнуть</w>, чтобы снова успокоиться."
                            }
                        },
                        {
                            "text": "Als je langzaam <w>inademt</w>, daalt je hartslag vanzelf.",
                            "target_lang_translations": {
                                "en": "If you <w>breathe in</w> slowly, your heart rate drops automatically.",
                                "ru": "Если ты медленно <w>вдыхаешь</w>, твой пульс замедляется сам по себе."
                            }
                        }
                    ],
                    "synonyms": [
                        "inhaleren"
                    ],
                    "antonyms": [
                        "uitademen"
                    ],
                    "common_phrases": [
                        "diep inademen",
                        "rustig inademen",
                        "door de neus inademen"
                    ],
                    "target_lang_definitions": {
                        "en": "To draw air into the lungs.",
                        "ru": "Втягивать воздух в легкие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breathe in",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "inhale",
                                "target_lang_sense_clarification": "formal or medical"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вдыхать",
                                "target_lang_sense_clarification": "процесс втягивания воздуха"
                            },
                            {
                                "target_lang_word": "вдохнуть",
                                "target_lang_sense_clarification": "сделать вдох (однократное действие)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "137bafb8-2663-3e38-9008-608c00fb90c9",
                    "sense_definition": "Iets (zoals rook, gas of geur) samen met de lucht in de longen opnemen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "breathing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brandweerman had tijdens het blussen te veel rook <w>ingeademd</w>.",
                            "target_lang_translations": {
                                "en": "The firefighter had <w>inhaled</w> too much smoke while putting out the fire.",
                                "ru": "Пожарный <w>вдохнул</w> слишком много дыма во время тушения."
                            }
                        },
                        {
                            "text": "Het is erg gevaarlijk als je giftige dampen <w>inademt</w>.",
                            "target_lang_translations": {
                                "en": "It is very dangerous if you <w>inhale</w> toxic fumes.",
                                "ru": "Очень опасно, если ты <w>вдыхаешь</w> ядовитые пары."
                            }
                        }
                    ],
                    "synonyms": [
                        "inhaleren",
                        "opsnuiven"
                    ],
                    "antonyms": [
                        "uitademen"
                    ],
                    "common_phrases": [
                        "rook inademen",
                        "gas inademen",
                        "dampen inademen"
                    ],
                    "target_lang_definitions": {
                        "en": "To take substances like smoke, gas, or scents into the lungs.",
                        "ru": "Вбирать что-либо (например, дым, газ или запах) в легкие вместе с воздухом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inhale",
                                "target_lang_sense_clarification": "smoke, gas, or fumes"
                            },
                            {
                                "target_lang_word": "breathe in",
                                "target_lang_sense_clarification": "to take into lungs"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вдыхать",
                                "target_lang_sense_clarification": "поглощать вместе с воздухом"
                            },
                            {
                                "target_lang_word": "надышаться",
                                "target_lang_sense_clarification": "вдохнуть слишком много чего-либо вредного"
                            }
                        ]
                    }
                }
            ]
        }
    ]

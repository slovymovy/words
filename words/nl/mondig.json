    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5cdb0389-fca2-3359-85a1-d2a301af518d",
                    "sense_definition": "In staat om voor zichzelf op te komen en duidelijk zijn mening te uiten.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar wist niet goed raad met de <w>mondige</w> leerlingen in zijn klas.",
                            "target_lang_translations": {
                                "en": "The teacher didn't quite know how to handle the <w>assertive</w> students in his class.",
                                "ru": "Учитель не знал, как справиться с <w>бойкими</w> учениками в своем классе."
                            }
                        },
                        {
                            "text": "Tegenwoordig zijn patiënten veel <w>mondiger</w> en stellen ze kritische vragen aan de dokter.",
                            "target_lang_translations": {
                                "en": "Nowadays, patients are much more <w>vocal</w> and ask the doctor critical questions.",
                                "ru": "В наши дни пациенты стали гораздо <w>увереннее в себе</w> и задают врачу критические вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "assertief"
                    ],
                    "antonyms": [
                        "onmondig"
                    ],
                    "common_phrases": [
                        "mondige burgers",
                        "een mondige consument",
                        "mondige kinderen"
                    ],
                    "target_lang_definitions": {
                        "en": "Able to stand up for oneself and clearly express one's opinion.",
                        "ru": "Способный постоять за себя и ясно выражать свое мнение"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assertive",
                                "target_lang_sense_clarification": "confident and self-assured"
                            },
                            {
                                "target_lang_word": "vocal",
                                "target_lang_sense_clarification": "expressing opinions freely"
                            },
                            {
                                "target_lang_word": "outspoken",
                                "target_lang_sense_clarification": "frank in stating opinions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уверенный в себе",
                                "target_lang_sense_clarification": "ассертивный, не боящийся высказываться"
                            },
                            {
                                "target_lang_word": "умеющий постоять за себя",
                                "target_lang_sense_clarification": "способный защитить свои интересы"
                            },
                            {
                                "target_lang_word": "бойкий",
                                "target_lang_sense_clarification": "активный и смелый в общении (часто о детях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03b4abbd-a5d4-3f4e-9abb-25c214339c7d",
                    "sense_definition": "Meerderjarig en wettelijk bevoegd om zelfstandig beslissingen te nemen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de wet is hij nu <w>mondig</w> en mag hij zijn eigen contracten tekenen.",
                            "target_lang_translations": {
                                "en": "According to the law, he is now <w>of age</w> and may sign his own contracts.",
                                "ru": "По закону он теперь <w>совершеннолетний</w> и может подписывать собственные контракты."
                            }
                        },
                        {
                            "text": "Vroeger werd men pas op een latere leeftijd <w>mondig</w> verklaard dan nu.",
                            "target_lang_translations": {
                                "en": "In the past, people were declared <w>of age</w> at a later age than today.",
                                "ru": "Раньше человека признавали <w>дееспособным</w> в более позднем возрасте, чем сейчас."
                            }
                        }
                    ],
                    "antonyms": [
                        "onmondig"
                    ],
                    "common_phrases": [
                        "mondig worden",
                        "mondig verklaard"
                    ],
                    "target_lang_definitions": {
                        "en": "Of legal age and legally competent to make independent decisions.",
                        "ru": "Совершеннолетний и юридически правомочный принимать самостоятельные решения"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of age",
                                "target_lang_sense_clarification": "having reached adulthood"
                            },
                            {
                                "target_lang_word": "legally competent",
                                "target_lang_sense_clarification": "having legal capacity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "совершеннолетний",

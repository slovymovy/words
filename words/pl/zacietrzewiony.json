    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9082f9c7-bd46-3fc4-99e8-eba5a04bd2f4",
                    "sense_definition": "Gwałtownie i zawzięcie broniący swoich racji, często w gniewie, nie zważając na argumenty innych ani na konsekwencje.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest tak <w>zacietrzewiony</w> w swoich poglądach, że nie słucha żadnych racjonalnych argumentów.",
                            "target_lang_translations": {
                                "ru": "Мой сосед настолько <w>ожесточён</w> в своих взглядах, что не слушает никаких разумных доводов."
                            }
                        },
                        {
                            "text": "Ja nie jestem jednak <w>zacietrzewiony</w> i nie jestem pamiętliwy, więc możemy spokojnie porozmawiać.",
                            "target_lang_translations": {
                                "ru": "Я, однако, не <w>ожесточён</w> и не злопамятен, так что мы можем спокойно поговорить."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaperzony"
                    ],
                    "antonyms": [
                        "opanowany",
                        "spokojny"
                    ],
                    "common_phrases": [
                        "zacietrzewiony dyskutant",
                        "zacietrzewiony wrogiem",
                        "zacietrzewiony w sporze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Яростно и упрямо отстаивающий свою правоту, часто в гневе, не обращая внимания на аргументы других и последствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожесточённый",
                                "target_lang_sense_clarification": "о человеке: озлобленный, упорствующий в конфликте"
                            },
                            {
                                "target_lang_word": "ярый",
                                "target_lang_sense_clarification": "страстный, фанатичный в своих убеждениях"
                            },
                            {
                                "target_lang_word": "запальчивый",
                                "target_lang_sense_clarification": "легко приходящий в гнев, горячий в споре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79ebfb39-335f-359c-925d-dbfcd6aa0fff",
                    "sense_definition": "Wyrażający silne emocje, gniew lub upór; świadczący o byciu zacietrzewionym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>zacietrzewiony</w> ton głosu zaniepokoił wszystkich zebranych przy stole.",
                            "target_lang_translations": {
                                "ru": "Его <w>яростный</w> тон голоса обеспокоил всех собравшихся за столом."
                            }
                        },
                        {
                            "text": "Spojrzał na mnie w sposób <w>zacietrzewiony</w>, co sugerowało koniec merytorycznej dyskusji.",
                            "target_lang_translations": {
                                "ru": "Он посмотрел на меня <w>ожесточённым</w> взглядом, что предвещало конец конструктивной дискуссии."
                            }
                        }
                    ],
                    "antonyms": [
                        "łagodny"
                    ],
                    "common_phrases": [
                        "zacietrzewiony ton",
                        "zacietrzewiona mina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий сильные эмоции, гнев или упрямство; свидетельствующий о возбуждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яростный",
                                "target_lang_sense_clarification": "выражающий сильный гнев"
                            },
                            {
                                "target_lang_word": "ожесточённый",
                                "target_lang_sense_clarification": "резкий, непримиримый (о тоне, взгляде)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4c49f833-56db-3add-8505-b1df3ebcf519",
                    "sense_definition": "Imiesłów przymiotnikowy bierny od czasownika <w>zacietrzewić</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tłum, <w>zacietrzewiony</w> przez agresywnego mówcę, ruszył w stronę ratusza.",
                            "target_lang_translations": {
                                "ru": "Толпа, <w>раззадоренная</w> агрессивным оратором, двинулась в сторону ратуши."
                            }
                        },
                        {
                            "text": "Został <w>zacietrzewiony</w> ciągłymi atakami prasy i postanowił odpowiedzieć pozwem.",
                            "target_lang_translations": {
                                "ru": "Он был <w>ожесточён</w> постоянными нападками прессы и решил ответить иском."
                            }
                        }
                    ],
                    "traits": [
                        {

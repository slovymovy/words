    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "5b8e21a5-af96-37ff-b93d-f496f2d94433",
                    "sense_definition": "Welk aantal of welke hoeveelheid van iets; gebruikt voor een zelfstandig naamwoord.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hoeveel</w> boeken heb je deze maand gelezen?",
                            "target_lang_translations": {
                                "en": "<w>How many</w> books have you read this month?",
                                "ru": "<w>Сколько</w> книг ты прочитал в этом месяце?"
                            }
                        },
                        {
                            "text": "Ik vraag me af <w>hoeveel</w> tijd we nog hebben voor de trein vertrekt.",
                            "target_lang_translations": {
                                "en": "I wonder <w>how much</w> time we have left before the train leaves.",
                                "ru": "Интересно, <w>сколько</w> времени у нас еще есть до отправления поезда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hoeveel kost het",
                        "hoeveel jaar oud",
                        "hoeveel uur",
                        "hoeveel mensen"
                    ],
                    "target_lang_definitions": {
                        "en": "What number or quantity of something; used before a noun.",
                        "ru": "Какое количество или число чего-либо; используется в сочетании с существительным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "how many",
                                "target_lang_sense_clarification": "used with countable nouns"
                            },
                            {
                                "target_lang_word": "how much",
                                "target_lang_sense_clarification": "used with uncountable nouns"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сколько",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "5ec2aa05-2f79-3323-8f0f-209baac585f9",
                    "sense_definition": "Welk aantal; vraagt naar een specifiek nummer of een exacte telling.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hoeveel</w> is vijf plus zeven?",
                            "target_lang_translations": {
                                "en": "<w>How much</w> is five plus seven?",
                                "ru": "<w>Сколько</w> будет пять плюс семь?"
                            }
                        },
                        {
                            "text": "Op <w>hoeveel</w> schat je de totale kosten?",
                            "target_lang_translations": {
                                "en": "At <w>how much</w> do you estimate the total costs?",
                                "ru": "Во <w>сколько</w> ты оцениваешь общие расходы?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "What number; asks for a specific number or exact count.",
                        "ru": "Какое число; вопрос о конкретном значении или точном счете."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "how much",
                                "target_lang_sense_clarification": "asking for a value or calculation"
                            },
                            {
                                "target_lang_word": "how many",
                                "target_lang_sense_clarification": "asking for a specific count"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сколько",
                                "target_lang_sense_clarification": "числовое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4a8c78c-6720-3eae-80d0-8c671a81f7ac",
                    "sense_definition": "In welke mate of gradatie; duidt op de intensiteit of omvang van iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vond het verbijsterend om te horen <w>hoeveel</w> indruk het verhaal op hem had gemaakt.",
                            "target_lang_translations": {
                                "en": "I found it staggering to hear <w>how much</w> of an impression the story had made on him.",
                                "ru": "Меня поразило, когда я услышал, <w>насколько</w> сильное впечатление произвела на него эта история."
                            }
                        },
                        {
                            "text": "Ze weten net als ik <w>hoeveel</w> er van deze beslissing afhangt.",
                            "target_lang_translations": {
                                "en": "They know just like I do <w>how much</w> depends on this decision.",
                                "ru": "Они, как и я, знают, <w>сколько</w> зависит от этого решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoezeer"
                    ],
                    "common_phrases": [
                        "niet weten hoeveel"
                    ],
                    "target_lang_definitions": {
                        "en": "To what degree or extent; indicates the intensity or scope of something.",
                        "ru": "В какой степени или мере; указывает на интенсивность или масштаб чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "how much",
                                "target_lang_sense_clarification": "degree or extent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "насколько",
                                "target_lang_sense_clarification": "степень"
                            },
                            {
                                "target_lang_word": "как сильно",
                                "target_lang_sense_clarification": "интенсивность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "50e1241e-0264-39fc-8219-814a05fcaabb",
                    "sense_definition": "Welk aantal of welke hoeveelheid; zelfstandig gebruikt zonder direct volgend zelfstandig naamwoord.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er waren veel gasten op het feest, maar ik weet niet precies <w>hoeveel</w>.",
                            "target_lang_translations": {
                                "en": "There were many guests at the party, but I don't know exactly <w>how many</w>.",
                                "ru": "На вечеринке было много гостей, но я не знаю точно <w>сколько</w>."
                            }
                        },
                        {
                            "text": "<w>Hoeveel</w> heb je er nodig voor het recept?",
                            "target_lang_translations": {
                                "en": "<w>How many</w> do you need for the recipe?",
                                "ru": "<w>Сколько</w> тебе их нужно для рецепта?"
                            }

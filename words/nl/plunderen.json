                {
                    "sense_id": "f09eb31e-c287-31e6-9e4d-fc68338ec3ae",
                    "sense_definition": "Met geweld en op grote schaal spullen wegnemen, vaak tijdens een oorlog of rel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten <w>plunderden</w> het dorp na de overwinning.",
                            "target_lang_translations": {
                                "en": "The soldiers <w>plundered</w> the village after the victory.",
                                "ru": "Солдаты <w>разграбили</w> деревню после победы."
                            }
                        },
                        {
                            "text": "Tijdens de rellen werden veel winkels in het centrum <w>geplunderd</w>.",
                            "target_lang_translations": {
                                "en": "During the riots, many shops in the city center were <w>looted</w>.",
                                "ru": "Во время беспорядков многие магазины в центре были <w>разграблены</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "leegroven",
                        "bestelen",
                        "roven"
                    ],
                    "antonyms": [
                        "beschermen",
                        "bewaken"
                    ],
                    "common_phrases": [
                        "een stad plunderen",
                        "winkels plunderen"
                    ],
                    "target_lang_definitions": {
                        "en": "To take goods by force on a large scale, typically during a war or riot.",
                        "ru": "Силой отнимать имущество в больших масштабах, обычно во время войны или беспорядков."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plunder",
                                "target_lang_sense_clarification": "general term for taking goods by force, often in war"
                            },
                            {
                                "target_lang_word": "loot",
                                "target_lang_sense_clarification": "often used for stealing from shops during riots"
                            },
                            {
                                "target_lang_word": "pillage",
                                "target_lang_sense_clarification": "literary or historical term, implies destruction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грабить",
                                "target_lang_sense_clarification": "общий термин для отъема имущества силой"
                            },
                            {
                                "target_lang_word": "разграблять",
                                "target_lang_sense_clarification": "обычно в отношении места, например, города или магазина"
                            },
                            {
                                "target_lang_word": "мародёрствовать",
                                "target_lang_sense_clarification": "грабить во время войны или катастрофы"
                            }
                        ]

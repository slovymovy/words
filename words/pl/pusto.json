                },
                {
                    "sense_id": "40ddfca0-273d-368c-b12b-9b679e21db24",
                    "sense_definition": "W sposób świadczący o braku myśli, refleksji, emocji lub głębszego sensu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_emptiness",
                    "name_type": "no",
                    "examples": [
                        {

                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>expected</w> storm never arrived, leaving the day surprisingly calm.",
                            "target_lang_translations": {
                                "ru": "<w>Ожидаемый</w> шторм так и не пришел, и день выдался удивительно спокойным."
                            }

                            },
                            {
                                "target_lang_word": "Элиша",
                                "target_lang_sense_clarification": "транслитерация английского имени"
                            },
                            {
                                "target_lang_word": "Элайша",
                                "target_lang_sense_clarification": "вариант транслитерации"
                            }
                        ]

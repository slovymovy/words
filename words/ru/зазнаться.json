    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53296549-16c7-3ae6-b179-5cffbaf490e9",
                    "sense_definition": "Стать высокомерным, переоценить свои достоинства и начать пренебрежительно относиться к другим.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrogance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После победы в конкурсе он совсем <w>зазнался</w> и перестал здороваться с соседями.",
                            "target_lang_translations": {
                                "en": "After winning the competition, he completely <w>got a big head</w> and stopped greeting his neighbors."
                            }
                        },
                        {
                            "text": "Не стоит <w>зазнаваться</w>, даже если ты достиг большого успеха в работе.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>get stuck-up</w>, even if you have achieved great success at work."
                            }
                        }
                    ],
                    "synonyms": [
                        "возгордиться"
                    ],
                    "common_phrases": [
                        "совсем зазнался",
                        "не надо зазнаваться",
                        "зазнаться от успеха"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в разговорной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become arrogant or conceited; to start thinking too highly of oneself and treating others with disdain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get a big head",
                                "target_lang_sense_clarification": "idiomatic, implies becoming arrogant due to success"
                            },
                            {
                                "target_lang_word": "become conceited",
                                "target_lang_sense_clarification": "general term for developing excessive pride"
                            },
                            {
                                "target_lang_word": "get stuck-up",
                                "target_lang_sense_clarification": "informal, acting superior to others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f533d960-f8a8-3f13-8c0c-f3fa0efd8570",
                    "sense_definition": "Познакомиться, завести знакомство с кем-либо.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "acquaintance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой человек мечтал <w>зазнаться</w> с известным писателем.",
                            "target_lang_translations": {
                                "en": "The young man dreamed of <w>making the acquaintance</w> of the famous writer."
                            }
                        },
                        {
                            "text": "Ему удалось <w>зазнаться</w> с влиятельными людьми в городе.",
                            "target_lang_translations": {
                                "en": "He managed to <w>get acquainted</w> with influential people in the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "возгордиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "указано в источнике"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "устаревшее значение, встречающееся в классической литературе"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To get acquainted or establish a connection with someone (archaic or regional usage)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get acquainted",
                                "target_lang_sense_clarification": "to meet and become familiar with someone"
                            },
                            {
                                "target_lang_word": "make someone's acquaintance",
                                "target_lang_sense_clarification": "formal phrase for meeting someone"
                            }
                        ]
                    }
                }
            ]
        }
    ]

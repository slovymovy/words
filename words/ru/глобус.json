    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52ce65e0-b542-3781-b00f-f6d6265a3cdf",
                    "sense_definition": "Трёхмерная модель Земли или другой планеты, обычно используемая в образовательных целях.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geography_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель географии поставил на стол большой <w>глобус</w>.",
                            "target_lang_translations": {
                                "en": "The geography teacher put a large <w>globe</w> on the table."
                            }
                        },
                        {
                            "text": "Мы долго крутили <w>глобус</w>, пытаясь найти нашу страну.",
                            "target_lang_translations": {
                                "en": "We spun the <w>globe</w> for a long time, trying to find our country."
                            }
                        }
                    ],
                    "common_phrases": [
                        "школьный глобус",
                        "крутить глобус",
                        "показать на глобусе",
                        "глобус звездного неба"
                    ],
                    "target_lang_definitions": {
                        "en": "A three-dimensional model of the Earth or another planet, usually used for educational purposes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "globe",
                                "target_lang_sense_clarification": "model of Earth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c00bbe9c-9bbd-3e94-b2fe-96303652cf75",
                    "sense_definition": "Планета Земля, весь мир (обычно в разговорной речи).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "world_planet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта удивительная новость быстро облетела весь <w>глобус</w>.",
                            "target_lang_translations": {
                                "en": "This amazing news quickly flew around the entire <w>globe</w>."
                            }
                        },
                        {
                            "text": "Путешественник мечтал посетить каждый уголок нашего <w>глобуса</w>.",
                            "target_lang_translations": {
                                "en": "The traveler dreamed of visiting every corner of our <w>globe</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Земля",
                        "мир",
                        "планета",
                        "земной шар"
                    ],
                    "common_phrases": [
                        "весь глобус",
                        "другая сторона глобуса"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "используется в значении «весь мир» или «планета»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Planet Earth, the whole world (usually in colloquial speech)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "globe",
                                "target_lang_sense_clarification": "the planet Earth"
                            },
                            {
                                "target_lang_word": "world",
                                "target_lang_sense_clarification": "synonym for planet"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d98bf53d-7d73-38f7-8d6b-1e0d463c254f",
                    "sense_definition": "Шар или сфера как геометрическое тело.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geometry_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старинных математических трудах эту фигуру иногда называли <w>глобусом</w>.",
                            "target_lang_translations": {
                                "en": "In ancient mathematical works, this figure was sometimes called a <w>globe</w>."
                            }
                        },
                        {
                            "text": "Учёный начертил идеальный <w>глобус</w> на листе пергамента.",
                            "target_lang_translations": {
                                "en": "The scientist drew a perfect <w>globe</w> on a sheet of parchment."
                            }
                        }
                    ],
                    "synonyms": [
                        "шар",
                        "сфера"
                    ],
                    "traits": [
                        {

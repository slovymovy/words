    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a95d4d16-c057-3c12-ae23-5219f11670dc",
                    "sense_definition": "A person who walks around or performs other actions while asleep.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "literal_sleep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sleepwalker</w> managed to unlock the front door without waking up."
                        },
                        {
                            "text": "My brother is a chronic <w>sleepwalker</w>, so we have to hide the keys at night."
                        }
                    ],
                    "synonyms": [
                        "somnambulist"
                    ],
                    "common_phrases": [
                        "chronic sleepwalker"
                    ]
                },
                {
                    "sense_id": "be58134b-2d74-3e17-a8b3-09124233f447",
                    "sense_definition": "A person who acts without awareness or pays no attention to the reality around them.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_oblivious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went through his daily routine like a <w>sleepwalker</w>, never noticing the changes in the office."
                        },
                        {
                            "text": "The citizens were political <w>sleepwalkers</w> who ignored the approaching crisis until it was too late."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "figurative",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "416312c2-c08f-3d51-bed8-9b6ed57da8ef",
                    "sense_definition": "A person in a hypnotic trance or state of induced mesmeric sleep.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hypnotic_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The magician commanded the <w>sleepwalker</w> to raise his hand during the show."
                        },
                        {
                            "text": "She sat in the chair with a blank stare, acting like a mesmerized <w>sleepwalker</w>."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "referring to mesmerism"
                        }
                    ]
                },
                {
                    "sense_id": "84ff39bd-77ed-33a4-b2ca-f052419e4403",
                    "sense_definition": "A person controlled by a sorcerer in voodoo folklore; a zombie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "folklore_zombie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Local legends tell of a <w>sleepwalker</w> who works in the fields all night under a spell."
                        },
                        {
                            "text": "The villagers feared the sorcerer who claimed he could turn anyone into a <w>sleepwalker</w>."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "specialized",
                            "comment": "folklore"
                        }
                    ]
                }
            ]

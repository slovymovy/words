    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5739ac94-f462-3bec-9fca-fc7cf635ef26",
                    "sense_definition": "Mężczyzna mieszkający w Atenach (stolicy Grecji) lub pochodzący z tego miasta.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "inhabitants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój znajomy <ateńczyk> pokazał mi najciekawsze zabytki miasta.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый <w>афинянин</w> показал мне самые интересные достопримечательности города."
                            }
                        },
                        {
                            "text": "Jako rodowity <ateńczyk>, znał każdą uliczkę w swojej dzielnicy.",
                            "target_lang_translations": {
                                "ru": "Как коренной <w>афинянин</w>, он знал каждую улочку в своём районе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodowity ateńczyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, живущий в Афинах (столице Греции) или родом из этого города."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "афинянин",
                                "target_lang_sense_clarification": "житель города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5adf146b-3109-36c4-91ff-547b5986c3b2",
                    "sense_definition": "Obywatel starożytnego greckiego miasta-państwa Ateny.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "inhabitants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy wolny <ateńczyk> miał prawo głosu na zgromadzeniu ludowym.",
                            "target_lang_translations": {
                                "ru": "Каждый свободный <w>афинянин</w> имел право голоса на народном собрании."
                            }
                        },
                        {
                            "text": "Starożytni <ateńczycy> cenili sztukę, filozofię i retorykę.",
                            "target_lang_translations": {
                                "ru": "Древние <w>афиняне</w> ценили искусство, философию и риторику."
                            }
                        }
                    ],
                    "antonyms": [
                        "spartanin"
                    ],
                    "common_phrases": [
                        "starożytny ateńczyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гражданин древнегреческого города-государства Афины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "афинянин",
                                "target_lang_sense_clarification": "житель Древних Афин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "834547d8-196b-3dcd-9e20-2b99c28b9340",
                    "sense_definition": "Внимательно смотреть на кого-либо или что-либо, следить глазами за действиями или изменениями.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы любили <наблюдать> за жизнью диких животных в парке.",
                            "target_lang_translations": {
                                "en": "The tourists loved to <watch> the life of wild animals in the park."
                            }
                        },
                        {
                            "text": "Мы будем <наблюдать> за звездами всю ночь, если небо будет ясным.",
                            "target_lang_translations": {
                                "en": "We will <watch> the stars all night if the sky is clear."
                            }
                        }
                    ],
                    "synonyms": [
                        "следить"
                    ],
                    "common_phrases": [
                        "наблюдать за игрой",
                        "наблюдать за звездами",
                        "наблюдать со стороны",
                        "наблюдать за природой"
                    ],
                    "target_lang_definitions": {
                        "en": "To look carefully at someone or something, to follow actions or changes with one's eyes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "look at attentively"
                            },
                            {
                                "target_lang_word": "observe",
                                "target_lang_sense_clarification": "view carefully"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7421312c-610c-3287-b929-b176d79752d6",
                    "sense_definition": "Изучать или исследовать что-либо, внимательно следя за ходом процесса или явления.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Астрономы готовятся <наблюдать> редкое солнечное затмение.",
                            "target_lang_translations": {
                                "en": "Astronomers are preparing to <observe> a rare solar eclipse."
                            }
                        },
                        {
                            "text": "Ученые продолжают <наблюдать> изменения климата в этом регионе.",
                            "target_lang_translations": {
                                "en": "Scientists continue to <monitor> climate changes in this region."
                            }
                        }
                    ],
                    "synonyms": [
                        "изучать",
                        "исследовать",
                        "отслеживать"
                    ],
                    "common_phrases": [
                        "наблюдать затмение",
                        "наблюдать поведение",
                        "наблюдать изменения",
                        "наблюдать процесс"
                    ],
                    "target_lang_definitions": {
                        "en": "To study or investigate something, carefully following the course of a process or phenomenon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "observe",
                                "target_lang_sense_clarification": "study scientifically"
                            },
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "track progress or changes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa3adb1e-a61d-3fa4-ae64-8003b4a12028",
                    "sense_definition": "Видеть или замечать появление чего-либо, становиться свидетелем какого-либо явления.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В последнее время мы можем <наблюдать> значительный рост цен на продукты.",
                            "target_lang_translations": {
                                "en": "Recently we can <observe> a significant rise in food prices."
                            }
                        },
                        {
                            "text": "Жители города могли <наблюдать> странную картину в вечернем небе.",
                            "target_lang_translations": {
                                "en": "The city residents could <observe> a strange scene in the evening sky."
                            }
                        }
                    ],
                    "synonyms": [
                        "встречать"
                    ],
                    "common_phrases": [

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0770dd59-6159-37ba-b996-028868e1e6b6",
                    "sense_definition": "Gałęzie drzew iglastych (np. świerku lub jodły) używane do celów dekoracyjnych lub do osłaniania roślin ogrodowych przed mrozem",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogrodnik przyniósł świeży <stroisz>, aby zabezpieczyć różaneczniki przed nadchodzącą zimą.",
                            "target_lang_translations": {
                                "ru": "Садовник принес свежий <лапник>, чтобы укрыть рододендроны от предстоящей зимы."
                            }
                        },
                        {
                            "text": "Na grobach bliskich często układa się jodłowy <stroisz> jako dekorację.",
                            "target_lang_translations": {
                                "ru": "На могилы близких часто кладут пихтовый <лапник> в качестве украшения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stroisz świerkowy",
                        "okrywać stroiszem",
                        "stroisz jodłowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ветки хвойных деревьев (ели или пихты), используемые для декора или укрытия садовых растений от мороза"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лапник",
                                "target_lang_sense_clarification": "срубленные ветки хвойных деревьев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e868a450-8248-304e-b852-96f3ab2c2ccd",
                    "sense_definition": "Długa rączka lub uchwyt narzędzia, w którym osadzona jest jego część robocza",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas pracy w polu złamał się drewniany <stroisz> od grabi.",
                            "target_lang_translations": {
                                "ru": "Во время работы в поле сломался деревянный <черенок> граблей."
                            }
                        },
                        {
                            "text": "Gospodarz musiał dorobić nowy <stroisz>, aby móc dalej używać wideł.",
                            "target_lang_translations": {
                                "ru": "Хозяину пришлось сделать новый <черенок>, чтобы дальше пользоваться вилами."
                            }
                        }
                    ],
                    "synonyms": [
                        "trzonek",
                        "stylisko",
                        "dzierżak"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная рукоятка или черенок инструмента, в котором закреплена его рабочая часть"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черенок",
                                "target_lang_sense_clarification": "рукоятка сельскохозяйственного орудия"
                            },
                            {
                                "target_lang_word": "рукоятка",
                                "target_lang_sense_clarification": "часть инструмента для держания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0442de99-48a7-3e8d-98b0-45806a9cae8d",
                    "sense_definition": "Tyczka lub żerdź wbita w ziemię, służąca do podpierania rosnących roślin",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wbił w ziemię wysoki <stroisz>, by fasola miała się po czym piąć.",
                            "target_lang_translations": {
                                "ru": "Он вбил в землю высокую <жердь>, чтобы фасоли было по чему виться."
                            }
                        },
                        {
                            "text": "Każdy krzak pomidora w ogrodzie był przywiązany do osobnego <stroiszu>.",
                            "target_lang_translations": {
                                "ru": "Каждый куст помидоров в саду был привязан к отдельному <колышку>."

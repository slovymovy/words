                {
                    "sense_id": "0fb68265-1c76-3e47-a13a-80405d79f21d",
                    "sense_definition": "De wereld van planten, dieren, bergen en zeeën zoals die oorspronkelijk is en niet door mensen is gemaakt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het weekend wandelen wij graag in de <w>natuur</w>.",
                            "target_lang_translations": {
                                "en": "On weekends, we like to walk in <w>nature</w>.",
                                "ru": "В выходные мы любим гулять на <w>природе</w>."
                            }
                        },
                        {
                            "text": "Na jaren in de stad had hij behoefte aan meer <w>natuur</w> om zich heen.",
                            "target_lang_translations": {
                                "en": "After years in the city, he felt the need for more <w>nature</w> around him.",
                                "ru": "После многих лет в городе он испытывал потребность в большем количестве <w>природы</w> вокруг себя."
                            }
                        }
                    ],
                    "antonyms": [
                        "cultuur"
                    ],
                    "common_phrases": [
                        "in de natuur",
                        "de vrije natuur",
                        "terug naar de natuur",
                        "moeder natuur"
                    ],
                    "target_lang_definitions": {
                        "en": "The world of plants, animals, mountains, and seas as it originally is and not created by humans.",
                        "ru": "Мир растений, животных, гор и морей в его естественном состоянии, не созданный человеком."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nature",
                                "target_lang_sense_clarification": "physical world"
                            },
                            {
                                "target_lang_word": "the outdoors",
                                "target_lang_sense_clarification": "open air"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "природа",
                                "target_lang_sense_clarification": "окружающий мир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90284a40-8de0-3971-9988-8be56580c174",
                    "sense_definition": "De aangeboren eigenschappen of het karakter van een persoon, dier of ding.",

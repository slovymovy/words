    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "572daef0-ccee-3b53-bf8d-b006d163df5f",
                    "sense_definition": "Een doek van textiel die water opneemt, gebruikt om het lichaam of de handen mee af te drogen na het wassen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "drying_cloth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik droog mijn handen af met een schone <w>handdoek</w>.",
                            "target_lang_translations": {
                                "en": "I dry my hands with a clean <w>towel</w>.",
                                "ru": "Я вытираю руки чистым <w>полотенцем</w>."
                            }
                        },
                        {
                            "text": "Na het douchen hang ik mijn natte <w>handdoek</w> over de verwarming.",
                            "target_lang_translations": {
                                "en": "After showering, I hang my wet <w>towel</w> over the heater.",
                                "ru": "После душа я вешаю мокрое <w>полотенце</w> на батарею."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een schone handdoek",
                        "natte handdoek",
                        "handdoek ophangen"
                    ],
                    "target_lang_definitions": {
                        "en": "A cloth made of textile that absorbs water, used to dry the body or hands after washing.",
                        "ru": "Ткань, впитывающая воду, используемая для вытирания тела или рук после мытья."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "towel",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "полотенце",
                                "target_lang_sense_clarification": "для тела или рук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "869334fe-c0d9-3e5f-8a1d-36662912bb3d",
                    "sense_definition": "Een doek die specifiek wordt gebruikt om gewassen serviesgoed en bestek mee af te drogen.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "drying_cloth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij pakte een <w>handdoek</w> uit de lade om het kopje af te drogen.",
                            "target_lang_translations": {
                                "en": "He took a <w>dish towel</w> from the drawer to dry the cup.",
                                "ru": "Он достал <w>полотенце</w> из ящика, чтобы вытереть чашку."
                            }
                        },
                        {
                            "text": "Is er nog een droge <w>handdoek</w> voor de rest van de afwas?",
                            "target_lang_translations": {
                                "en": "Is there still a dry <w>dish towel</w> for the rest of the dishes?",
                                "ru": "Есть ли еще сухое <w>полотенце</w> для остальной посуды?"
                            }
                        }
                    ],
                    "synonyms": [
                        "theedoek"
                    ],
                    "common_phrases": [
                        "de vaat afdrogen"
                    ],
                    "target_lang_definitions": {
                        "en": "A cloth specifically used to dry washed dishes and cutlery.",
                        "ru": "Ткань, используемая специально для вытирания вымытой посуды и столовых приборов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dish towel",
                                "target_lang_sense_clarification": "for drying dishes"
                            },
                            {
                                "target_lang_word": "tea towel",
                                "target_lang_sense_clarification": "UK/Commonwealth term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кухонное полотенце",
                                "target_lang_sense_clarification": "для посуды"
                            },
                            {
                                "target_lang_word": "полотенце",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
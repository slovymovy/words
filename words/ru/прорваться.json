    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1298d28f-8bb4-3740-a1af-a800aa11e9ca",
                    "sense_definition": "С силой преодолеть препятствие, пройти сквозь заслон, окружение или толпу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдатам удалось <прорваться> из окружения и соединиться с основными силами.",
                            "target_lang_translations": {
                                "en": "The soldiers managed to <break out> of the encirclement and join the main forces."
                            }
                        },
                        {
                            "text": "Журналисты пытались <прорваться> к знаменитости через плотное кольцо охраны.",
                            "target_lang_translations": {
                                "en": "The journalists tried to <break through> to the celebrity through the tight security ring."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробиться"
                    ],
                    "antonyms": [
                        "отступить"
                    ],
                    "common_phrases": [
                        "прорваться через толпу",
                        "прорваться с боем",
                        "прорваться через кордон"
                    ],
                    "target_lang_definitions": {
                        "en": "To force one's way through an obstacle, barrier, crowd, or enemy lines."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break through",
                                "target_lang_sense_clarification": "to pass a barrier or crowd"
                            },
                            {
                                "target_lang_word": "break out",
                                "target_lang_sense_clarification": "to escape from encirclement"
                            },
                            {
                                "target_lang_word": "burst through",
                                "target_lang_sense_clarification": "to enter or pass suddenly and forcefully"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc7082c2-507e-3657-bfec-0324e672c954",
                    "sense_definition": "Лопнуть, разрушиться под напором чего-либо (о плотине, трубе, нарыве).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старая дамба не выдержала напора воды и <прорвалась>.",
                            "target_lang_translations": {
                                "en": "The old dam could not withstand the water pressure and <burst>."
                            }
                        },
                        {
                            "text": "У пациента наконец <прорвался> гнойник, и температура спала.",
                            "target_lang_translations": {
                                "en": "The patient's abscess finally <burst>, and the fever went down."
                            }
                        }
                    ],
                    "synonyms": [
                        "лопнуть"
                    ],
                    "common_phrases": [
                        "нарыв прорвался",
                        "плотина прорвалась"
                    ],
                    "target_lang_definitions": {
                        "en": "To rupture or collapse under pressure (referring to a dam, pipe, or abscess)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burst",
                                "target_lang_sense_clarification": "to break open suddenly"
                            },
                            {
                                "target_lang_word": "rupture",
                                "target_lang_sense_clarification": "medical or technical context"
                            },
                            {
                                "target_lang_word": "give way",
                                "target_lang_sense_clarification": "to collapse under load"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3673f98-24e1-3608-b776-6bb23ba13906",
                    "sense_definition": "Порваться насквозь, стать дырявым (о ткани, бумаге, одежде).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пакет с продуктами был слишком тяжёлым и <прорвался> по дороге домой.",
                            "target_lang_translations": {
                                "en": "The grocery bag was too heavy and <tore> on the way home."
                            }
                        },
                        {
                            "text": "Ткань на локтях пиджака со временем <прорвалась>.",
                            "target_lang_translations": {
                                "en": "The fabric on the jacket's elbows <tore> over time."
                            }
                        }
                    ],
                    "synonyms": [
                        "порваться",
                        "продырявиться"
                    ],
                    "common_phrases": [
                        "карман прорвался",
                        "мешок прорвался"
                    ],
                    "target_lang_definitions": {
                        "en": "To tear completely or develop a hole (referring to fabric, paper, or containers)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "general term for ripping material"
                            },
                            {
                                "target_lang_word": "rip",
                                "target_lang_sense_clarification": "to tear quickly or roughly"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "of a bag or container"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
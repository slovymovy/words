            "senses": [
                {
                    "sense_id": "32aad03a-cc69-3353-9609-6b2e678e1648",
                    "sense_definition": "Zdrobnienie od imienia żeńskiego <w>Jadwiga</w> lub <w>Agnieszka</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "name_diminutive",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja ciocia <w>Jaga</w> piecze najlepsze ciasta w całej rodzinie.",
                            "target_lang_translations": {
                                "ru": "Моя тетя <w>Яга</w> печет лучшие пироги во всей семье."
                            }
                        },
                        {
                            "text": "Cześć <w>Jaga</w>, czy idziesz z nami dzisiaj do kina?",
                            "target_lang_translations": {
                                "ru": "Привет, <w>Яга</w>, ты идешь с нами сегодня в кино?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciocia Jaga",
                        "babcia Jaga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женского имени Ядвига или Агнешка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Яга",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4eb401fd-5f52-3528-9625-b256ed9ab2e4",
                    "sense_definition": "Potoczne, często obraźliwe określenie na złą, kłótliwą lub starą kobietę; skrót od postaci Baby Jagi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pejorative_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie słuchaj tej starej <w>jagi</w>, ona zawsze na wszystko narzeka.",
                            "target_lang_translations": {
                                "ru": "Не слушай эту старую <w>каргу</w>, она вечно на всё жалуется."
                            }
                        },
                        {
                            "text": "Zachowuje się jak wredna <w>jaga</w> i nikt w biurze jej nie lubi.",
                            "target_lang_translations": {
                                "ru": "Она ведет себя как вредная <w>ведьма</w>, и никто в офисе её не любит."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiedźma",
                        "jędza",
                        "czarownica",
                        "pudło"
                    ],
                    "antonyms": [
                        "anioł"
                    ],
                    "common_phrases": [
                        "stara jaga",
                        "wredna jaga"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто оскорбительное название злой, сварливой или старой женщины; сокращение от Бабы Яги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карга",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "992ff7ee-9300-35b9-9ee8-ca19f649dd55",
                    "sense_definition": "Twardy, sprężysty i napięty, zachowujący swój kształt pod naciskiem (zwykle o ciele, skórze lub świeżych owocach i warzywach).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularne ćwiczenia sprawiają, że skóra staje się bardziej <w>jędrna</w> i elastyczna.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения делают кожу более <w>упругой</w> и эластичной."
                            }
                        },
                        {
                            "text": "Na targu wybrałam tylko świeże, <w>jędrne</w> jabłka bez żadnych obić.",
                            "target_lang_translations": {
                                "ru": "На рынке я выбрала только свежие, <w>крепкие</w> яблоки без вмятин."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprężysty",
                        "twardy",
                        "zbity",
                        "napięty"
                    ],
                    "antonyms": [
                        "zwiotczały",
                        "wiotki",
                        "obwisły",
                        "miękki",
                        "sflaczały"
                    ],
                    "common_phrases": [
                        "jędrna skóra",
                        "jędrne owoce",
                        "jędrne ciało",
                        "jędrne piersi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый, упругий и подтянутый, сохраняющий форму при нажатии (обычно о теле, коже или свежих фруктах и овощах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упругий",
                                "target_lang_sense_clarification": "о коже, теле, мышцах"
                            },
                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "о фруктах, овощах"
                            },
                            {
                                "target_lang_word": "плотный",
                                "target_lang_sense_clarification": "о консистенции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d599416-8d19-3c47-9938-fa4d07fb0aae",
                    "sense_definition": "Pełen wyrazu, dosadny, a zarazem zwięzły i treściwy (o stylu wypowiedzi, języku lub dowcipie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "style_and_language",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pisarz słynie z <w>jędrnego</w> stylu, który doskonale oddaje realia epoki.",
                            "target_lang_translations": {
                                "ru": "Писатель славится <w>сочным</w> стилем, который прекрасно передает реалии эпохи."
                            }
                        },
                        {
                            "text": "Jego <w>jędrny</w> dowcip rozbawił całe towarzystwo do łez.",
                            "target_lang_translations": {
                                "ru": "Его <w>сочная</w> шутка рассмешила всю компанию до слез."
                            }
                        }
                    ],
                    "synonyms": [
                        "dosadny",
                        "soczysty",
                        "wyrazisty",
                        "treściwy",
                        "zwięzły"
                    ],
                    "antonyms": [
                        "bezbarwny",
                        "mdły",
                        "rozwlekły"
                    ],
                    "common_phrases": [
                        "jędrny styl",
                        "jędrny język",
                        "jędrna polszczyzna",
                        "jędrny dowcip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выразительный, меткий, и в то же время краткий и содержательный (о стиле речи, языке или шутке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочный",
                                "target_lang_sense_clarification": "о языке, стиле"
                            },
                            {
                                "target_lang_word": "меткий",
                                "target_lang_sense_clarification": "о слове, выражении"
                            },
                            {
                                "target_lang_word": "ядрёный",
                                "target_lang_sense_clarification": "разговорное; о крепком словце или шутке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "target_lang_definitions": {
                        "ru": "Выдаваться вперед за пределы поверхности или края."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступать",
                                "target_lang_sense_clarification": "выдаваться вперед"
                            },
                            {
                                "target_lang_word": "выдаваться",
                                "target_lang_sense_clarification": "торчать наружу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dcf6f93-b939-38f1-ae85-8d7587570063",
                    "sense_definition": "To present oneself or convey a particular impression to others.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tries to <w>project</w> an image of calm authority.",
                            "target_lang_translations": {
                                "ru": "Она пытается <w>создать</w> образ спокойной властности."
                            }
                        },
                        {
                            "text": "You need to <w>project</w> confidence during the job interview.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>излучать</w> уверенность во время собеседования."
                            }
                        }
                    ],
                    "synonyms": [
                        "convey",
                        "present"
                    ],
                    "common_phrases": [
                        "project confidence",
                        "project an image"
                    ],
                    "target_lang_definitions": {

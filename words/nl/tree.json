{
    "word_family": [
        "trede",
        "treden"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "626b759f-ea12-385a-b642-966b78e359e5",
                    "sense_definition": "Een plat vlak of opstapje dat onderdeel is van een trap of ladder.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze struikelde bijna over de loszittende <w>tree</w> van de oude trap.",
                            "target_lang_translations": {
                                "en": "She almost tripped over the loose <w>step</w> of the old staircase.",
                                "ru": "Она чуть не споткнулась о расшатанную <w>ступеньку</w> старой лестницы."
                            }
                        },
                        {
                            "text": "Nog maar een paar <w>treetjes</w> en we zijn boven op de zolder.",
                            "target_lang_translations": {
                                "en": "Just a few more <w>steps</w> and we are up in the attic.",
                                "ru": "Ещё всего пара <w>ступенек</w>, и мы будем наверху, на чердаке."
                            }
                        }
                    ],
                    "synonyms": [
                        "trede"
                    ],
                    "common_phrases": [
                        "de onderste tree",
                        "een tree lager"
                    ],
                    "target_lang_definitions": {
                        "en": "A flat surface or step that is part of a staircase or ladder.",
                        "ru": "Плоская поверхность или выступ, являющийся частью лестницы или стремянки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "step",
                                "target_lang_sense_clarification": "part of a staircase"
                            },
                            {
                                "target_lang_word": "stair",
                                "target_lang_sense_clarification": "single step in a staircase"
                            },
                            {
                                "target_lang_word": "rung",
                                "target_lang_sense_clarification": "crossbar of a ladder"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ступень",
                                "target_lang_sense_clarification": "часть лестницы"
                            },
                            {
                                "target_lang_word": "ступенька",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d138f48-b94d-31c9-80fc-058a51168913",
                    "sense_definition": "De afstand die wordt afgelegd bij het zetten van één stap.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een grote <w>tree</w> sprong hij over de smalle sloot heen.",
                            "target_lang_translations": {
                                "en": "With a big <w>stride</w> he jumped over the narrow ditch.",
                                "ru": "Одним большим <w>шагом</w> он перепрыгнул через узкую канаву."
                            }
                        },
                        {
                            "text": "Hij liep met rasse <w>tree</w> door de drukke winkelstraat.",
                            "target_lang_translations": {
                                "en": "He walked through the busy shopping street at a brisk <w>pace</w>.",
                                "ru": "Он шёл быстрым <w>шагом</w> по оживлённой торговой улице."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The distance covered when taking a single step.",
                        "ru": "Расстояние, преодолеваемое при совершении одного шага."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stride",
                                "target_lang_sense_clarification": "long step"
                            },
                            {
                                "target_lang_word": "pace",
                                "target_lang_sense_clarification": "speed or manner of walking"
                            },
                            {
                                "target_lang_word": "step",
                                "target_lang_sense_clarification": "distance of a foot movement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "движение ногой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0136503a-3bd8-317d-8c75-599503a274d6",
                    "sense_definition": "Een oude lengtemaat die ongeveer overeenkomt met de afstand van een grote stap.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schat zou begraven liggen op twintig <w>tree</w> van de grote eik.",
                            "target_lang_translations": {
                                "en": "The treasure was said to be buried twenty <w>paces</w> from the large oak.",
                                "ru": "Говорят, сокровище зарыто в двадцати <w>шагах</w> от большого дуба."
                            }
                        },
                        {
                            "text": "Hij mat de lengte van het veld af in elke <w>tree</w> die hij zette.",
                            "target_lang_translations": {
                                "en": "He measured the length of the field in every <w>pace</w> he took.",
                                "ru": "Он отмерял длину поля каждым <w>шагом</w>, который делал."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historische maat"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An old unit of length roughly corresponding to the distance of a large step.",
                        "ru": "Старинная мера длины, примерно соответствующая расстоянию большого шага."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pace",
                                "target_lang_sense_clarification": "unit of length"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "мера длины"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "0bd26156-bd65-346d-853b-9619e7e11299",
                    "sense_definition": "een laag die iets bedekt of beschermt",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de winter ligt er vaak een wit <w>dek</w> van sneeuw op de daken.",

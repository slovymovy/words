            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d19d083a-6cb4-3e4f-ba1f-e221649ce73a",
                    "sense_definition": "Mieszkanka miasta.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "resident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako rodowita <w>mieszczanka</w> nie wyobrażała sobie życia na głuchej prowincji.",
                            "target_lang_translations": {
                                "ru": "Как коренная <w>горожанка</w>, она не представляла себе жизни в глухой провинции."
                            }
                        },
                        {
                            "text": "Każda <w>mieszczanka</w> docenia bliskość sklepów i komunikacji miejskiej.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>горожанка</w> ценит близость магазинов и общественного транспорта."
                            }
                        }
                    ],
                    "synonyms": [
                        "miastowa"
                    ],
                    "antonyms": [
                        "wieśniaczka"
                    ],
                    "common_phrases": [
                        "rodowita mieszczanka",
                        "typowa mieszczanka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жительница города."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горожанка",
                                "target_lang_sense_clarification": "жительница города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2796dfc-2b12-3a21-aaff-306b042f4302",
                    "sense_definition": "Kobieta należąca do stanu mieszczańskiego (historycznej grupy społecznej).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_class",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamożna <w>mieszczanka</w> zamówiła u krawca suknię z najdroższego jedwabiu.",
                            "target_lang_translations": {
                                "ru": "Богатая <w>мещанка</w> заказала у портного платье из самого дорогого шёлка."
                            }
                        },
                        {
                            "text": "W dawnych wiekach <w>mieszczanka</w> nie miała takich samych praw jak szlachcianka.",
                            "target_lang_translations": {
                                "ru": "В прежние века <w>мещанка</w> не имела таких же прав, как дворянка."
                            }
                        }
                    ],
                    "antonyms": [
                        "szlachcianka",
                        "chłopka"
                    ],
                    "common_phrases": [
                        "bogata mieszczanka",
                        "strój mieszczanki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, принадлежащая к мещанскому сословию (историческая социальная группа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мещанка",
                                "target_lang_sense_clarification": "сословие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc11e777-e9a8-3a33-9b8a-a95dd6c83113",
                    "sense_definition": "Kobieta o konserwatywnych, zachowawczych poglądach, lubiąca ustabilizowane i wygodne życie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mentality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej ciotka to straszna <w>mieszczanka</w>, która dba tylko o pozory i opinię sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Её тётка — ужасная <w>мещанка</w>, которая заботится только о внешних приличиях и мнении соседей."
                            }
                        },
                        {
                            "text": "Nie bądź taką <w>mieszczanką</w> i zrób wreszcie coś spontanicznego.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <w>мещанкой</w> и сделай наконец что-нибудь спонтанное."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drobna mieszczanka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина с консервативными, мелкобуржуазными взглядами, ценящая стабильность и комфорт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мещанка",
                                "target_lang_sense_clarification": "образ мыслей"
                            },
                            {
                                "target_lang_word": "обывательница",
                                "target_lang_sense_clarification": "узкий кругозор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "designer",
        "redesign"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3c8f873-9efa-3109-b636-8bb113e08519",
                    "sense_definition": "A detailed plan or drawing produced to show the look and function of a building, object, or system before it is made.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation_planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The initial <w>design</w> of the park was rejected for being too expensive.",
                            "target_lang_translations": {
                                "ru": "Первоначальный <w>проект</w> парка был отклонен из-за слишком высокой стоимости."
                            }
                        },
                        {
                            "text": "The architect showed us the preliminary <w>designs</w> for the new library.",
                            "target_lang_translations": {
                                "ru": "Архитектор показал нам предварительные <w>проекты</w> новой библиотеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "blueprint",
                        "plan",
                        "diagram"
                    ],
                    "common_phrases": [
                        "architectural design",
                        "urban design",
                        "design flaw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детальный план или чертёж, показывающий внешний вид и функции здания, объекта или системы до их создания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проект",
                                "target_lang_sense_clarification": "план создания сооружения или устройства"
                            },
                            {
                                "target_lang_word": "чертёж",
                                "target_lang_sense_clarification": "графическое изображение, схема"
                            },
                            {
                                "target_lang_word": "конструкция",
                                "target_lang_sense_clarification": "устройство, строение чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b73f22dc-37b1-343c-b6a7-d9ef9719b025",
                    "sense_definition": "The art or process of deciding how something will look and work.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation_planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Danish furniture <w>design</w> is world-famous for its simplicity and functionality.",
                            "target_lang_translations": {
                                "ru": "Датский <w>дизайн</w> мебели всемирно известен своей простотой и функциональностью."
                            }
                        },
                        {
                            "text": "She is studying graphic <w>design</w> at the university.",
                            "target_lang_translations": {
                                "ru": "Она изучает графический <w>дизайн</w> в университете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "graphic design",
                        "interior design",
                        "product design",
                        "web design"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусство или процесс проектирования внешнего вида и функциональности объектов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дизайн",
                                "target_lang_sense_clarification": "художественное конструирование"
                            },
                            {
                                "target_lang_word": "проектирование",
                                "target_lang_sense_clarification": "процесс создания проекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b62b8fc9-b1a2-38a0-bcc1-43a542ee0945",
                    "sense_definition": "A decorative pattern or arrangement of lines, shapes, and colors.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_pattern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are working on some new <w>designs</w> for our range of summer shirts.",
                            "target_lang_translations": {
                                "ru": "Мы работаем над новыми <w>узорами</w> для нашей линейки летних рубашек."
                            }
                        },
                        {
                            "text": "The curtains feature a beautiful floral <w>design</w> that brightens the room.",
                            "target_lang_translations": {
                                "ru": "На шторах красивый цветочный <w>узор</w>, который оживляет комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "proposal",
                        "scheme"
                    ],
                    "common_phrases": [
                        "floral design",
                        "geometric design"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный узор или расположение линий, форм и цветов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узор",
                                "target_lang_sense_clarification": "орнамент, рисунок на поверхности"
                            },
                            {
                                "target_lang_word": "рисунок",
                                "target_lang_sense_clarification": "изображение"
                            },
                            {
                                "target_lang_word": "оформление",
                                "target_lang_sense_clarification": "внешний вид, декорирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d6e7fa3-f113-31c1-a38f-afb51517bbaf",
                    "sense_definition": "A secret or specific intention, plan, or plot behind an action.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "purpose_intention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have <w>designs</w> on winning the league next season.",
                            "target_lang_translations": {
                                "ru": "У нас есть <w>намерения</w> выиграть лигу в следующем сезоне."
                            }
                        },
                        {
                            "text": "I suspect he left the door unlocked by <w>design</w>, not by accident.",
                            "target_lang_translations": {
                                "ru": "Я подозреваю, что он оставил дверь незапертой с <w>умыслом</w>, а не случайно."
                            }
                        }
                    ],
                    "synonyms": [
                        "plan",
                        "plot",
                        "scheme",
                        "project"
                    ],
                    "common_phrases": [
                        "by design",
                        "have designs on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайное или конкретное намерение, план или замысел, стоящий за действием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замысел",
                                "target_lang_sense_clarification": "задуманный план действий"
                            },
                            {
                                "target_lang_word": "намерение",
                                "target_lang_sense_clarification": "желание, стремление сделать что-либо"
                            },
                            {
                                "target_lang_word": "умысел",
                                "target_lang_sense_clarification": "преднамеренность, часто скрытая или негативная"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ffc2944a-3760-3507-b509-661afd8c6dde",
                    "sense_definition": "To plan and make a drawing of how something will look and work before it is made.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "creation_planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>designs</w> gaming laptops for a major technology company.",
                            "target_lang_translations": {
                                "ru": "Он <w>разрабатывает</w> игровые ноутбуки для крупной технологической компании."
                            }
                        },
                        {
                            "text": "The engineer <w>designed</w> a bridge that could withstand strong earthquakes.",
                            "target_lang_translations": {
                                "ru": "Инженер <w>спроектировал</w> мост, который мог выдержать сильные землетрясения."
                            }
                        }
                    ],
                    "synonyms": [
                        "plan"
                    ],
                    "common_phrases": [
                        "design a building",
                        "design a website",
                        "custom designed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Планировать и создавать чертежи того, как что-то будет выглядеть и работать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проектировать",
                                "target_lang_sense_clarification": "создавать проект здания или системы"
                            },
                            {
                                "target_lang_word": "разрабатывать",
                                "target_lang_sense_clarification": "создавать что-то новое, прорабатывать детали"
                            },
                            {
                                "target_lang_word": "конструировать",
                                "target_lang_sense_clarification": "создавать конструкцию механизма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "082c62a9-f16b-3723-b659-997267e00bd1",
                    "sense_definition": "To plan or intend something for a specific purpose.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "purpose_intention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This course is <w>designed</w> to help beginners learn the basics of coding.",
                            "target_lang_translations": {
                                "ru": "Этот курс <w>разработан</w>, чтобы помочь новичкам изучить основы программирования."
                            }
                        },
                        {
                            "text": "The safety features were <w>designed</w> to protect workers from injury.",
                            "target_lang_translations": {
                                "ru": "Функции безопасности были <w>разработаны</w> для защиты рабочих от травм."
                            }
                        }
                    ],
                    "synonyms": [
                        "plan"
                    ],
                    "common_phrases": [
                        "designed to",
                        "designed for",
                        "specifically designed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначать или планировать что-либо для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предназначать",
                                "target_lang_sense_clarification": "определять для какой-либо цели"
                            },
                            {
                                "target_lang_word": "разрабатывать",
                                "target_lang_sense_clarification": "создавать с определенной целью"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
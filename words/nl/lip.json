                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пить или есть жидкую пищу, втягивая её губами и издавая характерный звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прихлёбывать",
                                "target_lang_sense_clarification": "пить со звуком"

{
    "word_family": [
        "transparent",
        "transparently",
        "transparence"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6db57233-1d85-3647-907d-9ebe5451a155",
                    "sense_definition": "The quality of being transparent, allowing light to pass through so that objects behind can be distinctly seen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water in the mountain lake has incredible <w>transparency</w>.",
                            "target_lang_translations": {
                                "ru": "Вода в горном озере обладает невероятной <w>прозрачностью</w>."
                            }
                        },
                        {
                            "text": "Glass is often chosen for windows because of its high <w>transparency</w>.",
                            "target_lang_translations": {
                                "ru": "Стекло часто выбирают для окон из-за его высокой <w>прозрачности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "transparence",
                        "transparentness"
                    ],
                    "antonyms": [
                        "opacity"
                    ],
                    "common_phrases": [
                        "degree of transparency",
                        "high transparency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть прозрачным, пропускать свет, позволяя отчетливо видеть предметы позади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "физическое свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0ee219e-d1db-38cb-b88c-3dd5fd8c81e9",
                    "sense_definition": "The quality of being open, honest, and accessible to scrutiny, especially in business or government.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens demanded greater <w>transparency</w> from their local government.",
                            "target_lang_translations": {
                                "ru": "Граждане требовали большей <w>прозрачности</w> от местных властей."
                            }
                        },
                        {
                            "text": "There was a complete lack of <w>transparency</w> regarding how the funds were spent.",
                            "target_lang_translations": {
                                "ru": "Наблюдалось полное отсутствие <w>прозрачности</w> в вопросе расходования средств."
                            }
                        }
                    ],
                    "antonyms": [
                        "secrecy",
                        "opacity"
                    ],
                    "common_phrases": [
                        "lack of transparency",
                        "financial transparency",
                        "full transparency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество открытости, честности и доступности для контроля, особенно в бизнесе или государственном управлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "открытость информации"
                            },
                            {
                                "target_lang_word": "транспарентность",
                                "target_lang_sense_clarification": "книжное, официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b81a0f2-85ea-396c-89ce-23b1815b6b02",
                    "sense_definition": "A thin sheet of translucent material with an image or text, used with an overhead projector.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher placed a <w>transparency</w> on the projector to show the diagram.",
                            "target_lang_translations": {
                                "ru": "Учитель положил <w>плёнку</w> на проектор, чтобы показать диаграмму."
                            }
                        },
                        {
                            "text": "He printed the chart onto a <w>transparency</w> for his presentation.",
                            "target_lang_translations": {
                                "ru": "Он распечатал график на <w>плёнке</w> для своей презентации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "overhead transparency"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий лист прозрачного материала с изображением или текстом, используемый с оверхед-проектором (кодоскопом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плёнка",
                                "target_lang_sense_clarification": "для проектора"
                            },
                            {
                                "target_lang_word": "слайд",
                                "target_lang_sense_clarification": "изображение для проекции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54d923a8-ab5a-33dd-aaaa-cf2c4e6edc9e",
                    "sense_definition": "The property of a system or process functioning without the user being aware of its complexity or presence.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Network <w>transparency</w> allows users to access remote files as if they were local.",
                            "target_lang_translations": {
                                "ru": "Сетевая <w>прозрачность</w> позволяет пользователям получать доступ к удаленным файлам так, как если бы они были локальными."
                            }
                        },
                        {
                            "text": "The goal of the update is total <w>transparency</w> of the background operations.",
                            "target_lang_translations": {
                                "ru": "Цель обновления — полная <w>прозрачность</w> фоновых операций."
                            }
                        }
                    ],
                    "common_phrases": [
                        "network transparency",
                        "data transparency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство системы или процесса функционировать без осознания пользователем их сложности или наличия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "в информатике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0604e6bd-f99b-34fb-82d7-36babb0bfd9d",
                    "sense_definition": "A picture or artwork painted on translucent material, intended to be viewed with light shining through it.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displayed a rare 19th-century <w>transparency</w> illuminated from behind.",
                            "target_lang_translations": {
                                "ru": "В музее был выставлен редкий <w>транспарант</w> XIX века, подсвеченный сзади."
                            }
                        },
                        {
                            "text": "Candles were placed behind the <w>transparency</w> to reveal the painted scene.",
                            "target_lang_translations": {
                                "ru": "Свечи помещали за <w>транспарантом</w>, чтобы проявить нарисованную сцену."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "art form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Картина или произведение искусства на полупрозрачном материале, предназначенные для просмотра на просвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транспарант",
                                "target_lang_sense_clarification": "устаревшее, вид искусства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07464f16-3c20-3937-9eab-113a56eb3bf5",
                    "sense_definition": "An object or entity that is transparent.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ghost was described as a floating <w>transparency</w> in the hallway.",
                            "target_lang_translations": {
                                "ru": "Призрак описывался как парящая <w>прозрачность</w> в коридоре."
                            }
                        },
                        {
                            "text": "In the dim light, the thin fabric appeared as a mere <w>transparency</w>.",
                            "target_lang_translations": {
                                "ru": "В тусклом свете тонкая ткань казалась лишь <w>прозрачностью</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объект или сущность, которые прозрачны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "поэтическое, субстантивация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
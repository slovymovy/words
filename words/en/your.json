                {
                    "sense_id": "03d65c1d-6553-319d-a73a-ee9971e72ebc",
                    "sense_definition": "Belonging to or associated with the person or people being addressed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please write down <w>your</w> name on this form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите <w>ваше</w> имя в этом бланке."
                            }
                        },
                        {
                            "text": "You should try to solve the problem on <w>your</w> own.",
                            "target_lang_translations": {
                                "ru": "Тебе следует попытаться решить проблему <w>своими</w> силами."
                            }
                        },
                        {
                            "text": "Is this <w>your</w> car parked outside?",
                            "target_lang_translations": {
                                "ru": "Это <w>твоя</w> машина припаркована снаружи?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "your name",
                        "on your own",
                        "your turn",
                        "your welcome"
                    ],
                    "target_lang_definitions": {

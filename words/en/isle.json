            "senses": [
                {
                    "sense_id": "08f2b8c0-6d26-334d-98e5-193eb5d89222",
                    "sense_definition": "An island, usually very small.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We took a small boat to the rocky <w>isle</w> in the bay.",
                            "target_lang_translations": {
                                "ru": "Мы отправились на маленькой лодке к скалистому <w>островку</w> в бухте."
                            }
                        },
                        {
                            "text": "Many tourists travel to the Emerald <w>Isle</w> to enjoy its beautiful scenery.",
                            "target_lang_translations": {
                                "ru": "Многие туристы едут на Изумрудный <w>остров</w>, чтобы насладиться его прекрасными пейзажами."
                            }
                        }
                    ],
                    "synonyms": [
                        "island",
                        "islet"
                    ],
                    "antonyms": [
                        "mainland"
                    ],
                    "common_phrases": [
                        "British Isles",
                        "Emerald Isle",
                        "Isle of Man",
                        "Isle of Wight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остров, обычно небольшой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "общий термин, часто поэтический"
                            },
                            {
                                "target_lang_word": "островок",
                                "target_lang_sense_clarification": "маленький остров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db2aeac9-0f4d-3f81-90ae-8d91458f9e56",
                    "sense_definition": "Obsolete spelling of <w>aisle</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old text, the bride walked down the <w>isle</w> of the church.",
                            "target_lang_translations": {
                                "ru": "В старом тексте невеста шла по <w>проходу</w> церкви."
                            }
                        },
                        {
                            "text": "He stood in the narrow <w>isle</w> between the rows of seats.",
                            "target_lang_translations": {
                                "ru": "Он стоял в узком <w>проходе</w> между рядами сидений."
                            }
                        }
                    ],
                    "synonyms": [
                        "aisle"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее написание слова aisle (проход)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проход",
                                "target_lang_sense_clarification": "между рядами"
                            }
                        ]
                    }
                }
            ]
        },
        {

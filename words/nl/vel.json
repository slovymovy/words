{
    "word_family": [
        "ontvellen",
        "vervellen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "717c2a78-7198-3154-bee7-ec0bf24bd061",
                    "sense_definition": "De buitenste laag weefsel die het lichaam van een mens of dier bedekt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze zit de laatste tijd niet zo lekker in haar <w>vel</w>.",
                            "target_lang_translations": {
                                "en": "She hasn't been feeling very comfortable in her own <w>skin</w> lately.",
                                "ru": "В последнее время она чувствует себя не в своей <w>тарелке</w>."
                            }
                        },
                        {
                            "text": "Door de felle zon is mijn <w>vel</w> een beetje verbrand.",
                            "target_lang_translations": {
                                "en": "My <w>skin</w> is a bit burnt because of the bright sun.",
                                "ru": "Из-за яркого солнца моя <w>кожа</w> немного обгорела."
                            }
                        }
                    ],
                    "synonyms": [
                        "huid"
                    ],
                    "common_phrases": [
                        "lekker in je vel",
                        "uit je vel springen",
                        "vel over been"
                    ],
                    "target_lang_definitions": {
                        "en": "The outer layer of tissue that covers the body of a human or animal.",
                        "ru": "Внешний слой ткани, покрывающий тело человека или животного."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "skin",
                                "target_lang_sense_clarification": "outer covering of the body"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кожа",
                                "target_lang_sense_clarification": "наружный покров тела"
                            },
                            {
                                "target_lang_word": "шкура",
                                "target_lang_sense_clarification": "разг. кожа человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40c263d5-9f36-3ca5-823c-ef49292e4f55",
                    "sense_definition": "Een dun, rechthoekig stuk materiaal, meestal papier, om op te schrijven of te tekenen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik een <w>vel</w> papier van je lenen voor mijn aantekeningen?",
                            "target_lang_translations": {
                                "en": "Can I borrow a <w>sheet</w> of paper from you for my notes?",
                                "ru": "Можно одолжить у тебя <w>лист</w> бумаги для моих заметок?"
                            }
                        },
                        {
                            "text": "Schrijf je naam duidelijk bovenaan het witte <w>vel</w>.",
                            "target_lang_translations": {
                                "en": "Write your name clearly at the top of the white <w>sheet</w>.",
                                "ru": "Напиши свое имя четко в верхней части белого <w>листа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blad"
                    ],
                    "common_phrases": [
                        "vel papier",
                        "wit vel",
                        "schoon vel"
                    ],
                    "target_lang_definitions": {
                        "en": "A thin, rectangular piece of material, usually paper, used for writing or drawing.",
                        "ru": "Тонкий прямоугольный кусок материала, обычно бумаги, для письма или рисования."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sheet",
                                "target_lang_sense_clarification": "piece of paper"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лист",
                                "target_lang_sense_clarification": "кусок бумаги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d8aaf18-7448-349b-8344-1c684b398245",
                    "sense_definition": "Een dun vliesje dat ontstaat op de oppervlakte van warme vloeistoffen, zoals melk of soep, als deze afkoelen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "food_texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vind warme melk vies als er een <w>vel</w> op drijft.",
                            "target_lang_translations": {
                                "en": "I find warm milk disgusting if there is a <w>skin</w> floating on it.",
                                "ru": "Мне противно теплое молоко, если в нем плавает <w>пенка</w>."
                            }
                        },
                        {
                            "text": "Je moet goed blijven roeren om <w>vellen</w> in de saus te voorkomen.",
                            "target_lang_translations": {
                                "en": "You have to keep stirring well to prevent <w>skins</w> in the sauce.",
                                "ru": "Нужно постоянно помешивать, чтобы в соусе не образовалась <w>плёнка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vel op de melk"
                    ],
                    "target_lang_definitions": {
                        "en": "A thin film that forms on the surface of warm liquids, such as milk or soup, as they cool down.",
                        "ru": "Тонкая плёнка, образующаяся на поверхности теплых жидкостей, таких как молоко или суп, при их остывании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "skin",
                                "target_lang_sense_clarification": "layer on liquid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пенка",
                                "target_lang_sense_clarification": "на молоке или кипяченой жидкости"
                            },
                            {
                                "target_lang_word": "плёнка",
                                "target_lang_sense_clarification": "тонкий слой на поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72877165-2419-310b-bcf2-f8b0254aa40c",
                    "sense_definition": "De afgestroopte huid van een dier, vaak nog met de haren of wol eraan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager verkocht het <w>vel</w> van de beer op de markt.",
                            "target_lang_translations": {
                                "en": "The hunter sold the bear's <w>pelt</w> at the market.",
                                "ru": "Охотник продал медвежью <w>шкуру</w> на рынке."
                            }
                        },
                        {
                            "text": "Vroeger gebruikten mensen <w>vellen</w> van dieren om kleding te maken.",
                            "target_lang_translations": {
                                "en": "In the past, people used animal <w>skins</w> to make clothing.",
                                "ru": "Раньше люди использовали звериные <w>шкуры</w> для изготовления одежды."
                            }
                        }
                    ],
                    "synonyms": [
                        "pels"
                    ],
                    "target_lang_definitions": {
                        "en": "The stripped skin of an animal, often still with the hair or wool attached.",
                        "ru": "Снятая с животного кожа, часто с шерстью или мехом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pelt",
                                "target_lang_sense_clarification": "animal skin with fur"
                            },
                            {
                                "target_lang_word": "skin",
                                "target_lang_sense_clarification": "animal hide"
                            },
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "animal skin used for leather"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шкура",
                                "target_lang_sense_clarification": "снятая кожа животного с шерстью"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e4afe625-54e8-3949-b9a6-15b7d5f02d9d",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>vellen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vel</w> geen oordeel over mensen die ik niet ken.",
                            "target_lang_translations": {
                                "en": "I <w>pass</w> no judgment on people I don't know.",
                                "ru": "Я не <w>выношу</w> суждений о людях, которых не знаю."
                            }
                        },
                        {
                            "text": "<w>Vel</w> die zieke boom voordat hij op het huis valt.",
                            "target_lang_translations": {
                                "en": "<w>Fell</w> that sick tree before it falls on the house.",
                                "ru": "<w>Свали</w> это больное дерево, пока оно не упало на дом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'vellen' (to fell, to pass judgment).",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'vellen' (валить, выносить решение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "to pronounce a judgment"
                            },
                            {
                                "target_lang_word": "fell",
                                "target_lang_sense_clarification": "to cut down a tree"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "о приговоре или решении"
                            },
                            {
                                "target_lang_word": "валить",
                                "target_lang_sense_clarification": "рубить деревья"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
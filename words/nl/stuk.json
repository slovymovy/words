                {
                    "sense_id": "102b3650-9e26-3278-acb8-da6f16145f01",
                    "sense_definition": "Een deel, gedeelte of brokstuk van een groter geheel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "part_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je ook een <w>stuk</w> van de taart?",
                            "target_lang_translations": {
                                "ru": "Хочешь тоже <w>кусок</w> торта?"
                            }
                        },
                        {
                            "text": "De dure vaas viel in duizend <w>stukken</w> op de grond.",
                            "target_lang_translations": {
                                "ru": "Дорогая ваза упала на пол и разлетелась на тысячу <w>осколков</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "deel",
                        "brokstuk",
                        "fragment",
                        "scherf"
                    ],
                    "antonyms": [
                        "geheel"
                    ],
                    "common_phrases": [
                        "in stukken",
                        "aan stukken",
                        "een stuk taart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть, доля или обломок чего-то большего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусок",
                                "target_lang_sense_clarification": "часть целого"
                            },
                            {
                                "target_lang_word": "осколок",
                                "target_lang_sense_clarification": "разбитая часть"
                            }
                        ]
                    }
                },
                {

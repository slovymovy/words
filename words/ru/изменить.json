            "pos": "verb",
            "senses": [
                {
                    "sense_id": "871ae066-689a-3293-9b5b-054c81145736",
                    "sense_definition": "Сделать иным, поменять вид, форму или качество кого-либо или чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_alter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <изменить> планы и поехать на море завтра.",
                            "target_lang_translations": {
                                "en": "We decided to <change> our plans and go to the sea tomorrow."
                            }
                        },
                        {
                            "text": "Новая причёска помогла ей полностью <изменить> внешность.",
                            "target_lang_translations": {
                                "en": "The new hairstyle helped her completely <change> her appearance."
                            }
                        }
                    ],
                    "synonyms": [
                        "переиначить",
                        "модифицировать",
                        "переиначить",
                        "предать"
                    ],
                    "antonyms": [
                        "сохранить",
                        "оставить"
                    ],
                    "common_phrases": [
                        "изменить мир",
                        "изменить жизнь",
                        "изменить решение",
                        "изменить внешность",
                        "изменить отношение"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone or something different; to change the appearance, form, or quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "to make or become different"
                            },
                            {
                                "target_lang_word": "alter",
                                "target_lang_sense_clarification": "to change a characteristic, often slightly"
                            },
                            {
                                "target_lang_word": "modify",
                                "target_lang_sense_clarification": "to make partial or minor changes to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "367d3687-5b3a-3fa0-91cd-85ab125c2580",
                    "sense_definition": "Нарушить верность кому-либо или чему-либо (супругу, другу, родине, своим принципам).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "betrayal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он пообещал никогда не <изменить> своим принципам ради денег.",
                            "target_lang_translations": {
                                "en": "He promised never to <betray> his principles for the sake of money."
                            }
                        },
                        {
                            "text": "Герой романа не смог <изменить> родине и остался верным присяге.",
                            "target_lang_translations": {
                                "en": "The novel's hero could not <betray> his homeland and remained faithful to his oath."
                            }
                        }
                    ],
                    "synonyms": [
                        "предать"
                    ],
                    "antonyms": [
                        "остаться верным"
                    ],
                    "common_phrases": [
                        "изменить мужу",
                        "изменить жене",
                        "изменить родине",
                        "изменить себе",
                        "изменить слову"
                    ],
                    "target_lang_definitions": {
                        "en": "To break faith or loyalty to someone or something (a spouse, friend, homeland, or principles)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "betray",
                                "target_lang_sense_clarification": "to be disloyal to a country, principle, or person"
                            },
                            {
                                "target_lang_word": "cheat on",
                                "target_lang_sense_clarification": "to be unfaithful to a romantic partner"
                            },
                            {
                                "target_lang_word": "be unfaithful to",
                                "target_lang_sense_clarification": "formal term for breaking a commitment or vow"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70f98af1-221c-3fd5-b874-81c7bdade4b1",
                    "sense_definition": "Перестать действовать или служить (о памяти, чувствах, интуиции, выдержке).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fail_faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В самый ответственный момент выдержка <изменила> опытному спортсмену.",
                            "target_lang_translations": {
                                "en": "At the most crucial moment, the experienced athlete's composure <failed> him."
                            }
                        },
                        {
                            "text": "Если мне не <изменяет> память, мы уже встречались в прошлом году.",
                            "target_lang_translations": {
                                "en": "If my memory does not <fail> me, we already met last year."
                            }
                        }
                    ],
                    "synonyms": [

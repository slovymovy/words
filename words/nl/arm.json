                {
                    "sense_id": "53a952c9-c4a1-30b9-816a-ddbc2ecd3368",
                    "sense_definition": "Deel van een stoel of bank waar je je arm op kunt leggen.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zat ontspannen op de <w>arm</w> van de bank te praten.",
                            "target_lang_translations": {

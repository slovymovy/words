{
    "word_family": [
        "копить",
        "накопить",
        "накопление",
        "скопление",
        "накопитель",
        "копилка",
        "накопительный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5faf854-182e-3f50-81b4-1b1ea8a06c4d",
                    "sense_definition": "Постепенно образоваться, набраться в большом количестве (о вещах, веществах, делах или деньгах).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accumulation_things",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За время моего отпуска на столе <скопилось> много бумаг.",
                            "target_lang_translations": {
                                "en": "A lot of papers <piled up> on the desk during my vacation."
                            }
                        },
                        {
                            "text": "В углу комнаты <скопилась> пыль, которую нужно убрать.",
                            "target_lang_translations": {
                                "en": "Dust <accumulated> in the corner of the room, which needs to be cleaned."
                            }
                        }
                    ],
                    "synonyms": [
                        "накопиться"
                    ],
                    "antonyms": [
                        "растратиться"
                    ],
                    "common_phrases": [
                        "скопилось много дел",
                        "скопилась пыль",
                        "скопились долги",
                        "жир скопился"
                    ],
                    "target_lang_definitions": {
                        "en": "To gradually form or gather in large quantities (about things, substances, tasks, or money)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accumulate",
                                "target_lang_sense_clarification": "to gradually increase in quantity or number"
                            },
                            {
                                "target_lang_word": "pile up",
                                "target_lang_sense_clarification": "to form a heap or backlog (often used for work, papers, or debts)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8be39d5a-c60c-32c2-bb6e-e147a3acca62",
                    "sense_definition": "Сойтись и собраться в одном месте в большом количестве (о людях или животных).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "accumulation_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У входа в театр <скопилась> огромная толпа зрителей.",
                            "target_lang_translations": {
                                "en": "A huge crowd of spectators <gathered> at the entrance to the theater."
                            }
                        },
                        {
                            "text": "Люди <скопились> на площади, ожидая начала праздничного концерта.",
                            "target_lang_translations": {
                                "en": "People <crowded> into the square, waiting for the start of the festive concert."
                            }
                        }
                    ],
                    "synonyms": [
                        "собраться"
                    ],
                    "antonyms": [
                        "разойтись"
                    ],
                    "common_phrases": [
                        "скопилась толпа",
                        "люди скопились",
                        "народ скопился"
                    ],
                    "target_lang_definitions": {
                        "en": "To come together and gather in one place in large numbers (about people or animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gather",
                                "target_lang_sense_clarification": "to come together in a group"
                            },
                            {
                                "target_lang_word": "crowd",
                                "target_lang_sense_clarification": "to gather in large numbers, filling a space"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "8f3ac409-44e7-3bc6-b52c-8f1271073d50",
                    "sense_definition": "Trzecia osoba liczby pojedynczej rodzaju żeńskiego czasu przeszłego czasownika <w>spać</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja córka <spała> wczoraj do południa, bo była bardzo zmęczona.",
                            "target_lang_translations": {
                                "ru": "Моя дочь <спала> вчера до полудня, так как очень устала."
                            }
                        },
                        {
                            "text": "Dziewczynka smacznie <spała> w swoim łóżeczku przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Девочка сладко <спала> в своей кроватке всю ночь."
                            }
                        }
                    ],
                    "antonyms": [
                        "czuwała"
                    ],
                    "common_phrases": [
                        "smacznie spała",
                        "długo spała",
                        "spała jak zabita",
                        "spokojnie spała"
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа прошедшего времени глагола \"spać\" (спать) в женском роде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спала",
                                "target_lang_sense_clarification": "глагол, прошедшее время, женский род"
                            }
                        ]
                    }
                }
            ]
        },
        {

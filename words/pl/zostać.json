{
    "word_family": [
        "zostawać",
        "pozostać",
        "ostać",
        "pozostawienie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd638d02-1df5-3b0b-8d91-6d404ec48742",
                    "sense_definition": "Nie opuszczać miejsca, w którym się jest; przebywać gdzieś nadal",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ten weekend chętnie <zostanę> w domu i odpocznę.",
                            "target_lang_translations": {
                                "ru": "В эти выходные я с удовольствием <останусь> дома и отдохну."
                            }
                        },
                        {
                            "text": "Musisz <zostać> w łóżku, dopóki nie wyzdrowiejesz.",
                            "target_lang_translations": {
                                "ru": "Ты должен <остаться> в постели, пока не выздоровеешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozostać"
                    ],
                    "antonyms": [
                        "wyjść",
                        "wyjechać",
                        "opuścić"
                    ],
                    "common_phrases": [
                        "zostać w domu",
                        "zostać na noc",
                        "zostać po godzinach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не покидать место, в котором находишься; продолжать пребывать где-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаться",
                                "target_lang_sense_clarification": "не уходить, продолжить пребывание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7fff9ae-eb89-3657-8068-4ecbe83075dd",
                    "sense_definition": "Zmienić się w kogoś lub coś; objąć jakąś funkcję, rolę lub stanowisko",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój syn chce w przyszłości <zostać> strażakiem.",
                            "target_lang_translations": {
                                "ru": "Мой сын хочет в будущем <стать> пожарным."
                            }
                        },
                        {
                            "text": "Ona wkrótce <zostanie> dyrektorem tej dużej firmy.",
                            "target_lang_translations": {
                                "ru": "Она скоро <станет> директором этой крупной фирмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozostać"
                    ],
                    "common_phrases": [
                        "zostać lekarzem",
                        "zostać rodzicem",
                        "zostać przyjacielem",
                        "zostać mistrzem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превратиться в кого-то или что-то; получить какую-либо должность, роль или звание"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стать",
                                "target_lang_sense_clarification": "сделаться кем-либо, получить статус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a73cf9cc-e57e-3450-8f70-5db3fb1f5ada",
                    "sense_definition": "Być dostępnym jako reszta po wykorzystaniu lub odjęciu części; uchować się",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lodówce nie <zostało> już nic do jedzenia.",
                            "target_lang_translations": {
                                "ru": "В холодильнике уже ничего не <осталось> из еды."
                            }
                        },
                        {
                            "text": "Niestety <zostało> nam bardzo niewiele czasu na decyzję.",
                            "target_lang_translations": {
                                "ru": "К сожалению, у нас <осталось> очень мало времени на принятие решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozostać",
                        "ostać"
                    ],
                    "common_phrases": [
                        "nic nie zostało",
                        "zostało mało czasu",
                        "zostało mi trochę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть в наличии как остаток после использования или вычитания части; сохраниться"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаться",
                                "target_lang_sense_clarification": "сохраниться, быть в остатке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d57a318-a825-37e1-8197-fe2760f93306",
                    "sense_definition": "Czasownik posiłkowy służący do tworzenia strony biernej dla czasowników dokonanych",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piękny dom <został> zbudowany w zeszłym roku.",
                            "target_lang_translations": {
                                "ru": "Этот красивый дом <был> построен в прошлом году."
                            }
                        },
                        {
                            "text": "Wszyscy pracownicy <zostaną> zaproszeni na spotkanie świąteczne.",
                            "target_lang_translations": {
                                "ru": "Все сотрудники <будут> приглашены на рождественскую встречу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zostać zrobionym",
                        "zostać wybranym",
                        "zostać zaproszonym"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice auxiliary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательный глагол для образования страдательного залога совершенного вида"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "вспомогательный глагол пассива"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9529361a-c61f-33e8-8cfb-62065fb254b1",
                    "sense_definition": "Znaleźć się w określonej, zwykle trudnej sytuacji w wyniku jakichś zdarzeń",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po pożarze rodzina <została> bez dachu nad głową.",
                            "target_lang_translations": {
                                "ru": "После пожара семья <осталась> без крыши над головой."
                            }
                        },
                        {
                            "text": "Nie chcę <zostać> na lodzie z tymi wszystkimi problemami.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <остаться> ни с чем со всеми этими проблемами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zostać bez grosza",
                        "zostać na lodzie",
                        "zostać bez środków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказаться в определенной, обычно трудной ситуации в результате каких-либо событий"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаться",
                                "target_lang_sense_clarification": "оказаться в положении"
                            },
                            {
                                "target_lang_word": "оказаться",
                                "target_lang_sense_clarification": "попасть в ситуацию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "стёжка"
                    ],
                    "common_phrases": [
                        "лесная тропа",
                        "узкая тропа",
                        "звериная тропа",
                        "горная тропа"
                    ],
                    "target_lang_definitions": {
                        "en": "A narrow dirt road or track worn by people or animals."
                    },

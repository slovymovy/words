                    "sense_id": "f8483c95-569e-3fdf-846a-4e152cdd0b4f",
                    "sense_definition": "Het grootste aantal personen in een bepaalde groep.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_majority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meesten</w> zijn het daar niet mee eens.",
                            "target_lang_translations": {
                                "en": "<w>Most</w> do not agree with that.",
                                "ru": "<w>Большинство</w> с этим не согласны."
                            }
                        },
                        {
                            "text": "De <w>meesten</w> van ons gaan met de fiets naar het werk.",
                            "target_lang_translations": {
                                "en": "<w>Most</w> of us cycle to work.",
                                "ru": "<w>Большинство</w> из нас ездят на работу на велосипеде."
                            }
                        }
                    ],
                    "synonyms": [
                        "de meerderheid",
                        "het gros"
                    ],
                    "antonyms": [
                        "weinigen",
                        "enkelingen"
                    ],
                    "common_phrases": [
                        "de meesten van",
                        "de meesten onder"
                    ],
                    "target_lang_definitions": {
                        "en": "The largest number of people in a certain group.",
                        "ru": "Наибольшее количество людей в определенной группе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "most",

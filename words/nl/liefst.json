    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "462491f9-9eca-3f5e-aeb5-72eadd03fcf2",
                    "sense_definition": "Overtreffende trap van <w>graag</w>; geeft aan dat men iets meer wil dan al het andere.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "preference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik drink 's ochtends het <w>liefst</w> een kopje koffie.",
                            "target_lang_translations": {
                                "en": "In the morning, I <w>prefer</w> to drink a cup of coffee.",
                                "ru": "По утрам я <w>больше всего</w> люблю выпить чашечку кофе."
                            }
                        },
                        {
                            "text": "Zij gaat in de vakantie het <w>liefst</w> naar een warm land.",
                            "target_lang_translations": {
                                "en": "On vacation, she <w>prefers</w> to go to a warm country.",
                                "ru": "В отпуск она <w>охотнее всего</w> едет в теплую страну."
                            }
                        }
                    ],
                    "synonyms": [
                        "bij voorkeur"
                    ],
                    "common_phrases": [
                        "het liefst",
                        "wat ... het liefst"
                    ],
                    "target_lang_definitions": {
                        "en": "Superlative of 'graag'; indicates that one wants something more than anything else.",
                        "ru": "Превосходная степень от \"graag\" (охотно); указывает на то, что кто-то хочет чего-то больше, чем всего остального."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prefer",
                                "target_lang_sense_clarification": "verb construction (to do something preferably)"
                            },
                            {
                                "target_lang_word": "most of all",
                                "target_lang_sense_clarification": "adverbial phrase indicating highest preference"
                            },
                            {
                                "target_lang_word": "best",
                                "target_lang_sense_clarification": "used with 'like' (like best)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "больше всего",
                                "target_lang_sense_clarification": "выражение предпочтения"
                            },
                            {
                                "target_lang_word": "охотнее всего",
                                "target_lang_sense_clarification": "о желании сделать что-либо"
                            },
                            {
                                "target_lang_word": "предпочтительнее всего",
                                "target_lang_sense_clarification": "формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abf005b1-7d9e-3591-b4d6-79a438346238",
                    "sense_definition": "Gebruikt om nadruk te leggen op een onverwacht groot aantal of een grote hoeveelheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het huis heeft maar <w>liefst</w> vier badkamers.",
                            "target_lang_translations": {
                                "en": "The house has <w>no less than</w> four bathrooms.",
                                "ru": "В доме <w>целых</w> четыре ванные комнаты."
                            }
                        },
                        {
                            "text": "Er kwamen <w>liefst</w> duizend mensen naar het concert kijken.",
                            "target_lang_translations": {
                                "en": "<w>As many as</w> a thousand people came to watch the concert.",
                                "ru": "Посмотреть концерт пришло <w>не менее</w> тысячи человек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "maar liefst"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to emphasize an unexpectedly large number or quantity.",
                        "ru": "Используется для подчеркивания неожиданно большого числа или количества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "no less than",
                                "target_lang_sense_clarification": "emphasizing a large amount"
                            },
                            {
                                "target_lang_word": "as many as",
                                "target_lang_sense_clarification": "emphasizing a large count"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "целых",
                                "target_lang_sense_clarification": "акцент на большом количестве"
                            },
                            {
                                "target_lang_word": "не менее",
                                "target_lang_sense_clarification": "указание на минимальное количество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [

                        "minderjarige",
                        "wicht"
                    ],
                    "antonyms": [
                        "volwassene"
                    ],
                    "common_phrases": [
                        "klein kind",
                        "als een kind",
                        "geen kind aan hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "A young human being who is not yet an adult.",
                        "ru": "Молодой человек, который ещё не достиг взрослого возраста."

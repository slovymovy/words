            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f703feb4-430a-33b8-87e9-607ea8bc3b9b",
                    "sense_definition": "Minder sterk, hevig of krachtig worden.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De storm zal in de loop van de avond waarschijnlijk <w>afzwakken</w>.",
                            "target_lang_translations": {
                                "en": "The storm will probably <w>subside</w> in the course of the evening.",
                                "ru": "Шторм, вероятно, <w>ослабнет</w> в течение вечера."
                            }
                        },
                        {
                            "text": "De economische groei is dit jaar iets <w>afgezwakt</w>.",
                            "target_lang_translations": {
                                "en": "Economic growth has <w>slowed down</w> slightly this year.",
                                "ru": "Экономический рост в этом году немного <w>замедлился</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzwakken"
                    ],
                    "antonyms": [
                        "toenemen",
                        "versterken",
                        "aanzwellen"
                    ],
                    "common_phrases": [
                        "de wind zwakt af",
                        "langzaam afzwakken",
                        "de storm zwakt af"
                    ],
                    "target_lang_definitions": {
                        "en": "To become less strong, intense, or powerful.",
                        "ru": "Становиться менее сильным, интенсивным или мощным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weaken",
                                "target_lang_sense_clarification": "general decrease in strength"
                            },
                            {
                                "target_lang_word": "subside",
                                "target_lang_sense_clarification": "for storms, wind, or emotions"
                            },
                            {
                                "target_lang_word": "diminish",
                                "target_lang_sense_clarification": "to become less in size or importance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ослабевать",
                                "target_lang_sense_clarification": "общая сила, интенсивность"
                            },
                            {
                                "target_lang_word": "стихать",
                                "target_lang_sense_clarification": "о ветре, шторме"
                            },
                            {
                                "target_lang_word": "идти на убыль",
                                "target_lang_sense_clarification": "уменьшаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9654c0c4-64fa-396e-95d0-a726a824a722",
                    "sense_definition": "Een mening, uitspraak of tekst minder scherp of stellig maken.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moest zijn felle kritiek later enigszins <w>afzwakken</w>.",
                            "target_lang_translations": {
                                "en": "He had to <w>tone down</w> his fierce criticism somewhat later.",
                                "ru": "Позже ему пришлось несколько <w>смягчить</w> свою резкую критику."
                            }
                        },
                        {
                            "text": "In het eindrapport hebben ze de conclusies wat <w>afgezwakt</w>.",
                            "target_lang_translations": {
                                "en": "In the final report, they <w>watered down</w> the conclusions somewhat.",
                                "ru": "В окончательном отчете они немного <w>смягчили</w> выводы."
                            }
                        }
                    ],
                    "synonyms": [
                        "nuanceren",
                        "mitigeren"
                    ],
                    "antonyms": [
                        "aanscherpen",
                        "overdrijven"
                    ],
                    "common_phrases": [
                        "een mening afzwakken",
                        "kritiek afzwakken",
                        "de toon afzwakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To make an opinion, statement, or text less sharp or firm.",
                        "ru": "Делать мнение, высказывание или текст менее резким или категоричным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tone down",
                                "target_lang_sense_clarification": "to make something less offensive or harsh"
                            },
                            {
                                "target_lang_word": "soften",
                                "target_lang_sense_clarification": "to make less severe"
                            },
                            {
                                "target_lang_word": "water down",
                                "target_lang_sense_clarification": "to make less effective or forceful"
                            }
                        ],
                        "ru": [

{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "356729d1-5cf2-3ad7-990f-77f112927ef5",
                    "sense_definition": "Voor elk; in verhouding tot een bepaalde eenheid (zoals tijd, afstand of aantal).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ratio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet deze medicijnen driemaal <w>per</w> dag innemen.",
                            "target_lang_translations": {
                                "en": "You must take this medicine three times <w>a</w> day.",
                                "ru": "Тебе нужно принимать эти лекарства три раза <w>в</w> день."
                            }
                        },
                        {
                            "text": "De auto verbruikt ongeveer vijf liter benzine <w>per</w> honderd kilometer.",
                            "target_lang_translations": {
                                "en": "The car consumes about five liters of petrol <w>per</w> hundred kilometers.",
                                "ru": "Машина расходует около пяти литров бензина <w>на</w> сто километров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "per dag",
                        "per jaar",
                        "per persoon",
                        "per stuk",
                        "per uur"
                    ],
                    "target_lang_definitions": {
                        "en": "For each; in proportion to a specific unit (such as time, distance, or number).",
                        "ru": "Для каждого; в расчете на определенную единицу (времени, расстояния или количества)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "per",
                                "target_lang_sense_clarification": "for each unit"
                            },
                            {
                                "target_lang_word": "a",
                                "target_lang_sense_clarification": "frequency per time unit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "временной интервал (в день, в год)"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "на единицу измерения или человека"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "за единицу товара (за штуку)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6919a553-3da1-395b-9792-887d9b0e4ca6",
                    "sense_definition": "Door middel van; met gebruik van (vooral bij vervoer en communicatie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "means",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij gaan dit jaar <w>per</w> trein op vakantie naar Frankrijk.",
                            "target_lang_translations": {
                                "en": "We are going on holiday to France <w>by</w> train this year.",
                                "ru": "В этом году мы поедем в отпуск во Францию <w>на</w> поезде."
                            }
                        },
                        {
                            "text": "De officiële uitnodiging wordt u <w>per</w> post toegestuurd.",
                            "target_lang_translations": {
                                "en": "The official invitation will be sent to you <w>by</w> post.",
                                "ru": "Официальное приглашение будет отправлено вам <w>по</w> почте."
                            }
                        }
                    ],
                    "synonyms": [
                        "door middel van",
                        "met"
                    ],
                    "common_phrases": [
                        "per auto",
                        "per trein",
                        "per post",
                        "per e-mail",
                        "per vliegtuig"
                    ],
                    "target_lang_definitions": {
                        "en": "By means of; using (especially for transport and communication).",
                        "ru": "Посредством; с использованием (особенно транспорта и связи)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "transport or means"
                            },
                            {
                                "target_lang_word": "via",
                                "target_lang_sense_clarification": "communication or route"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "способ связи (по почте, по телефону)"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "вид транспорта (на поезде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13714b37-5dcd-3802-af3e-560d4b7900ca",
                    "sense_definition": "Volgens; ingedeeld op basis van een categorie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De resultaten van het onderzoek zijn <w>per</w> land verschillend.",
                            "target_lang_translations": {
                                "en": "The results of the study differ <w>by</w> country.",
                                "ru": "Результаты исследования различаются <w>по</w> странам."
                            }
                        },
                        {
                            "text": "We hebben de verkoopcijfers <w>per</w> regio geanalyseerd.",
                            "target_lang_translations": {
                                "en": "We analyzed the sales figures <w>by</w> region.",
                                "ru": "Мы проанализировали показатели продаж <w>по</w> регионам."
                            }
                        }
                    ],
                    "synonyms": [
                        "volgens",
                        "naar"
                    ],
                    "common_phrases": [
                        "per land",
                        "per groep",
                        "per categorie",
                        "per definitie"
                    ],
                    "target_lang_definitions": {
                        "en": "According to; classified based on a category.",
                        "ru": "Согласно; распределено по категориям."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "categorization"
                            },
                            {
                                "target_lang_word": "per",
                                "target_lang_sense_clarification": "distribution"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "распределение по группам или признакам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e309ea54-0052-3489-a92d-a6c583386236",
                    "sense_definition": "Met ingang van; vanaf een bepaald tijdstip.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe wetgeving wordt <w>per</w> 1 januari van kracht.",
                            "target_lang_translations": {
                                "en": "The new legislation becomes effective <w>as of</w> January 1st.",
                                "ru": "Новое законодательство вступает в силу <w>с</w> 1 января."
                            }
                        },
                        {
                            "text": "De werknemer heeft zijn ontslag <w>per</w> direct ingediend.",
                            "target_lang_translations": {
                                "en": "The employee submitted his resignation <w>effective</w> immediately.",
                                "ru": "Сотрудник подал заявление об увольнении <w>с</w> немедленным вступлением в силу."
                            }
                        }
                    ],
                    "synonyms": [
                        "met ingang van",
                        "vanaf"
                    ],
                    "common_phrases": [
                        "per direct",
                        "per heden",
                        "per datum"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Effective from; starting from a certain point in time.",
                        "ru": "Начиная с; с определенного момента времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as of",
                                "target_lang_sense_clarification": "dates"
                            },
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "start time"
                            },
                            {
                                "target_lang_word": "effective",
                                "target_lang_sense_clarification": "status change"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "с даты или момента времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "722e467f-7e06-3757-a266-edb1b8c276ee",
                    "sense_definition": "Door; wegens (vooral gebruikt in vaste uitdrukkingen).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>per</w> ongeluk jouw koffie opgedronken.",
                            "target_lang_translations": {
                                "en": "I drank your coffee <w>by</w> accident.",
                                "ru": "Я <w>по</w> ошибке выпил твой кофе."
                            }
                        },
                        {
                            "text": "De brief is <w>per</w> abuis bij de buren bezorgd.",
                            "target_lang_translations": {
                                "en": "The letter was delivered to the neighbors <w>by</w> mistake.",
                                "ru": "Письмо было <w>по</w> ошибке доставлено соседям."
                            }
                        }
                    ],
                    "synonyms": [
                        "door",
                        "wegens"
                    ],
                    "common_phrases": [
                        "per ongeluk",
                        "per abuis",
                        "per toeval"
                    ],
                    "target_lang_definitions": {
                        "en": "By; due to (mainly used in fixed expressions).",
                        "ru": "Из-за; по причине (используется в устойчивых выражениях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "in fixed expressions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "в выражениях причины (по ошибке)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
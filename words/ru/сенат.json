                {
                    "sense_id": "a48f56c0-259d-3857-a2d6-effec3a47067",
                    "sense_definition": "Высшая палата парламента в ряде государств.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Американский <w>сенат</w> одобрил новый законопроект о налогах.",
                            "target_lang_translations": {
                                "en": "The US <w>Senate</w> approved the new tax bill."
                            }
                        },
                        {
                            "text": "Чтобы стать законом, документ должен пройти через <w>сенат</w>.",
                            "target_lang_translations": {
                                "en": "To become law, the document must pass through the <w>senate</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "верхняя палата"
                    ],
                    "antonyms": [
                        "нижняя палата"
                    ],
                    "common_phrases": [
                        "американский сенат",
                        "член сената",
                        "выборы в сенат",
                        "заседание сената"
                    ],
                    "target_lang_definitions": {
                        "en": "The upper house of parliament in a number of states."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "senate",
                                "target_lang_sense_clarification": "legislative body"
                            }
                        ]

                },
                {
                    "sense_id": "5ea05f64-fa99-3491-9e4b-87c38701647f",
                    "sense_definition": "Grupa trzech osób lub rzeczy, które są ze sobą powiązane lub wspólnie działają.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasi napastnicy tworzą niezwykle skuteczny <w>tercet</w> na boisku.",

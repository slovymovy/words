    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "77d8f0df-9cdd-388c-ad23-98924f414394",
                    "sense_definition": "Lacking a permanent place of residence.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Whenever I pass the park, I see the <w>homeless</w> people sleeping on the benches.",
                            "target_lang_translations": {
                                "ru": "Всякий раз, проходя мимо парка, я вижу <w>бездомных</w> людей, спящих на скамейках."
                            }
                        },
                        {
                            "text": "The severe storm destroyed many houses and left hundreds of families <w>homeless</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный шторм разрушил множество домов и оставил сотни семей <w>бездомными</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "unhoused",
                        "displaced",
                        "destitute"
                    ],
                    "antonyms": [
                        "housed"
                    ],
                    "common_phrases": [
                        "homeless shelter",
                        "homeless person",
                        "become homeless",
                        "left homeless"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий постоянного места жительства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездомный",
                                "target_lang_sense_clarification": "не имеющий жилья"
                            },
                            {
                                "target_lang_word": "лишенный крова",
                                "target_lang_sense_clarification": "оставшийся без жилья (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dfc2fc8-27b5-339d-8845-cfdfb1e188f1",
                    "sense_definition": "Containing no place that might be called home.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor gazed out at the vast, <w>homeless</w> ocean that stretched endlessly before him.",
                            "target_lang_translations": {
                                "ru": "Моряк смотрел на огромный, <w>бесприютный</w> океан, бесконечно простирающийся перед ним."
                            }
                        },
                        {
                            "text": "She felt a deep sense of isolation in the <w>homeless</w> silence of the desert.",
                            "target_lang_translations": {
                                "ru": "Она ощущала глубокое чувство изоляции в <w>бесприютной</w> тишине пустыни."
                            }
                        }
                    ],
                    "synonyms": [
                        "desolate"
                    ],
                    "common_phrases": [
                        "homeless sea",
                        "homeless wind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не дающий приюта или чувства дома; пустынный (поэтич.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесприютный",
                                "target_lang_sense_clarification": "не дающий приюта, унылый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "556d535c-085c-3d0c-90a6-5c035f7744b7",
                    "sense_definition": "A person who is homeless.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local charity provides hot meals and warm clothing for the <w>homeless</w>.",
                            "target_lang_translations": {
                                "ru": "Местная благотворительная организация предоставляет горячую еду и теплую одежду <w>бездомным</w>."
                            }
                        },
                        {
                            "text": "New government policies aim to protect the rights of the <w>homeless</w> in urban areas.",
                            "target_lang_translations": {
                                "ru": "Новая государственная политика направлена на защиту прав <w>бездомных</w> в городских районах."
                            }
                        }
                    ],
                    "synonyms": [
                        "vagrant",
                        "transient"
                    ],
                    "common_phrases": [
                        "the homeless",
                        "help the homeless"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, не имеющий жилья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездомный",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "бомж",
                                "target_lang_sense_clarification": "разг., лицо без определенного места жительства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
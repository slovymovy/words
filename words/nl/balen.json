{
    "word_family": [
        "baal",
        "baaldag"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3c95b304-5c1b-3ddf-98a0-a178b20a3611",
                    "sense_definition": "Zich ergeren of teleurgesteld zijn omdat een situatie tegenzit of niet leuk is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>baal</w> ervan dat het regent op mijn vrije dag.",
                            "target_lang_translations": {
                                "en": "I <w>am bummed</w> that it's raining on my day off.",
                                "ru": "Я <w>расстроен</w> тем, что в мой выходной идет дождь."
                            }
                        },
                        {
                            "text": "Hij <w>baalde</w> enorm toen hij hoorde dat hij de trein had gemist.",
                            "target_lang_translations": {
                                "en": "He was really <w>bummed</w> when he heard that he had missed the train.",
                                "ru": "Он ужасно <w>расстроился</w>, когда услышал, что опоздал на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich ergeren",
                        "teleurgesteld zijn"
                    ],
                    "antonyms": [
                        "genieten",
                        "zich verheugen"
                    ],
                    "common_phrases": [
                        "ergens van balen",
                        "flink balen",
                        "zitten te balen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be annoyed or disappointed because a situation is adverse or unpleasant.",
                        "ru": "Раздражаться или быть разочарованным из-за того, что ситуация складывается неудачно или неприятно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be bummed",
                                "target_lang_sense_clarification": "informal, to feel disappointed"
                            },
                            {
                                "target_lang_word": "be fed up",
                                "target_lang_sense_clarification": "to be annoyed or tired of something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расстраиваться",
                                "target_lang_sense_clarification": "быть огорченным чем-либо"
                            },
                            {
                                "target_lang_word": "досадовать",
                                "target_lang_sense_clarification": "испытывать раздражение из-за неудачи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "fd1e1c8f-ee9b-3c2e-8b5b-45dd31c2bb3c",
                    "sense_definition": "Uitroep om aan te geven dat men iets heel vervelend of jammer vindt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_disappointment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat <w>balen</w> dat je niet naar het feest kunt komen!",
                            "target_lang_translations": {
                                "en": "What a <w>bummer</w> that you can't come to the party!",
                                "ru": "Как <w>жаль</w>, что ты не сможешь прийти на вечеринку!"
                            }
                        },
                        {
                            "text": "De wedstrijd is afgelast door het slechte weer, echt <w>balen</w>.",
                            "target_lang_translations": {
                                "en": "The match has been cancelled due to bad weather, [it] really <w>sucks</w>.",
                                "ru": "Матч отменили из-за плохой погоды, полный <w>облом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jammer",
                        "helaas",
                        "verdorie"
                    ],
                    "common_phrases": [
                        "wat balen",
                        "echt balen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation to indicate that one finds something very annoying or unfortunate.",
                        "ru": "Восклицание, выражающее сильную досаду, разочарование или сожаление."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bummer",
                                "target_lang_sense_clarification": "disappointing situation"
                            },
                            {
                                "target_lang_word": "sucks",
                                "target_lang_sense_clarification": "informal, very annoying"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "облом",
                                "target_lang_sense_clarification": "разговорное выражение неудачи или разочарования"
                            },
                            {
                                "target_lang_word": "досада",
                                "target_lang_sense_clarification": "чувство неудовольствия"
                            },
                            {
                                "target_lang_word": "жаль",
                                "target_lang_sense_clarification": "выражение сожаления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

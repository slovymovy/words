                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flourishing",
                                "target_lang_sense_clarification": "vigorous growth or development, especially in culture, economy, or arts"
                            },
                            {
                                "target_lang_word": "heyday",
                                "target_lang_sense_clarification": "the period of a person's or thing's greatest success, popularity, or vigor"
                            },
                            {
                                "target_lang_word": "prime",
                                "target_lang_sense_clarification": "the state or time of greatest strength, vigor, or success in a person's life"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07039c03-c94f-3142-8aae-3f13fa8dc3d1",
                    "sense_definition": "Превратиться, сделаться кем-либо или каким-либо, перейти в новое состояние.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После дождя дорога <стала> очень грязной."
                        },
                        {
                            "text": "Мой брат <стал> врачом после окончания университета."
                        }
                    ],
                    "synonyms": [
                        "сделаться",
                        "превратиться"
                    ],
                    "common_phrases": [
                        "стало быть",
                        "стало лучше",
                        "стало известно"
                    ]
                },
                {
                    "sense_id": "4e723ad2-da9d-3bb5-bddf-098644341c2d",
                    "sense_definition": "Начать совершать какое-либо действие (используется как вспомогательный глагол с инфинитивом несовершенного вида).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда стемнело, мы <стали> собираться домой."
                        },
                        {
                            "text": "Он <стал> писать письмо, но не закончил его."
                        }
                    ],
                    "antonyms": [
                        "закончить",
                        "продолжить",
                        "недоставать"
                    ],
                    "common_phrases": [
                        "не стал делать",
                        "стал думать"
                    ]
                },
                {
                    "sense_id": "be714762-cf2a-3d81-868d-a085e7aaf424",
                    "sense_definition": "Принять вертикальное положение, встать на ноги или занять определённое место.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Стань> прямо и не двигайся, пока я тебя фотографирую."
                        },
                        {
                            "text": "Я <стал> в очередь за билетами."
                        }
                    ],
                    "synonyms": [
                        "встать"
                    ],
                    "antonyms": [
                        "лечь",
                        "согнуться"
                    ],
                    "common_phrases": [
                        "стать на колени",
                        "стать в очередь"
                    ]
                },
                {
                    "sense_id": "653f4a52-dcef-3350-a021-3e2796c6ece6",
                    "sense_definition": "Прекратить движение или работу; остановиться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мои часы внезапно <стали>, и я опоздал на встречу."
                        },
                        {
                            "text": "Машина <стала> посреди дороги из-за поломки двигателя."
                        }
                    ],
                    "synonyms": [
                        "остановиться",
                        "застопориться"
                    ],
                    "antonyms": [
                        "двигаться"
                    ]
                },
                {
                    "sense_id": "fec842ec-f0b6-31ba-8d0e-1005c4bb8dfe",
                    "sense_definition": "Перестать существовать, исчезнуть или умереть (используется в безличных предложениях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его не <стало> после долгой и тяжёлой болезни."
                        },
                        {
                            "text": "В магазине совсем не <стало> свежего хлеба."
                        }
                    ],
                    "synonyms": [
                        "наличествовать",
                        "существовать"
                    ],
                    "antonyms": [
                        "отсутствовать"
                    ]
                },
                {
                    "sense_id": "cc615532-557c-3c4e-bdc7-661ccf45e5fd",
                    "sense_definition": "Обойтись в какую-либо сумму денег.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во что мне <станет> ремонт этой квартиры?"
                        },
                        {
                            "text": "Эта поездка <стала> нам в копеечку."
                        }
                    ],
                    "synonyms": [
                        "стоить",
                        "обойтись"
                    ],
                    "common_phrases": [
                        "во что бы то ни стало"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "37271607-6f72-3d67-b0b8-a72bd4f2912d",
                    "sense_definition": "Покрыться льдом, замёрзнуть (о реке или водоёме).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "freeze",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зима была холодной, и река <стала> уже в ноябре."
                        },
                        {
                            "text": "Лёд на озере <стал> достаточно крепким для катания."
                        }
                    ],
                    "synonyms": [
                        "замёрзнуть",
                        "установиться"
                    ],
                    "antonyms": [
                        "растаять"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc970d15-6c0e-346e-8f4d-1673893a4029",
                    "sense_definition": "Основание, причина или нужда (в современном языке употребляется почти исключительно в вопросительных конструкциях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "need",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С какой <стати> я должен выполнять твою работу?"
                        },
                        {
                            "text": "Нет никакой <стати> ехать туда в такую погоду."
                        }
                    ],
                    "synonyms": [
                        "нужда",
                        "надобность",
                        "необходимость"
                    ],
                    "common_phrases": [
                        "с какой стати"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used primarily in fixed phrases"
                        }
                    ]
                },
                {
                    "sense_id": "bfeefdad-5563-37da-9b4f-3025279d2ed3",
                    "sense_definition": "Соответствие кому-либо или чему-либо по характеру, свойствам или положению (обычно в значении «подходящая пара» или «ровня»).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "match",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта умная и красивая девушка ему под <стать>."
                        },
                        {
                            "text": "Они нашли друг друга, оба упрямые и гордые — под <стать>."
                        }
                    ],
                    "synonyms": [
                        "характер",
                        "суть"
                    ],
                    "common_phrases": [
                        "под стать"
                    ]
                },
                {
                    "sense_id": "6942da9a-e6ec-3b16-97d8-aab2578b7eda",
                    "sense_definition": "Внешний вид, телосложение, осанка (обычно о красивой, стройной фигуре).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "build",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Военная <стать> выдавала в нём бывшего офицера."
                        },
                        {
                            "text": "Все любовались её гордой <статью> и плавными движениями."
                        }
                    ],
                    "synonyms": [
                        "телосложение",
                        "осанка",
                        "фигура",
                        "стан"
                    ]
                }
            ]
        }
    ]
}
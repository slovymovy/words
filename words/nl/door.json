                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mixture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zit per ongeluk veel te veel zout <w>door</w> de soep.",
                            "target_lang_translations": {
                                "en": "There is accidentally way too much salt <w>in</w> the soup."
                            }

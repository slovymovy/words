    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0b730ba5-8691-317b-831a-80100f33ee14",
                    "sense_definition": "Plechtig beloven dat men de waarheid spreekt of iets zal doen, vaak door een eed af te leggen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "oath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe ministers moeten trouw aan de koning <zweren>.",
                            "target_lang_translations": {
                                "ru": "Новые министры должны <присягнуть> на верность королю."
                            }
                        },
                        {
                            "text": "Hij <zwoer> dat hij het geld morgen zou terugbetalen.",
                            "target_lang_translations": {
                                "ru": "Он <поклялся>, что вернет деньги завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "beloven",
                        "eed afleggen"
                    ],
                    "common_phrases": [
                        "trouw zweren",
                        "bij hoog en laag zweren",
                        "een eed zweren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественно обещать говорить правду или сделать что-либо, часто принося клятву."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клясться",
                                "target_lang_sense_clarification": "давать торжественное обещание"
                            },
                            {
                                "target_lang_word": "присягать",
                                "target_lang_sense_clarification": "официально клясться в верности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d64c823-1977-3cae-9d18-67b48e732103",
                    "sense_definition": "Ontstoken zijn en etter produceren, meestal gezegd van een wond.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fester",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je de wond niet goed schoonmaakt, gaat hij <zweren>.",
                            "target_lang_translations": {
                                "ru": "Если не очистить рану как следует, она начнет <гноиться>."
                            }
                        },
                        {
                            "text": "Zijn vinger begon te <zweren> nadat er een splinter in was gekomen.",
                            "target_lang_translations": {
                                "ru": "Его палец начал <нарывать> после того, как в него попала заноза."
                            }
                        }
                    ],
                    "synonyms": [
                        "etteren",
                        "ontsteken"
                    ],
                    "antonyms": [
                        "genezen"
                    ],
                    "common_phrases": [
                        "een zwerende vinger",
                        "een zwerende wond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть воспаленным и выделять гной, обычно говорится о ране."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гноиться",
                                "target_lang_sense_clarification": "выделять гной"
                            },
                            {
                                "target_lang_word": "нарывать",
                                "target_lang_sense_clarification": "воспаляться с образованием гноя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c8b7770-de46-3d97-8beb-19675c15b7cb",
                    "sense_definition": "Vloeken of godslasterlijke taal gebruiken uit boosheid of frustratie.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "curse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep te <zweren> toen hij zijn trein miste.",
                            "target_lang_translations": {
                                "ru": "Он ходил и <ругался>, когда опоздал на свой поезд."
                            }
                        },
                        {
                            "text": "Het is niet netjes om te <zweren> in het openbaar.",
                            "target_lang_translations": {
                                "ru": "Неприлично <ругаться> в общественных местах."
                            }
                        }
                    ],
                    "synonyms": [
                        "vloeken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ругаться или использовать грубые слова от злости или разочарования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ругаться",
                                "target_lang_sense_clarification": "использовать бранные слова"
                            },
                            {
                                "target_lang_word": "сквернословить",
                                "target_lang_sense_clarification": "говорить непристойности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0d0a724e-c812-34c6-aedf-11bff3f5613c",
                    "sense_definition": "Meervoud van <w>zweer</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zieke man zat onder de pijnlijke <zweren>.",
                            "target_lang_translations": {
                                "ru": "Больной человек был покрыт болезненными <язвами>."
                            }
                        },
                        {
                            "text": "Deze zalf helpt goed tegen open <zweren> op de huid.",
                            "target_lang_translations": {
                                "ru": "Эта мазь хорошо помогает от открытых <язв> на коже."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"zweer\" (язва, нарыв)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язвы",
                                "target_lang_sense_clarification": "болезненные раны на коже"
                            },
                            {
                                "target_lang_word": "нарывы",
                                "target_lang_sense_clarification": "гнойные воспаления"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "c9ae3c6a-b8cf-34cb-a416-2e6d0004c9a4",
                    "sense_definition": "A place name used for various towns and locations, such as in New Zealand or the United States.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We visited the town of <w>Gore</w> during our trip to New Zealand.",
                            "target_lang_translations": {

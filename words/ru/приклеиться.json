    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6328e6e-85a2-351b-93a8-b46c00d2b6f0",
                    "sense_definition": "Прикрепиться к чему-либо с помощью клея или другого клейкого вещества.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_sticking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Почтовая марка намертво <w>приклеилась</w> к конверту.",
                            "target_lang_translations": {
                                "en": "The postage stamp <w>stuck</w> firmly to the envelope."
                            }
                        },
                        {
                            "text": "Пластырь плохо <w>приклеился</w> к коже и начал отпадать.",
                            "target_lang_translations": {
                                "en": "The plaster didn't <w>stick</w> well to the skin and started falling off."
                            }
                        }
                    ],
                    "synonyms": [
                        "прилепиться"
                    ],
                    "antonyms": [
                        "отклеиться"
                    ],
                    "common_phrases": [
                        "намертво приклеиться",
                        "прочно приклеиться",
                        "плохо приклеиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To attach to something using glue or another adhesive substance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to stick",
                                "target_lang_sense_clarification": "using glue or adhesive"
                            },
                            {
                                "target_lang_word": "to adhere",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6b24de0-95cc-3c2b-99dc-b1b87aaf06ec",
                    "sense_definition": "Плотно прижаться, прилипнуть к поверхности (обычно из-за влаги, пота, грязи или статического электричества).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_sticking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мокрая от дождя рубашка неприятно <w>приклеилась</w> к телу.",
                            "target_lang_translations": {
                                "en": "The rain-soaked shirt unpleasantly <w>stuck</w> to the body."
                            }
                        },
                        {
                            "text": "Тонкие волосы <w>приклеились</w> к её лбу из-за сильной жары.",
                            "target_lang_translations": {
                                "en": "Thin hair <w>stuck</w> to her forehead due to the intense heat."
                            }
                        }
                    ],
                    "synonyms": [
                        "прилипнуть",
                        "пристать"
                    ],
                    "antonyms": [
                        "отлипнуть"
                    ],
                    "common_phrases": [
                        "приклеиться к телу",
                        "волосы приклеились",
                        "платье приклеилось"
                    ],
                    "target_lang_definitions": {
                        "en": "To press tightly against or stick to a surface (usually due to moisture, sweat, dirt, or static electricity)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to stick",
                                "target_lang_sense_clarification": "to a surface due to moisture or static"
                            },
                            {
                                "target_lang_word": "to cling",
                                "target_lang_sense_clarification": "tightly to a surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e3ceed9-6236-39fc-b711-03f08716c2e3",
                    "sense_definition": "Неотрывно смотреть на что-либо или находиться вплотную к чему-либо, не отходя.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети весь вечер <w>приклеились</w> к экрану телевизора.",
                            "target_lang_translations": {
                                "en": "The children were <w>glued</w> to the TV screen all evening."
                            }
                        },
                        {
                            "text": "Любопытные туристы <w>приклеились</w> к окнам автобуса, рассматривая город.",
                            "target_lang_translations": {
                                "en": "Curious tourists were <w>glued</w> to the bus windows, examining the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "прилипнуть"
                    ],
                    "common_phrases": [
                        "приклеиться к экрану",
                        "приклеиться к окну",
                        "приклеиться взглядом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To look unceasingly at something or stay right next to something without leaving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to be glued",
                                "target_lang_sense_clarification": "to a screen or window (figurative)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4385390-23c3-3905-a4f6-2eeee59c99d5",
                    "sense_definition": "Прочно закрепиться за кем-либо (о прозвище, кличке, репутации).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_reputation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это смешное прозвище <w>приклеилось</w> к нему ещё в начальной школе.",
                            "target_lang_translations": {
                                "en": "This funny nickname <w>stuck</w> to him back in elementary school."
                            }
                        },
                        {
                            "text": "Дурная слава <w>приклеилась</w> к этому старому дому.",
                            "target_lang_translations": {
                                "en": "A bad reputation <w>stuck</w> to this old house."
                            }
                        }
                    ],
                    "synonyms": [
                        "прицепиться",
                        "закрепиться"
                    ],
                    "common_phrases": [
                        "прозвище приклеилось",
                        "кличка приклеилась"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become firmly attached to someone (regarding a nickname, moniker, or reputation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to stick",
                                "target_lang_sense_clarification": "of a nickname or reputation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "257c0cf6-64e8-3f01-8959-efdf250f1337",
                    "sense_definition": "Навязчиво присоединиться к кому-либо, начать следовать за кем-то, не отставая.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_pestering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Какой-то незнакомый человек <w>приклеился</w> к нам на улице и шёл до самого метро.",
                            "target_lang_translations": {
                                "en": "Some stranger <w>latched onto</w> us on the street and walked all the way to the metro."
                            }
                        },
                        {
                            "text": "Младший брат <w>приклеился</w> ко мне и мешал делать уроки.",
                            "target_lang_translations": {
                                "en": "My younger brother <w>stuck</w> to me and kept me from doing homework."
                            }
                        }
                    ],
                    "synonyms": [
                        "увязаться",
                        "прицепиться",
                        "пристать"
                    ],
                    "antonyms": [
                        "отстать"
                    ],
                    "common_phrases": [
                        "приклеиться как банный лист"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To intrusively join someone, start following someone without falling behind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to latch onto",
                                "target_lang_sense_clarification": "someone (intrusively)"
                            },
                            {
                                "target_lang_word": "to tag along",
                                "target_lang_sense_clarification": "with someone (colloquial)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
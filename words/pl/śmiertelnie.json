{
    "word_family": [
        "śmierć",
        "śmiertelny",
        "śmiertelność",
        "śmiertelnik"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0b795fc4-5443-3969-9830-64c1cc0bafd5",
                    "sense_definition": "W sposób powodujący śmierć lub prowadzący do niej.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czarownica zbierała <w>śmiertelnie</w> trujące muchomory w lesie.",
                            "target_lang_translations": {
                                "ru": "Ведьма собирала <w>смертельно</w> ядовитые мухоморы в лесу."
                            }
                        },
                        {
                            "text": "Kierowca został <w>śmiertelnie</w> potrącony na przejściu dla pieszych.",
                            "target_lang_translations": {
                                "ru": "Водитель был <w>насмерть</w> сбит на пешеходном переходе."
                            }
                        }
                    ],
                    "synonyms": [
                        "letalnie",
                        "na śmierć"
                    ],
                    "common_phrases": [
                        "śmiertelnie chory",
                        "śmiertelnie ranny",
                        "śmiertelnie trujący",
                        "śmiertelnie niebezpieczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, вызывающим смерть или ведущим к ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смертельно",
                                "target_lang_sense_clarification": "приводя к гибели"
                            },
                            {
                                "target_lang_word": "насмерть",
                                "target_lang_sense_clarification": "с глаголами (сбить, разбиться)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39704f2a-e6ca-31d7-9398-927ddc2ba02c",
                    "sense_definition": "Bardzo, w stopniu ekstremalnym; używane dla podkreślenia intensywności cechy lub stanu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed samym wyjazdem pokłóciłem się z Pawłem o jakąś drobnostkę i wsiedliśmy do samolotu <w>śmiertelnie</w> na siebie obrażeni.",
                            "target_lang_translations": {
                                "ru": "Перед самым отъездом я поссорился с Павлом из-за какой-то мелочи, и мы сели в самолет, <w>смертельно</w> обиженные друг на друга."
                            }
                        },
                        {
                            "text": "Ten wykład jest <w>śmiertelnie</w> nudny i wszyscy już zasypiają.",
                            "target_lang_translations": {
                                "ru": "Эта лекция <w>смертельно</w> скучная, и все уже засыпают."
                            }
                        }
                    ],
                    "synonyms": [
                        "bardzo",
                        "ekstremalnie",
                        "niezwykle",
                        "potwornie",
                        "kompletnie",
                        "maksymalnie",
                        "mocno"
                    ],
                    "common_phrases": [
                        "śmiertelnie poważny",
                        "śmiertelnie nudny",
                        "śmiertelnie zmęczony",
                        "śmiertelnie zakochany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень, в крайней степени; используется для подчеркивания интенсивности качества или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смертельно",
                                "target_lang_sense_clarification": "в значении \"крайне\""
                            },
                            {
                                "target_lang_word": "до смерти",
                                "target_lang_sense_clarification": "очень сильно (идиома)"
                            },
                            {
                                "target_lang_word": "ужасно",
                                "target_lang_sense_clarification": "разговорное усиление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "172d4097-dcf4-3d28-94cf-e43506fdecb0",
                    "sense_definition": "W sposób przypominający śmierć lub kojarzący się z nią (np. wyglądem, ciszą).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego twarz była <w>śmiertelnie</w> blada ze strachu.",
                            "target_lang_translations": {
                                "ru": "Его лицо было <w>мертвенно</w> бледным от страха."
                            }
                        },
                        {
                            "text": "W opuszczonym domu panowała <w>śmiertelnie</w> grobowa cisza.",
                            "target_lang_translations": {
                                "ru": "В заброшенном доме царила <w>мертвая</w> гробовая тишина."
                            }
                        }
                    ],
                    "synonyms": [
                        "trupio",
                        "grobowo"
                    ],
                    "common_phrases": [
                        "śmiertelnie blady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, напоминающим смерть или ассоциирующимся с ней (например, внешним видом, тишиной)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мертвенно",
                                "target_lang_sense_clarification": "как мертвец (о бледности)"
                            },
                            {
                                "target_lang_word": "смертельно",
                                "target_lang_sense_clarification": "подобно смерти"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
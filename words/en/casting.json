    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c399ac98-7d8f-37d3-bb44-2aa3b51e2647",
                    "sense_definition": "The act or process of selecting actors, singers, dancers, or models for a role in a performance.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "entertainment_selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>casting</w> director spent weeks looking for the perfect actor to play the villain.",
                            "target_lang_translations": {
                                "ru": "Директор по <w>кастингу</w> потратил недели на поиск идеального актера на роль злодея."
                            }
                        },
                        {
                            "text": "She was nervous before the <w>casting</w> call but performed her monologue perfectly.",
                            "target_lang_translations": {
                                "ru": "Она нервничала перед <w>прослушиванием</w>, но идеально исполнила свой монолог."
                            }
                        }
                    ],
                    "synonyms": [
                        "audition"
                    ],
                    "common_phrases": [
                        "casting director",
                        "casting call",
                        "casting agency",
                        "casting couch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс отбора актеров, певцов, танцоров или моделей для исполнения роли в представлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кастинг",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "пробы",
                                "target_lang_sense_clarification": "преимущественно в кино"
                            },
                            {
                                "target_lang_word": "прослушивание",
                                "target_lang_sense_clarification": "в театре или музыке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0e5a3c4-4a43-35d2-8195-8352e0baf134",
                    "sense_definition": "A manufacturing process in which liquid material is poured into a mold to harden into a specific shape.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "manufacturing_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bronze <w>casting</w> is an ancient technique used to create durable sculptures.",
                            "target_lang_translations": {
                                "ru": "<w>Литьё</w> из бронзы — это древняя техника, используемая для создания долговечных скульптур."
                            }
                        },
                        {
                            "text": "The factory specializes in the die <w>casting</w> of aluminum parts for cars.",
                            "target_lang_translations": {
                                "ru": "Завод специализируется на <w>литье</w> под давлением алюминиевых деталей для автомобилей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "die casting",
                        "sand casting",
                        "investment casting",
                        "metal casting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производственный процесс, при котором жидкий материал заливается в форму для затвердевания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "литьё",
                                "target_lang_sense_clarification": "технологический процесс"
                            },
                            {
                                "target_lang_word": "отливка",
                                "target_lang_sense_clarification": "процесс изготовления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4beaed9-f63d-3e09-8740-cced0b6bb8c1",
                    "sense_definition": "An object that has been made by pouring molten material into a mold.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "manufacturing_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic inspected the iron <w>casting</w> for any cracks or defects.",
                            "target_lang_translations": {
                                "ru": "Механик осмотрел чугунную <w>отливку</w> на предмет трещин или дефектов."
                            }
                        },
                        {
                            "text": "This museum exhibit features a delicate gold <w>casting</w> from the 18th century.",
                            "target_lang_translations": {
                                "ru": "В экспозиции этого музея представлено изящное золотое <w>литое изделие</w> XVIII века."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iron casting",
                        "steel casting",
                        "hollow casting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, изготовленный путем заливки расплавленного материала в форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отливка",
                                "target_lang_sense_clarification": "предмет"
                            },
                            {
                                "target_lang_word": "литое изделие",
                                "target_lang_sense_clarification": "готовая продукция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47dcefa9-c273-3f59-9629-31a04d748e0a",
                    "sense_definition": "The act of converting a value from one data type to another in computer programming.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Implicit <w>casting</w> happens automatically when the compiler converts an integer to a float.",
                            "target_lang_translations": {
                                "ru": "Неявное <w>приведение типов</w> происходит автоматически, когда компилятор преобразует целое число в число с плавающей запятой."
                            }
                        },
                        {
                            "text": "The programmer used type <w>casting</w> to ensure the variable was treated as a string.",
                            "target_lang_translations": {
                                "ru": "Программист использовал <w>приведение типов</w>, чтобы гарантировать, что переменная будет обрабатываться как строка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "type casting",
                        "dynamic casting",
                        "static casting"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобразование значения из одного типа данных в другой в программировании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приведение типов",
                                "target_lang_sense_clarification": "термин программирования"
                            },
                            {
                                "target_lang_word": "преобразование",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b9c8680-eccc-3e98-82a7-651176cdb849",
                    "sense_definition": "The waste matter excreted by earthworms, often used to enrich soil.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gardeners often buy bags of worm <w>castings</w> to use as a natural fertilizer.",
                            "target_lang_translations": {
                                "ru": "Садоводы часто покупают мешки с <w>биогумусом</w> для использования в качестве натурального удобрения."
                            }
                        },
                        {
                            "text": "The lawn was covered in small mounds of earthworm <w>castings</w> after the rain.",
                            "target_lang_translations": {
                                "ru": "После дождя газон был покрыт маленькими холмиками <w>выбросов</w> дождевых червей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "worm castings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отходы жизнедеятельности дождевых червей, часто используемые для обогащения почвы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биогумус",
                                "target_lang_sense_clarification": "как удобрение"
                            },
                            {
                                "target_lang_word": "экскременты",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7483d336-bab9-3cc2-beff-0abc2990a85b",
                    "sense_definition": "The regurgitation of indigestible material, such as fur or feathers, by a hawk.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "falconry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The falconer checked the bird's <w>castings</w> to ensure it was digesting its food properly.",
                            "target_lang_translations": {
                                "ru": "Сокольник проверил <w>погадки</w> птицы, чтобы убедиться, что она правильно переваривает пищу."
                            }
                        },
                        {
                            "text": "Regular <w>casting</w> is a sign of a healthy digestive system in birds of prey.",
                            "target_lang_translations": {
                                "ru": "Регулярное сбрасывание <w>погадки</w> является признаком здоровой пищеварительной системы у хищных птиц."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "falconry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Срыгивание хищной птицей неперевариваемых материалов, таких как мех или перья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погадка",
                                "target_lang_sense_clarification": "термин соколиной охоты и орнитологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48d79e6a-8231-35f3-8220-5c2c5fe67f67",
                    "sense_definition": "The action of hounds spreading out to search for a lost scent during a hunt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dogs were <w>casting</w> across the field trying to pick up the trail of the fox.",
                            "target_lang_translations": {
                                "ru": "Собаки <w>рыскали</w> по полю, пытаясь взять след лисы."
                            }
                        },
                        {
                            "text": "After a moment of <w>casting</w>, the lead hound signaled that the scent was found.",
                            "target_lang_translations": {
                                "ru": "После недолгого <w>поиска следа</w> вожак стаи подал сигнал, что запах найден."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "hunting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действия гончих, рассыпающихся в поисках потерянного следа во время охоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поиск следа",
                                "target_lang_sense_clarification": "действие собак"
                            },
                            {
                                "target_lang_word": "рыскание",
                                "target_lang_sense_clarification": "поиск добычи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e169560a-5063-30a9-867a-ae9aa904a2e3",
                    "sense_definition": "The present participle and gerund of the verb <w>cast</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fisherman stood by the river, <w>casting</w> his line into the deep water.",
                            "target_lang_translations": {
                                "ru": "Рыбак стоял у реки, <w>забрасывая</w> леску в глубокую воду."
                            }
                        },
                        {
                            "text": "The large oak tree was <w>casting</w> a long shadow across the garden.",
                            "target_lang_translations": {
                                "ru": "Большой дуб <w>отбрасывал</w> длинную тень через весь сад."
                            }

                    ],
                    "target_lang_definitions": {
                        "ru": "крупная морская рыба семейства пилорылых с характерным длинным рылом с зубами по бокам"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыба-пила",
                                "target_lang_sense_clarification": "рыба"
                            },
                            {
                                "target_lang_word": "пилорыл",
                                "target_lang_sense_clarification": "зоология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2213a87-fde3-3dd0-98f0-80dc3657e459",
                    "sense_definition": "instrument muzyczny w postaci elastycznej stalowej płyty, na której gra się smyczkiem",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muzyk wydobywał z grającej <piły> niesamowite, wibrujące dźwięki.",
                            "target_lang_translations": {
                                "ru": "Музыкант извлекал из поющей <пилы> невероятные, вибрирующие звуки."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie słyszałem koncertu na <piłę> i orkiestrę.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не слышал концерта для <пилы> с оркестром."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "музыкальный инструмент в виде гибкой стальной пластины, на которой играют смычком"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "882cc0a9-0929-3eb1-96c6-458f3a5563f8",
                    "sense_definition": "Iemand die bloed, organen of weefsel afstaat voor medisch gebruik door anderen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_donor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts zocht dringend naar een geschikte <w>donor</w> voor de patiënt.",
                            "target_lang_translations": {
                                "ru": "Врач срочно искал подходящего <w>донора</w> для пациента."
                            }
                        },
                        {
                            "text": "Je kunt je online registreren als <w>donor</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете зарегистрироваться как <w>донор</w> онлайн."
                            }
                        }
                    ],
                    "synonyms": [
                        "orgaandonor",
                        "gever"
                    ],
                    "antonyms": [
                        "recipiënt",
                        "receptor"
                    ],
                    "common_phrases": [
                        "geregistreerde donor",
                        "anonieme donor",
                        "geschikte donor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто отдает кровь, органы или ткани для медицинского использования другими людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "донор",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "623d4dcf-f01f-33ac-9b78-ffec78a402d5",
                    "sense_definition": "Een persoon of instantie die geld of goederen schenkt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_giver",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gulle <w>donor</w> maakte het project financieel mogelijk.",
                            "target_lang_translations": {
                                "ru": "Щедрый <w>жертвователь</w> сделал этот проект финансово возможным."
                            }
                        },
                        {
                            "text": "Elke <w>donor</w> ontvangt een bedankbrief van de stichting.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>жертвователь</w> получает благодарственное письмо от фонда."
                            }
                        }
                    ],
                    "synonyms": [
                        "donateur",
                        "schenker",
                        "gever"
                    ],
                    "common_phrases": [
                        "gulle donor",
                        "particuliere donor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или организация, жертвующие деньги или товары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жертвователь",
                                "target_lang_sense_clarification": "благотворительность"
                            },
                            {
                                "target_lang_word": "спонсор",
                                "target_lang_sense_clarification": "финансовая поддержка"
                            },
                            {
                                "target_lang_word": "донор",
                                "target_lang_sense_clarification": "гранты и международная помощь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "929e5dae-4218-39b4-81e1-7d3849ba6afb",
                    "sense_definition": "Een atoom of molecuul dat een elektron afstaat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry_donor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het atoom gedraagt zich als een <w>donor</w> in het kristalrooster.",
                            "target_lang_translations": {
                                "ru": "Атом ведет себя как <w>донор</w> в кристаллической решетке."
                            }
                        },
                        {
                            "text": "De <w>donor</w> geeft een elektron af aan de acceptor.",
                            "target_lang_translations": {
                                "ru": "<w>Донор</w> отдаёт электрон акцептору."
                            }
                        }
                    ],
                    "antonyms": [
                        "acceptor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Атом или молекула, отдающие электрон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "донор",
                                "target_lang_sense_clarification": "химия/физика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

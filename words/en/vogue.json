                    "sense_id": "ce0d7dc4-3add-3a43-8130-e4d648f6625c",
                    "sense_definition": "The prevailing fashion or style at a particular time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "fashion_popularity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miniskirts were the <w>vogue</w> in the '60s.",
                            "target_lang_translations": {
                                "ru": "Мини-юбки были в <w>моде</w> в 60-х."
                            }
                        },
                        {
                            "text": "Short hair is currently the <w>vogue</w> for men this summer.",
                            "target_lang_translations": {
                                "ru": "Короткие стрижки сейчас в <w>моде</w> у мужчин этим летом."

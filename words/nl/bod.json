    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4ecf8aa4-2038-3f18-a8ff-ce368d069940",
                    "sense_definition": "Een bedrag dat iemand voorstelt te betalen voor iets, bijvoorbeeld op een veiling of bij de aankoop van een huis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bid_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>bod</w> was veel te laag voor de waarde van de auto.",
                            "target_lang_translations": {
                                "en": "His <w>offer</w> was much too low for the value of the car.",
                                "ru": "Его <w>предложение</w> было слишком низким по сравнению со стоимостью автомобиля."
                            }
                        },
                        {
                            "text": "Wij hebben gisteren een officieel <w>bod</w> op het huis gedaan.",
                            "target_lang_translations": {
                                "en": "We made an official <w>offer</w> on the house yesterday.",
                                "ru": "Вчера мы сделали официальное <w>предложение</w> о покупке дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanbod"
                    ],
                    "antonyms": [
                        "vraagprijs"
                    ],
                    "common_phrases": [
                        "een bod doen",
                        "een bod uitbrengen",
                        "een bod accepteren",
                        "een bod afslaan"
                    ],
                    "target_lang_definitions": {
                        "en": "An amount that someone proposes to pay for something, for example at an auction or when purchasing a house.",
                        "ru": "Сумма, которую кто-то предлагает заплатить за что-то, например, на аукционе или при покупке недвижимости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "offer",
                                "target_lang_sense_clarification": "proposal to buy"
                            },
                            {
                                "target_lang_word": "bid",
                                "target_lang_sense_clarification": "at an auction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "на аукционе"
                            },
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "цены"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a0a6278-c64b-34d6-b706-fba6b4457720",
                    "sense_definition": "De handeling van het bieden of de beurt om iets te doen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bid_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze deed een <w>bod</w> op de antieke tafel tijdens de veiling.",
                            "target_lang_translations": {
                                "en": "She made a <w>bid</w> for the antique table during the auction.",
                                "ru": "Она сделала <w>ставку</w> на антикварный стол во время аукциона."
                            }
                        },
                        {
                            "text": "Tijdens de vergadering kwamen verschillende nieuwe ideeën aan <w>bod</w>.",
                            "target_lang_translations": {
                                "en": "During the meeting, several new ideas came up for <w>discussion</w>.",
                                "ru": "Во время собрания <w>обсуждались</w> различные новые идеи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan bod komen"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of bidding or the turn to do something.",
                        "ru": "Действие предложения цены или (в устойчивых выражениях) очередь, возможность быть обсужденным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "opportunity to act or speak"
                            },
                            {
                                "target_lang_word": "bid",
                                "target_lang_sense_clarification": "action of bidding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "в выражении \"aan bod komen\""
                            },
                            {
                                "target_lang_word": "обсуждение",
                                "target_lang_sense_clarification": "тема разговора"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
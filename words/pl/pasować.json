{
    "word_family": [
        "dopasować",
        "spasować",
        "przypasować",
        "pasowanie",
        "dopasowanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "911c6ec3-6909-3009-b553-8801b3ff68a3",
                    "sense_definition": "Być odpowiedniego rozmiaru lub kształtu; dobrze leżeć na kimś (o ubraniu) lub mieścić się w czymś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "compatibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te nowe buty idealnie <pasują> na moje stopy.",
                            "target_lang_translations": {
                                "ru": "Эти новые туфли идеально <подходят> мне по размеру."
                            }
                        },
                        {
                            "text": "Ten klucz nie <pasuje> do zamka w drzwiach wejściowych.",
                            "target_lang_translations": {
                                "ru": "Этот ключ не <подходит> к замку входной двери."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasować jak ulał",
                        "pasować do zamka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть подходящего размера или формы; хорошо сидеть на ком-то (об одежде) или подходить к чему-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "по размеру, форме или к чему-либо"
                            },
                            {
                                "target_lang_word": "быть впору",
                                "target_lang_sense_clarification": "об одежде или обуви"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13eeca8f-aef6-3520-81be-9fce8dc007e0",
                    "sense_definition": "Dobrze wyglądać w zestawieniu z czymś innym; tworzyć zgraną całość pod względem stylu lub koloru.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "compatibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czerwona sukienka bardzo <pasuje> do twojej urody.",
                            "target_lang_translations": {
                                "ru": "Красное платье очень <идет> тебе."
                            }
                        },
                        {
                            "text": "Twoje zachowanie nie <pasuje> do powagi tej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Твое поведение не <соответствует> серьезности этой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "harmonizować (ze sobą)"
                    ],
                    "antonyms": [
                        "gryźć się"
                    ],
                    "common_phrases": [
                        "pasować do siebie",
                        "pasować do twarzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо смотреться вместе с чем-то; гармонировать по стилю или цвету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "быть к лицу, украшать"
                            },
                            {
                                "target_lang_word": "сочетаться",
                                "target_lang_sense_clarification": "гармонировать"
                            },
                            {
                                "target_lang_word": "соответствовать",
                                "target_lang_sense_clarification": "быть уместным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41b7a2c9-79b6-3fbc-b009-8af2d2599cae",
                    "sense_definition": "Być dogodnym, zgodnym z czyimiś planami, potrzebami lub terminami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "compatibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy <pasuje> ci spotkanie w przyszły wtorek o godzinie dziesiątej?",
                            "target_lang_translations": {
                                "ru": "Тебе <подходит> встреча в следующий вторник в десять часов?"
                            }
                        },
                        {
                            "text": "Ten termin mi niestety nie <pasuje>, ponieważ wyjeżdżam.",
                            "target_lang_translations": {
                                "ru": "Это время мне, к сожалению, не <подходит>, так как я уезжаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpowiadać"
                    ],
                    "common_phrases": [
                        "pasować komuś",
                        "termin mi pasuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть удобным, устраивать кого-то по времени или планам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "быть удобным"
                            },
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "о времени или условиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2737baa0-63ff-3907-8eee-fb15b5a366d8",
                    "sense_definition": "Rezygnować z udziału w grze (np. w karty), licytacji lub wycofywać się z jakiegoś działania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "game_logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam bardzo słabe karty, więc w tym rozdaniu <pasuję>.",
                            "target_lang_translations": {
                                "ru": "У меня очень слабые карты, поэтому в этой раздаче я <пасую>."
                            }
                        },
                        {
                            "text": "Kiedy zobaczył, jak trudne jest to zadanie, postanowił, że <pasuje>.",
                            "target_lang_translations": {
                                "ru": "Когда он увидел, насколько трудно это задание, он решил, что <пасует>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mówić pas",
                        "ja pasuję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказываться от хода в игре или участия в чем-либо; сдаваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасовать",
                                "target_lang_sense_clarification": "в игре или перед трудностями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90621ea6-60c8-3860-ae75-7002fe18e22b",
                    "sense_definition": "Uroczyście nadawać komuś tytuł (np. rycerski) lub przyjmować do grona członków jakiejś społeczności.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ceremony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król uroczyście <pasował> go na rycerza po wygranej bitwie.",
                            "target_lang_translations": {
                                "ru": "Король торжественно <посвятил> его в рыцари после выигранной битвы."
                            }
                        },
                        {
                            "text": "Dyrektor szkoły wielkim ołówkiem <pasował> pierwszoklasistów na uczniów.",
                            "target_lang_translations": {
                                "ru": "Директор школы огромным карандашом <посвящал> первоклассников в ученики."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasować na rycerza",
                        "pasować na ucznia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественно присваивать звание или принимать в сообщество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящать",
                                "target_lang_sense_clarification": "в рыцари, в студенты"
                            },
                            {
                                "target_lang_word": "возводить",
                                "target_lang_sense_clarification": "в ранг или сан"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8d21b0c-a9fe-3d7e-8ef4-a4a7af04dfd7",
                    "sense_definition": "Zmagać się z czymś trudnym, walczyć z przeciwnościami lub siłować się z kimś (używane z zaimkiem „się”).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "struggle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez całe życie musiał <pasować> się z wieloma problemami zdrowotnymi.",
                            "target_lang_translations": {
                                "ru": "Всю жизнь ему приходилось <бороться> со многими проблемами со здоровьем."
                            }
                        },
                        {
                            "text": "Dwaj zapaśnicy długo <pasowali> się na macie, zanim jeden z nich wygrał.",
                            "target_lang_translations": {
                                "ru": "Два борца долго <боролись> на ковре, прежде чем один из них победил."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmagać się",
                        "siłować się",
                        "borykać się",
                        "mierzyć się",
                        "walczyć",
                        "boksować się"
                    ],
                    "common_phrases": [
                        "pasować się z życiem",
                        "pasować się z problemami"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or archaic in this context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бороться с трудностями или соперником (устар., книжн.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бороться",
                                "target_lang_sense_clarification": "с трудностями или соперником"
                            },
                            {
                                "target_lang_word": "сражаться",
                                "target_lang_sense_clarification": "вести бой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
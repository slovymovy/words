    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2dafd287-4f34-3922-8788-3cd8441ead6c",
                    "sense_definition": "Связанный с тропическим деревом какао, его плодами (бобами) или получаемым из них жирным маслом.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "botany_culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для приготовления настоящего шоколада необходимо качественное <w>какаовое</w> масло.",
                            "target_lang_translations": {
                                "en": "To make real chocolate, high-quality <w>cocoa</w> butter is necessary."
                            }
                        },
                        {
                            "text": "На выставке нам показали, как растут и выглядят сырые <w>какаовые</w> бобы.",
                            "target_lang_translations": {
                                "en": "At the exhibition, they showed us how raw <w>cocoa</w> beans grow and look."
                            }
                        }
                    ],
                    "common_phrases": [
                        "какаовое масло",
                        "какаовые бобы",
                        "какаовые деревья"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the tropical cacao tree, its fruit (beans), or the fatty oil obtained from them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cocoa",
                                "target_lang_sense_clarification": "beans, butter"
                            },
                            {
                                "target_lang_word": "cacao",
                                "target_lang_sense_clarification": "botanical, raw beans"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f9ebcf8-f98f-3cc1-83ab-4e46977655ce",
                    "sense_definition": "Приготовленный с использованием порошка какао или имеющий характерный для него вкус и цвет.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Утром детям часто дают горячий <w>какаовый</w> напиток с молоком.",
                            "target_lang_translations": {
                                "en": "In the morning, children are often given a hot <w>cocoa</w> drink with milk."
                            }
                        },
                        {
                            "text": "Стены гостиной были выкрашены в тёплый <w>какаовый</w> цвет.",
                            "target_lang_translations": {
                                "en": "The living room walls were painted a warm <w>cocoa</w> color."
                            }
                        }
                    ],
                    "synonyms": [
                        "шоколадный"
                    ],
                    "common_phrases": [
                        "какаовый напиток",
                        "какаовый цвет",
                        "какаовый бисквит"
                    ],
                    "target_lang_definitions": {
                        "en": "Prepared using cocoa powder or having its characteristic taste or color."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cocoa",
                                "target_lang_sense_clarification": "drink, powder, color"
                            }
                        ]
                    }
                }
            ]
        }

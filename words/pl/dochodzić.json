    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abce9b1e-b223-3416-8729-0a0df59a4eed",
                    "sense_definition": "Zbliżać się do określonego miejsca lub celu, przemieszczając się pieszo.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony turysta powoli <w>dochodził</w> do schroniska na szczycie góry.",
                            "target_lang_translations": {
                                "ru": "Уставший турист медленно <w>доходил</w> до приюта на вершине горы."
                            }
                        },
                        {
                            "text": "Codziennie rano <w>dochodzę</w> do przystanku autobusowego w pięć minut.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <w>дохожу</w> до автобусной остановки за пять минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbliżać się",
                        "podchodzić"
                    ],
                    "antonyms": [
                        "oddalać się",
                        "odchodzić"
                    ],
                    "common_phrases": [
                        "dochodzić do domu",
                        "dochodzić do celu",
                        "dochodzić do ściany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приближаться к определенному месту или цели пешком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "до места"
                            },
                            {
                                "target_lang_word": "добираться",
                                "target_lang_sense_clarification": "с трудом или издалека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65eca396-5e26-3565-bb9f-cc02ff7b5f72",
                    "sense_definition": "O czasie: zbliżać się do określonej godziny lub momentu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Już <w>dochodzi</w> osiemnasta, więc musimy zacząć zbierać się do wyjścia.",
                            "target_lang_translations": {
                                "ru": "Уже <w>подходит</w> восемнадцать часов, поэтому нам нужно начинать собираться."
                            }
                        },
                        {
                            "text": "Widzę na zegarku, że <w>dochodzi</w> północ, a my wciąż pracujemy.",
                            "target_lang_translations": {
                                "ru": "Я вижу на часах, что <w>близится</w> полночь, а мы всё ещё работаем."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbliżać się",
                        "wybijać"
                    ],
                    "common_phrases": [
                        "dochodzi godzina",
                        "dochodzi południe",
                        "dochodzi czas"
                    ],
                    "target_lang_definitions": {
                        "ru": "О времени: приближаться к определенному часу или моменту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "близиться",
                                "target_lang_sense_clarification": "приближаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67b16128-5064-3768-882d-3c18bd72ef99",
                    "sense_definition": "Osiągać pewien stan, rezultat lub wyciągać wniosek w wyniku jakiegoś procesu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej dyskusji w końcu <w>dochodzimy</w> do porozumienia w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "После долгой дискуссии мы наконец <w>приходим</w> к соглашению по этому вопросу."
                            }
                        },
                        {
                            "text": "Pacjent powoli <w>dochodzi</w> do siebie po skomplikowanej operacji.",
                            "target_lang_translations": {
                                "ru": "Пациент медленно <w>приходит</w> в себя после сложной операции."
                            }
                        }
                    ],
                    "synonyms": [
                        "osiągać",
                        "uzyskiwać"
                    ],
                    "common_phrases": [
                        "dochodzić do wniosku",
                        "dochodzić do porozumienia",
                        "dochodzić do siebie",
                        "dochodzić do wprawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать определенного состояния, результата или делать вывод в результате процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "к выводу, к соглашению, в себя"
                            },
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "соглашения, результата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "078240b8-7777-3755-993a-fca9908db86d",
                    "sense_definition": "Być słyszalnym, widocznym lub stawać się wiadomym (o dźwiękach, zapachach, informacjach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zza zamkniętych drzwi <w>dochodziły</w> do nas głośne krzyki i śmiechy.",
                            "target_lang_translations": {
                                "ru": "Из-за закрытых дверей до нас <w>доносились</w> громкие крики и смех."
                            }
                        },
                        {
                            "text": "Do mieszkańców miasta <w>dochodzą</w> niepokojące informacje o planowanych podwyżkach.",
                            "target_lang_translations": {
                                "ru": "До жителей города <w>доходят</w> тревожные сведения о планируемом повышении цен."
                            }
                        }
                    ],
                    "synonyms": [
                        "docierać",
                        "dobiegać"
                    ],
                    "common_phrases": [
                        "dochodzą słuchy",
                        "dochodzą wieści",
                        "dochodzić do uszu",
                        "dochodzić do głosu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть слышным, видным или становиться известным (о звуках, запахах, информации)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доноситься",
                                "target_lang_sense_clarification": "о звуках, запахах"
                            },
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "об информации, слухах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cfa4133-fb2d-3fcd-942c-c0099e53cd73",
                    "sense_definition": "Osiągać pewną wielkość, liczbę, poziom lub granicę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Temperatura w cieniu <w>dochodzi</w> dzisiaj do trzydziestu stopni Celsjusza.",
                            "target_lang_translations": {
                                "ru": "Температура в тени сегодня <w>доходит</w> до тридцати градусов Цельсия."
                            }
                        },
                        {
                            "text": "Liczba uczestników koncertu <w>dochodziła</w> do tysiąca osób.",
                            "target_lang_translations": {
                                "ru": "Число участников концерта <w>доходило</w> до тысячи человек."
                            }
                        }
                    ],
                    "synonyms": [
                        "sięgać",
                        "wynosić"
                    ],
                    "common_phrases": [
                        "dochodzić do granicy",
                        "dochodzić do perfekcji",
                        "dochodzić do setki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать определенной величины, числа, уровня или границы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "до уровня, числа"
                            },
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "величины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cde867b-b570-3ca3-8b8b-013045f5bcc1",
                    "sense_definition": "O potrawach: być w końcowej fazie gotowania lub pieczenia, stawać się gotowym do spożycia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostaw mięso w piekarniku, niech powoli <w>dochodzi</w> w niższej temperaturze.",
                            "target_lang_translations": {
                                "ru": "Оставь мясо в духовке, пусть оно медленно <w>доходит</w> при более низкой температуре."
                            }
                        },
                        {
                            "text": "Ryż musi jeszcze chwilę <w>dochodzić</w> pod przykryciem, żeby wchłonął całą wodę.",
                            "target_lang_translations": {
                                "ru": "Рис должен еще немного <w>доходить</w> под крышкой, чтобы впитать всю воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "dojrzewać",
                        "mięknąć"
                    ],
                    "common_phrases": [
                        "ciasto dochodzi",
                        "zupa dochodzi",
                        "dochodzić w piekarniku",
                        "dochodzić na wolnym ogniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "О блюдах: быть в конечной стадии приготовления, становиться готовым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "кулинарное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "978cd569-8335-3ae7-bfda-b09c25cb89f5",
                    "sense_definition": "Starać się uzyskać coś, co się komuś należy, np. sprawiedliwość lub odszkodowanie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poszkodowany pracownik będzie <w>dochodził</w> swoich praw przed sądem pracy.",
                            "target_lang_translations": {
                                "ru": "Пострадавший работник будет <w>добиваться</w> своих прав в суде по трудовым спорам."
                            }
                        },
                        {
                            "text": "Wiele lat <w>dochodzili</w> sprawiedliwości, zanim otrzymali należne im odszkodowanie.",
                            "target_lang_translations": {
                                "ru": "Много лет они <w>добивались</w> справедливости, прежде чем получили причитающуюся им компенсацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "domagać się",
                        "żądać"
                    ],
                    "antonyms": [
                        "zrzekać się",
                        "rezygnować"
                    ],
                    "common_phrases": [
                        "dochodzić swoich praw",
                        "dochodzić sprawiedliwości",
                        "dochodzić roszczeń",
                        "dochodzić odszkodowania"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться получить то, что причитается (например, справедливость или компенсацию)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добиваться",
                                "target_lang_sense_clarification": "прав, справедливости"
                            },
                            {
                                "target_lang_word": "требовать",
                                "target_lang_sense_clarification": "компенсации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fea0e4e-a7cc-3a11-88b0-3580adb0c07e",
                    "sense_definition": "Umierać, kończyć życie (często powoli i w cierpieniu).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary król <w>dochodził</w> swych dni w otoczeniu wiernych sług.",
                            "target_lang_translations": {
                                "ru": "Старый король <w>угасал</w> в окружении верных слуг."
                            }
                        },
                        {
                            "text": "Ranny żołnierz <w>dochodził</w> na polu bitwy, czekając na pomoc medyczną.",
                            "target_lang_translations": {
                                "ru": "Раненый солдат <w>умирал</w> на поле битвы, ожидая медицинской помощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "umierać",
                        "konać",
                        "gasnąć"
                    ],
                    "common_phrases": [
                        "dochodzić swych dni",
                        "dochodzić kresu życia"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often literary or euphemistic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умирать, заканчивать жизнь (часто медленно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угасать",
                                "target_lang_sense_clarification": "медленно умирать"
                            },
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "общая"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "начало",
        "начинание",
        "начальник",
        "начальный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e130266-195d-309b-944e-daf29d5976bb",
                    "sense_definition": "Приступать к какому-либо действию или занятию",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы всегда <начинаем> рабочий день с проверки почты.",
                            "target_lang_translations": {
                                "en": "We always <start> the workday by checking our email."
                            }
                        },
                        {
                            "text": "Пора <начинать> готовиться к предстоящим экзаменам.",
                            "target_lang_translations": {
                                "en": "It's time to <start> preparing for the upcoming exams."
                            }
                        }
                    ],
                    "synonyms": [
                        "приступать"
                    ],
                    "antonyms": [
                        "заканчивать",
                        "кончать",
                        "завершать"
                    ],
                    "common_phrases": [
                        "начинать работу",
                        "начинать разговор",
                        "начинать сначала",
                        "начинать с нуля"
                    ],
                    "target_lang_definitions": {
                        "en": "To start doing something or taking action"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "most common, general term"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "slightly more formal than start"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2517f177-2916-3202-9f13-18caa51e1cbe",
                    "sense_definition": "Проявлять первые признаки какого-либо действия или состояния",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой на улице очень рано <начинает> темнеть.",
                            "target_lang_translations": {
                                "en": "In winter, it <starts> getting dark outside very early."
                            }
                        },
                        {
                            "text": "У меня <начинает> болеть голова от громкого шума.",
                            "target_lang_translations": {
                                "en": "My head is <starting> to ache from the loud noise."
                            }
                        }
                    ],
                    "common_phrases": [
                        "начинает темнеть",
                        "начинает казаться",
                        "начинает холодать"
                    ],
                    "target_lang_definitions": {
                        "en": "To begin happening or showing signs of a state"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "used for processes, weather, or states"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "synonym for start"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93f7a836-c4e0-3713-9bec-143d074019fc",
                    "sense_definition": "Заниматься кем-либо или чем-либо в первую очередь, раньше других",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Любые изменения в жизни нужно <начинать> с себя.",
                            "target_lang_translations": {
                                "en": "Any changes in life need to <start> with yourself."
                            }
                        },
                        {
                            "text": "Учитель решил <начинать> опрос с новых учеников.",
                            "target_lang_translations": {
                                "en": "The teacher decided to <start> the quiz with the new students."
                            }
                        }
                    ],
                    "common_phrases": [
                        "начинать с себя",
                        "начинать с главного"
                    ],
                    "target_lang_definitions": {
                        "en": "To deal with someone or something first"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "usually followed by 'with'"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "usually followed by 'with'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "357c8daa-8f2c-3275-95b6-acbc773d0ba5",
                    "sense_definition": "Приступать к потреблению или использованию чего-либо",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "consumption_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили не <начинать> праздничный торт до прихода гостей.",
                            "target_lang_translations": {
                                "en": "We decided not to <start> the celebration cake until the guests arrived."
                            }
                        },
                        {
                            "text": "Не стоит <начинать> вторую бутылку воды, пока не допили первую.",
                            "target_lang_translations": {
                                "en": "You shouldn't <start> a second bottle of water until you've finished the first one."
                            }
                        }
                    ],
                    "common_phrases": [
                        "начинать новую пачку",
                        "начинать хлеб"
                    ],
                    "target_lang_definitions": {
                        "en": "To start eating, drinking, or using something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "to begin consuming or using"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
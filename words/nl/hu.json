{
    "word_family": [
        "huiveren",
        "huivering"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "fd0df98c-ee6a-3c98-a36f-ceed52e635ab",
                    "sense_definition": "Uitroep van huivering door angst, kou of afschuw.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hu</w>, wat is het hier ontzettend koud en donker!",
                            "target_lang_translations": {
                                "ru": "<w>Брр</w>, как же здесь холодно и темно!",
                                "en": "<w>Brr</w>, it is terribly cold and dark here!"
                            }
                        },
                        {
                            "text": "Toen ik die grote spin zag lopen, riep ik van schrik <w>hu</w>.",
                            "target_lang_translations": {
                                "ru": "Когда я увидел этого огромного ползущего паука, я от страха вскрикнул <w>брр</w>.",
                                "en": "When I saw that big spider walking, I cried out <w>yikes</w> in fright."
                            }
                        }
                    ],
                    "synonyms": [
                        "brr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, выражающий дрожь от страха, холода или отвращения.",
                        "en": "Exclamation expressing shivering due to fear, cold, or disgust."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брр",
                                "target_lang_sense_clarification": "выражение холода или отвращения"
                            },
                            {
                                "target_lang_word": "ой",
                                "target_lang_sense_clarification": "выражение внезапного испуга"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "brr",
                                "target_lang_sense_clarification": "expression of cold or shivering"
                            },
                            {
                                "target_lang_word": "ugh",
                                "target_lang_sense_clarification": "expression of disgust"
                            },
                            {
                                "target_lang_word": "yikes",
                                "target_lang_sense_clarification": "expression of sudden fright"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9659536-4b19-3744-a09a-518fa65f3ca6",
                    "sense_definition": "Uitroep om een trekdier aan te sporen of (afhankelijk van de toon) juist te laten stoppen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koetsier riep luid <w>hu</w> om het paard in beweging te krijgen.",
                            "target_lang_translations": {
                                "ru": "Кучер громко крикнул <w>но</w>, чтобы лошадь тронулась с места.",
                                "en": "The coachman shouted <w>giddyup</w> loudly to get the horse moving."
                            }
                        },
                        {
                            "text": "Met een krachtig <w>hu</w> spoorde de boer zijn ossen aan op het veld.",
                            "target_lang_translations": {
                                "ru": "Мощным <w>но</w> крестьянин подгонял своих быков в поле.",
                                "en": "With a powerful <w>gee up</w>, the farmer urged his oxen on in the field."
                            }
                        }
                    ],
                    "synonyms": [
                        "hortsik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый для понукания тяглового животного или (в зависимости от тона) для его остановки.",
                        "en": "Exclamation used to urge a draft animal on or (depending on tone) to stop it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "команда животному двигаться"
                            },
                            {
                                "target_lang_word": "тпру",
                                "target_lang_sense_clarification": "команда животному остановиться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "giddyup",
                                "target_lang_sense_clarification": "command to move"
                            },
                            {
                                "target_lang_word": "gee up",
                                "target_lang_sense_clarification": "command to move (often for oxen/horses)"
                            },
                            {
                                "target_lang_word": "whoa",
                                "target_lang_sense_clarification": "command to stop"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "zeilboot",
        "zeilschip",
        "zeiler",
        "dekzeil",
        "zeilmaker",
        "zeilsteen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7b9e5cad-f584-3b75-add5-f8c6b172616b",
                    "sense_definition": "Een groot stuk stevig doek dat aan de mast van een boot wordt vastgemaakt om wind te vangen en het schip vooruit te bewegen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "nautical_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos hees het <w>zeil</w> toen de wind begon te waaien.",
                            "target_lang_translations": {
                                "ru": "Матрос поднял <w>парус</w>, когда начал дуть ветер."
                            }
                        },
                        {
                            "text": "Het bedrijf moest alle zeilen bijzetten om het faillissement te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Компании пришлось поднять все <w>паруса</w>, чтобы избежать банкротства."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheepszeil"
                    ],
                    "common_phrases": [
                        "alle zeilen bijzetten",
                        "de wind in de zeilen hebben",
                        "het zeil strijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое полотно из прочной ткани, которое крепится к мачте лодки, чтобы ловить ветер и приводить судно в движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парус",
                                "target_lang_sense_clarification": "часть судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8e06967-afa9-3866-9943-2888980f690a",
                    "sense_definition": "Een waterdicht doek dat wordt gebruikt om voorwerpen af te dekken en te beschermen tegen regen of vuil.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "covering_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We spanden een blauw <w>zeil</w> over de aanhangwagen om de lading droog te houden.",
                            "target_lang_translations": {
                                "ru": "Мы натянули синий <w>брезент</w> на прицеп, чтобы сохранить груз сухим."
                            }
                        },
                        {
                            "text": "Tijdens de storm zaten we droog onder een groot stuk <w>zeil</w>.",
                            "target_lang_translations": {
                                "ru": "Во время шторма мы сидели в сухости под большим куском <w>брезента</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dekkleed",
                        "dekzeil"
                    ],
                    "common_phrases": [
                        "onder zeil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Водонепроницаемое полотно, используемое для укрытия предметов и защиты их от дождя или грязи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брезент",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "тент",
                                "target_lang_sense_clarification": "навес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a967d57-3747-30d1-b472-702365c29819",
                    "sense_definition": "Een gladde vloerbedekking gemaakt van kunststof die makkelijk schoon te maken is.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "flooring_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de keuken hebben we nieuw <w>zeil</w> met een houten patroon laten leggen.",
                            "target_lang_translations": {
                                "ru": "На кухне мы постелили новый <w>линолеум</w> с рисунком под дерево."
                            }
                        },
                        {
                            "text": "Het oude <w>zeil</w> in de gang was versleten en moest worden vervangen.",
                            "target_lang_translations": {
                                "ru": "Старый <w>линолеум</w> в коридоре износился, и его нужно было заменить."
                            }
                        }
                    ],
                    "synonyms": [
                        "linoleum",
                        "vinyl"
                    ],
                    "common_phrases": [
                        "zeil leggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гладкое напольное покрытие из синтетического материала, которое легко мыть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "линолеум",
                                "target_lang_sense_clarification": "покрытие пола"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc60b8eb-550c-36fe-acb5-256effc14379",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>zeilen</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zeil</w> elke zomer met mijn familie op de Friese meren.",
                            "target_lang_translations": {
                                "ru": "Каждое лето я <w>хожу под парусом</w> с семьей по Фризским озерам."
                            }
                        },
                        {
                            "text": "Als het mooi weer is, <w>zeil</w> ik graag naar de eilanden.",
                            "target_lang_translations": {
                                "ru": "В хорошую погоду я люблю <w>ходить под парусом</w> к островам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>zeilen</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хожу под парусом",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "плаваю под парусом",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4262d1e2-29b5-3b1d-84a7-14b2b464cd53",
                    "sense_definition": "Gebiedende wijs van <w>zeilen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zeil</w> voorzichtig, want er staat vandaag een harde wind!",
                            "target_lang_translations": {
                                "ru": "<w>Иди под парусом</w> осторожно, так как сегодня сильный ветер!"
                            }
                        },
                        {
                            "text": "<w>Zeil</w> naar de overkant en wacht daar op de rest van de groep.",
                            "target_lang_translations": {
                                "ru": "<w>Плыви</w> на ту сторону и жди там остальную часть группы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>zeilen</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плыви",
                                "target_lang_sense_clarification": "команда"
                            },
                            {
                                "target_lang_word": "иди под парусом",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "skromny",
        "skromnie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "272548b9-9f74-3eb7-b1d4-277a3c4f6a8a",
                    "sense_definition": "Cecha kogoś, kto nie wywyższa się, nie dba o rozgłos i nie oczekuje uznania od innych, mimo posiadanych zalet lub osiągnięć.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wrodzona <w>skromność</w> sprawia, że nie lubi opowiadać o swoich sukcesach.",
                            "target_lang_translations": {
                                "ru": "Его врожденная <w>скромность</w> приводит к тому, что он не любит рассказывать о своих успехах."
                            }
                        },
                        {
                            "text": "Przez fałszywą <w>skromność</w> nie przyznała się, że to ona napisała ten wiersz.",
                            "target_lang_translations": {
                                "ru": "Из-за ложной <w>скромности</w> она не призналась, что это она написала это стихотворение."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokora",
                        "umiar",
                        "cichość"
                    ],
                    "antonyms": [
                        "pycha",
                        "zarozumiałość",
                        "próżność",
                        "buta",
                        "chełpliwość",
                        "megalomania",
                        "wyniosłość"
                    ],
                    "common_phrases": [
                        "fałszywa skromność",
                        "wrodzona skromność",
                        "zbytnia skromność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество человека, который не превозносит себя, не ищет славы и не ждет признания от других, несмотря на свои достоинства или достижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скромность",
                                "target_lang_sense_clarification": "черта характера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0beb995c-a5ae-32eb-941f-0ecf928ae45c",
                    "sense_definition": "Cecha czegoś, co jest proste, pozbawione zbędnych ozdób, przepychu lub nie rzuca się w oczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "style_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wnętrze tego domu urządzono z dużą <w>skromnością</w> i smakiem.",
                            "target_lang_translations": {
                                "ru": "Интерьер этого дома был оформлен с большой <w>скромностью</w> и вкусом."
                            }
                        },
                        {
                            "text": "<w>Skromność</w> jej ubioru kontrastowała z bogatą biżuterią innych gości.",
                            "target_lang_translations": {
                                "ru": "<w>Скромность</w> её наряда контрастировала с богатыми украшениями других гостей."
                            }
                        },
                        {
                            "text": "Ceremonia przebiegła z zachowaniem pełnej <w>skromności</w>.",
                            "target_lang_translations": {
                                "ru": "Церемония прошла с соблюдением полной <w>скромности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "prostota",
                        "zwyczajność",
                        "zwykłość",
                        "brak wyszukania"
                    ],
                    "antonyms": [
                        "strojność",
                        "ozdobność",
                        "dekoracyjność",
                        "wyszukanie",
                        "efektowność"
                    ],
                    "common_phrases": [
                        "skromność ubioru",
                        "skromność wystroju",
                        "cechować się skromnością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство чего-то простого, лишенного лишних украшений, роскоши или не бросающегося в глаза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скромность",
                                "target_lang_sense_clarification": "внешний вид, стиль"
                            },
                            {
                                "target_lang_word": "простота",
                                "target_lang_sense_clarification": "отсутствие роскоши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "732b45b4-74ae-32a0-ac6f-c531469474de",
                    "sense_definition": "Cecha kogoś, kto jest powściągliwy, wstydliwy lub przestrzega norm moralnych, zwłaszcza w sferze relacji damsko-męskich.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice cenili u niej przede wszystkim <w>skromność</w> i dobre wychowanie.",
                            "target_lang_translations": {
                                "ru": "Родители ценили в ней прежде всего <w>скромность</w> и хорошее воспитание."
                            }
                        },
                        {
                            "text": "Młoda dziewczyna powinna zachować <w>skromność</w> w swoim zachowaniu.",
                            "target_lang_translations": {
                                "ru": "Молодая девушка должна сохранять <w>скромность</w> в своем поведении."
                            }
                        }
                    ],
                    "synonyms": [
                        "wstydliwość",
                        "obyczajność",
                        "przyzwoitość",
                        "niewinność",
                        "powściągliwość"
                    ],
                    "antonyms": [
                        "bezwstyd",
                        "nieobyczajność",
                        "nieprzyzwoitość",
                        "zuchwałość",
                        "śmiałość"
                    ],
                    "common_phrases": [
                        "cnota skromności",
                        "zachować skromność",
                        "granice skromności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество человека, который сдержан, застенчив или соблюдает моральные нормы, особенно в отношениях между полами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скромность",
                                "target_lang_sense_clarification": "поведение"
                            },
                            {
                                "target_lang_word": "сдержанность",
                                "target_lang_sense_clarification": "в проявлении чувств"
                            },
                            {
                                "target_lang_word": "целомудрие",
                                "target_lang_sense_clarification": "нравственная чистота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3119544-2e60-350a-8d34-ec951d6a6fe7",
                    "sense_definition": "Cecha czegoś, co ma małą wartość materialną, jest niewielkie lub niewystarczające.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Skromność</w> domowego budżetu nie pozwoliła im na zagraniczne wakacje.",
                            "target_lang_translations": {
                                "ru": "<w>Скромность</w> семейного бюджета не позволила им поехать в отпуск за границу."
                            }
                        },
                        {
                            "text": "Projekt upadł przez <w>skromność</w> dostępnych środków finansowych.",
                            "target_lang_translations": {
                                "ru": "Проект провалился из-за <w>скромности</w> доступных финансовых средств."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubóstwo",
                        "niedostatek",
                        "szczupłość"
                    ],
                    "antonyms": [
                        "bogactwo",
                        "dostatek"
                    ],
                    "common_phrases": [
                        "skromność budżetu",
                        "skromność środków",
                        "skromność zasobów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство чего-то, что имеет малую материальную ценность, небольшой размер или является недостаточным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скромность",
                                "target_lang_sense_clarification": "ограниченность ресурсов"
                            },
                            {
                                "target_lang_word": "скудость",
                                "target_lang_sense_clarification": "недостаток"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
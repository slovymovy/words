{
    "word_family": [
        "skąpstwo",
        "skąpiec",
        "skąpo",
        "skąpić"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cae32542-6591-3acd-864f-2124797fbf49",
                    "sense_definition": "Taki, który przesadnie oszczędza i niechętnie wydaje pieniądze lub dzieli się czymś z innymi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cecha_charakteru",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest tak <w>skąpy</w>, że nigdy nie zostawia napiwków w restauracji.",
                            "target_lang_translations": {
                                "ru": "Мой сосед настолько <w>скупой</w>, что никогда не оставляет чаевых в ресторане."
                            }
                        },
                        {
                            "text": "Nie bądź taki <w>skąpy</w> i podziel się cukierkami ze swoją młodszą siostrą.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>жадным</w> и поделись конфетами со своей младшей сестрой."
                            }
                        }
                    ],
                    "synonyms": [
                        "chytry",
                        "centusiowski"
                    ],
                    "antonyms": [
                        "rozrzutny"
                    ],
                    "common_phrases": [
                        "skąpy człowiek",
                        "skąpy dla rodziny",
                        "skąpy grosz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто чрезмерно экономит и неохотно тратит деньги или делится чем-либо с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скупой",
                                "target_lang_sense_clarification": "чрезмерно бережливый, избегающий расходов"
                            },
                            {
                                "target_lang_word": "жадный",
                                "target_lang_sense_clarification": "стремящийся получить как можно больше, не желающий делиться"
                            },
                            {
                                "target_lang_word": "скаредный",
                                "target_lang_sense_clarification": "разг. очень скупой, мелочный в тратах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dab712d9-78b8-3162-ac2d-30b2034977a4",
                    "sense_definition": "Występujący w bardzo małej ilości, niewystarczający lub (o ubraniu) krótki i odsłaniający ciało.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ilość_rozmiar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja posiada tylko <w>skąpe</w> informacje na temat sprawcy tego wypadku.",
                            "target_lang_translations": {
                                "ru": "У полиции есть только <w>скупые</w> сведения о виновнике этой аварии."
                            }
                        },
                        {
                            "text": "Aktorka założyła bardzo <w>skąpy</w> strój, który przyciągał uwagę wszystkich fotoreporterów.",
                            "target_lang_translations": {
                                "ru": "Актриса надела очень <w>откровенный</w> наряд, который привлекал внимание всех фоторепортеров."
                            }
                        }
                    ],
                    "synonyms": [
                        "mały",
                        "niedostateczny",
                        "ograniczony"
                    ],
                    "antonyms": [
                        "obfity"
                    ],
                    "common_phrases": [
                        "skąpy strój",
                        "skąpe oświetlenie",
                        "skąpa wiedza",
                        "skąpe dochody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий в очень малом количестве, недостаточный или (об одежде) короткий и открывающий тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скудный",
                                "target_lang_sense_clarification": "недостаточный по количеству, бедный (о запасах, растительности)"
                            },
                            {
                                "target_lang_word": "скупой",
                                "target_lang_sense_clarification": "недостаточный, редкий (о свете, слезах, информации)"
                            },
                            {
                                "target_lang_word": "откровенный",
                                "target_lang_sense_clarification": "об одежде: слишком открытый, короткий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
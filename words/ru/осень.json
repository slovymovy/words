            "senses": [
                {
                    "sense_id": "f6bc7c8a-f59d-3bb9-a0a1-6f0aed1487d9",
                    "sense_definition": "Время года между летом и зимой.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наступила золотая <w>осень</w>, и парки стали очень красивыми.",
                            "target_lang_translations": {
                                "en": "Golden <w>autumn</w> has arrived, and the parks have become very beautiful."
                            }
                        },
                        {
                            "text": "<w>Осень</w> приносит с собой дожди и прохладную погоду.",
                            "target_lang_translations": {
                                "en": "<w>Autumn</w> brings rain and cool weather with it."
                            }
                        }
                    ],
                    "antonyms": [
                        "весна"
                    ],
                    "common_phrases": [
                        "золотая осень",
                        "поздняя осень",
                        "наступила осень",
                        "дождливая осень"
                    ],
                    "target_lang_definitions": {
                        "en": "The season of the year between summer and winter."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "autumn",
                                "target_lang_sense_clarification": "season"
                            },
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "season (US)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5f62745-8670-357b-86ae-baeaad675b21",
                    "sense_definition": "Период увядания, старости или заката жизни (переносное значение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это была <w>осень</w> его карьеры, когда он решил уйти на покой.",
                            "target_lang_translations": {
                                "en": "It was the <w>autumn</w> of his career when he decided to retire."
                            }
                        },
                        {
                            "text": "<w>Осень</w> жизни нужно принимать с благодарностью и мудростью.",
                            "target_lang_translations": {
                                "en": "The <w>autumn</w> of life must be accepted with gratitude and wisdom."
                            }
                        }
                    ],
                    "synonyms": [
                        "закат",
                        "увядание"
                    ],
                    "antonyms": [
                        "расцвет",
                        "весна"
                    ],
                    "common_phrases": [
                        "осень жизни"
                    ],
                    "target_lang_definitions": {
                        "en": "A period of decline, old age, or the final stage of life (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "autumn",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04a74573-fc11-3ce3-b2f2-44531fce93bb",
                    "sense_definition": "Год (используется охотниками при указании возраста животных).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этой охотничьей собаке пошла уже третья <w>осень</w>.",
                            "target_lang_translations": {
                                "en": "This hunting dog is already in its third <w>year</w>."
                            }
                        },
                        {
                            "text": "Старому волку, которого мы видели, наверное, десятая <w>осень</w>.",
                            "target_lang_translations": {

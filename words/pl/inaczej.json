{
    "word_family": [
        "inny",
        "inność"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4f2de8c9-939c-330b-9ba2-62f8d334c591",
                    "sense_definition": "W sposób odmienny, różniący się od czegoś lub kogoś; nie tak samo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy z nas widzi ten problem nieco <inaczej>.",
                            "target_lang_translations": {
                                "ru": "Каждый из нас видит эту проблему немного <иначе>."
                            }
                        },
                        {
                            "text": "Musimy to zrobić <inaczej>, żeby wszystko zadziałało.",
                            "target_lang_translations": {
                                "ru": "Мы должны сделать это <по-другому>, чтобы всё заработало."
                            }
                        }
                    ],
                    "synonyms": [
                        "odmiennie",
                        "odwrotnie",
                        "przeciwnie"
                    ],
                    "antonyms": [
                        "tak samo",
                        "identycznie",
                        "podobnie",
                        "jednakowo"
                    ],
                    "common_phrases": [
                        "zupełnie inaczej",
                        "wyglądać inaczej",
                        "myśleć inaczej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иным образом, не так, как раньше или как кто-то другой; по-другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иначе",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "по-другому",
                                "target_lang_sense_clarification": "синоним"
                            },
                            {
                                "target_lang_word": "по-иному",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "5cc51b08-7b3f-3bd9-948f-0976b6ffe79a",
                    "sense_definition": "Słowo łączące zdania, wskazujące na negatywne konsekwencje niespełnienia warunku; w przeciwnym razie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz się pospieszyć, <inaczej> ucieknie nam ostatni autobus.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно поторопиться, <иначе> наш последний автобус уйдёт."
                            }
                        },
                        {
                            "text": "Załóż ciepłą czapkę, bo <inaczej> się przeziębisz.",
                            "target_lang_translations": {
                                "ru": "Надень тёплую шапку, потому что <иначе> ты простудишься."
                            }
                        }
                    ],
                    "synonyms": [
                        "w przeciwnym razie",
                        "w przeciwnym wypadku"
                    ],
                    "common_phrases": [
                        "bo inaczej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, связывающий предложения и указывающий на негативные последствия невыполнения условия; в противном случае."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иначе",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "а то",
                                "target_lang_sense_clarification": "разговорный вариант"
                            },
                            {
                                "target_lang_word": "в противном случае",
                                "target_lang_sense_clarification": "формальный вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "58f40c30-7a14-3c32-ab77-dbff78cba652",
                    "sense_definition": "Wyrażenie wprowadzające wyjaśnienie lub inne sformułowanie tego, co zostało powiedziane wcześniej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "explanation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest nasz priorytet, <inaczej> mówiąc, najważniejsza sprawa.",
                            "target_lang_translations": {
                                "ru": "Это наш приоритет, <иначе> говоря, самое важное дело."
                            }
                        },
                        {
                            "text": "Kwas askorbinowy, <inaczej> witamina C, jest niezbędny dla zdrowia.",
                            "target_lang_translations": {
                                "ru": "Аскорбиновая кислота, <иначе> (или) витамин С, необходима для здоровья."
                            }
                        }
                    ],
                    "synonyms": [
                        "czyli",
                        "to znaczy",
                        "innymi słowy",
                        "inaczej mówiąc"
                    ],
                    "common_phrases": [
                        "inaczej mówiąc",
                        "inaczej zwany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение, вводящее пояснение или другое название того, что было сказано ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иначе",
                                "target_lang_sense_clarification": "в выражениях 'иначе говоря', 'иначе называемый'"
                            },
                            {
                                "target_lang_word": "другими словами",
                                "target_lang_sense_clarification": "смысловой эквивалент"
                            },
                            {
                                "target_lang_word": "то есть",
                                "target_lang_sense_clarification": "пояснение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb083896-f39e-3161-9054-4edec692427f",
                    "sense_definition": "Słowo używane żartobliwie lub ironicznie, aby zaprzeczyć cesze wymienionej bezpośrednio przed nim.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "irony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten pomysł jest, delikatnie mówiąc, mądry <inaczej>.",
                            "target_lang_translations": {
                                "ru": "Эта идея, мягко говоря, умная <иначе> (глупая)."
                            }
                        },
                        {
                            "text": "Jego zachowanie było kulturalne <inaczej>, co wszystkich oburzyło.",
                            "target_lang_translations": {
                                "ru": "Его поведение было культурным <иначе> (бескультурным), что всех возмутило."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inteligentny inaczej"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane ironicznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое шутливо или иронично для отрицания положительного качества, упомянутого перед ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иначе",
                                "target_lang_sense_clarification": "ироничное использование"
                            },
                            {
                                "target_lang_word": "альтернативно",
                                "target_lang_sense_clarification": "в ироничных кальках (напр. альтернативно одарённый)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "synonyms": [
                        "vanzelfsprekend",
                        "onvermijdelijk"
                    ],
                    "common_phrases": [
                        "automatisch gevolg",
                        "automatische incasso"
                    ],
                    "target_lang_definitions": {
                        "en": "Occurring naturally or as a necessary consequence.",
                        "ru": "Происходящий сам по себе или следующий как логический результат чего-то другого."

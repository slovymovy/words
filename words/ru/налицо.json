                                "target_lang_sense_clarification": "easily perceived or understood"
                            },
                            {
                                "target_lang_word": "plain to see",
                                "target_lang_sense_clarification": "clearly visible or apparent"
                            },
                            {
                                "target_lang_word": "in plain sight",
                                "target_lang_sense_clarification": "impossible to miss; clearly visible"
                            }
                        ]
                    }
                },
                {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc98380a-d3aa-329f-a2e4-2f0b2aec3dcb",
                    "sense_definition": "Lampa uliczna umieszczona na wysokim słupie, służąca do oświetlania dróg, chodników i placów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lighting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem na naszej ulicy zapala się każda <w>latarnia</w>.",
                            "target_lang_translations": {
                                "ru": "Вечером на нашей улице загорается каждый <w>фонарь</w>."
                            }
                        },
                        {
                            "text": "Czekam na ciebie pod <w>latarnią</w> przy wejściu do parku.",
                            "target_lang_translations": {
                                "ru": "Я жду тебя под <w>фонарём</w> у входа в парк."
                            }
                        }
                    ],
                    "common_phrases": [
                        "latarnia uliczna",
                        "pod latarnią",
                        "światło latarni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уличный светильник на высоком столбе, служащий для освещения дорог, тротуаров и площадей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонарь",
                                "target_lang_sense_clarification": "уличный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ae8fea2-23c1-38e2-82d7-788b1cc5747a",
                    "sense_definition": "Budowla nawigacyjna w postaci wieży na wybrzeżu, wysyłająca sygnały świetlne dla statków.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "navigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara <w>latarnia</w> morska jest teraz popularną atrakcją turystyczną.",
                            "target_lang_translations": {
                                "ru": "Старый <w>маяк</w> сейчас является популярной туристической достопримечательностью."
                            }
                        },
                        {
                            "text": "Podczas gęstej mgły <w>latarnia</w> ostrzega statki przed niebezpiecznymi skałami.",
                            "target_lang_translations": {
                                "ru": "В густом тумане <w>маяк</w> предостерегает суда от опасных скал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "latarnia morska",
                        "obsługa latarni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Навигационное сооружение в виде башни на побережье, посылающее световые сигналы судам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маяк",
                                "target_lang_sense_clarification": "навигационный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc980ee0-9101-3c6d-a14b-58413624ecb1",
                    "sense_definition": "Nadbudówka z oknami umieszczona na szczycie kopuły lub dachu, służąca do oświetlania wnętrza budynku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez renesansową <w>latarnię</w> do wnętrza kościoła wpada dużo słońca.",
                            "target_lang_translations": {
                                "ru": "Через ренессансный <w>фонарь</w> внутрь церкви попадает много солнца."
                            }
                        },
                        {
                            "text": "Architekt zaprojektował ozdobną <w>latarnię</w> wieńczącą kopułę katedry.",
                            "target_lang_translations": {
                                "ru": "Архитектор спроектировал декоративный <w>фонарь</w>, венчающий купол собора."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "architektura"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Надстройка с окнами на вершине купола или крыши, служащая для освещения внутреннего пространства здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонарь",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            },
                            {
                                "target_lang_word": "световой фонарь",
                                "target_lang_sense_clarification": "архитектура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2b98183-f824-3fc9-9c7e-a119b77ff1a5",
                    "sense_definition": "W języku łowieckim: oczy wilka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy dostrzegł w ciemnym lesie błyszczące <w>latarnie</w> drapieżnika.",
                            "target_lang_translations": {
                                "ru": "Охотник заметил в темном лесу сверкающие <w>глаза</w> хищника."
                            }
                        },
                        {
                            "text": "Wilcze <w>latarnie</w> odbijały światło księżyca w zimową noc.",
                            "target_lang_translations": {
                                "ru": "Волчьи <w>глаза</w> отражали свет луны в зимнюю ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "lampy"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "łowiectwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На охотничьем жаргоне: глаза волка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глаза",
                                "target_lang_sense_clarification": "волка (охотн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35e735a8-57f5-30d8-abb2-d81bd3ba86de",
                    "sense_definition": "Szeroka, jasna plama na głowie konia, obejmująca chrapy i oczy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "horse_markings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten młody ogier ma charakterystyczną <w>latarnię</w> na pysku.",
                            "target_lang_translations": {
                                "ru": "У этого молодого жеребца характерная <w>лысина</w> на морде."
                            }
                        },
                        {
                            "text": "Klacz z dużą białą <w>latarnią</w> wyróżniała się w stadzie.",
                            "target_lang_translations": {
                                "ru": "Кобыла с большой белой <w>лысиной</w> выделялась в табуне."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "jeździectwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкое светлое пятно на голове лошади, охватывающее ноздри и глаза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лысина",
                                "target_lang_sense_clarification": "отметина у лошади"
                            },
                            {
                                "target_lang_word": "фонарь",
                                "target_lang_sense_clarification": "обширная лысина (проф.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b361fa2e-c721-3cc6-95e5-de0e5c543541",
                    "sense_definition": "Kobieta zachowująca się swawolnie lub niegrzecznie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dialect_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi gadali, że z niej niezła <w>latarnia</w>.",
                            "target_lang_translations": {
                                "ru": "Соседи болтали, что она порядочная <w>гулящая</w>."
                            }
                        },
                        {
                            "text": "Uspokój się wreszcie, ty mała <w>latarnio</w>.",
                            "target_lang_translations": {
                                "ru": "Успокойся наконец, ты, маленькая <w>бесстыдница</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Śląsk Cieszyński"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, ведущая себя распутно или неприлично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распутница",
                                "target_lang_sense_clarification": "бранное"
                            },
                            {
                                "target_lang_word": "бесстыдница",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
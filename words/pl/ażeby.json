    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "df0c0e23-642d-3a52-94d5-61d354a2d071",
                    "sense_definition": "Spójnik wprowadzający zdanie podrzędne, wyrażający cel czynności lub treść życzenia, polecenia lub obawy; synonim słów 'aby', 'żeby'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobiliśmy wszystko, co w naszej mocy, <ażeby> goście czuli się u nas dobrze.",
                            "target_lang_translations": {
                                "ru": "Мы сделали всё, что было в наших силах, <w>чтобы</w> гости чувствовали себя у нас хорошо."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił, <ażeby> uczniowie otworzyli książki na stronie dziesiątej.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил, <w>чтобы</w> ученики открыли книги на десятой странице."
                            }
                        }
                    ],
                    "synonyms": [
                        "aby",
                        "żeby",
                        "by"
                    ],
                    "common_phrases": [
                        "starać się, ażeby",
                        "chcieć, ażeby",
                        "dbać, ażeby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий придаточное предложение, выражающий цель действия, желание, приказ или опасение; синоним слов 'aby', 'żeby'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "наиболее общий и нейтральный союз цели"
                            },
                            {
                                "target_lang_word": "дабы",
                                "target_lang_sense_clarification": "книжный или устаревший вариант, придающий торжественность"
                            },
                            {
                                "target_lang_word": "для того чтобы",
                                "target_lang_sense_clarification": "составной союз, подчеркивающий цель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30601d99-7a0c-3392-91a7-5bb88ca4d14d",
                    "sense_definition": "Dawna forma spójnika <w>ażby</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych tekstach można napotkać zapisy, gdzie autor używał formy <ażeby> w znaczeniu archaicznym.",
                            "target_lang_translations": {
                                "ru": "В старых текстах можно встретить записи, где автор использовал форму <w>дабы</w> в архаичном значении."
                            }
                        },
                        {
                            "text": "Badacze literatury staropolskiej analizują konteksty, w których pojawiało się słowo <ażeby>.",
                            "target_lang_translations": {
                                "ru": "Исследователи старопольской литературы анализируют контексты, в которых появлялось слово <w>дабы</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшая форма союза <w>ażby</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дабы",
                                "target_lang_sense_clarification": "архаичный союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "11759fb2-b7fd-38b7-87f4-cd23882501f0",
                    "sense_definition": "Potoczne wyrażenie silnego sprzeciwu lub zaprzeczenia, oznaczające 'za nic w świecie' lub 'choćby nie wiem co'.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zgodzę się na takie warunki współpracy, <ażeby>!",
                            "target_lang_translations": {
                                "ru": "Я не соглашусь на такие условия сотрудничества, <w>ни за что</w>!"
                            }
                        },
                        {
                            "text": "Nie pójdę tam z nimi na to spotkanie, <ażeby>!",
                            "target_lang_translations": {
                                "ru": "Я не пойду туда с ними на эту встречу, <w>ещё чего</w>!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное выражение сильного протеста или отрицания, означающее «ни за что на свете» или «что бы ни случилось»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни за что",
                                "target_lang_sense_clarification": "категорический отказ"
                            },
                            {
                                "target_lang_word": "ещё чего",
                                "target_lang_sense_clarification": "разговорное выражение возмущения и отказа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "078181cb-b505-31b8-a349-5f3dbcfda7f0",
                    "sense_definition": "Weg van een plaats, oppervlak of object; duidt een verwijdering of neerwaartse beweging aan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep de trap <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Он спустился <w>вниз</w> по лестнице."
                            }
                        },
                        {
                            "text": "Haal je voeten van de bank <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Убери ноги <w>с</w> дивана."
                            }
                        }
                    ],
                    "antonyms": [
                        "onaf"
                    ],
                    "common_phrases": [
                        "op en af",
                        "van ... af"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочь от места, поверхности или объекта; указывает на удаление или движение вниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вниз",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "предлог удаления с поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9d6961b-2ae8-301a-83a3-cafc41598c58",
                    "sense_definition": "Als deel van een werkwoord of constructie: geeft aan dat iets voltooid wordt of verwijderd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij waste de vuile borden <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Она <w>вы</w>мыла грязные тарелки."
                            }
                        },
                        {
                            "text": "De verf gaat er moeilijk <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Краска плохо <w>от</w>ходит."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Как часть глагола или конструкции: указывает на то, что действие завершено или объект удален."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "удаление"
                            },
                            {
                                "target_lang_word": "долой",
                                "target_lang_sense_clarification": "удаление (экспрессивно)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4e047924-4090-3fb5-a2d5-3ee435991324",
                    "sense_definition": "Klaar of voltooid (van werk of een taak).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "finished",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is je huiswerk al <w>af</w>?",
                            "target_lang_translations": {
                                "ru": "Твоё домашнее задание уже <w>готово</w>?"
                            }
                        },
                        {
                            "text": "Het nieuwe gebouw is bijna <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Новое здание почти <w>готово</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "klaar"
                    ],
                    "antonyms": [
                        "onaf"
                    ],
                    "common_phrases": [
                        "bijna af",
                        "helemaal af"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый или завершённый (о работе или задаче)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готов",
                                "target_lang_sense_clarification": "сделанный"
                            },
                            {
                                "target_lang_word": "закончен",
                                "target_lang_sense_clarification": "завершённый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "028cfbec-b150-3508-8cb3-cbf121ea8f49",
                    "sense_definition": "Niet meer mee mogen doen in een spel, bijvoorbeeld na getikt te zijn.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "game_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jij bent <w>af</w>!",
                            "target_lang_translations": {
                                "ru": "Ты <w>выбыл</w>!"
                            }
                        },
                        {
                            "text": "Als je beweegt, ben je <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Если пошевелишься — ты <w>выбыл</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jij bent af"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбывший из игры, например, после того, как был осален."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбыл",
                                "target_lang_sense_clarification": "из игры"
                            },
                            {
                                "target_lang_word": "вне игры",
                                "target_lang_sense_clarification": "статус игрока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "924b935d-802f-3ec8-8cb7-995bf402d369",
                    "sense_definition": "Helemaal uitgeput of doodmoe.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "exhausted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange reis waren we helemaal <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой поездки мы были совершенно <w>без сил</w>."
                            }
                        },
                        {
                            "text": "De hardloper was <w>af</w> toen hij de finish bereikte.",
                            "target_lang_translations": {
                                "ru": "Бегун был <w>вымотан</w>, когда добрался до финиша."
                            }
                        }
                    ],
                    "common_phrases": [
                        "helemaal af zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью истощённый или смертельно уставший."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без сил",
                                "target_lang_sense_clarification": "истощённый"
                            },
                            {
                                "target_lang_word": "вымотан",
                                "target_lang_sense_clarification": "очень уставший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0aff448c-a776-3ddd-bdda-6c797ed03828",
                    "sense_definition": "Niet langer een bepaalde functie, rol of status hebbend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "status_lost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sinds gisteren is hij minister <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Со вчерашнего дня он <w>бывший</w> министр."
                            }
                        },
                        {
                            "text": "Nu hij geslaagd is, is hij student <w>af</w>.",
                            "target_lang_translations": {
                                "ru": "Теперь, когда он выпустился, он <w>больше не</w> студент."
                            }
                        }
                    ],
                    "common_phrases": [
                        "politicus af"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше не имеющий определённой функции, роли или статуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бывший",
                                "target_lang_sense_clarification": "экс-статус"
                            },
                            {
                                "target_lang_word": "больше не",
                                "target_lang_sense_clarification": "отрицание текущего статуса"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
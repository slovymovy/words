    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0cf09688-7a84-374f-a55e-bdf74bc9f2f6",
                    "sense_definition": "Vervoeging van <w>onderdrukken</w>: tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering <w>onderdrukt</w> elke vorm van kritiek in de media.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>подавляет</w> любую форму критики в СМИ."
                            }
                        },
                        {
                            "text": "Jij <w>onderdrukt</w> je emoties te veel, wat niet gezond is voor je mentale toestand.",
                            "target_lang_translations": {
                                "ru": "Ты слишком сильно <w>сдерживаешь</w> свои эмоции, что вредно для твоего психического состояния."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола <w>onderdrukken</w>: второе и третье лицо единственного числа настоящего времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавляет",
                                "target_lang_sense_clarification": "общий смысл, угнетение или пресечение"
                            },
                            {
                                "target_lang_word": "сдерживает",
                                "target_lang_sense_clarification": "об эмоциях или порывах"
                            },
                            {
                                "target_lang_word": "угнетает",
                                "target_lang_sense_clarification": "о тирании или психологическом давлении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a555b0f5-0aaa-3ead-bd52-0bdc1db6ac13",
                    "sense_definition": "Voltooid deelwoord van <w>onderdrukken</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft de rellen in het centrum met harde hand <w>onderdrukt</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция жесткой рукой <w>подавила</w> беспорядки в центре."
                            }
                        },
                        {
                            "text": "Hij heeft zijn gevoelens jarenlang <w>onderdrukt</w> en durft ze nu pas te uiten.",
                            "target_lang_translations": {
                                "ru": "Он годами <w>подавлял</w> свои чувства и только сейчас осмелился их выразить."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от <w>onderdrukken</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавленный",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "подавил",
                                "target_lang_sense_clarification": "глагол прошедшего времени (в составе перфекта)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cddd2344-b434-32bc-9516-ab88cfe4c7d1",
                    "sense_definition": "Op een onterechte manier in vrijheid beperkt of overheerst door machthebbers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "oppression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>onderdrukte</w> bevolking kwam na jaren van tirannie eindelijk in opstand.",
                            "target_lang_translations": {
                                "ru": "<w>Угнетенное</w> население наконец восстало после многих лет тирании."
                            }
                        },
                        {
                            "text": "In veel historische verhalen speelt de strijd van een <w>onderdrukte</w> minderheid een grote rol.",
                            "target_lang_translations": {
                                "ru": "Во многих исторических рассказах борьба <w>угнетенного</w> меньшинства играет большую роль."
                            }
                        }
                    ],
                    "synonyms": [
                        "geknecht",
                        "overheerst"
                    ],
                    "antonyms": [
                        "vrij",
                        "bevrijd"
                    ],
                    "common_phrases": [
                        "onderdrukte minderheid",
                        "onderdrukte volkeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несправедливо ограниченный в свободе или находящийся под властью правителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угнетенный",
                                "target_lang_sense_clarification": "о людях, народах или меньшинствах"
                            },
                            {
                                "target_lang_word": "порабощенный",
                                "target_lang_sense_clarification": "лишенный свободы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51afdf1e-9395-3310-85a6-8850098df1c4",
                    "sense_definition": "Met moeite ingehouden of niet volledig geuit, vaak gezegd van emoties of geluiden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "suppression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een <w>onderdrukte</w> glimlach luisterde hij naar het onwaarschijnlijke excuus van zijn leerling.",
                            "target_lang_translations": {
                                "ru": "Со <w>сдержанной</w> улыбкой он слушал невероятное оправдание своего ученика."
                            }
                        },
                        {
                            "text": "Er klonk een <w>onderdrukt</w> gesnik vanuit de slaapkamer, alsof iemand stilletjes lag te huilen.",
                            "target_lang_translations": {
                                "ru": "Из спальни донеслось <w>сдавленное</w> рыдание, словно кто-то тихо плакал."
                            }
                        }
                    ],
                    "synonyms": [
                        "ingehouden",
                        "verbeten"
                    ],
                    "antonyms": [
                        "uitbundig",
                        "luidkeels"
                    ],
                    "common_phrases": [
                        "onderdrukte woede",
                        "onderdrukte lach",
                        "onderdrukte tranen"
                    ],
                    "target_lang_definitions": {
                        "ru": "С трудом сдерживаемый или не полностью выраженный (об эмоциях или звуках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдержанный",

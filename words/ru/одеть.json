    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "77dc0e2d-11a0-3c86-95ba-971104cc5a5e",
                    "sense_definition": "Облечь кого-либо в одежду, помочь кому-либо надеть одежду.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама старалась тепло <одеть> ребёнка перед выходом на улицу.",
                            "target_lang_translations": {
                                "en": "Mom tried to <dress> the child warmly before going outside."
                            }
                        },
                        {
                            "text": "Врач попросил медсестру <одеть> пациента после осмотра.",
                            "target_lang_translations": {
                                "en": "The doctor asked the nurse to <dress> the patient after the examination."
                            }
                        }
                    ],
                    "synonyms": [
                        "облачить"
                    ],
                    "antonyms": [
                        "раздеть"
                    ],
                    "common_phrases": [
                        "тепло одеть",
                        "одеть ребёнка",
                        "одеть по моде",
                        "одеть куклу"
                    ],
                    "target_lang_definitions": {
                        "en": "To put clothes on someone else or help them get dressed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dress",
                                "target_lang_sense_clarification": "someone"
                            },
                            {
                                "target_lang_word": "clothe",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c968989-f8c7-3967-9e58-b21fd67aac69",
                    "sense_definition": "Надеть что-либо на себя или на часть тела (в строгой литературной норме считается ошибкой, правильно: надеть).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он быстро <одел> шапку и выбежал из дома.",
                            "target_lang_translations": {
                                "en": "He quickly <put on> his hat and ran out of the house."
                            }
                        },
                        {
                            "text": "Девушка решила <одеть> очки, чтобы лучше видеть доску.",
                            "target_lang_translations": {
                                "en": "The girl decided to <put on> her glasses to see the board better."
                            }
                        }
                    ],
                    "antonyms": [
                        "снять"
                    ],
                    "common_phrases": [
                        "одеть шапку",
                        "одеть куртку",
                        "одеть очки",
                        "одеть пальто"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Часто используется в разговорной речи вместо «надеть», но считается стилистически неверным."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To put an item of clothing on oneself (colloquial usage)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "clothing or accessories"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22189b5c-d4de-3dce-9acd-3787cc498cec",
                    "sense_definition": "Обеспечить кого-либо одеждой, купить кому-либо необходимую одежду.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "supply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители должны накормить и <одеть> своих детей.",
                            "target_lang_translations": {
                                "en": "Parents must feed and <clothe> their children."
                            }
                        },
                        {
                            "text": "Фонд помог <одеть> и обуть сирот к зиме.",
                            "target_lang_translations": {
                                "en": "The foundation helped <clothe> and provide shoes for the orphans for winter."
                            }
                        }
                    ],
                    "common_phrases": [
                        "одеть и обуть",
                        "одеть семью",
                        "одеть с иголочки"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide someone with necessary clothing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clothe",
                                "target_lang_sense_clarification": "provide with clothes"
                            },
                            {
                                "target_lang_word": "dress",
                                "target_lang_sense_clarification": "supply with clothes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bc4f0a1-c863-3687-86f3-05b66da7c7b8",
                    "sense_definition": "Покрыть поверхность чем-либо, окутать (о снеге, листве, тумане и т.п.).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_cover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Первый снег успел <одеть> землю в белое покрывало.",
                            "target_lang_translations": {
                                "en": "The first snow managed to <clothe> the earth in a white blanket."
                            }
                        },
                        {
                            "text": "Весна скоро должна <одеть> леса в зелёный наряд.",
                            "target_lang_translations": {
                                "en": "Spring should soon <clothe> the forests in green attire."
                            }
                        }
                    ],
                    "antonyms": [
                        "оголить"
                    ],
                    "common_phrases": [
                        "одеть в золото",
                        "одеть в гранит",
                        "одеть в листву"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Характерно для книжного или поэтического стиля."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a surface or envelop something (used figuratively)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "a surface"
                            },
                            {
                                "target_lang_word": "clothe",
                                "target_lang_sense_clarification": "figurative"
                            },
                            {
                                "target_lang_word": "blanket",
                                "target_lang_sense_clarification": "cover completely"
                            }
                        ]
                    }
                }
            ]
        }
    ]

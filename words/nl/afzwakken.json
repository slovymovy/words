                },
                {
                    "sense_id": "7aa19210-95d5-37cc-9769-38c86ce60444",
                    "sense_definition": "Minder streng maken van regels of eisen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid wil de strenge milieuregels voorlopig niet <w>afzwakken</w>.",
                            "target_lang_translations": {
                                "en": "The government does not want to <w>dilute</w> the strict environmental rules for the time being.",
                                "ru": "Правительство пока не хочет <w>смягчать</w> строгие экологические правила."
                            }
                        },
                        {
                            "text": "We zullen de eisen voor toelating iets moeten <w>afzwakken</w>.",
                            "target_lang_translations": {
                                "en": "We will have to <w>lower</w> the admission requirements slightly.",
                                "ru": "Нам придется немного <w>снизить</w> требования к поступлению."
                            }
                        }
                    ],
                    "synonyms": [
                        "versoepelen"
                    ],
                    "antonyms": [
                        "verscherpen",
                        "verstrengen"
                    ],
                    "common_phrases": [
                        "de regels afzwakken",
                        "eisen afzwakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To make rules or requirements less strict.",
                        "ru": "Делать правила или требования менее строгими."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "relax",
                                "target_lang_sense_clarification": "to make rules less strict"
                            },
                            {
                                "target_lang_word": "dilute",
                                "target_lang_sense_clarification": "to reduce the strength of a standard"
                            },
                            {
                                "target_lang_word": "lower",
                                "target_lang_sense_clarification": "to reduce the level of requirements"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смягчать",
                                "target_lang_sense_clarification": "правила"
                            },
                            {
                                "target_lang_word": "ослаблять",

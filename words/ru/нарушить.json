                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "food items"
                            },
                            {
                                "target_lang_word": "slice",
                                "target_lang_sense_clarification": "bread or pie"
                            }
                        ]
                    }
                }
            ]

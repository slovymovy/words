{
    "word_family": [
        "beweging",
        "beweegreden",
        "beweegbaar",
        "beweeglijk",
        "voortbewegen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6528aac-86ff-35a3-b59b-54cd6c560ea0",
                    "sense_definition": "Van plaats of houding veranderen; niet stilstaan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet elke dag voldoende <w>bewegen</w> om gezond te blijven.",
                            "target_lang_translations": {
                                "en": "You must <w>move</w> enough every day to stay healthy.",
                                "ru": "Ты должен достаточно <w>двигаться</w> каждый день, чтобы оставаться здоровым."
                            }
                        },
                        {
                            "text": "De bladeren aan de bomen <w>bewegen</w> zachtjes in de wind.",
                            "target_lang_translations": {
                                "en": "The leaves on the trees <w>move</w> gently in the wind.",
                                "ru": "Листья на деревьях тихо <w>шевелятся</w> на ветру."
                            }
                        }
                    ],
                    "synonyms": [
                        "verroeren"
                    ],
                    "antonyms": [
                        "stilstaan"
                    ],
                    "common_phrases": [
                        "blijven bewegen",
                        "zich voortbewegen",
                        "vrij bewegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To change place or posture; not standing still.",
                        "ru": "Менять положение или позу; не стоять на месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "general physical motion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "двигаться",
                                "target_lang_sense_clarification": "совершать движения, перемещаться"
                            },
                            {
                                "target_lang_word": "шевелиться",
                                "target_lang_sense_clarification": "слегка двигаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce09fba1-551e-3c18-88df-9857e3198c82",
                    "sense_definition": "Iets of iemand van plaats laten veranderen; in beweging brengen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan mijn vingers nauwelijks <w>bewegen</w> door de extreme kou.",
                            "target_lang_translations": {
                                "en": "I can hardly <w>move</w> my fingers due to the extreme cold.",
                                "ru": "Я едва могу <w>шевелить</w> пальцами из-за сильного холода."
                            }
                        },
                        {
                            "text": "De fysiotherapeut vroeg mij om mijn arm voorzichtig te <w>bewegen</w>.",
                            "target_lang_translations": {
                                "en": "The physiotherapist asked me to carefully <w>move</w> my arm.",
                                "ru": "Физиотерапевт попросил меня осторожно <w>подвигать</w> рукой."
                            }
                        }
                    ],
                    "synonyms": [
                        "verplaatsen",
                        "verschuiven"
                    ],
                    "antonyms": [
                        "stilstaan"
                    ],
                    "common_phrases": [
                        "niet kunnen bewegen",
                        "armen en benen bewegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something or someone to change place; to set in motion.",
                        "ru": "Заставлять что-то или кого-то менять местоположение; приводить в движение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to cause to move body parts or objects"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "двигать",
                                "target_lang_sense_clarification": "перемещать предметы"
                            },
                            {
                                "target_lang_word": "шевелить",
                                "target_lang_sense_clarification": "делать движения частями тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d37976ce-2a9d-3451-82d1-1e7f48c9d1af",
                    "sense_definition": "Iemand overhalen of aanzetten om iets te doen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat heeft jou ertoe <w>bewogen</w> om deze belangrijke beslissing te nemen?",
                            "target_lang_translations": {
                                "en": "What <w>moved</w> you to make this important decision?",
                                "ru": "Что <w>побудило</w> тебя принять это важное решение?"
                            }
                        },
                        {
                            "text": "Niemand kon hem <w>bewegen</w> om zijn plannen voor de vakantie te wijzigen.",
                            "target_lang_translations": {
                                "en": "No one could <w>persuade</w> him to change his plans for the holiday.",
                                "ru": "Никто не мог <w>склонить</w> его изменить свои планы на отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "overhalen",
                        "aanzetten",
                        "motiveren"
                    ],
                    "common_phrases": [
                        "ertoe bewegen",
                        "hemel en aarde bewegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To persuade or incite someone to do something.",
                        "ru": "Убеждать или побуждать кого-либо сделать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to influence or motivate someone to act"
                            },
                            {
                                "target_lang_word": "persuade",
                                "target_lang_sense_clarification": "to convince someone to do something"
                            },
                            {
                                "target_lang_word": "induce",
                                "target_lang_sense_clarification": "to bring about or cause an action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "побуждать",
                                "target_lang_sense_clarification": "склонять к действию"
                            },
                            {
                                "target_lang_word": "склонять",
                                "target_lang_sense_clarification": "убеждать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99f1f3e4-c7eb-3673-9460-f9f71a067c4b",
                    "sense_definition": "Zich begeven in een bepaalde omgeving of sociale groep.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>beweegt</w> zich al jarenlang in de hoogste politieke kringen.",
                            "target_lang_translations": {
                                "en": "He has been <w>moving</w> in the highest political circles for years.",
                                "ru": "Он уже много лет <w>вращается</w> в высших политических кругах."
                            }
                        },
                        {
                            "text": "Als artiest <w>beweegt</w> zij zich gemakkelijk tussen verschillende culturen.",
                            "target_lang_translations": {
                                "en": "As an artist, she <w>moves</w> easily between different cultures.",
                                "ru": "Как артист, она легко <w>перемещается</w> между различными культурами."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkeren",
                        "omgaan"
                    ],
                    "common_phrases": [
                        "zich in kringen bewegen",
                        "zich vrij bewegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be present or active in a specific environment or social group.",
                        "ru": "Находиться или действовать в определенной среде или социальной группе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to circulate within social circles"
                            },
                            {
                                "target_lang_word": "circulate",
                                "target_lang_sense_clarification": "to move among a group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вращаться",
                                "target_lang_sense_clarification": "быть в определенном кругу общения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "przełamywać",
        "przełamanie",
        "przełom",
        "przełomowy",
        "nieprzełamany"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "23e729c3-f045-3ddd-a26f-67bd0e3b5e3d",
                    "sense_definition": "Złamać coś na dwie lub więcej części, naruszając jego strukturę fizyczną.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fizyczne_lamanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna chwycił suchą gałąź i <przełamał> ją na pół.",
                            "target_lang_translations": {
                                "ru": "Мужчина схватил сухую ветку и <переломил> её пополам."
                            }
                        },
                        {
                            "text": "Podczas wigilii będziemy tradycyjnie <przełamać> się opłatkiem.",
                            "target_lang_translations": {
                                "ru": "В сочельник мы по традиции <преломим> друг с другом облатку."
                            }
                        }
                    ],
                    "synonyms": [
                        "złamać",
                        "rozpołowić",
                        "pęknąć"
                    ],
                    "antonyms": [
                        "scalić",
                        "złączyć",
                        "skleić"
                    ],
                    "common_phrases": [
                        "przełamać chleb",
                        "przełamać na pół",
                        "przełamać się opłatkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сломать что-то на две или более части, нарушив его физическую структуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переломить",
                                "target_lang_sense_clarification": "разделить надвое с усилием"
                            },
                            {
                                "target_lang_word": "разломить",
                                "target_lang_sense_clarification": "разделить на куски (например, хлеб)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae7c2de1-c792-353f-a787-489dc53e59ad",
                    "sense_definition": "Pokonać przeszkodę, barierę, opór lub trudną sytuację; zwyciężyć w walce z czymś abstrakcyjnym.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "pokonywanie_barier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <przełamać> lody i zacząć ze sobą szczerze rozmawiać.",
                            "target_lang_translations": {
                                "ru": "Мы должны <растопить> лёд и начать искренне разговаривать друг с другом."
                            }
                        },
                        {
                            "text": "Nowa strategia pozwoliła armii <przełamać> linię obrony przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Новая стратегия позволила армии <прорвать> линию обороны противника."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokonać",
                        "przezwyciężyć",
                        "zwalczyć"
                    ],
                    "antonyms": [
                        "poddać się",
                        "ulec"
                    ],
                    "common_phrases": [
                        "przełamać lody",
                        "przełamać milczenie",
                        "przełamać rutynę",
                        "przełamać impas",
                        "przełamać barierę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преодолеть препятствие, барьер, сопротивление или трудную ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преодолеть",
                                "target_lang_sense_clarification": "справиться с трудностью"
                            },
                            {
                                "target_lang_word": "переломить",
                                "target_lang_sense_clarification": "изменить ход событий"
                            },
                            {
                                "target_lang_word": "прорвать",
                                "target_lang_sense_clarification": "линию обороны или фронт"
                            },
                            {
                                "target_lang_word": "растопить",
                                "target_lang_sense_clarification": "лёд (в переносном смысле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38332846-89b6-3af4-ad0d-5598748cc73c",
                    "sense_definition": "Przezwyciężyć własną niechęć, wstyd lub strach przed zrobieniem czegoś (używane z zaimkiem zwrotnym).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pokonywanie_barier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo ogromnego stresu, aktor <przełamał> się i wyszedł na scenę.",
                            "target_lang_translations": {
                                "ru": "Несмотря на огромный стресс, актёр <пересилил> себя и вышел на сцену."
                            }
                        },
                        {
                            "text": "Długo nie mogłem się <przełamać>, żeby zadzwonić do dawnego przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Я долго не мог <решиться> позвонить старому другу."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemóc się",
                        "odważyć się"
                    ],
                    "antonyms": [
                        "zrezygnować",
                        "wycofać się"
                    ],
                    "common_phrases": [
                        "przełamać się wewnętrznie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Преодолеть собственное нежелание, стыд или страх перед совершением чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересилить себя",
                                "target_lang_sense_clarification": "заставить себя сделать что-то"
                            },
                            {
                                "target_lang_word": "преодолеть себя",
                                "target_lang_sense_clarification": "справиться с внутренним барьером"
                            },
                            {
                                "target_lang_word": "решиться",
                                "target_lang_sense_clarification": "осмелиться на действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
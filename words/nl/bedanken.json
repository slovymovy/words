    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d03ff34e-ef1a-39f1-b848-7dca19e31bd7",
                    "sense_definition": "Iemand dank betuigen of erkentelijkheid tonen voor iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil jullie <w>bedanken</w> voor jullie gastvrijheid.",
                            "target_lang_translations": {
                                "ru": "Я хочу <w>поблагодарить</w> вас за ваше гостеприимство.",
                                "en": "I want to <w>thank</w> you for your hospitality."
                            }
                        },
                        {
                            "text": "Zij <w>bedankte</w> haar collega's hartelijk voor de fijne samenwerking.",
                            "target_lang_translations": {
                                "ru": "Она сердечно <w>поблагодарила</w> своих коллег за приятное сотрудничество.",
                                "en": "She heartily <w>thanked</w> her colleagues for the pleasant cooperation."
                            }
                        }
                    ],
                    "synonyms": [
                        "danken"
                    ],
                    "common_phrases": [
                        "iemand hartelijk bedanken",
                        "bedanken voor de moeite",
                        "alvast bedankt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать благодарность кому-либо или проявлять признательность за что-либо.",
                        "en": "To express gratitude or appreciation to someone."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благодарить",
                                "target_lang_sense_clarification": "выражать признательность"
                            },
                            {
                                "target_lang_word": "поблагодарить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "thank",
                                "target_lang_sense_clarification": "to express gratitude"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3059f41b-5869-3085-955b-27314f3baf61",
                    "sense_definition": "Een aanbod of uitnodiging beleefd weigeren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Quinten besloot om te <w>bedanken</w> voor de aangeboden baan.",
                            "target_lang_translations": {
                                "ru": "Квинтен решил <w>отказаться</w> от предложенной работы.",
                                "en": "Quinten decided to <w>decline</w> the offered job."
                            }
                        },
                        {
                            "text": "Voor dat risicovolle avontuur <w>bedank</w> ik liever.",
                            "target_lang_translations": {
                                "ru": "От этого рискованного приключения я лучше <w>откажусь</w>.",
                                "en": "I would rather <w>pass</w> on that risky adventure."
                            }
                        }
                    ],
                    "synonyms": [
                        "afslaan",
                        "weigeren",
                        "passen"
                    ],
                    "antonyms": [
                        "aannemen",
                        "accepteren"
                    ],
                    "common_phrases": [
                        "vriendelijk bedanken",
                        "bedanken voor de eer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливо отклонить предложение или приглашение.",
                        "en": "To politely refuse an offer or invitation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "вежливо отклонять предложение"
                            },
                            {
                                "target_lang_word": "отклонять",
                                "target_lang_sense_clarification": "не принимать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "decline",
                                "target_lang_sense_clarification": "to politely refuse"
                            },
                            {
                                "target_lang_word": "turn down",
                                "target_lang_sense_clarification": "to refuse an offer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dccbcd7-cb5d-3515-8f46-a36b0017a115",
                    "sense_definition": "Een lidmaatschap, abonnement of functie opzeggen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag voor de krant <w>bedanken</w> omdat ik hem niet meer lees.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>отказаться</w> от газеты, так как больше её не читаю.",
                                "en": "I would like to <w>cancel</w> the newspaper because I no longer read it."
                            }
                        },
                        {
                            "text": "Na tien jaar trouwe dienst heeft de voorzitter gisteren officieel <w>bedankt</w>.",
                            "target_lang_translations": {
                                "ru": "После десяти лет верной службы председатель вчера официально <w>подал в отставку</w>.",
                                "en": "After ten years of loyal service, the chairman officially <w>resigned</w> yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "opzeggen",
                        "aftreden"
                    ],
                    "antonyms": [
                        "aanmelden",
                        "lid worden"
                    ],
                    "common_phrases": [
                        "bedanken als lid",
                        "bedanken voor het lidmaatschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от членства, подписки или уйти с должности.",
                        "en": "To cancel a membership or subscription, or to resign from a position."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "от подписки или участия"
                            },
                            {
                                "target_lang_word": "уходить в отставку",
                                "target_lang_sense_clarification": "с должности"
                            },
                            {
                                "target_lang_word": "слагать полномочия",
                                "target_lang_sense_clarification": "официально покидать пост"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cancel",
                                "target_lang_sense_clarification": "a subscription or membership"
                            },
                            {
                                "target_lang_word": "resign",
                                "target_lang_sense_clarification": "from a position"
                            }
                        ]
                    }
                }
            ]
        }

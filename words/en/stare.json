                },
                {
                    "sense_id": "b9396144-4adf-38da-8303-d581a072e8a8",
                    "sense_definition": "A village or settlement in Poland or Ukraine.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited the small village of <w>Stare</w> during their trip to Poland.",
                            "target_lang_translations": {
                                "ru": "Во время поездки в Польшу они посетили небольшую деревню <w>Старе</w>."
                            }

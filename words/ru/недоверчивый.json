    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9e39e1e3-022c-3a3b-b9eb-32622f61536b",
                    "sense_definition": "Склонный к недоверию, подозрительный, с трудом верящий другим людям.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "character_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <w>недоверчивый</w> старик всегда закрывает дверь на три замка.",
                            "target_lang_translations": {
                                "en": "This <w>distrustful</w> old man always locks the door with three locks."
                            }
                        },
                        {
                            "text": "Она была очень <w>недоверчива</w> и долго не соглашалась на наше предложение.",
                            "target_lang_translations": {
                                "en": "She was very <w>mistrustful</w> and would not agree to our proposal for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "подозрительный"
                    ],
                    "antonyms": [
                        "доверчивый"
                    ],
                    "common_phrases": [
                        "недоверчивый человек",
                        "недоверчивый характер",
                        "крайне недоверчивый",
                        "недоверчивый покупатель"
                    ],
                    "target_lang_definitions": {
                        "en": "Prone to mistrust, suspicious, finding it difficult to trust others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distrustful",
                                "target_lang_sense_clarification": "lacking trust in others"
                            },
                            {
                                "target_lang_word": "mistrustful",
                                "target_lang_sense_clarification": "having or showing a lack of trust"
                            },
                            {
                                "target_lang_word": "suspicious",
                                "target_lang_sense_clarification": "inclined to suspect others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1096759e-7e47-31d0-a8e8-22a9764dab58",
                    "sense_definition": "Выражающий недоверие (о взгляде, тоне, жесте).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник бросил на сотрудника строгий и <w>недоверчивый</w> взгляд.",
                            "target_lang_translations": {
                                "en": "The boss cast a strict and <w>suspicious</w> look at the employee."
                            }
                        },
                        {
                            "text": "В ответ на мои слова послышался <w>недоверчивый</w> смех.",
                            "target_lang_translations": {
                                "en": "In response to my words, a <w>distrustful</w> laugh was heard."
                            }
                        }
                    ],
                    "synonyms": [
                        "подозрительный"
                    ],
                    "antonyms": [
                        "доверчивый"
                    ],
                    "common_phrases": [
                        "недоверчивый взгляд",
                        "недоверчивый тон",
                        "недоверчивая улыбка",
                        "недоверчивый вид"
                    ],
                    "target_lang_definitions": {
                        "en": "Expressing mistrust (referring to a look, tone, or gesture)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspicious",
                                "target_lang_sense_clarification": "expressing suspicion (e.g. a look)"
                            },
                            {
                                "target_lang_word": "mistrustful",
                                "target_lang_sense_clarification": "indicating a lack of trust"
                            },
                            {
                                "target_lang_word": "distrustful",
                                "target_lang_sense_clarification": "showing distrust"
                            }
                        ]
                    }
                }
            ]

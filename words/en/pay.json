{
    "word_family": [
        "payment",
        "payable",
        "payee",
        "payer",
        "repay",
        "prepay",
        "overpay",
        "underpay",
        "payout",
        "paycheck",
        "payroll"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a7da7a95-dba2-3bba-a415-a61663f6775b",
                    "sense_definition": "To give money to someone in exchange for goods or services.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exchange_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>pay</w> for the groceries before we leave.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>заплати́ть</w> за продукты, прежде чем мы уйдем."
                            }
                        },
                        {
                            "text": "She <w>paid</w> the taxi driver with cash.",
                            "target_lang_translations": {
                                "ru": "Она <w>заплати́ла</w> таксисту наличными."
                            }
                        }
                    ],
                    "synonyms": [
                        "spend",
                        "expend",
                        "fork out",
                        "shell out"
                    ],
                    "antonyms": [
                        "earn"
                    ],
                    "common_phrases": [
                        "pay cash",
                        "pay for",
                        "pay by card"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать деньги кому-либо в обмен на товары или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плати́ть",
                                "target_lang_sense_clarification": "отдавать деньги"
                            },
                            {
                                "target_lang_word": "заплати́ть",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "опла́чивать",
                                "target_lang_sense_clarification": "вносить плату за что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb100040-b96c-3d7b-81c4-641f4db7153d",
                    "sense_definition": "To discharge a debt or obligation by giving the required amount.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "discharge_debt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He always <w>pays</w> his bills on time.",
                            "target_lang_translations": {
                                "ru": "Он всегда <w>опла́чивает</w> свои счета вовремя."
                            }
                        },
                        {
                            "text": "You must <w>pay</w> the fine within thirty days.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>оплати́ть</w> штраф в течение тридцати дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay up",
                        "foot the bill",
                        "defray"
                    ],
                    "antonyms": [
                        "default"
                    ],
                    "common_phrases": [
                        "pay a bill",
                        "pay rent",
                        "pay taxes",
                        "pay a fine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Погашать долг или обязательство, отдавая требуемую сумму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опла́чивать",
                                "target_lang_sense_clarification": "счета, квитанции"
                            },
                            {
                                "target_lang_word": "плати́ть",
                                "target_lang_sense_clarification": "налоги, штрафы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a62e786-2e71-3ae1-a3ba-267a569b22d3",
                    "sense_definition": "To give or express something non-monetary, such as attention, respect, or a visit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "give_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>pay</w> attention to what I am saying.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>обрати́те</w> внимание на то, что я говорю."
                            }
                        },
                        {
                            "text": "We should <w>pay</w> a visit to your grandmother soon.",
                            "target_lang_translations": {
                                "ru": "Нам стоит скоро <w>нанести́</w> визит твоей бабушке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pay attention",
                        "pay a visit",
                        "pay respect",
                        "pay a compliment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять или выражать что-либо нематериальное, например внимание, уважение или визит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уделя́ть",
                                "target_lang_sense_clarification": "внимание, время"
                            },
                            {
                                "target_lang_word": "обраща́ть",
                                "target_lang_sense_clarification": "внимание (во фразе 'pay attention')"
                            },
                            {
                                "target_lang_word": "наноси́ть",
                                "target_lang_sense_clarification": "визит"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abc6f9b2-8883-3dca-a851-1130f5b9c468",
                    "sense_definition": "To result in a profit or advantage; to be worth the effort.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "be_profitable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>pays</w> to be honest in the long run.",
                            "target_lang_translations": {
                                "ru": "В долгосрочной перспективе быть честным <w>окупа́ется</w>."
                            }
                        },
                        {
                            "text": "Hard work will eventually <w>pay</w> off.",
                            "target_lang_translations": {
                                "ru": "Упорный труд в конечном итоге <w>оку́пится</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "crime doesn't pay",
                        "it pays to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приносить прибыль или преимущество; стоить затраченных усилий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окупа́ться",
                                "target_lang_sense_clarification": "приносить выгоду"
                            },
                            {
                                "target_lang_word": "сто́ить",
                                "target_lang_sense_clarification": "иметь смысл (it pays to)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c4386d5-dd59-3abc-b695-0fabd0af3148",
                    "sense_definition": "To suffer negative consequences for a past action.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "suffer_consequences",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He will <w>pay</w> for his rude behavior.",
                            "target_lang_translations": {
                                "ru": "Он <w>попла́тится</w> за свое грубое поведение."
                            }
                        },
                        {
                            "text": "You shouldn't make others <w>pay</w> for your mistakes.",
                            "target_lang_translations": {
                                "ru": "Ты не должен заставлять других <w>распла́чиваться</w> за твои ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay the price",
                        "pay the piper"
                    ],
                    "common_phrases": [
                        "pay for it",
                        "pay dearly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдать от негативных последствий за прошлые действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распла́чиваться",
                                "target_lang_sense_clarification": "нести наказание"
                            },
                            {
                                "target_lang_word": "поплати́ться",
                                "target_lang_sense_clarification": "понести ущерб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddcb9370-b8fe-3c42-b707-8f99b1482a61",
                    "sense_definition": "To coat a ship's seams with tar or pitch for waterproofing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_pay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailors <w>payed</w> the deck seams with hot pitch.",
                            "target_lang_translations": {
                                "ru": "Моряки <w>смоли́ли</w> швы палубы горячим варом."
                            }
                        },
                        {
                            "text": "It takes skill to <w>pay</w> the hull properly.",
                            "target_lang_translations": {
                                "ru": "Требуется мастерство, чтобы правильно <w>просмоли́ть</w> корпус."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать швы судна смолой или варом для водонепроницаемости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смоли́ть",
                                "target_lang_sense_clarification": "покрывать смолой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05287055-806a-36b2-9998-f4d704a3595e",
                    "sense_definition": "Money received in exchange for work performed.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "salary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers demanded higher <w>pay</w> for their labor.",
                            "target_lang_translations": {
                                "ru": "Рабочие требовали более высокую <w>опла́ту</w> за свой труд."
                            }
                        },
                        {
                            "text": "She receives her <w>pay</w> at the end of the month.",
                            "target_lang_translations": {
                                "ru": "Она получает <w>зарпла́ту</w> в конце месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "fork out",
                        "shell out"
                    ],
                    "common_phrases": [
                        "sick pay",
                        "holiday pay",
                        "severance pay",
                        "take-home pay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги, полученные в обмен на выполненную работу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарпла́та",
                                "target_lang_sense_clarification": "заработная плата"
                            },
                            {
                                "target_lang_word": "опла́та",
                                "target_lang_sense_clarification": "вознаграждение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3706bbec-6b31-362c-8d72-0ae4cd7be504",
                    "sense_definition": "Requiring payment of a fee to be used or accessed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "requiring_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had to use a <w>pay</w> phone to call home.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось воспользоваться <w>пла́тным</w> таксофоном, чтобы позвонить домой."
                            }
                        },
                        {
                            "text": "Most channels are now on <w>pay</w> television.",
                            "target_lang_translations": {
                                "ru": "Большинство каналов теперь на <w>пла́тном</w> телевидении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pay phone",
                        "pay TV",
                        "pay wall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требующий оплаты сбора для использования или доступа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пла́тный",
                                "target_lang_sense_clarification": "требующий оплаты"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9443d725-a382-31e1-be97-a926bd48b0a0",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Pay</w> signed the document.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Пэй</w> подписал документ."
                            }
                        },
                        {
                            "text": "I met a family with the surname <w>Pay</w>.",
                            "target_lang_translations": {
                                "ru": "Я встретил семью с фамилией <w>Пэй</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "surname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пэй",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3ce6452-ab61-3018-95ba-4be81a900c29",
                    "sense_definition": "A diminutive of the unisex given name <w>Payton</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Pay</w> is coming to the party later.",
                            "target_lang_translations": {
                                "ru": "<w>Пэй</w> придет на вечеринку позже."
                            }
                        },
                        {
                            "text": "Her nickname is <w>Pay</w>.",
                            "target_lang_translations": {
                                "ru": "Её прозвище — <w>Пэй</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма унисекс-имени Пэйтон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пэй",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
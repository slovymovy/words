{
    "word_family": [
        "avail",
        "availability",
        "unavailable"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a09e8efb-4d40-304d-8e21-cbeacac1cddb",
                    "sense_definition": "Able to be obtained, used, or found.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obtainable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fresh fruit is <w>available</w> at the market all year round.",
                            "target_lang_translations": {
                                "ru": "Свежие фрукты <w>доступны</w> на рынке круглый год."
                            }
                        },
                        {
                            "text": "More information is <w>available</w> on our website.",
                            "target_lang_translations": {
                                "ru": "Более подробная информация <w>доступна</w> на нашем сайте."
                            }
                        }
                    ],
                    "synonyms": [
                        "on offer",
                        "gettable",
                        "usable",
                        "vacant"
                    ],
                    "antonyms": [
                        "unavailable"
                    ],
                    "common_phrases": [
                        "readily available",
                        "widely available",
                        "now available",
                        "available resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для получения, использования или нахождения; имеющийся в наличии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досту́пный",
                                "target_lang_sense_clarification": "который можно получить или использовать"
                            },
                            {
                                "target_lang_word": "в нали́чии",
                                "target_lang_sense_clarification": "имеющийся (о товарах)"
                            },
                            {
                                "target_lang_word": "име́ющийся",
                                "target_lang_sense_clarification": "существующий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76b0c516-a5cc-3145-afe9-094c1a9acfd0",
                    "sense_definition": "Free to meet someone, speak on the telephone, or participate in an activity; not busy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "personal_availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>available</w> for a meeting tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Я <w>свободен</w> для встречи завтра утром."
                            }
                        },
                        {
                            "text": "The manager is not <w>available</w> right now, but she will call you back.",
                            "target_lang_translations": {
                                "ru": "Менеджер сейчас не <w>свободен</w>, но она вам перезвонит."
                            }
                        }
                    ],
                    "antonyms": [
                        "unavailable"
                    ],
                    "common_phrases": [
                        "available for",
                        "make oneself available"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не состоящий в романтических отношениях и готовый к поиску партнера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свобо́дный",
                                "target_lang_sense_clarification": "не состоящий в отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76b0c516-a5cc-3145-afe9-094c1a9acfd0",
                    "sense_definition": "Not currently in a romantic relationship and open to finding a partner.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personal_availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He asked if she was <w>available</w>, but she said she had a boyfriend.",
                            "target_lang_translations": {
                                "ru": "Он спросил, <w>свободна</w> ли она, но она ответила, что у нее есть парень."
                            }
                        },
                        {
                            "text": "My brother is single and <w>available</w>.",
                            "target_lang_translations": {
                                "ru": "Мой брат холост и <w>свободен</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "taken"
                    ],
                    "common_phrases": [
                        "single and available"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не состоящий в романтических отношениях и готовый к поиску партнера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свобо́дный",
                                "target_lang_sense_clarification": "не состоящий в отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edbcbd98-e9b3-33f5-a3d1-d188fe726843",
                    "sense_definition": "Legally valid or capable of being used in a legal context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "validity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer looked for an <w>available</w> plea to help his client.",
                            "target_lang_translations": {
                                "ru": "Адвокат искал <w>действительный</w> довод, чтобы помочь своему клиенту."
                            }
                        },
                        {
                            "text": "There is no <w>available</w> defense in this case.",
                            "target_lang_translations": {
                                "ru": "В этом деле нет <w>применимой</w> защиты."
                            }
                        }
                    ],
                    "synonyms": [
                        "valid"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical or legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридически действительный или применимый в правовом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действи́тельный",
                                "target_lang_sense_clarification": "имеющий юридическую силу"
                            },
                            {
                                "target_lang_word": "правомо́чный",
                                "target_lang_sense_clarification": "обладающий законным основанием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "560cb163-b36a-3b94-840b-f9bae486b030",
                    "sense_definition": "Having sufficient power, force, or efficacy to achieve a purpose.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "efficacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They sought an <w>available</w> means to end the conflict.",
                            "target_lang_translations": {
                                "ru": "Они искали <w>действенное</w> средство, чтобы положить конец конфликту."
                            }
                        },
                        {
                            "text": "The medicine was found to be <w>available</w> against the disease.",
                            "target_lang_translations": {
                                "ru": "Лекарство оказалось <w>действенным</w> против этой болезни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий достаточной силой или эффективностью для достижения цели (устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "де́йственный",
                                "target_lang_sense_clarification": "эффективный"
                            },
                            {
                                "target_lang_word": "поле́зный",
                                "target_lang_sense_clarification": "приносящий пользу"
                            }
                        ]
                    }
                }
            ]
        }

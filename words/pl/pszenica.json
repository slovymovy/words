{
    "word_family": [
        "pszeniczny",
        "pszeniczka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4ce342d6-b485-33b1-81c0-ee82898bfad0",
                    "sense_definition": "Roślina zbożowa z rodziny wiechlinowatych, powszechnie uprawiana na całym świecie, której kłosy zawierają ziarna służące do produkcji mąki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plant_grain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku rolnicy zasiali <w>pszenicę</w> na wszystkich swoich polach.",
                            "target_lang_translations": {
                                "ru": "В этом году фермеры посеяли <w>пшеницу</w> на всех своих полях."
                            }
                        },
                        {
                            "text": "<w>Pszenica</w> ozima wymaga odpowiedniego nawożenia jeszcze przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Озимая <w>пшеница</w> требует соответствующего удобрения еще до наступления зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zboże"
                    ],
                    "common_phrases": [
                        "pszenica ozima",
                        "pszenica jara",
                        "pole pszenicy",
                        "łan pszenicy",
                        "uprawa pszenicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Злаковое растение, из зерен которого производят муку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пшеница",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc0dad9a-be22-3efd-af37-d68d32c76af0",
                    "sense_definition": "Ziarno pozyskiwane z rośliny o tej samej nazwie, wykorzystywane w przemyśle spożywczym, głównie do wyrobu mąki, kaszy i pasz.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plant_grain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młynarz zmielił dostarczoną <w>pszenicę</w> na białą mąkę do wypieku chleba.",
                            "target_lang_translations": {
                                "ru": "Мельник смолол доставленную <w>пшеницу</w> в белую муку для выпечки хлеба."
                            }
                        },
                        {
                            "text": "Gospodarz przyniósł z magazynu worek <w>pszenicy</w>, aby nakarmić drób.",
                            "target_lang_translations": {
                                "ru": "Хозяин принес со склада мешок <w>пшеницы</w>, чтобы накормить домашнюю птицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziarno"
                    ],
                    "common_phrases": [
                        "worek pszenicy",
                        "ziarno pszenicy",
                        "skup pszenicy",
                        "tona pszenicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зерно этого растения, используемое для производства муки и кормов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пшеница",
                                "target_lang_sense_clarification": "зерно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9ee1ca7-f5fa-3950-8dd4-deeac78788ef",
                    "sense_definition": "Tradycyjna potrawa wigilijna sporządzana z obtłuczonej pszenicy, maku, miodu i bakalii; kutia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wigilijnym stole w ich domu zawsze pojawiała się słodka <w>pszenica</w> z makiem.",
                            "target_lang_translations": {
                                "ru": "На рождественском столе в их доме всегда была сладкая <w>пшеница</w> с маком."
                            }
                        },
                        {
                            "text": "Babcia przygotowała pyszną <w>pszenicę</w> według starego, rodzinnego przepisu.",
                            "target_lang_translations": {
                                "ru": "Бабушка приготовила вкусную <w>кутью</w> по старому семейному рецепту."
                            }
                        }
                    ],
                    "synonyms": [
                        "kutia"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Bukowina"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционное рождественское блюдо из пшеницы, мака, меда и сухофруктов; кутья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кутья",
                                "target_lang_sense_clarification": "блюдо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "35465344-cd3e-3f96-9d72-4fe73b2823f9",
                    "sense_definition": "Polskie nazwisko.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Pszenica</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Пшеница</w> был избран председателем совета микрорайона."
                            }
                        },
                        {
                            "text": "Nie znam nikogo, kto nosiłby nazwisko <w>Pszenica</w>.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого, кто носил бы фамилию <w>Пшеница</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пшеница",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
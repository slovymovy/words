    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b410440-5617-31c9-b57c-ecb8c9a8aaf2",
                    "sense_definition": "Osoba nieporadna, roztargniona, która nie rozumie bieżącej sytuacji lub nie potrafi załatwić prostych spraw.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_clumsy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy pracownik to straszny <w>nieogar</w>, bo ciągle myli zamówienia klientów.",
                            "target_lang_translations": {
                                "ru": "Этот новый сотрудник — жуткая <w>бестолочь</w>, так как постоянно путает заказы клиентов."
                            }
                        },
                        {
                            "text": "Mój brat to taki <w>nieogar</w>, że zgubił klucze do domu trzy razy w tym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Мой брат такой <w>растяпа</w>, что потерял ключи от дома три раза за этот месяц."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjeb"
                    ],
                    "antonyms": [
                        "ogar"
                    ],
                    "common_phrases": [
                        "straszny nieogar",
                        "totalny nieogar"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "słowo potoczne, często używane przez młodzież"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неуклюжий, рассеянный человек, который не ориентируется в ситуации или не может решить простые вопросы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растяпа",
                                "target_lang_sense_clarification": "о рассеянном, неуклюжем человеке"
                            },
                            {
                                "target_lang_word": "тормоз",
                                "target_lang_sense_clarification": "о том, кто медленно соображает (сленг)"
                            },
                            {
                                "target_lang_word": "бестолочь",
                                "target_lang_sense_clarification": "о бестолковом, неумелом человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cb31133-daee-320d-a8b6-6b1135baa60c",
                    "sense_definition": "Stan dużego nieporządku, chaosu organizacyjnego lub brak kontroli nad tym, co się dzieje.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim pokoju panuje taki <w>nieogar</w>, że nie mogę znaleźć telefonu ani portfela.",
                            "target_lang_translations": {
                                "ru": "У меня в комнате такой <w>бардак</w>, что я не могу найти ни телефона, ни кошелька."
                            }
                        },
                        {
                            "text": "Przepraszam za ten organizacyjny <w>nieogar</w>, ale mieliśmy nagłą awarię systemu.",
                            "target_lang_translations": {
                                "ru": "Извините за эту организационную <w>неразбериху</w>, но у нас произошел внезапный сбой системы."
                            }
                        }
                    ],
                    "synonyms": [
                        "bałagan",
                        "bajzel",
                        "syf",
                        "nieład",
                        "nieporządek",
                        "miszmasz"
                    ],
                    "antonyms": [
                        "porządek"
                    ],
                    "common_phrases": [
                        "organizacyjny nieogar",
                        "mieć nieogar"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "słowo potoczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сильного беспорядка, организационного хаоса или отсутствия контроля над происходящим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бардак",
                                "target_lang_sense_clarification": "беспорядок, хаос (разг.)"
                            },
                            {
                                "target_lang_word": "неразбериха",
                                "target_lang_sense_clarification": "запутанная ситуация, отсутствие порядка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
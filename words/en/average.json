        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cc2f66b5-af93-3d67-a264-7cf8df4fa8d6",
                    "sense_definition": "To have a specific amount as an average value over a period of time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "statistical_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature <w>averaged</w> twenty degrees during our holiday in Spain.",
                            "target_lang_translations": {
                                "ru": "Температура <w>составляла в среднем</w> двадцать градусов во время нашего отпуска в Испании."
                            }
                        },
                        {
                            "text": "She <w>averaged</w> very high marks throughout the semester in all her subjects.",
                            "target_lang_translations": {
                                "ru": "Она <w>получала в среднем</w> очень высокие оценки по всем предметам в течение семестра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "average out at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенную величину в качестве среднего значения за определенный период времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составлять в среднем",

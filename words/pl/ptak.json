    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "65d4d23b-8b6d-3e46-91b3-67fd1be64c09",
                    "sense_definition": "Zwierzę kręgowe, które ma ciało pokryte piórami, posiada dziób i skrzydła, a większość jego gatunków potrafi latać.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą dokarmiamy <ptaki> w naszym ogrodzie ziarnem i słoniną.",
                            "target_lang_translations": {
                                "ru": "Зимой мы подкармливаем <птиц> в нашем саду зерном и салом."
                            }
                        },
                        {
                            "text": "Ten egzotyczny <ptak> ma niezwykle kolorowe pióra.",
                            "target_lang_translations": {
                                "ru": "У этой экзотической <птицы> необычайно красочные перья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wolny jak ptak",
                        "śpiew ptaków",
                        "gniazdo ptaka",
                        "niebieski ptak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позвоночное животное, тело которого покрыто перьями, имеющее клюв и крылья; большинство видов способно летать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "птица",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "054307ce-6170-3c1b-ac34-b0e7565ba3f2",
                    "sense_definition": "Potoczne, często eufemistyczne określenie męskiego narządu płciowego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "body_part_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały chłopiec biegał po plaży nago, nie zasłaniając swojego <ptaka>.",
                            "target_lang_translations": {
                                "ru": "Маленький мальчик бегал по пляжу голым, не прикрывая свой <писюн>."
                            }
                        },
                        {
                            "text": "Lekarz urolog dokładnie zbadał <ptaka> pacjenta, aby wykluczyć infekcję.",
                            "target_lang_translations": {
                                "ru": "Врач-уролог тщательно осмотрел <член> пациента, чтобы исключить инфекцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "penis",
                        "prącie",
                        "siusiak",
                        "członek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "euphemistic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто эвфемистическое название мужского полового органа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "член",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "писюн",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "abb08bb0-4729-358a-ad75-07d4e4dea083",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Ptak> jest naszym nowym sąsiadem i mieszka pod piątką.",
                            "target_lang_translations": {
                                "ru": "Господин <Птак> — наш новый сосед, он живёт в квартире номер пять."
                            }
                        },
                        {
                            "text": "Na liście gości weselnych znalazła się rodzina państwa <Ptaków>.",
                            "target_lang_translations": {
                                "ru": "В списке свадебных гостей оказалась семья <Птаков>."
                            }
                        }
                    ],

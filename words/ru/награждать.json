    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "76678f5c-793e-3528-a4a8-f1c92fdb8808",
                    "sense_definition": "Давать или вручать какую-либо награду в знак признания заслуг, отличий или успехи.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "award_giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор школы будет торжественно <награждать> победителей олимпиады.",
                            "target_lang_translations": {
                                "en": "The school principal will solemnly <award> the winners of the Olympiad."
                            }
                        },
                        {
                            "text": "Командование решило <награждать> солдат за проявленную храбрость медалями.",
                            "target_lang_translations": {
                                "en": "The command decided to <award> the soldiers medals for the bravery they displayed."
                            }
                        }
                    ],
                    "synonyms": [
                        "премировать"
                    ],
                    "common_phrases": [
                        "награждать орденом",
                        "награждать медалью",
                        "награждать победителей",
                        "посмертно награждать"
                    ],
                    "target_lang_definitions": {
                        "en": "To give or present an award in recognition of merit, distinction, or success."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "award",
                                "target_lang_sense_clarification": "prizes, medals, or official recognition"
                            },
                            {
                                "target_lang_word": "reward",
                                "target_lang_sense_clarification": "general recompense for effort or merit"
                            },
                            {
                                "target_lang_word": "decorate",
                                "target_lang_sense_clarification": "with a medal or order (military context)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a502713-272d-3bb3-8817-541bb233ee66",
                    "sense_definition": "Наделять кого-либо каким-либо качеством, свойством или способностью (обычно природным или ценным).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "endowing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Природа умеет щедро <награждать> людей удивительными талантами.",
                            "target_lang_translations": {
                                "en": "Nature knows how to generously <endow> people with amazing talents."
                            }
                        },
                        {
                            "text": "Родители не всегда могут <награждать> своих детей крепким здоровьем.",
                            "target_lang_translations": {
                                "en": "Parents cannot always <endow> their children with robust health."
                            }
                        }
                    ],
                    "synonyms": [
                        "наделять"
                    ],
                    "antonyms": [
                        "обделять"
                    ],
                    "common_phrases": [
                        "щедро награждать",
                        "природа награждает",
                        "награждать талантом",
                        "награждать умом"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide someone with a specific quality, trait, or ability (usually natural or valuable)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "endow",
                                "target_lang_sense_clarification": "with qualities, talents, or features"
                            },
                            {
                                "target_lang_word": "gift",
                                "target_lang_sense_clarification": "with natural abilities"
                            },
                            {
                                "target_lang_word": "bless",
                                "target_lang_sense_clarification": "with health, luck, or children"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a39a002-07bf-3b22-848a-53b28a0d568f",
                    "sense_definition": "Давать кому-либо обидное прозвище или наносить удар (используется иронически).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ironic_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчишки во дворе любили <награждать> новичков смешными кличками.",
                            "target_lang_translations": {
                                "en": "The boys in the yard loved to <dub> newcomers funny nicknames."
                            }
                        },
                        {
                            "text": "В порыве гнева он мог <награждать> обидчика увесистым тумаком.",
                            "target_lang_translations": {
                                "en": "In a fit of anger, he could <reward> the offender with a heavy punch."
                            }
                        }
                    ],
                    "common_phrases": [
                        "награждать прозвищем",
                        "награждать пинком",
                        "награждать взглядом",
                        "награждать эпитетом"
                    ],
                    "target_lang_definitions": {
                        "en": "To give someone an offensive nickname or deliver a physical blow (used ironically)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dub",
                                "target_lang_sense_clarification": "give a nickname or title"
                            },
                            {
                                "target_lang_word": "reward",
                                "target_lang_sense_clarification": "ironically, with something unpleasant"
                            },
                            {
                                "target_lang_word": "bestow",
                                "target_lang_sense_clarification": "ironic or formal usage"
                            }
                        ]
                    }
                }
            ]

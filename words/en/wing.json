                },
                {
                    "sense_id": "3b01f053-cfb3-38ed-99a4-f27b6b98143e",
                    "sense_definition": "A panel of a car which encloses the wheel area, especially the front wheels.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "side_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car's front <w>wing</w> was dented in the accident.",
                            "target_lang_translations": {
                                "ru": "Переднее <w>крыло</w> автомобиля помялось в аварии."
                            }
                        },
                        {
                            "text": "He had to replace the damaged <w>wing</w> after the crash.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось заменить повреждённое <w>крыло</w> после аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "fender"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"

                {
                    "sense_id": "d2e24511-572f-37d3-92cb-cc738054f215",
                    "sense_definition": "Sprawiać, że ktoś staje się bardziej szlachetny, wrażliwy, kulturalny i lepszy moralnie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_development",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sztuka i literatura mają wielką moc, by <w>uczłowieczać</w> nawet najbardziej zatwardziałe serca.",
                            "target_lang_translations": {
                                "ru": "Искусство и литература обладают великой силой <w>очеловечивать</w> даже самые очерствевшие сердца."
                            }
                        },
                        {
                            "text": "Wierzę, że ciężka praca nad sobą może <w>uczłowieczać</w> ludzi i zmieniać ich podejście do świata.",
                            "target_lang_translations": {
                                "ru": "Я верю, что тяжелая работа над собой может <w>очеловечивать</w> людей и менять их отношение к миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "uszlachetniać",
                        "cywilizować",
                        "uwrażliwiać"
                    ],
                    "antonyms": [
                        "odczłowieczać",
                        "dehumanizować",
                        "znieczulać"
                    ],
                    "common_phrases": [
                        "uczłowieczać wizerunek",
                        "uczłowieczać system",
                        "uczłowieczać relacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать кого-либо более человечным, благородным, культурным и нравственно лучшим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очеловечивать",
                                "target_lang_sense_clarification": "делать более гуманным и нравственным"
                            },
                            {
                                "target_lang_word": "облагораживать",
                                "target_lang_sense_clarification": "делать благородным, возвышать"
                            },
                            {
                                "target_lang_word": "гуманизировать",
                                "target_lang_sense_clarification": "смягчать нравы, делать гуманным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b39d279-d91f-3313-a83f-13fb3705fed3",
                    "sense_definition": "Nadawać cechy ludzkie zwierzętom, przedmiotom, zjawiskom przyrody lub pojęciom abstrakcyjnym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anthropomorphism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autorzy bajek dla dzieci lubią <w>uczłowieczać</w> zwierzęta, przypisując im ludzkie wady i zalety.",
                            "target_lang_translations": {
                                "ru": "Авторы детских сказок любят <w>очеловечивать</w> животных, приписывая им человеческие недостатки и достоинства."
                            }
                        },
                        {
                            "text": "Starożytni grecy mieli tendencję, by <w>uczłowieczać</w> siły natury, wyobrażając je sobie jako bogów.",
                            "target_lang_translations": {
                                "ru": "Древние греки имели склонность <w>очеловечивать</w> силы природы, представляя их себе в виде богов."
                            }
                        }
                    ],
                    "synonyms": [
                        "antropomorfizować",
                        "uosabiać",
                        "personifikować"
                    ],
                    "antonyms": [
                        "uprzedmiotawiać",
                        "odczłowieczać"
                    ],
                    "common_phrases": [

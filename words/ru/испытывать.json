                        "испытывать страх",
                        "испытывать боль",
                        "испытывать трудности",
                        "испытывать чувство"
                    ],
                    "target_lang_definitions": {
                        "en": "To undergo or feel a specific physical or emotional state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "experience",
                                "target_lang_sense_clarification": "to undergo a feeling or event"
                            },
                            {

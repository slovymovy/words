                        {
                            "text": "The wind <w>bore</w> the sound of bells across the valley.",
                            "target_lang_translations": {
                                "ru": "Ветер <w>донес</w> звук колоколов через долину."
                            }
                        }
                    ],
                    "synonyms": [
                        "carry",
                        "convey",
                        "bring",
                        "transport"
                    ],
                    "common_phrases": [
                        "bearing gifts",
                        "bearing news"
                    ],
                    "target_lang_definitions": {

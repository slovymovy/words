                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car <w>careered</w> down the road and crashed into a hedge.",
                            "target_lang_translations": {
                                "ru": "Машина <w>неслась</w> по дороге и врезалась в живую изгородь."
                            }

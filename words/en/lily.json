    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1bb20c1-c4ef-3da5-81c4-70d4f6c833c0",
                    "sense_definition": "Any of several flowers in the genus Lilium, typically having large, trumpet-shaped, and fragrant blooms.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "flower_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She planted a beautiful white <w>lily</w> in the center of her garden.",
                            "target_lang_translations": {
                                "ru": "Она посадила красивую белую <w>лилию</w> в центре своего сада."
                            }
                        },
                        {
                            "text": "The bouquet was filled with roses and a fragrant orange <w>lily</w>.",
                            "target_lang_translations": {
                                "ru": "Букет был наполнен розами и ароматной оранжевой <w>лилией</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gild the lily",
                        "tiger lily",
                        "Easter lily",
                        "lily of the valley"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цветок рода Лилия (Lilium), обычно с крупными, ароматными бутонами в форме трубы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лилия",
                                "target_lang_sense_clarification": "цветок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b96a8f5b-8015-3d58-8197-aa3eaecd0a1d",
                    "sense_definition": "Any of various plants that resemble true lilies but belong to different families, such as the water lily.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "flower_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A large green frog sat on the leaf of a water <w>lily</w>.",
                            "target_lang_translations": {
                                "ru": "Большая зеленая лягушка сидела на листе водяной <w>лилии</w>."
                            }
                        },
                        {
                            "text": "Although it is called a peace <w>lily</w>, this plant is not a true lily.",
                            "target_lang_translations": {
                                "ru": "Хотя это растение называют «мирной <w>лилией</w>», оно не является настоящей лилией."
                            }
                        }
                    ],
                    "common_phrases": [
                        "water lily",
                        "peace lily",
                        "calla lily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Различные растения, напоминающие настоящие лилии, но принадлежащие к другим семействам (например, кувшинка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лилия",
                                "target_lang_sense_clarification": "в названиях других растений (напр. водяная лилия)"
                            },
                            {
                                "target_lang_word": "кувшинка",
                                "target_lang_sense_clarification": "водяная лилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "128dd483-7042-360c-8635-ae8cf7bcc138",
                    "sense_definition": "A stylized flower design used in heraldry, often referring to the fleur-de-lis.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "symbol_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight carried a shield painted with a golden <w>lily</w> on a blue field.",
                            "target_lang_translations": {
                                "ru": "Рыцарь нес щит, на котором была изображена золотая <w>лилия</w> на синем поле."
                            }
                        },
                        {
                            "text": "This ancient crest features a <w>lily</w> as a symbol of royalty.",
                            "target_lang_translations": {
                                "ru": "На этом древнем гербе изображена <w>лилия</w> как символ королевской власти."
                            }
                        }
                    ],
                    "synonyms": [
                        "fleur-de-lis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стилизованное изображение цветка в геральдике, часто относящееся к флёр-де-лис."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "39eb4b99-48d7-3bfd-8d82-aa6c3acd92ee",
                    "sense_definition": "An English surname originating as an occupation for a merchant.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Chapman</w> has been teaching history at our school for over ten years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Чепмен</w> преподает историю в нашей школе уже более десяти лет."
                            }
                        },
                        {
                            "text": "We invited the <w>Chapman</w> family to join us for dinner next Saturday.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Чепмен</w> поужинать с нами в следующую субботу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "occupational origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Английская фамилия, происходящая от названия профессии торговца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Чепмен",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca10d97f-1aed-375f-ba3c-a896566ae4fa",
                    "sense_definition": "A place name used for various towns and communities in the United States and Australia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They stopped in the small town of <w>Chapman</w> during their road trip across Kansas.",
                            "target_lang_translations": {
                                "ru": "Они остановились в маленьком городке <w>Чепмен</w> во время своего путешествия по Канзасу."
                            }
                        },
                        {
                            "text": "The family moved to <w>Chapman</w> to enjoy a quieter life in the countryside.",
                            "target_lang_translations": {
                                "ru": "Семья переехала в <w>Чепмен</w>, чтобы насладиться более спокойной жизнью в сельской местности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название различных городов и населенных пунктов в США и Австралии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Чепмен",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02615cc3-0f38-33fc-83fa-d3605a064533",
                    "sense_definition": "A dealer or merchant, especially one who travels from place to place.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "merchant_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The traveling <w>chapman</w> arrived in the village selling ribbons, tools, and small trinkets.",
                            "target_lang_translations": {
                                "ru": "Странствующий <w>торговец</w> прибыл в деревню, продавая ленты, инструменты и мелкие безделушки."
                            }
                        },
                        {
                            "text": "In medieval times, a <w>chapman</w> was often the only source of news and goods for remote areas.",
                            "target_lang_translations": {
                                "ru": "В средневековье <w>коробейник</w> часто был единственным источником новостей и товаров для отдаленных районов."
                            }
                        }
                    ],
                    "synonyms": [
                        "peddler",
                        "cheapjack"
                    ],
                    "common_phrases": [
                        "traveling chapman",
                        "itinerant chapman"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Торговец или купец, особенно тот, кто путешествует с места на место (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "странствующий торговец",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "коробейник",
                                "target_lang_sense_clarification": "торговец мелкими товарами в разнос"
                            },
                            {
                                "target_lang_word": "разносчик",
                                "target_lang_sense_clarification": "уличный торговец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43690c1b-469a-338f-be27-da7cdf65fdb6",
                    "sense_definition": "A purchaser or customer.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "merchant_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The merchant hoped to find a willing <w>chapman</w> for his expensive silk fabrics.",
                            "target_lang_translations": {
                                "ru": "Купец надеялся найти готового <w>покупателя</w> для своих дорогих шелковых тканей."
                            }
                        },
                        {
                            "text": "If you cannot find a <w>chapman</w> by noon, we must lower the price of the goods.",
                            "target_lang_translations": {
                                "ru": "Если вы не найдете <w>покупателя</w> к полудню, нам придется снизить цену на товары."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покупатель или клиент (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покупатель",
                                "target_lang_sense_clarification": "человек, приобретающий товар"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
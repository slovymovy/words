                {
                    "sense_id": "16d32694-da07-327e-97f9-f98cc24b06d1",
                    "sense_definition": "The fact or process of no longer having something or having less of it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "deprivation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>loss</w> of his job caused significant financial stress.",

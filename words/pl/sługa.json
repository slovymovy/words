                {
                    "sense_id": "133bea04-2c92-3dbe-a1c0-c28321508886",
                    "sense_definition": "Osoba, która wykonuje prace dla kogoś innego, jest od kogoś zależna i spełnia jego polecenia; w dawnym znaczeniu pracownik domowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "servant_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierny <w>sługa</w> zawsze dbał o interesy swojego pana.",
                            "target_lang_translations": {
                                "ru": "Верный <w>слуга</w> всегда заботился об интересах своего господина."
                            }
                        },
                        {
                            "text": "Nie jestem twoim <w>sługą</w>, więc posprzątaj po sobie sam.",
                            "target_lang_translations": {
                                "ru": "Я тебе не <w>слуга</w>, поэтому убери за собой сам."
                            }
                        }
                    ],
                    "synonyms": [
                        "służący",
                        "pachołek"
                    ],
                    "antonyms": [
                        "pan"
                    ],
                    "common_phrases": [
                        "wierny sługa",
                        "sługa Boży",
                        "uniżony sługa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который выполняет работу для кого-то другого, зависит от него и исполняет его приказы; в устаревшем значении — домашний работник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слуга",
                                "target_lang_sense_clarification": "человек, выполняющий чьи-то поручения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba21b0dd-3ce8-344c-b3c6-e47abbc896e9",
                    "sense_definition": "Dworzanin lub urzędnik pełniący służbę na dworze władcy (w kontekście historycznym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "servant_court",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Królewski <w>sługa</w> przyniósł ważne wieści z frontu.",
                            "target_lang_translations": {
                                "ru": "Королевский <w>слуга</w> принёс важные вести с фронта."
                            }
                        },
                        {
                            "text": "Wielu <w>sług</w> zabiegało o względy i łaskę monarchy.",
                            "target_lang_translations": {
                                "ru": "Многие <w>слуги</w> добивались расположения и милости монарха."
                            }
                        }
                    ],

            "senses": [
                {
                    "sense_id": "55bf8625-7238-3784-ab8d-23a6cff83cf0",
                    "sense_definition": "Oddać coś komuś na własność w zamian za pieniądze.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <sprzedać> ten stary samochód, żeby kupić nowy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <w>продать</w> эту старую машину, чтобы купить новую."
                            }
                        },
                        {
                            "text": "Udało nam się <sprzedać> dom z dużym zyskiem.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <w>продать</w> дом с большой прибылью."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbyć"
                    ],
                    "antonyms": [
                        "kupić",
                        "nabyć"
                    ],
                    "common_phrases": [
                        "sprzedać samochód",
                        "tanio sprzedać",
                        "sprzedać z zyskiem",
                        "sprzedać na raty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать что-то кому-то в собственность в обмен на деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продать",
                                "target_lang_sense_clarification": "обменять на деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22653582-09f5-3a07-8fa2-e5ad23c06cae",
                    "sense_definition": "Zdradzić swoje wartości, ideały lub bliskich dla osiągnięcia korzyści osobistych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "betrayal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polityk został oskarżony o to, że chciał <sprzedać> interesy kraju.",
                            "target_lang_translations": {
                                "ru": "Политика обвинили в том, что он хотел <w>продать</w> интересы страны."
                            }
                        },
                        {
                            "text": "Nigdy nie zamierzam <sprzedać> swoich przekonań za żadne pieniądze.",
                            "target_lang_translations": {
                                "ru": "Я никогда не собираюсь <w>продавать</w> свои убеждения ни за какие деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdradzić"
                    ],
                    "common_phrases": [
                        "sprzedać duszę",
                        "sprzedać ideały",
                        "sprzedać przyjaciół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предать свои ценности, идеалы или близких ради личной выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продать",
                                "target_lang_sense_clarification": "предать ради выгоды"
                            },
                            {
                                "target_lang_word": "предать",
                                "target_lang_sense_clarification": "изменить, выдать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d266da3-ec6b-36f6-b3a6-c73702cf5a43",
                    "sense_definition": "Poinformować władze lub policję o czyimś przewinieniu; donieść na kogoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "betrayal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestępca bał się, że wspólnik może go <sprzedać> policji.",
                            "target_lang_translations": {
                                "ru": "Преступник боялся, что сообщник может <w>сдать</w> его полиции."
                            }
                        },
                        {
                            "text": "To niehonorowe, żeby <sprzedać> kolegów dla łagodniejszego wyroku.",
                            "target_lang_translations": {
                                "ru": "Это бесчестно — <w>сдать</w> коллег ради более мягкого приговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "wsypać",
                        "wydać"
                    ],
                    "common_phrases": [
                        "sprzedać kumpla",
                        "sprzedać na policji"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщить властям или полиции о чьем-то проступке; донести на кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "выдать полиции"
                            },
                            {
                                "target_lang_word": "заложить",
                                "target_lang_sense_clarification": "донести"
                            }
                        ]
                    }
                }

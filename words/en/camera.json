                {
                    "sense_id": "89c62c8f-8df8-3bb8-9aca-168f74f2bee5",
                    "sense_definition": "The viewpoint in a three-dimensional game or simulation.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "viewpoint",
                    "name_type": "no",
                    "examples": [
                        {

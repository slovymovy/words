    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f11f113-3315-3193-9d60-3f6385ec1645",
                    "sense_definition": "Pressure, worry, or difficulty caused by a demanding situation or heavy responsibilities.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "tension_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The long hours at work put a severe <w>strain</w> on his health.",
                            "target_lang_translations": {
                                "ru": "Длительная работа создала серьезную <w>нагрузку</w> на его здоровье."
                            }
                        },
                        {
                            "text": "She felt the <w>strain</w> of trying to balance her job and her family life.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала <w>напряжение</w> от попыток совместить работу и семейную жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "propensity",
                        "proneness"
                    ],
                    "antonyms": [
                        "ease",
                        "relaxation"
                    ],
                    "common_phrases": [
                        "under strain",
                        "put a strain on",
                        "financial strain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давление, беспокойство или трудности, вызванные сложной ситуацией или большой ответственностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напряжение",
                                "target_lang_sense_clarification": "состояние стресса или давления"
                            },
                            {
                                "target_lang_word": "нагрузка",
                                "target_lang_sense_clarification": "тяжесть, бремя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a1f1ff1-ff54-359c-b048-b9f7c6ba2fe3",
                    "sense_definition": "An injury to a muscle or part of the body caused by stretching it too much or working it too hard.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athlete had to withdraw from the race due to a leg <w>strain</w>.",
                            "target_lang_translations": {
                                "ru": "Спортсмену пришлось сняться с соревнований из-за <w>растяжения</w> ноги."
                            }
                        },
                        {
                            "text": "He suffered a painful muscle <w>strain</w> while lifting heavy boxes.",
                            "target_lang_translations": {
                                "ru": "Он получил болезненное <w>растяжение</w> мышцы, поднимая тяжелые коробки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "muscle strain",
                        "back strain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Травмировать часть тела, перенапрягая или слишком сильно растягивая её."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растянуть",
                                "target_lang_sense_clarification": "травмировать связки или мышцы"
                            },
                            {
                                "target_lang_word": "потянуть",
                                "target_lang_sense_clarification": "разг. травмировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ec84fdc-6f38-3ad6-972a-9f166c97832a",
                    "sense_definition": "A specific variety or type of a bacterium, virus, or plant.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "biology_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists have identified a new <w>strain</w> of the flu virus.",
                            "target_lang_translations": {
                                "ru": "Ученые выявили новый <w>штамм</w> вируса гриппа."
                            }
                        },
                        {
                            "text": "This particular <w>strain</w> of wheat is resistant to cold weather.",
                            "target_lang_translations": {
                                "ru": "Этот конкретный <w>сорт</w> пшеницы устойчив к холодной погоде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "new strain",
                        "virus strain",
                        "flu strain"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Особая разновидность или тип бактерии, вируса или растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штамм",
                                "target_lang_sense_clarification": "вируса или бактерии"
                            },
                            {
                                "target_lang_word": "сорт",
                                "target_lang_sense_clarification": "растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e2a29c5-b1aa-384b-8879-e4a36f25097d",
                    "sense_definition": "A particular quality, tendency, or trait in a person's character or ancestry.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "character_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a <w>strain</w> of stubbornness that runs in her family.",
                            "target_lang_translations": {
                                "ru": "В её семье прослеживается <w>черта</w> упрямства."
                            }
                        },
                        {
                            "text": "He showed a surprising <w>strain</w> of humor during the serious meeting.",
                            "target_lang_translations": {
                                "ru": "Он проявил удивительную <w>нотку</w> юмора во время серьезной встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "propensity",
                        "proneness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особое качество, склонность или черта в характере человека или его происхождении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черта",
                                "target_lang_sense_clarification": "особенность характера"
                            },
                            {
                                "target_lang_word": "склонность",
                                "target_lang_sense_clarification": "предрасположенность"
                            },
                            {
                                "target_lang_word": "жилка",
                                "target_lang_sense_clarification": "особая способность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e944ae3-9e4a-3544-9be6-d58baa863149",
                    "sense_definition": "The sound of music, especially a melody or a part of a song.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could hear the faint <w>strains</w> of a violin coming from the window.",
                            "target_lang_translations": {
                                "ru": "Мы могли слышать слабые <w>звуки</w> скрипки, доносящиеся из окна."
                            }
                        },
                        {
                            "text": "The orchestra played the opening <w>strains</w> of the national anthem.",
                            "target_lang_translations": {
                                "ru": "Оркестр сыграл первые <w>аккорды</w> национального гимна."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "455a1827-1746-3423-9cb0-00ad1f0f52f3",
                    "sense_definition": "Предопределённый, неизбежный; такой, исход или результат которого известен заранее и не подлежит изменению.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "predetermined",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Исход этого поединка казался <w>предрешённым</w> с самых первых минут.",
                            "target_lang_translations": {
                                "en": "The outcome of this duel seemed <w>foregone</w> from the very first minutes."
                            }
                        },
                        {
                            "text": "Это был <w>предрешённый</w> результат, которого никто не мог изменить.",
                            "target_lang_translations": {
                                "en": "It was a <w>predetermined</w> result that no one could change."
                            }
                        }
                    ],
                    "synonyms": [
                        "предопределённый",
                        "неизбежный",
                        "неминуемый",
                        "очевидный"
                    ],
                    "antonyms": [
                        "неопределённый",
                        "неизвестный",
                        "сомнительный"
                    ],
                    "common_phrases": [
                        "предрешённый исход",
                        "предрешённый результат",
                        "дело предрешено",
                        "предрешённый финал"
                    ],
                    "target_lang_definitions": {
                        "en": "Predetermined, inevitable; having an outcome or result that is known in advance and cannot be changed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "foregone",
                                "target_lang_sense_clarification": "often used in the phrase 'foregone conclusion'"
                            },
                            {
                                "target_lang_word": "predetermined",
                                "target_lang_sense_clarification": "decided or established in advance"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8f5943b0-cee2-3d6d-ba8e-17d694e3233f",
                    "sense_definition": "Страдательное причастие прошедшего времени от глагола <w>предрешить</w>; такой, который решили или утвердили заранее.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "predetermined",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Назначение нового руководителя было <w>предрешено</w> советом директоров ещё вчера.",
                            "target_lang_translations": {
                                "en": "The appointment of the new manager was <w>decided</w> by the board of directors yesterday."
                            }
                        },
                        {
                            "text": "Будущее компании было <w>предрешено</w> подписанием этого важного контракта.",
                            "target_lang_translations": {
                                "en": "The company's future was <w>sealed</w> by the signing of this important contract."
                            }
                        }
                    ],
                    "synonyms": [

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e660e6c-2139-3473-bebe-b2a481f38cb0",
                    "sense_definition": "Ostatni fragment jakiegoś wydarzenia, okresu czasu lub pozostałość większej całości",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sklepy odzieżowe wyprzedają teraz tanio ostatnie <w>końcówki</w> zimowej kolekcji.",
                            "target_lang_translations": {
                                "ru": "Магазины одежды сейчас дешево распродают последние <w>остатки</w> зимней коллекции."
                            }
                        },
                        {
                            "text": "Z całego filmu najbardziej podobała mi się emocjonująca <w>końcówka</w>.",
                            "target_lang_translations": {
                                "ru": "Из всего фильма мне больше всего понравилась захватывающая <w>концовка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "końcówka roku",
                        "końcówka meczu",
                        "sama końcówka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последняя часть события, периода времени или остаток чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концовка",
                                "target_lang_sense_clarification": "финал фильма, книги или спортивного события"
                            },
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "оставшаяся часть товара или коллекции"
                            },
                            {
                                "target_lang_word": "окончание",
                                "target_lang_sense_clarification": "завершение действия или периода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0319a036-9062-36b8-8b86-1e5f3b9d0daa",
                    "sense_definition": "Wymienna lub stała część stanowiąca fizyczne zakończenie jakiegoś urządzenia lub przedmiotu",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie nowoczesne elektryczne szczoteczki do zębów mają wymienne <w>końcówki</w>.",
                            "target_lang_translations": {
                                "ru": "У всех современных электрических зубных щеток есть сменные <w>насадки</w>."
                            }
                        },
                        {
                            "text": "Majster sprawnie podważył wieko puszki płaską <w>końcówką</w> śrubokrętu.",
                            "target_lang_translations": {
                                "ru": "Мастер ловко поддел крышку банки плоским <w>наконечником</w> отвертки."
                            }
                        }
                    ],
                    "common_phrases": [

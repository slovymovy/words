            "senses": [
                {
                    "sense_id": "59a2b3a8-b8ae-32ec-873b-28d6408072fb",
                    "sense_definition": "An initialism of <w>football</w> <w>club</w>, commonly used in the names of soccer teams.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Liverpool <w>FC</w> won the championship match last night.",
                            "target_lang_translations": {
                                "ru": "<w>ФК</w> «Ливерпуль» выиграл чемпионский матч вчера вечером."
                            }
                        },
                        {
                            "text": "He has supported his local <w>FC</w> since he was a child.",
                            "target_lang_translations": {
                                "ru": "Он болел за свой местный <w>футбольный клуб</w> с детства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "local FC",
                        "support an FC"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «football club» (футбольный клуб), часто используемая в названиях футбольных команд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ФК",
                                "target_lang_sense_clarification": "аббревиатура в названиях"
                            },
                            {
                                "target_lang_word": "футбольный клуб",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbf8a9a9-dfb4-3f10-951c-2c8f216f636e",
                    "sense_definition": "An initialism of <w>foster</w> <w>care</w>, a system in which a minor is placed into a ward, group home, or private home.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The social worker specialized in placing children into <w>FC</w>.",
                            "target_lang_translations": {
                                "ru": "Социальный работник специализировался на устройстве детей в <w>приемные семьи</w>."
                            }
                        },
                        {
                            "text": "Many children in <w>FC</w> are waiting for a permanent family.",
                            "target_lang_translations": {
                                "ru": "Многие дети в <w>системе опеки</w> ждут постоянную семью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "placed in FC",
                        "FC system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «foster care» (патронатное воспитание), системы, при которой несовершеннолетний помещается под опеку или в приемную семью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патронатное воспитание",
                                "target_lang_sense_clarification": "система устройства детей"
                            },
                            {
                                "target_lang_word": "система опеки",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acb51cb1-62e4-3ebb-8028-d335aa2678ef",
                    "sense_definition": "An initialism of <w>family</w> <w>court</w>, a court of law that handles cases regarding domestic matters.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The custody battle was settled in <w>FC</w> after several months.",
                            "target_lang_translations": {
                                "ru": "Спор об опеке был урегулирован в <w>суде по семейным делам</w> спустя несколько месяцев."
                            }
                        },
                        {
                            "text": "Lawyers in <w>FC</w> deal with sensitive domestic issues.",
                            "target_lang_translations": {
                                "ru": "Юристы в <w>суде по семейным делам</w> занимаются деликатными семейными вопросами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «family court» (суд по семейным делам), суда, рассматривающего дела, касающиеся семейных вопросов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суд по семейным делам",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caa0a7a8-63a0-3ab0-be81-5c6c80dd849d",
                    "sense_definition": "An initialism of <w>finance</w> <w>charge</w>, a fee charged for the use of credit or the extension of existing credit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can avoid the <w>FC</w> by paying your credit card balance in full.",

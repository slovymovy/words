            "senses": [
                {
                    "sense_id": "c8efd0e6-5fa9-3909-a2e3-8fff5a97206e",
                    "sense_definition": "Настольная игра для двух участников на клетчатой доске, в которой используются специальные круглые фишки.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "game_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По вечерам дедушка учил внука играть в <шашки>.",
                            "target_lang_translations": {
                                "en": "In the evenings, the grandfather taught his grandson to play <checkers>."
                            }
                        },
                        {
                            "text": "Мы решили сыграть в <шашки>, чтобы скоротать время в поезде.",
                            "target_lang_translations": {
                                "en": "We decided to play <checkers> to pass the time on the train."
                            }
                        }
                    ],
                    "common_phrases": [
                        "играть в шашки",
                        "партия в шашки",
                        "турнир по шашкам"
                    ],
                    "target_lang_definitions": {
                        "en": "A board game for two players played on a checkered board using special round pieces."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "checkers",
                                "target_lang_sense_clarification": "US/Canada"
                            },
                            {
                                "target_lang_word": "draughts",
                                "target_lang_sense_clarification": "UK/Commonwealth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d679be8d-6fb8-3733-a1eb-9074eabd61bf",
                    "sense_definition": "Форма именительного или винительного падежа множественного числа существительного <w>шашка</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

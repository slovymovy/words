                },
                {
                    "sense_id": "e6d62461-e239-3df9-a5c3-81c5174d4123",
                    "sense_definition": "A condition of servitude endured by a slave; the state of being owned by another person.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "system_of_oppression",
                    "name_type": "no",
                    "examples": [
                        {

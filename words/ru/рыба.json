    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "023d4ed8-9fbd-3b70-bcd3-9e39298068ae",
                    "sense_definition": "Водное холоднокровное позвоночное животное, дышащее жабрами, с конечностями в виде плавников и кожей, обычно покрытой чешуёй.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старик ловил неводом <рыбу> в синем море.",
                            "target_lang_translations": {
                                "en": "The old man was catching <w>fish</w> with a seine net in the blue sea."
                            }
                        },
                        {
                            "text": "В прозрачной воде плавала маленькая серебристая <рыба>.",
                            "target_lang_translations": {
                                "en": "A small silvery <w>fish</w> was swimming in the clear water."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ловить рыбу",
                        "живая рыба",
                        "золотая рыба",
                        "нема как рыба"
                    ],
                    "target_lang_definitions": {
                        "en": "An aquatic cold-blooded vertebrate animal that breathes with gills, has fins, and is usually covered with scales."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fish",
                                "target_lang_sense_clarification": "animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7431c06d-9160-3769-8775-0cce821384ae",
                    "sense_definition": "Блюдо или продукт питания, приготовленные из мяса этого животного.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ужин мама приготовила вкусную жареную <рыбу> с картошкой.",
                            "target_lang_translations": {
                                "en": "For dinner, Mom cooked delicious fried <w>fish</w> with potatoes."
                            }
                        },
                        {
                            "text": "В этом ресторане подают отличную солёную <рыбу>.",
                            "target_lang_translations": {
                                "en": "This restaurant serves excellent salted <w>fish</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "жареная рыба",
                        "красная рыба",
                        "чистить рыбу",
                        "есть рыбу"
                    ],
                    "target_lang_definitions": {
                        "en": "A dish or food product prepared from the flesh of this animal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fish",
                                "target_lang_sense_clarification": "food"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50bfd206-638e-32fa-bd2e-a4088737b1aa",
                    "sense_definition": "Вялый, бесстрастный, холодный или равнодушный ко всему человек.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он такая <рыба>, никогда не проявляет никаких эмоций.",
                            "target_lang_translations": {
                                "en": "He is such a <w>cold fish</w>; he never shows any emotions."
                            }
                        },
                        {
                            "text": "Не будь <рыбой>, скажи хоть что-нибудь в своё оправдание!",
                            "target_lang_translations": {
                                "en": "Don't be a <w>cold fish</w>; say at least something in your defense!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "холодная рыба",
                        "ни рыба ни мясо"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is lethargic, dispassionate, cold, or indifferent to everything."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cold fish",
                                "target_lang_sense_clarification": "idiom"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1890f03f-ff18-34eb-ad99-ce4b964dbd08",
                    "sense_definition": "Предварительный набросок, заготовка или бессмысленный текст-заполнитель для макета.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [

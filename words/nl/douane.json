                {
                    "sense_id": "54e7b90a-c66d-3353-b51d-f57182cb426f",
                    "sense_definition": "Een overheidsdienst die belast is met de controle op het internationaal verkeer van mensen, dieren en goederen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>douane</w> controleert alle goederen die het land in- uit- en doorgevoerd worden.",
                            "target_lang_translations": {
                                "en": "<w>Customs</w> checks all goods entering, leaving, or passing through the country.",
                                "ru": "<w>Таможня</w> проверяет все товары, которые ввозятся, вывозятся или следуют транзитом через страну."
                            }
                        },
                        {
                            "text": "We moesten onze koffers openmaken voor de <w>douane</w>.",
                            "target_lang_translations": {
                                "en": "We had to open our suitcases for <w>customs</w>.",
                                "ru": "Нам пришлось открыть чемоданы перед <w>таможней</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "door de douane",
                        "bij de douane"
                    ],
                    "target_lang_definitions": {
                        "en": "A government authority responsible for controlling the flow of goods and people into and out of a country",
                        "ru": "Государственная служба, ответственная за контроль международного перемещения людей, животных и товаров"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "customs",
                                "target_lang_sense_clarification": "government agency"
                            }
                        ],

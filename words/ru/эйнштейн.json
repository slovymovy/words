    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f0ac8690-4384-358a-a93b-71d3609d58bf",
                    "sense_definition": "Альберт Эйнштейн, великий физик-теоретик, один из основателей современной теоретической физики и лауреат Нобелевской премии.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_specific",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "В школе мы изучали теорию относительности, которую создал <w>Эйнштейн</w>.",
                            "target_lang_translations": {
                                "en": "In school, we studied the theory of relativity, which <w>Einstein</w> created."
                            }
                        },
                        {
                            "text": "Альберт <w>Эйнштейн</w> изменил наше представление о времени и пространстве.",
                            "target_lang_translations": {
                                "en": "Albert <w>Einstein</w> changed our understanding of time and space."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Альберт Эйнштейн"
                    ],
                    "target_lang_definitions": {
                        "en": "Albert Einstein, the great theoretical physicist, one of the founders of modern theoretical physics and a Nobel Prize winner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Einstein",
                                "target_lang_sense_clarification": "famous physicist"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a496ad0c-e6f8-3c83-91a3-8cedc50c7c4d",
                    "sense_definition": "Очень умный, гениальный человек (часто используется иронично по отношению к тому, кто совершил глупость или сказал очевидную вещь).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты решил эту сложную задачу за минуту? Ну ты просто <w>Эйнштейн</w>!",
                            "target_lang_translations": {
                                "en": "You solved this difficult problem in a minute? Well, you're just an <w>Einstein</w>!"
                            }
                        },
                        {
                            "text": "Не нужно быть <w>Эйнштейном</w>, чтобы понять эту простую инструкцию.",
                            "target_lang_translations": {
                                "en": "You don't have to be an <w>Einstein</w> to understand this simple instruction."
                            }
                        }
                    ],
                    "synonyms": [
                        "гений",
                        "умник",
                        "вундеркинд",
                        "мозг"
                    ],
                    "antonyms": [
                        "глупец",
                        "дурак",
                        "бестолочь"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An extremely smart, genius person (often used ironically for someone who has done something stupid or said something obvious)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Einstein",
                                "target_lang_sense_clarification": "genius or smart person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bca4efb-d5b4-305d-ade2-8f7580f3d566",
                    "sense_definition": "Немецкая и еврейская фамилия.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname_general",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "В телефонном справочнике города значилось несколько человек по фамилии <w>Эйнштейн</w>.",
                            "target_lang_translations": {
                                "en": "Several people with the surname <w>Einstein</w> were listed in the city telephone directory."
                            }
                        },
                        {
                            "text": "Ганс <w>Эйнштейн</w> был известным профессором гидротехники.",
                            "target_lang_translations": {
                                "en": "Hans <w>Einstein</w> was a famous professor of hydraulic engineering."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A German and Jewish surname."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Einstein",
                                "target_lang_sense_clarification": "surname"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe8b243a-f4d0-302f-887f-7dfc0dfb7606",
                    "sense_definition": "Единица измерения энергии, используемая в фотохимии (равна энергии одного моля фотонов).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Один <w>эйнштейн</w> численно равен постоянной Авогадро.",
                            "target_lang_translations": {
                                "en": "One <w>einstein</w> is numerically equal to the Avogadro constant."
                            }
                        },
                        {
                            "text": "В расчетах квантового выхода реакции часто используется <w>эйнштейн</w>.",
                            "target_lang_translations": {
                                "en": "The <w>einstein</w> is often used in calculations of the quantum yield of a reaction."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A unit of energy measurement used in photochemistry (equal to the energy of one mole of photons)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "einstein",
                                "target_lang_sense_clarification": "unit of energy"
                            }
                        ]
                    }
                }
            ]
        }
    ]

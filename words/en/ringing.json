                },
                {
                    "sense_id": "d7b7849a-ae01-3b17-8249-6239121165e8",
                    "sense_definition": "The theft of cars and illegally changing their identities for resale.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police uncovered a large-scale car <w>ringing</w> operation in the city.",
                            "target_lang_translations": {
                                "ru": "Полиция раскрыла крупную операцию по <w>перебивке номеров</w> автомобилей в городе."
                            }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6aefad5d-0dbd-3530-acf5-ca13018c34dc",
                    "sense_definition": "Patrzeć do wnętrza czegoś, często przez otwór, uchylone drzwi lub z ukrycia, aby coś sprawdzić lub zobaczyć.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vision_check",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama cicho <zaglądała> do pokoju, żeby sprawdzić, czy dzieci już śpią.",
                            "target_lang_translations": {
                                "ru": "Мама тихо <w>заглядывала</w> в комнату, чтобы проверить, спят ли уже дети."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy ktoś <zagląda> mi przez ramię, kiedy piszę wiadomość na telefonie.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда кто-то <w>заглядывает</w> мне через плечо, когда я пишу сообщение на телефоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "podglądać",
                        "wglądać",
                        "dostrzegać",
                        "zazdrościć"
                    ],
                    "common_phrases": [
                        "zaglądać przez ramię",
                        "zaglądać do środka",
                        "zaglądać przez dziurkę od klucza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смотреть внутрь чего-либо, часто через отверстие, приоткрытую дверь или из укрытия, чтобы что-то проверить или увидеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглядывать",
                                "target_lang_sense_clarification": "смотреть внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a1a74f4-3ac2-3396-b7c7-1797d8996d9a",
                    "sense_definition": "Odwiedzać kogoś lub wstępować w jakieś miejsce na krótki czas, często bez wcześniejszej zapowiedzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiecaj, że będziesz do nas częściej <zaglądać> w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Пообещай, что будешь чаще к нам <w>заглядывать</w> в следующем году."
                            }
                        },
                        {
                            "text": "W drodze do pracy często <zaglądam> do tej małej piekarni po świeże bułki.",
                            "target_lang_translations": {
                                "ru": "По дороге на работу я часто <w>заглядываю</w> в эту маленькую пекарню за свежими булочками."
                            }
                        }
                    ],
                    "synonyms": [
                        "wglądać"
                    ],
                    "common_phrases": [
                        "zaglądać do kogoś",
                        "zaglądać na kawę",
                        "często tu zaglądać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts regarding visits"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Навещать кого-либо или заходить куда-либо на короткое время, часто без предупреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглядывать",
                                "target_lang_sense_clarification": "навещать, заходить ненадолго"
                            },
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "посещать мимоходом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "453a477d-42d4-3c2e-ae1b-7d59764fa4e3",
                    "sense_definition": "Sprawdzać informacje w książce, notatkach lub innym źródle wiedzy; rzucać okiem na tekst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vision_check",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas nauki nowego języka warto regularnie <zaglądać> do słownika.",
                            "target_lang_translations": {
                                "ru": "При изучении нового языка стоит регулярно <w>заглядывать</w> в словарь."
                            }
                        },
                        {
                            "text": "Student nerwowo <zaglądał> do swoich notatek tuż przed wejściem na egzamin.",
                            "target_lang_translations": {
                                "ru": "Студент нервно <w>заглядывал</w> в свои конспекты прямо перед входом на экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "wglądać"
                    ],
                    "common_phrases": [
                        "zaglądać do słownika",
                        "zaglądać do notatek",
                        "zaglądać do książek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять информацию в книге, заметках или другом источнике знаний; бегло просматривать текст."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглядывать",
                                "target_lang_sense_clarification": "сверяться с источником"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bfd2685-5609-32ad-afc1-109f1eea5a59",
                    "sense_definition": "O słońcu lub świetle: wpadać do wnętrza pomieszczenia i je oświetlać.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "inanimate_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poranne słońce śmiało <zaglądało> przez nieszczelne zasłony w naszej sypialni.",
                            "target_lang_translations": {
                                "ru": "Утреннее солнце смело <w>заглядывало</w> сквозь неплотные шторы в нашей спальне."
                            }
                        },
                        {
                            "text": "Lubię ten pokój, ponieważ słońce <zagląda> tu przez większą część dnia.",
                            "target_lang_translations": {
                                "ru": "Мне нравится эта комната, потому что солнце <w>заглядывает</w> сюда большую часть дня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "słońce zagląda w okna",
                        "księżyc zagląda"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "personification of light/sun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О солнце или свете: проникать внутрь помещения и освещать его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглядывать",
                                "target_lang_sense_clarification": "о свете или солнце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "078e6bb8-ebda-32d2-87df-549da83c3a19",
                    "sense_definition": "O obiektach na zewnątrz (np. drzewach): być widocznym w oknie, znajdować się bardzo blisko szyby.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "inanimate_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare drzewo rosnące w ogrodzie <zaglądało> gałęziami do kuchni na pierwszym piętrze.",
                            "target_lang_translations": {
                                "ru": "Старое дерево, растущее в саду, <w>заглядывало</w> ветвями в кухню на втором этаже."
                            }
                        },
                        {
                            "text": "Ciemny i gęsty las ponuro <zaglądał> w okna opuszczonego domu na wzgórzu.",
                            "target_lang_translations": {
                                "ru": "Темный и густой лес мрачно <w>заглядывал</w> в окна заброшенного дома на холме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gałęzie zaglądają do okien",
                        "śmierć zagląda w oczy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "personification of objects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О внешних объектах (например, деревьях): быть видным в окне, находиться очень близко к стеклу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглядывать",
                                "target_lang_sense_clarification": "о предметах за окном"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a784a6b6-2113-3250-9ef0-d8d8b4e74895",
                    "sense_definition": "О человеке или его поведении: невежливый, резкий, нарушающий правила приличия, недостаточно культурный.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>грубое</w> поведение на празднике испортило настроение всем гостям.",
                            "target_lang_translations": {
                                "en": "His <w>rude</w> behavior at the party ruined the mood for all the guests."
                            }
                        },
                        {
                            "text": "Не стоит давать <w>грубый</w> ответ на вежливую просьбу коллеги.",
                            "target_lang_translations": {
                                "en": "You shouldn't give a <w>rude</w> answer to a colleague's polite request."
                            }
                        }
                    ],
                    "synonyms": [
                        "невежливый",
                        "невоспитанный",
                        "некультурный",
                        "неотёсанный",
                        "простой"
                    ],
                    "antonyms": [
                        "вежливый",
                        "культурный",
                        "деликатный",
                        "ласковый",
                        "изящный"
                    ],
                    "common_phrases": [
                        "грубое поведение",
                        "грубый ответ",
                        "грубые слова",
                        "быть грубым"
                    ],
                    "target_lang_definitions": {
                        "en": "About a person or their behavior: impolite, sharp, violating rules of decency, insufficiently cultured."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rude",
                                "target_lang_sense_clarification": "lacking manners"
                            },
                            {
                                "target_lang_word": "coarse",
                                "target_lang_sense_clarification": "lacking refinement"
                            },
                            {
                                "target_lang_word": "rough",
                                "target_lang_sense_clarification": "lacking gentleness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a49af00b-8ea4-3a6a-94be-fa853de37b56",
                    "sense_definition": "Имеющий неровную, шершавую поверхность; неприятный на ощупь, не гладкий.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <w>грубая</w> ткань совершенно не подходит для пошива летнего платья.",
                            "target_lang_translations": {
                                "en": "This <w>coarse</w> fabric is completely unsuitable for sewing a summer dress."
                            }
                        },
                        {
                            "text": "Рабочие носили перчатки, чтобы <w>грубая</w> кожа не натирала руки.",
                            "target_lang_translations": {
                                "en": "The workers wore gloves so the <w>rough</w> leather wouldn't rub their hands."
                            }
                        }
                    ],
                    "synonyms": [
                        "шершавый",
                        "жестокий",
                        "неотёсанный",
                        "корявый"
                    ],
                    "antonyms": [
                        "звонкий",
                        "тонкий",
                        "незначительный",
                        "отделанный"
                    ],
                    "common_phrases": [
                        "грубая ткань",
                        "грубая кожа",
                        "грубая шерсть",
                        "грубая поверхность"
                    ],
                    "target_lang_definitions": {
                        "en": "Having an uneven, rough surface; unpleasant to the touch, not smooth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rough",
                                "target_lang_sense_clarification": "uneven surface"
                            },
                            {
                                "target_lang_word": "coarse",
                                "target_lang_sense_clarification": "texture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "511c7b91-cc5d-39b6-a48f-145ee58592b2",
                    "sense_definition": "Об ошибке или нарушении: очень значительный, недопустимый, очевидный.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "error",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент допустил одну <w>грубую</w> ошибку в диктанте и получил четвёрку.",
                            "target_lang_translations": {
                                "en": "The student made one <w>gross</w> mistake in the dictation and got a B."
                            }
                        },
                        {
                            "text": "За <w>грубое</w> нарушение правил безопасности сотрудника уволили с работы.",
                            "target_lang_translations": {
                                "en": "The employee was fired for a <w>gross</w> violation of safety rules."
                            }
                        }
                    ],
                    "synonyms": [
                        "вопиющий",
                        "некультурный",
                        "невежливый",
                        "жестокий",
                        "невоспитанный"
                    ],
                    "antonyms": [
                        "незначительный",
                        "культурный",
                        "тонкий",
                        "изящный",
                        "деликатный"
                    ],
                    "common_phrases": [
                        "грубая ошибка",
                        "грубое нарушение",
                        "грубый просчёт"
                    ],
                    "target_lang_definitions": {
                        "en": "About an error or violation: very significant, inadmissible, obvious."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gross",
                                "target_lang_sense_clarification": "of a mistake"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7ec5365-38c1-3f4b-89d6-2f79e93be480",
                    "sense_definition": "О голосе или звуке: низкий, хриплый, лишённый мелодичности.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из коридора послышался <w>грубый</w> голос, который требовал открыть дверь.",
                            "target_lang_translations": {
                                "en": "A <w>gruff</w> voice was heard from the corridor, demanding the door be opened."
                            }
                        },
                        {
                            "text": "После простуды у певицы стал очень <w>грубый</w> и хриплый голос.",
                            "target_lang_translations": {
                                "en": "After the cold, the singer's voice became very <w>rough</w> and hoarse."
                            }
                        }
                    ],
                    "synonyms": [
                        "хриплый",
                        "глухой",
                        "скрипучий"
                    ],
                    "antonyms": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bfd40fe3-258f-3407-afec-fe4f45d4584d",
                    "sense_definition": "Подняться из глубины воды или другой жидкости на поверхность",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный кит медленно <всплыл> на поверхность океана.",
                            "target_lang_translations": {
                                "en": "A huge whale slowly <surfaced> on the ocean."
                            }
                        },
                        {
                            "text": "Деревянная палка сразу <всплыла>, так как она легче воды.",
                            "target_lang_translations": {
                                "en": "The wooden stick immediately <floated up> because it is lighter than water."
                            }
                        }
                    ],
                    "common_phrases": [
                        "всплыть на поверхность",
                        "всплыть со дна"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise from the depths of water or another liquid to the surface"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surface",
                                "target_lang_sense_clarification": "to rise to the top of a liquid"
                            },
                            {
                                "target_lang_word": "float up",
                                "target_lang_sense_clarification": "to rise due to buoyancy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fc9d999-b656-3f6c-8123-0e856e2de084",
                    "sense_definition": "Стать явным, известным или неожиданно появиться (о фактах, воспоминаниях, проблемах)",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ходе расследования <всплыли> новые интересные факты.",
                            "target_lang_translations": {
                                "en": "During the investigation, new interesting facts <came to light>."
                            }
                        },
                        {
                            "text": "В моей памяти внезапно <всплыло> давно забытое имя.",
                            "target_lang_translations": {
                                "en": "A long-forgotten name suddenly <popped up> in my memory."
                            }
                        }
                    ],
                    "synonyms": [
                        "обнару́житься",
                        "появи́ться"
                    ],
                    "common_phrases": [
                        "правда всплыла",
                        "всплыли подробности",
                        "всплыть в памяти"
                    ],
                    "target_lang_definitions": {
                        "en": "To become apparent, known, or appear unexpectedly (regarding facts, memories, or problems)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surface",
                                "target_lang_sense_clarification": "to become known or apparent"
                            },
                            {
                                "target_lang_word": "come to light",
                                "target_lang_sense_clarification": "to be revealed (facts/truth)"
                            },
                            {
                                "target_lang_word": "pop up",
                                "target_lang_sense_clarification": "to appear unexpectedly"
                            },
                            {
                                "target_lang_word": "emerge",
                                "target_lang_sense_clarification": "to become manifest"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "5e3cd0c9-e150-3735-8307-18cdc6b55ec4",
                    "sense_definition": "A brief electronic message or notification sent to someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I sent a quick <w>ping</w> to my colleague to ask for help.",
                            "target_lang_translations": {
                                "ru": "Я отправил быстрое <w>сообщение</w> коллеге с просьбой о помощи."
                            }
                        },
                        {
                            "text": "Did you receive my <w>ping</w> about the meeting time?",
                            "target_lang_translations": {
                                "ru": "Ты получил мое <w>сообщение</w> о времени встречи?"
                            }
                        }
                    ],
                    "synonyms": [
                        "message",
                        "notification"
                    ],
                    "common_phrases": [
                        "send a ping"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in business and technology contexts"

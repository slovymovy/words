    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "722502ea-3d33-32b1-a9e5-d6b32e4175c0",
                    "sense_definition": "Naturalna, zewnętrzna powłoka okrywająca ciało ludzi i zwierząt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą moja <w>skóra</w> staje się bardzo sucha i wymaga nawilżenia.",
                            "target_lang_translations": {
                                "ru": "Зимой моя <w>кожа</w> становится очень сухой и требует увлажнения."
                            }
                        },
                        {
                            "text": "Włosy pokrywają prawie całą powierzchnię <w>skóry</w> człowieka.",
                            "target_lang_translations": {
                                "ru": "Волосы покрывают почти всю поверхность <w>кожи</w> человека."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sucha skóra",
                        "rak skóry",
                        "pielęgnacja skóry",
                        "kolor skóry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественная внешняя оболочка, покрывающая тело людей и животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кожа",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3435486-6d34-3c7c-a8a8-709f9fcb83d7",
                    "sense_definition": "Materiał uzyskany z wyprawionej powłoki zwierzęcej, służący do produkcji ubrań, obuwia i galanterii.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem sobie nowe, eleganckie buty z naturalnej <w>skóry</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил себе новые элегантные туфли из натуральной <w>кожи</w>."
                            }
                        },
                        {
                            "text": "Torebka wykonana ze sztucznej <w>skóry</w> jest tańsza i łatwiejsza w utrzymaniu.",
                            "target_lang_translations": {
                                "ru": "Сумка, изготовленная из искусственной <w>кожи</w>, дешевле и проще в уходе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sztuczna skóra",
                        "naturalna skóra",
                        "kurtka ze skóry",
                        "wyroby ze skóry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, полученный из выделанной шкуры животного, используемый для производства одежды, обуви и галантереи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кожа",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "503df18e-8475-3733-9e49-0a4559f121c2",
                    "sense_definition": "Zewnętrzna, twardsza warstwa owoców, warzyw lub pieczywa.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed zjedzeniem jabłka warto dokładnie umyć jego <w>skórę</w>.",
                            "target_lang_translations": {
                                "ru": "Перед тем как съесть яблоко, стоит тщательно вымыть его <w>кожуру</w>."
                            }
                        },
                        {
                            "text": "Niektórzy ludzie nie lubią jeść chleba z twardą <w>skórą</w>.",
                            "target_lang_translations": {
                                "ru": "Некоторые люди не любят есть хлеб с твердой <w>коркой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skórka"
                    ],
                    "common_phrases": [
                        "skóra od chleba",
                        "skóra z banana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний, более твердый слой фруктов, овощей или хлеба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кожура",
                                "target_lang_sense_clarification": "фруктов, овощей"
                            },
                            {
                                "target_lang_word": "шкурка",
                                "target_lang_sense_clarification": "фруктов, овощей (разг.)"
                            },
                            {
                                "target_lang_word": "корка",
                                "target_lang_sense_clarification": "хлеба"
                            },
                            {
                                "target_lang_word": "кожица",
                                "target_lang_sense_clarification": "тонкая оболочка плодов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c37e5e32-5988-36c1-834e-8d937bf875ea",
                    "sense_definition": "Kurtka uszyta ze skórzanego materiału, często kojarzona z motocyklistami lub subkulturami.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z samochodu wysiadł wysoki mężczyzna ubrany w czarną <w>skórę</w>.",
                            "target_lang_translations": {
                                "ru": "Из машины вышел высокий мужчина, одетый в черную <w>кожанку</w>."
                            }
                        },
                        {
                            "text": "Na koncert rockowy założył swoją starą, znoszoną <w>skórę</w>.",
                            "target_lang_translations": {
                                "ru": "На рок-концерт он надел свою старую, поношенную <w>косуху</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czarna skóra",
                        "chodzić w skórze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to a leather jacket"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Куртка, сшитая из кожаного материала, часто ассоциируемая с мотоциклистами или субкультурами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кожанка",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "косуха",
                                "target_lang_sense_clarification": "байкерская куртка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a519de7-5791-3ec3-b415-737d15a9fefb",
                    "sense_definition": "Metaforyczne określenie własnego życia, zdrowia lub bezpieczeństwa, używane w wyrażeniach idiomatycznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiał szybko uciekać z płonącego budynku, żeby ratować własną <w>skórę</w>.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось быстро бежать из горящего здания, чтобы спасти собственную <w>шкуру</w>."
                            }
                        },
                        {
                            "text": "Przekonałem się na własnej <w>skórze</w>, jak trudna jest to praca.",
                            "target_lang_translations": {
                                "ru": "Я убедился на собственной <w>шкуре</w>, насколько это трудная работа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ratować własną skórę",
                        "na własnej skórze",
                        "dobrać się do skóry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метафорическое обозначение собственной жизни, здоровья или безопасности, используемое в идиоматических выражениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкура",
                                "target_lang_sense_clarification": "в идиомах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f5cbe3c-2791-3504-a3c2-ae18d16006fe",
                    "sense_definition": "Żargonowe określenie ludzkich zwłok, używane w środowisku medycznym lub pogrzebowym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głośna afera w pogotowiu dotyczyła nielegalnego handlu <w>skórami</w>.",
                            "target_lang_translations": {
                                "ru": "Громкий скандал в скорой помощи касался незаконной торговли <w>трупами</w>."
                            }
                        },
                        {
                            "text": "W żargonie pracowników zakładu pogrzebowego ciało zmarłego to po prostu <w>skóra</w>.",
                            "target_lang_translations": {
                                "ru": "На жаргоне работников похоронного бюро тело умершего — это просто <w>труп</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łowcy skór",
                        "handel skórami"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "medical or funeral industry slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жаргонное название человеческого трупа, используемое в медицинской или похоронной среде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труп",
                                "target_lang_sense_clarification": "значение сленга"
                            },
                            {
                                "target_lang_word": "тело",
                                "target_lang_sense_clarification": "значение сленга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7473906b-db3f-3d5b-a143-d2916a3addce",
                    "sense_definition": "Potoczne lub gwarowe określenie portfela na pieniądze.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kieszonkowiec zwinął mu <w>skórę</w> z tylnej kieszeni spodni.",
                            "target_lang_translations": {
                                "ru": "Карманник вытащил у него <w>кошелёк</w> из заднего кармана брюк."
                            }
                        },
                        {
                            "text": "Zgubiłem gdzieś moją <w>skórę</w> ze wszystkimi dokumentami i gotówką.",
                            "target_lang_translations": {
                                "ru": "Я где-то потерял свой <w>кошелёк</w> со всеми документами и наличными."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "prison or street slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное или жаргонное название кошелька для денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошелёк",
                                "target_lang_sense_clarification": "значение сленга"
                            },
                            {
                                "target_lang_word": "лопатник",
                                "target_lang_sense_clarification": "жарг."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
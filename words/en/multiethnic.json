            "senses": [
                {
                    "sense_id": "a6a37c8f-99f9-32e5-aa21-9a6cb7cdcdac",
                    "sense_definition": "Consisting of or relating to several different ethnic groups.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "London is a truly <w>multiethnic</w> city with people from all over the world.",
                            "target_lang_translations": {
                                "ru": "Лондон — по-настоящему <w>многонациональный</w> город, где живут люди со всего мира."
                            }
                        },
                        {
                            "text": "They are working hard to build a stable and inclusive <w>multiethnic</w> society.",
                            "target_lang_translations": {
                                "ru": "Они усердно работают над созданием стабильного и инклюзивного <w>мультиэтнического</w> общества."
                            }
                        }
                    ],
                    "synonyms": [
                        "multicultural",
                        "polyethnic",
                        "diverse"
                    ],
                    "antonyms": [
                        "monoethnic",
                        "homogeneous"
                    ],
                    "common_phrases": [
                        "multiethnic society",
                        "multiethnic community",
                        "multiethnic state",
                        "multiethnic background"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из нескольких различных этнических групп или относящийся к ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "многонациональный",
                                "target_lang_sense_clarification": "объединяющий представителей многих наций и народностей"
                            },
                            {
                                "target_lang_word": "мультиэтнический",
                                "target_lang_sense_clarification": "относящийся к нескольким этносам; часто используется в социологическом контексте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07f7cd9c-1f71-361c-8c8e-91cb69b61f4c",
                    "sense_definition": "A person who belongs to more than one ethnic group.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a <w>multiethnic</w>, she celebrates traditions from both sides of her family.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>человеком смешанного происхождения</w>, она соблюдает традиции обеих ветвей своей семьи."
                            }
                        },
                        {
                            "text": "The survey asked <w>multiethnics</w> to describe their unique cultural backgrounds.",
                            "target_lang_translations": {
                                "ru": "В опросе просили <w>людей смешанного этнического происхождения</w> описать свой уникальный культурный фон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "identify as multiethnic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, принадлежащий к более чем одной этнической группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человек смешанного происхождения",
                                "target_lang_sense_clarification": "лицо, имеющее предков разных национальностей"
                            }
                        ]
                    }
                }
            ]

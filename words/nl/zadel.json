                    "sense_id": "ae03e49f-728d-3a3d-bb4c-de9e7d24f7b1",
                    "sense_definition": "Een zitplaats van leer of kunststof op de rug van een dier (zoals een paard) of op een voertuig (zoals een fiets of brommer)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "seat_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn fiets heeft een nieuw <w>zadel</w> nodig omdat het oude gescheurd is.",
                            "target_lang_translations": {
                                "ru": "Моему велосипеду нужно новое <w>седло</w>, так как старое порвалось."
                            }
                        },
                        {
                            "text": "De ruiter sprong soepel in het <w>zadel</w> van zijn paard.",
                            "target_lang_translations": {
                                "ru": "Всадник ловко запрыгнул в <w>седло</w> своей лошади."
                            }
                        }
                    ],
                    "synonyms": [
                        "zitplaats"
                    ],
                    "common_phrases": [
                        "in het zadel",
                        "vast in het zadel",
                        "uit het zadel lichten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сиденье из кожи или пластика на спине животного (например, лошади) или на транспортном средстве (например, велосипеде или мопеде)"
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "29598bc8-9e70-3f1d-abda-a9e3ef366f09",
                    "sense_definition": "Форма творительного падежа единственного числа мужского или среднего рода местоимения <w>тот</w> (или <w>то</w>).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tot_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед <w>тем</w> как выйти из дома, он проверил документы.",
                            "target_lang_translations": {
                                "en": "Before leaving the house, he checked his documents."
                            }
                        },
                        {
                            "text": "Мы не согласны с <w>тем</w> решением, которое принял директор.",
                            "target_lang_translations": {
                                "en": "We do not agree with <w>that</w> decision which the director made."
                            }
                        },
                        {
                            "text": "Вместе с <w>тем</w> мы должны учитывать мнение остальных участников.",
                            "target_lang_translations": {
                                "en": "Along with <w>that</w>, we must consider the opinion of the other participants."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перед тем как",
                        "вместе с тем",
                        "в связи с тем",
                        "с тем чтобы",
                        "за тем"
                    ],
                    "target_lang_definitions": {
                        "en": "Instrumental case singular masculine or neuter form of the pronoun 'that' (or 'it')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative pronoun"
                            },
                            {
                                "target_lang_word": "it",
                                "target_lang_sense_clarification": "referring to a previously mentioned fact or object"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0775136a-4ca4-35e1-ab7b-3e60b2c0406d",
                    "sense_definition": "Форма дательного падежа множественного числа местоимения <w>тот</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tot_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель подошёл к <w>тем</w> ученикам, которые сидели у окна.",
                            "target_lang_translations": {
                                "en": "The teacher approached <w>those</w> students who were sitting by the window."
                            }
                        },
                        {
                            "text": "Мы привыкли доверять только <w>тем</w> людям, которых давно знаем.",
                            "target_lang_translations": {
                                "en": "We are used to trusting only <w>those</w> people whom we have known for a long time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "к тем",
                        "по тем",
                        "благодаря тем"
                    ],
                    "target_lang_definitions": {
                        "en": "Dative case plural form of the pronoun 'that' (meaning 'those')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "those",
                                "target_lang_sense_clarification": "demonstrative pronoun (plural)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9549033f-5548-3655-9423-19c36275276d",
                    "sense_definition": "Указательное слово, используемое для сопоставления степени проявления признака; обычно употребляется во второй части сложного союза «чем..., тем...».",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чем больше мы тренируемся, <w>тем</w> лучше становятся наши результаты.",
                            "target_lang_translations": {
                                "en": "The more we train, <w>the</w> better our results become."
                            }
                        },
                        {
                            "text": "Ситуация была сложной, но, <w>тем</w> не менее, мы справились с задачей.",
                            "target_lang_translations": {
                                "en": "The situation was complex, but <w>nevertheless</w>, we handled the task."
                            }
                        },
                        {
                            "text": "Чем выше поднимаешься в горы, <w>тем</w> труднее становится дышать.",
                            "target_lang_translations": {
                                "en": "The higher you climb into the mountains, <w>the</w> harder it becomes to breathe."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тем не менее",
                        "тем более",
                        "тем самым",
                        "тем лучше",
                        "тем временем"
                    ],
                    "target_lang_definitions": {
                        "en": "Demonstrative word used to compare degrees; usually used in the second part of the complex conjunction 'the..., the...'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the",
                                "target_lang_sense_clarification": "used in comparative structures"
                            },
                            {
                                "target_lang_word": "all the",
                                "target_lang_sense_clarification": "intensifier (e.g., all the more)"
                            },
                            {
                                "target_lang_word": "so much",
                                "target_lang_sense_clarification": "degree intensifier"
                            }
                        ]
                    }
                }
            ]
        },

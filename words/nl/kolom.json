{
    "word_family": [
        "kolomhoofd",
        "kolombreedte"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "771cc463-b1d7-3c15-bc8d-c311aadff71f",
                    "sense_definition": "Een verticale reeks van gegevens of getallen die onder elkaar staan in een tabel of schema.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "data_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet alle namen in de eerste <w>kolom</w> van de lijst.",
                            "target_lang_translations": {
                                "ru": "Поместите все имена в первый <w>столбец</w> списка."
                            }
                        },
                        {
                            "text": "In een spreadsheet is het makkelijk om een hele <w>kolom</w> op te tellen.",
                            "target_lang_translations": {
                                "ru": "В электронной таблице легко сложить целый <w>столбец</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "rij"
                    ],
                    "common_phrases": [
                        "eerste kolom",
                        "linker kolom",
                        "kolom toevoegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный ряд данных или чисел, расположенных друг под другом в таблице или схеме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбец",
                                "target_lang_sense_clarification": "в таблице"
                            },
                            {
                                "target_lang_word": "колонка",
                                "target_lang_sense_clarification": "ряд данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79059d8e-289b-3c93-a0b4-40d0752fad7f",
                    "sense_definition": "Een verticaal gedeelte van een pagina waarin de tekst is geplaatst, gescheiden van andere gedeelten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "layout",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tekst in het tijdschrift was verdeeld in drie smalle <w>kolommen</w>.",
                            "target_lang_translations": {
                                "ru": "Текст в журнале был разделен на три узкие <w>колонки</w>."
                            }
                        },
                        {
                            "text": "Lees het artikel in de rechter <w>kolom</w> voor meer informatie.",
                            "target_lang_translations": {
                                "ru": "Прочитайте статью в правой <w>колонке</w> для получения дополнительной информации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "twee kolommen",
                        "smalle kolom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальная часть страницы, в которой размещен текст, отделенная от других частей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колонка",
                                "target_lang_sense_clarification": "текста"
                            },
                            {
                                "target_lang_word": "столбец",
                                "target_lang_sense_clarification": "текста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90a4cae2-9e40-386c-a31d-807f36b99627",
                    "sense_definition": "Een verticaal, vrijstaand bouwelement van steen, hout of metaal dat dient ter ondersteuning of versiering.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het zware dak van de tempel rust op marmeren <w>kolommen</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжелая крыша храма покоится на мраморных <w>колоннах</w>."
                            }
                        },
                        {
                            "text": "De architect plaatste een grote <w>kolom</w> bij de ingang van het gebouw.",
                            "target_lang_translations": {
                                "ru": "Архитектор разместил большую <w>колонну</w> у входа в здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "zuil"
                    ],
                    "common_phrases": [
                        "stenen kolom",
                        "betonnen kolom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный строительный элемент из камня, дерева или металла, служащий для опоры или украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колонна",
                                "target_lang_sense_clarification": "архитектура"
                            },
                            {
                                "target_lang_word": "столб",
                                "target_lang_sense_clarification": "опора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c82fdc45-870a-3bed-851b-52dfd47b2954",
                    "sense_definition": "Een artikel of rubriek in een krant of tijdschrift, vaak verwijzend naar de ruimte die het inneemt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "journalism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn kritische mening verscheen regelmatig in de <w>kolommen</w> van het dagblad.",
                            "target_lang_translations": {
                                "ru": "Его критическое мнение регулярно появлялось в <w>колонках</w> ежедневной газеты."
                            }
                        },
                        {
                            "text": "De journalist vulde zijn wekelijkse <w>kolom</w> met politiek commentaar.",
                            "target_lang_translations": {
                                "ru": "Журналист заполнил свою еженедельную <w>колонку</w> политическим комментарием."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Статья или раздел в газете или журнале, часто подразумевающий занимаемое место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колонка",
                                "target_lang_sense_clarification": "в газете"
                            },
                            {
                                "target_lang_word": "рубрика",
                                "target_lang_sense_clarification": "раздел"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
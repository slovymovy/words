                    "frequency": "High",
                    "semantic_group_id": "age_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат <w>моложе</w> меня на два года.",
                            "target_lang_translations": {
                                "en": "My brother is two years <w>younger</w> than me."
                            }
                        },
                        {
                            "text": "Она выглядит значительно <w>моложе</w> своих лет.",
                            "target_lang_translations": {
                                "en": "She looks significantly <w>younger</w> than her age."
                            }
                        }
                    ],

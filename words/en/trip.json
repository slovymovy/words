{
    "word_family": [
        "tripper",
        "trippy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7479cc80-1893-396e-9093-3c7a624c6b9f",
                    "sense_definition": "A journey; an excursion or jaunt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are planning a <w>trip</w> to the mountains next summer.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>поездку</w> в горы следующим летом."
                            }
                        },
                        {
                            "text": "Have a safe <w>trip</w> back home after the party.",
                            "target_lang_translations": {
                                "ru": "Хорошей <w>поездки</w> домой после вечеринки."
                            }
                        }
                    ],
                    "synonyms": [
                        "journey",
                        "excursion",
                        "tour"
                    ],
                    "common_phrases": [
                        "business trip",
                        "road trip",
                        "day trip",
                        "school trip",
                        "round trip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие или короткая поездка из одного места в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "перемещение на транспорте, обычно короткое"
                            },
                            {
                                "target_lang_word": "путешествие",
                                "target_lang_sense_clarification": "длительная поездка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c29692dd-1d23-31a0-9a7d-1438be6857c8",
                    "sense_definition": "A period of time in which one experiences drug-induced reverie or hallucinations.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He described his first acid <w>trip</w> as a terrifying experience.",
                            "target_lang_translations": {
                                "ru": "Он описал свой первый кислотный <w>трип</w> как ужасающий опыт."
                            }
                        },
                        {
                            "text": "The movie simulates a psychedelic <w>trip</w> with colorful visuals.",
                            "target_lang_translations": {
                                "ru": "Фильм имитирует психоделический <w>трип</w> с помощью красочных визуальных эффектов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bad trip",
                        "acid trip"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние изменённого сознания или галлюцинации, вызванные приёмом наркотиков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трип",
                                "target_lang_sense_clarification": "сленг, психоделическое переживание"
                            },
                            {
                                "target_lang_word": "кайф",
                                "target_lang_sense_clarification": "состояние эйфории"
                            },
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "начало действия наркотика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffc0103f-d16d-3031-9333-087e41b16881",
                    "sense_definition": "A stumble or misstep.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A sudden <w>trip</w> on the stairs caused him to drop the box.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>спотыкание</w> на лестнице заставило его уронить коробку."
                            }
                        },
                        {
                            "text": "She recovered from her <w>trip</w> quickly and continued running.",
                            "target_lang_translations": {
                                "ru": "Она быстро оправилась после <w>спотыкания</w> и продолжила бежать."
                            }
                        }
                    ],
                    "synonyms": [
                        "stumble",
                        "misstep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спотыкание или неверный шаг; потеря равновесия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спотыкание",
                                "target_lang_sense_clarification": "действие по глаголу спотыкаться"
                            },
                            {
                                "target_lang_word": "оплошность",
                                "target_lang_sense_clarification": "переносное значение: ошибка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ae00091-8006-34ee-b7b8-83a9e32ed328",
                    "sense_definition": "Intense involvement in or enjoyment of a condition, often used in psychological phrases.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My manager is on a power <w>trip</w> and keeps changing the rules.",
                            "target_lang_translations": {
                                "ru": "Мой менеджер <w>упивается властью</w> и постоянно меняет правила."
                            }
                        },
                        {
                            "text": "Don't try to lay a guilt <w>trip</w> on me for not coming.",
                            "target_lang_translations": {
                                "ru": "Не пытайся навязать мне <w>чувство вины</w> за то, что я не пришел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "guilt trip",
                        "ego trip",
                        "power trip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интенсивное переживание или навязывание определённого эмоционального состояния (часто в идиомах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загон",
                                "target_lang_sense_clarification": "сленг: навязчивая идея или состояние"
                            },
                            {
                                "target_lang_word": "бзик",
                                "target_lang_sense_clarification": "разг.: странность, причуда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56d5d7f2-ea96-3667-b59b-61725e1e99cd",
                    "sense_definition": "A mechanical cutout device or switch that activates to stop a machine or circuit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The safety <w>trip</w> stopped the engine when it overheated.",
                            "target_lang_translations": {
                                "ru": "Аварийный <w>выключатель</w> остановил двигатель, когда тот перегрелся."
                            }
                        },
                        {
                            "text": "You need to reset the <w>trip</w> in the fuse box to get the lights back on.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сбросить <w>автомат</w> в щитке, чтобы снова включить свет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trip switch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often referred to as a trip switch"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Механическое устройство или выключатель для автоматической остановки машины или размыкания цепи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выключатель",
                                "target_lang_sense_clarification": "устройство размыкания"
                            },
                            {
                                "target_lang_word": "автомат",
                                "target_lang_sense_clarification": "автоматический предохранитель"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "662fe926-b182-32b9-afc7-cc26dcc85ae4",
                    "sense_definition": "To fall over or stumble over an object as a result of striking it with one's foot.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>trip</w> over the loose cables on the floor.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы не <w>споткнуться</w> о лежащие на полу кабели."
                            }
                        },
                        {
                            "text": "I saw him <w>trip</w> and fall while running for the bus.",
                            "target_lang_translations": {
                                "ru": "Я видел, как он <w>споткнулся</w> и упал, когда бежал к автобусу."
                            }
                        }
                    ],
                    "synonyms": [
                        "stumble"
                    ],
                    "common_phrases": [
                        "trip over",
                        "trip up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Споткнуться или оступиться, задев ногой за что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спотыкаться",
                                "target_lang_sense_clarification": "терять равновесие при ходьбе"
                            },
                            {
                                "target_lang_word": "оступаться",
                                "target_lang_sense_clarification": "делать неверный шаг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa5e474f-fa74-3c01-9a64-a6ed4e5a6847",
                    "sense_definition": "To cause a person or animal to fall or stumble by knocking their feet from under them.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player tried to <w>trip</w> his opponent to stop the attack.",
                            "target_lang_translations": {
                                "ru": "Игрок пытался <w>подставить подножку</w> сопернику, чтобы остановить атаку."
                            }
                        },
                        {
                            "text": "He extended his leg to <w>trip</w> the thief as he ran past.",
                            "target_lang_translations": {
                                "ru": "Он выставил ногу, чтобы <w>сбить</w> вора <w>с ног</w>, когда тот пробегал мимо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trip up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбить кого-либо с ног, подставив подножку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подставить подножку",
                                "target_lang_sense_clarification": "сбить с ног"
                            },
                            {
                                "target_lang_word": "сбить с ног",
                                "target_lang_sense_clarification": "заставить упасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60a77187-cacf-326b-a9e3-8367e7b3bbc1",
                    "sense_definition": "To experience a state of reverie or to hallucinate, due to consuming psychoactive drugs.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They spent the whole night <w>tripping</w> on mushrooms in the forest.",
                            "target_lang_translations": {
                                "ru": "Они провели всю ночь в лесу, <w>торча</w> под грибами."
                            }
                        },
                        {
                            "text": "The lights and music made me feel like I was <w>tripping</w>.",
                            "target_lang_translations": {
                                "ru": "Свет и музыка заставили меня почувствовать себя так, будто я <w>глючу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trip out"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать галлюцинации или эйфорию под воздействием наркотиков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торчать",
                                "target_lang_sense_clarification": "сленг: быть под воздействием"
                            },
                            {
                                "target_lang_word": "кайфовать",
                                "target_lang_sense_clarification": "получать удовольствие"
                            },
                            {
                                "target_lang_word": "глючить",
                                "target_lang_sense_clarification": "сленг: видеть галлюцинации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e29ba6ba-c61a-3e06-88b9-8dc54996649c",
                    "sense_definition": "To become unreasonably upset, especially over something unimportant; to act foolishly.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Why are you <w>tripping</w> over such a small mistake?",
                            "target_lang_translations": {
                                "ru": "Почему ты <w>паришься</w> из-за такой мелкой ошибки?"
                            }
                        },
                        {
                            "text": "Don't <w>trip</w>; everything is going to be fine.",
                            "target_lang_translations": {
                                "ru": "Не <w>парься</w>, всё будет хорошо."
                            }
                        }
                    ],
                    "synonyms": [
                        "overreact",
                        "freak out"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспричинно расстраиваться, нервничать или вести себя глупо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "париться",
                                "target_lang_sense_clarification": "сленг: переживать по пустякам"
                            },
                            {
                                "target_lang_word": "психовать",
                                "target_lang_sense_clarification": "нервничать, выходить из себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fba8705a-1e28-3b97-a4cc-0d1d01633027",
                    "sense_definition": "To activate a mechanism, switch, or alarm, or to be activated automatically.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The smoke from the kitchen caused the fire alarm to <w>trip</w>.",
                            "target_lang_translations": {
                                "ru": "Дым с кухни заставил пожарную сигнализацию <w>сработать</w>."
                            }
                        },
                        {
                            "text": "An overload in the system will <w>trip</w> the circuit breaker immediately.",
                            "target_lang_translations": {
                                "ru": "Перегрузка в системе немедленно <w>выбьет</w> автоматический выключатель."
                            }
                        }
                    ],
                    "synonyms": [
                        "activate",
                        "trigger"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сработать (об устройстве), активировать механизм или отключиться (о предохранителе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сработать",
                                "target_lang_sense_clarification": "активироваться"
                            },
                            {
                                "target_lang_word": "выбить",
                                "target_lang_sense_clarification": "разг.: отключить (пробки, автомат)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa5caa88-d797-3b73-8aed-20ff0781c363",
                    "sense_definition": "To move with light, quick steps; to skip or dance lightly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children <w>trip</w> happily through the meadow picking flowers.",
                            "target_lang_translations": {
                                "ru": "Дети радостно <w>скачут</w> по лугу, собирая цветы."
                            }
                        },
                        {
                            "text": "She came <w>tripping</w> down the stairs to greet her guests.",
                            "target_lang_translations": {
                                "ru": "Она <w>сбежала</w> вниз по лестнице, чтобы поприветствовать гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "skip"
                    ],
                    "common_phrases": [
                        "trip the light fantastic"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти легкой, быстрой походкой; бежать вприпрыжку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семенить",
                                "target_lang_sense_clarification": "идти мелкими шажками"
                            },
                            {
                                "target_lang_word": "порхать",
                                "target_lang_sense_clarification": "легко передвигаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "64f303d6-5b8c-3f5a-9afc-432c422369cb",
                    "sense_definition": "Of or relating to three of a kind in poker.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He managed to win the pot holding <w>trip</w> kings.",
                            "target_lang_translations": {
                                "ru": "Ему удалось выиграть банк с <w>тройкой</w> королей."
                            }
                        },
                        {
                            "text": "The player bet aggressively on his <w>trip</w> aces.",
                            "target_lang_translations": {
                                "ru": "Игрок агрессивно ставил на своих <w>трёх</w> тузах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "poker jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Комбинация в покере: три карты одного достоинства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тройка",
                                "target_lang_sense_clarification": "три карты одного ранга"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
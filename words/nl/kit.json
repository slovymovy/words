            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aff8abdc-b95b-372a-8765-76e58c5081ea",
                    "sense_definition": "Een dikvloeibaar materiaal dat hard wordt, gebruikt om naden en kieren waterdicht te maken of om materialen aan elkaar te lijmen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sealant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de randen van het bad opnieuw met <w>kit</w> afwerken.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно заново обработать края ванны <w>герметиком</w>.",
                                "en": "You need to finish the edges of the bath again with <w>caulk</w>."
                            }
                        },
                        {
                            "text": "Gebruik een goede watervaste <w>kit</w> in de badkamer.",
                            "target_lang_translations": {
                                "ru": "Используй хороший водостойкий <w>герметик</w> в ванной.",
                                "en": "Use a good waterproof <w>sealant</w> in the bathroom."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdichtmiddel"
                    ],
                    "common_phrases": [
                        "naden en kieren",
                        "kit aanbrengen",
                        "oude kit verwijderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густой материал, который затвердевает и используется для герметизации швов или склеивания материалов.",
                        "en": "A thick liquid material that hardens, used to waterproof seams or glue materials together."

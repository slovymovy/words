    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6c0f082-2ff4-3b3e-8882-b8551f8a769d",
                    "sense_definition": "Krzyczeć bardzo głośno, podnosić głos w sposób nieprzyjemny lub agresywny (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiad znowu zaczął się <wydzierać> na swojego psa, co obudziło całą okolicę.",
                            "target_lang_translations": {
                                "ru": "Сосед снова начал <орать> на свою собаку, что разбудило всю округу."
                            }
                        },
                        {
                            "text": "Nie musisz się tak <wydzierać>, stoję tuż obok ciebie i wszystko słyszę.",
                            "target_lang_translations": {
                                "ru": "Тебе не обязательно так <орать>, я стою прямо рядом с тобой и всё слышу."
                            }
                        }
                    ],
                    "synonyms": [
                        "krzyczeć",
                        "wrzeszczeć"
                    ],
                    "antonyms": [
                        "szeptać",
                        "milczeć"
                    ],
                    "common_phrases": [
                        "wydzierać się na kogoś",
                        "wydzierać się wniebogłosy",
                        "przestań się wydzierać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech to describe loud yelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кричать очень громко, повышать голос неприятным или агрессивным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орать",
                                "target_lang_sense_clarification": "разговорное, громко кричать"
                            },
                            {
                                "target_lang_word": "вопить",
                                "target_lang_sense_clarification": "издавать громкие, пронзительные крики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54b27d2a-11df-397e-acf7-46b584a2be42",
                    "sense_definition": "Gwałtownie odrywać lub wyrywać fragment czegoś z całości, często powodując uszkodzenie.",

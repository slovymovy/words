{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6acf3c0d-acd3-3404-963a-0456f74b285a",
                    "sense_definition": "Used to indicate that one is thinking, hesitating, or pondering something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "thinking_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hmm</w>... that's an interesting idea.",
                            "target_lang_translations": {
                                "ru": "<w>Хм</w>... это интересная мысль."
                            }
                        },
                        {
                            "text": "<w>Hmm</w>... I just can't decide between the two options.",
                            "target_lang_translations": {
                                "ru": "<w>Хм</w>... я просто не могу выбрать между двумя вариантами."
                            }
                        }
                    ],
                    "synonyms": [
                        "hnn"
                    ],
                    "common_phrases": [
                        "hmm let me see",
                        "hmm interesting",
                        "hmm I wonder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы показать, что человек думает, сомневается или размышляет над чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хм",
                                "target_lang_sense_clarification": "раздумье"
                            },
                            {
                                "target_lang_word": "гм",
                                "target_lang_sense_clarification": "сомнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6d9d07d-3a06-34a4-a24b-956e4b45cbb9",
                    "sense_definition": "Used to express mild surprise, confusion, or to ask someone to repeat what they said when not listening.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "query_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hmm</w>? Oh, I just need to get a drink of water.",
                            "target_lang_translations": {
                                "ru": "<w>Мм</w>? О, мне просто нужно попить воды."
                            }
                        },
                        {
                            "text": "<w>Hmm</w>? What did you say about the paring knife?",
                            "target_lang_translations": {
                                "ru": "<w>А</w>? Что ты сказал про нож для чистки овощей?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "hmm what",
                        "hmm pardon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения легкого удивления, замешательства или просьбы повторить сказанное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мм",
                                "target_lang_sense_clarification": "переспрос"
                            },
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "вопрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16a97037-7202-31fd-9033-a967de336b5d",
                    "sense_definition": "Used at the end of a question to demand an answer, confirmation, or explanation.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "demand_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Just where were you until 2 a.m.? <w>Hmm</w>?",
                            "target_lang_translations": {
                                "ru": "И где же ты был до двух ночи? <w>А</w>?"
                            }
                        },
                        {
                            "text": "You didn't think I would find out about this, did you? <w>Hmm</w>?",
                            "target_lang_translations": {
                                "ru": "Ты ведь не думал, что я об этом узнаю, да? <w>А</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "well hmm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце вопроса, чтобы потребовать ответа, подтверждения или объяснения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "требование ответа"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "побуждение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2a212315-a0e4-3d58-8f49-674499aa2b7e",
                    "sense_definition": "To make a thoughtful humming noise.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>hmmed</w> quietly while looking at the complex menu.",
                            "target_lang_translations": {
                                "ru": "Она тихо <w>хмыкнула</w>, разглядывая сложное меню."
                            }
                        },
                        {
                            "text": "He <w>hmmed</w> and hawed before finally answering the difficult question.",
                            "target_lang_translations": {
                                "ru": "Он <w>хмыкал</w> и мялся, прежде чем наконец ответить на сложный вопрос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hmm and haw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать задумчивый звук, похожий на мычание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хмыкать",
                                "target_lang_sense_clarification": "издавать звук"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e87b05b9-e615-361d-a191-d68efb24cfaf",
                    "sense_definition": "An initialism for a <w>hidden Markov model</w>, a statistical model used in probability theory.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The speech recognition software uses an <w>HMM</w> to predict the next word.",
                            "target_lang_translations": {
                                "ru": "Программа распознавания речи использует <w>СММ</w> для предсказания следующего слова."
                            }
                        },
                        {
                            "text": "We analyzed the data sequence using a standard <w>HMM</w> approach.",
                            "target_lang_translations": {
                                "ru": "Мы проанализировали последовательность данных, используя стандартный метод <w>СММ</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «скрытая марковская модель», статистическая модель, используемая в теории вероятностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "СММ",
                                "target_lang_sense_clarification": "скрытая марковская модель"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
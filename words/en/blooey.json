{
    "word_family": [
        "kablooey"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9c1dd57b-4e13-3c86-b53b-4553674295ce",
                    "sense_definition": "Not functioning correctly; out of order or in a state of chaos.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The whole project went <w>blooey</w> after the manager resigned.",
                            "target_lang_translations": {
                                "ru": "Весь проект пошел <w>прахом</w> после увольнения менеджера."
                            }
                        },
                        {
                            "text": "My computer went <w>blooey</w> right before the deadline.",
                            "target_lang_translations": {
                                "ru": "Мой компьютер полетел <w>к чертям</w> прямо перед дедлайном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go blooey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не функционирующий должным образом; сломанный или находящийся в состоянии хаоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наперекосяк",
                                "target_lang_sense_clarification": "о делах или планах, которые пошли не так"
                            },
                            {
                                "target_lang_word": "прахом",
                                "target_lang_sense_clarification": "о полном провале или разрушении"
                            },
                            {
                                "target_lang_word": "к чертям",
                                "target_lang_sense_clarification": "разговорное выражение для обозначения поломки или краха"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "905db0e3-559d-37e4-83d9-5c79157f1431",
                    "sense_definition": "Used to imitate the sound of an explosion or to mark a sudden, disastrous event.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I pushed the button and <w>blooey</w>, the sparks flew everywhere.",
                            "target_lang_translations": {
                                "ru": "Я нажал на кнопку, и <w>бабах</w> — искры полетели повсюду."
                            }
                        },
                        {
                            "text": "Everything was quiet, then <w>blooey</w>, the fireworks started.",
                            "target_lang_translations": {
                                "ru": "Всё было тихо, а потом <w>бац</w> — и начался фейерверк."
                            }
                        }
                    ],
                    "synonyms": [
                        "bam",
                        "bang",
                        "kablooie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для имитации звука взрыва или для обозначения внезапного, катастрофического события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабах",
                                "target_lang_sense_clarification": "звукоподражание взрыву"
                            },
                            {
                                "target_lang_word": "бац",
                                "target_lang_sense_clarification": "обозначение внезапности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
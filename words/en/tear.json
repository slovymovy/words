                    "common_phrases": [
                        "be torn between",
                        "tear apart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять сильную душевную боль или вызывать внутренний конфликт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрывать",
                                "target_lang_sense_clarification": "мучить противоречиями"

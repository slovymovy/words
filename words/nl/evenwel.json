                                "target_lang_sense_clarification": "indicating contrast"
                            },
                            {
                                "target_lang_word": "nevertheless",
                                "target_lang_sense_clarification": "in spite of that"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "тем не менее",
                                "target_lang_sense_clarification": "несмотря на это"
                            }
                        ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9b48c54-20db-3f02-a3e1-375d82da7c04",
                    "sense_definition": "Właściwość fizyczna materiałów polegająca na zdolności do powrotu do pierwotnego kształtu po odkształceniu lub łatwość wyginania się.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Guma jest materiałem znanym ze swojej dużej <w>elastyczności</w>.",
                            "target_lang_translations": {
                                "ru": "Резина — это материал, известный своей высокой <w>эластичностью</w>."
                            }
                        },
                        {
                            "text": "Ten nowoczesny materiał zachowuje <w>elastyczność</w> nawet w bardzo niskich temperaturach.",
                            "target_lang_translations": {
                                "ru": "Этот современный материал сохраняет <w>эластичность</w> даже при очень низких температурах."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprężystość",
                        "rozciągliwość",
                        "ciągliwość"
                    ],
                    "antonyms": [
                        "sztywność"
                    ],
                    "common_phrases": [
                        "duża elastyczność",
                        "elastyczność materiału",
                        "zachować elastyczność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое свойство материалов, заключающееся в способности возвращаться к первоначальной форме после деформации или легко гнуться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эластичность",
                                "target_lang_sense_clarification": "свойство материала"
                            },
                            {
                                "target_lang_word": "упругость",
                                "target_lang_sense_clarification": "способность возвращать форму"
                            },
                            {
                                "target_lang_word": "гибкость",
                                "target_lang_sense_clarification": "способность гнуться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c83e607-5237-3fc0-8a6f-a14138f8424b",
                    "sense_definition": "Zdolność ciała do wykonywania ruchów w szerokim zakresie; gibkość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularne ćwiczenia jogi znacznie poprawiają <w>elastyczność</w> całego ciała.",
                            "target_lang_translations": {
                                "ru": "Регулярные занятия йогой значительно улучшают <w>гибкость</w> всего тела."
                            }
                        },
                        {
                            "text": "Tancerze muszą dbać o swoją <w>elastyczność</w>, aby unikać bolesnych kontuzji.",
                            "target_lang_translations": {
                                "ru": "Танцоры должны следить за своей <w>гибкостью</w>, чтобы избегать болезненных травм."
                            }
                        }
                    ],
                    "antonyms": [
                        "sztywność"
                    ],
                    "common_phrases": [
                        "ćwiczyć elastyczność",
                        "elastyczność mięśni",
                        "poprawić elastyczność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность тела выполнять движения с большой амплитудой; гибкость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гибкость",
                                "target_lang_sense_clarification": "тела"
                            },
                            {
                                "target_lang_word": "эластичность",
                                "target_lang_sense_clarification": "мышц и связок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "691b1a17-b94a-32eb-b2b6-128bfac49e87",
                    "sense_definition": "Umiejętność łatwego dostosowania się do nowych warunków, sytuacji lub wymagań.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adaptability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej pracy wymagana jest duża <w>elastyczność</w> godzinowa oraz dyspozycyjność.",
                            "target_lang_translations": {
                                "ru": "На этой работе требуется большая <w>гибкость</w> графика и доступность."
                            }
                        },
                        {
                            "text": "Jego <w>elastyczność</w> w negocjacjach pozwoliła na szybkie podpisanie korzystnej umowy.",
                            "target_lang_translations": {
                                "ru": "Его <w>гибкость</w> в переговорах позволила быстро подписать выгодный договор."
                            }
                        }
                    ],
                    "synonyms": [
                        "plastyczność"
                    ],
                    "antonyms": [
                        "nieelastyczność",
                        "sztywność"
                    ],
                    "common_phrases": [
                        "elastyczność cenowa",
                        "elastyczność pracy",
                        "elastyczność umysłu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умение легко приспосабливаться к новым условиям, ситуациям или требованиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гибкость",
                                "target_lang_sense_clarification": "умение приспосабливаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f5449de7-c338-3b3a-98cf-468cf38f5655",
                    "sense_definition": "Присоединить к источнику энергии, системе связи или какой-либо технической сети для обеспечения работы.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мастер пришёл сегодня утром и быстро <w>подключил</w> нам скоростной интернет.",
                            "target_lang_translations": {
                                "en": "The technician came this morning and quickly <w>connected</w> our high-speed internet."
                            }
                        },
                        {
                            "text": "Не забудьте <w>подключить</w> зарядное устройство к телефону, если батарея разряжена.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>connect</w> the charger to the phone if the battery is low."
                            }
                        }
                    ],
                    "synonyms": [
                        "присоединить"
                    ],
                    "antonyms": [
                        "отключить"
                    ],
                    "common_phrases": [
                        "подключить интернет",
                        "подключить устройство",
                        "подключить к сети",
                        "подключить услугу"
                    ],
                    "target_lang_definitions": {
                        "en": "To connect to a power source, communication system, or technical network to ensure operation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connect",
                                "target_lang_sense_clarification": "to a network, device, or service"
                            },
                            {
                                "target_lang_word": "hook up",
                                "target_lang_sense_clarification": "informal; to install or activate a service/device"
                            },
                            {
                                "target_lang_word": "plug in",
                                "target_lang_sense_clarification": "physically to a power source"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3902c31-7653-39a8-a8e2-c84726582ab1",
                    "sense_definition": "Привлечь кого-либо к участию в общем деле или использовать дополнительные ресурсы для решения задачи.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "involvement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно срочно <w>подключить</w> к этому сложному проекту опытных специалистов.",
                            "target_lang_translations": {
                                "en": "We urgently need to <w>bring in</w> experienced specialists for this complex project."
                            }
                        },
                        {
                            "text": "Чтобы решить эту задачу, пришлось <w>подключить</w> всё своё воображение и смекалку.",
                            "target_lang_translations": {
                                "en": "To solve this problem, I had to <w>employ</w> all my imagination and wit."
                            }
                        }
                    ],
                    "synonyms": [
                        "привлечь"
                    ],
                    "antonyms": [
                        "исключить"
                    ],
                    "common_phrases": [
                        "подключить к работе",
                        "подключить коллег",
                        "подключить связи",
                        "подключить родителей"
                    ],
                    "traits": [
                        {

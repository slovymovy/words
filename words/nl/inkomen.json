{
    "word_family": [
        "inkomst",
        "inkom"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4b320583-a71c-3db6-8e54-bfdfe231af51",
                    "sense_definition": "Geld dat men ontvangt voor geleverde arbeid, uit een onderneming of uit vermogen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Net nadat mijn <w>inkomen</w> was gestort, heb ik een nieuwe computer gekocht.",
                            "target_lang_translations": {
                                "en": "Just after my <w>income</w> was deposited, I bought a new computer.",
                                "ru": "Сразу после того, как мне перечислили <w>доход</w>, я купил новый компьютер."
                            }
                        },
                        {
                            "text": "Met een vast <w>inkomen</w> is het makkelijker om een huis te kopen.",
                            "target_lang_translations": {
                                "en": "With a fixed <w>income</w>, it is easier to buy a house.",
                                "ru": "С постоянным <w>доходом</w> купить дом проще."
                            }
                        }
                    ],
                    "synonyms": [
                        "salaris",
                        "loon",
                        "inkomsten",
                        "verdienste"
                    ],
                    "antonyms": [
                        "uitgave"
                    ],
                    "common_phrases": [
                        "vast inkomen",
                        "bruto inkomen",
                        "inkomen genereren"
                    ],
                    "target_lang_definitions": {
                        "en": "Money received for delivered work, from a business, or from assets.",
                        "ru": "Деньги, получаемые за выполненную работу, от бизнеса или активов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "income",
                                "target_lang_sense_clarification": "money received regularly"
                            },
                            {
                                "target_lang_word": "earnings",
                                "target_lang_sense_clarification": "money gained from work"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "доход",
                                "target_lang_sense_clarification": "денежные поступления"
                            },
                            {
                                "target_lang_word": "заработок",
                                "target_lang_sense_clarification": "деньги, полученные за труд"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c52cacf-7dd2-3241-a2dc-76de94b8b901",
                    "sense_definition": "Zich kunnen verplaatsen in iemands gedachten of gevoelens; begrijpen wat iemand bedoelt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ja, daar kan ik wel <w>inkomen</w>.",
                            "target_lang_translations": {
                                "en": "Yes, I can <w>understand</w> that.",
                                "ru": "Да, это я могу <w>понять</w>."
                            }
                        },
                        {
                            "text": "Ik kan er helemaal <w>inkomen</w> dat je boos bent over die beslissing.",
                            "target_lang_translations": {
                                "en": "I can completely <w>understand</w> that you are angry about that decision.",
                                "ru": "Я прекрасно могу <w>понять</w>, что ты злишься из-за этого решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "begrijpen",
                        "invoelen",
                        "snappen"
                    ],
                    "common_phrases": [
                        "daar kan ik inkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to empathize with someone's thoughts or feelings; to understand what someone means.",
                        "ru": "Быть способным понять чьи-то мысли или чувства; понимать, что кто-то имеет в виду."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "comprehend a situation or feeling"
                            },
                            {
                                "target_lang_word": "relate to",
                                "target_lang_sense_clarification": "feel sympathy with"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "understand a point of view"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "понять",
                                "target_lang_sense_clarification": "уловить смысл или чувства"
                            },
                            {
                                "target_lang_word": "войти в положение",
                                "target_lang_sense_clarification": "проявить сочувствие и понимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a20b0c9a-298e-3bb1-986c-cf7924c04eab",
                    "sense_definition": "Op gang komen of wennen aan een activiteit bij de start ervan.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange vakantie moest ik echt weer even <w>inkomen</w> op mijn werk.",
                            "target_lang_translations": {
                                "en": "After the long vacation, I really had to <w>get back into the swing of things</w> at work.",
                                "ru": "После долгого отпуска мне действительно нужно было снова <w>втянуться</w> в работу."
                            }
                        },
                        {
                            "text": "De invaller had tien minuten nodig om in de wedstrijd te <w>inkomen</w>.",
                            "target_lang_translations": {
                                "en": "The substitute needed ten minutes to <w>get into</w> the match.",
                                "ru": "Запасному игроку потребовалось десять минут, чтобы <w>войти</w> в игру."
                            }
                        }
                    ],
                    "synonyms": [
                        "opwarmen",
                        "wennen",
                        "inspelen"
                    ],
                    "common_phrases": [
                        "even inkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To get going or get used to an activity at the start of it.",
                        "ru": "Начать действовать или привыкнуть к деятельности в её начале; войти в рабочий ритм."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get into",
                                "target_lang_sense_clarification": "become accustomed to"
                            },
                            {
                                "target_lang_word": "warm up",
                                "target_lang_sense_clarification": "prepare for activity"
                            },
                            {
                                "target_lang_word": "settle in",
                                "target_lang_sense_clarification": "become comfortable"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "втянуться",
                                "target_lang_sense_clarification": "привыкнуть к процессу"
                            },
                            {
                                "target_lang_word": "войти в ритм",
                                "target_lang_sense_clarification": "освоиться с темпом"
                            },
                            {
                                "target_lang_word": "освоиться",
                                "target_lang_sense_clarification": "почувствовать себя уверенно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "827511de-3b19-3b14-b0d4-7796f9a67de9",
                    "sense_definition": "Een ruimte of plaats binnengaan of binnendringen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regen is via het plafond de kamer <w>ingekomen</w>.",
                            "target_lang_translations": {
                                "en": "The rain <w>came into</w> the room through the ceiling.",
                                "ru": "Дождь <w>проник</w> в комнату через потолок."
                            }
                        },
                        {
                            "text": "Door de hevige storm is er veel water de kelder <w>ingekomen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the heavy storm, a lot of water <w>entered</w> the basement.",
                                "ru": "Из-за сильного шторма в подвал <w>попало</w> много воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "binnenkomen",
                        "binnendringen"
                    ],
                    "antonyms": [
                        "uitgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter or penetrate a room or place.",
                        "ru": "Войти или проникнуть в помещение или место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come in",
                                "target_lang_sense_clarification": "enter a place"
                            },
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "go into"
                            },
                            {
                                "target_lang_word": "get in",
                                "target_lang_sense_clarification": "penetrate"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проникнуть",
                                "target_lang_sense_clarification": "попасть внутрь (часто о воде или холоде)"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "оказаться внутри"
                            }
                        ]
                    }
                }
            ]
        }

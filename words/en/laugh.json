                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "laugh_action_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her loud <w>laugh</w> filled the room and made everyone smile."
                        },
                        {
                            "text": "We had a good <w>laugh</w> about the misunderstanding yesterday."
                        }
                    ],
                    "synonyms": [
                        "chuckle",
                        "giggle",
                        "guffaw"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "16bcc767-0978-3673-8c39-dcec9ad6db7d",
                    "sense_definition": "Uznać coś za mało ważne i świadomie nie zwrócić na to uwagi, na przykład na zakaz, ostrzeżenie lub radę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca autobusu <w>zlekceważył</w> czerwone światło i doprowadził do niebezpiecznej sytuacji na drodze.",
                            "target_lang_translations": {
                                "ru": "Водитель автобуса <w>проигнорировал</w> красный свет и создал опасную ситуацию на дороге."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>zlekceważyć</w> tych niepokojących objawów i jak najszybciej udać się do lekarza.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>игнорировать</w> эти тревожные симптомы и нужно как можно скорее обратиться к врачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbagatelizować",
                        "zbagatelizować",
                        "zbagatelizować",
                        "zbagatelizować"
                    ],
                    "antonyms": [
                        "uwzględnić",
                        "posłuchać"
                    ],
                    "common_phrases": [
                        "zlekceważyć zakaz",
                        "zlekceważyć ostrzeżenie",
                        "zlekceważyć niebezpieczeństwo",
                        "zlekceważyć prawo",
                        "zlekceważyć radę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Счесть что-либо неважным и сознательно не обратить на это внимания (например, на запрет, предупреждение или совет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проигнорировать",
                                "target_lang_sense_clarification": "оставить без внимания"
                            },
                            {
                                "target_lang_word": "пренебречь",
                                "target_lang_sense_clarification": "не придать должного значения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "624437b7-e980-3077-b85f-65bd851c9b2b",
                    "sense_definition": "Potraktować kogoś z brakiem szacunku, okazując mu wyższość, obojętność lub uznając go za gorszego.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mistrz świata <w>zlekceważył</w> swojego młodego przeciwnika i niespodziewanie przegrał ten ważny pojedynek.",
                            "target_lang_translations": {
                                "ru": "Чемпион мира <w>недооценил</w> своего молодого противника и неожиданно проиграл этот важный поединок."
                            }
                        },
                        {
                            "text": "Klient poczuł się urażony, gdy sprzedawca całkowicie go <w>zlekceważył</w> i obsługiwał innych.",
                            "target_lang_translations": {
                                "ru": "Клиент почувствовал себя оскорбленным, когда продавец полностью его <w>проигнорировал</w> и обслуживал других."
                            }
                        }
                    ],
                    "antonyms": [
                        "uszanować",
                        "docenić"
                    ],
                    "common_phrases": [
                        "zlekceważyć przeciwnika",
                        "zlekceważyć rywala",
                        "zlekceważyć rozmówcę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отнестись к кому-либо без уважения, демонстрируя превосходство, равнодушие или считая его хуже себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недооценить",
                                "target_lang_sense_clarification": "посчитать слабым или неважным (о противнике)"
                            },
                            {
                                "target_lang_word": "отнестись с пренебрежением",
                                "target_lang_sense_clarification": "проявить неуважение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
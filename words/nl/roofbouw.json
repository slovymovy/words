            "senses": [
                {
                    "sense_id": "7908cfe0-7f82-3dac-a6fd-a713be4828d7",
                    "sense_definition": "Het uitputten van grond of natuurlijke hulpbronnen door overmatige exploitatie zonder herstel.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "exploitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De protesten tegen <w>roofbouw</w> in de oerbossen namen toe.",
                            "target_lang_translations": {
                                "ru": "Протесты против <w>хищнической эксплуатации</w> девственных лесов усилились."
                            }
                        },
                        {
                            "text": "Door jarenlange <w>roofbouw</w> is de landbouwgrond onvruchtbaar geworden.",
                            "target_lang_translations": {
                                "ru": "Из-за многолетнего <w>истощения</w> сельскохозяйственные угодья стали бесплодными."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitputting"
                    ],
                    "common_phrases": [
                        "roofbouw plegen",
                        "roofbouw op de natuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Истощение почвы или природных ресурсов вследствие чрезмерной эксплуатации без восстановления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хищническая эксплуатация",
                                "target_lang_sense_clarification": "природных ресурсов"
                            },
                            {
                                "target_lang_word": "истощение",
                                "target_lang_sense_clarification": "почвы, ресурсов"
                            },
                            {
                                "target_lang_word": "хищническое использование",
                                "target_lang_sense_clarification": "земли, лесов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30994e36-5de6-32ec-8044-44d56cf76ba0",
                    "sense_definition": "Het overmatig belasten van het lichaam of de geest, wat leidt tot schade of uitputting.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "exploitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met die ongezonde levensstijl pleeg je <w>roofbouw</w> op je lichaam.",
                            "target_lang_translations": {
                                "ru": "С таким нездоровым образом жизни ты работаешь на <w>износ</w>."
                            }
                        },
                        {
                            "text": "Het vele overwerken was pure <w>roofbouw</w> op zijn gezondheid.",
                            "target_lang_translations": {
                                "ru": "Частые переработки приводили к полному <w>истощению</w> его здоровья."
                            }
                        }
                    ],
                    "synonyms": [

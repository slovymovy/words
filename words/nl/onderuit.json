{
    "word_family": [
        "onderuitgaan",
        "onderuithalen",
        "onderuitzakken",
        "onderuitschoppen"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "db7af8e0-62a1-3e41-b67c-e4b5f35c6b29",
                    "sense_definition": "Op de grond vallen doordat men het evenwicht verliest of doordat de benen wegglijden; het verliezen van steun.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "falling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het gladde ijs ging de fietser hard <w>onderuit</w>.",
                            "target_lang_translations": {
                                "en": "On the slippery ice, the cyclist went <w>down</w> hard.",
                                "ru": "На скользком льду велосипедист сильно <w>упал</w>."
                            }
                        },
                        {
                            "text": "De speler werd in het strafschopgebied <w>onderuit</w> gehaald door de verdediger.",
                            "target_lang_translations": {
                                "en": "The player was <w>brought down</w> by the defender in the penalty area.",
                                "ru": "Игрок был <w>сбит с ног</w> защитником в штрафной площадке."
                            }
                        }
                    ],
                    "synonyms": [
                        "omver"
                    ],
                    "antonyms": [
                        "overeind"
                    ],
                    "common_phrases": [
                        "onderuit gaan",
                        "onderuit halen"
                    ],
                    "target_lang_definitions": {
                        "en": "Falling to the ground due to loss of balance or slipping; losing support.",
                        "ru": "Падение на землю из-за потери равновесия или скольжения; потеря опоры."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "down",
                                "target_lang_sense_clarification": "falling to the ground"
                            },
                            {
                                "target_lang_word": "off one's feet",
                                "target_lang_sense_clarification": "losing balance completely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с ног",
                                "target_lang_sense_clarification": "в контексте сбивания или падения"
                            },
                            {
                                "target_lang_word": "наземь",
                                "target_lang_sense_clarification": "направление падения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04c790de-cb14-3258-a596-809dc02f6a9e",
                    "sense_definition": "Zich onttrekken aan een verplichting, afspraak of ongewenste situatie; erin slagen om iets niet te hoeven doen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "escaping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben bang dat we er niet <w>onderuit</w> kunnen om belasting te betalen.",
                            "target_lang_translations": {
                                "en": "I am afraid we cannot <w>get out of</w> paying taxes.",
                                "ru": "Боюсь, нам не <w>отвертеться</w> от уплаты налогов."
                            }
                        },
                        {
                            "text": "Hij probeerde er <w>onderuit</w> te komen, maar de regels waren streng.",
                            "target_lang_translations": {
                                "en": "He tried to <w>get out of</w> it, but the rules were strict.",
                                "ru": "Он пытался <w>уклониться</w> от этого, но правила были строгими."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er niet onderuit kunnen",
                        "er onderuit komen"
                    ],
                    "target_lang_definitions": {
                        "en": "Avoiding or escaping a duty, obligation, or unpleasant situation.",
                        "ru": "Уклонение от обязательства, договоренности или нежелательной ситуации; возможность избежать чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out of",
                                "target_lang_sense_clarification": "escaping an obligation"
                            },
                            {
                                "target_lang_word": "avoid",
                                "target_lang_sense_clarification": "evading a duty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отвертеться",
                                "target_lang_sense_clarification": "разговорное; избежать выполнения чего-либо"
                            },
                            {
                                "target_lang_word": "уклониться",
                                "target_lang_sense_clarification": "избежать ответственности или дела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f716aad-efb4-38da-9afb-e306d8fcabd7",
                    "sense_definition": "Vanuit een positie onder iets vandaan naar buiten of naar voren bewegend.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monteur kwam <w>onderuit</w> de auto gekropen toen hij klaar was.",
                            "target_lang_translations": {
                                "en": "The mechanic came crawling <w>out from under</w> the car when he was finished.",
                                "ru": "Механик вылез <w>из-под</w> машины, когда закончил."
                            }
                        },
                        {
                            "text": "Ze haalde een oud fotoalbum <w>onderuit</w> de kist op zolder.",
                            "target_lang_translations": {
                                "en": "She retrieved an old photo album <w>from the bottom of</w> the chest in the attic.",
                                "ru": "Она достала старый фотоальбом <w>со дна</w> сундука на чердаке."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Moving outwards from underneath something.",
                        "ru": "Движение наружу или вперед из положения под чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from under",
                                "target_lang_sense_clarification": "emerging from beneath"
                            },
                            {
                                "target_lang_word": "out from under",
                                "target_lang_sense_clarification": "movement from a position below"
                            },
                            {
                                "target_lang_word": "from the bottom of",
                                "target_lang_sense_clarification": "retrieving from the lowest part"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "из-под",
                                "target_lang_sense_clarification": "предлог направления движения"
                            },
                            {
                                "target_lang_word": "со дна",
                                "target_lang_sense_clarification": "из самой нижней части чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c3fb016a-5577-3801-9428-48c21887a28b",
                    "sense_definition": "Uitroep om te waarschuwen dat er iets groots (zoals een boom) omvalt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "falling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De houthakker riep luid \"<w>onderuit</w>!\" toen de boom begon te kraken.",
                            "target_lang_translations": {
                                "en": "The lumberjack shouted \"<w>timber</w>!\" loudly when the tree started to crack.",
                                "ru": "Лесоруб громко крикнул: \"<w>Берегись</w>!\", когда дерево начало трещать."
                            }
                        },
                        {
                            "text": "Pas op, <w>onderuit</w>! Die stapel dozen valt jouw kant op.",
                            "target_lang_translations": {
                                "en": "Watch out, <w>look out below</w>! That stack of boxes is falling your way.",
                                "ru": "Осторожно, <w>падает</w>! Эта стопка коробок валится в твою сторону."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A warning cry used when a large object (like a tree) is falling.",
                        "ru": "Возглас, предупреждающий о падении чего-то большого (например, дерева)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "timber",
                                "target_lang_sense_clarification": "warning when a tree falls"
                            },
                            {
                                "target_lang_word": "look out below",
                                "target_lang_sense_clarification": "general warning for falling objects"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Берегись!",
                                "target_lang_sense_clarification": "предупреждение об опасности"
                            },
                            {
                                "target_lang_word": "Падает!",
                                "target_lang_sense_clarification": "предупреждение о падении объекта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "target_lang_definitions": {
                        "en": "Uneventful or fine; used to describe a state of affairs.",
                        "ru": "Хорошо или нормально; без происшествий (как ответ на вопрос о делах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quiet",
                                "target_lang_sense_clarification": "with little activity or news"
                            },
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "satisfactory or okay"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "потихоньку",
                                "target_lang_sense_clarification": "разговорное выражение"
                            },
                            {
                                "target_lang_word": "спокойно",
                                "target_lang_sense_clarification": "без новостей или изменений"
                            }
                        ]
                    }
                }
            ]
        }

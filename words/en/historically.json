    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "79d6102c-9d49-3230-a522-de7770552bfe",
                    "sense_definition": "In the past or according to tradition.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "timeline_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two countries have <w>historically</w> been allies during times of war.",
                            "target_lang_translations": {
                                "ru": "Эти две страны <w>исторически</w> были союзниками во время войн."
                            }
                        },
                        {
                            "text": "This region is <w>historically</w> significant because of its ancient ruins.",
                            "target_lang_translations": {
                                "ru": "Этот регион <w>исторически</w> значим благодаря своим древним руинам."
                            }
                        }
                    ],
                    "synonyms": [
                        "traditionally",
                        "formerly",
                        "previously"
                    ],
                    "antonyms": [
                        "currently",
                        "nowadays",
                        "recently"
                    ],
                    "common_phrases": [
                        "historically significant",
                        "historically accurate"
                    ],
                    "target_lang_definitions": {
                        "ru": "В прошлом или в соответствии с традицией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исторически",
                                "target_lang_sense_clarification": "на протяжении истории, традиционно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62097213-8e63-38f6-9c60-f4850558470b",
                    "sense_definition": "With reference to historical events, facts, or the study of history.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "timeline_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most scholars agree that the king existed <w>historically</w>, although details of his life are unclear.",
                            "target_lang_translations": {
                                "ru": "Большинство ученых согласны, что этот король существовал <w>исторически</w>, хотя детали его жизни неясны."
                            }
                        },
                        {
                            "text": "<w>Historically</w> speaking, this period was a time of great innovation.",
                            "target_lang_translations": {
                                "ru": "<w>Исторически</w> говоря, этот период был временем великих инноваций."
                            }
                        }
                    ],
                    "synonyms": [
                        "factually"
                    ],
                    "common_phrases": [
                        "historically speaking",
                        "exist historically"
                    ],
                    "target_lang_definitions": {
                        "ru": "В контексте исторических событий, фактов или изучения истории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исторически",
                                "target_lang_sense_clarification": "с точки зрения истории, как факт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3698ab4-1fb7-36a5-9aaa-8ad4da3f2199",
                    "sense_definition": "To a degree that is extremely rare or has not happened before.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Interest rates have dropped to <w>historically</w> low levels this year.",
                            "target_lang_translations": {
                                "ru": "В этом году процентные ставки упали до <w>исторически</w> низкого уровня."
                            }
                        },
                        {
                            "text": "The team is performing at a <w>historically</w> high standard this season.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне команда выступает на <w>исторически</w> высоком уровне."
                            }
                        }
                    ],
                    "synonyms": [
                        "unprecedentedly",
                        "exceptionally"
                    ],
                    "common_phrases": [
                        "historically low",
                        "historically high"
                    ],
                    "target_lang_definitions": {
                        "ru": "В степени, которая является крайне редкой или не случалась ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исторически",
                                "target_lang_sense_clarification": "за всю историю, беспрецедентно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
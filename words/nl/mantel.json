                },
                {
                    "sense_id": "fb042cc1-6f30-37b2-a9d1-d9551f69e00d",
                    "sense_definition": "De dikke laag gesteente die zich tussen de aardkorst en de aardkern bevindt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Magma vindt zijn oorsprong vaak diep in de <w>mantel</w> van de aarde.",
                            "target_lang_translations": {
                                "en": "Magma often originates deep in the Earth's <w>mantle</w>."
                            }
                        },
                        {
                            "text": "Wetenschappers bestuderen de samenstelling van de <w>mantel</w> om vulkaanuitbarstingen te begrijpen.",
                            "target_lang_translations": {
                                "en": "Scientists study the composition of the <w>mantle</w> to understand volcanic eruptions."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de mantel van de aarde",
                        "de bovenste mantel"
                    ],
                    "target_lang_definitions": {
                        "en": "The thick layer of rock located between the Earth's crust and its core."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mantle",
                                "target_lang_sense_clarification": "geological layer"

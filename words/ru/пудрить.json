    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2b0e6269-56b2-3eae-828d-9fcde2182d6f",
                    "sense_definition": "Наносить косметическую пудру на кожу лица или тела для придания матовости или изменения оттенка.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cosmetic_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актриса начала <пудрить> лицо перед выходом на сцену.",
                            "target_lang_translations": {
                                "en": "The actress began to <w>powder</w> her face before going on stage."
                            }
                        },
                        {
                            "text": "Она любила подолгу <пудрить> нос, глядя в маленькое зеркальце.",
                            "target_lang_translations": {
                                "en": "She loved to <w>powder</w> her nose for a long time, looking into a small mirror."
                            }
                        }
                    ],
                    "synonyms": [
                        "припудривать"
                    ],
                    "common_phrases": [
                        "пудрить нос",
                        "пудрить лицо",
                        "пудрить щёки"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply cosmetic powder to the skin of the face or body to make it matte or change the shade."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "powder",
                                "target_lang_sense_clarification": "apply cosmetic powder"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "076723b7-c2db-3311-ac57-b62e84160163",
                    "sense_definition": "Вводить в заблуждение, обманывать, говорить неправду (почти всегда используется в устойчивом выражении со словом «мозги»).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хватит мне <пудрить> мозги своими пустыми обещаниями!",
                            "target_lang_translations": {
                                "en": "Stop <w>messing with</w> my head with your empty promises!"
                            }
                        },
                        {
                            "text": "Он пытался <пудрить> нам мозги, но мы сразу поняли его хитрый план.",
                            "target_lang_translations": {
                                "en": "He tried to <w>fool</w> us, but we immediately understood his cunning plan."
                            }
                        }
                    ],
                    "synonyms": [
                        "обманывать",
                        "дурачить",
                        "морочить"
                    ],
                    "common_phrases": [
                        "пудрить мозги"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To mislead, deceive, or tell lies (almost always used in the fixed expression with the word \"brains\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mess with",
                                "target_lang_sense_clarification": "confuse or deceive (usually used with 'head' or 'mind')"
                            },
                            {
                                "target_lang_word": "fool",
                                "target_lang_sense_clarification": "deceive or trick someone"
                            },
                            {
                                "target_lang_word": "brainwash",
                                "target_lang_sense_clarification": "manipulate someone's thoughts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d50f0295-b24e-3945-a756-680959c0b223",
                    "sense_definition": "Посыпать поверхность чего-либо тонким слоем мелкого порошка (сахара, снега и т. п.).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "covering_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кондитер стал аккуратно <пудрить> горячие пончики сладкой смесью.",
                            "target_lang_translations": {
                                "en": "The pastry chef began to carefully <w>dust</w> the hot donuts with the sweet mixture."
                            }
                        },
                        {
                            "text": "Лёгкий снежок начал <пудрить> серые крыши домов.",
                            "target_lang_translations": {
                                "en": "Light snow began to <w>dust</w> the grey roofs of the houses."
                            }
                        }
                    ],
                    "synonyms": [
                        "посыпать",
                        "припорошить"
                    ],
                    "target_lang_definitions": {
                        "en": "To sprinkle the surface of something with a thin layer of fine powder (sugar, snow, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dust",
                                "target_lang_sense_clarification": "sprinkle with fine powder like sugar or snow"
                            },
                            {
                                "target_lang_word": "sprinkle",
                                "target_lang_sense_clarification": "scatter small particles over a surface"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
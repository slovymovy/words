{
    "word_family": [
        "pomniczek",
        "pomnikowy",
        "pomnikarstwo",
        "pomnikarz"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93b790ef-8006-3863-9965-313ef8cda410",
                    "sense_definition": "Dzieło rzeźbiarskie lub architektoniczne wzniesione w miejscu publicznym dla upamiętnienia wybitnej osoby lub ważnego wydarzenia historycznego",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W centrum warszawskiego rynku stoi <w>pomnik</w> Syrenki.",
                            "target_lang_translations": {
                                "ru": "В центре рыночной площади Варшавы стоит <w>памятник</w> Русалке."
                            }
                        },
                        {
                            "text": "Delegacja złożyła kwiaty pod <w>pomnikiem</w> Nieznanego Żołnierza.",
                            "target_lang_translations": {
                                "ru": "Делегация возложила цветы к <w>памятнику</w> Неизвестного солдата."
                            }
                        }
                    ],
                    "synonyms": [
                        "monument"
                    ],
                    "common_phrases": [
                        "odsłonięcie pomnika",
                        "budowa pomnika",
                        "pomnik przyrody",
                        "pomnik historii",
                        "złożyć kwiaty pod pomnikiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скульптурное или архитектурное сооружение, воздвигнутое в общественном месте для увековечения памяти выдающейся личности или важного исторического события"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "памятник",
                                "target_lang_sense_clarification": "скульптура или сооружение"
                            },
                            {
                                "target_lang_word": "монумент",
                                "target_lang_sense_clarification": "величественное сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d23bc9c-1505-336c-a779-d369e36ff4ab",
                    "sense_definition": "Ozdobna płyta kamienna lub konstrukcja stawiana na grobie w celu upamiętnienia zmarłego",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzina zamówiła nowy, granitowy <w>pomnik</w> na grobie dziadka.",
                            "target_lang_translations": {
                                "ru": "Семья заказала новый гранитный <w>памятник</w> на могилу дедушки."
                            }
                        },
                        {
                            "text": "Przed dniem Wszystkich Świętych ludzie myją <w>pomniki</w> na cmentarzu.",
                            "target_lang_translations": {
                                "ru": "Перед Днем Всех Святых люди моют <w>памятники</w> на кладбище."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagrobek"
                    ],
                    "common_phrases": [
                        "postawić pomnik",
                        "znicze na pomniku",
                        "płyta pomnika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная каменная плита или конструкция, устанавливаемая на могиле для увековечения памяти умершего"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надгробие",
                                "target_lang_sense_clarification": "могильная плита"
                            },
                            {
                                "target_lang_word": "памятник",
                                "target_lang_sense_clarification": "на могиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70b937f1-dbe8-3ef8-ad70-231a80542724",
                    "sense_definition": "Dzieło, osiągnięcie lub osoba o trwałym, wyjątkowym znaczeniu dla historii, kultury lub nauki",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wybitna powieść to trwały <w>pomnik</w> literatury polskiej.",
                            "target_lang_translations": {
                                "ru": "Этот выдающийся роман — вечный <w>памятник</w> польской литературы."
                            }
                        },
                        {
                            "text": "Swoją ciężką pracą i odkryciami wystawił sobie <w>pomnik</w> trwalszy niż ze spiżu.",
                            "target_lang_translations": {
                                "ru": "Своим упорным трудом и открытиями он воздвиг себе <w>памятник</w> прочнее бронзы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żywy pomnik",
                        "trwały pomnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произведение, достижение или личность, имеющие непреходящее, исключительное значение для истории, культуры или науки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "памятник",
                                "target_lang_sense_clarification": "выдающееся произведение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
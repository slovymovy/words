                {
                    "sense_id": "abc29705-dae1-3ed3-b731-04722ee4be98",
                    "sense_definition": "Mający określoną cenę lub wartość materialną.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "value_price",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary obraz jest <w>wart</w> ogromną sumę pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Эта старая картина <w>стоит</w> огромную сумму денег."
                            }
                        },
                        {
                            "text": "Czy ten samochód jest <w>wart</w> swojej wysokiej ceny?",
                            "target_lang_translations": {
                                "ru": "<w>Стоит</w> ли этот автомобиль своей высокой цены?"
                            }
                        }
                    ],
                    "antonyms": [
                        "niewart"
                    ],
                    "common_phrases": [
                        "wart swojej ceny",
                        "ile to jest warte",
                        "nie jest tego wart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий определенную цену или материальную ценность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "иметь определенную цену"
                            },
                            {
                                "target_lang_word": "стоящий",
                                "target_lang_sense_clarification": "имеющий стоимость"
                            }
                        ]

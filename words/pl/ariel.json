                },
                {
                    "sense_id": "62ba6738-5bf3-38ca-8154-6311d66b2f61",
                    "sense_definition": "miasto w Izraelu; także biblijna, symboliczna nazwa Jerozolimy",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Podczas wycieczki odwiedziliśmy miasto <w>Ariel</w> na Zachodnim Brzegu.",
                            "target_lang_translations": {
                                "ru": "Во время экскурсии мы посетили город <w>Ариэль</w> на Западном берегу."
                            }
                        },
                        {
                            "text": "W tekstach prorockich Jerozolima bywa nazywana mianem <w>Ariel</w>.",
                            "target_lang_translations": {
                                "ru": "В пророческих текстах Иерусалим иногда называют именем <w>Ариил</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "miasto Ariel"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2ab9891-7436-3677-8f4d-8f79f7555ebe",
                    "sense_definition": "Sprawić, że niezależne elementy, grupy ludzi lub państwa łączą się, tworząc jedną całość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unification_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy władca postanowił <zjednoczyć> skłócone plemiona w jedno silne państwo.",
                            "target_lang_translations": {
                                "ru": "Новый правитель решил <w>объединить</w> враждующие племена в одно сильное государство."
                            }
                        },
                        {
                            "text": "Musimy <zjednoczyć> nasze siły, aby szybciej ukończyć ten trudny projekt.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>объединить</w> наши силы, чтобы быстрее завершить этот сложный проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "połączyć",
                        "scalić",
                        "zunifikować"
                    ],
                    "antonyms": [
                        "rozpaść się",
                        "rozpaść się"
                    ],
                    "common_phrases": [
                        "zjednoczyć siły",
                        "zjednoczyć kraj",
                        "zjednoczyć naród"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы независимые элементы, группы людей или государства соединились, образуя единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединить",
                                "target_lang_sense_clarification": "создать единое целое из разрозненных частей"
                            },
                            {
                                "target_lang_word": "сплотить",
                                "target_lang_sense_clarification": "сделать группу людей более дружной и крепкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "423028fb-6522-31a0-9cd5-ed72374aa272",
                    "sense_definition": "Połączyć się z innymi w celu wspólnego działania lub utworzenia wspólnoty (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unification_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W obliczu nadchodzącego zagrożenia wszyscy mieszkańcy miasta <zjednoczyli> się.",
                            "target_lang_translations": {
                                "ru": "Перед лицом надвигающейся угрозы все жители города <w>объединились</w>."
                            }
                        },
                        {
                            "text": "Partie opozycyjne <zjednoczyły> się, aby wygrać nadchodzące wybory parlamentarne.",
                            "target_lang_translations": {
                                "ru": "Оппозиционные партии <w>объединились</w>, чтобы выиграть предстоящие парламентские выборы."
                            }
                        }
                    ],
                    "synonyms": [
                        "złączyć się",
                        "scalić się",
                        "zespolić się"
                    ],
                    "antonyms": [
                        "rozpaść się"
                    ],
                    "common_phrases": [
                        "zjednoczyć się w działaniu",
                        "zjednoczyć się przeciw wrogowi",
                        "zjednoczyć się w bólu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединиться с другими для совместных действий или создания сообщества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединиться",
                                "target_lang_sense_clarification": "начать действовать сообща"
                            },
                            {
                                "target_lang_word": "сплотиться",
                                "target_lang_sense_clarification": "стать более дружными и единодушными"
                            }
                        ]
                    }
                }
            ]
        }
    ]

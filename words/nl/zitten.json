{
    "word_family": [
        "zitplaats",
        "zitting",
        "voorzitter",
        "zitvlak",
        "inzittende",
        "zitbank",
        "zitje"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f376653f-e6a7-3259-aae9-6486558e5c21",
                    "sense_definition": "Op het zitvlak rusten, waarbij het bovenlichaam rechtop blijft, bijvoorbeeld op een stoel of bank.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange wandeling wilden we graag even op een bankje <w>zitten</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки нам очень хотелось немного <w>посидеть</w> на скамейке."
                            }
                        },
                        {
                            "text": "Ze <w>zaten</w> gezellig samen op de bank naar de televisie te kijken.",
                            "target_lang_translations": {
                                "ru": "Они уютно <w>сидели</w> вместе на диване и смотрели телевизор."
                            }
                        }
                    ],
                    "antonyms": [
                        "staan"
                    ],
                    "common_phrases": [
                        "gaan zitten",
                        "lekker zitten",
                        "rechtop zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опираться на ягодицы, держа туловище вертикально (например, на стуле или диване)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "находиться в сидячем положении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58ca2a97-100e-345b-a2ea-2a6da91e79ce",
                    "sense_definition": "Zich op een bepaalde plaats bevinden of ergens in geplaatst zijn.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan mijn sleutels niet vinden, maar ze moeten ergens in mijn tas <w>zitten</w>.",
                            "target_lang_translations": {
                                "ru": "Я не могу найти свои ключи, но они должны где-то <w>лежать</w> в моей сумке."
                            }
                        },
                        {
                            "text": "Hij <w>zit</w> momenteel in de trein op weg naar zijn werk.",
                            "target_lang_translations": {
                                "ru": "Он сейчас <w>едет</w> в поезде на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijn",
                        "zijn"
                    ],
                    "common_phrases": [
                        "in de weg zitten",
                        "vol zitten",
                        "vast zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в определенном месте или лежать внутри чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лежать",
                                "target_lang_sense_clarification": "о предметах внутри чего-либо"
                            },
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "быть расположенным"
                            },
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "присутствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "923933fa-1a40-3a1b-b2bc-df56bf8b4ebf",
                    "sense_definition": "Hulpwerkwoord dat een voortdurende handeling uitdrukt, meestal in combinatie met 'te' en een ander werkwoord.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Terwijl jij rustig je tijd neemt, <w>zit</w> ik hier voor niets te wachten.",
                            "target_lang_translations": {
                                "ru": "Пока ты не торопишься, я <w>сижу</w> здесь и жду зря."
                            }
                        },
                        {
                            "text": "Hij <w>zat</w> de hele avond te lezen in zijn favoriete boek.",
                            "target_lang_translations": {
                                "ru": "Он весь вечер <w>сидел</w> и читал свою любимую книгу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zitten te wachten",
                        "zitten te kijken",
                        "zitten te slapen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "duratief"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательный глагол, выражающий продолжительное действие (обычно в сочетании с 'te' и другим глаголом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "выражение длительного действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9695459-5a6d-3825-b82f-d8c9ce1b0a40",
                    "sense_definition": "Een gevangenisstraf uitzitten; in de gevangenis verblijven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "prison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De beruchte crimineel heeft vijftien jaar <w>gezeten</w> voor zijn daden.",
                            "target_lang_translations": {
                                "ru": "Печально известный преступник <w>отсидел</w> пятнадцать лет за свои деяния."
                            }
                        },
                        {
                            "text": "Hij is bang dat hij weer moet <w>zitten</w> als hij opnieuw de fout in gaat.",
                            "target_lang_translations": {
                                "ru": "Он боится, что ему снова придется <w>сидеть</w>, если он опять оступится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vast zitten",
                        "moeten zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отбывать тюремное заключение; находиться в тюрьме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "отбывать срок (разг.)"
                            },
                            {
                                "target_lang_word": "отбывать срок",
                                "target_lang_sense_clarification": "находиться в заключении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07b99988-8d12-3e3f-9626-b97798fa3267",
                    "sense_definition": "Een vergadering of bespreking hebben met iemand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zit</w> morgenochtend met de directeur om de nieuwe plannen te bespreken.",
                            "target_lang_translations": {
                                "ru": "Я <w>встречаюсь</w> завтра утром с директором, чтобы обсудить новые планы."
                            }
                        },
                        {
                            "text": "We hebben lang met het team <w>gezeten</w> om tot een oplossing te komen.",
                            "target_lang_translations": {
                                "ru": "Мы долго <w>сидели</w> с командой, чтобы прийти к решению."
                            }
                        }
                    ],
                    "common_phrases": [
                        "samen zitten",
                        "met iemand zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить встречу или обсуждение с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "проводить встречу"
                            },
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "обсуждать (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
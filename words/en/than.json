    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "c14bf0bb-f210-3eb6-9ef3-553c02b91427",
                    "sense_definition": "Used in comparisons to introduce the second element or the basis of comparison.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comparison_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is much taller <w>than</w> her brother.",
                            "target_lang_translations": {
                                "ru": "Она намного выше, <w>чем</w> её брат."
                            }
                        },
                        {
                            "text": "I would rather stay home <w>than</w> go out tonight.",
                            "target_lang_translations": {
                                "ru": "Я бы лучше остался дома, <w>чем</w> пошёл куда-то сегодня вечером."
                            }
                        },
                        {
                            "text": "The test was easier <w>than</w> I expected.",
                            "target_lang_translations": {
                                "ru": "Тест оказался легче, <w>чем</w> я ожидал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "more than",
                        "less than",
                        "rather than",
                        "other than",
                        "better than"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в сравнениях для введения второго элемента или основы сравнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чем",
                                "target_lang_sense_clarification": "основной союз для сравнения"
                            },
                            {
                                "target_lang_word": "нежели",
                                "target_lang_sense_clarification": "книжный или устаревший вариант союза «чем»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "644ea5f6-235e-3016-8999-a059cbd09e71",
                    "sense_definition": "Because; for.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",

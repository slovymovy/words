                        "wolność",
                        "pozwolenie"
                    ],
                    "antonyms": [
                        "bezwolność"
                    ],
                    "common_phrases": [
                        "silna wola",
                        "wolna wola",
                        "siła woli",
                        "wbrew woli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность человека сознательно принимать решения и управлять своими поступками."

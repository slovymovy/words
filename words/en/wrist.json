{
    "word_family": [
        "wristband",
        "wristwatch",
        "wristlet"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22704abe-63db-3734-8a9f-a6d313f3e814",
                    "sense_definition": "The joint connecting the hand to the forearm.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_joint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a silver bracelet loosely around her left <w>wrist</w>.",
                            "target_lang_translations": {
                                "ru": "Она носила серебряный браслет, свободно болтающийся на левом <w>запястье</w>."
                            }
                        },
                        {
                            "text": "With a simple flick of the <w>wrist</w>, he tossed the card into the hat.",
                            "target_lang_translations": {
                                "ru": "Одним простым движением <w>кисти</w> он забросил карту в шляпу."
                            }
                        }
                    ],
                    "synonyms": [
                        "carpus"
                    ],
                    "common_phrases": [
                        "flick of the wrist",
                        "slap on the wrist",
                        "limp wrist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сустав, соединяющий кисть руки с предплечьем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запястье",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bbbc202-ca0f-326b-b763-016648d3a9db",
                    "sense_definition": "A stud or pin that forms a pivot or journal in a mechanism.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic had to replace the worn <w>wrist</w> pin in the engine block.",
                            "target_lang_translations": {
                                "ru": "Механику пришлось заменить изношенный <w>поршневой</w> палец в блоке двигателя."
                            }
                        },
                        {
                            "text": "This steel <w>wrist</w> connects the piston to the connecting rod effectively.",
                            "target_lang_translations": {
                                "ru": "Этот стальной <w>поршневой палец</w> эффективно соединяет поршень с шатуном."
                            }
                        }
                    ],
                    "synonyms": [
                        "wrist pin"
                    ],
                    "common_phrases": [
                        "wrist pin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Штифт или палец, служащий осью вращения в механизме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поршневой палец",
                                "target_lang_sense_clarification": "механика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4e2bdc5c-4c04-3a79-8028-d4223e380f7d",
                    "sense_definition": "To shoot a puck or ball with a quick snapping motion of the wrist.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The forward managed to <w>wrist</w> the puck into the top corner of the net.",
                            "target_lang_translations": {
                                "ru": "Нападающий сумел <w>отправить кистевым броском</w> шайбу в верхний угол ворот."
                            }
                        },
                        {
                            "text": "He <w>wristed</w> a shot past the goalie before the defense could react.",
                            "target_lang_translations": {
                                "ru": "Он <w>нанес кистевой бросок</w> мимо вратаря, прежде чем защита успела среагировать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wrist a shot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросать шайбу или мяч резким движением кисти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросать кистевым",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
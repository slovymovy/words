                            "target_lang_translations": {
                                "en": "The strong man <w>bent</w> the iron bar with his bare hands."
                            }
                        },
                        {
                            "text": "De bomen <w>bogen</w> diep door de zware windstoten.",
                            "target_lang_translations": {
                                "en": "The trees <w>bent</w> deep under the heavy gusts of wind."
                            }
                        }
                    ],
                    "synonyms": [
                        "plooien"
                    ],
                    "antonyms": [
                        "rechten"
                    ],
                    "common_phrases": [
                        "het hoofd buigen",

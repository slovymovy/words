{
    "word_family": [
        "input",
        "throughput"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d28cd203-0403-3f30-9d4e-ef132ccbabac",
                    "sense_definition": "The amount of something that a person, a machine, or an organization produces.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "production_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory increased its <w>output</w> by twenty percent this year.",
                            "target_lang_translations": {
                                "ru": "В этом году завод увеличил <w>выпуск</w> продукции на двадцать процентов."
                            }
                        },
                        {
                            "text": "We need to improve our daily <w>output</w> to meet the deadline.",
                            "target_lang_translations": {
                                "ru": "Нам нужно увеличить ежедневную <w>выработку</w>, чтобы успеть к сроку."
                            }
                        }
                    ],
                    "synonyms": [
                        "production",
                        "yield",
                        "turnover"
                    ],
                    "antonyms": [
                        "input"
                    ],
                    "common_phrases": [
                        "industrial output",
                        "annual output",
                        "manufacturing output",
                        "increase output"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество продукции, произведенной человеком, машиной или организацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпуск",
                                "target_lang_sense_clarification": "объем произведенной продукции"
                            },
                            {
                                "target_lang_word": "выработка",
                                "target_lang_sense_clarification": "количество сделанного за определенное время"
                            },
                            {
                                "target_lang_word": "объем производства",
                                "target_lang_sense_clarification": "общий показатель деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86ce6ac8-946c-33f3-b472-bde611731add",
                    "sense_definition": "Information produced by a computer program or hardware and sent to a screen, printer, or file.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computing_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program sends its <w>output</w> to a text file automatically.",
                            "target_lang_translations": {
                                "ru": "Программа автоматически отправляет свои <w>выходные данные</w> в текстовый файл."
                            }
                        },
                        {
                            "text": "Please check the printer <w>output</w> for any errors in the document.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте <w>распечатку</w> принтера на наличие ошибок в документе."
                            }
                        }
                    ],
                    "synonyms": [
                        "data",
                        "result"
                    ],
                    "antonyms": [
                        "input"
                    ],
                    "common_phrases": [
                        "digital output",
                        "video output",
                        "audio output"
                    ],
                    "target_lang_definitions": {
                        "ru": "Информация, созданная компьютерной программой или оборудованием и отправленная на экран, принтер или в файл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывод",
                                "target_lang_sense_clarification": "процесс или результат выдачи данных"
                            },
                            {
                                "target_lang_word": "выходные данные",
                                "target_lang_sense_clarification": "результат работы программы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ff37aef-066e-317d-8f40-6ab8bbd5996b",
                    "sense_definition": "The amount of power or energy produced by a system or device.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "energy_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engine has a maximum power <w>output</w> of two hundred horsepower.",
                            "target_lang_translations": {
                                "ru": "Двигатель имеет максимальную выходную <w>мощность</w> в двести лошадиных сил."
                            }
                        },
                        {
                            "text": "Solar panels vary in their energy <w>output</w> depending on the weather conditions.",
                            "target_lang_translations": {
                                "ru": "Энергетическая <w>выработка</w> солнечных панелей варьируется в зависимости от погодных условий."
                            }
                        }
                    ],
                    "synonyms": [
                        "power",
                        "wattage",
                        "energy"
                    ],
                    "antonyms": [
                        "input"
                    ],
                    "common_phrases": [
                        "power output",
                        "high output",
                        "maximum output"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество энергии или мощности, вырабатываемое системой или устройством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мощность",
                                "target_lang_sense_clarification": "выходная мощность устройства"
                            },
                            {
                                "target_lang_word": "выработка",
                                "target_lang_sense_clarification": "количество произведенной энергии"
                            },
                            {
                                "target_lang_word": "отдача",
                                "target_lang_sense_clarification": "эффективность или возвращаемая энергия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3a58f0c-ba6f-34da-bf0a-736c38560922",
                    "sense_definition": "The volume of fluid expelled by the body, such as urine or blood pumped by the heart.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physiology_fluid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors monitored the patient's urine <w>output</w> closely after the surgery.",
                            "target_lang_translations": {
                                "ru": "После операции врачи внимательно следили за <w>выделением</w> мочи у пациента."
                            }
                        },
                        {
                            "text": "Regular exercise increases cardiac <w>output</w> significantly over time.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения со временем значительно увеличивают сердечный <w>выброс</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "discharge"
                    ],
                    "antonyms": [
                        "intake"
                    ],
                    "common_phrases": [
                        "cardiac output",
                        "urine output"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объем жидкости, выделяемой организмом, например мочи или крови, перекачиваемой сердцем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделение",
                                "target_lang_sense_clarification": "объем выводимой жидкости"
                            },
                            {
                                "target_lang_word": "выброс",
                                "target_lang_sense_clarification": "объем крови, перекачиваемой сердцем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a22aa83d-8ffd-3e8a-816c-97c224a89870",
                    "sense_definition": "A connection point on a device where signals or power are sent out.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "electronics_terminal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Plug the speakers into the audio <w>output</w> on the back of the computer.",
                            "target_lang_translations": {
                                "ru": "Подключите колонки к аудио<w>выходу</w> на задней панели компьютера."
                            }
                        },
                        {
                            "text": "The device has an HDMI <w>output</w> for connecting to an external monitor.",
                            "target_lang_translations": {
                                "ru": "Устройство имеет HDMI-<w>выход</w> для подключения к внешнему монитору."
                            }
                        }
                    ],
                    "synonyms": [
                        "port",
                        "socket",
                        "jack"
                    ],
                    "antonyms": [
                        "input"
                    ],
                    "common_phrases": [
                        "output jack",
                        "output terminal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка подключения на устройстве, через которую отправляются сигналы или питание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выход",
                                "target_lang_sense_clarification": "разъем для вывода сигнала"
                            },
                            {
                                "target_lang_word": "разъем",
                                "target_lang_sense_clarification": "гнездо подключения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa82c78c-f31a-377f-af85-63cf1ce65bc4",
                    "sense_definition": "To produce, create, or generate a specific quantity of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "production_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This machine can <w>output</w> over a thousand units per hour.",
                            "target_lang_translations": {
                                "ru": "Эта машина может <w>выпускать</w> более тысячи единиц в час."
                            }
                        },
                        {
                            "text": "The system <w>outputs</w> a detailed report every morning at eight.",
                            "target_lang_translations": {
                                "ru": "Система <w>выдает</w> подробный отчет каждое утро в восемь часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "produce",
                        "generate",
                        "create"
                    ],
                    "antonyms": [
                        "input"
                    ],
                    "common_phrases": [
                        "output results",
                        "output goods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить, создавать или генерировать определенное количество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "производить продукцию"
                            },
                            {
                                "target_lang_word": "производить",
                                "target_lang_sense_clarification": "создавать или изготавливать"
                            },
                            {
                                "target_lang_word": "вырабатывать",
                                "target_lang_sense_clarification": "генерировать энергию или ресурс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fc5e2f5-8c07-3326-ac53-02636728b209",
                    "sense_definition": "To send data from a computer to a device, screen, or file.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "computing_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The script <w>outputs</w> the results to a log file for later review.",
                            "target_lang_translations": {
                                "ru": "Скрипт <w>выводит</w> результаты в лог-файл для последующего просмотра."
                            }
                        },
                        {
                            "text": "When I hit enter, the computer <w>outputs</w> an error message on the screen.",
                            "target_lang_translations": {
                                "ru": "Когда я нажимаю ввод, компьютер <w>выводит</w> сообщение об ошибке на экран."
                            }
                        }
                    ],
                    "synonyms": [
                        "transmit",
                        "display",
                        "print"
                    ],
                    "antonyms": [
                        "input"
                    ],
                    "common_phrases": [
                        "output to file",
                        "output to screen",
                        "output data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять данные с компьютера на устройство, экран или в файл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выводить",
                                "target_lang_sense_clarification": "отображать или передавать данные"
                            },
                            {
                                "target_lang_word": "выдавать",
                                "target_lang_sense_clarification": "предоставлять результат"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
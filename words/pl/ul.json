            "senses": [
                {
                    "sense_id": "ea89a498-94c9-33a6-bb25-cdef72a47034",
                    "sense_definition": "Specjalna konstrukcja, zazwyczaj w formie drewnianej skrzynki, służąca do hodowli pszczół i pozyskiwania miodu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pszczelarz ostrożnie otworzył <w>ul</w>, aby sprawdzić stan owadów po zimie.",
                            "target_lang_translations": {
                                "ru": "Пчеловод осторожно открыл <w>улей</w>, чтобы проверить состояние насекомых после зимы."
                            }
                        },
                        {
                            "text": "W pasiece stało kilkanaście kolorowych <w>uli</w>, wokół których unosił się zapach wosku.",
                            "target_lang_translations": {
                                "ru": "На пасеке стояло несколько разноцветных <w>ульев</w>, вокруг которых витал запах воска."
                            }
                        },
                        {
                            "text": "W klasie podczas przerwy huczało jak w <w>ulu</w>.",
                            "target_lang_translations": {
                                "ru": "В классе во время перемены гудело как в <w>улье</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "barć"
                    ],
                    "common_phrases": [
                        "huczeć jak w ulu",
                        "zaglądać do ula",
                        "wylot z ula"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальная конструкция, обычно в виде деревянного ящика, служащая для разведения пчёл и получения мёда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "улей",
                                "target_lang_sense_clarification": "домик для пчёл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27cfea11-2764-3044-8339-7d1eb6412303",
                    "sense_definition": "Potoczne lub slangowe określenie zakładu karnego, miejsca odosobnienia dla przestępców.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za kradzież z włamaniem trafił na dwa lata do <w>ula</w>.",
                            "target_lang_translations": {
                                "ru": "За кражу со взломом он попал на два года в <w>тюрягу</w>."
                            }
                        },
                        {
                            "text": "Po wyjściu z <w>ula</w> postanowił zmienić swoje życie i znaleźć uczciwą pracę.",
                            "target_lang_translations": {
                                "ru": "После выхода из <w>тюряги</w> он решил изменить свою жизнь и найти честную работу."
                            }
                        }
                    ],
                    "synonyms": [

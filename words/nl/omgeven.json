    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b6962a08-030d-3764-bc08-143202852bea",
                    "sense_definition": "Zich aan alle kanten om iets of iemand heen bevinden; insluiten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het huis is geheel <w>omgeven</w> door prachtige bossen.",
                            "target_lang_translations": {
                                "ru": "Дом полностью <w>окружён</w> прекрасными лесами.",
                                "en": "The house is completely <w>surrounded</w> by beautiful forests."
                            }
                        },
                        {
                            "text": "Het oude kasteel wordt <w>omgeven</w> door een diepe gracht.",
                            "target_lang_translations": {
                                "ru": "Старый замок <w>окружён</w> глубоким рвом.",
                                "en": "The old castle is <w>surrounded</w> by a deep moat."
                            }
                        }
                    ],
                    "synonyms": [
                        "omringen"
                    ],
                    "common_phrases": [
                        "omgeven door",
                        "omgeven met"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться вокруг чего-либо или кого-либо со всех сторон; окружать.",
                        "en": "To be situated around something or someone on all sides; to enclose."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окружать",
                                "target_lang_sense_clarification": "находиться со всех сторон"
                            },
                            {
                                "target_lang_word": "окаймлять",
                                "target_lang_sense_clarification": "окружать по краям, образовывать рамку"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "surround",
                                "target_lang_sense_clarification": "to be present on all sides"
                            },
                            {
                                "target_lang_word": "encircle",
                                "target_lang_sense_clarification": "to form a circle around"
                            },
                            {
                                "target_lang_word": "enclose",
                                "target_lang_sense_clarification": "to shut in on all sides"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0107579a-970b-3c99-84a4-28b20a185855",
                    "sense_definition": "Iets laten rondgaan in een kring of gezelschap (zoals speelkaarten).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>gaf</w> de kaarten <w>om</w> aan de andere spelers.",
                            "target_lang_translations": {
                                "ru": "Он <w>раздал</w> карты другим игрокам.",
                                "en": "He <w>dealt</w> the cards to the other players."
                            }
                        },
                        {
                            "text": "De gastvrouw <w>gaf</w> de schaal met hapjes <w>om</w>.",
                            "target_lang_translations": {
                                "ru": "Хозяйка <w>пустила</w> блюдо с закусками <w>по кругу</w>.",
                                "en": "The hostess <w>passed</w> the bowl of snacks <w>around</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "omringen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly replaced by 'ronddelen' or 'doorgeven'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать что-либо по кругу в группе людей (например, игральные карты или угощение).",
                        "en": "To let something go around in a circle or group (such as playing cards)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздавать",
                                "target_lang_sense_clarification": "распределять (например, карты)"
                            },
                            {
                                "target_lang_word": "передавать по кругу",
                                "target_lang_sense_clarification": "давать другим по очереди"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pass around",
                                "target_lang_sense_clarification": "to hand from person to person"
                            },
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "to distribute playing cards"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "823b5068-3bda-3d5c-8133-6763b427ebf3",
                    "sense_definition": "A small restaurant selling light meals and drinks, especially coffee.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "coffee_shop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to meet at the <w>cafe</w> for breakfast.",
                            "target_lang_translations": {
                                "ru": "Мы решили встретиться в <w>кафе</w> за завтраком."
                            }
                        },
                        {
                            "text": "She ordered a latte and a croissant at the local <w>cafe</w>.",
                            "target_lang_translations": {
                                "ru": "Она заказала латте и круассан в местной <w>кофейне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "coffee shop",
                        "coffeehouse",
                        "bistro"
                    ],
                    "common_phrases": [
                        "internet cafe",
                        "sidewalk cafe",
                        "outdoor cafe"
                    ],
                    "target_lang_definitions": {

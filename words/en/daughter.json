    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71ed0928-0287-36b7-9198-9598457db9c7",
                    "sense_definition": "One’s female child.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I already have a son, so I would like to have a <w>daughter</w>.",
                            "target_lang_translations": {
                                "ru": "У меня уже есть сын, поэтому я хотел бы <w>дочь</w>."
                            }
                        },
                        {
                            "text": "Her eldest <w>daughter</w> has just started primary school.",
                            "target_lang_translations": {
                                "ru": "Её старшая <w>дочь</w> только что пошла в начальную школу."
                            }
                        },
                        {
                            "text": "They discovered that the man had two <w>daughters</w> living in the same city.",
                            "target_lang_translations": {
                                "ru": "Они обнаружили, что у мужчины было две <w>дочери</w>, живущие в том же городе."
                            }
                        }
                    ],
                    "antonyms": [
                        "son"
                    ],
                    "common_phrases": [
                        "eldest daughter",
                        "youngest daughter",
                        "only daughter",
                        "teenage daughter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ребёнок женского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дочь",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "дочка",
                                "target_lang_sense_clarification": "разговорное, уменьшительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dfe8546-ed65-3b3d-af45-4e23f31b4133",
                    "sense_definition": "A female descendant or a woman considered as the product of a specific place, time, or group.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a <w>daughter</w> of the revolution, she held strong political views.",
                            "target_lang_translations": {
                                "ru": "Как <w>дочь</w> революции, она придерживалась твёрдых политических взглядов."
                            }
                        },
                        {
                            "text": "She felt like a true <w>daughter</w> of nature when walking through the forest.",
                            "target_lang_translations": {
                                "ru": "Гуляя по лесу, она чувствовала себя настоящей <w>дочерью</w> природы."
                            }
                        }
                    ],
                    "antonyms": [
                        "son"
                    ],
                    "common_phrases": [
                        "daughter of Eve"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or poetic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина как порождение определённой среды, времени или группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дочь",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d6c24a9-d153-3f12-9e1c-92f913ceac35",
                    "sense_definition": "A familiar term of address to a female person from an older or more authoritative person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't worry, <w>daughter</w>, everything will be alright in the end.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся, <w>дочка</w>, в конце концов всё будет хорошо."
                            }
                        },
                        {
                            "text": "Come here, my <w>daughter</w>, and tell me what is troubling you.",
                            "target_lang_translations": {
                                "ru": "Подойди, <w>дочь</w> моя, и расскажи мне, что тебя тревожит."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by clergy or elderly people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеское или ласковое обращение пожилого человека к молодой женщине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дочка",
                                "target_lang_sense_clarification": "ласковое обращение"
                            },
                            {
                                "target_lang_word": "дочь",
                                "target_lang_sense_clarification": "обращение (часто торжественное или религиозное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b27925d-b2ed-30e6-90df-8f769e559e7a",
                    "sense_definition": "A language that has developed from an older parent language.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "French and Spanish are <w>daughter</w> languages of Latin.",
                            "target_lang_translations": {
                                "ru": "Французский и испанский — это <w>дочерние</w> языки латыни."
                            }
                        },
                        {
                            "text": "Linguists study how a <w>daughter</w> language diverges from its ancestor.",
                            "target_lang_translations": {
                                "ru": "Лингвисты изучают, как <w>дочерний</w> язык отделяется от своего предка."
                            }
                        }
                    ],
                    "antonyms": [
                        "parent"
                    ],
                    "common_phrases": [
                        "daughter language"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical: linguistics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык, произошедший от более древнего праязыка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дочерний",
                                "target_lang_sense_clarification": "в сочетании 'дочерний язык'"
                            },
                            {
                                "target_lang_word": "потомок",
                                "target_lang_sense_clarification": "в сочетании 'язык-потомок'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fed61bee-c242-3127-abf5-681eac104465",
                    "sense_definition": "A nuclide formed by the radioactive decay of another nuclide.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The uranium isotope decays into a stable <w>daughter</w> element.",
                            "target_lang_translations": {
                                "ru": "Изотоп урана распадается в стабильный <w>дочерний</w> элемент."
                            }
                        },
                        {
                            "text": "Scientists measured the ratio of the parent isotope to the <w>daughter</w> isotope.",
                            "target_lang_translations": {
                                "ru": "Учёные измерили соотношение материнского изотопа к <w>дочернему</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "parent"
                    ],
                    "common_phrases": [
                        "daughter isotope",
                        "daughter nuclide"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical: physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нуклид, образующийся в результате радиоактивного распада другого нуклида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дочерний",
                                "target_lang_sense_clarification": "в сочетании 'дочерний элемент/изотоп'"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "desktop",
        "deskbound",
        "deskwork",
        "desking"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4d458e83-b2fa-39c4-8906-19f5f06821b6",
                    "sense_definition": "A piece of furniture with a flat or sloping top, used for writing, reading, or working, often equipped with drawers or compartments.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sat at her <w>desk</w> to finish her homework before dinner.",
                            "target_lang_translations": {
                                "ru": "Она села за свой <w>письменный стол</w>, чтобы закончить домашнюю работу до ужина."
                            }
                        },
                        {
                            "text": "Please leave the signed documents on my <w>desk</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставьте подписанные документы на моем <w>столе</w>."
                            }
                        },
                        {
                            "text": "The office <w>desk</w> was cluttered with papers and coffee cups.",
                            "target_lang_translations": {
                                "ru": "Офисный <w>стол</w> был завален бумагами и чашками из-под кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "table",
                        "workstation",
                        "bureau"
                    ],
                    "common_phrases": [
                        "sit at a desk",
                        "writing desk",
                        "office desk",
                        "reception desk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет мебели с плоской или наклонной поверхностью, используемый для письма, чтения или работы, часто оснащенный ящиками или отделениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "письменный стол",
                                "target_lang_sense_clarification": "мебель для письма"
                            },
                            {
                                "target_lang_word": "стол",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "парта",
                                "target_lang_sense_clarification": "школьный стол"
                            },
                            {
                                "target_lang_word": "рабочий стол",
                                "target_lang_sense_clarification": "место для работы"
                            },
                            {
                                "target_lang_word": "бюро",
                                "target_lang_sense_clarification": "стол с ящиками и крышкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "047ddf09-706a-35a6-9ebf-7bac0f26edca",
                    "sense_definition": "A specific department or service counter in an organization, such as a hotel, airport, newspaper, or financial firm, that deals with a particular function.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "department",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you have computer problems, call the help <w>desk</w> for assistance.",
                            "target_lang_translations": {
                                "ru": "Если у вас проблемы с компьютером, позвоните в <w>службу поддержки</w> за помощью."
                            }
                        },
                        {
                            "text": "We checked in at the front <w>desk</w> as soon as we arrived at the hotel.",
                            "target_lang_translations": {
                                "ru": "Мы зарегистрировались на <w>стойке регистрации</w>, как только прибыли в отель."
                            }
                        },
                        {
                            "text": "He works at the sports <w>desk</w> of a major national newspaper.",
                            "target_lang_translations": {
                                "ru": "Он работает в спортивном <w>отделе</w> крупной национальной газеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "counter",
                        "department",
                        "section"
                    ],
                    "common_phrases": [
                        "front desk",
                        "help desk",
                        "information desk",
                        "news desk",
                        "service desk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдел или стойка обслуживания в организации (отель, аэропорт, редакция), занимающаяся определенной функцией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стойка",
                                "target_lang_sense_clarification": "регистрации или обслуживания"
                            },
                            {
                                "target_lang_word": "отдел",
                                "target_lang_sense_clarification": "подразделение организации"
                            },
                            {
                                "target_lang_word": "бюро",
                                "target_lang_sense_clarification": "справочное или служебное"
                            },
                            {
                                "target_lang_word": "стол",
                                "target_lang_sense_clarification": "ведомство или служба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eab45ede-6a74-308f-b276-ed757c41b592",
                    "sense_definition": "A mixing console used in audio engineering to control and combine sound signals.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sound engineer adjusted the volume levels on the mixing <w>desk</w>.",
                            "target_lang_translations": {
                                "ru": "Звукорежиссер отрегулировал уровни громкости на микшерном <w>пульте</w>."
                            }
                        },
                        {
                            "text": "Signals from the microphones are sent directly to the <w>desk</w> for processing.",
                            "target_lang_translations": {
                                "ru": "Сигналы с микрофонов отправляются непосредственно на <w>пульт</w> для обработки."
                            }
                        }
                    ],
                    "synonyms": [
                        "mixer",
                        "console",
                        "mixing desk"
                    ],
                    "common_phrases": [
                        "mixing desk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short for mixing desk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Микшерный пульт, используемый в звукорежиссуре для управления и объединения звуковых сигналов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микшерный пульт",
                                "target_lang_sense_clarification": "аудиооборудование"
                            },
                            {
                                "target_lang_word": "пульт",
                                "target_lang_sense_clarification": "сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bf46f2a-fafa-344f-b7d1-9302c262ed01",
                    "sense_definition": "A position or seat for a musician in an orchestra, typically shared by two players sharing a music stand.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was thrilled to be promoted to the first <w>desk</w> of the violins.",
                            "target_lang_translations": {
                                "ru": "Она была в восторге от того, что ее перевели за первый <w>пульт</w> скрипок."
                            }
                        },
                        {
                            "text": "The players at the rear <w>desk</w> struggled to see the conductor clearly.",
                            "target_lang_translations": {
                                "ru": "Музыканты за задним <w>пультом</w> с трудом видели дирижера."
                            }
                        }
                    ],
                    "synonyms": [
                        "stand"
                    ],
                    "common_phrases": [
                        "first desk"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical music term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или пюпитр музыканта в оркестре, обычно общий для двух исполнителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульт",
                                "target_lang_sense_clarification": "пюпитр в оркестре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33b80a5b-95bc-3839-8d74-edc5ad39ba24",
                    "sense_definition": "A lectern or reading table in a church from which services are read.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The minister stood at the <w>desk</w> to read the morning lesson.",
                            "target_lang_translations": {
                                "ru": "Священник встал за <w>кафедру</w>, чтобы прочитать утреннюю проповедь."
                            }
                        },
                        {
                            "text": "An old bible lay open on the wooden <w>desk</w> in the choir.",
                            "target_lang_translations": {
                                "ru": "Старая библия лежала открытой на деревянном <w>аналое</w> на хорах."
                            }
                        }
                    ],
                    "synonyms": [
                        "lectern",
                        "pulpit"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or religious context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кафедра или столик для чтения в церкви, с которого читаются службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафедра",
                                "target_lang_sense_clarification": "возвышение для чтеца"
                            },
                            {
                                "target_lang_word": "аналой",
                                "target_lang_sense_clarification": "высокий столик с наклонным верхом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0181dbf8-bc8f-3e37-a516-a59dcf7bf765",
                    "sense_definition": "To reject a submitted article or manuscript immediately upon receipt without sending it for peer review.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "publishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The editor decided to <w>desk</w> the paper because it did not fit the journal's scope.",
                            "target_lang_translations": {
                                "ru": "Редактор решил <w>отклонить</w> статью, так как она не соответствовала тематике журнала."
                            }
                        },
                        {
                            "text": "Many academic submissions are <w>desked</w> before they ever reach a reviewer.",
                            "target_lang_translations": {
                                "ru": "Многие научные работы <w>отклоняются</w> еще до того, как попадают к рецензенту."
                            }
                        }
                    ],
                    "synonyms": [
                        "reject"
                    ],
                    "antonyms": [
                        "accept"
                    ],
                    "common_phrases": [
                        "desk reject"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "publishing/academic jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отклонить поданную статью или рукопись сразу после получения, без отправки на рецензирование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклонять",
                                "target_lang_sense_clarification": "рукопись без рецензии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b140dff-ec43-3dbb-984c-e61c878751a4",
                    "sense_definition": "To furnish or equip a room with desks.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "furnish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new classroom was <w>desked</w> to accommodate thirty students.",
                            "target_lang_translations": {
                                "ru": "Новый класс был <w>оборудован партами</w> для размещения тридцати учеников."
                            }
                        },
                        {
                            "text": "They <w>desked</w> the office with modern furniture.",
                            "target_lang_translations": {
                                "ru": "Они <w>обставили столами</w> офис современной мебелью."
                            }
                        }
                    ],
                    "synonyms": [
                        "furnish"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обставлять или оборудовать комнату столами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обставлять столами",
                                "target_lang_sense_clarification": "меблировать"
                            },
                            {
                                "target_lang_word": "оборудовать партами",
                                "target_lang_sense_clarification": "учебный класс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37257385-4d91-3a27-b445-352e56ffde2b",
                    "sense_definition": "To shut up or enclose something, as if inside a desk.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>desked</w> the letter away in a secret drawer.",
                            "target_lang_translations": {
                                "ru": "Он <w>спрятал</w> письмо в потайной ящик стола."
                            }
                        },
                        {
                            "text": "She <w>desked</w> her valuables to keep them safe.",
                            "target_lang_translations": {
                                "ru": "Она <w>заперла</w> свои ценности в стол для сохранности."
                            }
                        }
                    ],
                    "synonyms": [
                        "enclose",
                        "store"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Запереть или спрятать что-то, как будто внутри стола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прятать в стол",
                                "target_lang_sense_clarification": "убирать с глаз"
                            },
                            {
                                "target_lang_word": "запирать",
                                "target_lang_sense_clarification": "в ящик стола"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
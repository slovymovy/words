    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6994e7a5-86a8-3d10-bbdf-9fbf512d1dc4",
                    "sense_definition": "Betalen voor goederen of diensten, bijvoorbeeld in een winkel of restaurant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "betalen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ober, mag ik alstublieft <w>afrekenen</w>?",
                            "target_lang_translations": {
                                "ru": "Официант, можно мне <w>рассчитаться</w>?",
                                "en": "Waiter, may I <w>settle the bill</w> please?"
                            }
                        },
                        {
                            "text": "Hij heeft de boodschappen bij de kassa <w>afgerekend</w>.",
                            "target_lang_translations": {
                                "ru": "Он <w>расплатился</w> за покупки на кассе.",
                                "en": "He <w>paid for</w> the groceries at the checkout."
                            }
                        }
                    ],
                    "synonyms": [
                        "betalen"
                    ],
                    "common_phrases": [
                        "willen afrekenen",
                        "aan de kassa afrekenen",
                        "apart afrekenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Платить за товары или услуги, например, в магазине или ресторане.",
                        "en": "To pay for goods or services, for example in a shop or restaurant."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассчитаться",
                                "target_lang_sense_clarification": "оплатить счет"
                            },
                            {
                                "target_lang_word": "расплатиться",
                                "target_lang_sense_clarification": "отдать деньги за покупку"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "to give money for goods or services"
                            },
                            {
                                "target_lang_word": "settle the bill",
                                "target_lang_sense_clarification": "to pay the total amount owed, e.g. in a restaurant"
                            },
                            {
                                "target_lang_word": "check out",
                                "target_lang_sense_clarification": "to pay at a register in a shop"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ec2bf39-5e86-32f4-b1a3-f19eb2ece606",
                    "sense_definition": "Een conflict beëindigen, wraak nemen of een moeilijke situatie uit het verleden verwerken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De held wil definitief met de schurk <w>afrekenen</w>.",
                            "target_lang_translations": {
                                "ru": "Герой хочет окончательно <w>разделаться</w> со злодеем.",
                                "en": "The hero wants to <w>settle the score</w> with the villain once and for all."
                            }
                        },
                        {
                            "text": "Hij moet nog met zijn slechte gewoontes <w>afrekenen</w>.",
                            "target_lang_translations": {
                                "ru": "Ему еще нужно <w>покончить</w> со своими вредными привычками.",
                                "en": "He still needs to <w>deal with</w> his bad habits."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergelden",
                        "afhandelen"
                    ],
                    "common_phrases": [
                        "afrekenen met",
                        "voorgoed afrekenen met"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешить конфликт, отомстить или разобраться с трудной ситуацией из прошлого.",
                        "en": "To end a conflict, take revenge, or deal with a difficult situation from the past."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделаться",
                                "target_lang_sense_clarification": "разобраться с кем-то или чем-то"
                            },
                            {
                                "target_lang_word": "покончить",
                                "target_lang_sense_clarification": "прекратить что-то навсегда"
                            },
                            {
                                "target_lang_word": "свести счёты",
                                "target_lang_sense_clarification": "отомстить"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "settle the score",
                                "target_lang_sense_clarification": "to take revenge or resolve a conflict"
                            },
                            {
                                "target_lang_word": "deal with",
                                "target_lang_sense_clarification": "to handle a difficult situation or habit"
                            },
                            {
                                "target_lang_word": "get even",
                                "target_lang_sense_clarification": "to take revenge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b3bb911-6214-3a31-ae8c-1f306ed085e4",
                    "sense_definition": "Iemand beoordelen of verantwoordelijk houden voor behaalde resultaten of gemaakte fouten.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "beoordelen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De manager wordt <w>afgerekend</w> op de verkoopcijfers van dit kwartaal.",
                            "target_lang_translations": {
                                "ru": "Менеджера <w>оценивают</w> по показателям продаж за этот квартал.",
                                "en": "The manager is <w>judged</w> on this quarter's sales figures."
                            }
                        },
                        {
                            "text": "Politici worden vaak hard <w>afgerekend</w> op hun verkiezingsbeloftes.",
                            "target_lang_translations": {
                                "ru": "Политиков часто жестко <w>судят</w> по их предвыборным обещаниям.",
                                "en": "Politicians are often harshly <w>held accountable</w> for their election promises."
                            }
                        }
                    ],
                    "synonyms": [
                        "beoordelen"
                    ],
                    "common_phrases": [
                        "afgerekend worden op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать кого-то или возлагать ответственность за достигнутые результаты или ошибки.",
                        "en": "To judge someone or hold them accountable for achieved results or mistakes made."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "судить по результатам"
                            },
                            {
                                "target_lang_word": "судить",
                                "target_lang_sense_clarification": "составлять мнение на основе чего-то"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "judge",
                                "target_lang_sense_clarification": "to evaluate based on performance"
                            },
                            {
                                "target_lang_word": "hold accountable",
                                "target_lang_sense_clarification": "to make responsible for results"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d07563b2-b286-3ca4-936a-2e8d7f41e526",
                    "sense_definition": "Iemand vermoorden, vaak gebruikt in een criminele context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "doden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bendeleider gaf de opdracht om de verrader <w>af</w> te <w>rekenen</w>.",
                            "target_lang_translations": {
                                "ru": "Главарь банды дал приказ <w>убрать</w> предателя.",
                                "en": "The gang leader gave the order to <w>take out</w> the traitor."
                            }
                        },
                        {
                            "text": "In de film werd de getuige koelbloedig <w>afgerekend</w>.",
                            "target_lang_translations": {
                                "ru": "В фильме свидетеля хладнокровно <w>ликвидировали</w>.",
                                "en": "In the movie, the witness was cold-bloodedly <w>liquidated</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermoorden",
                        "omleggen",
                        "kaltstellen"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "crimineel circuit"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убить кого-то, часто используется в криминальном контексте.",
                        "en": "To kill someone, often used in a criminal context."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убрать",
                                "target_lang_sense_clarification": "убить (сленг)"
                            },
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "убить, устранить"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "to kill (slang)"
                            },
                            {
                                "target_lang_word": "liquidate",
                                "target_lang_sense_clarification": "to kill or eliminate"
                            }

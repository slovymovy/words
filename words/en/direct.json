                        "contact direct"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напрямую; непосредственно; без пересадок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напрямую",
                                "target_lang_sense_clarification": "без посредников"
                            },
                            {
                                "target_lang_word": "без пересадок",
                                "target_lang_sense_clarification": "о перелетах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

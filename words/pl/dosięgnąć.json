            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d973ff99-be2a-3748-846d-ac643383e312",
                    "sense_definition": "Dotknąć lub chwycić coś, wyciągając w tym celu rękę lub inny przedmiot.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_reach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę <w>dosięgnąć</w> pudełka z cukrem na górnej półce, czy możesz mi pomóc?",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>достать</w> коробку с сахаром на верхней полке, ты можешь мне помочь?"
                            }
                        },
                        {
                            "text": "Dziecko stanęło na palcach, aby <w>dosięgnąć</w> dzwonka do drzwi.",
                            "target_lang_translations": {
                                "ru": "Ребенок встал на цыпочки, чтобы <w>дотянуться</w> до дверного звонка."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbliżyć się",
                        "zbliżyć się"
                    ],
                    "common_phrases": [
                        "dosięgnąć ręką",
                        "dosięgnąć półki",
                        "dosięgnąć celu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коснуться или схватить что-то, вытянув руку или предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "взять что-то, находящееся на расстоянии"
                            },
                            {
                                "target_lang_word": "дотянуться",
                                "target_lang_sense_clarification": "коснуться удаленного объекта, вытянув руку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4dd8dcb-9e24-3660-b91e-edfe97191656",
                    "sense_definition": "Aspekt dokonany od: <w>dosięgać</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli przysuniesz sobie krzesło, bez problemu <w>dosięgniesz</w> tej książki.",
                            "target_lang_translations": {
                                "ru": "Если пододвинешь себе стул, без проблем <w>достанешь</w> эту книгу."
                            }
                        },
                        {
                            "text": "Musisz się pospieszyć, żeby <w>dosięgnąć</w> uciekający autobus.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно поторопиться, чтобы <w>догнать</w> уходящий автобус."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "perfective aspect"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенный вид глагола: <w>dosięgać</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "физически дотянуться до чего-либо"

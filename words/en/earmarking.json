        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e7c7980d-8fb3-31a8-b700-3fcf12c55052",
                    "sense_definition": "Present participle and gerund of <w>earmark</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee is currently <w>earmarking</w> funds for the construction of the new library.",
                            "target_lang_translations": {
                                "ru": "Комитет в настоящее время занимается <w>выделением</w> средств на строительство новой библиотеки."
                            }
                        },
                        {
                            "text": "We suggest <w>earmarking</w> these resources for emergency use only.",
                            "target_lang_translations": {
                                "ru": "Мы предлагаем <w>зарезервировать</w> эти ресурсы только для экстренного использования."
                            }
                        }
                    ],
                    "synonyms": [
                        "allocating",
                        "designating",
                        "reserving",
                        "appropriating"
                    ],
                    "common_phrases": [
                        "earmarking funds",
                        "earmarking resources",
                        "earmarking money"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола earmark (выделять, предназначать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделение",
                                "target_lang_sense_clarification": "процесс предоставления средств или ресурсов"
                            },
                            {
                                "target_lang_word": "предназначение",
                                "target_lang_sense_clarification": "определение цели использования"
                            },
                            {
                                "target_lang_word": "ассигнование",
                                "target_lang_sense_clarification": "выделение денежных средств (официально)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d22e6b04-471d-3626-9c70-a5c7d19d30bb",
                    "sense_definition": "The act of designating funds or resources for a specific purpose.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>earmarking</w> of tax revenue for education was a popular decision among voters.",
                            "target_lang_translations": {
                                "ru": "<w>Целевое выделение</w> налоговых поступлений на образование было популярным решением среди избирателей."
                            }
                        },
                        {
                            "text": "Critics argued that the <w>earmarking</w> of the budget lacked transparency.",
                            "target_lang_translations": {
                                "ru": "Критики утверждали, что <w>распределению</w> бюджета не хватало прозрачности."
                            }
                        }
                    ],
                    "synonyms": [
                        "allocation",
                        "appropriation",
                        "designation"
                    ],
                    "common_phrases": [
                        "earmarking of funds",
                        "budgetary earmarking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт выделения средств или ресурсов для конкретной, заранее определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целевое выделение",
                                "target_lang_sense_clarification": "направление ресурсов на конкретные нужды"
                            },
                            {
                                "target_lang_word": "ассигнование",
                                "target_lang_sense_clarification": "финансовый термин"
                            },
                            {
                                "target_lang_word": "резервирование",
                                "target_lang_sense_clarification": "сохранение для определенной цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15b62457-984c-36ce-937b-0780a5f1145f",
                    "sense_definition": "The process of placing an identifying mark on the ear of an animal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proper <w>earmarking</w> helps farmers track their livestock effectively.",
                            "target_lang_translations": {
                                "ru": "Правильное <w>биркование</w> помогает фермерам эффективно отслеживать свой скот."
                            }
                        },
                        {
                            "text": "The <w>earmarking</w> of the sheep usually happens in the spring.",
                            "target_lang_translations": {
                                "ru": "<w>Клеймение</w> овец обычно происходит весной."
                            }
                        }
                    ],
                    "synonyms": [
                        "tagging",
                        "branding",
                        "marking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс нанесения опознавательного знака на ухо животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клеймение",
                                "target_lang_sense_clarification": "нанесение метки на ухо"
                            },
                            {
                                "target_lang_word": "биркование",
                                "target_lang_sense_clarification": "крепление бирки к уху"
                            },
                            {
                                "target_lang_word": "мечение",
                                "target_lang_sense_clarification": "общий термин для маркировки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

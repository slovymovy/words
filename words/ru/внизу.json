{
    "word_family": [
        "низ",
        "нижний",
        "низкий",
        "снизу",
        "книзу",
        "понизу",
        "низина"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "bd7a0422-1cb1-398e-a9bc-93229ba90e15",
                    "sense_definition": "В нижней части пространства, на низком уровне по отношению к чему-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_low",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я полез на чердак, а мои друзья остались ждать <w>внизу</w>.",
                            "target_lang_translations": {
                                "en": "I climbed into the attic, while my friends stayed waiting <w>below</w>."
                            }
                        },
                        {
                            "text": "Пожалуйста, поставьте свою подпись <w>внизу</w> страницы.",
                            "target_lang_translations": {
                                "en": "Please sign <w>at the bottom</w> of the page."
                            }
                        }
                    ],
                    "synonyms": [
                        "снизу"
                    ],
                    "antonyms": [
                        "наверху",
                        "вверху"
                    ],
                    "common_phrases": [
                        "ждать внизу",
                        "остаться внизу",
                        "где-то внизу",
                        "текст внизу"
                    ],
                    "target_lang_definitions": {
                        "en": "In the lower part of a space, or at a low level relative to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "below",
                                "target_lang_sense_clarification": "general location in a lower position"
                            },
                            {
                                "target_lang_word": "at the bottom",
                                "target_lang_sense_clarification": "at the lowest part of an object (e.g., a page)"
                            },
                            {
                                "target_lang_word": "underneath",
                                "target_lang_sense_clarification": "directly below or covered by something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef542b71-0761-3f9f-b43f-e9d194e10e24",
                    "sense_definition": "На нижнем этаже здания или помещения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_low",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наши соседи <w>внизу</w> очень громко слушают музыку.",
                            "target_lang_translations": {
                                "en": "Our neighbors <w>downstairs</w> listen to music very loudly."
                            }
                        },
                        {
                            "text": "В этом доме кухня и гостиная находятся <w>внизу</w>.",
                            "target_lang_translations": {
                                "en": "In this house, the kitchen and living room are <w>downstairs</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "наверху"
                    ],
                    "common_phrases": [
                        "соседи внизу",
                        "спуститься вниз",
                        "жить внизу"
                    ],
                    "target_lang_definitions": {
                        "en": "On the lower floor of a building or premises."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "downstairs",
                                "target_lang_sense_clarification": "on a lower floor"
                            },
                            {
                                "target_lang_word": "below",
                                "target_lang_sense_clarification": "on the floor underneath"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f2be11cb-6be4-3317-bbfe-17fd17ed0965",
                    "sense_definition": "Указывает на нахождение в нижней части чего-либо (употребляется с родительным падежом).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Внизу</w> стены видны старые следы намокания.",
                            "target_lang_translations": {
                                "en": "<w>At the bottom of</w> the wall, old traces of dampness are visible."
                            }
                        },
                        {
                            "text": "Мы договорились встретиться с коллегами <w>внизу</w> лестницы.",
                            "target_lang_translations": {
                                "en": "We agreed to meet our colleagues <w>at the foot of</w> the stairs."
                            }
                        }
                    ],
                    "antonyms": [
                        "вверху"
                    ],
                    "common_phrases": [
                        "внизу страницы",
                        "внизу лестницы",
                        "внизу живота",
                        "внизу списка"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates location in the lower part of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at the bottom of",
                                "target_lang_sense_clarification": "in the lowest part of something"
                            },
                            {
                                "target_lang_word": "at the foot of",
                                "target_lang_sense_clarification": "at the base (e.g., of stairs, a mountain)"
                            },
                            {
                                "target_lang_word": "below",
                                "target_lang_sense_clarification": "lower than"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
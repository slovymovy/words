        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e0ee2b7f-0e5c-3fbc-abb4-d3d917427f4a",
                    "sense_definition": "Existing in a natural or raw state; not yet processed or refined.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "natural_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country exports vast amounts of <w>crude</w> oil to international markets.",
                            "target_lang_translations": {
                                "ru": "Страна экспортирует огромные объемы <w>сырой</w> нефти на международные рынки."
                            }
                        },
                        {
                            "text": "They discovered a deposit of <w>crude</w> ore in the mountains.",
                            "target_lang_translations": {
                                "ru": "Они обнаружили месторождение <w>необработанной</w> руды в горах."
                            }

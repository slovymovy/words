    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "73fceff3-b20a-300e-80e8-a9b97c05de9f",
                    "sense_definition": "Poddawać produkty spożywcze procesowi fermentacji w celu ich utrwalenia lub nadania im kwaśnego smaku; przygotowywać zaczyn na ciasto lub zupę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "culinary_fermentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia co roku <zakwasza> kapustę w wielkiej, dębowej beczce.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка каждый год <квасит> капусту в большой дубовой бочке."
                            }
                        },
                        {
                            "text": "Aby przygotować tradycyjny żurek, musisz najpierw <zakwaszać> mąkę przez kilka dni.",
                            "target_lang_translations": {
                                "ru": "Чтобы приготовить традиционный журек, нужно сначала несколько дней <заквашивать> муку."
                            }
                        }
                    ],
                    "synonyms": [
                        "kisić",
                        "kwasić",
                        "zakiszać"
                    ],
                    "common_phrases": [
                        "zakwaszać ogórki",
                        "zakwaszać barszcz",
                        "zakwaszać ciasto",
                        "zakwaszać kapustę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать продукты ферментации для консервации или придания кислого вкуса; готовить закваску для теста или супа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квасить",
                                "target_lang_sense_clarification": "овощи, например капусту"
                            },
                            {
                                "target_lang_word": "заквашивать",
                                "target_lang_sense_clarification": "с помощью закваски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "777664b5-36c7-38a0-b968-a4063b720b0f",
                    "sense_definition": "Doprowadzać do zachwiania równowagi kwasowo-zasadowej w organizmie lub mięśniach, powodując nadmiar substancji kwaśnych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physiological_acidification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dieta bogata w cukier i przetworzoną żywność może silnie <zakwaszać> twój organizm.",
                            "target_lang_translations": {
                                "ru": "Диета, богатая сахаром и обработанными продуктами, может сильно <закислять> твой организм."
                            }
                        },
                        {
                            "text": "Zbyt intensywny trening bez odpowiedniej regeneracji często <zakwasza> mięśnie.",
                            "target_lang_translations": {
                                "ru": "Слишком интенсивная тренировка без надлежащего восстановления часто <закисляет> мышцы."
                            }
                        }
                    ],
                    "antonyms": [
                        "odkwaszać"
                    ],
                    "common_phrases": [
                        "zakwaszać organizm",
                        "zakwaszać mięśnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушать кислотно-щелочной баланс в организме или мышцах, повышая кислотность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закислять",
                                "target_lang_sense_clarification": "организм или мышцы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "927b8010-a184-36f8-9c5d-bf535eeeb9fb",
                    "sense_definition": "Ulegać procesowi fermentacji, stawać się kwaśnym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_fermentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli zostawisz świeże mleko na słońcu, bardzo szybko <zakwasza> się i psuje.",
                            "target_lang_translations": {
                                "ru": "Если оставишь свежее молоко на солнце, оно очень быстро <скисает> и портится."
                            }
                        },
                        {
                            "text": "Przygotowany zaczyn na chleb <zakwasza> się powoli w ciepłym miejscu.",
                            "target_lang_translations": {
                                "ru": "Приготовленная закваска для хлеба медленно <закисает> в теплом месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwaśnieć",
                        "kisić się",
                        "kwasić się"
                    ],
                    "common_phrases": [
                        "mleko się zakwasza",
                        "zaczyn się zakwasza"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive usage with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергаться процессу брожения, становиться кислым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скисать",
                                "target_lang_sense_clarification": "портиться (о молоке)"
                            },
                            {
                                "target_lang_word": "закисать",
                                "target_lang_sense_clarification": "бродить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9acbf386-1994-37ed-801e-501410f3e635",
                    "sense_definition": "Zmieniać odczyn substancji, gleby lub środowiska na kwaśny poprzez dodanie kwasu lub czynników kwasotwórczych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chemical_acidification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niektóre nawozy sztuczne mogą nadmiernie <zakwaszać> glebę w twoim ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Некоторые искусственные удобрения могут чрезмерно <закислять> почву в твоем саду."
                            }
                        },
                        {
                            "text": "Zanieczyszczone deszcze <zakwaszają> wody w jeziorach, co szkodzi rybom.",
                            "target_lang_translations": {
                                "ru": "Загрязненные дожди <закисляют> воду в озерах, что вредит рыбам."
                            }
                        }
                    ],
                    "antonyms": [
                        "odkwaszać"
                    ],
                    "common_phrases": [
                        "zakwaszać glebę",
                        "zakwaszać roztwór",
                        "zakwaszać środowisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать среду, почву или раствор кислым, понижая уровень pH."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закислять",
                                "target_lang_sense_clarification": "почву или среду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
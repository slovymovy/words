                            "target_lang_translations": {
                                "en": "Please come <w>closer</w>, I can't hear well."
                            }
                        },
                        {
                            "text": "Этот город расположен намного <w>ближе</w> к Москве.",
                            "target_lang_translations": {
                                "en": "This city is located much <w>closer</w> to Moscow."
                            }
                        }
                    ],
                    "antonyms": [
                        "дальше"
                    ],
                    "common_phrases": [
                        "подойти ближе",
                        "узнать ближе"
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adverb 'close'; at a shorter distance in space or time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "closer",
                                "target_lang_sense_clarification": "at a shorter distance"
                            },
                            {
                                "target_lang_word": "nearer",
                                "target_lang_sense_clarification": "less distant"
                            }
                        ]
                    }
                }
            ]
        },

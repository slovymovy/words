{
    "word_family": [
        "dworcowy",
        "przydworcowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7889f48-1de3-3c22-a628-34e43f2fc778",
                    "sense_definition": "Miejsce przyjazdu, odjazdu i postoju autobusów lub pociągów wraz z budynkiem przystosowanym do obsługi pasażerów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_hub",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Umówiłem się z nimi na <w>dworcu</w> dziesięć minut przed odjazdem autobusu.",
                            "target_lang_translations": {
                                "ru": "Я договорился с ними встретиться на <w>вокзале</w> за десять минут до отправления автобуса."
                            }
                        },
                        {
                            "text": "Muszę szybko jechać na <w>dworzec</w>, żeby zdążyć na pociąg do Warszawy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро ехать на <w>вокзал</w>, чтобы успеть на поезд в Варшаву."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dworzec kolejowy",
                        "dworzec autobusowy",
                        "dworzec główny",
                        "na dworcu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место прибытия, отправления и стоянки автобусов или поездов вместе со зданием, предназначенным для обслуживания пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вокзал",
                                "target_lang_sense_clarification": "здание станции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "578834e1-7b2f-35bc-b839-b50914bd2d43",
                    "sense_definition": "Obiekt przystosowany do obsługi ruchu i transportu lotniczego lub promowego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "transport_hub",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowoczesny <w>dworzec</w> lotniczy obsługuje tysiące pasażerów każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Современный <w>аэровокзал</w> обслуживает тысячи пассажиров каждый день."
                            }
                        },
                        {
                            "text": "Statek wycieczkowy wpłynął powoli do portu, gdzie znajdował się <w>dworzec</w> morski.",
                            "target_lang_translations": {
                                "ru": "Круизный лайнер медленно вошел в порт, где находился морской <w>вокзал</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "terminal",
                        "port"
                    ],
                    "common_phrases": [
                        "dworzec lotniczy",
                        "dworzec morski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объект, приспособленный для обслуживания воздушного или паромного движения и транспорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терминал",
                                "target_lang_sense_clarification": "пассажирский комплекс"
                            },
                            {
                                "target_lang_word": "вокзал",
                                "target_lang_sense_clarification": "в сочетаниях: аэровокзал, морской вокзал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c4862f5-71db-3224-a198-967e2474e775",
                    "sense_definition": "Dawne określenie dworu szlacheckiego lub siedziby władcy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej kronice opisano wspaniały <w>dworzec</w>, w którym odbywały się uczty.",
                            "target_lang_translations": {
                                "ru": "В старой хронике описана великолепная <w>усадьба</w>, в которой проходили пиры."
                            }
                        },
                        {
                            "text": "Szlachcic zaprosił wędrowców na swój <w>dworzec</w>, aby mogli odpocząć.",
                            "target_lang_translations": {
                                "ru": "Дворянин пригласил путников в свою <w>усадьбу</w>, чтобы они могли отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "port"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo używane dawniej w znaczeniu dworu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название дворянской усадьбы или резиденции правителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усадьба",
                                "target_lang_sense_clarification": "поместье"
                            },
                            {
                                "target_lang_word": "двор",
                                "target_lang_sense_clarification": "резиденция"
                            }
                        ]
                    }
                }
            ]
        }
    ]

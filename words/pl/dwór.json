            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f958047e-8b53-3ebe-9290-74d142ae9e20",
                    "sense_definition": "Otwarta przestrzeń na zewnątrz budynku; miejsce pod gołym niebem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "outside_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci ubrały kurtki i wybiegły na <w>dwór</w>.",
                            "target_lang_translations": {
                                "ru": "Дети надели куртки и выбежали во <w>двор</w>."
                            }
                        },
                        {
                            "text": "Zimą na <w>dworze</w> bardzo szybko robi się ciemno.",
                            "target_lang_translations": {
                                "ru": "Зимой на <w>улице</w> очень быстро темнеет."
                            }
                        }
                    ],
                    "synonyms": [
                        "zewnątrz"
                    ],
                    "common_phrases": [
                        "iść na dwór",
                        "być na dworze",
                        "wyjść na dwór"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытое пространство снаружи здания; место под открытым небом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "улица",
                                "target_lang_sense_clarification": "пространство вне дома, снаружи"
                            },
                            {
                                "target_lang_word": "двор",
                                "target_lang_sense_clarification": "участок земли при доме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dc77028-4c59-378f-9699-cd28f6ffd297",
                    "sense_definition": "Tradycyjny, duży dom mieszkalny, będący historyczną siedzibą rodziny szlacheckiej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "residence_noble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>dwór</w> otoczony był pięknym parkiem z dębami.",
                            "target_lang_translations": {
                                "ru": "Старая <w>усадьба</w> была окружена красивым парком с дубами."
                            }
                        },
                        {
                            "text": "W tym muzeum można zobaczyć, jak wyglądał polski <w>dwór</w> szlachecki.",
                            "target_lang_translations": {
                                "ru": "В этом музее можно увидеть, как выглядела польская шляхетская <w>усадьба</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwór szlachecki",
                        "polski dwór",
                        "zabytkowy dwór"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "odnosi się do historii i dawnej architektury"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционный большой жилой дом, историческая усадьба дворянской семьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усадьба",
                                "target_lang_sense_clarification": "господский дом, имение"
                            },
                            {
                                "target_lang_word": "поместье",
                                "target_lang_sense_clarification": "земельное владение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f9cd80e-02b6-3e6d-9cb6-9be731444b73",
                    "sense_definition": "Oficjalna siedziba władcy, króla lub księcia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "royal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz przybył na <w>dwór</w> króla z ważną wiadomością.",
                            "target_lang_translations": {
                                "ru": "Рыцарь прибыл ко <w>двору</w> короля с важным известием."
                            }
                        },
                        {
                            "text": "Wersal to najsłynniejszy <w>dwór</w> w Europie.",
                            "target_lang_translations": {
                                "ru": "Версаль — самый известный <w>двор</w> в Европе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na dworze króla",
                        "przybyć na dwór"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная резиденция правителя, короля или князя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двор",
                                "target_lang_sense_clarification": "резиденция монарха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa24b2e2-85cf-39cd-87c6-d510095fb890",
                    "sense_definition": "Grupa ludzi stanowiąca najbliższe otoczenie władcy; świta królewska.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "royal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały <w>dwór</w> plotkował o nowym romansie księcia.",
                            "target_lang_translations": {
                                "ru": "Весь <w>двор</w> сплетничал о новом романе принца."
                            }
                        },
                        {
                            "text": "Jej uroda i talent zachwyciły cesarski <w>dwór</w>.",
                            "target_lang_translations": {
                                "ru": "Её красота и талант восхитили императорский <w>двор</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwór królewski",
                        "dama dworu"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, составляющая ближайшее окружение правителя; королевская свита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двор",
                                "target_lang_sense_clarification": "монарх и его приближённые"
                            },
                            {
                                "target_lang_word": "свита",
                                "target_lang_sense_clarification": "сопровождение знатной особы"
                            }
                        ]
                    }
                }
            ]
        }

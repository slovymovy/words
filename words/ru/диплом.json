    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1e6241a-100c-33b7-bf17-12696806d837",
                    "sense_definition": "Документ, свидетельствующий об окончании высшего или среднего специального учебного заведения и присвоении квалификации.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education_doc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для устройства на эту работу необходим <w>диплом</w> о высшем образовании.",
                            "target_lang_translations": {
                                "en": "A higher education <w>diploma</w> is required to apply for this job."
                            }
                        },
                        {
                            "text": "Она окончила университет с отличием и получила красный <w>диплом</w>.",
                            "target_lang_translations": {
                                "en": "She graduated from the university with honors and received a red <w>diploma</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "свидетельство"
                    ],
                    "common_phrases": [
                        "красный диплом",
                        "получить диплом",
                        "диплом о высшем образовании",
                        "копия диплома"
                    ],
                    "target_lang_definitions": {
                        "en": "A document testifying to the completion of a higher or secondary specialized educational institution and the awarding of a qualification."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "diploma",
                                "target_lang_sense_clarification": "document"
                            },
                            {
                                "target_lang_word": "degree",
                                "target_lang_sense_clarification": "qualification"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad511534-338c-3ade-99bb-03912b2663de",
                    "sense_definition": "Документ, служащий наградой за успехи в конкурсе, соревновании или выставке.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Победителям олимпиады торжественно вручили <w>дипломы</w> и подарки.",
                            "target_lang_translations": {
                                "en": "The winners of the Olympiad were solemnly presented with <w>certificates</w> and gifts."
                            }
                        },
                        {
                            "text": "На стене в его кабинете висит почётный <w>диплом</w> за участие в конференции.",
                            "target_lang_translations": {
                                "en": "An honorary <w>diploma</w> for participation in the conference hangs on the wall in his office."
                            }
                        }
                    ],
                    "synonyms": [
                        "грамота",
                        "награда"
                    ],
                    "common_phrases": [
                        "почётный диплом",
                        "диплом первой степени",
                        "вручить диплом",
                        "диплом лауреата"
                    ],
                    "target_lang_definitions": {
                        "en": "A document serving as an award for success in a contest, competition, or exhibition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "certificate",
                                "target_lang_sense_clarification": "award document"
                            },
                            {
                                "target_lang_word": "diploma",
                                "target_lang_sense_clarification": "formal award"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "678385aa-3ce4-30dc-997f-e8215175d6f0",
                    "sense_definition": "Выпускная квалификационная работа или проект студента для получения специальности.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "education_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенты последнего курса весь семестр пишут <w>диплом</w>.",
                            "target_lang_translations": {
                                "en": "Final-year students spend the whole semester writing their <w>thesis</w>."
                            }
                        },
                        {
                            "text": "Мой научный руководитель помог мне выбрать интересную тему для <w>диплома</w>.",
                            "target_lang_translations": {
                                "en": "My supervisor helped me choose an interesting topic for my <w>thesis</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "дипломная работа",
                        "дипломный проект"
                    ],
                    "common_phrases": [
                        "писать диплом",
                        "защита диплома",
                        "тема диплома",
                        "руководитель диплома"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to the thesis work itself"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A final qualifying work or project by a student to obtain a specialty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thesis",
                                "target_lang_sense_clarification": "academic paper"
                            },
                            {
                                "target_lang_word": "graduation project",
                                "target_lang_sense_clarification": "practical work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d0ae56f-aa1a-3bbc-80bc-c6dbb6483d01",
                    "sense_definition": "Официальное свидетельство, подтверждающее авторство научного открытия.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "science_doc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Комитет выдал учёным <w>диплом</w> на открытие нового физического эффекта.",
                            "target_lang_translations": {
                                "en": "The committee issued the scientists a <w>diploma</w> for the discovery of a new physical effect."
                            }
                        },
                        {
                            "text": "В реестре зарегистрирован <w>диплом</w>, подтверждающий приоритет изобретателя.",
                            "target_lang_translations": {
                                "en": "A <w>certificate</w> confirming the inventor's priority is registered in the registry."
                            }
                        }
                    ],
                    "synonyms": [
                        "свидетельство"
                    ],
                    "target_lang_definitions": {
                        "en": "An official certificate confirming the authorship of a scientific discovery."
                    },

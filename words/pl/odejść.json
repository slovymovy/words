                {
                    "sense_id": "978a5a1d-dd7f-358d-b687-9634cbb53ef9",
                    "sense_definition": "Oddalić się z jakiegoś miejsca lub od jakiejś osoby na pewną odległość.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_away",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <odejść> od krawędzi peronu dla własnego bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <отойдите> от края платформы ради вашей безопасности."
                            }
                        },
                        {
                            "text": "Musiał <odejść> na chwilę od biurka, żeby odebrać telefon.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось на минуту <отойти> от стола, чтобы ответить на звонок."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddalić się",
                        "odsunąć się"
                    ],
                    "antonyms": [
                        "przyjść",
                        "zbliżyć się"
                    ],
                    "common_phrases": [
                        "odejść od okna",
                        "odejść na bok",
                        "odejść kawałek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалиться с какого-либо места или от какого-либо человека на определенное расстояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отойти",
                                "target_lang_sense_clarification": "переместиться на небольшое расстояние, отступить"
                            },
                            {
                                "target_lang_word": "уйти",
                                "target_lang_sense_clarification": "покинуть место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc5bb1db-5f7d-380b-83a6-332de654b831",
                    "sense_definition": "Zrezygnować z pracy, funkcji lub zakończyć z kimś związek i opuścić tę osobę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abandonment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po dziesięciu latach postanowił <odejść> z firmy i założyć własny biznes.",
                            "target_lang_translations": {
                                "ru": "После десяти лет он решил <уйти> из компании и открыть свой бизнес."
                            }
                        },
                        {
                            "text": "To była trudna decyzja, aby <odejść> od męża.",
                            "target_lang_translations": {
                                "ru": "Это было трудное решение — <уйти> от мужа."
                            }
                        }
                    ],
                    "synonyms": [
                        "porzucić",
                        "zrezygnować",
                        "opuścić",
                        "rzucić"
                    ],
                    "antonyms": [
                        "zbliżyć się"
                    ],
                    "common_phrases": [
                        "odejść z pracy",
                        "odejść na emeryturę",
                        "odejść od żony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уволиться с работы, оставить должность или разорвать отношения с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уйти",
                                "target_lang_sense_clarification": "с работы или от партнера"
                            },
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "оставить кого-то (разг.)"
                            },
                            {
                                "target_lang_word": "уволиться",
                                "target_lang_sense_clarification": "с работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d75d2388-f447-3ce5-ba0a-f3c0d08cc92a",
                    "sense_definition": "Umrzeć; przestać żyć (słowo używane, aby wyrazić się w sposób delikatny i pełen szacunku).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz dziadek <odszedł> spokojnie we śnie w wieku dziewięćdziesięciu lat.",
                            "target_lang_translations": {
                                "ru": "Наш дедушка спокойно <ушел> во сне в возрасте девяноста лет."
                            }
                        },
                        {
                            "text": "Wszyscy byli smutni, gdy dowiedzieli się, że ich ulubiony aktor <odszedł>.",
                            "target_lang_translations": {
                                "ru": "Все были опечалены, узнав, что их любимый актер <ушел>."
                            }
                        }
                    ],
                    "synonyms": [
                        "umrzeć",
                        "skonać",
                        "zgasnąć"
                    ],
                    "common_phrases": [
                        "odejść na zawsze",
                        "odejść z tego świata",
                        "nagle odejść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умереть; перестать жить (используется для выражения деликатности и уважения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уйти",
                                "target_lang_sense_clarification": "из жизни (эвфемизм)"
                            },
                            {
                                "target_lang_word": "скончаться",
                                "target_lang_sense_clarification": "официально или уважительно"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0eef7886-961b-3b04-958d-f16ece22ed90",
                    "sense_definition": "Tijd verspillen door niets nuttigs te doen of doelloos rond te hangen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt niet de hele dag blijven <w>slampampen</w> als je je examens wilt halen.",
                            "target_lang_translations": {
                                "en": "You cannot keep <w>loafing</w> all day if you want to pass your exams.",
                                "ru": "Ты не можешь весь день <w>бездельничать</w>, если хочешь сдать экзамены."
                            }
                        },
                        {
                            "text": "Hij liep maar wat te <w>slampampen</w> in de stad omdat hij zijn trein had gemist.",
                            "target_lang_translations": {
                                "en": "He was just <w>loitering</w> around town because he had missed his train.",
                                "ru": "Он просто <w>слонялся</w> по городу, так как опоздал на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "luieren",
                        "lummelen",
                        "rondhangen",
                        "dagdieven",
                        "leeglopen"
                    ],
                    "common_phrases": [
                        "lopen te slampampen",
                        "zitten te slampampen"
                    ],
                    "target_lang_definitions": {
                        "en": "To waste time doing nothing useful or to hang around aimlessly.",
                        "ru": "Тратить время впустую, ничего полезного не делая, или бесцельно слоняться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loaf",
                                "target_lang_sense_clarification": "spending time in a lazy way"
                            },
                            {
                                "target_lang_word": "loiter",
                                "target_lang_sense_clarification": "standing or waiting around without apparent purpose"
                            },
                            {
                                "target_lang_word": "idle",
                                "target_lang_sense_clarification": "spending time doing nothing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слоняться",
                                "target_lang_sense_clarification": "бесцельно бродить"
                            },
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "проводить время без дела"
                            },
                            {
                                "target_lang_word": "околачиваться",
                                "target_lang_sense_clarification": "находиться где-либо без определенного дела (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f33cd59-37ae-38ac-aca9-b21861f5fd12",
                    "sense_definition": "Zich overdadig te buiten gaan aan eten en drinken tijdens een feest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gasten bleven tot diep in de nacht <w>slampampen</w> en zingen.",
                            "target_lang_translations": {
                                "en": "The guests continued to <w>carouse</w> and sing until late at night.",
                                "ru": "Гости продолжали <w>кутить</w> и петь до глубокой ночи."
                            }
                        },
                        {
                            "text": "Na de overwinning gingen de soldaten uitgebreid <w>slampampen</w> in de herberg.",
                            "target_lang_translations": {
                                "en": "After the victory, the soldiers went to <w>revel</w> extensively at the inn.",
                                "ru": "После победы солдаты отправились с размахом <w>пировать</w> в таверну."
                            }
                        }
                    ],
                    "synonyms": [
                        "brassen"
                    ],
                    "common_phrases": [
                        "overdadig slampampen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "deze betekenis is verouderd"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To indulge excessively in food and drink during a celebration.",
                        "ru": "Чрезмерно предаваться еде и питью во время празднества; кутить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carouse",
                                "target_lang_sense_clarification": "drinking and enjoying oneself in a noisy way"
                            },
                            {
                                "target_lang_word": "revel",
                                "target_lang_sense_clarification": "engaging in lively and noisy festivities"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кутить",
                                "target_lang_sense_clarification": "шумно веселиться с выпивкой"
                            },
                            {
                                "target_lang_word": "пировать",
                                "target_lang_sense_clarification": "участвовать в обильном застолье"
                            },
                            {
                                "target_lang_word": "бражничать",
                                "target_lang_sense_clarification": "пьянствовать (устар./разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

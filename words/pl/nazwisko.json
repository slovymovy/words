            "senses": [
                {
                    "sense_id": "73a88186-01aa-3ad0-a51a-78d97e996e49",
                    "sense_definition": "Nazwa rodowa wspólna dla członków rodziny, używana oficjalnie wraz z imieniem do identyfikacji osoby.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_name",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę wpisać swoje imię i <w>nazwisko</w> w formularzu zgłoszeniowym.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, впишите свое имя и <w>фамилию</w> в форму заявки."
                            }
                        },
                        {
                            "text": "Jej <w>nazwisko</w> panieńskie było trudne do wymówienia dla obcokrajowców.",
                            "target_lang_translations": {
                                "ru": "Ее девичья <w>фамилия</w> была труднопроизносимой для иностранцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "godność"
                    ],
                    "common_phrases": [
                        "nazwisko panieńskie",
                        "nazwisko rodowe",
                        "nosić nazwisko",
                        "zmienić nazwisko",
                        "mieć na nazwisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Родовое имя, общее для членов семьи, используемое официально вместе с личным именем для идентификации личности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фамилия",
                                "target_lang_sense_clarification": "родовое имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b00b6d6c-4c13-3f87-ac60-d5494122a2a5",
                    "sense_definition": "Osoba sławna, ciesząca się dużym uznaniem i autorytetem w określonej dziedzinie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "famous_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To bardzo gorące <w>nazwisko</w> w branży filmowej w tym roku.",
                            "target_lang_translations": {
                                "ru": "Это очень громкое <w>имя</w> в киноиндустрии в этом году."
                            }
                        },
                        {
                            "text": "Festiwal przyciągnął wielkie <w>nazwiska</w> ze świata muzyki klasycznej.",
                            "target_lang_translations": {
                                "ru": "Фестиваль привлек великие <w>имена</w> из мира классической музыки."
                            }
                        }
                    ],
                    "common_phrases": [

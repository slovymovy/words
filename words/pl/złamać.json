                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломаться",
                                "target_lang_sense_clarification": "психологически, не выдержать давления"
                            },
                            {
                                "target_lang_word": "сдаться",
                                "target_lang_sense_clarification": "прекратить сопротивление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42264241-9625-38f6-9687-1b24ba602c11",
                    "sense_definition": "Uformować tekst w szpalty lub strony, nadając mu ostateczny układ graficzny.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony redaktor potrafi tak <złamać> tekst, aby był czytelny i estetyczny.",
                            "target_lang_translations": {
                                "ru": "Опытный редактор может так <сверстать> текст, чтобы он был читабельным и эстетичным."
                            }
                        },
                        {
                            "text": "Artykuł został źle <złamany>, przez co zdjęcia nie pasują do opisów.",
                            "target_lang_translations": {
                                "ru": "Статья была плохо <сверстана>, из-за чего фотографии не подходят к описаниям."

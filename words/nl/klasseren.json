            "pos": "verb",
            "senses": [
                {
                    "sense_id": "818b8857-539f-3e10-9f3f-73165a027a9b",
                    "sense_definition": "Zaken in een bepaalde volgorde of reeks zetten, vaak om ze overzichtelijk te maken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "ordering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bibliothecaris moet de nieuwe boeken nog alfabetisch <w>klasseren</w>.",
                            "target_lang_translations": {
                                "ru": "Библиотекарь должен еще <w>рассортировать</w> новые книги в алфавитном порядке.",
                                "en": "The librarian still has to <w>sort</w> the new books alphabetically."
                            }
                        },
                        {
                            "text": "Je kunt de documenten het beste op datum <w>klasseren</w>.",
                            "target_lang_translations": {
                                "ru": "Документы лучше всего <w>систематизировать</w> по дате.",
                                "en": "It is best to <w>file</w> the documents by date."
                            }
                        }
                    ],
                    "synonyms": [
                        "rangschikken",
                        "ordenen",
                        "sorteren"
                    ],
                    "antonyms": [
                        "door elkaar gooien"
                    ],
                    "common_phrases": [
                        "alfabetisch klasseren",
                        "chronologisch klasseren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размещать предметы в определенном порядке или последовательности, часто для наглядности.",
                        "en": "To place items in a specific order or sequence, often to make them organized."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сортировать",
                                "target_lang_sense_clarification": "разделять по признакам"
                            },
                            {
                                "target_lang_word": "систематизировать",
                                "target_lang_sense_clarification": "приводить в систему"
                            },
                            {
                                "target_lang_word": "упорядочивать",
                                "target_lang_sense_clarification": "приводить в порядок"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "file",
                                "target_lang_sense_clarification": "documents or records"
                            },
                            {
                                "target_lang_word": "sort",
                                "target_lang_sense_clarification": "arrange in order"
                            },
                            {
                                "target_lang_word": "arrange",
                                "target_lang_sense_clarification": "put in order"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27fcdb40-0da3-3525-9721-f9de3f0186ea",
                    "sense_definition": "Iets of iemand in een bepaalde categorie, groep of klasse indelen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "categorizing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biologen <w>klasseren</w> dit dier als een reptiel.",
                            "target_lang_translations": {
                                "ru": "Биологи <w>классифицируют</w> это животное как рептилию.",
                                "en": "Biologists <w>classify</w> this animal as a reptile."
                            }
                        },
                        {
                            "text": "We kunnen deze klachten <w>klasseren</w> onder technische problemen.",
                            "target_lang_translations": {
                                "ru": "Мы можем <w>отнести</w> эти жалобы к техническим проблемам.",
                                "en": "We can <w>categorize</w> these complaints under technical problems."
                            }
                        }
                    ],
                    "synonyms": [
                        "indelen",
                        "categoriseren",
                        "rubriceren"
                    ],
                    "common_phrases": [
                        "per onderwerp klasseren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относить что-либо или кого-либо к определенной категории, группе или классу.",
                        "en": "To assign something or someone to a specific category, group, or class."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классифицировать",
                                "target_lang_sense_clarification": "распределять по классам"
                            },
                            {
                                "target_lang_word": "относить",
                                "target_lang_sense_clarification": "к категории или группе"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "classify",
                                "target_lang_sense_clarification": "assign to a class"
                            },
                            {
                                "target_lang_word": "categorize",
                                "target_lang_sense_clarification": "put into a category"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "031023b1-b3fd-35fc-aad4-5ccc2709e600",
                    "sense_definition": "Een bepaalde plaats in de rangschikking behalen of zich kwalificeren voor een volgende ronde in een wedstrijd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet wist zich als derde te <w>klasseren</w> voor de finale.",
                            "target_lang_translations": {
                                "ru": "Атлет смог <w>квалифицироваться</w> в финал третьим.",
                                "en": "The athlete managed to <w>qualify</w> third for the final."
                            }
                        },
                        {
                            "text": "Hij hoopte zich goed te <w>klasseren</w> in het algemeen klassement.",
                            "target_lang_translations": {
                                "ru": "Он надеялся <w>занять</w> высокое <w>место</w> в общем зачете.",
                                "en": "He hoped to <w>rank</w> well in the general classification."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich plaatsen",
                        "zich kwalificeren",
                        "eindigen"
                    ],
                    "common_phrases": [
                        "zich klasseren voor",
                        "zich goed klasseren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занять определенное место в рейтинге или пройти в следующий раунд соревнований.",
                        "en": "To achieve a certain place in a ranking or qualify for a next round in a competition."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квалифицироваться",
                                "target_lang_sense_clarification": "пройти отбор"
                            },
                            {
                                "target_lang_word": "занять место",
                                "target_lang_sense_clarification": "в рейтинге или соревновании"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "qualify",
                                "target_lang_sense_clarification": "for a competition"
                            },
                            {
                                "target_lang_word": "rank",
                                "target_lang_sense_clarification": "achieve a position"
                            },
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "finish in a position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a5b7c5b-4101-33b3-a2d4-333b57de5eed",
                    "sense_definition": "Een juridische zaak of dossier afsluiten zonder verdere actie te ondernemen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter besloot de zaak te <w>klasseren</w> wegens gebrek aan bewijs.",
                            "target_lang_translations": {
                                "ru": "Судья решил <w>прекратить</w> дело из-за недостатка улик.",
                                "en": "The judge decided to <w>dismiss</w> the case due to lack of evidence."
                            }
                        },
                        {
                            "text": "Het dossier werd zonder gevolg <w>geklasseerd</w>.",
                            "target_lang_translations": {
                                "ru": "Досье было <w>сдано в архив</w> без последствий.",
                                "en": "The file was <w>shelved</w> without further action."
                            }
                        }
                    ],
                    "synonyms": [
                        "seponeren",
                        "terzijde leggen"
                    ],
                    "common_phrases": [
                        "de zaak klasseren",
                        "zonder gevolg klasseren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Закрыть юридическое дело или досье без принятия дальнейших мер.",
                        "en": "To close a legal case or file without taking further action."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать в архив",
                                "target_lang_sense_clarification": "о документах или делах"
                            },
                            {
                                "target_lang_word": "прекратить",
                                "target_lang_sense_clarification": "о судебном деле"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "dismiss",
                                "target_lang_sense_clarification": "a legal case"
                            },
                            {
                                "target_lang_word": "shelve",
                                "target_lang_sense_clarification": "put aside without action"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "stop pursuing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4ffbc74-1e98-3310-a594-46e3ad3bf17e",
                    "sense_definition": "Een gebouw, landschap of object officieel de status van beschermd monument geven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "heritage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente wil het oude stadhuis laten <w>klasseren</w> om sloop te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет хочет <w>признать</w> старую ратушу <w>памятником</w>, чтобы предотвратить снос.",
                                "en": "The municipality wants to have the old town hall <w>listed</w> to prevent demolition."
                            }
                        },
                        {
                            "text": "Dit kasteel is <w>geklasseerd</w> als historisch monument.",
                            "target_lang_translations": {

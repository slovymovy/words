{
    "word_family": [
        "soes",
        "soezerig"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "35078d78-b184-30c9-88f1-49e1d42462ed",
                    "sense_definition": "Half slapen of in een lichte slaap verkeren, vaak zittend.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sleeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de warmte in de trein begon ik langzaam te <w>soezen</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за тепла в поезде я начал медленно <w>дремать</w>."
                            }
                        },
                        {
                            "text": "Mijn opa zat na de lunch heerlijk in zijn stoel te <w>soezen</w>.",
                            "target_lang_translations": {
                                "ru": "После обеда мой дедушка сладко <w>дремал</w>, сидя в своем кресле."
                            }
                        }
                    ],
                    "synonyms": [
                        "dommelen",
                        "dutten"
                    ],
                    "common_phrases": [
                        "zitten te soezen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в состоянии полусна или легкого сна, часто в сидячем положении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дремать",
                                "target_lang_sense_clarification": "находиться в полусне"
                            },
                            {
                                "target_lang_word": "клевать носом",
                                "target_lang_sense_clarification": "засыпать сидя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c8f0607-55d0-3cb5-b6b6-65f422ac4dea",
                    "sense_definition": "Meervoud van <w>soes</w>; kleine, luchtige gebakjes gevuld met slagroom of banketbakkersroom.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bakker verkoopt verse <w>soezen</w> met slagroom.",
                            "target_lang_translations": {
                                "ru": "Пекарь продает свежие <w>заварные пирожные</w> со взбитыми сливками."
                            }
                        },
                        {
                            "text": "Op het feestje werden schalen met heerlijke <w>soezen</w> uitgedeeld.",
                            "target_lang_translations": {
                                "ru": "На вечеринке раздавали блюда с вкусными <w>заварными пирожными</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькие воздушные пирожные из заварного теста, наполненные взбитыми сливками или кремом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заварные пирожные",
                                "target_lang_sense_clarification": "выпечка"
                            },
                            {
                                "target_lang_word": "профитроли",
                                "target_lang_sense_clarification": "вид пирожных"
                            }
                        ]
                    }
                }
            ]
        }
    ]

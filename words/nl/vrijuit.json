            "senses": [
                {
                    "sense_id": "09a2f4f9-b96b-3d99-bfc0-fa5cba4b3f9a",
                    "sense_definition": "Op een openhartige manier, zonder terughoudendheid of angst om iets te zeggen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij durfde eindelijk <w>vrijuit</w> te vertellen wat hem al die tijd dwarszat.",
                            "target_lang_translations": {
                                "en": "He finally dared to speak <w>freely</w> about what had been bothering him all this time.",
                                "ru": "Он наконец осмелился <w>открыто</w> рассказать о том, что его беспокоило всё это время."
                            }
                        },
                        {
                            "text": "Tijdens de vergadering kon iedereen <w>vrijuit</w> zijn mening geven over het nieuwe beleid.",
                            "target_lang_translations": {
                                "en": "During the meeting, everyone could give their opinion <w>freely</w> about the new policy.",
                                "ru": "Во время собрания каждый мог <w>свободно</w> высказать свое мнение о новой политике."
                            }
                        }
                    ],
                    "synonyms": [
                        "ronduit"
                    ],
                    "common_phrases": [
                        "vrijuit spreken",
                        "vrijuit praten"
                    ],
                    "target_lang_definitions": {
                        "en": "In a candid manner, without restraint or fear of speaking out.",
                        "ru": "Откровенно, без сдержанности или страха высказаться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "freely",
                                "target_lang_sense_clarification": "without hesitation or restriction"
                            },
                            {
                                "target_lang_word": "openly",
                                "target_lang_sense_clarification": "without hiding anything"
                            },
                            {
                                "target_lang_word": "frankly",
                                "target_lang_sense_clarification": "in an honest and direct way"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "открыто",
                                "target_lang_sense_clarification": "не скрывая мыслей"
                            },
                            {
                                "target_lang_word": "свободно",
                                "target_lang_sense_clarification": "без стеснения"
                            },
                            {
                                "target_lang_word": "откровенно",
                                "target_lang_sense_clarification": "честно и прямо"

{
    "word_family": [
        "otrzeć",
        "otarcie",
        "obcierać",
        "wycierać",
        "ścierać",
        "zacierać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a791c04-2bce-3f47-a814-f0ffe80005f0",
                    "sense_definition": "Usuwać ciecz, wilgoć lub brud z powierzchni, przesuwając po niej ręką, materiałem lub narzędziem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_removing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna siedziała na ławce i dyskretnie <w>ocierała</w> płynące po policzkach łzy.",
                            "target_lang_translations": {
                                "ru": "Девушка сидела на скамейке и незаметно <w>вытирала</w> текущие по щекам слезы."
                            }
                        },
                        {
                            "text": "Zmęczony biegacz co chwilę <w>ocierał</w> pot z czoła wierzchem dłoni.",
                            "target_lang_translations": {
                                "ru": "Уставший бегун то и дело <w>отирал</w> пот со лба тыльной стороной ладони."
                            }
                        }
                    ],
                    "synonyms": [
                        "wycierać",
                        "osuszać"
                    ],
                    "antonyms": [
                        "moczyć",
                        "brudzić"
                    ],
                    "common_phrases": [
                        "ocierać łzy",
                        "ocierać pot",
                        "ocierać czoło",
                        "ocierać twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять жидкость, влагу или грязь с поверхности, проводя по ней рукой, тканью или инструментом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытирать",
                                "target_lang_sense_clarification": "удалять влагу или грязь"
                            },
                            {
                                "target_lang_word": "отирать",
                                "target_lang_sense_clarification": "удалять пот или слезы"
                            },
                            {
                                "target_lang_word": "утирать",
                                "target_lang_sense_clarification": "вытирать лицо, слезы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5463fe53-a4a6-3e8f-9c70-6e98ec348a48",
                    "sense_definition": "Dotykać kogoś lub czegoś lekko podczas ruchu, przesuwając się tuż obok (często z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wąski korytarz sprawiał, że przechodnie nieustannie <w>ocierali się</w> o siebie ramionami.",
                            "target_lang_translations": {
                                "ru": "Из-за узкого коридора прохожие постоянно <w>задевали</w> друг друга плечами."
                            }
                        },
                        {
                            "text": "Mój kot głośno mruczy i chętnie <w>ociera się</w> o nogi gości.",
                            "target_lang_translations": {
                                "ru": "Мой кот громко мурлычет и охотно <w>трется</w> о ноги гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "muskać",
                        "dotykać",
                        "trącać"
                    ],
                    "antonyms": [
                        "omijać"
                    ],
                    "common_phrases": [
                        "ocierać się o ściany",
                        "ocierać się o ludzi",
                        "ocierać się o nogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка касаться кого-то или чего-то во время движения, проходя вплотную (часто используется как возвратный глагол)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тереться",
                                "target_lang_sense_clarification": "касаться при движении, ластиться"
                            },
                            {
                                "target_lang_word": "задевать",
                                "target_lang_sense_clarification": "касаться, проходя мимо"
                            },
                            {
                                "target_lang_word": "соприкасаться",
                                "target_lang_sense_clarification": "вступать в контакт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "592fe70d-127f-3e6d-8202-d48e4988842f",
                    "sense_definition": "Powodować uszkodzenie skóry lub powierzchni przedmiotu poprzez długotrwałe lub mocne tarcie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "injury_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te nowe skórzane buty strasznie <w>ocierają</w> mi pięty podczas chodzenia.",
                            "target_lang_translations": {
                                "ru": "Эти новые кожаные туфли ужасно <w>натирают</w> мне пятки при ходьбе."
                            }
                        },
                        {
                            "text": "Dziecko przewróciło się na chodniku i boleśnie <w>ocierało</w> sobie kolana przy każdej próbie wstania.",
                            "target_lang_translations": {
                                "ru": "Ребенок упал на тротуаре и больно <w>обдирал</w> себе колени при каждой попытке встать."
                            }
                        }
                    ],
                    "synonyms": [
                        "obcierać",
                        "ranić",
                        "kaleczyć"
                    ],
                    "common_phrases": [
                        "buty ocierają",
                        "ocierać naskórek",
                        "ocierać pięty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать повреждение кожи или поверхности предмета в результате длительного или сильного трения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "натирать",
                                "target_lang_sense_clarification": "вызывать мозоли или раздражение"
                            },
                            {
                                "target_lang_word": "обдирать",
                                "target_lang_sense_clarification": "сдирать кожу трением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2a6cf80-954e-32a5-b271-1635fa98bdd7",
                    "sense_definition": "Być bardzo blisko jakiegoś stanu, zjawiska lub granicy; balansować na krawędzi czegoś (używane z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego ryzykowne zachowanie podczas wspinaczki często <w>ocierało się</w> o śmierć.",
                            "target_lang_translations": {
                                "ru": "Его рискованное поведение во время восхождения часто <w>граничило</w> со смертью."
                            }
                        },
                        {
                            "text": "Ten film był tak dziwny, że momentami <w>ocierał się</w> o kicz.",
                            "target_lang_translations": {
                                "ru": "Этот фильм был таким странным, что местами <w>граничил</w> с китчем."
                            }
                        }
                    ],
                    "synonyms": [
                        "graniczyć",
                        "balansować"
                    ],
                    "common_phrases": [
                        "ocierać się o śmierć",
                        "ocierać się o kicz",
                        "ocierać się o geniusz",
                        "ocierać się o śmieszność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть очень близким к какому-либо состоянию, явлению или границе; балансировать на грани чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "граничить",
                                "target_lang_sense_clarification": "быть на грани чего-либо"
                            },
                            {
                                "target_lang_word": "балансировать",
                                "target_lang_sense_clarification": "находиться в неустойчивом состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "566790d9-040f-35c3-b546-ced9789eaa29",
                    "sense_definition": "Delikatnie ścierać wierzchnią warstwę skórki owoców cytrusowych na tarce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do przygotowania tego deseru musisz dokładnie umyć cytrynę i <w>ocierać</w> z niej skórkę.",
                            "target_lang_translations": {
                                "ru": "Для приготовления этого десерта нужно тщательно вымыть лимон и <w>натирать</w> с него цедру."
                            }
                        },
                        {
                            "text": "Kucharz powoli <w>ocierał</w> pomarańczę, aby dodać aromatu do ciasta.",
                            "target_lang_translations": {
                                "ru": "Повар медленно <w>натирал</w> апельсин, чтобы добавить аромат в тесто."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścierać"
                    ],
                    "common_phrases": [
                        "ocierać skórkę z cytryny",
                        "ocierać cytrusy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аккуратно снимать верхний слой кожуры цитрусовых (цедру) с помощью терки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "натирать",
                                "target_lang_sense_clarification": "измельчать на терке"
                            },
                            {
                                "target_lang_word": "снимать цедру",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c81ae0e0-b868-3a48-83bf-34dce9a6b766",
                    "sense_definition": "To sell goods to a foreign country.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Japan <w>exports</w> electronic goods throughout the world.",
                            "target_lang_translations": {
                                "ru": "Япония <w>экспортирует</w> электронные товары по всему миру."
                            }
                        },
                        {
                            "text": "The company plans to <w>export</w> its products to Europe next year.",
                            "target_lang_translations": {
                                "ru": "Компания планирует <w>экспортировать</w> свою продукцию в Европу в следующем году."
                            }
                        }
                    ],
                    "antonyms": [
                        "import"
                    ],
                    "common_phrases": [
                        "export goods",
                        "export products",
                        "export to countries"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продавать товары или услуги в другую страну."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспортировать",
                                "target_lang_sense_clarification": "продавать за границу"
                            },
                            {
                                "target_lang_word": "вывозить",
                                "target_lang_sense_clarification": "отправлять товары за границу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f1071be-e002-31f2-b146-6ac3d6f7c4f1",
                    "sense_definition": "To send data from one program or format to another.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>export</w> the file as a PDF document.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>экспортировать</w> этот файл как PDF-документ."
                            }
                        },
                        {
                            "text": "The software allows users to <w>export</w> data easily.",
                            "target_lang_translations": {
                                "ru": "Программное обеспечение позволяет пользователям легко <w>экспортировать</w> данные."
                            }
                        }
                    ],
                    "antonyms": [
                        "import"
                    ],
                    "common_phrases": [
                        "export data",
                        "export files",
                        "export as PDF"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохранять данные в формате, который может быть использован другой программой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспортировать",
                                "target_lang_sense_clarification": "данные или файлы"
                            },
                            {
                                "target_lang_word": "выгружать",
                                "target_lang_sense_clarification": "данные из системы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5d18f9d-c594-3d02-94fa-dd515a8f1548",
                    "sense_definition": "To cause an idea, culture, or practice to spread to another part of the world.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country tried to <w>export</w> its culture to neighboring nations.",
                            "target_lang_translations": {
                                "ru": "Страна пыталась <w>экспортировать</w> свою культуру в соседние государства."
                            }
                        },
                        {
                            "text": "It is difficult to <w>export</w> democracy to regions with different traditions.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>экспортировать</w> демократию в регионы с другими традициями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "export culture",
                        "export democracy",
                        "export ideas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространять идеи, культуру или обычаи в другие части мира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспортировать",
                                "target_lang_sense_clarification": "идеи или культуру"
                            },
                            {
                                "target_lang_word": "распространять",
                                "target_lang_sense_clarification": "влияние или практики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d2f0ee5-896b-3a47-9d0a-f741422fb0d1",
                    "sense_definition": "To carry away.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They sought to <w>export</w> honour from a man through envy.",
                            "target_lang_translations": {
                                "ru": "Они стремились <w>похитить</w> честь человека через зависть."
                            }
                        },
                        {
                            "text": "The thieves managed to <w>export</w> the stolen items before anyone noticed.",
                            "target_lang_translations": {
                                "ru": "Ворам удалось <w>вывезти</w> украденные вещи, прежде чем кто-либо заметил."
                            }
                        }
                    ],
                    "synonyms": [
                        "remove"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уносить или увозить прочь (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уносить",
                                "target_lang_sense_clarification": "похищать или забирать"
                            },
                            {
                                "target_lang_word": "вывозить",
                                "target_lang_sense_clarification": "удалять откуда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43ea4fc0-9125-3ece-acb3-4f02649ec907",
                    "sense_definition": "To put up a child for international adoption.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The agency was investigated for trying to <w>export</w> children illegally.",
                            "target_lang_translations": {
                                "ru": "Агентство находилось под следствием за попытку незаконно <w>вывезти</w> детей."
                            }
                        },
                        {
                            "text": "Laws were changed to prevent people from seeking to <w>export</w> orphans for profit.",
                            "target_lang_translations": {
                                "ru": "Законы были изменены, чтобы помешать людям пытаться <w>вывозить</w> сирот ради наживы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять детей за границу для усыновления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывозить",
                                "target_lang_sense_clarification": "детей для усыновления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

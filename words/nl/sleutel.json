            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd293290-d766-3e29-804c-4afd9d3c24af",
                    "sense_definition": "Een metalen voorwerp waarmee je een slot kunt openen en sluiten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_key",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan mijn <w>sleutel</w> nergens vinden.",
                            "target_lang_translations": {
                                "en": "I can't find my <w>key</w> anywhere.",
                                "ru": "Я нигде не могу найти свой <w>ключ</w>."
                            }
                        },
                        {
                            "text": "Hij stak de <w>sleutel</w> in het slot van de voordeur.",
                            "target_lang_translations": {
                                "en": "He put the <w>key</w> in the lock of the front door.",
                                "ru": "Он вставил <w>ключ</w> в замок входной двери."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de sleutel omdraaien",
                        "sleutel in het slot",
                        "bos sleutels"
                    ],
                    "target_lang_definitions": {
                        "en": "A metal object used to open and close a lock.",
                        "ru": "Металлический предмет, используемый для открывания и закрывания замка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "key",
                                "target_lang_sense_clarification": "for a lock"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ключ",
                                "target_lang_sense_clarification": "от замка"
                            }
                        ]

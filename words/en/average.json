                        {
                            "text": "It was just an <w>average</w> day at the office with nothing special happening.",
                            "target_lang_translations": {
                                "ru": "Это был самый <w>обычный</w> день в офисе, ничего особенного не происходило."
                            }
                        }
                    ],
                    "synonyms": [
                        "typical",
                        "normal",
                        "standard",
                        "usual",
                        "regular",
                        "conventional"
                    ],
                    "common_phrases": [
                        "average person",
                        "average day",
                        "average family"
                    ],
                    "target_lang_definitions": {

                },
                {
                    "sense_id": "ae0787d7-fa02-3761-ab57-85f3e2a71f7f",
                    "sense_definition": "Een onderwerp, idee of probleem tijdelijk uitstellen om er later op terug te komen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_postpone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Laten we deze discussie even <w>parkeren</w> tot de volgende vergadering.",
                            "target_lang_translations": {
                                "en": "Let's <w>park</w> this discussion for a moment until the next meeting.",
                                "ru": "Давайте пока <w>отложим</w> эту дискуссию до следующего собрания."
                            }
                        },
                        {
                            "text": "Dat is een interessant punt, maar we moeten het nu <w>parkeren</w> vanwege de tijd.",
                            "target_lang_translations": {
                                "en": "That is an interesting point, but we have to <w>park</w> it now due to time constraints.",
                                "ru": "Это интересный момент, но нам нужно сейчас его <w>отложить</w> из-за нехватки времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitstellen",
                        "opschorten"
                    ],
                    "antonyms": [

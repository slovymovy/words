{
    "word_family": [
        "подозрение",
        "подозрительный",
        "подозрительность",
        "заподозрить",
        "подозрительно"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "184a34d9-d048-36ba-aeaa-9fd1a63324ba",
                    "sense_definition": "Считать кого-либо виновным в чём-либо предосудительном, нечестном или незаконном.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "guilt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция <подозревает> соседа в краже велосипеда.",
                            "target_lang_translations": {
                                "en": "The police <suspect> the neighbor of stealing the bicycle."
                            }
                        },
                        {
                            "text": "Я никогда не <подозревал> своего друга в обмане.",
                            "target_lang_translations": {
                                "en": "I never <suspected> my friend of deceit."
                            }
                        }
                    ],
                    "antonyms": [
                        "верить"
                    ],
                    "common_phrases": [
                        "подозревать в убийстве",
                        "подозревать в измене",
                        "подозревать в краже",
                        "подозревать во лжи"
                    ],
                    "target_lang_definitions": {
                        "en": "To believe that someone is guilty of a crime or doing something wrong without having definite proof."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspect",
                                "target_lang_sense_clarification": "to believe someone is guilty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1def465-509c-3d2c-9b43-261b03edbc42",
                    "sense_definition": "Предполагать возможность чего-либо, догадываться о наличии чего-либо (часто скрытого или неприятного).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "supposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи <подозревают> у пациента редкую аллергию.",
                            "target_lang_translations": {
                                "en": "Doctors <suspect> the patient has a rare allergy."
                            }
                        },
                        {
                            "text": "Она начала смутно <подозревать>, что за ней следят.",
                            "target_lang_translations": {
                                "en": "She began to vaguely <suspect> that she was being watched."
                            }
                        }
                    ],
                    "synonyms": [
                        "предполагать",
                        "догадываться",
                        "допускать"
                    ],
                    "common_phrases": [
                        "подозревать неладное",
                        "смутно подозревать",
                        "подозревать худшее"
                    ],
                    "target_lang_definitions": {
                        "en": "To think that something, often something bad or hidden, is likely to be true or exist."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspect",
                                "target_lang_sense_clarification": "to think something is likely true"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8298fcd-c44a-3ae8-8544-d474e23c19d9",
                    "sense_definition": "Думать или полагать, выражая неуверенное мнение или предчувствие.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "supposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я сильно <подозреваю>, что мы сегодня опоздаем на поезд.",
                            "target_lang_translations": {
                                "en": "I strongly <suspect> that we will be late for the train today."
                            }
                        },
                        {
                            "text": "<Подозреваю>, что этот план никому не понравится.",
                            "target_lang_translations": {
                                "en": "I <suspect> that no one will like this plan."
                            }
                        }
                    ],
                    "synonyms": [
                        "думать",
                        "считать"
                    ],
                    "common_phrases": [
                        "сильно подозреваю",
                        "смею подозревать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To have a feeling or opinion that something is true, often with a sense of uncertainty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspect",
                                "target_lang_sense_clarification": "to have a hunch or feeling"
                            },
                            {
                                "target_lang_word": "suppose",
                                "target_lang_sense_clarification": "to think or assume"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
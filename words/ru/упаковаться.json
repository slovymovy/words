            "senses": [
                {
                    "sense_id": "13a4a774-5d94-3b2e-b5a7-d1e725f1b57c",
                    "sense_definition": "Собрать и уложить свои вещи в сумки, чемоданы или коробки (обычно перед отъездом или переездом).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы быстро <упаковались> и вызвали такси в аэропорт.",
                            "target_lang_translations": {
                                "en": "We quickly <w>packed</w> and called a taxi to the airport."
                            }
                        },
                        {
                            "text": "Ты уже <упаковался> или тебе нужна помощь со сборами?",
                            "target_lang_translations": {
                                "en": "Have you <w>packed</w> yet or do you need help getting ready?"
                            }
                        }
                    ],
                    "synonyms": [
                        "собраться",
                        "уложиться"
                    ],
                    "antonyms": [
                        "распаковаться"
                    ],
                    "common_phrases": [
                        "быстро упаковаться",
                        "полностью упаковаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To gather and put one's belongings into bags or suitcases, usually before a trip or move."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pack",
                                "target_lang_sense_clarification": "to put belongings into luggage"
                            },
                            {
                                "target_lang_word": "pack up",
                                "target_lang_sense_clarification": "to finish packing one's things"
                            },
                            {
                                "target_lang_word": "get packed",
                                "target_lang_sense_clarification": "to complete the process of packing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd5ea8bb-ba96-3168-bfb1-80a65772e5ba",
                    "sense_definition": "Поместиться, уложиться внутрь чего-либо при упаковке (о предметах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все книги отлично <упаковались> в одну большую коробку.",
                            "target_lang_translations": {
                                "en": "All the books <w>packed</w> perfectly into one big box."
                            }
                        },
                        {
                            "text": "Одежда <упаковалась> в чемодан очень компактно.",
                            "target_lang_translations": {
                                "en": "The clothes <w>packed</w> very compactly into the suitcase."
                            }
                        }
                    ],
                    "synonyms": [
                        "поместиться",
                        "уложиться",
                        "влезть"
                    ],
                    "common_phrases": [
                        "компактно упаковаться",
                        "хорошо упаковаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To fit or be placed inside a container during the packing process (referring to objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pack",
                                "target_lang_sense_clarification": "to fit into a container"
                            },
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "to be capable of going into a space"
                            }
                        ]
                    }
                }
            ]
        }

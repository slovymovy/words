{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "a5d7fe07-1d90-3768-939b-bd466005be0a",
                    "sense_definition": "On the far side of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small village lies just <w>beyond</w> the river.",
                            "target_lang_translations": {
                                "ru": "Маленькая деревня находится прямо <w>за</w> рекой."
                            }
                        },
                        {
                            "text": "Please do not swim <w>beyond</w> the safety buoys.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не заплывайте <w>за</w> буйки."
                            }
                        }
                    ],
                    "antonyms": [
                        "before"
                    ],
                    "common_phrases": [
                        "beyond the horizon",
                        "beyond the sea"
                    ],
                    "target_lang_definitions": {
                        "ru": "На другой стороне чего-либо; дальше чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "позади чего-либо, на другой стороне"
                            },
                            {
                                "target_lang_word": "по ту сторону",
                                "target_lang_sense_clarification": "на противоположной стороне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1c9c547-1166-3fd1-ae23-94e7726f5e22",
                    "sense_definition": "Later than a specific time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "location_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The meeting continued well <w>beyond</w> the scheduled time.",
                            "target_lang_translations": {
                                "ru": "Встреча продолжалась еще долго <w>после</w> назначенного времени."
                            }
                        },
                        {
                            "text": "You should not stay out <w>beyond</w> midnight on a school night.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует гулять <w>после</w> полуночи в учебные дни."
                            }
                        }
                    ],
                    "antonyms": [
                        "before",
                        "earlier"
                    ],
                    "common_phrases": [
                        "beyond midnight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позже определенного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "позже указанного времени"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "позже (обычно о позднем времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a532a587-0515-3a16-a087-294292336f6e",
                    "sense_definition": "Greater than or exceeding a limit, expectation, or scope.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The success of the project went far <w>beyond</w> our expectations.",
                            "target_lang_translations": {
                                "ru": "Успех проекта вышел далеко <w>за</w> рамки наших ожиданий."
                            }
                        },
                        {
                            "text": "He acted <w>beyond</w> the call of duty to help the customer.",
                            "target_lang_translations": {
                                "ru": "Он сделал <w>больше</w>, чем требовал долг, чтобы помочь клиенту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beyond expectations",
                        "beyond belief",
                        "beyond doubt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше или выше определенного предела, ожиданий или рамок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сверх",
                                "target_lang_sense_clarification": "больше, чем ожидалось или требовалось"
                            },
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "превосходя уровень или предел"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "выходя за рамки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edde1d65-66d3-36b8-9e21-87bcf97a6e4a",
                    "sense_definition": "Past the limits of; not capable of being affected or reached by.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old car was damaged <w>beyond</w> repair after the accident.",
                            "target_lang_translations": {
                                "ru": "Старая машина после аварии <w>не подлежала</w> ремонту."
                            }
                        },
                        {
                            "text": "The situation is now <w>beyond</w> my control.",
                            "target_lang_translations": {
                                "ru": "Ситуация теперь <w>вне</w> моего контроля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beyond repair",
                        "beyond control",
                        "beyond help",
                        "beyond reach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вне пределов чего-либо; не поддающийся воздействию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вне",
                                "target_lang_sense_clarification": "за пределами сферы действия или влияния"
                            },
                            {
                                "target_lang_word": "не поддающийся",
                                "target_lang_sense_clarification": "когда что-то невозможно исправить или изменить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcf85819-2023-390b-98ec-00f282f367a9",
                    "sense_definition": "Too difficult for someone to understand.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>beyond</w> me why he decided to quit his job.",
                            "target_lang_translations": {
                                "ru": "Мне <w>непостижимо</w>, почему он решил уволиться."
                            }
                        },
                        {
                            "text": "Advanced physics is completely <w>beyond</w> most high school students.",
                            "target_lang_translations": {
                                "ru": "Высшая физика совершенно <w>недоступна</w> для понимания большинства старшеклассников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beyond me",
                        "beyond comprehension"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слишком сложно для понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выше понимания",
                                "target_lang_sense_clarification": "слишком сложно для кого-либо"
                            },
                            {
                                "target_lang_word": "непостижимо",
                                "target_lang_sense_clarification": "невозможно понять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "826ee327-dae2-3e61-a249-8fce2067f6b4",
                    "sense_definition": "In addition to; apart from.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have nothing to say <w>beyond</w> what I have already told you.",
                            "target_lang_translations": {
                                "ru": "Мне нечего сказать <w>кроме</w> того, что я вам уже говорил."
                            }
                        },
                        {
                            "text": "She has no family in this country <w>beyond</w> her brother.",
                            "target_lang_translations": {
                                "ru": "У нее нет семьи в этой стране, <w>кроме</w> брата."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В дополнение к; кроме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "за исключением или в дополнение к"
                            },
                            {
                                "target_lang_word": "помимо",
                                "target_lang_sense_clarification": "в дополнение к"
                            },
                            {
                                "target_lang_word": "не считая",
                                "target_lang_sense_clarification": "исключая"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c92f79c9-106d-3496-b5f7-bcb1f5ed7a3e",
                    "sense_definition": "Farther along in space or time.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_loc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We looked at the mountains and the valleys <w>beyond</w>.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели на горы и долины <w>за</w> ними."
                            }
                        },
                        {
                            "text": "The technology of the 1990s and <w>beyond</w> changed the world.",
                            "target_lang_translations": {
                                "ru": "Технологии 90-х годов и <w>далее</w> изменили мир."
                            }
                        }
                    ],
                    "common_phrases": [
                        "above and beyond",
                        "infinity and beyond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дальше в пространстве или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальше",
                                "target_lang_sense_clarification": "на большем расстоянии или в будущем"
                            },
                            {
                                "target_lang_word": "за пределами",
                                "target_lang_sense_clarification": "снаружи, дальше видимого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a3e65b3-0e87-37c6-aec7-9037987fa455",
                    "sense_definition": "Extremely; to a very high degree.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_intense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>beyond</w> grateful for your generous help.",
                            "target_lang_translations": {
                                "ru": "Я <w>безмерно</w> благодарен за вашу щедрую помощь."
                            }
                        },
                        {
                            "text": "The view from the top of the tower was <w>beyond</w> beautiful.",
                            "target_lang_translations": {
                                "ru": "Вид с вершины башни был <w>невероятно</w> красив."
                            }
                        }
                    ],
                    "synonyms": [
                        "exceedingly",
                        "highly",
                        "immensely",
                        "terribly"
                    ],
                    "common_phrases": [
                        "beyond excited"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as an intensifier"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно; в высшей степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невероятно",
                                "target_lang_sense_clarification": "очень, чрезвычайно"
                            },
                            {
                                "target_lang_word": "безмерно",
                                "target_lang_sense_clarification": "в высшей степени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4e3ae135-3c0b-3225-b716-c9e7ad61caf8",
                    "sense_definition": "The unknown or the afterlife.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He claims to receive messages from the great <w>beyond</w>.",
                            "target_lang_translations": {
                                "ru": "Он утверждает, что получает послания из <w>потустороннего мира</w>."
                            }
                        },
                        {
                            "text": "We all wonder what lies in the <w>beyond</w> after death.",
                            "target_lang_translations": {
                                "ru": "Мы все гадаем, что нас ждет в <w>загробном мире</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the great beyond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Загробная жизнь или неизвестное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загробный мир",
                                "target_lang_sense_clarification": "жизнь после смерти"
                            },
                            {
                                "target_lang_word": "потустороннее",
                                "target_lang_sense_clarification": "неизвестное, мистическое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
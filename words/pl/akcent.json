            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8337b87-85dc-3bfb-924b-1b031a323839",
                    "sense_definition": "Specyficzny sposób wymawiania słów, charakterystyczny dla cudzoziemców lub mieszkańców określonego regionu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "speech_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mężczyzna mówi z dziwnym <w>akcentem</w>, więc trudno go zrozumieć.",
                            "target_lang_translations": {
                                "ru": "Этот мужчина говорит со странным <w>акцентом</w>, поэтому его трудно понять."
                            }
                        },
                        {
                            "text": "Mimo wielu lat spędzonych w Polsce, ona wciąż ma francuski <w>akcent</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на много лет, проведенных в Польше, у нее все еще французский <w>акцент</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obcy akcent",
                        "polski akcent",
                        "mówić z akcentem",
                        "silny akcent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическая манера произношения слов, характерная для иностранцев или жителей определенного региона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "произношение"
                            },
                            {
                                "target_lang_word": "выговор",
                                "target_lang_sense_clarification": "манера речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b82ec672-6759-3920-b2ba-3d43c7b8237b",
                    "sense_definition": "Wyróżnienie jednej sylaby w wyrazie poprzez jej silniejsze lub głośniejsze wymówienie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W języku polskim <w>akcent</w> pada zazwyczaj na przedostatnią sylabę wyrazu.",
                            "target_lang_translations": {
                                "ru": "В польском языке <w>ударение</w> падает обычно на предпоследний слог слова."
                            }
                        },
                        {
                            "text": "Nauczyciel poprawił mój błędny <w>akcent</w> w tym trudnym słowie.",
                            "target_lang_translations": {
                                "ru": "Учитель исправил моё неправильное <w>ударение</w> в этом трудном слове."
                            }
                        }
                    ],
                    "synonyms": [
                        "akcent wyrazowy",
                        "przycisk",
                        "nacisk"

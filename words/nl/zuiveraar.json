                {
                    "sense_id": "dc0743e4-0a78-32c0-9370-8746729aeab2",
                    "sense_definition": "Iemand of een apparaat dat verontreinigingen verwijdert om iets schoon te maken.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>zuiveraar</w> haalt alle schadelijke stoffen uit het drinkwater.",
                            "target_lang_translations": {
                                "en": "The <w>purifier</w> removes all harmful substances from the drinking water.",
                                "ru": "<w>Очиститель</w> удаляет все вредные вещества из питьевой воды."
                            }
                        },
                        {
                            "text": "In de fabriek werkt hij als <w>zuiveraar</w> van chemische restproducten.",
                            "target_lang_translations": {
                                "en": "In the factory, he works as a <w>purifier</w> of chemical residual products.",
                                "ru": "На заводе он работает <w>очистителем</w> химических отходов."
                            }
                        }
                    ],
                    "synonyms": [
                        "purist"
                    ],
                    "antonyms": [
                        "vervuiler"
                    ],
                    "common_phrases": [
                        "waterzuiveraar",
                        "luchtzuiveraar"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone or a device that removes impurities to make something clean.",
                        "ru": "Тот, кто или то, что удаляет загрязнения для очистки чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "purifier",
                                "target_lang_sense_clarification": "device or agent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "очиститель",
                                "target_lang_sense_clarification": "устройство или вещество"
                            },
                            {
                                "target_lang_word": "фильтр",
                                "target_lang_sense_clarification": "устройство для фильтрации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d04bc97-8625-3e6c-86f0-56ac2c0d3163",
                    "sense_definition": "Iemand die streeft naar het behoud van de oorspronkelijke zuiverheid, bijvoorbeeld in taal, cultuur of bevolking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ideology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als <w>zuiveraar</w> van de taal vermijdt hij elk Engels leenwoord.",
                            "target_lang_translations": {
                                "en": "As a language <w>purist</w>, he avoids every English loanword.",
                                "ru": "Будучи языковым <w>пуристом</w>, он избегает любых английских заимствований."
                            }
                        },
                        {
                            "text": "De politieke <w>zuiveraar</w> wilde de partij ontdoen van afwijkende meningen.",
                            "target_lang_translations": {
                                "en": "The political <w>purifier</w> wanted to rid the party of dissenting opinions.",
                                "ru": "Политический <w>чистильщик</w> хотел избавить партию от инакомыслия."
                            }
                        }
                    ],
                    "synonyms": [
                        "purist"
                    ],
                    "common_phrases": [
                        "etnisch zuiveraar",
                        "taalzuiveraar"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who strives to preserve original purity, for example in language, culture, or population.",
                        "ru": "Человек, стремящийся сохранить первоначальную чистоту, например, в языке, культуре или составе населения."
                    },

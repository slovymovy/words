    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00f1f719-2d67-3d47-9924-0c674d4e0206",
                    "sense_definition": "Rzemieślnik zajmujący się obróbką metalu na gorąco, wykuwaniem przedmiotów żelaznych i podkuwaniem koni.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "craftsman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach w każdej wiosce pracował <w>kowal</w>.",
                            "target_lang_translations": {
                                "ru": "В давние времена в каждой деревне работал <w>кузнец</w>."
                            }
                        },
                        {
                            "text": "Miejscowy <w>kowal</w> naprawił nam pękniętą oś w wozie.",
                            "target_lang_translations": {
                                "ru": "Местный <w>кузнец</w> починил нам треснувшую ось в телеге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kowal własnego losu",
                        "zdrowy jak kowal",
                        "warsztat kowala"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремесленник, занимающийся горячей обработкой металла, ковкой железных изделий и подковыванием лошадей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кузнец",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37f2d6cc-580c-3d99-8a3c-a7c765c4737f",
                    "sense_definition": "Pospolity owad o charakterystycznym czerwono-czarnym wzorze na odwłoku, często spotykany w dużych grupach pod drzewami (lipami).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod starą lipą biegało stado małych <w>kowali</w>.",
                            "target_lang_translations": {
                                "ru": "Под старой липой бегала стайка маленьких <w>клопов-солдатиков</w>."
                            }
                        },
                        {
                            "text": "Dzieci obserwowały, jak <w>kowal</w> wygrzewa się na słońcu.",
                            "target_lang_translations": {
                                "ru": "Дети наблюдали, как <w>клоп-солдатик</w> греется на солнце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kowal bezskrzydły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обычное насекомое с характерным красно-черным узором на брюшке, часто встречающееся большими группами под деревьями (липами)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клоп-солдатик",
                                "target_lang_sense_clarification": "насекомое"
                            },
                            {
                                "target_lang_word": "красноклоп",
                                "target_lang_sense_clarification": "научное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7018d3d7-6b34-34e8-a19b-3166ea715715",
                    "sense_definition": "Uczeń, który uczy się bardzo dużo, często na pamięć i bez zrozumienia, aby zdobyć dobre oceny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nikt go nie lubił w klasie, bo był strasznym <w>kowalem</w>.",
                            "target_lang_translations": {
                                "ru": "Никто не любил его в классе, потому что он был жутким <w>зубрилой</w>."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>kowalem</w>, chodź z nami na boisko.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>зубрилой</w>, пошли с нами на площадку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "szkolne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ученик, который очень много учится, часто наизусть и без понимания, чтобы получить хорошие оценки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зубрила",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "заучка",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b9a7606-60f8-34b1-9e7c-45a834695fbf",
                    "sense_definition": "Duży, drapieżny chrząszcz wodny, który potrafi świetnie pływać.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "insect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W stawie pływał drapieżny <w>kowal</w>, polując na larwy.",
                            "target_lang_translations": {
                                "ru": "В пруду плавал хищный <w>плавунец</w>, охотясь на личинок."
                            }
                        },
                        {
                            "text": "Ten gatunek chrząszcza to <w>kowal</w>, znany też jako pływak.",
                            "target_lang_translations": {
                                "ru": "Этот вид жука — <w>плавунец</w>, известный также как водяной жук."
                            }
                        }
                    ],
                    "synonyms": [
                        "pływak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный хищный водяной жук, который отлично плавает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавунец",
                                "target_lang_sense_clarification": "водный жук"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d0ca9bdb-40a2-3cc6-a40e-2220a380c33d",
                    "sense_definition": "Jedno z najpopularniejszych nazwisk w Polsce.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Kowal</w> jest naszym nowym sąsiadem z naprzeciwka.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Коваль</w> — наш новый сосед напротив."
                            }
                        },
                        {
                            "text": "Na liście obecności jest trzech uczniów o nazwisku <w>Kowal</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих трое учеников по фамилии <w>Коваль</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из самых популярных фамилий в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Коваль",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cad326fc-5961-3f2b-80b1-0149aa9b6306",
                    "sense_definition": "Miasto w Polsce, położone w województwie kujawsko-pomorskim.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "city",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moja babcia urodziła się w małym mieście <w>Kowal</w>.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка родилась в маленьком городе <w>Коваль</w>."
                            }
                        },
                        {
                            "text": "Przejeżdżaliśmy przez <w>Kowal</w> w drodze do Torunia.",
                            "target_lang_translations": {
                                "ru": "Мы проезжали через <w>Коваль</w> по дороге в Торунь."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в Польше, расположенный в Куявско-Поморском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Коваль",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
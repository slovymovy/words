                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "projectile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The medieval archer loaded a <w>bolt</w> into his crossbow.",
                            "target_lang_translations": {
                                "ru": "Средневековый лучник зарядил <w>болт</w> в свой арбалет."
                            }
                        },
                        {
                            "text": "A steel <w>bolt</w> pierced the target with great force.",
                            "target_lang_translations": {
                                "ru": "Стальной <w>болт</w> пронзил мишень с огромной силой."
                            }
                        }
                    ],
                    "synonyms": [
                        "quarrel",
                        "arrow"
                    ],
                    "common_phrases": [
                        "crossbow bolt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая тяжелая стрела, предназначенная для стрельбы из арбалета."

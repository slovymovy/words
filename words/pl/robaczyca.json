                    "sense_id": "59338781-92aa-3f80-a8e1-54e398c0cf89",
                    "sense_definition": "Choroba pasożytnicza wywoływana przez helminty (robaki), dotykająca ludzi lub zwierzęta",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weterynarz przepisał leki, ponieważ u mojego psa zdiagnozowano <w>robaczycę</w>.",
                            "target_lang_translations": {
                                "ru": "Ветеринар прописал лекарства, так как у моей собаки диагностировали <w>гельминтоз</w>."
                            }
                        },
                        {
                            "text": "Nieleczona <w>robaczyca</w> może prowadzić do poważnego osłabienia organizmu i utraty wagi.",
                            "target_lang_translations": {
                                "ru": "Нелеченый <w>гельминтоз</w> может привести к серьезному ослаблению организма и потере веса."
                            }
                        }
                    ],
                    "synonyms": [
                        "helmintoza",
                        "choroba robacza"
                    ],
                    "common_phrases": [
                        "leczyć robaczycę",
                        "objawy robaczycy",
                        "ostra robaczyca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Паразитарное заболевание, вызываемое гельминтами (глистами), поражающее людей или животных"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гельминтоз",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "глистная инвазия",
                                "target_lang_sense_clarification": "описание состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f3ebc14-af3e-3c2d-bd30-10fab8c51e90",
                    "sense_definition": "Larwa minoga żyjąca w mule rzecznym, różniąca się wyglądem od dorosłego osobnika",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_larva",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas badań dna rzeki biolodzy wyłowili rzadką <w>robaczycę</w>.",
                            "target_lang_translations": {
                                "ru": "Во время исследования дна реки биологи выловили редкую <w>пескоройку</w>."
                            }
                        },
                        {
                            "text": "<w>Robaczyca</w> nie posiada oczu i żyje zagrzebana w osadach dennych.",
                            "target_lang_translations": {
                                "ru": "<w>Пескоройка</w> не имеет глаз и живет, зарывшись в донные отложения."
                            }
                        }
                    ],
                    "synonyms": [
                        "ślepica",
                        "węgornica"
                    ],
                    "common_phrases": [
                        "stadium robaczycy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личинка миноги, обитающая в речном иле и отличающаяся внешним видом от взрослой особи"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пескоройка",
                                "target_lang_sense_clarification": "зоологический термин"
                            }
                        ]
                    }
                }
            ]
        }

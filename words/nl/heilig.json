    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b4904a6d-e08e-3c2f-9c91-2a112ea901bb",
                    "sense_definition": "Verbonden met het goddelijke of gewijd aan een godheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De priester zegende het <w>heilige</w> water tijdens de dienst.",
                            "target_lang_translations": {
                                "en": "The priest blessed the <w>holy</w> water during the service."
                            }
                        },
                        {
                            "text": "Voor veel gelovigen is Jeruzalem een <w>heilige</w> stad.",
                            "target_lang_translations": {
                                "en": "For many believers, Jerusalem is a <w>holy</w> city."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewijd",
                        "goddelijk",
                        "sacraal"
                    ],
                    "antonyms": [
                        "onheilig"
                    ],
                    "common_phrases": [
                        "de Heilige Geest",
                        "heilig water",
                        "het Heilige Land",
                        "de Heilige Schrift"
                    ],
                    "target_lang_definitions": {
                        "en": "Connected to the divine or dedicated to a deity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "holy",
                                "target_lang_sense_clarification": "dedicated to a religious purpose or a god"
                            },
                            {
                                "target_lang_word": "sacred",
                                "target_lang_sense_clarification": "consecrated or highly revered"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd06cccb-ff80-3883-b585-2e6be53c6537",
                    "sense_definition": "Van zeer groot belang; wat men absoluut niet wil schenden of opgeven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrijheid van meningsuiting is in dit land een <w>heilig</w> goed.",
                            "target_lang_translations": {
                                "en": "Freedom of speech is a <w>sacred</w> asset in this country."
                            }
                        },
                        {
                            "text": "Hij is er <w>heilig</w> van overtuigd dat hij de waarheid spreekt.",
                            "target_lang_translations": {
                                "en": "He is <w>firmly</w> convinced that he is speaking the truth."
                            }
                        }
                    ],
                    "synonyms": [
                        "onschendbaar",
                        "onaantastbaar"
                    ],
                    "common_phrases": [
                        "heilig overtuigd zijn",
                        "heilig verklaren",
                        "een heilig huisje"
                    ],
                    "target_lang_definitions": {
                        "en": "Of very great importance; something one absolutely does not want to violate or give up."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sacred",
                                "target_lang_sense_clarification": "regarded with great respect and reverence; not to be interfered with"
                            },
                            {
                                "target_lang_word": "inviolable",
                                "target_lang_sense_clarification": "never to be broken, infringed, or dishonored"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a24c4c60-2bf7-37b1-984a-0a099dac3d2a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>heiligen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>heilig</w> de dag door rust te nemen en te reflecteren.",
                            "target_lang_translations": {
                                "en": "I <w>sanctify</w> the day by taking rest and reflecting."
                            }
                        },
                        {
                            "text": "<w>Heilig</w> uw naam door altijd eerlijk en rechtvaardig te leven.",
                            "target_lang_translations": {
                                "en": "<w>Hallow</w> your name by always living honestly and justly."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of <w>heiligen</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sanctify",
                                "target_lang_sense_clarification": "to make holy or set apart as sacred"
                            },
                            {
                                "target_lang_word": "hallow",
                                "target_lang_sense_clarification": "to honor as holy"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
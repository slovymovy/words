            "senses": [
                {
                    "sense_id": "a722777f-989b-3098-a652-8eeab330f62e",
                    "sense_definition": "An event or period during which a shop sells goods at reduced prices.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "commerce_discount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought this jacket for half price during the winter <w>sale</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил эту куртку за полцены на зимней <w>распродаже</w>."
                            }
                        },
                        {
                            "text": "The local store is having a clearance <w>sale</w> to make room for new stock.",
                            "target_lang_translations": {
                                "ru": "В местном магазине проходит полная <w>распродажа</w>, чтобы освободить место для нового товара."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on sale",
                        "clearance sale",
                        "summer sale",
                        "garage sale",
                        "flash sale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие или период времени, когда магазин продает товары по сниженным ценам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распродажа",
                                "target_lang_sense_clarification": "снижение цен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62428e3a-f9dd-35ad-af3f-7d307a499cb0",
                    "sense_definition": "The act of exchanging goods or services for money.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "commerce_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sale</w> of the house was finalized yesterday afternoon.",
                            "target_lang_translations": {
                                "ru": "<w>Продажа</w> дома была окончательно оформлена вчера днем."
                            }
                        },
                        {
                            "text": "He put his old car up <w>for sale</w> on the internet.",
                            "target_lang_translations": {
                                "ru": "Он выставил свою старую машину <w>на продажу</w> в интернете."
                            }
                        }
                    ],
                    "antonyms": [
                        "purchase"
                    ],
                    "common_phrases": [
                        "for sale",
                        "point of sale",
                        "make a sale",
                        "bill of sale",
                        "terms of sale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт обмена товаров или услуг на деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продажа",
                                "target_lang_sense_clarification": "обмен товара на деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d654b955-47d1-3d91-b6cc-b6d81fce5dc6",
                    "sense_definition": "A public event where items are sold to the person who offers the highest price.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4df15dc5-a537-36c0-84e6-06fe249990ff",
                    "sense_definition": "The killing of animals, generally for food.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "killing_animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer prepared the cattle for <w>slaughter</w> before the winter.",
                            "target_lang_translations": {
                                "ru": "Фермер подготовил скот к <w>убою</w> перед зимой."
                            }
                        },
                        {
                            "text": "Strict regulations govern the humane <w>slaughter</w> of livestock.",
                            "target_lang_translations": {
                                "ru": "Строгие правила регулируют гуманный <w>забой</w> скота."
                            }
                        }
                    ],
                    "synonyms": [
                        "mess"
                    ],
                    "common_phrases": [
                        "cattle slaughter",
                        "lambs to the slaughter",
                        "fit for slaughter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умерщвление животных, как правило, для получения пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убой",
                                "target_lang_sense_clarification": "профессиональный или технический термин"
                            },
                            {
                                "target_lang_word": "забой",
                                "target_lang_sense_clarification": "процесс убийства скота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5de1d472-1b62-388e-a837-7940ca4e2807",
                    "sense_definition": "A massacre; the violent killing of a large number of people.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "killing_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The war resulted in the terrible <w>slaughter</w> of thousands of innocent civilians.",
                            "target_lang_translations": {
                                "ru": "Война привела к ужасной <w>резне</w> тысяч невинных мирных жителей."
                            }
                        },
                        {
                            "text": "Historians described the battle as a scene of wholesale <w>slaughter</w>.",
                            "target_lang_translations": {
                                "ru": "Историки описывали эту битву как сцену массовой <w>бойни</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mess"
                    ],
                    "common_phrases": [
                        "wholesale slaughter",
                        "scene of slaughter",
                        "innocent slaughter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Массовое убийство; жестокое умерщвление большого количества людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резня",
                                "target_lang_sense_clarification": "жестокое массовое убийство"
                            },
                            {
                                "target_lang_word": "бойня",
                                "target_lang_sense_clarification": "кровопролитное сражение или убийство"
                            },
                            {
                                "target_lang_word": "массовое убийство",
                                "target_lang_sense_clarification": "нейтральный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8daec13a-25ff-3871-82fd-59b8ee8ec156",
                    "sense_definition": "A rout or decisive defeat, often used in sports or games.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The championship game ended in a total <w>slaughter</w> for the visiting team.",
                            "target_lang_translations": {
                                "ru": "Финальная игра чемпионата закончилась полным <w>разгромом</w> для команды гостей."
                            }
                        },
                        {
                            "text": "It wasn't just a loss; it was an absolute <w>slaughter</w> on the field.",
                            "target_lang_translations": {
                                "ru": "Это был не просто проигрыш, это был абсолютный <w>разгром</w> на поле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a heavy defeat"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разгром или решительное поражение, часто используется в спорте или играх."

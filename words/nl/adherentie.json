            "senses": [
                {
                    "sense_id": "8d7f595c-7724-353c-9a0a-bc3c83a9fea1",
                    "sense_definition": "De mate waarin een patiënt de adviezen en voorschriften van een behandelaar opvolgt, zoals het innemen van medicijnen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_compliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een goede <w>adherentie</w> is essentieel voor het slagen van deze langdurige therapie.",
                            "target_lang_translations": {
                                "en": "Good <w>adherence</w> is essential for the success of this long-term therapy.",
                                "ru": "Хорошая <w>приверженность лечению</w> необходима для успеха этой длительной терапии."
                            }
                        },
                        {
                            "text": "De arts probeert de <w>adherentie</w> te verbeteren door duidelijke uitleg te geven.",
                            "target_lang_translations": {
                                "en": "The doctor tries to improve <w>compliance</w> by giving clear explanations.",
                                "ru": "Врач пытается улучшить <w>комплаентность</w>, давая четкие объяснения."
                            }
                        }
                    ],
                    "synonyms": [
                        "therapietrouw",
                        "compliantie"
                    ],
                    "antonyms": [
                        "ontrouw"
                    ],
                    "common_phrases": [
                        "adherentie verbeteren",
                        "slechte adherentie"
                    ],
                    "target_lang_definitions": {
                        "en": "The extent to which a patient follows a practitioner's advice and prescriptions, such as taking medication.",
                        "ru": "Степень, в которой пациент соблюдает рекомендации и назначения врача, например, прием лекарств."
                    },
                    "translations": {

{
    "word_family": [
        "verdedigen",
        "verdediger",
        "verdedigbaar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9875c066-42b9-35ae-a10b-5348531ba3d0",
                    "sense_definition": "De handeling van het beschermen van iemand of iets tegen een aanval, kritiek of gevaar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aanval is soms de beste <w>verdediging</w>.",
                            "target_lang_translations": {
                                "ru": "Нападение иногда — лучшая <w>защита</w>."
                            }
                        },
                        {
                            "text": "Hij schoot direct in de <w>verdediging</w> toen hij kritiek kreeg.",
                            "target_lang_translations": {
                                "ru": "Он сразу ушел в <w>оборону</w>, когда его раскритиковали."
                            }
                        }
                    ],
                    "synonyms": [
                        "defensie"
                    ],
                    "antonyms": [
                        "agressie",
                        "agressie"
                    ],
                    "common_phrases": [
                        "in de verdediging",
                        "de beste verdediging",
                        "uit zelfverdediging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по защите кого-либо или чего-либо от нападения, критики или опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "оборона",
                                "target_lang_sense_clarification": "активное сопротивление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1fc4a5f-3cae-38b9-927f-a05086d4e26e",
                    "sense_definition": "De groep spelers of soldaten die de taak heeft om de tegenstander tegen te houden en het eigen doel of gebied te beschermen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_defenders",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verdediging</w> van deze ploeg is niet sterk genoeg voor de finale.",
                            "target_lang_translations": {
                                "ru": "<w>Защита</w> этой команды недостаточно сильна для финала."
                            }
                        },
                        {
                            "text": "Onze <w>verdediging</w> hield stand tegen de vijandelijke troepen.",
                            "target_lang_translations": {
                                "ru": "Наша <w>оборона</w> выстояла против вражеских войск."
                            }
                        }
                    ],
                    "synonyms": [
                        "defensie",
                        "defensie"
                    ],
                    "antonyms": [
                        "agressie"
                    ],
                    "common_phrases": [
                        "een sterke verdediging",
                        "de verdediging organiseren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа игроков или солдат, чья задача — сдерживать противника и защищать свои ворота или территорию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборона",
                                "target_lang_sense_clarification": "военная или спортивная линия"
                            },
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "спортивная команда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc3d0c87-e8a7-37e5-a327-671022707bb5",
                    "sense_definition": "De advocaat of groep advocaten die een verdachte bijstaat in een rechtszaak.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verdediging</w> vroeg de rechter om vrijspraak voor de verdachte.",
                            "target_lang_translations": {
                                "ru": "<w>Защита</w> попросила судью об оправдании обвиняемого."
                            }
                        },
                        {
                            "text": "Na de pauze was het woord aan de <w>verdediging</w>.",
                            "target_lang_translations": {
                                "ru": "После перерыва слово было предоставлено <w>защите</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het woord is aan de verdediging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адвокат или группа адвокатов, представляющих обвиняемого в суде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7f2fee8-cead-327a-8ee9-c8d58f1914e6",
                    "sense_definition": "Een betoog, verklaring of geschrift bedoeld om iemand vrij te pleiten of een standpunt te rechtvaardigen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_argument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij voerde dit argument aan ter <w>verdediging</w> van zijn daden.",
                            "target_lang_translations": {
                                "ru": "Он привел этот аргумент в <w>защиту</w> своих действий."
                            }
                        },
                        {
                            "text": "Haar <w>verdediging</w> was niet overtuigend genoeg voor de commissie.",
                            "target_lang_translations": {
                                "ru": "Ее <w>защита</w> была недостаточно убедительной для комиссии."
                            }
                        }
                    ],
                    "synonyms": [
                        "defensie"
                    ],
                    "common_phrases": [
                        "ter verdediging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Речь, заявление или текст, предназначенные для оправдания кого-либо или обоснования точки зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "аргументация"
                            },
                            {
                                "target_lang_word": "оправдание",
                                "target_lang_sense_clarification": "снятие вины"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
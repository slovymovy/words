{
    "word_family": [
        "endure",
        "endurance",
        "endurable",
        "unendurable"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2947649c-54ea-3d64-8d3f-bf5d042d10bc",
                    "sense_definition": "Lasting for a long time without significant alteration; continuing through time in the same relative state.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author left an <w>enduring</w> legacy that continues to influence writers today.",
                            "target_lang_translations": {
                                "ru": "Автор оставил <w>непреходящее</w> наследие, которое продолжает влиять на писателей и сегодня."
                            }
                        },
                        {
                            "text": "They have an <w>enduring</w> friendship that has lasted for over thirty years.",
                            "target_lang_translations": {
                                "ru": "Их связывает <w>прочная</w> дружба, которая длится уже более тридцати лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "lasting",
                        "prolonged"
                    ],
                    "antonyms": [
                        "temporary",
                        "fleeting",
                        "short-lived"
                    ],
                    "common_phrases": [
                        "enduring legacy",
                        "enduring appeal",
                        "enduring love",
                        "enduring symbol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительный, сохраняющийся в течение долгого времени без значительных изменений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непреходящий",
                                "target_lang_sense_clarification": "вечный, не теряющий значения со временем (о наследии, ценностях)"
                            },
                            {
                                "target_lang_word": "прочный",
                                "target_lang_sense_clarification": "крепкий, надежный (об отношениях, дружбе)"
                            },
                            {
                                "target_lang_word": "долговечный",
                                "target_lang_sense_clarification": "рассчитанный на долгий срок службы или существования"
                            },
                            {
                                "target_lang_word": "стойкий",
                                "target_lang_sense_clarification": "устойчивый, способный сохраняться несмотря на трудности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "189eb7c9-aa90-33ec-9d37-e7650c7ed3d3",
                    "sense_definition": "Present participle and gerund of <w>endure</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>enduring</w> a lot of pain while recovering from the surgery.",
                            "target_lang_translations": {
                                "ru": "Она <w>терпит</w> сильную боль, восстанавливаясь после операции."
                            }
                        },
                        {
                            "text": "The soldiers were <w>enduring</w> harsh conditions during the winter campaign.",
                            "target_lang_translations": {
                                "ru": "Солдаты <w>переносили</w> суровые условия во время зимней кампании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "enduring hardship",
                        "enduring pain"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола endure (терпеть, выносить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпящий",
                                "target_lang_sense_clarification": "испытывающий боль или неудобства"
                            },
                            {
                                "target_lang_word": "переносящий",
                                "target_lang_sense_clarification": "выдерживающий трудности или испытания"
                            },
                            {
                                "target_lang_word": "выносящий",
                                "target_lang_sense_clarification": "справляющийся с тяжелыми условиями"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f0331853-f564-3f65-b820-0026648021d5",
                    "sense_definition": "The act or state of suffering or lasting; endurance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>endurings</w> of the explorers were recorded in their daily journals.",
                            "target_lang_translations": {
                                "ru": "<w>Испытания</w> исследователей были записаны в их ежедневных журналах."
                            }
                        },
                        {
                            "text": "After many <w>endurings</w> of misfortune, he finally found peace.",
                            "target_lang_translations": {
                                "ru": "После многих <w>страданий</w> от несчастий он наконец обрел покой."
                            }
                        }
                    ],
                    "synonyms": [
                        "diuturnal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс перенесения страданий или сами страдания и испытания (устаревшее или редкое использование)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытание",
                                "target_lang_sense_clarification": "тяжелое событие, трудность"
                            },
                            {
                                "target_lang_word": "страдание",
                                "target_lang_sense_clarification": "физическая или душевная боль"
                            },
                            {
                                "target_lang_word": "перенесение",
                                "target_lang_sense_clarification": "процесс проживания трудностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
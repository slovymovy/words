{
    "word_family": [
        "książę",
        "księstwo",
        "książęcy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ea7c995-b38b-3709-aaaf-e7e9848720ba",
                    "sense_definition": "Córka księcia lub księżnej; tytuł arystokratyczny przysługujący żeńskim potomkom władców książęcych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "royalty_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda <w>księżniczka</w> uczyła się dworskiej etykiety od najmłodszych lat.",
                            "target_lang_translations": {
                                "ru": "Юная <w>принцесса</w> изучала придворный этикет с самых ранних лет."
                            }
                        },
                        {
                            "text": "W tym zamku mieszkała kiedyś prawdziwa <w>księżniczka</w>.",
                            "target_lang_translations": {
                                "ru": "В этом замке когда-то жила настоящая <w>принцесса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mała księżniczka",
                        "tytuł księżniczki",
                        "piękna księżniczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дочь князя или герцога; аристократический титул, принадлежащий потомкам женского пола княжеских или герцогских родов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принцесса",
                                "target_lang_sense_clarification": "общий титул"
                            },
                            {
                                "target_lang_word": "княжна",
                                "target_lang_sense_clarification": "дочь князя (в славянском контексте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "298ec46e-8629-35ae-bc00-10ede2731f60",
                    "sense_definition": "Córka króla lub królowej; w języku potocznym często używane zamiennie ze słowem królewna.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "royalty_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bajkowa <w>księżniczka</w> czekała na ratunek w wysokiej wieży.",
                            "target_lang_translations": {
                                "ru": "Сказочная <w>принцесса</w> ждала спасения в высокой башне."
                            }
                        },
                        {
                            "text": "Każda dziewczynka chciała wyglądać na balu jak <w>księżniczka</w>.",
                            "target_lang_translations": {
                                "ru": "Каждая девочка хотела выглядеть на балу как <w>принцесса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "królewna"
                    ],
                    "common_phrases": [
                        "księżniczka na ziarnku grochu",
                        "suknia księżniczki",
                        "korona księżniczki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дочь короля или королевы; в разговорной речи часто используется как синоним слова «королевна» (принцесса)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принцесса",
                                "target_lang_sense_clarification": "дочь монарха, сказочный персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e86eedf-1bfd-37e0-822d-73dc9b357cfc",
                    "sense_definition": "Gatunek średniej wielkości ptaka z rodziny papug, znany również jako księżniczka walijska.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wolierze siedziała kolorowa <w>księżniczka</w> i skubała ziarno.",
                            "target_lang_translations": {
                                "ru": "В вольере сидела яркая <w>принцесса Уэльская</w> и клевала зерно."
                            }
                        },
                        {
                            "text": "Hodowla papug wzbogaciła się o rzadką odmianę <w>księżniczki</w>.",
                            "target_lang_translations": {
                                "ru": "Питомник попугаев пополнился редкой разновидностью <w>принцессы Уэльской</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "księżniczka walijska",
                        "papuga księżniczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид птицы среднего размера из семейства попугаевых, также известный как попугай Александры или принцесса Уэльская."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попугай принцессы Уэльской",
                                "target_lang_sense_clarification": "вид попугая"
                            },
                            {
                                "target_lang_word": "роскошный попугай Александры",
                                "target_lang_sense_clarification": "научное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce6aaf0-af5e-3081-84c7-50df66058f87",
                    "sense_definition": "Gatunek słodkowodnej ryby akwariowej z rodziny pielęgnicowatych, znany jako księżniczka z Burundi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <w>księżniczka</w> złożyła ikrę w szczelinie skalnej.",
                            "target_lang_translations": {
                                "ru": "Моя <w>принцесса Бурунди</w> отложила икру в расщелине скалы."
                            }
                        },
                        {
                            "text": "W tym akwarium pływają tylko <w>księżniczki</w>, ponieważ są to ryby terytorialne.",
                            "target_lang_translations": {
                                "ru": "В этом аквариуме плавают только <w>принцессы Бурунди</w>, так как это территориальные рыбы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "księżniczka z Burundi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид пресноводной аквариумной рыбы из семейства цихловых, известный как принцесса Бурунди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принцесса Бурунди",
                                "target_lang_sense_clarification": "вид рыбы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        {
                            "text": "The application was rejected in the first <w>instance</w>, but they appealed the decision.",
                            "target_lang_translations": {
                                "ru": "Заявление было отклонено в первой <w>инстанции</w>, но они обжаловали это решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "stage",
                        "step"
                    ],
                    "common_phrases": [
                        "in the first instance"

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e426d99b-94ff-3183-b834-6134a8f2ef50",
                    "sense_definition": "W sposób zgodny z rzeczywistością, faktycznie istniejący, a nie wyimaginowany.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To zagrożenie istnieje <w>realnie</w> i nie możemy go ignorować.",
                            "target_lang_translations": {
                                "ru": "Эта угроза существует <w>реально</w>, и мы не можем её игнорировать."
                            }
                        },
                        {
                            "text": "Czy te postacie historyczne żyły <w>realnie</w>, czy są tylko legendą?",
                            "target_lang_translations": {
                                "ru": "Жили ли эти исторические личности <w>на самом деле</w>, или они всего лишь легенда?"
                            }
                        }
                    ],
                    "synonyms": [
                        "rzeczywiście",
                        "prawdziwie"
                    ],
                    "common_phrases": [
                        "istnieć realnie",
                        "dziać się realnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В соответствии с действительностью, фактически существуя, а не будучи воображаемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реально",
                                "target_lang_sense_clarification": "фактически, на самом деле"
                            },
                            {
                                "target_lang_word": "в действительности",
                                "target_lang_sense_clarification": "на самом деле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5b14988-05cc-3724-b424-5db5728c83f5",
                    "sense_definition": "W sposób możliwy do wykonania, osiągnięcia lub urzeczywistnienia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "feasibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <w>realnie</w> ocenić nasze szanse na wygranie tego przetargu.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>реально</w> оценить наши шансы на победу в этом тендере."
                            }
                        },
                        {
                            "text": "Ten plan jest ambitny, ale czy jest <w>realnie</w> wykonalny w tydzień?",
                            "target_lang_translations": {
                                "ru": "Этот план амбициозен, но <w>реально</w> ли он выполним за неделю?"
                            }
                        }
                    ],
                    "synonyms": [
                        "możliwie"
                    ],
                    "common_phrases": [
                        "oceniać realnie",
                        "realnie patrząc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способом, возможным для выполнения, достижения или осуществления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реально",
                                "target_lang_sense_clarification": "выполнимо, осуществимо"
                            },
                            {
                                "target_lang_word": "объективно",
                                "target_lang_sense_clarification": "при оценке возможностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cca4d7f-79de-30a3-8baa-8372871bd8ad",
                    "sense_definition": "W sposób trzeźwy i rozsądny, kierując się zdrowym rozsądkiem bez ulegania emocjom.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powinniśmy podejść do tego problemu <w>realnie</w> i bez zbędnych nerwów.",
                            "target_lang_translations": {
                                "ru": "Мы должны подойти к этой проблеме <w>реалистично</w> и без лишних нервов."
                            }
                        },
                        {
                            "text": "On zawsze myśli <w>realnie</w> i nie buja w obłokach.",
                            "target_lang_translations": {
                                "ru": "Он всегда мыслит <w>реалистично</w> и не витает в облаках."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdroworozsądkowo"
                    ],
                    "common_phrases": [
                        "myśleć realnie",
                        "podchodzić realnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трезво и разумно, руководствуясь здравым смыслом, не поддаваясь эмоциям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реалистично",
                                "target_lang_sense_clarification": "трезво оценивая ситуацию"
                            },
                            {
                                "target_lang_word": "трезво",
                                "target_lang_sense_clarification": "рассудительно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

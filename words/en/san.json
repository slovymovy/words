                            {
                                "target_lang_word": "язык сан",
                                "target_lang_sense_clarification": "лингвистика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e22d459e-f36e-3a42-9541-f4836dd2c31e",
                    "sense_definition": "A river in southeastern Poland and western Ukraine.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The <w>San</w> forms a natural border between the two countries for several kilometers.",
                            "target_lang_translations": {
                                "ru": "<w>Сан</w> образует естественную границу между двумя странами на протяжении нескольких километров."
                            }
                        },
                        {
                            "text": "We went fishing on the banks of the <w>San</w> last summer.",
                            "target_lang_translations": {

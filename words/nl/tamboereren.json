{
    "word_family": [
        "tamboer",
        "tamboerijn"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5a041820-441a-3dfc-bca5-7df2951c3015",
                    "sense_definition": "Ergens voortdurend de nadruk op leggen of ergens op blijven aandringen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politici blijven maar <w>tamboereren</w> op het belang van economische groei.",
                            "target_lang_translations": {
                                "ru": "Политики продолжают <w>твердить</w> о важности экономического роста.",
                                "en": "The politicians keep <w>harping on</w> about the importance of economic growth."
                            }
                        },
                        {
                            "text": "Hij bleef tijdens de vergadering <w>tamboereren</w> op dat ene kleine detail.",
                            "target_lang_translations": {
                                "ru": "Во время собрания он продолжал <w>педалировать</w> эту одну мелкую деталь.",
                                "en": "During the meeting, he kept <w>hammering away</w> at that one small detail."
                            }
                        }
                    ],
                    "synonyms": [
                        "aandringen",
                        "benadrukken",
                        "onderstrepen"
                    ],
                    "common_phrases": [
                        "tamboereren op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно акцентировать внимание на чем-либо или настойчиво повторять одно и то же.",
                        "en": "To constantly emphasize something or persist in insisting on it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твердить",
                                "target_lang_sense_clarification": "постоянно повторять"
                            },
                            {
                                "target_lang_word": "педалировать",
                                "target_lang_sense_clarification": "усиленно подчеркивать"
                            },
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "упорно утверждать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "harp on",
                                "target_lang_sense_clarification": "to talk about something constantly and annoyingly"
                            },
                            {
                                "target_lang_word": "hammer home",
                                "target_lang_sense_clarification": "to emphasize a point strongly"
                            },
                            {
                                "target_lang_word": "keep banging on",
                                "target_lang_sense_clarification": "informal: to talk repeatedly about something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f864f0d8-dbea-3456-ae99-41ffa395d753",
                    "sense_definition": "Luidruchtig uiting geven aan vreugde of triomf.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rejoice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten niet te vroeg <w>tamboereren</w> over de goede resultaten.",
                            "target_lang_translations": {
                                "ru": "Нам не следует слишком рано <w>трубить</w> о хороших результатах.",
                                "en": "We shouldn't <w>trumpet</w> the good results too soon."
                            }
                        },
                        {
                            "text": "Na de overwinning liepen de supporters luidkeels te <w>tamboereren</w>.",
                            "target_lang_translations": {
                                "ru": "После победы болельщики ходили и громко <w>ликовали</w>.",
                                "en": "After the victory, the supporters went around loudly <w>crowing</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "juichen",
                        "jubelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шумно выражать радость или торжество; громогласно объявлять о чем-либо.",
                        "en": "To noisily express joy or triumph; to loudly proclaim something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубить",
                                "target_lang_sense_clarification": "разглашать повсюду"
                            },
                            {
                                "target_lang_word": "ликовать",
                                "target_lang_sense_clarification": "бурно радоваться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "trumpet",
                                "target_lang_sense_clarification": "to proclaim or announce loudly"
                            },
                            {
                                "target_lang_word": "crow",
                                "target_lang_sense_clarification": "to express triumph or boast"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "26d503a1-b7e7-397a-8b74-fa4ff5786556",
                    "sense_definition": "Door het genoemde heen; van de ene zijde naar de andere.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zat een gat in de muur en ik kon <w>erdoor</w> kijken.",
                            "target_lang_translations": {
                                "ru": "В стене была дыра, и я мог смотреть <w>сквозь неё</w>."
                            }
                        },
                        {
                            "text": "De deur was gesloten, dus we konden niet <w>erdoor</w>.",
                            "target_lang_translations": {
                                "ru": "Дверь была закрыта, поэтому мы не могли пройти <w>через неё</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "erdoorheen"
                    ],
                    "common_phrases": [
                        "erdoor kijken",
                        "erdoor vallen",
                        "erdoor kunnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сквозь или через упомянутый предмет; с одной стороны на другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сквозь это",
                                "target_lang_sense_clarification": "проникновение взгляда, света или движение через препятствие"
                            },
                            {
                                "target_lang_word": "через это",
                                "target_lang_sense_clarification": "перемещение с одной стороны на другую"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70673999-a825-345c-9387-a218532ffaf5",
                    "sense_definition": "Tussen of door het genoemde gemengd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mixture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klop de eieren en roer de suiker <w>erdoor</w>.",
                            "target_lang_translations": {
                                "ru": "Взбейте яйца и вмешайте <w>туда</w> сахар."
                            }
                        },
                        {
                            "text": "De saus is bijna klaar, je moet alleen nog wat kruiden <w>erdoor</w> doen.",
                            "target_lang_translations": {
                                "ru": "Соус почти готов, тебе нужно только добавить <w>туда</w> немного приправ."
                            }
                        }
                    ],
                    "synonyms": [
                        "erdoorheen"
                    ],
                    "common_phrases": [
                        "erdoor roeren",
                        "erdoor mengen",
                        "erdoor doen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вперемешку с чем-либо; добавлено внутрь упомянутого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туда",
                                "target_lang_sense_clarification": "внутрь смеси или состава"
                            },
                            {
                                "target_lang_word": "в это",
                                "target_lang_sense_clarification": "добавление внутрь чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df87affd-26c5-3d02-a421-558a84a525fc",
                    "sense_definition": "Vanwege het genoemde; met het genoemde als oorzaak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuws was erg slecht en iedereen was <w>erdoor</w> van slag.",
                            "target_lang_translations": {
                                "ru": "Новости были очень плохими, и все были <w>из-за этого</w> расстроены."
                            }
                        },
                        {
                            "text": "De regen was hevig en we zijn <w>erdoor</w> helemaal nat geworden.",
                            "target_lang_translations": {
                                "ru": "Дождь был сильным, и мы <w>из-за него</w> промокли до нитки."
                            }
                        }
                    ],
                    "synonyms": [
                        "daardoor",
                        "hierdoor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Из-за упомянутого; по причине этого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из-за этого",
                                "target_lang_sense_clarification": "указание на причину"
                            },
                            {
                                "target_lang_word": "этим",
                                "target_lang_sense_clarification": "творительный падеж, причина или средство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

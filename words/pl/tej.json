    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "e365e2c4-6c5c-343f-b569-4c2c18b25243",
                    "sense_definition": "Forma odmienna zaimka wskazującego <w>ta</w> w dopełniaczu, celowniku i miejscowniku liczby pojedynczej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie znam <tej> kobiety, która stoi przy wejściu.",
                            "target_lang_translations": {
                                "ru": "Я не знаю <этой> женщины, которая стоит у входа."
                            }
                        },
                        {
                            "text": "Dzięki <tej> książce nauczyłem się wiele o historii Polski.",
                            "target_lang_translations": {
                                "ru": "Благодаря <этой> книге я многое узнал об истории Польши."
                            }
                        },
                        {
                            "text": "Mieszkam w <tej> okolicy od ponad dziesięciu lat.",
                            "target_lang_translations": {
                                "ru": "Я живу в <этой> местности уже более десяти лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "o tej porze",
                        "w tej chwili",
                        "z tej strony",
                        "do tej pory",
                        "po tej stronie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Падежная форма указательного местоимения \"ta\" (эта) в родительном, дательном и предложном падежах единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этой",
                                "target_lang_sense_clarification": "форма местоимения \"эта\""
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c346539c-21c2-3f05-b832-62dd1b872651",
                    "sense_definition": "Regionalna forma zwrotu do drugiej osoby (kolegi, znajomego), charakterystyczna dla gwary poznańskiej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słuchaj <tej>, musimy już wracać do domu.",
                            "target_lang_translations": {
                                "ru": "Послушай, <слышь>, нам уже пора возвращаться домой."
                            }
                        },
                        {
                            "text": "Chodź no tutaj szybko, <tej>!",
                            "target_lang_translations": {
                                "ru": "А ну иди сюда быстро, <слышь>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) harbata, tyj"
                    ],
                    "common_phrases": [
                        "no tej"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Poznań / Wielkopolska"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональная форма обращения к собеседнику (другу, знакомому), характерная для познанского диалекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слышь",
                                "target_lang_sense_clarification": "разговорное обращение"
                            },
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "de58ac63-c759-3220-89af-a394d3a32635",
                    "sense_definition": "An informal word used to express agreement or affirmation; yes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are you coming to the party tonight? <w>Yep</w>, I will be there.",
                            "target_lang_translations": {
                                "ru": "Ты идешь сегодня на вечеринку? — <w>Ага</w>, я буду."
                            }
                        },
                        {
                            "text": "<w>Yep</w>, that is exactly what I meant to say.",
                            "target_lang_translations": {
                                "ru": "<w>Ага</w>, именно это я и хотел сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "See Thesaurus:yay",
                        "See Thesaurus:wow"
                    ],
                    "antonyms": [
                        "nope"
                    ],
                    "common_phrases": [
                        "yep sure",
                        "yep indeed"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное слово, используемое для выражения согласия или подтверждения; да."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "разговорное выражение согласия"
                            },
                            {
                                "target_lang_word": "угу",
                                "target_lang_sense_clarification": "звук, выражающий подтверждение или согласие"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "стандартная форма согласия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26165277-3680-36c2-8232-1dd21e079982",
                    "sense_definition": "An affirmative reply or statement; an instance of saying yes.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "affirmation_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will take your silence as a <w>yep</w>.",
                            "target_lang_translations": {
                                "ru": "Я приму твое молчание за <w>согласие</w>."
                            }
                        },
                        {
                            "text": "He gave a quick <w>yep</w> and hung up the phone.",
                            "target_lang_translations": {
                                "ru": "Он быстро бросил <w>ага</w> и повесил трубку."
                            }
                        }
                    ],
                    "antonyms": [
                        "nope"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Утвердительный ответ или заявление; случай произнесения слова «да»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "утвердительный ответ"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "в значении существительного"
                            }
                        ]
                    }
                }
            ]
        },

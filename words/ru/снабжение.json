    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7fa23782-bbd6-3d9b-9afd-e0afe6e13053",
                    "sense_definition": "Действие по глаголу <w>снабжать</w>; обеспечение кого-либо или чего-либо необходимыми материалами, средствами или ресурсами.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бесперебойное <w>снабжение</w> электричеством жизненно важно для работы больницы.",
                            "target_lang_translations": {
                                "en": "Uninterrupted electricity <w>supply</w> is vital for the hospital's operation."
                            }
                        },
                        {
                            "text": "Снабжение города питьевой водой было временно приостановлено из-за аварии.",
                            "target_lang_translations": {
                                "en": "The <w>supply</w> of drinking water to the city was temporarily suspended due to an accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "обеспечение",
                        "поставка"
                    ],
                    "common_phrases": [
                        "снабжение водой",
                        "снабжение продовольствием",
                        "снабжение электроэнергией",
                        "централизованное снабжение"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of supplying; providing someone or something with necessary materials, funds, or resources."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "supply",
                                "target_lang_sense_clarification": "act of providing"
                            },
                            {
                                "target_lang_word": "provision",
                                "target_lang_sense_clarification": "formal act of providing"
                            },
                            {
                                "target_lang_word": "supplying",
                                "target_lang_sense_clarification": "process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "707ab948-d332-3531-b78a-f5022029c293",
                    "sense_definition": "Система мероприятий или служба, занимающаяся организацией доставки и распределения ресурсов для удовлетворения потребностей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат уже пять лет возглавляет отдел снабжения на крупном заводе.",
                            "target_lang_translations": {
                                "en": "My brother has been heading the <w>supply</w> department at a large factory for five years."
                            }
                        },
                        {
                            "text": "Плохое <w>снабжение</w> стало главной причиной задержки строительства.",
                            "target_lang_translations": {
                                "en": "Poor <w>supply</w> became the main reason for the construction delay."
                            }
                        }
                    ],
                    "synonyms": [
                        "обеспечение",
                        "логистика"
                    ],
                    "common_phrases": [
                        "отдел снабжения",
                        "служба снабжения",
                        "договор снабжения",
                        "план снабжения"
                    ],
                    "target_lang_definitions": {
                        "en": "A system of measures or a service dedicated to organizing the delivery and distribution of resources."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "supply",
                                "target_lang_sense_clarification": "system or department"
                            },
                            {
                                "target_lang_word": "procurement",
                                "target_lang_sense_clarification": "business function"
                            },
                            {
                                "target_lang_word": "logistics",
                                "target_lang_sense_clarification": "organization of delivery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "113645c5-fd43-3e34-bf3d-4aa8c16ace4d",
                    "sense_definition": "Совокупность предметов, вещей или продуктов, которыми обеспечивается кто-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Экспедиция потеряла всё своё <w>снабжение</w> во время сильного шторма.",
                            "target_lang_translations": {
                                "en": "The expedition lost all its <w>supplies</w> during the severe storm."
                            }
                        },
                        {
                            "text": "Солдаты получили полное вещевое <w>снабжение</w> перед отправкой на учения.",
                            "target_lang_translations": {
                                "en": "The soldiers received full clothing <w>supplies</w> before being sent to exercises."
                            }

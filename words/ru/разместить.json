                {
                    "sense_id": "7c04d406-bd27-3fbf-a1a4-6ecd7542c27e",
                    "sense_definition": "Поместить, расположить что-либо в определённом месте или порядке (предметы, информацию, рекламу).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "placement_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <разместить> новый диван в гостиной у окна.",
                            "target_lang_translations": {
                                "en": "We decided to <place> the new sofa in the living room by the window."
                            }
                        },
                        {
                            "text": "Вам нужно <разместить> объявление о продаже на популярном сайте.",
                            "target_lang_translations": {
                                "en": "You need to <post> a sale ad on a popular website."
                            }
                        }
                    ],
                    "synonyms": [
                        "расположить",
                        "поместить",
                        "установить",
                        "расставить"
                    ],
                    "antonyms": [
                        "убрать",
                        "удалить",
                        "снять"
                    ],
                    "common_phrases": [
                        "разместить информацию",
                        "разместить объявление",
                        "разместить на сайте",
                        "разместить мебель",
                        "разместить статью"
                    ],
                    "target_lang_definitions": {
                        "en": "To put or arrange something in a specific place or order (objects, information, advertising)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "general meaning for objects"
                            },
                            {
                                "target_lang_word": "post",
                                "target_lang_sense_clarification": "for information or ads on the internet"
                            },
                            {
                                "target_lang_word": "arrange",
                                "target_lang_sense_clarification": "to set out in a specific order"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cf7788b-40cf-3679-964e-e8e97045e359",
                    "sense_definition": "Предоставить кому-либо место для проживания, ночлега или временного пребывания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "accommodation",
                    "name_type": "no",
                    "examples": [

                {
                    "sense_id": "c75633b6-7d49-39d9-a47b-3d89564c7e61",
                    "sense_definition": "Occurring repeatedly at a precise time or at regular time intervals; always on schedule.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He arrived every day with <w>clocklike</w> regularity.",
                            "target_lang_translations": {
                                "ru": "Он приходил каждый день с регулярностью <w>часового механизма</w>."
                            }
                        },
                        {
                            "text": "The team executed the plan with <w>clocklike</w> precision.",
                            "target_lang_translations": {
                                "ru": "Команда выполнила план с <w>часовой</w> точностью."
                            }
                        }
                    ],
                    "synonyms": [

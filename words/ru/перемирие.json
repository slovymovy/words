    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e5db918-0cdc-3ec6-b452-d2a306b33965",
                    "sense_definition": "Временное прекращение военных действий по соглашению между воюющими сторонами.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генералы встретились на границе, чтобы обсудить условия и заключить <w>перемирие</w>.",
                            "target_lang_translations": {
                                "en": "The generals met at the border to discuss terms and conclude a <w>truce</w>."
                            }
                        },
                        {
                            "text": "К сожалению, враг нарушил <w>перемирие</w> и снова начал обстрел города.",
                            "target_lang_translations": {
                                "en": "Unfortunately, the enemy violated the <w>truce</w> and began shelling the city again."
                            }
                        }
                    ],
                    "synonyms": [
                        "прекращение огня"
                    ],
                    "antonyms": [
                        "война",
                        "боевые действия"
                    ],
                    "common_phrases": [
                        "заключить перемирие",
                        "нарушить перемирие",
                        "долгожданное перемирие"
                    ],
                    "target_lang_definitions": {
                        "en": "A temporary cessation of military actions by agreement between warring parties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "truce",
                                "target_lang_sense_clarification": "general term for a suspension of fighting"
                            },
                            {
                                "target_lang_word": "ceasefire",
                                "target_lang_sense_clarification": "often refers specifically to the suspension of active shooting"
                            },
                            {
                                "target_lang_word": "armistice",
                                "target_lang_sense_clarification": "formal agreement, often a prelude to a peace treaty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9676859b-b9e8-359e-9552-fc661ad8250a",
                    "sense_definition": "Временное прекращение вражды, ссоры или несогласия в бытовом общении.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После бурного спора между соседями наступило хрупкое <w>перемирие</w>.",
                            "target_lang_translations": {
                                "en": "After a heated argument, a fragile <w>truce</w> settled between the neighbors."
                            }
                        },
                        {
                            "text": "Ради спокойствия детей родители решили объявить <w>перемирие</w> на выходные.",
                            "target_lang_translations": {
                                "en": "For the sake of the children's peace, the parents decided to call a <w>truce</w> for the weekend."
                            }
                        }
                    ],
                    "synonyms": [
                        "затишье"
                    ],
                    "antonyms": [
                        "ссора",
                        "конфликт"
                    ],
                    "common_phrases": [
                        "хрупкое перемирие",
                        "временное перемирие"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A temporary end to hostility, arguments, or disagreement in everyday interactions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "truce",
                                "target_lang_sense_clarification": "metaphorical use for personal conflicts"
                            }
                        ]
                    }
                }
            ]
        }
    ]

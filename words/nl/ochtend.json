                {
                    "sense_id": "39abf27a-edae-36f1-9f20-d0350b9c81f3",
                    "sense_definition": "Het deel van de dag tussen de nacht en de middag, meestal van 6.00 tot 12.00 uur.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elke <w>ochtend</w> drink ik een kopje koffie bij het ontbijt.",
                            "target_lang_translations": {
                                "ru": "Каждое <w>утро</w> я выпиваю чашку кофе за завтраком.",
                                "en": "Every <w>morning</w> I drink a cup of coffee at breakfast."
                            }
                        },
                        {
                            "text": "De volgende <w>ochtend</w> was het erg stil op straat.",
                            "target_lang_translations": {
                                "ru": "На следующее <w>утро</w> на улице было очень тихо.",
                                "en": "The next <w>morning</w> it was very quiet on the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "morgen"
                    ],
                    "antonyms": [
                        "avond"
                    ],
                    "common_phrases": [
                        "in de ochtend",
                        "vroege ochtend",
                        "elke ochtend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть дня между ночью и полуднем, обычно с 6:00 до 12:00.",
                        "en": "The part of the day between night and afternoon, usually from 6:00 to 12:00."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утро",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "morning",
                                "target_lang_sense_clarification": "time of day"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65af69fa-778a-3260-a554-f7f62274f565",
                    "sense_definition": "De tijd rond of kort na het opstaan.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben in de <w>ochtend</w> nooit op mijn best.",
                            "target_lang_translations": {
                                "ru": "По <w>утрам</w> я никогда не бываю в лучшей форме.",
                                "en": "I am never at my best in the <w>morning</w>."
                            }
                        },
                        {
                            "text": "Na een lange nacht had hij een zware <w>ochtend</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой ночи у него было тяжелое <w>утро</w>.",
                                "en": "After a long night, he had a rough <w>morning</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Время сразу после пробуждения.",
                        "en": "The time around or shortly after waking up."
                    },
                    "translations": {
                        "ru": [

                },
                {
                    "sense_id": "23534d5f-23fc-3848-a0c0-06aaa85d591a",
                    "sense_definition": "Used to emphasize that something is nothing more than what is specified; mere.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accident was caused by a <w>simple</w> mistake that anyone could have made.",
                            "target_lang_translations": {
                                "ru": "Авария произошла из-за <w>простой</w> ошибки, которую мог совершить каждый."
                            }
                        },
                        {
                            "text": "It is a <w>simple</w> matter of choosing the right color for the walls.",
                            "target_lang_translations": {
                                "ru": "Это <w>простой</w> вопрос выбора правильного цвета для стен."
                            }
                        }
                    ],
                    "synonyms": [
                        "mere",
                        "pure",
                        "only"
                    ],
                    "common_phrases": [
                        "simple mistake",
                        "simple matter",
                        "simple fact"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания того, что что-то является именно тем, что указано, и ничем иным."

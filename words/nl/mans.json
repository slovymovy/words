    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5a19c606-3d6b-3688-b3fc-ed19a78514d4",
                    "sense_definition": "In staat om iets te presteren; dapper, krachtig of weerbaar.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bewees tijdens de zware tocht dat hij heel wat <w>mans</w> is.",
                            "target_lang_translations": {
                                "en": "He proved during the tough trek that he is quite <w>capable</w>.",
                                "ru": "Во время тяжелого похода он доказал, что он <w>многого стоит</w>."
                            }
                        },
                        {
                            "text": "Zonder de juiste gereedschappen is zelfs de beste vakman niet veel <w>mans</w>.",
                            "target_lang_translations": {
                                "en": "Without the right tools, even the best craftsman isn't <w>worth</w> much.",
                                "ru": "Без правильных инструментов даже лучший мастер <w>мало на что годен</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekwaam",
                        "weerbaar"
                    ],
                    "antonyms": [
                        "onbekwaam"
                    ],
                    "common_phrases": [
                        "heel wat mans",
                        "niet veel mans"
                    ],
                    "target_lang_definitions": {
                        "en": "Capable of performing something; brave, powerful, or resilient.",
                        "ru": "Способный что-то сделать; храбрый, сильный или стойкий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "capable",
                                "target_lang_sense_clarification": "having the ability or strength to do things"
                            },
                            {
                                "target_lang_word": "worth one's salt",
                                "target_lang_sense_clarification": "idiomatic, implying competence or value"
                            },
                            {
                                "target_lang_word": "of substance",
                                "target_lang_sense_clarification": "having strength or character"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стоящий",
                                "target_lang_sense_clarification": "обладающий ценностью или способностями"
                            },
                            {
                                "target_lang_word": "дельный",
                                "target_lang_sense_clarification": "способный, толковый"
                            },
                            {
                                "target_lang_word": "бравый",
                                "target_lang_sense_clarification": "смелый, мужественный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f5abbe3f-c05e-3aa6-bc8a-e34737cc4552",
                    "sense_definition": "Vorm die bezit aanduidt (genitief) van <w>man</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude teksten leest men soms over de plicht des <w>mans</w>.",
                            "target_lang_translations": {
                                "en": "In old texts, one sometimes reads about the duty of the <w>man</w>.",
                                "ru": "В старых текстах иногда читают о долге <w>мужа</w>."
                            }
                        },
                        {
                            "text": "Het is des <w>mans</w> eer te na om op te geven.",
                            "target_lang_translations": {
                                "en": "It is beneath the <w>man's</w> honor to give up.",
                                "ru": "Сдаться — это ниже чести <w>мужчины</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "des mans"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in fixed expressions or older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Form indicating possession (genitive) of 'man'.",
                        "ru": "Форма, указывающая на принадлежность (родительный падеж) слова 'man'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of the man",
                                "target_lang_sense_clarification": "archaic genitive construction"
                            },
                            {
                                "target_lang_word": "man's",
                                "target_lang_sense_clarification": "possessive form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мужчины",
                                "target_lang_sense_clarification": "форма родительного падежа"
                            },
                            {
                                "target_lang_word": "мужа",
                                "target_lang_sense_clarification": "устаревшая или высокая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92fab46a-1e0f-30fa-aee7-34f16d2cf78c",
                    "sense_definition": "Een bakje dat wordt gebruikt om geld op te halen, bijvoorbeeld door een muzikant of collectant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De straatmuzikant hield zijn <w>mans</w> omhoog in de hoop op een kleine bijdrage.",
                            "target_lang_translations": {
                                "en": "The street musician held up his <w>collection box</w> in hopes of a small contribution.",
                                "ru": "Уличный музыкант поднял свою <w>кружку для сбора денег</w> в надежде на небольшой взнос."
                            }
                        },
                        {
                            "text": "Na het orgelspel ging de <w>mans</w> rond onder de toeschouwers.",
                            "target_lang_translations": {
                                "en": "After the organ performance, the <w>money box</w> went around among the spectators.",
                                "ru": "После игры на органе среди зрителей пошла по кругу <w>кружка для пожертвований</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "geldbakje",
                        "collectebus"
                    ],
                    "target_lang_definitions": {
                        "en": "A small container used to collect money, for example by a musician or collector.",
                        "ru": "Небольшая емкость для сбора денег, например, у музыканта или сборщика пожертвований."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collection box",
                                "target_lang_sense_clarification": "container for donations"
                            },
                            {
                                "target_lang_word": "money tin",
                                "target_lang_sense_clarification": "container for collecting coins"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кружка для сбора денег",
                                "target_lang_sense_clarification": "емкость для пожертвований"
                            },
                            {
                                "target_lang_word": "копилка",
                                "target_lang_sense_clarification": "емкость для сбора монет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d40492d7-668c-3bab-babe-d2f5889a1469",
                    "sense_definition": "Meervoud van <w>man</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In sommige dialecten spreekt men van twee <w>mans</w> in plaats van twee mannen.",
                            "target_lang_translations": {
                                "en": "In some dialects, one speaks of two <w>men</w> instead of two 'mannen'.",
                                "ru": "В некоторых диалектах говорят о двух <w>мужчинах</w> вместо двух 'mannen'."
                            }
                        },
                        {
                            "text": "De kapitein zocht nog enkele <w>mans</w> voor de bemanning.",
                            "target_lang_translations": {
                                "en": "The captain was looking for a few more <w>men</w> for the crew.",
                                "ru": "Капитан искал еще нескольких <w>мужчин</w> для команды."
                            }
                        }
                    ],

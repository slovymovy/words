                {
                    "sense_id": "1537c7b5-65df-34b9-997b-d280a4ba30c8",
                    "sense_definition": "Komunikat lub sygnał informujący o niebezpieczeństwie lub ważnym zdarzeniu",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj na mój telefon przyszedł głośny <w>alert</w> RCB.",

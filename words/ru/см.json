                    "sense_id": "493215c6-6bdb-3d9b-a1d0-f0c1a2f5edfc",
                    "sense_definition": "Условное сокращённое обозначение единицы измерения длины, равной одной сотой метра (<w>сантиметр</w>).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "units_of_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Длина этого карандаша составляет ровно пятнадцать <w>см</w>.",
                            "target_lang_translations": {
                                "en": "The length of this pencil is exactly fifteen <w>cm</w>."
                            }
                        },
                        {
                            "text": "На чертеже все размеры указаны в <w>см</w> для удобства расчётов.",
                            "target_lang_translations": {
                                "en": "In the drawing, all dimensions are indicated in <w>cm</w> for ease of calculation."
                            }
                        }
                    ],
                    "synonyms": [
                        "сантиметр"
                    ],
                    "common_phrases": [
                        "кв. см",
                        "куб. см",
                        "несколько см"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A standard abbreviation for a unit of length equal to one hundredth of a meter (centimeter)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cm",
                                "target_lang_sense_clarification": "abbreviation"
                            },
                            {
                                "target_lang_word": "centimeter",
                                "target_lang_sense_clarification": "unit of length"
                            }
                        ]
                    }
                }
            ]
        },
        {

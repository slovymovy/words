    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ee74c130-d779-3aeb-b140-56ef12d01da3",
                    "sense_definition": "Groot, zwaar en daardoor traag of moeilijk te bewegen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "heavy_slow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrachtwagen was te <w>log</w> om door de smalle straatjes van het oude centrum te rijden.",
                            "target_lang_translations": {
                                "ru": "Грузовик был слишком <w>громоздким</w>, чтобы проехать по узким улочкам старого центра."
                            }
                        },
                        {
                            "text": "Het <w>logge</w> dier kwam langzaam in beweging toen het de toeristen zag.",
                            "target_lang_translations": {
                                "ru": "<w>Неповоротливое</w> животное медленно зашевелилось, увидев туристов."
                            }
                        }
                    ],
                    "synonyms": [
                        "logplankje",
                        "logbestand",
                        "logplankje"
                    ],
                    "antonyms": [
                        "wendbaar",
                        "licht",
                        "vlot"
                    ],
                    "common_phrases": [
                        "een log gevaarte",
                        "logge bewegingen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой, тяжелый и поэтому медленный или неповоротливый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неповоротливый",
                                "target_lang_sense_clarification": "о живых существах или движениях"
                            },
                            {
                                "target_lang_word": "громоздкий",
                                "target_lang_sense_clarification": "о предметах или транспортных средствах"
                            },
                            {
                                "target_lang_word": "тяжеловесный",
                                "target_lang_sense_clarification": "общий смысл тяжести и медлительности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "647f10b1-7809-3ad6-a834-3880a0f2c8ef",
                    "sense_definition": "Saai, traag van begrip of zonder inspiratie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dull_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hitte voelde hij zich <w>log</w> en had hij geen zin om te werken.",
                            "target_lang_translations": {
                                "ru": "Из-за жары он чувствовал себя <w>вялым</w> и не хотел работать."
                            }
                        },
                        {
                            "text": "Zijn <w>logge</w> manier van denken zorgde ervoor dat hij de grap niet begreep.",
                            "target_lang_translations": {
                                "ru": "Его <w>медлительное</w> мышление привело к тому, что он не понял шутку."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewone logaritme"
                    ],
                    "antonyms": [
                        "scherpzinnig",
                        "levendig"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скучный, медленно соображающий или лишенный вдохновения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вялый",
                                "target_lang_sense_clarification": "о физическом или ментальном состоянии"
                            },
                            {
                                "target_lang_word": "медлительный",
                                "target_lang_sense_clarification": "о мышлении или реакции"
                            },
                            {
                                "target_lang_word": "инертный",
                                "target_lang_sense_clarification": "безынициативный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e0c31fb-b00f-3831-88a4-ffa7e88a9e76",
                    "sense_definition": "Een digitaal bestand waarin gebeurtenissen of wijzigingen van een computersysteem automatisch worden bijgehouden.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "digital_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De systeembeheerder bekeek het <w>log</w> om de oorzaak van de storing te vinden.",
                            "target_lang_translations": {
                                "ru": "Системный администратор просмотрел <w>журнал</w>, чтобы найти причину сбоя."
                            }
                        },
                        {
                            "text": "Alle inlogpogingen worden opgeslagen in een beveiligd <w>log</w> op de server.",
                            "target_lang_translations": {
                                "ru": "Все попытки входа сохраняются в защищенном <w>логе</w> на сервере."
                            }
                        }
                    ],
                    "synonyms": [
                        "logbestand"
                    ],
                    "common_phrases": [
                        "het log bekijken",
                        "in het log kijken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровой файл, в котором автоматически фиксируются события или изменения в компьютерной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "системный журнал событий"
                            },
                            {
                                "target_lang_word": "лог",
                                "target_lang_sense_clarification": "технический термин/сленг"
                            },
                            {
                                "target_lang_word": "протокол",
                                "target_lang_sense_clarification": "запись действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bb85ff5-4ad7-3feb-b174-bbf81484a820",
                    "sense_definition": "Een website waarop regelmatig persoonlijke berichten of artikelen worden geplaatst (afkorting van weblog).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "website",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij schrijft elke week een nieuw verhaal op haar persoonlijke <w>log</w>.",
                            "target_lang_translations": {
                                "ru": "Каждую неделю она пишет новый рассказ в своем личном <w>блоге</w>."
                            }
                        },
                        {
                            "text": "Op zijn <w>log</w> deelt hij foto's en ervaringen van zijn reizen door Azië.",
                            "target_lang_translations": {
                                "ru": "В своем <w>блоге</w> он делится фотографиями и впечатлениями от путешествий по Азии."
                            }
                        }
                    ],
                    "synonyms": [
                        "blog",
                        "weblog"
                    ],
                    "common_phrases": [
                        "een log bijhouden",
                        "op mijn log"
                    ],
                    "target_lang_definitions": {
                        "ru": "Веб-сайт, на котором регулярно публикуются личные сообщения или статьи (сокращение от weblog)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блог",
                                "target_lang_sense_clarification": "интернет-дневник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd276ea9-3116-3aa0-9a42-c6b462a636f0",
                    "sense_definition": "Wiskundige term die aangeeft tot welke macht een grondtal verheven moet worden (afkorting van logaritme).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de wiskundeles moesten we de <w>log</w> van honderd berekenen.",
                            "target_lang_translations": {
                                "ru": "На уроке математики мы должны были вычислить <w>логарифм</w> ста."
                            }
                        },
                        {
                            "text": "De formule maakt gebruik van <w>log</w> om de exponentiële groei te beschrijven.",
                            "target_lang_translations": {
                                "ru": "Формула использует <w>логарифм</w> для описания экспоненциального роста."
                            }
                        }
                    ],
                    "synonyms": [
                        "lg",
                        "ln",
                        "briggse logaritme",
                        "neperiaanse logaritme",
                        "gewone logaritme"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Математический термин, обозначающий степень, в которую нужно возвести основание (сокращение от logaritme)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "логарифм",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45189d24-74e9-38f8-a613-dbce2947e5ad",
                    "sense_definition": "Een instrument om de snelheid van een schip te meten.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein wierp de <w>log</w> uit om te zien hoe snel het schip voer.",
                            "target_lang_translations": {
                                "ru": "Капитан выбросил <w>лаг</w>, чтобы узнать скорость корабля."
                            }
                        },
                        {
                            "text": "Moderne schepen gebruiken elektronische apparatuur in plaats van een ouderwetse <w>log</w>.",
                            "target_lang_translations": {
                                "ru": "Современные корабли используют электронное оборудование вместо старомодного <w>лага</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "logplankje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибор для измерения скорости судна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лаг",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cbfaee45-4026-34ab-a47c-5ae934dbe4cf",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>loggen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>log</w> elke ochtend in op mijn computer om mijn e-mail te lezen.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <w>вхожу</w> в систему на компьютере, чтобы прочитать почту."
                            }
                        },
                        {
                            "text": "Als ik klaar ben met werken, <w>log</w> ik direct uit.",
                            "target_lang_translations": {
                                "ru": "Когда я заканчиваю работу, я сразу <w>выхожу</w> из системы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik log in",
                        "ik log uit"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола 'loggen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вхожу",
                                "target_lang_sense_clarification": "в систему (log in)"
                            },
                            {
                                "target_lang_word": "выхожу",
                                "target_lang_sense_clarification": "из системы (log uit)"
                            },
                            {
                                "target_lang_word": "регистрирую",
                                "target_lang_sense_clarification": "записываю данные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fea834cd-3891-3c2b-9213-8fe1f22203c8",
                    "sense_definition": "Gebiedende wijs van <w>loggen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Log</w> eerst in voordat je probeert het bestand te openen.",
                            "target_lang_translations": {
                                "ru": "Сначала <w>войди</w> в систему, прежде чем пытаться открыть файл."
                            }
                        },
                        {
                            "text": "<w>Log</w> al je uren zorgvuldig in het systeem voor de administratie.",
                            "target_lang_translations": {
                                "ru": "Тщательно <w>регистрируй</w> все свои часы в системе для администрации."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'loggen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "войди",
                                "target_lang_sense_clarification": "в систему"
                            },
                            {
                                "target_lang_word": "записывай",
                                "target_lang_sense_clarification": "регистрируй данные"
                            }

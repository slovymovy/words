{
    "word_family": [
        "aangeven",
        "aangifte"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "972c8639-ca7e-3491-bcf3-c104ee0a70ca",
                    "sense_definition": "Iemand die bij de politie of justitie melding maakt van een strafbaar feit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reporter_crime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie nam de verklaring van de <w>aangever</w> zeer serieus.",
                            "target_lang_translations": {
                                "en": "The police took the <w>complainant</w>'s statement very seriously.",
                                "ru": "Полиция отнеслась к показаниям <w>заявителя</w> очень серьезно."
                            }
                        },
                        {
                            "text": "Zonder <w>aangever</w> is het voor de recherche lastig om een onderzoek te starten.",
                            "target_lang_translations": {
                                "en": "Without a <w>reporting party</w>, it is difficult for the detectives to start an investigation.",
                                "ru": "Без <w>заявителя</w> следователям трудно начать расследование."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanbrenger",
                        "tipgever",
                        "informant"
                    ],
                    "common_phrases": [
                        "de aangever van het misdrijf",
                        "verklaring van de aangever"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who reports a criminal offense to the police or justice department.",
                        "ru": "Тот, кто сообщает в полицию или органы юстиции о преступлении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complainant",
                                "target_lang_sense_clarification": "legal context"
                            },
                            {
                                "target_lang_word": "reporting party",
                                "target_lang_sense_clarification": "police context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заявитель",
                                "target_lang_sense_clarification": "лицо, подающее заявление"
                            },
                            {
                                "target_lang_word": "потерпевший",
                                "target_lang_sense_clarification": "если заявитель является жертвой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c662ad11-ec7a-3a22-b69b-78c1cf3b18b3",
                    "sense_definition": "Iemand die officieel gegevens doorgeeft aan een instantie, bijvoorbeeld bij de burgerlijke stand of de belastingdienst.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "reporter_admin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vader was de <w>aangever</w> van de geboorte van zijn zoon.",
                            "target_lang_translations": {
                                "en": "The father was the <w>informant</w> for the birth of his son.",
                                "ru": "Отец выступил <w>заявителем</w> при регистрации рождения сына."
                            }
                        },
                        {
                            "text": "Elke <w>aangever</w> moet de formulieren volledig en naar waarheid invullen.",
                            "target_lang_translations": {
                                "en": "Every <w>declarant</w> must fill out the forms completely and truthfully.",
                                "ru": "Каждый <w>заявитель</w> должен заполнить бланки полностью и правдиво."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aangever van de geboorte",
                        "aangever bij de douane"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who officially submits information to an authority, for example at the civil registry or tax office.",
                        "ru": "Лицо, официально передающее данные в учреждение (например, в ЗАГС или налоговую)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "declarant",
                                "target_lang_sense_clarification": "customs or tax context"
                            },
                            {
                                "target_lang_word": "informant",
                                "target_lang_sense_clarification": "vital records context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заявитель",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "декларант",
                                "target_lang_sense_clarification": "на таможне или в налоговой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ce83358-f459-37fd-bc95-462c60c011ef",
                    "sense_definition": "Een speler die de bal naar een teamgenoot speelt zodat deze kan scoren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_assist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was de <w>aangever</w> bij het winnende doelpunt van de wedstrijd.",
                            "target_lang_translations": {
                                "en": "He was the <w>provider</w> for the match-winning goal.",
                                "ru": "Он стал <w>ассистентом</w> победного гола в матче."
                            }
                        },
                        {
                            "text": "De spits bedankte de <w>aangever</w> voor de perfecte voorzet.",
                            "target_lang_translations": {
                                "en": "The striker thanked the <w>assist maker</w> for the perfect cross.",
                                "ru": "Нападающий поблагодарил <w>распасовщика</w> за идеальную передачу."
                            }
                        }
                    ],
                    "antonyms": [
                        "afmaker"
                    ],
                    "common_phrases": [
                        "de aangever van het doelpunt"
                    ],
                    "target_lang_definitions": {
                        "en": "A player who passes the ball to a teammate so they can score.",
                        "ru": "Игрок, делающий передачу партнеру по команде для взятия ворот."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "provider",
                                "target_lang_sense_clarification": "sports context"
                            },
                            {
                                "target_lang_word": "assist maker",
                                "target_lang_sense_clarification": "sports context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ассистент",
                                "target_lang_sense_clarification": "автор голевой передачи"
                            },
                            {
                                "target_lang_word": "распасовщик",
                                "target_lang_sense_clarification": "игрок, раздающий пасы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46bf19b0-2800-39d4-8c39-6c915a4f9dab",
                    "sense_definition": "De persoon in een komisch duo die de serieuze rol speelt en de grappen van de ander voorbereidt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "comedy_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als <w>aangever</w> moet je serieus blijven terwijl het publiek om de ander lacht.",
                            "target_lang_translations": {
                                "en": "As the <w>straight man</w>, you have to remain serious while the audience laughs at the other person.",
                                "ru": "В роли <w>подыгрывающего</w> нужно сохранять серьезность, пока публика смеется над партнером."
                            }
                        },
                        {
                            "text": "Het duo bestaat uit een drukke grappenmaker en een droge <w>aangever</w>.",
                            "target_lang_translations": {
                                "en": "The duo consists of a busy joker and a dry <w>straight man</w>.",
                                "ru": "Дуэт состоит из энергичного шутника и невозмутимого <w>подыгрывающего</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aangever en afmaker"
                    ],
                    "target_lang_definitions": {
                        "en": "The person in a comedy duo who plays the serious role and sets up the other's jokes.",
                        "ru": "Участник комического дуэта, играющий серьезную роль и подготавливающий шутки партнера."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "straight man",
                                "target_lang_sense_clarification": "comedy context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подыгрывающий",
                                "target_lang_sense_clarification": "роль в дуэте"
                            },
                            {
                                "target_lang_word": "партнёр",
                                "target_lang_sense_clarification": "в контексте сценического дуэта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99f7a12e-d2e1-321f-a2f6-7302e1214de8",
                    "sense_definition": "Iemand die spullen of materialen aanreikt aan een ander die het werk uitvoert.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "literal_hander",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de bouwplaats werkt hij als <w>aangever</w> voor de ervaren metselaar.",
                            "target_lang_translations": {
                                "en": "On the construction site, he works as a <w>helper</w> for the experienced bricklayer.",
                                "ru": "На стройке он работает <w>подсобником</w> у опытного каменщика."
                            }
                        },
                        {
                            "text": "De <w>aangever</w> gaf de stenen één voor één door aan zijn collega.",
                            "target_lang_translations": {
                                "en": "The <w>assistant</w> passed the bricks one by one to his colleague.",
                                "ru": "<w>Подсобник</w> передавал кирпичи один за другим своему коллеге."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who hands tools or materials to another person performing the work.",
                        "ru": "Человек, подающий материалы или инструменты мастеру."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "helper",
                                "target_lang_sense_clarification": "manual labor"
                            },
                            {
                                "target_lang_word": "assistant",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подсобник",
                                "target_lang_sense_clarification": "подсобный рабочий"
                            },
                            {
                                "target_lang_word": "подручный",
                                "target_lang_sense_clarification": "помощник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

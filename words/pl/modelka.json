            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9c99a48b-ed00-362c-8877-859c755e0abd",
                    "sense_definition": "Kobieta, która zawodowo prezentuje stroje, fryzury lub kosmetyki na pokazach mody i w sesjach zdjęciowych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "osoba_pozująca",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ania jest ładną i wysoką dziewczyną, więc ma szansę zostać <w>modelką</w>.",
                            "target_lang_translations": {
                                "ru": "Аня — красивая и высокая девушка, поэтому у неё есть шанс стать <w>моделью</w>."
                            }
                        },
                        {
                            "text": "Słynna <w>modelka</w> wyszła na wybieg w pięknej czerwonej sukni.",
                            "target_lang_translations": {
                                "ru": "Известная <w>модель</w> вышла на подиум в красивом красном платье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zostać modelką",
                        "znana modelka",
                        "agencja modelek",
                        "szczupła modelka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которая профессионально демонстрирует одежду, прически или косметику на показах мод и фотосессиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "манекенщица",
                                "target_lang_sense_clarification": "на подиуме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db2a1bf3-cf89-38ac-8485-7b6a78a588d1",
                    "sense_definition": "Kobieta pozująca artyście do zdjęć, obrazów lub rzeźb.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "osoba_pozująca",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malarz poprosił swoją <w>modelkę</w>, aby zmieniła pozę.",
                            "target_lang_translations": {
                                "ru": "Художник попросил свою <w>модель</w> сменить позу."
                            }
                        },
                        {
                            "text": "Ta kobieta była ulubioną <w>modelką</w> znanego rzeźbiarza.",
                            "target_lang_translations": {
                                "ru": "Эта женщина была любимой <w>моделью</w> известного скульптора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pozować jako modelka",
                        "modelka malarza",
                        "szukać modelki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, позирующая художнику для картин, фотографий или скульптур."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "натурщица",

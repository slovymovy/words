    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "7b562e16-2d7e-3f91-8f02-678eeb8a9e40",
                    "sense_definition": "Соединяет однородные члены предложения, а также части сложного предложения, указывая на их связь или последовательность.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На столе лежали красное яблоко <и> сладкая груша."
                        },
                        {
                            "text": "Солнце уже село, <и> на улице стало прохладно."
                        },
                        {
                            "text": "Мы долго гуляли по парку <и> разговаривали о жизни."
                        }
                    ],
                    "synonyms": [
                        "да"
                    ],
                    "antonyms": [
                        "а",
                        "не",
                        "без"
                    ],
                    "common_phrases": [
                        "день и ночь",
                        "муж и жена",
                        "отцы и дети",
                        "ты и я"
                    ]
                },
                {
                    "sense_id": "24235d95-be4c-382f-916a-f3a649c7d6fc",
                    "sense_definition": "Употребляется в повторяющейся конструкции (и..., и...) при перечислении для охвата всех названных предметов, лиц или действий.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "России для защиты нужны <и> армия, <и> флот."
                        },
                        {
                            "text": "На празднике были <и> взрослые, <и> дети."
                        }
                    ],
                    "common_phrases": [
                        "и смех и грех",
                        "и там и сям",
                        "и так и эдак"
                    ]
                },
                {
                    "sense_id": "45bef530-7976-3ae0-9ea4-5a20af3145ea",
                    "sense_definition": "Соединяет повторяющиеся слова для выражения длительности действия или интенсивности признака.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "intensification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице становится всё холоднее <и> холоднее."
                        },
                        {
                            "text": "Мы шли по лесу, а дождь всё лил <и> лил."
                        }
                    ]
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "02def7cf-b538-3b83-9905-12723416166b",
                    "sense_definition": "Усиливает значение следующего слова, соответствуя по смыслу частицам «даже» или «также».",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <и> представить себе не мог такого поворота событий."
                        },
                        {
                            "text": "Он <и> не пытался спорить с начальником."
                        },
                        {
                            "text": "Даже если это правда, я <и> это знаю."
                        }
                    ],
                    "synonyms": [
                        "даже",
                        "даже и",
                        "ещё и"
                    ],
                    "common_phrases": [
                        "и без того",
                        "и правда"
                    ]
                },
                {
                    "sense_id": "80f597eb-aae5-399c-9d9c-0b9022c4d025",
                    "sense_definition": "Подчёркивает соответствие действия ожиданию, вывод или неизбежное следствие (часто в сочетании с «так», «вот», «поэтому»).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не знаю ответа, поэтому <и> спрашиваю тебя."
                        },
                        {
                            "text": "Он обещал прийти вовремя, так он <и> сделал."
                        },
                        {
                            "text": "Вот <и> закончилось наше короткое лето."
                        }
                    ],
                    "common_phrases": [
                        "так и есть",
                        "так и надо",
                        "вот и всё",
                        "так и не"
                    ]
                }
            ]
        },

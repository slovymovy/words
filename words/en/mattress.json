    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8edb8699-bb19-30ca-9697-a05245a26cc9",
                    "sense_definition": "A large, thick pad filled with soft material or springs, used as a bed or part of a bed for sleeping on.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to buy a new <w>mattress</w> because the old one is too lumpy.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новый <w>матрас</w>, потому что старый стал слишком бугристым."
                            }
                        },
                        {
                            "text": "She put a clean sheet over the <w>mattress</w> before making the bed.",
                            "target_lang_translations": {
                                "ru": "Она постелила чистую простыню на <w>матрас</w>, прежде чем заправить кровать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "air mattress",
                        "memory foam mattress",
                        "mattress protector",
                        "spring mattress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая толстая подстилка, набитая мягким материалом или пружинами, используемая как кровать или часть кровати для сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матрас",
                                "target_lang_sense_clarification": "для сна"
                            },
                            {
                                "target_lang_word": "матрац",
                                "target_lang_sense_clarification": "вариант написания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b85fc79-572f-3c88-ad80-fbb4527725d9",
                    "sense_definition": "A flat structure made of concrete, brushwood, or wire mesh used to support a foundation or protect a riverbank from erosion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Engineers installed a concrete <w>mattress</w> to stabilize the riverbank.",
                            "target_lang_translations": {
                                "ru": "Инженеры уложили бетонный <w>мат</w> для укрепления берега реки."
                            }
                        },
                        {
                            "text": "The foundation of the bridge rests on a heavy stone <w>mattress</w>.",
                            "target_lang_translations": {
                                "ru": "Фундамент моста покоится на тяжелом каменном <w>тюфяке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "concrete mattress",
                        "river mattress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская конструкция из бетона, хвороста или проволочной сетки, используемая для поддержки фундамента или защиты берега реки от эрозии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюфяк",
                                "target_lang_sense_clarification": "гидротехнический"
                            },
                            {
                                "target_lang_word": "мат",
                                "target_lang_sense_clarification": "строительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "537d6333-f550-307d-a66f-3174e1c1d780",
                    "sense_definition": "A layer of insulating material wrapped around a boiler or pipes to retain heat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers applied a fiberglass <w>mattress</w> to the boiler to improve efficiency.",
                            "target_lang_translations": {
                                "ru": "Рабочие обернули бойлер <w>матом</w> из стекловолокна для повышения эффективности."
                            }
                        },
                        {
                            "text": "Old insulation was removed and a new thermal <w>mattress</w> was fitted.",
                            "target_lang_translations": {
                                "ru": "Старую изоляцию удалили и установили новый теплоизоляционный <w>мат</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lagging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой изоляционного материала, обернутый вокруг котла или труб для сохранения тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мат",
                                "target_lang_sense_clarification": "изоляционный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a55fe5fc-6caa-3740-9aa0-fa1924bdd4e9",
                    "sense_definition": "To cover a surface with a thick, soft layer.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thick layers of pine needles <w>mattressed</w> the forest floor.",
                            "target_lang_translations": {
                                "ru": "Толстые слои сосновых иголок <w>устилали</w> лесную почву."
                            }
                        },
                        {
                            "text": "The heavy snow <w>mattressed</w> the fields in white.",
                            "target_lang_translations": {
                                "ru": "Сильный снегопад <w>укрыл</w> поля белым одеялом."
                            }
                        }
                    ],
                    "synonyms": [
                        "blanket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность толстым мягким слоем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устилать",
                                "target_lang_sense_clarification": "покрывать слоем"
                            },
                            {
                                "target_lang_word": "укрывать",
                                "target_lang_sense_clarification": "мягко обволакивать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
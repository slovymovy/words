    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f7ee7dfe-c847-3296-9e62-fd7b2e0648dc",
                    "sense_definition": "Relating to Scotland, its culture, geography, or characteristics.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We visited a beautiful <w>Scottish</w> castle during our holiday.",
                            "target_lang_translations": {
                                "ru": "Мы посетили красивый <w>шотландский</w> замок во время отпуска."
                            }
                        },
                        {
                            "text": "He loves listening to traditional <w>Scottish</w> music.",
                            "target_lang_translations": {
                                "ru": "Он любит слушать традиционную <w>шотландскую</w> музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "Scots"
                    ],
                    "common_phrases": [
                        "Scottish Highlands",
                        "Scottish terrier",
                        "Scottish borders",
                        "Scottish accent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Шотландии, её культуре, географии или особенностям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шотландский",
                                "target_lang_sense_clarification": "относящийся к стране или культуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74eaa3a0-c455-31e3-9e5c-01b1342eab44",
                    "sense_definition": "Relating to a person who is native to or born in Scotland.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother is <w>Scottish</w> and makes excellent shortbread.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка — <w>шотландка</w>, и она готовит отличное песочное печенье."
                            }
                        },
                        {
                            "text": "A famous <w>Scottish</w> actor starred in the new movie.",
                            "target_lang_translations": {
                                "ru": "В новом фильме снялся известный <w>шотландский</w> актер."
                            }
                        }
                    ],
                    "synonyms": [
                        "Scots"
                    ],
                    "common_phrases": [
                        "Scottish descent",
                        "Scottish people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к человеку, который родился в Шотландии или является её уроженцем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шотландский",
                                "target_lang_sense_clarification": "о происхождении"
                            },
                            {
                                "target_lang_word": "шотландец",
                                "target_lang_sense_clarification": "существительное, мужской род"
                            },
                            {
                                "target_lang_word": "шотландка",
                                "target_lang_sense_clarification": "существительное, женский род"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2c16c13-a179-314e-af05-e532b29f7e5b",
                    "sense_definition": "The people who come from Scotland, considered as a group.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Scottish</w> are known for their warm hospitality.",
                            "target_lang_translations": {
                                "ru": "<w>Шотландцы</w> известны своим теплым гостеприимством."
                            }
                        },
                        {
                            "text": "Many of the <w>Scottish</w> celebrate Burns Night every year.",
                            "target_lang_translations": {
                                "ru": "Многие <w>шотландцы</w> ежегодно празднуют Ночь Бернса."
                            }
                        }
                    ],
                    "synonyms": [
                        "Scots"
                    ],
                    "common_phrases": [
                        "the Scottish"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди родом из Шотландии как группа; шотландский народ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шотландцы",
                                "target_lang_sense_clarification": "народ"
                            }

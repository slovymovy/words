{
    "word_family": [
        "gemanierd",
        "maniërisme"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b06bad58-f45c-3c28-ac67-124e8d878ed1",
                    "sense_definition": "De wijze waarop iets gebeurt of gedaan wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>manier</w> waarop je mij aanspreekt staat mij niet aan.",
                            "target_lang_translations": {
                                "en": "I do not like the <w>way</w> you speak to me.",
                                "ru": "Мне не нравится <w>манера</w>, с которой ты ко мне обращаешься."
                            }
                        },
                        {
                            "text": "Op deze <w>manier</w> kunnen we het probleem waarschijnlijk niet oplossen.",
                            "target_lang_translations": {
                                "en": "We probably cannot solve the problem in this <w>way</w>.",
                                "ru": "Этим <w>способом</w> мы, вероятно, не сможем решить проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "wijze",
                        "methode",
                        "aanpak",
                        "handelwijze"
                    ],
                    "common_phrases": [
                        "op die manier",
                        "op deze manier",
                        "op geen enkele manier",
                        "op een of andere manier"
                    ],
                    "target_lang_definitions": {
                        "en": "The way in which something happens or is done.",
                        "ru": "Способ, которым что-то происходит или делается."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "way",
                                "target_lang_sense_clarification": "method or manner of doing something"
                            },
                            {
                                "target_lang_word": "manner",
                                "target_lang_sense_clarification": "mode of action; slightly more formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "способ",
                                "target_lang_sense_clarification": "метод или путь достижения цели"
                            },
                            {
                                "target_lang_word": "образ",
                                "target_lang_sense_clarification": "характер действия (часто в выражении \"таким образом\")"
                            },
                            {
                                "target_lang_word": "манера",
                                "target_lang_sense_clarification": "стиль исполнения или особенность поведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b7d376d-3e11-3a30-8949-03e944f0c783",
                    "sense_definition": "Gedrag dat getuigt van beleefdheid en goede opvoeding.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "etiquette",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn ouders hebben hem gelukkig goede <w>manieren</w> bijgebracht.",
                            "target_lang_translations": {
                                "en": "Fortunately, his parents taught him good <w>manners</w>.",
                                "ru": "К счастью, родители привили ему хорошие <w>манеры</w>."
                            }
                        },
                        {
                            "text": "Het getuigt van slechte <w>manieren</w> om met volle mond te praten.",
                            "target_lang_translations": {
                                "en": "It shows bad <w>manners</w> to talk with your mouth full.",
                                "ru": "Разговаривать с набитым ртом — это признак плохих <w>манер</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fatsoen",
                        "omgangsvormen",
                        "wellevendheid"
                    ],
                    "common_phrases": [
                        "goede manieren",
                        "manieren leren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vaak in het meervoud"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Behavior that shows politeness and good upbringing.",
                        "ru": "Поведение, свидетельствующее о вежливости и хорошем воспитании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manners",
                                "target_lang_sense_clarification": "social conduct or rules of etiquette"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "манеры",
                                "target_lang_sense_clarification": "правила поведения и этикет (обычно во множественном числе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
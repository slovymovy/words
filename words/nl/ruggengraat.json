                    "sense_id": "74c6c006-519c-36c1-8366-4e6cf3d4b221",
                    "sense_definition": "De reeks wervels in het midden van de rug die het lichaam steun geeft en het ruggenmerg beschermt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter maakte een röntgenfoto van zijn <w>ruggengraat</w> na de val.",
                            "target_lang_translations": {
                                "en": "The doctor took an X-ray of his <w>spine</w> after the fall."
                            }
                        },
                        {
                            "text": "De <w>ruggengraat</w> bestaat uit vele kleine botjes die wervels worden genoemd.",
                            "target_lang_translations": {
                                "en": "The <w>spine</w> consists of many small bones called vertebrae."
                            }
                        }
                    ],
                    "synonyms": [
                        "wervelkolom"
                    ],
                    "common_phrases": [
                        "kromme ruggengraat",
                        "gebroken ruggengraat"
                    ],
                    "target_lang_definitions": {

            "senses": [
                {
                    "sense_id": "a9a82f7b-7ea1-372d-a1e3-50fbab31c373",
                    "sense_definition": "Провести некоторое время, занимаясь стрельбой из оружия.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В выходные мы планируем поехать в тир и немного <w>пострелять</w> по мишеням.",
                            "target_lang_translations": {
                                "en": "On the weekend we plan to go to the shooting range and <w>shoot</w> at targets for a bit."
                            }
                        },
                        {
                            "text": "Отец взял ружье, чтобы просто <w>пострелять</w> в лесу и проверить прицел.",
                            "target_lang_translations": {
                                "en": "Father took the shotgun just to <w>shoot</w> in the forest and check the sights."
                            }
                        }
                    ],
                    "synonyms": [
                        "попалить"
                    ],
                    "common_phrases": [
                        "пострелять в тире",
                        "пострелять по мишеням",
                        "поехать пострелять",
                        "дать пострелять"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend some time shooting a weapon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shoot",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "do some shooting",
                                "target_lang_sense_clarification": "emphasizing the duration/activity"
                            },
                            {
                                "target_lang_word": "fire some shots",
                                "target_lang_sense_clarification": "specific action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "694d2a46-3ab8-35a2-81fe-55d245afc1f3",
                    "sense_definition": "Добыть некоторое количество дичи во время охоты.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hunting_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За утреннюю охоту им удалось <w>пострелять</w> несколько уток.",
                            "target_lang_translations": {
                                "en": "During the morning hunt, they managed to <w>shoot</w> several ducks."
                            }
                        },
                        {
                            "text": "Охотники надеялись <w>пострелять</w> зайцев, но не встретили ни одного.",
                            "target_lang_translations": {
                                "en": "The hunters hoped to <w>bag</w> some hares, but didn't meet a single one."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пострелять дичи",
                        "пострелять уток"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies a successful result of hunting"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To kill a certain amount of game while hunting."
                    },
                    "translations": {

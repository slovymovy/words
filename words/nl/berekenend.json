{
    "word_family": [
        "berekenen",
        "berekening",
        "berekenbaar"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f6e130a7-6d45-362a-a5ac-28b5a0a171ec",
                    "sense_definition": "Alleen denkend aan het eigen voordeel en hoe men dat kan bereiken, vaak op een kille of sluwe manier.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een <w>berekenend</w> persoon die mensen gebruikt voor zijn eigen succes.",
                            "target_lang_translations": {
                                "ru": "Он <w>расчётливый</w> человек, который использует людей ради собственного успеха."
                            }
                        },
                        {
                            "text": "De dief ging koel en <w>berekenend</w> te werk tijdens de inbraak.",
                            "target_lang_translations": {
                                "ru": "Вор действовал хладнокровно и <w>расчётливо</w> во время взлома."
                            }
                        }
                    ],
                    "synonyms": [
                        "egoïstisch",
                        "gewiekst",
                        "gehaaid",
                        "doortrapt",
                        "uitgekookt",
                        "listig"
                    ],
                    "antonyms": [
                        "onbaatzuchtig",
                        "edelmoedig"
                    ],
                    "common_phrases": [
                        "koel en berekenend",
                        "een berekenende zet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Думающий только о собственной выгоде и способах её достижения, часто холодным или хитрым образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расчётливый",
                                "target_lang_sense_clarification": "преследующий личную выгоду"
                            },
                            {
                                "target_lang_word": "корыстный",
                                "target_lang_sense_clarification": "стремящийся к наживе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e83a376-7c24-3a59-acdd-2b1d8ebaa012",
                    "sense_definition": "Zorgvuldig overwegend en plannend om een bepaald doel te bereiken, zonder impulsief te zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet liep een <w>berekenende</w> race om zijn krachten te sparen voor de eindsprint.",
                            "target_lang_translations": {
                                "ru": "Атлет провел <w>продуманный</w> забег, чтобы сберечь силы для финишного рывка."
                            }
                        },
                        {
                            "text": "Ze nam een <w>berekenend</w> risico door in dat nieuwe bedrijf te investeren.",
                            "target_lang_translations": {
                                "ru": "Она пошла на <w>рассчитанный</w> риск, инвестируя в эту новую компанию."
                            }
                        }
                    ],
                    "synonyms": [
                        "zorgvuldig",
                        "verstandig",
                        "slim"
                    ],
                    "common_phrases": [
                        "een berekenend risico"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно обдумывающий и планирующий действия для достижения цели, без импульсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продуманный",
                                "target_lang_sense_clarification": "спланированный заранее"
                            },
                            {
                                "target_lang_word": "взвешенный",
                                "target_lang_sense_clarification": "обдуманный"
                            },
                            {
                                "target_lang_word": "рассчитанный",
                                "target_lang_sense_clarification": "о риске или стратегии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1399e5da-c927-3836-b2b9-23ca2ea1b85a",
                    "sense_definition": "Het onvoltooid deelwoord van <w>berekenen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aannemer stond in de kamer, de kosten voor de verbouwing <w>berekenend</w>.",
                            "target_lang_translations": {
                                "ru": "Подрядчик стоял в комнате, <w>подсчитывая</w> расходы на ремонт."
                            }
                        },
                        {
                            "text": "Snel zijn kansen <w>berekenend</w>, besloot hij om toch mee te doen aan de wedstrijd.",
                            "target_lang_translations": {
                                "ru": "Быстро <w>рассчитывая</w> свои шансы, он решил всё же принять участие в соревновании."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола berekenen (вычислять, рассчитывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассчитывающий",
                                "target_lang_sense_clarification": "оценивающий шансы или возможности"
                            },
                            {
                                "target_lang_word": "подсчитывающий",
                                "target_lang_sense_clarification": "вычисляющий сумму"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ddd1b37c-01ed-3871-a1fa-c03ac77a943c",
                    "sense_definition": "To move a camera horizontally to follow a subject or show a wider view.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "camera_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The camera <w>panned</w> across the crowd to show the size of the audience.",
                            "target_lang_translations": {
                                "ru": "Камера <w>панорамировала</w> по толпе, чтобы показать размер аудитории."
                            }
                        },
                        {
                            "text": "He <w>panned</w> slowly to the left to capture the sunset.",
                            "target_lang_translations": {
                                "ru": "Он медленно <w>повел</w> камерой влево, чтобы запечатлеть закат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pan across",
                        "pan left",
                        "pan right"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать камеру горизонтально, чтобы следовать за объектом или показать более широкий обзор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панорамировать",
                                "target_lang_sense_clarification": "двигать камерой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f1047e8-708b-316b-88ae-e544fd793f2d",
                    "sense_definition": "To develop in a particular way or be successful (usually used with 'out').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "outcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We will have to wait and see how the situation <w>pans</w> out.",
                            "target_lang_translations": {
                                "ru": "Нам придется подождать и посмотреть, как <w>сложится</w> ситуация."
                            }
                        },
                        {
                            "text": "Unfortunately, his investment ideas didn't <w>pan</w> out as he had hoped.",
                            "target_lang_translations": {
                                "ru": "К сожалению, его инвестиционные идеи не <w>сработали</w> так, как он надеялся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pan out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развиваться определенным образом или быть успешным (обычно используется с 'out')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "складываться",
                                "target_lang_sense_clarification": "о ситуации"
                            },
                            {
                                "target_lang_word": "удаваться",
                                "target_lang_sense_clarification": "об успехе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "039c5b72-4969-3867-8658-f005486102c3",
                    "sense_definition": "To criticize something severely, such as a movie, play, or book.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "criticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The critics <w>panned</w> the new film for its weak plot and poor acting.",
                            "target_lang_translations": {
                                "ru": "Критики <w>разнесли</w> новый фильм за слабый сюжет и плохую игру актеров."
                            }
                        },
                        {
                            "text": "Although the book was <w>panned</w> by reviewers, it became a bestseller.",
                            "target_lang_translations": {
                                "ru": "Хотя книга была <w>раскритикована</w> рецензентами, она стала бестселлером."
                            }
                        }
                    ],
                    "synonyms": [
                        "criticize",
                        "slam"
                    ],
                    "common_phrases": [
                        "critically panned"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестко критиковать что-либо, например, фильм, пьесу или книгу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскритиковать",
                                "target_lang_sense_clarification": "подвергнуть критике"
                            },
                            {
                                "target_lang_word": "разнести",
                                "target_lang_sense_clarification": "сильно раскритиковать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "392b43c0-8273-39fb-afbd-fd18eeb97c24",
                    "sense_definition": "To wash earth or gravel in a shallow container to search for gold.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prospectors <w>panned</w> for gold in the river all day long.",
                            "target_lang_translations": {
                                "ru": "Старатели весь день <w>мыли</w> золото в реке."
                            }
                        },
                        {
                            "text": "They learned how to <w>pan</w> the sand to separate the heavy minerals.",
                            "target_lang_translations": {
                                "ru": "Они научились <w>промывать</w> песок, чтобы отделить тяжелые минералы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pan for gold"
                    ],
                    "target_lang_definitions": {
                        "ru": "Промывать землю или гравий в мелкой емкости для поиска золота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промывать",
                                "target_lang_sense_clarification": "мыть золото"
                            },
                            {
                                "target_lang_word": "мыть",
                                "target_lang_sense_clarification": "добывать промывкой"
                            }
                        ]
                    }
                }
            ]
        },

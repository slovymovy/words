        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "f62a802e-65d3-35e8-8262-1b63fae6d941",
                    "sense_definition": "Wordt gebruikt om woorden, zinsdelen of zinnen met elkaar te verbinden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader <w>en</w> moeder gaan samen op vakantie.",
                            "target_lang_translations": {
                                "ru": "Мой отец <w>и</w> мать едут вместе в отпуск.",
                                "en": "My father <w>and</w> mother are going on vacation together."
                            }
                        },
                        {
                            "text": "Hij stond op, pakte zijn jas <w>en</w> liep naar de deur.",
                            "target_lang_translations": {
                                "ru": "Он встал, взял пальто <w>и</w> направился к двери.",
                                "en": "He stood up, grabbed his coat <w>and</w> walked to the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "benevens",
                        "tevens",
                        "plus"
                    ],
                    "common_phrases": [
                        "af en toe",
                        "en zo",
                        "en dergelijke",
                        "kant en klaar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для соединения слов, частей предложения или целых предложений.",
                        "en": "Used to connect words, phrases, or sentences."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "союз"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "and",
                                "target_lang_sense_clarification": "conjunction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8484bf9-655c-3df4-ba91-c47468b56edb",
                    "sense_definition": "Wordt gebruikt om getallen bij elkaar op te tellen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twee <w>en</w> twee is vier.",
                            "target_lang_translations": {
                                "ru": "Два <w>и</w> два — четыре.",
                                "en": "Two <w>and</w> two is four."
                            }
                        },
                        {
                            "text": "Als je vijf <w>en</w> zeven optelt, krijg je twaalf.",
                            "target_lang_translations": {
                                "ru": "Если сложить пять <w>и</w> семь, получится двенадцать.",
                                "en": "If you add five <w>and</w> seven, you get twelve."
                            }
                        }
                    ],
                    "synonyms": [
                        "plus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при сложении чисел.",
                        "en": "Used to add numbers together."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "плюс"
                            },
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "математическое действие"
                            }
                        ],
                        "en": [
                            {

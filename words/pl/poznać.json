            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c638676a-30c3-3181-bf93-668c27b68d77",
                    "sense_definition": "Nawiązać z kimś znajomość, spotkać kogoś po raz pierwszy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo miło mi cię <poznać>.",
                            "target_lang_translations": {
                                "ru": "Мне очень приятно с тобой <познакомиться>."
                            }
                        },
                        {
                            "text": "Chciałbym <poznać> twoich przyjaciół ze szkoły.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <познакомиться> с твоими школьными друзьями."
                            }
                        },
                        {
                            "text": "Gdzie <poznałeś> swoją żonę?",
                            "target_lang_translations": {
                                "ru": "Где ты <познакомился> со своей женой?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zapoznać się"
                    ],
                    "common_phrases": [
                        "miło cię poznać",
                        "poznać nowych ludzi",
                        "chcieć kogoś poznać",
                        "warto go poznać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завязать знакомство с кем-либо, встретить кого-то впервые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "познакомиться",
                                "target_lang_sense_clarification": "завязать знакомство с кем-либо"
                            },
                            {
                                "target_lang_word": "узнать",
                                "target_lang_sense_clarification": "узнать человека поближе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c563599-1c1b-366f-8a60-a665c27f2a4c",
                    "sense_definition": "Zdobyć wiedzę na jakiś temat lub doświadczyć czegoś bezpośrednio.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróże to najlepszy sposób, aby <poznać> świat.",
                            "target_lang_translations": {
                                "ru": "Путешествия — это лучший способ <познать> мир."
                            }
                        },
                        {
                            "text": "Musisz <poznać> zasady gry, zanim zaczniesz w nią grać.",
                            "target_lang_translations": {
                                "ru": "Ты должен <узнать> правила игры, прежде чем начнешь в нее играть."
                            }
                        },
                        {
                            "text": "W końcu <poznaliśmy> całą prawdę o tym wydarzeniu.",
                            "target_lang_translations": {
                                "ru": "В конце концов мы <узнали> всю правду об этом событии."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapoznać się",
                        "zapoznać się",
                        "zapoznać się"
                    ],
                    "common_phrases": [
                        "poznać prawdę",
                        "poznać świat",
                        "poznać smak",
                        "poznać historię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить знания по какой-либо теме или испытать что-то на собственном опыте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узнать",
                                "target_lang_sense_clarification": "получить информацию, выяснить"
                            },
                            {
                                "target_lang_word": "познать",
                                "target_lang_sense_clarification": "постичь, глубоко изучить или испытать"
                            },
                            {
                                "target_lang_word": "ознакомиться",
                                "target_lang_sense_clarification": "получить общее представление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "990d4192-96a1-3570-883a-c52b31871ea8",
                    "sense_definition": "Zidentyfikować kogoś lub coś na podstawie wyglądu, głosu lub innych cech.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmieniłaś fryzurę i prawie cię nie <poznałem> na ulicy.",
                            "target_lang_translations": {
                                "ru": "Ты изменила прическу, и я почти не <узнал> тебя на улице."
                            }
                        },
                        {
                            "text": "Można go łatwo <poznać> po charakterystycznym głosie.",
                            "target_lang_translations": {
                                "ru": "Его можно легко <узнать> по характерному голосу."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpoznać się",
                        "rozpoznać się"
                    ],
                    "common_phrases": [
                        "nie poznać kogoś",
                        "trudno go poznać",
                        "dać się poznać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идентифицировать кого-то или что-то по внешности, голосу или другим признакам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узнать",
                                "target_lang_sense_clarification": "опознать знакомый объект или человека"
                            },
                            {
                                "target_lang_word": "распознать",
                                "target_lang_sense_clarification": "различить, идентифицировать по признакам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f3ec3e0-0fed-3395-8974-0d2867b00299",
                    "sense_definition": "Domyślić się czegoś lub wywnioskować coś na podstawie obserwacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po jego minie można <poznać>, że jest niezadowolony z wyników.",
                            "target_lang_translations": {
                                "ru": "По его лицу можно <понять>, что он недоволен результатами."
                            }
                        },
                        {
                            "text": "Jak <poznać>, że owoce są już dojrzałe i gotowe do jedzenia?",
                            "target_lang_translations": {
                                "ru": "Как <определить>, что фрукты уже спелые и готовы к употреблению?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "poznać po oczach",
                        "poznać po wyglądzie",
                        "poznać po głosie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Догадаться о чем-либо или сделать вывод на основе наблюдений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понять",
                                "target_lang_sense_clarification": "сделать вывод, осознать"
                            },
                            {
                                "target_lang_word": "определить",
                                "target_lang_sense_clarification": "выяснить по признакам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cbe973b-3166-3d49-8d46-084aa3c1713d",
                    "sense_definition": "Mieć wiedzę i doświadczenie pozwalające na trafną ocenę czegoś (używane z zaimkiem „się” i przyimkiem „na”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cognitive_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek świetnie <poznał> się na ludziach i rzadko się mylił.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка отлично <разбирался> в людях и редко ошибался."
                            }
                        },
                        {
                            "text": "Widać, że nie <poznałeś> się na tym żarcie.",
                            "target_lang_translations": {
                                "ru": "Видно, что ты не <понял> эту шутку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poznać się na czymś",
                        "poznać się na ludziach",
                        "poznać się na żartach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь знания и опыт, позволяющие верно оценить что-либо (используется с возвратным местоимением «się» и предлогом «na»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбираться",
                                "target_lang_sense_clarification": "быть знатоком в чем-либо"
                            },
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "смыслить, знать толк"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "frequency": "Low",
                    "semantic_group_id": "untruth_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli założenie jest błędne, wynik logiczny to <w>fałsz</w>.",
                            "target_lang_translations": {
                                "ru": "Если предположение ошибочно, логический результат — <w>ложь</w>."
                            }

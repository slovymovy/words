    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21cbca15-c9ce-3eed-ad37-e205656dce15",
                    "sense_definition": "A person or organization legally responsible for managing money, property, or business affairs for another person or group.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>trustee</w> has a legal duty to act in the best interests of the beneficiaries."
                        },
                        {
                            "text": "She was appointed as a <w>trustee</w> to oversee the distribution of the charity's funds."
                        }
                    ],
                    "synonyms": [
                        "fiduciary",
                        "administrator",
                        "custodian",
                        "steward"
                    ],
                    "antonyms": [
                        "beneficiary"
                    ],
                    "common_phrases": [
                        "board of trustees",
                        "appoint a trustee",
                        "public trustee"
                    ]
                },
                {
                    "sense_id": "8f6c3e13-ac47-3d6c-9164-89a6c95a771a",
                    "sense_definition": "A person holding a debtor's property or funds that have been legally attached for the benefit of a creditor.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The court ordered the <w>trustee</w> to freeze the assets until the debt dispute was resolved."
                        },
                        {
                            "text": "As a <w>trustee</w> in this process, the bank cannot release the funds to the account holder."
                        }
                    ],
                    "synonyms": [
                        "garnishee"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb03080f-6937-3967-beb1-1360d0e5a095",
                    "sense_definition": "To place property or assets into the care of a trustee.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy businessman decided to <w>trustee</w> his estate to avoid future legal complications."
                        },
                        {
                            "text": "All pension assets must be <w>trusteed</w> separately from the company's own funds."
                        }
                    ],
                    "synonyms": [
                        "entrust"
                    ]
                },
                {
                    "sense_id": "66be278e-36f7-3cb5-a08c-5589895126e9",
                    "sense_definition": "To legally seize or attach a debtor's wages or property held by a third party.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The creditor filed a motion to <w>trustee</w> the defendant's bank account."
                        },
                        {
                            "text": "If you do not pay the judgment, they may attempt to <w>trustee</w> your wages."
                        }
                    ],
                    "synonyms": [
                        "garnish"
                    ]
                }
            ]
        }
    ]

                    "sense_id": "5905bdf2-d2cb-32b5-8ea1-8ee34deeb2f5",
                    "sense_definition": "Een groot land in Centraal-Afrika, officieel de Democratische Republiek Congo, met Kinshasa als hoofdstad.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "country",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mijn buurman komt oorspronkelijk uit <w>Congo</w>.",
                            "target_lang_translations": {
                                "ru": "Мой сосед родом из <w>Конго</w>."
                            }
                        },
                        {
                            "text": "We gaan volgend jaar op reis naar <w>Congo</w>.",
                            "target_lang_translations": {
                                "ru": "В следующем году мы поедем в <w>Конго</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Congo-Kinshasa",
                        "Zaïre"
                    ],
                    "common_phrases": [
                        "in Congo",
                        "naar Congo",
                        "uit Congo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное государство в Центральной Африке, официально Демократическая Республика Конго, со столицей в Киншасе."
                    },
                    "translations": {
                        "ru": [

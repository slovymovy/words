    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "444fb3f5-d040-31f8-9bbc-9f5289812248",
                    "sense_definition": "Отсутствие сведений о чём-либо или ясности в каком-либо деле; состояние неопределённости.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "uncertainty_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго находились в полной <w>неизвестности</w> относительно планов руководства.",
                            "target_lang_translations": {
                                "en": "We were in complete <w>uncertainty</w> regarding the management's plans for a long time."
                            }
                        },
                        {
                            "text": "Эта пугающая <w>неизвестность</w> заставляла нас нервничать и переживать.",
                            "target_lang_translations": {
                                "en": "This frightening <w>uncertainty</w> made us nervous and worried."
                            }
                        }
                    ],
                    "synonyms": [
                        "неосведомлённость"
                    ],
                    "antonyms": [
                        "осведомлённость",
                        "знание"
                    ],
                    "common_phrases": [
                        "полная неизвестность",
                        "держать в неизвестности",
                        "пугающая неизвестность",
                        "мучиться в неизвестности"
                    ],
                    "target_lang_definitions": {
                        "en": "A lack of information or clarity about something; a state of uncertainty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "uncertainty",
                                "target_lang_sense_clarification": "state of doubt or lack of information"
                            },
                            {
                                "target_lang_word": "the unknown",
                                "target_lang_sense_clarification": "something that is not known or familiar"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee51919d-5a2d-3068-b2a6-9db22a6a0453",
                    "sense_definition": "Отсутствие известности, славы или признания в обществе.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "obscurity_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Талантливый художник умер в нищете и <w>неизвестности</w>, не дождавшись признания.",
                            "target_lang_translations": {
                                "en": "The talented artist died in poverty and <w>obscurity</w>, never receiving recognition."
                            }
                        },
                        {
                            "text": "После громкого успеха актёр снова погрузился в <w>неизвестность</w>.",
                            "target_lang_translations": {
                                "en": "After loud success, the actor plunged back into <w>obscurity</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "безвестность"
                    ],
                    "antonyms": [
                        "слава",
                        "известность"
                    ],
                    "common_phrases": [
                        "жить в неизвестности",
                        "уйти в неизвестность",
                        "выбраться из неизвестности"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being unknown, without fame or public recognition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obscurity",
                                "target_lang_sense_clarification": "state of being unknown to the public"
                            }
                        ]
                    }
                }
            ]
        }
    ]

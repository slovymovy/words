                {
                    "sense_id": "c129de24-bed0-3925-899e-497aeb9ed7ee",
                    "sense_definition": "The plural form of the proper noun <w>John</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plural_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "There are three <w>Johns</w> in my history class.",
                            "target_lang_translations": {
                                "ru": "В моем классе истории три <w>Джона</w>."
                            }
                        },
                        {
                            "text": "I know several <w>Johns</w>, so I have to use their last names.",
                            "target_lang_translations": {
                                "ru": "Я знаю нескольких <w>Джонов</w>, поэтому мне приходится называть их по фамилиям."
                            }
                        }
                    ],
                    "common_phrases": [

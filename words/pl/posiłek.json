            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21707702-2e09-3823-a9c2-d80f0957c8e3",
                    "sense_definition": "Jedzenie przygotowane do spożycia, zazwyczaj o określonej porze dnia",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Śniadanie to najważniejszy <w>posiłek</w> w ciągu całego dnia.",
                            "target_lang_translations": {
                                "ru": "Завтрак — это самый важный <w>приём пищи</w> за весь день."
                            }
                        },
                        {
                            "text": "Po długiej podróży każdy marzył o tym, by zjeść ciepły <w>posiłek</w>.",
                            "target_lang_translations": {
                                "ru": "После долгого путешествия каждый мечтал о том, чтобы поесть горячей <w>еды</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "strawa"
                    ],
                    "common_phrases": [
                        "ciepły posiłek",
                        "główny posiłek",
                        "przygotować posiłek",
                        "zjeść posiłek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Еда, приготовленная для употребления, обычно в определенное время дня"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём пищи",
                                "target_lang_sense_clarification": "отдельный случай еды (завтрак, обед и т.д.)"
                            },
                            {
                                "target_lang_word": "еда",
                                "target_lang_sense_clarification": "пища, кушанье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d87562f0-afa8-348e-8a67-afbbd8391189",
                    "sense_definition": "Czynność spożywania jedzenia",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trakcie <w>posiłku</w> nie powinniśmy rozmawiać przez telefon.",
                            "target_lang_translations": {
                                "ru": "Во время <w>еды</w> не следует разговаривать по телефону."
                            }
                        },
                        {
                            "text": "Krótka przerwa na <w>posiłek</w> pozwoliła pracownikom odpocząć.",
                            "target_lang_translations": {
                                "ru": "Короткий перерыв на <w>еду</w> позволил работникам отдохнуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w trakcie posiłku",
                        "przerwa na posiłek",
                        "modlitwa przed posiłkiem",
                        "do posiłku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс употребления пищи"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "еда",
                                "target_lang_sense_clarification": "процесс питания"
                            },
                            {
                                "target_lang_word": "трапеза",
                                "target_lang_sense_clarification": "торжественный или формальный приём пищи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aec6d069-ac46-37dc-904a-e8aa8950999b",
                    "sense_definition": "Dodatkowe oddziały wojska lub zaopatrzenie wysłane jako wsparcie",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "military_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Generał natychmiast wezwał <w>posiłki</w>, aby utrzymać zagrożoną pozycję.",
                            "target_lang_translations": {
                                "ru": "Генерал немедленно вызвал <w>подкрепление</w>, чтобы удержать позицию, находящуюся под угрозой."
                            }
                        },
                        {
                            "text": "Oczekiwane <w>posiłki</w> dotarły na front dopiero nad ranem.",
                            "target_lang_translations": {
                                "ru": "Ожидаемое <w>подкрепление</w> прибыло на фронт только под утро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wezwać posiłki",
                        "nadejście posiłków",
                        "czekać na posiłki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительные войска или снабжение, отправленные в качестве поддержки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкрепление",
                                "target_lang_sense_clarification": "военная помощь"
                            }
                        ]
                    }
                }
            ]
        }
    ]

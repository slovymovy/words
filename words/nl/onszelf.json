{
    "word_family": [
        "ons",
        "zelf",
        "wij"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5331a599-318f-3c4d-b6a6-1840e23b3921",
                    "sense_definition": "Het wederkerend voornaamwoord van de eerste persoon meervoud, dat terugverwijst naar het onderwerp 'wij' of 'we'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben <w>onszelf</w> beloofd om vaker te gaan sporten.",
                            "target_lang_translations": {
                                "ru": "Мы пообещали <w>себе</w> чаще заниматься спортом."
                            }
                        },
                        {
                            "text": "In de spiegel zagen we <w>onszelf</w> en moesten we lachen.",
                            "target_lang_translations": {
                                "ru": "В зеркале мы увидели <w>себя</w> и рассмеялись."
                            }
                        }
                    ],
                    "synonyms": [
                        "ons"
                    ],
                    "antonyms": [
                        "elkaar"
                    ],
                    "common_phrases": [
                        "bij onszelf",
                        "voor onszelf",
                        "onszelf zijn",
                        "over onszelf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратное местоимение первого лица множественного числа, которое отсылает к подлежащему \"мы\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "себе",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc57033f-0046-3acf-8cff-32d32be2e541",
                    "sense_definition": "De versterkte vorm van <w>ons</w>, gebruikt om nadruk te leggen op de eigen groep, vaak in contrast met anderen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij moeten <w>onszelf</w> de schuld geven en niemand anders.",
                            "target_lang_translations": {
                                "ru": "Мы должны винить <w>самих себя</w>, а не кого-то другого."
                            }
                        },
                        {
                            "text": "De directeur kwam het persoonlijk aan <w>onszelf</w> vertellen.",
                            "target_lang_translations": {
                                "ru": "Директор пришел рассказать это лично <w>нам самим</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "anderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Усиленная форма местоимения \"нас\", используемая для акцентирования внимания на собственной группе, часто в противовес другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самих себя",
                                "target_lang_sense_clarification": "усиление действия на себя"
                            },
                            {
                                "target_lang_word": "нас самих",
                                "target_lang_sense_clarification": "именно нас, лично нас"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
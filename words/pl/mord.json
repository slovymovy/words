                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten okrutny <w>mord</w> wstrząsnął całą okolicą i wywołał strach mieszkańców.",
                            "target_lang_translations": {
                                "ru": "Это жестокое <w>убийство</w> потрясло всю округу и вызвало страх у жителей."
                            }
                        },
                        {
                            "text": "Do <w>mordów</w> doszło również na terenie szpitala wojskowego.",
                            "target_lang_translations": {
                                "ru": "<w>Убийства</w> произошли также на территории военного госпиталя."
                            }
                        }
                    ],
                    "synonyms": [
                        "morderstwo",
                        "zabójstwo"
                    ],
                    "common_phrases": [
                        "masowy mord",
                        "mord rytualny",
                        "dokonać mordu"
                    ],
                    "target_lang_definitions": {
                        "ru": "умышленное, часто жестокое лишение кого-либо жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убийство",
                                "target_lang_sense_clarification": "лишение жизни"
                            },
                            {
                                "target_lang_word": "резня",
                                "target_lang_sense_clarification": "массовое или жестокое убийство"
                            }

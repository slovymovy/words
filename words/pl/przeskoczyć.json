            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b0488eb8-cca4-3fce-b448-0b3ffe79e126",
                    "sense_definition": "Wykonać skok ponad przeszkodą lub przemieścić się skokiem z jednego miejsca na drugie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej zwinnie <w>przeskoczył</w> przez wysoki mur i uciekł policji.",
                            "target_lang_translations": {
                                "ru": "Вор ловко <w>перепрыгнул</w> через высокую стену и убежал от полиции."
                            }
                        },
                        {
                            "text": "Mój pies bez problemu <w>przeskoczył</w> przez kałużę na chodniku.",
                            "target_lang_translations": {
                                "ru": "Моя собака без проблем <w>перепрыгнула</w> через лужу на тротуаре."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokonać"
                    ],
                    "common_phrases": [
                        "przeskoczyć przez płot",
                        "przeskoczyć przez mur",
                        "przeskoczyć rów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить прыжок через препятствие или переместиться прыжком с одного места на другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перепрыгнуть",
                                "target_lang_sense_clarification": "через препятствие"
                            },
                            {
                                "target_lang_word": "перескочить",
                                "target_lang_sense_clarification": "быстрым движением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a443ae7a-fc64-311e-96fe-ea5a36ffdcda",
                    "sense_definition": "Pominąć pewien element w ciągu lub serii, nie zatrzymując się na nim (np. stronę w książce, klasę w szkole).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "omission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń był tak zdolny, że <w>przeskoczył</w> jedną klasę w szkole podstawowej.",
                            "target_lang_translations": {
                                "ru": "Ученик был так способен, что <w>перескочил</w> через один класс в начальной школе."
                            }
                        },
                        {
                            "text": "Nudziły mnie opisy przyrody, więc <w>przeskoczyłem</w> kilka stron książki.",
                            "target_lang_translations": {
                                "ru": "Меня утомляли описания природы, поэтому я <w>перескочил</w> через несколько страниц книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "pominąć",
                        "opuścić"
                    ],
                    "common_phrases": [
                        "przeskoczyć rozdział",
                        "przeskoczyć kolejkę",
                        "przeskoczyć klasę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пропустить определенный элемент в последовательности или серии (например, страницу в книге, класс в школе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перескочить",
                                "target_lang_sense_clarification": "через что-то в ряду"
                            },
                            {
                                "target_lang_word": "пропустить",
                                "target_lang_sense_clarification": "опустить, не читая или не посещая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22411007-26a2-3f14-81e6-fb4e60119438",
                    "sense_definition": "Gwałtownie zmienić obiekt zainteresowania, temat rozmowy lub stan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas dyskusji polityk nagle <w>przeskoczył</w> na zupełnie inny, wygodniejszy dla niego temat.",
                            "target_lang_translations": {
                                "ru": "Во время дискуссии политик внезапно <w>перескочил</w> на совершенно другую, более удобную для него тему."
                            }
                        },
                        {
                            "text": "Wskaźnik temperatury nagle <w>przeskoczył</w> na czerwone pole, sygnalizując awarię.",
                            "target_lang_translations": {
                                "ru": "Индикатор температуры внезапно <w>перескочил</w> на красное поле, сигнализируя об аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmienić"
                    ],
                    "common_phrases": [
                        "przeskoczyć na inny temat",
                        "przeskoczyć na inny kanał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко сменить тему разговора, объект внимания или состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перескочить",
                                "target_lang_sense_clarification": "на другую тему"
                            },
                            {
                                "target_lang_word": "переключиться",
                                "target_lang_sense_clarification": "сменить внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4aba428d-53a2-30ff-bd97-e146162c13cc",
                    "sense_definition": "Szybko przemieścić się między dwoma punktami (często o zjawiskach fizycznych, np. iskrze).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Między dwoma przewodami elektrycznymi <w>przeskoczyła</w> jasna iskra.",
                            "target_lang_translations": {
                                "ru": "Между двумя электрическими проводами <w>проскочила</w> яркая искра."
                            }
                        },
                        {
                            "text": "Ogień błyskawicznie <w>przeskoczył</w> z płonącego drzewa na dach domu.",
                            "target_lang_translations": {
                                "ru": "Огонь молниеносно <w>перекинулся</w> с горящего дерева на крышу дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iskra przeskoczyła",
                        "ogień przeskoczył"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро переместиться между двумя точками (часто о физических явлениях, например, искре или огне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проскочить",
                                "target_lang_sense_clarification": "об искре"
                            },
                            {
                                "target_lang_word": "перекинуться",
                                "target_lang_sense_clarification": "об огне"
                            },
                            {
                                "target_lang_word": "перескочить",
                                "target_lang_sense_clarification": "переместиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34bac6a6-cb3d-30ab-882c-70900783e059",
                    "sense_definition": "Osiągnąć poziom wyższy niż ktoś inny lub poradzić sobie z czymś, co wydawało się trudne do pokonania (znaczenie przenośne).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten młody zawodnik szybko <w>przeskoczył</w> swoich starszych kolegów w rankingu.",
                            "target_lang_translations": {
                                "ru": "Этот молодой спортсмен быстро <w>обошел</w> своих старших коллег в рейтинге."
                            }
                        },
                        {
                            "text": "Nie uda nam się <w>przeskoczyć</w> tych skomplikowanych procedur w jeden dzień.",
                            "target_lang_translations": {
                                "ru": "Нам не удастся <w>обойти</w> эти сложные процедуры за один день."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewyższyć",
                        "pokonać"
                    ],
                    "common_phrases": [
                        "przeskoczyć samego siebie",
                        "przeskoczyć poprzeczkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane w kontekście radzenia sobie z problemami"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Превзойти кого-то по уровню или справиться с чем-то трудным (переносное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойти",
                                "target_lang_sense_clarification": "превзойти кого-то"
                            },
                            {
                                "target_lang_word": "преодолеть",
                                "target_lang_sense_clarification": "справиться с трудностью"
                            }
                        ]
                    }
                }
            ]
        }
    ]

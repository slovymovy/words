    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b41e8253-7100-3ccd-b4c6-d306c59ad434",
                    "sense_definition": "Suitable or appropriate in the circumstances.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "suitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor gave an <w>apt</w> description of the complex economic situation.",
                            "target_lang_translations": {
                                "ru": "Профессор дал <w>меткое</w> описание сложной экономической ситуации."
                            }
                        },
                        {
                            "text": "It was an <w>apt</w> time to discuss the changes to the project plan.",
                            "target_lang_translations": {
                                "ru": "Это было <w>подходящее</w> время, чтобы обсудить изменения в плане проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "appropriate",
                        "suitable",
                        "fitting",
                        "apposite",
                        "felicitous"
                    ],
                    "antonyms": [
                        "inappropriate",
                        "unsuitable"
                    ],
                    "common_phrases": [
                        "apt description",
                        "apt metaphor",
                        "apt comment",
                        "apt comparison"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий или уместный в данных обстоятельствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уместный",
                                "target_lang_sense_clarification": "соответствующий обстановке"
                            },
                            {
                                "target_lang_word": "меткий",
                                "target_lang_sense_clarification": "точный, о замечании или описании"
                            },
                            {
                                "target_lang_word": "подходящий",
                                "target_lang_sense_clarification": "годный, применимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3618fe13-698a-3c0e-afdf-799e81b60111",
                    "sense_definition": "Having a natural tendency to do something; likely.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "tendency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Without a warm coat, you are <w>apt</w> to catch a cold in this weather.",
                            "target_lang_translations": {
                                "ru": "Без теплого пальто вы <w>рискуете</w> простудиться в такую погоду."
                            }
                        },
                        {
                            "text": "He is <w>apt</w> to forget his keys when he leaves the house in a hurry.",
                            "target_lang_translations": {
                                "ru": "Он <w>склонен</w> забывать ключи, когда в спешке выходит из дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "inclined",
                        "prone",
                        "liable",
                        "disposed"
                    ],
                    "antonyms": [
                        "unlikely"
                    ],
                    "common_phrases": [
                        "apt to be",
                        "apt to forget",
                        "apt to happen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий естественную склонность к чему-либо; вероятный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склонный",
                                "target_lang_sense_clarification": "имеющий предрасположенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "762fb0a0-bbcc-3ba8-8dbc-0164a192f9b1",
                    "sense_definition": "Quick to learn or understand.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "intelligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was an <w>apt</w> student who mastered the difficult subject very quickly.",
                            "target_lang_translations": {
                                "ru": "Она была <w>способной</w> ученицей, которая очень быстро освоила этот трудный предмет."
                            }
                        },
                        {
                            "text": "The boy proved to be an <w>apt</w> learner during his apprenticeship.",
                            "target_lang_translations": {
                                "ru": "Мальчик оказался <w>понятливым</w> учеником во время стажировки."
                            }
                        }
                    ],
                    "synonyms": [
                        "quick",
                        "clever",
                        "sharp",
                        "bright"
                    ],
                    "antonyms": [
                        "slow",
                        "dull"
                    ],
                    "common_phrases": [
                        "apt pupil",
                        "apt student",
                        "apt scholar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро обучающийся или схватывающий суть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "способный",
                                "target_lang_sense_clarification": "одаренный, талантливый"
                            },
                            {
                                "target_lang_word": "понятливый",
                                "target_lang_sense_clarification": "быстро соображающий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf5af166-bebc-3960-868b-3388054d54da",
                    "sense_definition": "Abbreviation of <w>apartment</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "housing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please send the package to 123 Main Street, <w>Apt</w> 4B.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте посылку на Мэйн-стрит, 123, <w>кв.</w> 4B."
                            }
                        },
                        {
                            "text": "They live in a small <w>apt</w> near the university campus.",
                            "target_lang_translations": {
                                "ru": "Они живут в маленькой <w>квартире</w> недалеко от университетского кампуса."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"apartment\" (квартира)."

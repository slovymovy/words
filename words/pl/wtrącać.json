{
    "word_family": [
        "wtrącenie",
        "wtręt"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "58fb4d78-30f4-3a08-bcfc-1034e2d03027",
                    "sense_definition": "Angażować się w cudze sprawy lub rozmowy, często nie będąc o to proszonym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię, gdy ktoś nieproszony <w>wtrąca</w> się w moje prywatne życie.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда кто-то непрошеный <w>вмешивается</w> в мою личную жизнь."
                            }
                        },
                        {
                            "text": "Proszę, nie <w>wtrącaj</w> się do naszej rozmowy, to sprawa między nami.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>вмешивайся</w> в наш разговор, это дело между нами."
                            }
                        }
                    ],
                    "synonyms": [
                        "ingerować",
                        "mieszać się"
                    ],
                    "common_phrases": [
                        "nie wtrącaj się",
                        "wtrącać się w nieswoje sprawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вмешиваться в чужие дела или разговоры, часто без приглашения (обычно используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вмешиваться",
                                "target_lang_sense_clarification": "принимать участие в чужих делах"
                            },
                            {
                                "target_lang_word": "лезть",
                                "target_lang_sense_clarification": "разговорное; назойливо вмешиваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d91b5cc-d61b-33cd-b43b-8b7da2ca43fc",
                    "sense_definition": "Wypowiadać krótką uwagę lub komentarz, przerywając tok czyjejś wypowiedzi.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wykładu profesor często <w>wtrącał</w> zabawne anegdoty.",
                            "target_lang_translations": {
                                "ru": "Во время лекции профессор часто <w>вставлял</w> забавные анекдоты."
                            }
                        },
                        {
                            "text": "Chciałbym tylko <w>wtrącić</w> jedno słowo wyjaśnienia do tego, co powiedziałeś.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы только <w>вставить</w> одно слово пояснения к тому, что ты сказал."
                            }
                        }
                    ],
                    "synonyms": [
                        "przerywać",
                        "wchodzić w słowo"
                    ],
                    "common_phrases": [
                        "wtrącać swoje trzy grosze",
                        "wtrącać uwagę",
                        "wtrącać słowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать краткое замечание или комментарий, прерывая ход чьей-то речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставлять",
                                "target_lang_sense_clarification": "добавлять замечание или слово в разговор"
                            },
                            {
                                "target_lang_word": "ввертывать",
                                "target_lang_sense_clarification": "разговорное; вставлять слово или фразу к месту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdaf3b40-6f44-3af3-ba1a-ed92936f5eba",
                    "sense_definition": "Powodować znalezienie się kogoś w zamknięciu (np. w więzieniu) lub w trudnym położeniu życiowym.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd postanowił <w>wtrącić</w> oskarżonego do więzienia na pięć lat.",
                            "target_lang_translations": {
                                "ru": "Суд постановил <w>бросить</w> обвиняемого в тюрьму на пять лет."
                            }
                        },
                        {
                            "text": "Nagła choroba może <w>wtrącić</w> całą rodzinę w poważne kłopoty finansowe.",
                            "target_lang_translations": {
                                "ru": "Внезапная болезнь может <w>ввергнуть</w> всю семью в серьезные финансовые проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wpierdalać się",
                        "wpieprzać się"
                    ],
                    "common_phrases": [
                        "wtrącać do więzienia",
                        "wtrącać do lochu",
                        "wtrącać w nędzę",
                        "wtrącać w rozpacz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать кого-либо в заключение (например, в тюрьму) или ставить в тяжелое жизненное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ввергать",
                                "target_lang_sense_clarification": "приводить в тяжелое состояние (в нищету, в отчаяние)"
                            },
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "помещать в тюрьму (экспрессивное)"
                            },
                            {
                                "target_lang_word": "заключать",
                                "target_lang_sense_clarification": "помещать под стражу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
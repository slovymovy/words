                {
                    "sense_id": "278485b7-372f-39ce-9a12-53192f69f13a",
                    "sense_definition": "Een wild, vleesetend zoogdier (Canis lupus) dat in groepen jaagt en voorkomt op het noordelijk halfrond.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dat gebied kun je vaak <w>wolven</w> zien lopen.",
                            "target_lang_translations": {
                                "en": "In that area you can often see <w>wolves</w> walking.",
                                "ru": "В этой местности часто можно увидеть <w>волков</w>."
                            }
                        },
                        {
                            "text": "De <w>wolf</w> is de wilde voorouder van de hond.",
                            "target_lang_translations": {
                                "en": "The <w>wolf</w> is the wild ancestor of the dog.",
                                "ru": "<w>Волк</w> — это дикий предок собаки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een wolf in schaapskleren",
                        "hongerig als een wolf",
                        "huilen met de wolven in het bos"
                    ],
                    "target_lang_definitions": {
                        "en": "A wild, carnivorous mammal (Canis lupus) that hunts in packs and is found in the northern hemisphere.",
                        "ru": "Дикое плотоядное млекопитающее (Canis lupus), которое охотится стаями и обитает в Северном полушарии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wolf",
                                "target_lang_sense_clarification": "animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "волк",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                }
            ]
        },

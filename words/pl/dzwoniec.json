    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c4a1982-8520-3135-8eec-0b197508ebbe",
                    "sense_definition": "Mały ptak śpiewający z rodziny łuszczaków, charakteryzujący się oliwkowozielonym upierzeniem z żółtymi elementami na skrzydłach i ogonie oraz masywnym dziobem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie często pojawia się <w>dzwoniec</w>, szukając nasion w karmniku.",
                            "target_lang_translations": {
                                "ru": "В нашем саду часто появляется <w>зеленушка</w> в поисках семян в кормушке."
                            }
                        },
                        {
                            "text": "Marzeniem młodego ornitologa było zobaczyć, jak <w>dzwońce</w> budują gniazda w gęstych krzewach.",
                            "target_lang_translations": {
                                "ru": "Мечтой молодого орнитолога было увидеть, как <w>зеленушки</w> строят гнезда в густых кустах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "śpiew dzwońca",
                        "gniazdo dzwońca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая певчая птица семейства вьюрковых, отличающаяся оливково-зеленым оперением с желтыми элементами на крыльях и хвосте, а также массивным клювом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зеленушка",
                                "target_lang_sense_clarification": "птица"
                            },
                            {
                                "target_lang_word": "обыкновенная зеленушка",
                                "target_lang_sense_clarification": "вид птиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e3206a3-a009-3287-bc5b-3bef33abff93",
                    "sense_definition": "Pospolite, lekceważące określenie osoby mało inteligentnej, naiwnej lub zachowującej się bezmyślnie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>dzwoniec</w> znowu zapomniał zamknąć bramę na klucz, przez co uciekł nam pies.",
                            "target_lang_translations": {
                                "ru": "Этот <w>олух</w> снова забыл запереть ворота на ключ, из-за чего у нас убежала собака."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>dzwońcem</w> i zastanów się chwilę, zanim coś powiesz.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>болваном</w> и подумай немного, прежде чем что-то сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "głupek",
                        "tępak",
                        "baran",
                        "cymbał",
                        "bęcwał",
                        "matoł"
                    ],
                    "common_phrases": [
                        "skończony dzwoniec",
                        "głupi dzwoniec"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "obraźliwe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, пренебрежительное название недалекого, наивного или бездумно ведущего себя человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "олух",
                                "target_lang_sense_clarification": "глупый человек"
                            },
                            {
                                "target_lang_word": "болван",
                                "target_lang_sense_clarification": "тупица"
                            },
                            {
                                "target_lang_word": "дурень",
                                "target_lang_sense_clarification": "просторечное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8a69868-791e-3fd4-a68c-47d053405ae7",
                    "sense_definition": "Osoba pełniąca funkcję odźwiernego lub portiera, dbająca o otwieranie drzwi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "professions_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>dzwoniec</w> ukłonił się nisko i otworzył ciężkie wrota przed gośćmi.",
                            "target_lang_translations": {
                                "ru": "Старый <w>швейцар</w> низко поклонился и открыл тяжелые ворота перед гостями."
                            }
                        },
                        {
                            "text": "W dawnych pałacach przy wejściu zawsze czuwał <w>dzwoniec</w> gotowy do obsługi przybywających.",
                            "target_lang_translations": {
                                "ru": "В старинных дворцах у входа всегда дежурил <w>привратник</w>, готовый услужить прибывающим."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, выполняющий обязанности швейцара или привратника, отвечающий за открывание дверей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "швейцар",
                                "target_lang_sense_clarification": "служащий при входе"
                            },
                            {
                                "target_lang_word": "привратник",
                                "target_lang_sense_clarification": "сторож у ворот"
                            }
                        ]
                    }
                }
            ]
        }
    ]

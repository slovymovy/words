                {
                    "sense_id": "4186c949-ee28-3972-a250-896132d0710f",
                    "sense_definition": "Een plaats met tenten of eenvoudige huisjes waar mensen tijdelijk verblijven, vaak voor vakantie of als activiteit van een vereniging.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verblijfplaats",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen gaan volgende week op <w>kamp</w> met de scouting.",
                            "target_lang_translations": {
                                "ru": "Дети на следующей неделе едут в <w>лагерь</w> со скаутами.",
                                "en": "The children are going to <w>camp</w> with the scouts next week."
                            }
                        },
                        {
                            "text": "We zochten een vlak stuk grond om ons <w>kamp</w> op te zetten.",
                            "target_lang_translations": {
                                "ru": "Мы искали ровный участок земли, чтобы разбить наш <w>лагерь</w>.",
                                "en": "We looked for a flat piece of ground to set up our <w>camp</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op kamp gaan",
                        "kamp opslaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место с палатками или простыми домиками для временного проживания, часто для отдыха или мероприятий организации.",
                        "en": "A place with tents or simple cabins where people stay temporarily, often for holiday or as an organizational activity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лагерь",
                                "target_lang_sense_clarification": "место отдыха или сборов"

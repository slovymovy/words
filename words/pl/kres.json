    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec04c835-e847-365b-8eba-fbb893213dca",
                    "sense_definition": "Moment zakończenia istnienia lub trwania czegoś; ostateczny koniec.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "end_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długoletni konflikt wreszcie dobiegł <w>kresu</w> dzięki negocjacjom.",
                            "target_lang_translations": {
                                "ru": "Многолетний конфликт наконец подошёл к <w>концу</w> благодаря переговорам."
                            }
                        },
                        {
                            "text": "Każde życie ma swój <w>kres</w>, ale pamięć o człowieku pozostaje.",
                            "target_lang_translations": {
                                "ru": "У каждой жизни есть свой <w>конец</w>, но память о человеке остаётся."
                            }
                        }
                    ],
                    "synonyms": [
                        "schyłek"
                    ],
                    "common_phrases": [
                        "położyć kres",
                        "dobiegać kresu",
                        "u kresu sił",
                        "kres życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент прекращения существования или длительности чего-либо; окончательный конец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "завершение, финал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81ceb37e-2471-39ec-aff6-fd80852e601d",
                    "sense_definition": "Ostatni punkt, krawędź lub granica jakiegoś obszaru przestrzennego.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "boundary_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędrowcy dotarli do <w>kresu</w> lasu i zobaczyli rozległą dolinę.",
                            "target_lang_translations": {
                                "ru": "Путники добрались до <w>края</w> леса и увидели обширную долину."
                            }
                        },
                        {
                            "text": "Dom moich dziadków stał na samym <w>kresie</w> wsi.",
                            "target_lang_translations": {
                                "ru": "Дом моих дедушки и бабушки стоял на самом <w>краю</w> деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "granica",
                        "kraniec"
                    ],
                    "common_phrases": [
                        "kres wędrówki",
                        "na kresach świata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последняя точка, край или граница какой-либо пространственной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "предел, кромка"
                            },
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "пространственный предел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0be8754c-0bd5-32be-9c37-7f1dfc3299fd",
                    "sense_definition": "W matematyce: największe z dolnych lub najmniejsze z górnych ograniczeń danego zbioru.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student musiał wyznaczyć <w>kres</w> górny podanego zbioru liczb.",
                            "target_lang_translations": {
                                "ru": "Студент должен был определить верхнюю <w>грань</w> заданного множества чисел."
                            }
                        },
                        {
                            "text": "Istnienie <w>kresu</w> dolnego jest kluczowym elementem tego dowodu.",
                            "target_lang_translations": {
                                "ru": "Существование нижней <w>грани</w> является ключевым элементом этого доказательства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kres górny",
                        "kres dolny"
                    ],
                    "target_lang_definitions": {
                        "ru": "В математике: наибольшее из нижних или наименьшее из верхних ограничений данного множества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грань",
                                "target_lang_sense_clarification": "математический термин: точная граница"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96a27a94-93bd-3313-b6b0-0e4037a8ced9",
                    "sense_definition": "Forma gramatyczna (dopełniacz liczby mnogiej) rzeczownika <w>kresa</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na starym rysunku nie było widać wyraźnych <w>kres</w>.",
                            "target_lang_translations": {
                                "ru": "На старом рисунке не было видно чётких <w>черт</w>."
                            }
                        },
                        {
                            "text": "Analiza tych białych <w>kres</w> na ekranie radaru zajęła nam chwilę.",
                            "target_lang_translations": {
                                "ru": "Анализ этих белых <w>линий</w> на экране радара занял у нас некоторое время."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of kresa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный падеж множественного числа) существительного kresa (черта)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черта",
                                "target_lang_sense_clarification": "линия, штрих"
                            },
                            {
                                "target_lang_word": "линия",
                                "target_lang_sense_clarification": "полоса"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
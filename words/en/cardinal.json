                },
                {
                    "sense_id": "d8429465-6903-3b82-b0fa-87ffe369a5d8",
                    "sense_definition": "A number that indicates quantity or the size of a set (e.g., 0, 1, 2), as opposed to position.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "numbers",
                    "name_type": "no",
                    "examples": [
                        {

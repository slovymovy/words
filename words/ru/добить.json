    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "edb267fb-1c54-35b7-9933-f56a3225c2c8",
                    "sense_definition": "Убить окончательно того, кто был ранен или ещё жив; прикончить.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "kill_destroy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотник решил не мучить раненого зверя и быстро <w>добить</w> его.",
                            "target_lang_translations": {
                                "en": "The hunter decided not to torment the wounded beast and quickly <w>finish</w> it <w>off</w>."
                            }
                        },
                        {
                            "text": "Рыбак ударил рыбу палкой, чтобы <w>добить</w> её.",
                            "target_lang_translations": {
                                "en": "The fisherman hit the fish with a stick to <w>finish</w> it <w>off</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "прикончить"
                    ],
                    "antonyms": [
                        "оживить",
                        "реанимировать"
                    ],
                    "common_phrases": [
                        "добить раненого",
                        "добить зверя",
                        "добить выстрелом"
                    ],
                    "target_lang_definitions": {
                        "en": "To finally kill someone or something that is already wounded or dying; to finish off."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish off",
                                "target_lang_sense_clarification": "kill a wounded being"
                            },
                            {
                                "target_lang_word": "put out of one's misery",
                                "target_lang_sense_clarification": "end suffering"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8f0c231-5fc7-34af-b766-4fd99c01164c",
                    "sense_definition": "Полностью закончить какое-либо трудное дело, занятие или работу.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно сегодня <w>добить</w> этот отчёт, чтобы завтра отдыхать.",
                            "target_lang_translations": {
                                "en": "I need to <w>finish off</w> this report today so I can rest tomorrow."
                            }
                        },
                        {
                            "text": "Студент наконец <w>добил</w> свою курсовую работу.",
                            "target_lang_translations": {
                                "en": "The student finally <w>finished off</w> his term paper."
                            }
                        }
                    ],
                    "synonyms": [
                        "доделать",
                        "довершить"
                    ],
                    "common_phrases": [
                        "добить работу",
                        "добить проект",
                        "добить остатки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To completely finish a difficult task, job, or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish off",
                                "target_lang_sense_clarification": "complete a task"
                            },
                            {
                                "target_lang_word": "knock off",
                                "target_lang_sense_clarification": "finish work quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99555fb9-5cff-3984-8ba3-67b172ff76e9",
                    "sense_definition": "Окончательно сломить кого-либо морально или привести что-либо в полную негодность.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "kill_destroy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта плохая новость окончательно <w>добила</w> его.",
                            "target_lang_translations": {
                                "en": "This bad news finally <w>finished</w> him <w>off</w>."
                            }
                        },
                        {
                            "text": "Экономический кризис может <w>добить</w> малый бизнес в регионе.",
                            "target_lang_translations": {
                                "en": "The economic crisis could <w>destroy</w> small businesses in the region."
                            }
                        }
                    ],
                    "synonyms": [
                        "доконать",
                        "разбить"
                    ],
                    "antonyms": [
                        "воскресить"
                    ],
                    "common_phrases": [
                        "добить врага",
                        "окончательно добить"
                    ],
                    "target_lang_definitions": {
                        "en": "To finally break someone morally or bring something to complete ruin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish off",
                                "target_lang_sense_clarification": "ruin completely"
                            },
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "break morally"
                            },
                            {
                                "target_lang_word": "destroy",
                                "target_lang_sense_clarification": "ruin economically or physically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b9457d6-0c9f-3686-aef6-f21f135c91fc",
                    "sense_definition": "Перестать бить, издав последний звук (о часах или колоколах).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старинные часы на стене успели <w>добить</w> полночь.",
                            "target_lang_translations": {
                                "en": "The old clock on the wall managed to <w>finish striking</w> midnight."
                            }
                        },
                        {
                            "text": "Когда куранты <w>добили</w> двенадцать раз, начался салют.",
                            "target_lang_translations": {
                                "en": "When the chimes <w>finished striking</w> twelve times, the fireworks began."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop striking or chiming (of a clock or bell)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish striking",
                                "target_lang_sense_clarification": "clocks/bells"
                            }
                        ]
                    }
                }
            ]
        }
    ]

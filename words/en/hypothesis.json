            "senses": [
                {
                    "sense_id": "f126edc1-6ff3-3cf2-9dff-8ae30bbb82fe",
                    "sense_definition": "A proposed explanation for a phenomenon or problem that can be tested through investigation and experimentation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientists gathered data to test their <w>hypothesis</w> about climate change."
                        },
                        {
                            "text": "Our working <w>hypothesis</w> is that the disease is spread by water."
                        }
                    ],

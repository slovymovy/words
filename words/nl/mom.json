    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52f080da-4a3f-39e7-bb87-c39996c46b08",
                    "sense_definition": "Een reden die wordt opgegeven om de werkelijke bedoeling te verbergen; een bedrieglijke schijn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pretext_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertrok vroeg van het feest onder het <w>mom</w> van een dringende afspraak.",
                            "target_lang_translations": {
                                "ru": "Он ушел с вечеринки рано под <w>предлогом</w> срочной встречи.",
                                "en": "He left the party early under the <w>pretext</w> of an urgent appointment."
                            }
                        },
                        {
                            "text": "De regering voerde de nieuwe wet in onder het <w>mom</w> van veiligheid.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело новый закон под <w>видом</w> обеспечения безопасности.",
                                "en": "The government introduced the new law under the <w>guise</w> of safety."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorwendsel",
                        "dekmantel",
                        "smoes",
                        "schijn"
                    ],
                    "common_phrases": [
                        "onder het mom van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причина, указываемая для сокрытия истинных намерений; обманчивая видимость.",
                        "en": "A reason given to hide the real intention; a deceptive appearance."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предлог",
                                "target_lang_sense_clarification": "ложная причина"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "внешняя видимость, прикрытие"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "guise",
                                "target_lang_sense_clarification": "false appearance (often in 'under the guise of')"
                            },
                            {
                                "target_lang_word": "pretext",
                                "target_lang_sense_clarification": "false reason given to justify an action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57eac9e3-767d-32e5-80c5-24b9a2ed7e25",
                    "sense_definition": "Een bedekking voor het gezicht om onherkenbaar te blijven.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mask_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mysterieuze gast droeg een fluwelen <w>mom</w> tijdens het bal.",

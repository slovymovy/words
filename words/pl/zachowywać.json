                {
                    "sense_id": "5501df54-d025-31b5-93cb-c764b73409f2",
                    "sense_definition": "Postępować w określony sposób (odnosi się do zachowania ludzi lub zwierząt).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie powinni <w>zachowywać</w> się cicho podczas lekcji.",
                            "target_lang_translations": {
                                "ru": "Ученики должны <w>вести</w> себя тихо во время урока."
                            }
                        },
                        {
                            "text": "Mój pies zazwyczaj <w>zachowuje</w> się przyjaźnie wobec gości.",
                            "target_lang_translations": {
                                "ru": "Моя собака обычно <w>ведёт</w> себя дружелюбно по отношению к гостям."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprawować się",
                        "prowadzić się"
                    ],
                    "common_phrases": [
                        "zachowywać się poprawnie",
                        "zachowywać się cicho",
                        "zachowywać się dziwnie",
                        "zachowywać się grzecznie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя определённым образом (относится к поведению людей или животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести себя",
                                "target_lang_sense_clarification": "поступать каким-либо образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9938eac-cdc2-35d4-9b94-22603e4a0a6e",
                    "sense_definition": "Utrzymywać coś w niezmienionym stanie, nie dopuszczać do zniszczenia lub zmiany.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo stresującej sytuacji, staram się <w>zachowywać</w> spokój.",
                            "target_lang_translations": {
                                "ru": "Несмотря на стрессовую ситуацию, я стараюсь <w>сохранять</w> спокойствие."
                            }
                        },
                        {
                            "text": "Ten zabytkowy budynek <w>zachowuje</w> swój oryginalny styl.",
                            "target_lang_translations": {
                                "ru": "Это старинное здание <w>сохраняет</w> свой оригинальный стиль."
                            }
                        }
                    ],
                    "synonyms": [
                        "utrzymywać",
                        "konserwować"
                    ],
                    "antonyms": [
                        "zmieniać"
                    ],
                    "common_phrases": [
                        "zachowywać spokój",
                        "zachowywać zimną krew",
                        "zachowywać pozory",
                        "zachowywać ciszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживать что-либо в неизменном состоянии, не допускать разрушения или изменения."

                {
                    "sense_id": "f7523761-b4a3-396e-8970-8a9b253815d7",
                    "sense_definition": "Wymiar przedmiotu mierzony w przekroju poprzecznym, określający odległość między jego przeciwległymi powierzchniami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmierzyliśmy <w>grubość</w> deski przed jej przycięciem.",
                            "target_lang_translations": {
                                "ru": "Мы измерили <w>толщину</w> доски перед её распилом."
                            }
                        },
                        {
                            "text": "Warstwa lodu na jeziorze ma bezpieczną <w>grubość</w>.",
                            "target_lang_translations": {
                                "ru": "Слой льда на озере имеет безопасную <w>толщину</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "cienkość"
                    ],
                    "common_phrases": [
                        "grubość ściany",
                        "grubość lodu",
                        "grubość linii",
                        "całkowita grubość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размер предмета в поперечном сечении, определяющий расстояние между его противоположными поверхностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толщина",
                                "target_lang_sense_clarification": "физический размер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be8a0d64-0a0d-3d03-a97e-75dd5231c68e",
                    "sense_definition": "Cecha fizyczna człowieka lub zwierzęcia polegająca na posiadaniu dużej ilości tkanki tłuszczowej.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "body_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nadmierna <w>grubość</w> zaczęła utrudniać mu codzienne funkcjonowanie.",
                            "target_lang_translations": {
                                "ru": "Его чрезмерная <w>полнота</w> начала мешать ему в повседневной жизни."
                            }
                        },
                        {
                            "text": "Lekarz zwrócił uwagę na niepokojącą <w>grubość</w> pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач обратил внимание на тревожную <w>тучность</w> пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "otyłość",
                        "tusza",
                        "korpulencja",
                        "tęgość",
                        "pulchność"
                    ],
                    "antonyms": [
                        "chudość",
                        "szczupłość"
                    ],
                    "common_phrases": [
                        "nadmierna grubość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая особенность человека или животного, заключающаяся в наличии большого количества жировой ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полнота",
                                "target_lang_sense_clarification": "телосложение"
                            },
                            {
                                "target_lang_word": "тучность",
                                "target_lang_sense_clarification": "чрезмерная полнота"
                            },
                            {
                                "target_lang_word": "толщина",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "417bcd70-fce6-3996-8616-a91cbc85d3e0",
                    "sense_definition": "Cecha głosu lub dźwięku charakteryzująca się niskim, głębokim brzmieniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niezwykła <w>grubość</w> jego głosu idealnie pasowała do ról czarnych charakterów.",
                            "target_lang_translations": {
                                "ru": "Необычайная <w>густота</w> его голоса идеально подходила для ролей злодеев."
                            }
                        },
                        {
                            "text": "<w>Grubość</w> dźwięku kontrabasu wypełniła całą salę koncertową.",
                            "target_lang_translations": {
                                "ru": "<w>Глубина</w> звука контрабаса наполнила весь концертный зал."
                            }
                        }
                    ],

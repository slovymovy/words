            "senses": [
                {
                    "sense_id": "58c2dd7d-6746-33f0-8673-7ddf07ddc40b",
                    "sense_definition": "Действие по глаголу «объединять»; процесс создания единого целого из разрозненных частей или элементов.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для достижения общей цели необходимо <w>объединение</w> усилий всех участников команды.",
                            "target_lang_translations": {
                                "en": "To achieve the common goal, the <w>consolidation</w> of all team members' efforts is necessary."
                            }
                        },
                        {
                            "text": "Произошло <w>объединение</w> двух крупных компаний в одну корпорацию.",
                            "target_lang_translations": {
                                "en": "A <w>merger</w> of two large companies into one corporation took place."
                            }
                        }
                    ],
                    "synonyms": [
                        "слияние",
                        "интеграция",
                        "соединение",
                        "слияние"
                    ],
                    "antonyms": [
                        "разъединение",
                        "дезинтеграция",
                        "распад"
                    ],
                    "common_phrases": [
                        "объединение усилий",
                        "объединение земель",
                        "процесс объединения"
                    ],
                    "target_lang_definitions": {
                        "en": "The action or process of joining separate parts, people, or things into a single whole."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unification",
                                "target_lang_sense_clarification": "general act of uniting"
                            },
                            {
                                "target_lang_word": "merger",
                                "target_lang_sense_clarification": "of companies or organizations"
                            },
                            {
                                "target_lang_word": "consolidation",
                                "target_lang_sense_clarification": "of efforts or resources"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "105b0b6b-9e48-3f95-98e6-f272ae4552e2",
                    "sense_definition": "Организация, общество или группа, созданная из нескольких меньших групп или лиц для совместной деятельности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [

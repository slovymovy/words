    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ec97fbdb-f471-3d9c-b27e-13dabdef103f",
                    "sense_definition": "Budzący podziw, zachwyt lub zdumienie; nadzwyczajny i wspaniały.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój nowy samochód robi na wszystkich <w>niesamowite</w> wrażenie.",
                            "target_lang_translations": {
                                "ru": "Твоя новая машина производит на всех <w>невероятное</w> впечатление."
                            }
                        },
                        {
                            "text": "To był naprawdę <w>niesamowity</w> dzień pełen radości i atrakcji.",
                            "target_lang_translations": {
                                "ru": "Это был действительно <w>потрясающий</w> день, полный радости и развлечений."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezwykły"
                    ],
                    "common_phrases": [
                        "niesamowite wrażenie",
                        "niesamowita historia",
                        "niesamowity widok",
                        "niesamowity pomysł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий восхищение, восторг или изумление; необычайный и великолепный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потрясающий",
                                "target_lang_sense_clarification": "вызывающий сильное восхищение"
                            },
                            {
                                "target_lang_word": "невероятный",
                                "target_lang_sense_clarification": "трудно представимый, удивительный"
                            },
                            {
                                "target_lang_word": "удивительный",
                                "target_lang_sense_clarification": "вызывающий удивление, необыкновенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd0d414b-83cb-39e4-8cb5-be48eea8aa42",
                    "sense_definition": "Bardzo silny, intensywny, występujący w dużym nasileniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zewnątrz panował <w>niesamowity</w> upał, więc zostaliśmy w domu.",
                            "target_lang_translations": {
                                "ru": "На улице стояла <w>невероятная</w> жара, поэтому мы остались дома."
                            }
                        },
                        {
                            "text": "Poczułem <w>niesamowity</w> ból w nodze zaraz po upadku.",
                            "target_lang_translations": {
                                "ru": "Я почувствовал <w>невероятную</w> боль в ноге сразу после падения."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielki",
                        "nieopisany"
                    ],
                    "common_phrases": [
                        "niesamowity upał",
                        "niesamowity hałas",
                        "niesamowita prędkość",
                        "niesamowity głód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильный, интенсивный, проявляющийся в высокой степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невероятный",
                                "target_lang_sense_clarification": "чрезвычайно сильный (об ощущениях, явлениях)"
                            },
                            {
                                "target_lang_word": "неимоверный",
                                "target_lang_sense_clarification": "очень большой, значительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48d90117-0a55-3fdf-80e8-b74c7469adbe",
                    "sense_definition": "Tajemniczy, dziwny, budzący niepokój, lęk lub zdziwienie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym lesie panowała <w>niesamowita</w> cisza, która nas przerażała.",
                            "target_lang_translations": {
                                "ru": "В старом лесу стояла <w>жуткая</w> тишина, которая нас пугала."
                            }
                        },
                        {
                            "text": "To <w>niesamowite</w> zjawisko na niebie zaniepokoiło wszystkich mieszkańców.",
                            "target_lang_translations": {
                                "ru": "Это <w>таинственное</w> явление в небе встревожило всех жителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedziwny",
                        "niezwykły"
                    ],
                    "common_phrases": [
                        "niesamowity nastrój",
                        "niesamowita cisza",
                        "niesamowite zjawisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таинственный, странный, вызывающий беспокойство, страх или удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жуткий",
                                "target_lang_sense_clarification": "вызывающий страх, зловещий"
                            },
                            {
                                "target_lang_word": "таинственный",
                                "target_lang_sense_clarification": "загадочный, необъяснимый"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "roep",
        "roeper",
        "roeping",
        "oproep",
        "beroep",
        "aanroep",
        "omroep"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2c628f39-1704-3254-aede-a34ebe18cfd8",
                    "sense_definition": "Met luide stem spreken of geluid maken, vaak om de aandacht van iemand te trekken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "shout",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je in gevaar bent, moet je hard om hulp <roepen>.",
                            "target_lang_translations": {
                                "en": "If you are in danger, you must <w>call</w> loudly for help.",
                                "ru": "Если ты в опасности, нужно громко <звать> на помощь."
                            }
                        },
                        {
                            "text": "De marktkoopman <roept> luid om zijn verse groenten aan te prijzen.",
                            "target_lang_translations": {
                                "en": "The market vendor <w>shouts</w> loudly to praise his fresh vegetables.",
                                "ru": "Рыночный торговец громко <кричит>, расхваливая свои свежие овощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "schreeuwen",
                        "brullen",
                        "tieren"
                    ],
                    "antonyms": [
                        "fluisteren"
                    ],
                    "common_phrases": [
                        "om hulp roepen",
                        "hard roepen"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak with a loud voice or make a sound, often to attract someone's attention.",
                        "ru": "Говорить громким голосом или издавать звук, часто чтобы привлечь чье-то внимание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shout",
                                "target_lang_sense_clarification": "speak very loudly"
                            },
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "cry out to attract attention"
                            },
                            {
                                "target_lang_word": "yell",
                                "target_lang_sense_clarification": "shout loudly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кричать",
                                "target_lang_sense_clarification": "издавать громкий звук"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "обращаться громко, призывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5324af1-5af2-33bd-ae63-0a08a4f72f2d",
                    "sense_definition": "Iemand vragen of opdragen om naar een bepaalde plaats te komen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "summon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De moeder <riep> haar kinderen naar binnen voor het avondeten.",
                            "target_lang_translations": {
                                "en": "The mother <w>called</w> her children inside for dinner.",
                                "ru": "Мать <позвала> детей домой на ужин."
                            }
                        },
                        {
                            "text": "De directeur heeft de werknemer bij zich <geroepen> voor een gesprek.",
                            "target_lang_translations": {
                                "en": "The director has <w>summoned</w> the employee for a meeting.",
                                "ru": "Директор <вызвал> сотрудника к себе для разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "tieren"
                    ],
                    "common_phrases": [
                        "iemand binnen roepen",
                        "bij zich roepen"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask or order someone to come to a specific place.",
                        "ru": "Просить или приказывать кому-либо прийти в определенное место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "summon someone"
                            },
                            {
                                "target_lang_word": "summon",
                                "target_lang_sense_clarification": "order to come"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "просить подойти"
                            },
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "официально приглашать или требовать присутствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9394ff50-c3de-3174-8313-31763ca521d7",
                    "sense_definition": "Met stelligheid een mening of bewering verkondigen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er wordt vaak <geroepen> dat we meer moeten bezuinigen.",
                            "target_lang_translations": {
                                "en": "It is often <w>claimed</w> that we need to cut costs more.",
                                "ru": "Часто <заявляют>, что нам нужно больше экономить."
                            }
                        },
                        {
                            "text": "Hij <roept> maar wat zonder de feiten echt te kennen.",
                            "target_lang_translations": {
                                "en": "He just <w>shouts</w> whatever without really knowing the facts.",
                                "ru": "Он просто <говорит> что попало, не зная фактов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zomaar wat roepen"
                    ],
                    "target_lang_definitions": {
                        "en": "To proclaim an opinion or assertion with certainty.",
                        "ru": "Уверенно высказывать мнение или утверждение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "claim",
                                "target_lang_sense_clarification": "state without proof"
                            },
                            {
                                "target_lang_word": "shout",
                                "target_lang_sense_clarification": "proclaim loudly"
                            },
                            {
                                "target_lang_word": "assert",
                                "target_lang_sense_clarification": "state confidently"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заявлять",
                                "target_lang_sense_clarification": "высказывать мнение"
                            },
                            {
                                "target_lang_word": "утверждать",
                                "target_lang_sense_clarification": "настаивать на чем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e6b7c3c-29c5-3679-ad86-a9060a38e78e",
                    "sense_definition": "Meervoud van <w>roep</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De luide <roepen> van de vogels waren in het hele bos te horen.",
                            "target_lang_translations": {
                                "en": "The loud <w>calls</w> of the birds could be heard throughout the forest.",
                                "ru": "Громкие <крики> птиц были слышны во всем лесу."
                            }
                        },
                        {
                            "text": "Er klinken steeds meer <roepen> om verandering in het huidige beleid.",
                            "target_lang_translations": {
                                "en": "There are increasingly more <w>calls</w> for change in the current policy.",
                                "ru": "Раздается все больше <призывов> к изменению текущей политики."
                            }
                        }
                    ],

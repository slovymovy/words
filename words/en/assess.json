{
    "word_family": [
        "assessment",
        "assessor",
        "reassess",
        "reassessment",
        "assessable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e158095d-1a29-3903-9173-c40aac0ff356",
                    "sense_definition": "To determine, estimate, or judge the value, importance, or quality of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to carefully <w>assess</w> the situation before making a final decision.",
                            "target_lang_translations": {
                                "ru": "Нам нужно тщательно <w>оценить</w> ситуацию, прежде чем принимать окончательное решение."
                            }
                        },
                        {
                            "text": "The insurance company sent an expert to <w>assess</w> the damage to the car.",
                            "target_lang_translations": {
                                "ru": "Страховая компания прислала эксперта, чтобы <w>оценить</w> ущерб, нанесённый автомобилю."
                            }
                        }
                    ],
                    "synonyms": [
                        "evaluate",
                        "judge",
                        "estimate",
                        "appraise",
                        "gauge"
                    ],
                    "common_phrases": [
                        "assess the situation",
                        "assess the damage",
                        "assess the impact",
                        "assess the risk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять, оценивать или выносить суждение о ценности, важности или качестве чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "судить о качестве, ценности или значении"
                            },
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "устанавливать или выяснять (напр. ущерб, ситуацию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05a25dcb-40ee-3a99-bb95-cfe619613795",
                    "sense_definition": "To impose or charge a fee, fine, or penalty, especially as a punishment.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee <w>assessed</w> a penalty against the team for delaying the game.",
                            "target_lang_translations": {
                                "ru": "Судья <w>назначил</w> команде штраф за задержку игры."
                            }
                        },
                        {
                            "text": "A small late fee will be <w>assessed</w> on all accounts that are overdue.",
                            "target_lang_translations": {
                                "ru": "Небольшая плата за просрочку будет <w>начислена</w> на все просроченные счета."
                            }
                        }
                    ],
                    "synonyms": [
                        "impose",
                        "levy",
                        "charge"
                    ],
                    "common_phrases": [
                        "assess a penalty",
                        "assess a fee",
                        "assess a fine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Налагать или взимать плату, штраф или взыскание, особенно в качестве наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налагать",
                                "target_lang_sense_clarification": "назначать взыскание или штраф"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "устанавливать меру наказания (в спорте или праве)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c682e76-8e44-3adc-8f05-3fd89254cd0d",
                    "sense_definition": "To calculate and demand the amount of tax or money due from a person or entity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government will <w>assess</w> the amount of tax you owe based on your income.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>рассчитает</w> сумму налога, которую вы должны, исходя из вашего дохода."
                            }
                        },
                        {
                            "text": "Local authorities <w>assess</w> property taxes at the end of the fiscal year.",
                            "target_lang_translations": {
                                "ru": "Местные власти <w>начисляют</w> налоги на недвижимость в конце финансового года."
                            }
                        }
                    ],
                    "synonyms": [
                        "calculate",
                        "levy"
                    ],
                    "common_phrases": [
                        "assess taxes",
                        "assess the amount"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассчитывать и требовать сумму налога или денежных средств, подлежащих уплате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассчитывать",
                                "target_lang_sense_clarification": "вычислять сумму (налога или сбора)"
                            },
                            {
                                "target_lang_word": "облагать",
                                "target_lang_sense_clarification": "подвергать налогу или сбору"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
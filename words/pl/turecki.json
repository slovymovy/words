                    "semantic_group_id": "turkey_country",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Jej <w>turecki</w> jest na tyle dobry, że może swobodnie rozmawiać.",
                            "target_lang_translations": {
                                "ru": "Её <w>турецкий</w> настолько хорош, что она может свободно разговаривать."
                            }
                        },
                        {
                            "text": "Czy <w>turecki</w> jest trudnym językiem do nauki dla Polaków?",
                            "target_lang_translations": {
                                "ru": "<w>Турецкий</w> — сложный язык для изучения поляками?"
                            }
                        }
                    ],
                    "common_phrases": [

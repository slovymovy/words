{
    "word_family": [
        "manifesteren",
        "manifestatie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78811156-8198-3a32-aadc-4d598d95b2cf",
                    "sense_definition": "Een openbare schriftelijke verklaring van beginselen, doelen en voornemens, vaak van politieke of artistieke aard.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieke partij presenteerde gisteren haar nieuwe <w>manifest</w> aan de kiezers.",
                            "target_lang_translations": {
                                "ru": "Политическая партия вчера представила избирателям свой новый <w>манифест</w>."
                            }
                        },
                        {
                            "text": "In dit <w>manifest</w> leggen de kunstenaars uit wat zij met hun nieuwe stijl willen bereiken.",
                            "target_lang_translations": {
                                "ru": "В этом <w>манифесте</w> художники объясняют, чего они хотят достичь своим новым стилем."
                            }
                        }
                    ],
                    "synonyms": [
                        "handvest"
                    ],
                    "common_phrases": [
                        "politiek manifest",
                        "communistisch manifest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичная письменная декларация принципов, целей и намерений, часто политического или художественного характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манифест",
                                "target_lang_sense_clarification": "политическое или художественное заявление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "323b4386-b404-3a9f-b1cd-fba7e22be59f",
                    "sense_definition": "Een officiële lijst van de goederen die een schip of vliegtuig vervoert, opgesteld voor douane en autoriteiten.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein moet het <w>manifest</w> overhandigen zodra het schip de haven binnenloopt.",
                            "target_lang_translations": {
                                "ru": "Капитан должен передать <w>манифест</w>, как только судно войдет в порт."
                            }
                        },
                        {
                            "text": "Op het <w>manifest</w> stond precies vermeld welke gevaarlijke stoffen er aan boord waren.",
                            "target_lang_translations": {
                                "ru": "В <w>манифесте</w> было точно указано, какие опасные вещества находятся на борту."
                            }
                        }
                    ],
                    "synonyms": [
                        "cargalijst",
                        "cargolijst",
                        "ladingsbrief",
                        "ladingslijst",
                        "vrachtbrief"
                    ],
                    "common_phrases": [
                        "manifest van de lading"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный список грузов, перевозимых судном или самолетом, составленный для таможни и властей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манифест",
                                "target_lang_sense_clarification": "судовой документ, список грузов"
                            },
                            {
                                "target_lang_word": "грузовая декларация",
                                "target_lang_sense_clarification": "таможенный документ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e223f219-2158-3990-9dfe-7fa05813a4eb",
                    "sense_definition": "Duidelijk zichtbaar en voor iedereen onmiskenbaar aanwezig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er was sprake van een <w>manifeste</w> fout in de berekening die iedereen over het hoofd had gezien.",
                            "target_lang_translations": {
                                "ru": "В расчетах была допущена <w>явная</w> ошибка, которую все упустили из виду."
                            }
                        },
                        {
                            "text": "Zijn ontevredenheid over de gang van zaken was <w>manifest</w> tijdens de vergadering.",
                            "target_lang_translations": {
                                "ru": "Его недовольство ходом дел было <w>очевидным</w> во время собрания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "manifeste onwaarheid",
                        "manifeste leugen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Явный, очевидный и несомненный для всех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "отчетливо видимый, нескрываемый"
                            },
                            {
                                "target_lang_word": "очевидный",
                                "target_lang_sense_clarification": "несомненный, понятный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "odepchnąć",
        "odpychanie",
        "odpychający"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "03478ecc-107a-38f6-b662-6d6d07890d28",
                    "sense_definition": "Oddalać kogoś lub coś od siebie za pomocą pchnięcia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_push",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko ze złością <odpycha> talerz z jedzeniem, którego nie lubi.",
                            "target_lang_translations": {
                                "ru": "Ребенок со злостью <w>отталкивает</w> тарелку с едой, которую не любит."
                            }
                        },
                        {
                            "text": "Musiałem mocno <odpychać> ciężkie drzwi, żeby w końcu się otworzyły.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось сильно <w>толкать</w> тяжелую дверь, чтобы она наконец открылась."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsuwać",
                        "odtrącać"
                    ],
                    "antonyms": [
                        "przyciągać"
                    ],
                    "common_phrases": [
                        "odpychać rękami",
                        "odpychać od siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отодвигать кого-то или что-то от себя толчком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отталкивать",
                                "target_lang_sense_clarification": "физическое действие"
                            },
                            {
                                "target_lang_word": "отодвигать",
                                "target_lang_sense_clarification": "перемещать от себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4a5e9f2-7873-3735-8114-f6f7943123e9",
                    "sense_definition": "Odsuwać się od czegoś, wykonując ruch pchnięcia (np. rękami lub nogami).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_push",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pływak dynamicznie <odpycha> się od ściany basenu, aby nabrać prędkości.",
                            "target_lang_translations": {
                                "ru": "Пловец энергично <w>отталкивается</w> от стенки бассейна, чтобы набрать скорость."
                            }
                        },
                        {
                            "text": "Aby jechać na deskorolce, musisz rytmicznie <odpychać> się jedną nogą od chodnika.",
                            "target_lang_translations": {
                                "ru": "Чтобы ехать на скейтборде, ты должен ритмично <w>отталкиваться</w> одной ногой от тротуара."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsuwać się"
                    ],
                    "antonyms": [
                        "przybliżać się"
                    ],
                    "common_phrases": [
                        "odpychać się nogami",
                        "odpychać się od brzegu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отодвигаться от чего-либо, совершая толкательное движение (например, руками или ногами)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отталкиваться",
                                "target_lang_sense_clarification": "возвратное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8690ba1-8e0f-3f15-925d-f5514add9c56",
                    "sense_definition": "Wzbudzać w kimś uczucie niechęci, wstrętu lub antypatii.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_repulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego aroganckie zachowanie i brak kultury <odpycha> od niego wielu ludzi.",
                            "target_lang_translations": {
                                "ru": "Его высокомерное поведение и отсутствие культуры <w>отталкивает</w> от него многих людей."
                            }
                        },
                        {
                            "text": "Ten intensywny i nieprzyjemny zapach skutecznie <odpycha> klientów od wejścia do sklepu.",
                            "target_lang_translations": {
                                "ru": "Этот сильный и неприятный запах эффективно <w>отталкивает</w> клиентов от входа в магазин."
                            }
                        }
                    ],
                    "synonyms": [
                        "odstręczać",
                        "zrażać"
                    ],
                    "antonyms": [
                        "przyciągać",
                        "zachęcać"
                    ],
                    "common_phrases": [
                        "odpychać wyglądem",
                        "odpychać zachowaniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-либо чувство неприязни, отвращения или антипатии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отталкивать",
                                "target_lang_sense_clarification": "вызывать неприязнь"
                            },
                            {
                                "target_lang_word": "отвращать",
                                "target_lang_sense_clarification": "вызывать отвращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7785b766-62d2-3b60-9b33-a514079130f5",
                    "sense_definition": "Starać się nie dopuszczać do siebie pewnych myśli, uczuć lub podejrzeń.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_rejection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna instynktownie <odpycha> od siebie myśl o możliwej porażce w nadchodzącym konkursie.",
                            "target_lang_translations": {
                                "ru": "Мужчина инстинктивно <w>гонит</w> от себя мысль о возможном поражении в предстоящем конкурсе."
                            }
                        },
                        {
                            "text": "Staram się <odpychać> złe wspomnienia i skupiać wyłącznie na pozytywnych aspektach życia.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь <w>отгонять</w> плохие воспоминания и сосредотачиваться исключительно на положительных аспектах жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsuwać",
                        "odrzucać"
                    ],
                    "common_phrases": [
                        "odpychać myśli",
                        "odpychać od siebie podejrzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стараться не допускать к себе определенные мысли, чувства или подозрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отгонять",
                                "target_lang_sense_clarification": "мысли или чувства"
                            },
                            {
                                "target_lang_word": "гнать",
                                "target_lang_sense_clarification": "прочь от себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf52f2a7-c930-39c9-9673-44238d2f98a8",
                    "sense_definition": "Działać siłą, która powoduje wzajemne oddalanie się obiektów (np. w fizyce).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physics_repulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwa jednakowe bieguny magnesu zawsze się <odpychają>, co jest podstawowym prawem magnetyzmu.",
                            "target_lang_translations": {
                                "ru": "Два одинаковых полюса магнита всегда <w>отталкиваются</w>, что является основным законом магнетизма."
                            }
                        },
                        {
                            "text": "Ładunki elektryczne o tym samym znaku wzajemnie się <odpychają>.",
                            "target_lang_translations": {
                                "ru": "Электрические заряды одного знака взаимно <w>отталкиваются</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "przyciągać"
                    ],
                    "common_phrases": [
                        "bieguny się odpychają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздействовать силой, вызывающей взаимное удаление объектов (в физике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отталкивать",
                                "target_lang_sense_clarification": "физика"
                            },
                            {
                                "target_lang_word": "отталкиваться",
                                "target_lang_sense_clarification": "взаимное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "336ee62a-8fad-3471-8e46-5def45961562",
                    "sense_definition": "Skutecznie stawiać opór atakującemu przeciwnikowi i zmuszać go do odwrotu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_repulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzielni obrońcy miasta przez wiele dni zdołali <odpychać> kolejne szturmy wrogiej armii.",
                            "target_lang_translations": {
                                "ru": "Храбрые защитники города в течение многих дней сумели <w>отражать</w> очередные штурмы вражеской армии."
                            }
                        },
                        {
                            "text": "Wojsko skutecznie <odpycha> ataki nieprzyjaciela na granicy państwa.",
                            "target_lang_translations": {
                                "ru": "Войско эффективно <w>отражает</w> атаки неприятеля на границе государства."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpierać",
                        "przeciwstawiać się"
                    ],
                    "common_phrases": [
                        "odpychać atak",
                        "odpychać wroga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешно оказывать сопротивление атакующему противнику и заставлять его отступить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отражать",
                                "target_lang_sense_clarification": "атаку или нападение"
                            },
                            {
                                "target_lang_word": "отбивать",
                                "target_lang_sense_clarification": "военное действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
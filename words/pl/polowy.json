    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3f2f4597-ece5-3032-8ae1-dfd02ee2f60b",
                    "sense_definition": "Przeznaczony do użytku w warunkach wojennych, poza stałymi bazami lub koszarami; dotyczący działań na polu walki.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "military_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ranni żołnierze zostali szybko przetransportowani do szpitala <w>polowego</w>.",
                            "target_lang_translations": {
                                "ru": "Раненых солдат быстро перевезли в <w>полевой</w> госпиталь."
                            }
                        },
                        {
                            "text": "Kuchnia <w>polowa</w> wydała ciepłe posiłki wszystkim zgromadzonym uchodźcom.",
                            "target_lang_translations": {
                                "ru": "<w>Полевая</w> кухня выдала горячую еду всем собравшимся беженцам."
                            }
                        }
                    ],
                    "synonyms": [
                        "wojenny",
                        "frontowy",
                        "bojowy"
                    ],
                    "common_phrases": [
                        "szpital polowy",
                        "kuchnia polowa",
                        "mundur polowy",
                        "sąd polowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для использования в военных условиях, вне постоянных баз или казарм; относящийся к действиям на поле боя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полевой",
                                "target_lang_sense_clarification": "военный"
                            },
                            {
                                "target_lang_word": "походный",
                                "target_lang_sense_clarification": "передвижной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca282d8b-59e9-3b73-8ba7-60684cbb0a3b",
                    "sense_definition": "Związany z uprawą ziemi, rolnictwem lub pracami wykonywanymi na polu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agriculture_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosenne prace <w>polowe</w> rozpoczęły się w tym roku wyjątkowo wcześnie.",
                            "target_lang_translations": {
                                "ru": "Весенние <w>полевые</w> работы начались в этом году исключительно рано."
                            }
                        },
                        {
                            "text": "Rolnicy wyjechali na drogę <w>polową</w> swoimi traktorami.",
                            "target_lang_translations": {
                                "ru": "Фермеры выехали на <w>полевую</w> дорогу на своих тракторах."
                            }
                        }
                    ],
                    "synonyms": [
                        "rolniczy"
                    ],
                    "common_phrases": [
                        "prace polowe",
                        "droga polowa",
                        "uprawy polowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с обработкой земли, сельским хозяйством или работами, выполняемыми на поле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полевой",
                                "target_lang_sense_clarification": "сельскохозяйственный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7042968f-7862-35c3-8c50-b7d90a8f0251",
                    "sense_definition": "Odnoszący się do nabożeństw i posługi religijnej sprawowanej na wolnym powietrzu, zazwyczaj w kontekście wojskowym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biskup <w>polowy</w> wygłosił poruszające kazanie do żołnierzy wyjeżdżających na misję.",
                            "target_lang_translations": {
                                "ru": "<w>Полевой</w> епископ произнес трогательную проповедь солдатам, отправляющимся на задание."
                            }
                        },
                        {
                            "text": "W niedzielę rano odbyła się uroczysta msza <w>polowa</w> na placu apelowym.",
                            "target_lang_translations": {
                                "ru": "В воскресенье утром на плацу состоялась торжественная <w>полевая</w> месса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "msza polowa",
                        "biskup polowy",
                        "ołtarz polowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к богослужениям и религиозной службе, проводимым на открытом воздухе, обычно в военном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полевой",
                                "target_lang_sense_clarification": "религиозный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "42b49d72-45be-38c5-994c-1411ccc3e4c1",
                    "sense_definition": "Osoba zajmująca się dawniej pilnowaniem pól uprawnych przed zniszczeniem lub kradzieżą.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>polowy</w> codziennie obchodził granice wsi, sprawdzając stan upraw.",
                            "target_lang_translations": {
                                "ru": "Старый <w>полевой сторож</w> ежедневно обходил границы деревни, проверяя состояние посевов."
                            }
                        },
                        {
                            "text": "Chłopi bali się, że <w>polowy</w> przyłapie ich na wypasaniu bydła w niedozwolonym miejscu.",
                            "target_lang_translations": {
                                "ru": "Крестьяне боялись, что <w>полевой сторож</w> поймает их на выпасе скота в неположенном месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "myśliwy"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "word historically used to describe a field guard or ranger"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, занимавшийся в прошлом охраной возделываемых полей от уничтожения или кражи."
                    },
                    "translations": {
                        "ru": [

                },
                {
                    "sense_id": "295bf279-cc68-3555-b5a4-8a203d79db5f",
                    "sense_definition": "Een symbool, meestal een letter, dat een willekeurige waarde of een element uit een verzameling voorstelt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_variable",
                    "name_type": "no",
                    "examples": [
                        {

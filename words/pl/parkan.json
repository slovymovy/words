{
    "word_family": [
        "parkanik",
        "parkanowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "da142473-f379-3d5a-8f46-b81bc1d25cb0",
                    "sense_definition": "Ogrodzenie, zwykle drewniane, zbudowane z pionowych desek, sztachet lub bali.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fence_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wokół starego domu stał wysoki, drewniany <w>parkan</w>, który zasłaniał widok na ogród.",
                            "target_lang_translations": {
                                "ru": "Вокруг старого дома стоял высокий деревянный <w>забор</w>, который закрывал вид на сад."
                            }
                        },
                        {
                            "text": "Złodziej przeskoczył przez <w>parkan</w> i szybko uciekł w stronę lasu.",
                            "target_lang_translations": {
                                "ru": "Вор перепрыгнул через <w>забор</w> и быстро убежал в сторону леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "płot"
                    ],
                    "common_phrases": [
                        "drewniany parkan",
                        "wysoki parkan",
                        "otoczyć parkanem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограждение, обычно деревянное, состоящее из вертикальных досок, штакетника или брёвен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забор",
                                "target_lang_sense_clarification": "ограждение"
                            },
                            {
                                "target_lang_word": "ограда",
                                "target_lang_sense_clarification": "сооружение"
                            },
                            {
                                "target_lang_word": "частокол",
                                "target_lang_sense_clarification": "из кольев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca6ecdc6-14a2-3d9a-b582-977e26168144",
                    "sense_definition": "Ochraniacz na nogi noszony przez bramkarza hokejowego w celu zabezpieczenia przed uderzeniami krążka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz założył ciężkie <w>parkany</w>, aby chronić nogi podczas meczu.",
                            "target_lang_translations": {
                                "ru": "Вратарь надел тяжелые <w>щитки</w>, чтобы защитить ноги во время матча."
                            }
                        },
                        {
                            "text": "Bez solidnych <w>parkanów</w> gra na pozycji bramkarza byłaby bardzo niebezpieczna.",
                            "target_lang_translations": {
                                "ru": "Без надежных <w>щитков</w> игра на позиции вратаря была бы очень опасной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parkany hokejowe",
                        "parkany bramkarskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитное снаряжение для ног, которое носит хоккейный вратарь для защиты от ударов шайбой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щитки",
                                "target_lang_sense_clarification": "хоккейные"
                            },
                            {
                                "target_lang_word": "вратарские щитки",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f97bae2c-d676-3715-ac31-704fb6ad045c",
                    "sense_definition": "Rzeczna pułapka na ryby w postaci pionowej zagrody wykonanej z wikliny lub sieci.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fishing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rybacy ustawili w rzece wiklinowy <w>parkan</w>, licząc na obfity połów.",
                            "target_lang_translations": {
                                "ru": "Рыбаки установили в реке плетеный <w>закол</w>, рассчитывая на богатый улов."
                            }
                        },
                        {
                            "text": "Silny nurt rzeki uszkodził stary <w>parkan</w> służący do łowienia ryb.",
                            "target_lang_translations": {
                                "ru": "Сильное течение реки повредило старый <w>закол</w>, служивший для ловли рыбы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Речная ловушка для рыбы в виде вертикальной изгороди из прутьев или сетей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закол",
                                "target_lang_sense_clarification": "рыболовное заграждение"
                            },
                            {
                                "target_lang_word": "ез",
                                "target_lang_sense_clarification": "рыболовный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "common_phrases": [
                        "выставочный павильон",
                        "павильон выставки"
                    ],
                    "target_lang_definitions": {
                        "en": "A building, often temporary, designed to house exhibits at an exhibition or fair."
                    },
                    "translations": {

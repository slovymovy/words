    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d332509a-7b12-3d1c-b27e-9ebfe63ea046",
                    "sense_definition": "Наречие, указывающее на то, что действие совершилось или состояние наступило ранее ожидаемого срока; краткая форма слова «уже».",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <уж> закончили работу и собираемся домой.",
                            "target_lang_translations": {
                                "en": "We have <w>already</w> finished work and are getting ready to go home."
                            }
                        },
                        {
                            "text": "На улице <уж> стемнело, пора включать свет.",
                            "target_lang_translations": {
                                "en": "It has <w>already</w> gotten dark outside, time to turn on the light."
                            }
                        }
                    ],
                    "synonyms": [
                        "уже",
                        "уже́"
                    ],
                    "antonyms": [
                        "ещё"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "употребляется преимущественно в разговорной речи как сокращение от «уже»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial short form of \"already\", indicating that an action has occurred or a state has been reached earlier than expected."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "already",
                                "target_lang_sense_clarification": "adverb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40a4762a-1a59-34c0-9fe8-442760a7ee66",
                    "sense_definition": "Усилительная частица, используемая для подчёркивания эмоциональности высказывания, а также при выражении просьбы, уступки или уверенности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты <уж> постарайся сделать всё правильно.",
                            "target_lang_translations": {
                                "en": "You <w>do</w> try to do everything correctly."
                            }
                        },
                        {
                            "text": "Это не так <уж> сложно, если подумать.",
                            "target_lang_translations": {
                                "en": "It is not <w>that</w> difficult if you think about it."
                            }
                        },
                        {
                            "text": "Да <уж>, ситуация оказалась непростой.",
                            "target_lang_translations": {
                                "en": "Yes <w>indeed</w>, the situation turned out to be complicated."
                            }
                        }
                    ],
                    "synonyms": [
                        "ж",
                        "ну",
                        "так и быть"
                    ],
                    "common_phrases": [
                        "да уж",
                        "не так уж",
                        "вот уж",
                        "уж если",
                        "уж лучше"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "характерно для устной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An intensifying particle used to emphasize a statement, request, or emotion; often corresponds to \"really\", \"indeed\", or emphatic \"do\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "really",
                                "target_lang_sense_clarification": "intensifier"
                            },
                            {
                                "target_lang_word": "indeed",
                                "target_lang_sense_clarification": "emphasis in exclamations"
                            },
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "emphasis in requests"
                            }
                        ]
                    }
                }
            ]
        },
        {

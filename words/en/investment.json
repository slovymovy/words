    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3d2f42ee-f1a5-31bd-ad35-644ea1109b02",
                    "sense_definition": "The action of putting money into financial schemes, shares, property, or a commercial venture with the expectation of achieving a profit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Buying a house is often considered a safe <w>investment</w> for the future.",
                            "target_lang_translations": {
                                "ru": "Покупка дома часто считается надежной <w>инвестицией</w> в будущее."
                            }
                        },
                        {
                            "text": "The company plans to increase its <w>investment</w> in renewable energy sources.",
                            "target_lang_translations": {
                                "ru": "Компания планирует увеличить свои <w>инвестиции</w> в возобновляемые источники энергии."
                            }
                        }
                    ],
                    "antonyms": [
                        "divestment"
                    ],
                    "common_phrases": [
                        "return on investment",
                        "foreign investment",
                        "good investment",
                        "capital investment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по вложению денег в финансовые схемы, акции, недвижимость или коммерческое предприятие с ожиданием получения прибыли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инвестиция",
                                "target_lang_sense_clarification": "финансовое вложение"
                            },
                            {
                                "target_lang_word": "вложение",
                                "target_lang_sense_clarification": "размещение средств"
                            },
                            {
                                "target_lang_word": "инвестирование",
                                "target_lang_sense_clarification": "процесс вложения"
                            },
                            {
                                "target_lang_word": "капиталовложение",
                                "target_lang_sense_clarification": "вложение капитала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7af667ac-0d7c-3ad1-8f22-cdb88c2f1f35",
                    "sense_definition": "The act of devoting time, effort, or energy to a particular undertaking with the hope of a worthwhile result.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Learning a new language requires a significant <w>investment</w> of time and patience.",
                            "target_lang_translations": {
                                "ru": "Изучение нового языка требует значительного <w>вложения</w> времени и терпения."
                            }
                        },
                        {
                            "text": "Her emotional <w>investment</w> in the project made the failure hard to accept.",
                            "target_lang_translations": {
                                "ru": "Её эмоциональный <w>вклад</w> в проект сделал неудачу трудной для принятия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "investment of time",
                        "emotional investment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт посвящения времени, усилий или энергии определенному делу в надежде на стоящий результат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вложение",
                                "target_lang_sense_clarification": "трата ресурсов или сил"
                            },
                            {
                                "target_lang_word": "вклад",
                                "target_lang_sense_clarification": "участие в деле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44646829-2264-371c-8efa-038b2a33b9d2",
                    "sense_definition": "The act of surrounding a place with military forces to cut off supplies or escape; a siege.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general ordered the immediate <w>investment</w> of the enemy fortress.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал немедленно начать <w>осаду</w> вражеской крепости."
                            }
                        },
                        {
                            "text": "During the <w>investment</w> of the city, no food was allowed to enter.",
                            "target_lang_translations": {

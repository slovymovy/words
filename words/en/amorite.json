    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f7dc054-5167-3031-b3d1-b36f4ed10d9f",
                    "sense_definition": "A member of an ancient Semitic people who lived to the west of the Euphrates river.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Amorite</w> migration into Mesopotamia significantly altered the political landscape of the region.",
                            "target_lang_translations": {
                                "ru": "Миграция <w>амореев</w> в Месопотамию значительно изменила политический ландшафт региона."
                            }
                        },
                        {
                            "text": "Hammurabi was a famous <w>Amorite</w> king who established a lasting code of laws.",
                            "target_lang_translations": {
                                "ru": "Хаммурапи был знаменитым <w>аморейским</w> царем, создавшим долговечный свод законов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Amorite dynasty",
                        "Amorite king",
                        "ancient Amorite"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "ancient Middle East"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель древнего семитского народа, жившего к западу от реки Евфрат"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аморей",
                                "target_lang_sense_clarification": "представитель народа"
                            },
                            {
                                "target_lang_word": "аморит",
                                "target_lang_sense_clarification": "вариант названия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "34f2837a-25cc-33c3-90e1-a3f675ffb8bf",
                    "sense_definition": "Relating to the Amorites, their culture, or their language.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archaeologists have uncovered <w>Amorite</w> pottery in several sites across Syria.",
                            "target_lang_translations": {
                                "ru": "Археологи обнаружили <w>аморейскую</w> керамику в нескольких местах на территории Сирии."
                            }
                        },
                        {
                            "text": "The <w>Amorite</w> period is known for the rise of prominent city-states like Babylon.",
                            "target_lang_translations": {
                                "ru": "<w>Аморейский</w> период известен расцветом таких выдающихся городов-государств, как Вавилон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Amorite culture",
                        "Amorite period",
                        "Amorite influence"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к амореям, их культуре или языку"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аморейский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f2a401d-8adb-3477-9c1f-01e5e7e07e13",
                    "sense_definition": "Relating to the god Amurru, also known as Martu, who was worshipped by the Amorite people.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temple walls featured inscriptions dedicated to various <w>Amorite</w> deities.",
                            "target_lang_translations": {
                                "ru": "На стенах храма были надписи, посвященные различным <w>аморейским</w> божествам."
                            }
                        },
                        {
                            "text": "Scholars analyze ancient texts to understand <w>Amorite</w> religious practices and beliefs.",
                            "target_lang_translations": {
                                "ru": "Ученые анализируют древние тексты, чтобы понять <w>аморейские</w> религиозные практики и верования."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к богу Амурру (Марту), почитаемому амореями"

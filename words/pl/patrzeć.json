    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b184ba15-6acb-3af0-8cea-e72578733324",
                    "sense_definition": "Kierować wzrok na kogoś lub na coś, aby to zobaczyć",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception_visual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię siedzieć w parku i <patrzeć> na bawiące się dzieci.",
                            "target_lang_translations": {
                                "ru": "Я люблю сидеть в парке и <смотреть> на играющих детей."
                            }
                        },
                        {
                            "text": "Kiedy z nim rozmawiasz, powinieneś <patrzeć> mu prosto w oczy.",
                            "target_lang_translations": {
                                "ru": "Когда разговариваешь с ним, тебе следует <смотреть> ему прямо в глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoglądać",
                        "przyglądać się",
                        "gapić się",
                        "spoglądać się"
                    ],
                    "common_phrases": [
                        "patrzeć w oczy",
                        "patrzeć w lustro",
                        "patrzeć pod nogi",
                        "patrzeć w niebo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять взгляд на кого-то или что-то, чтобы увидеть это"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "направлять взгляд"
                            },
                            {
                                "target_lang_word": "глядеть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e8c0fc6-d6ed-3aa0-a271-466a49ea05e1",
                    "sense_definition": "Obserwować coś uważnie, śledzić wzrokiem przebieg jakiegoś wydarzenia lub czynności",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "perception_visual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała rodzina siedziała na kanapie i <patrzyła> na ciekawy film przyrodniczy.",
                            "target_lang_translations": {
                                "ru": "Вся семья сидела на диване и <смотрела> интересный фильм о природе."
                            }
                        },
                        {
                            "text": "Nie lubię <patrzeć>, jak ktoś ciężko pracuje, kiedy ja odpoczywam.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <смотреть>, как кто-то тяжело работает, пока я отдыхаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "oglądać",
                        "obserwować",
                        "wypatrywać"
                    ],
                    "common_phrases": [
                        "patrzeć w telewizor",
                        "patrzeć na ręce",
                        "patrzeć na zegarek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внимательно наблюдать за чем-то, следить глазами за ходом события"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "быть зрителем, наблюдать"
                            },
                            {
                                "target_lang_word": "наблюдать",
                                "target_lang_sense_clarification": "следить за процессом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64a7b7f3-0684-3e38-a1e5-600eef1ea474",
                    "sense_definition": "Mieć określony stosunek do kogoś lub czegoś; oceniać sytuację w pewien sposób",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego rodzice krzywo <patrzą> na ten ryzykowny pomysł biznesowy.",
                            "target_lang_translations": {
                                "ru": "Его родители косо <смотрят> на эту рискованную бизнес-идею."
                            }
                        },
                        {
                            "text": "Musisz <patrzeć> na świat bardziej optymistycznie, żeby być szczęśliwym.",
                            "target_lang_translations": {
                                "ru": "Ты должен <смотреть> на мир более оптимистично, чтобы быть счастливым."
                            }
                        }
                    ],
                    "synonyms": [
                        "oceniać",
                        "traktować",
                        "zważać",
                        "mieć na uwadze"
                    ],
                    "antonyms": [
                        "ignorować",
                        "lekceważyć"
                    ],
                    "common_phrases": [
                        "patrzeć krzywym okiem",
                        "patrzeć przez palce",
                        "patrzeć z góry",
                        "patrzeć realnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-то или чему-то определённым образом; оценивать ситуацию"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "иметь мнение, оценивать"
                            },
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "воспринимать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22455ecb-bbe4-3e08-bf16-094295a3295b",
                    "sense_definition": "Słowo używane w trybie rozkazującym, aby zwrócić czyjąś uwagę na coś interesującego, ważnego lub zaskakującego",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "interjection_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Patrz>, jaki piękny ptak usiadł na gałęzi tuż za naszym oknem!",
                            "target_lang_translations": {
                                "ru": "<Смотри>, какая красивая птица села на ветку прямо за нашим окном!"
                            }
                        },
                        {
                            "text": "No <patrz>, znowu zapomniałem wziąć kluczy z domu.",
                            "target_lang_translations": {
                                "ru": "Ну <надо же>, я снова забыл взять ключи из дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "zauważać",
                        "zwracać uwagę"
                    ],
                    "common_phrases": [
                        "no patrz",
                        "patrzcie go"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в повелительном наклонении для привлечения внимания к чему-то интересному или важному"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотри",
                                "target_lang_sense_clarification": "призыв к вниманию"
                            },
                            {
                                "target_lang_word": "гляди",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "358b2cc0-dcba-3d73-9b22-3373980c4a52",
                    "sense_definition": "Być zwróconym frontem lub oknami w określonym kierunku (o budynkach, oknach)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okna naszej sypialni <patrzą> na cichy i zielony ogród.",
                            "target_lang_translations": {
                                "ru": "Окна нашей спальни <выходят> на тихий и зелёный сад."
                            }
                        },
                        {
                            "text": "Balkon w tym apartamencie <patrzy> bezpośrednio na morze.",
                            "target_lang_translations": {
                                "ru": "Балкон в этих апартаментах <выходит> прямо на море."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyglądać"
                    ],
                    "common_phrases": [
                        "okna patrzą na",
                        "dom patrzy na"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть обращённым лицевой стороной или окнами в определённом направлении (о зданиях, окнах)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "об окнах, фасаде"
                            },
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "быть обращённым в сторону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4fce39a-d228-3f92-b53d-f30a0c49cd8e",
                    "sense_definition": "Szukać czegoś wzrokiem lub oczekiwać na coś z niecierpliwością",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "searching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy zmęczeni pracownicy <patrzyli> tylko końca tego długiego zebrania.",
                            "target_lang_translations": {
                                "ru": "Все уставшие сотрудники только и <ждали> конца этого долгого собрания."
                            }
                        },
                        {
                            "text": "Nie ma co <patrzeć> na innych, trzeba samemu wziąć się do pracy.",
                            "target_lang_translations": {
                                "ru": "Нечего <смотреть> на других, нужно самому браться за работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "szukać",
                        "czekać",
                        "ubiegać się"
                    ],
                    "common_phrases": [
                        "patrzeć końca",
                        "patrzeć okazji",
                        "patrzeć zysku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать что-то взглядом или с нетерпением ожидать чего-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "ожидать с нетерпением"
                            },
                            {
                                "target_lang_word": "высматривать",
                                "target_lang_sense_clarification": "искать глазами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
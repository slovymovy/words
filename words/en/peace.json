                },
                {
                    "sense_id": "e2fbc7ec-afd6-3dc0-8b6c-f5b1e5113a49",
                    "sense_definition": "A command to be silent or quiet.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "silence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Peace</w>! I cannot hear the music with all this noise.",
                            "target_lang_translations": {
                                "ru": "<w>Тихо</w>! Я не слышу музыку из-за этого шума."
                            }
                        },
                        {
                            "text": "Hold your <w>peace</w> and let the king speak.",
                            "target_lang_translations": {
                                "ru": "Храните <w>молчание</w> и дайте королю говорить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hold your peace"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приказ молчать или вести себя тихо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тихо",
                                "target_lang_sense_clarification": "призыв к тишине"
                            },
                            {
                                "target_lang_word": "молчание",
                                "target_lang_sense_clarification": "отсутствие речи"
                            }
                        ]
                    }
                }
            ]
        },
        {

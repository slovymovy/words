    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4e9bffe5-2b79-3f4a-92bd-caf89cfddc37",
                    "sense_definition": "Ubrać coś na siebie lub kogoś innego; umieścić element garderoby na ciele.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimno na dworze, więc musisz <nałożyć> ciepłą czapkę.",
                            "target_lang_translations": {
                                "ru": "На улице холодно, поэтому ты должен <надеть> теплую шапку."
                            }
                        },
                        {
                            "text": "Mama pomogła dziecku <nałożyć> buty przed wyjściem do przedszkola.",
                            "target_lang_translations": {
                                "ru": "Мама помогла ребенку <надеть> ботинки перед выходом в детский сад."
                            }
                        }
                    ],
                    "synonyms": [
                        "założyć się"
                    ],
                    "antonyms": [
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "nałożyć czapkę",
                        "nałożyć płaszcz",
                        "nałożyć buty",
                        "nałożyć ubranie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть что-то на себя или кого-то другого; поместить предмет одежды на тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надеть",
                                "target_lang_sense_clarification": "одежда, обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b91bc9a-89b0-3f54-b544-94867abc5285",
                    "sense_definition": "Umieścić porcję jedzenia na talerzu lub w naczyniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mogę ci <nałożyć> jeszcze trochę sałatki jarzynowej?",
                            "target_lang_translations": {
                                "ru": "Могу я <положить> тебе еще немного овощного салата?"
                            }
                        },
                        {
                            "text": "Gospodyni chciała <nałożyć> gościom ciasto, ale grzecznie odmówili.",
                            "target_lang_translations": {
                                "ru": "Хозяйка хотела <положить> гостям торт, но они вежливо отказались."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nałożyć obiadu",
                        "nałożyć zupy",
                        "nałożyć sobie dokładkę",
                        "nałożyć na talerz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положить порцию еды на тарелку или в посуду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "еда"
                            },
                            {
                                "target_lang_word": "наложить",
                                "target_lang_sense_clarification": "еда (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9aa82002-f0dd-39ab-9b36-30a2f38569c2",
                    "sense_definition": "Rozprowadzić warstwę substancji (np. kremu, farby) na powierzchni czegoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjściem na ostre słońce należy <nałożyć> krem z filtrem.",
                            "target_lang_translations": {
                                "ru": "Перед выходом на яркое солнце следует <нанести> солнцезащитный крем."
                            }
                        },
                        {
                            "text": "Fryzjerka musiała bardzo precyzyjnie <nałożyć> farbę na włosy klientki.",
                            "target_lang_translations": {
                                "ru": "Парикмахер должна была очень точно <нанести> краску на волосы клиентки."
                            }
                        }
                    ],
                    "antonyms": [
                        "zetrzeć",
                        "zmyć"
                    ],
                    "common_phrases": [
                        "nałożyć krem",
                        "nałożyć makijaż",
                        "nałożyć farbę",
                        "nałożyć maseczkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести слой вещества (например, крема, краски) на поверхность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанести",
                                "target_lang_sense_clarification": "крем, макияж, краска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "933039da-d06b-341a-a349-bcc2352ea4f5",
                    "sense_definition": "Oficjalnie obarczyć kogoś obowiązkiem, karą lub opłatą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "imposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd zdecydował <nałożyć> na oskarżonego wysoką grzywnę za wykroczenie.",
                            "target_lang_translations": {
                                "ru": "Суд решил <наложить> на обвиняемого высокий штраф за правонарушение."
                            }
                        },
                        {
                            "text": "Rząd planuje <nałożyć> nowe podatki na towary luksusowe w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует <ввести> новые налоги на предметы роскоши в следующем году."
                            }
                        }
                    ],
                    "antonyms": [
                        "znieść",
                        "uchylić"
                    ],
                    "common_phrases": [
                        "nałożyć karę",
                        "nałożyć sankcje",
                        "nałożyć podatek",
                        "nałożyć mandat",
                        "nałożyć areszt"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально возложить на кого-либо обязанность, наказание или плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наложить",
                                "target_lang_sense_clarification": "штраф, санкции, запрет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0366736-cdaf-350a-b00b-b88ea7302b8e",
                    "sense_definition": "Wystąpić w tym samym czasie lub częściowo przykryć jedno drugim (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_overlap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety nasze urlopy mogą się <nałożyć> w tym samym czasie, co utrudni pracę biura.",
                            "target_lang_translations": {
                                "ru": "К сожалению, наши отпуска могут <наложиться> друг на друга по времени, что затруднит работу офиса."
                            }
                        },
                        {
                            "text": "Te dwa wydarzenia historyczne musiały się na siebie <nałożyć>, tworząc kryzys.",
                            "target_lang_translations": {
                                "ru": "Эти два исторических события должны были <наложиться> друг на друга, создав кризис."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbiec się"
                    ],
                    "common_phrases": [
                        "terminy się nałożyły",
                        "daty mogą się nałożyć",
                        "głosy się nałożyły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произойти в одно и то же время или частично перекрыть друг друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наложиться",
                                "target_lang_sense_clarification": "совпасть по времени"
                            },
                            {
                                "target_lang_word": "совпасть",
                                "target_lang_sense_clarification": "о событиях"
                            }
                        ]
                    }
                }
            ]
        }
    ]

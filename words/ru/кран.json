    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d083f894-080a-3641-8b3d-b5331253859b",
                    "sense_definition": "Устройство для подачи воды или газа из трубы, обычно на кухне или в ванной.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "plumbing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он прошёл в ванную, отвернул <кран>, но воды не было.",
                            "target_lang_translations": {
                                "en": "He went into the bathroom, turned on the <w>tap</w>, but there was no water."
                            }
                        },
                        {
                            "text": "Пожалуйста, закрой <кран>, чтобы вода не текла зря.",
                            "target_lang_translations": {
                                "en": "Please turn off the <w>faucet</w> so the water doesn't run to waste."
                            }
                        }
                    ],
                    "synonyms": [
                        "водопроводный кран"
                    ],
                    "common_phrases": [
                        "открыть кран",
                        "закрыть кран",
                        "вода из крана",
                        "течь из крана"
                    ],
                    "target_lang_definitions": {
                        "en": "A device for controlling the flow of water or gas from a pipe, typically in a kitchen or bathroom."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tap",
                                "target_lang_sense_clarification": "British English / General"
                            },
                            {
                                "target_lang_word": "faucet",
                                "target_lang_sense_clarification": "American English"
                            },
                            {
                                "target_lang_word": "spigot",
                                "target_lang_sense_clarification": "outdoor or specific type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c9b6d46-8e67-38e6-9031-550962d504b5",
                    "sense_definition": "Большая машина или механизм для подъёма и перемещения тяжёлых грузов, используемая на стройке или в порту.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "machinery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный башенный <кран> поднимал поддоны с кирпичами на верхний этаж.",
                            "target_lang_translations": {
                                "en": "A huge tower <w>crane</w> was lifting pallets of bricks to the top floor."
                            }
                        },
                        {
                            "text": "На этой стройке работает мощный <кран>, который виден издалека.",
                            "target_lang_translations": {
                                "en": "A powerful <w>crane</w> is working at this construction site, visible from afar."
                            }
                        }
                    ],
                    "synonyms": [
                        "подъёмный кран"
                    ],
                    "common_phrases": [
                        "башенный кран",
                        "подъёмный кран",
                        "работать на кране"
                    ],
                    "target_lang_definitions": {
                        "en": "A large machine for lifting and moving heavy loads, used on construction sites or in ports."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crane",
                                "target_lang_sense_clarification": "machinery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "756a9c22-0934-3b46-aaf6-ec290636444f",
                    "sense_definition": "Техническое запорное устройство на трубе для перекрытия потока жидкости или газа.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "plumbing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сантехник перекрыл общий <кран> в подвале, чтобы начать ремонт.",
                            "target_lang_translations": {
                                "en": "The plumber shut off the main <w>valve</w> in the basement to start repairs."
                            }
                        },
                        {
                            "text": "Нужно проверить газовый <кран> на кухне перед отъездом.",
                            "target_lang_translations": {
                                "en": "We need to check the gas <w>valve</w> in the kitchen before leaving."
                            }
                        }
                    ],
                    "common_phrases": [
                        "газовый кран",
                        "перекрыть кран",
                        "стоп-кран"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {

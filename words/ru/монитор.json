    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5df7751d-4bfa-32ab-935f-c8efcf65766e",
                    "sense_definition": "Устройство для визуального отображения информации, экран компьютера или другого электронного прибора.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я весь день смотрел в <w>монитор</w> и теперь мои глаза устали.",
                            "target_lang_translations": {
                                "en": "I looked at the <w>monitor</w> all day and now my eyes are tired."
                            }
                        },
                        {
                            "text": "На рабочем столе стоит большой современный <w>монитор</w>.",
                            "target_lang_translations": {
                                "en": "There is a large modern <w>monitor</w> on the desktop."
                            }
                        }
                    ],
                    "synonyms": [
                        "дисплей",
                        "моник"
                    ],
                    "common_phrases": [
                        "экран монитора",
                        "включить монитор",
                        "разрешение монитора"
                    ],
                    "target_lang_definitions": {
                        "en": "A device used for observing, checking, or keeping a continuous record of something, specifically a screen or display for a computer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "computer screen"
                            },
                            {
                                "target_lang_word": "display",
                                "target_lang_sense_clarification": "screen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a55e52d4-470c-3f47-a86b-b3cd13e45b20",
                    "sense_definition": "Программа для наблюдения за состоянием системы или отладки других программ.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Системный <w>монитор</w> показывает текущую загрузку процессора и памяти.",
                            "target_lang_translations": {
                                "en": "The system <w>monitor</w> shows the current CPU and memory load."
                            }
                        },
                        {
                            "text": "Программист запустил <w>монитор</w> для поиска ошибок в коде.",
                            "target_lang_translations": {
                                "en": "The programmer launched the <w>monitor</w> to search for errors in the code."
                            }
                        }
                    ],
                    "common_phrases": [
                        "монитор ресурсов",
                        "команда монитора"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A software program used to observe the state of a system or to debug other programs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "software utility"
                            },
                            {
                                "target_lang_word": "system monitor",
                                "target_lang_sense_clarification": "computing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c34d05a-65b5-319f-877e-7698736a2223",
                    "sense_definition": "Низкобортный броненосный военный корабль с мощной артиллерией во вращающейся башне, предназначенный для действий у берегов.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжелый <w>монитор</w> медленно подошел к вражескому порту.",
                            "target_lang_translations": {
                                "en": "The heavy <w>monitor</w> slowly approached the enemy port."
                            }
                        },
                        {
                            "text": "В музее мы увидели модель корабля типа <w>монитор</w> времен гражданской войны.",
                            "target_lang_translations": {
                                "en": "In the museum, we saw a model of a <w>monitor</w> class ship from the Civil War era."
                            }
                        }
                    ],
                    "common_phrases": [
                        "речной монитор",
                        "бронированный монитор"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "military history"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A class of relatively small armored warship with powerful guns, historically used for coastal or riverine warfare."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "warship type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7e607c2-09ea-34c4-8c36-db7ec5b2e0d3",
                    "sense_definition": "Устройство, выпускающее мощную струю воды под напором; используется в горном деле или при тушении пожаров.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожарные направили лафетный <w>монитор</w> на очаг возгорания.",
                            "target_lang_translations": {
                                "en": "The firefighters aimed the deck <w>monitor</w> at the source of the fire."
                            }
                        },
                        {
                            "text": "Для размыва горной породы рабочие использовали мощный <w>монитор</w>.",
                            "target_lang_translations": {
                                "en": "The workers used a powerful <w>monitor</w> to wash away the rock."
                            }
                        }
                    ],
                    "synonyms": [
                        "гидромонитор",
                        "брызгало"
                    ],
                    "common_phrases": [
                        "пожарный монитор",
                        "гидравлический монитор"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A stationary nozzle that releases a powerful jet of water; used in mining or firefighting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "water cannon/nozzle"
                            },
                            {
                                "target_lang_word": "fire monitor",
                                "target_lang_sense_clarification": "firefighting device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f660050-9b8c-3590-9296-e7c6f5a2ae55",
                    "sense_definition": "Старший учащийся, помощник учителя в школах взаимного обучения.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель поручил, чтобы <w>монитор</w> проверил выполнение задания у младших учеников.",
                            "target_lang_translations": {
                                "en": "The teacher instructed the <w>monitor</w> to check the younger students' assignment."
                            }
                        },
                        {
                            "text": "В классе за дисциплиной следил назначенный <w>монитор</w>.",
                            "target_lang_translations": {
                                "en": "An appointed <w>monitor</w> watched over discipline in the class."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A senior student appointed to assist a teacher or oversee order in a class."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "student assistant"
                            },
                            {
                                "target_lang_word": "class monitor",
                                "target_lang_sense_clarification": "school role"
                            }
                        ]
                    }
                }
            ]

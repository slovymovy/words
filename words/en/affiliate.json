    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52d08756-23b6-3404-b00d-3615b7a54023",
                    "sense_definition": "An organization, company, or person that is officially connected to a larger body or network.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "connection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local television station is an <w>affiliate</w> of a major national network.",
                            "target_lang_translations": {
                                "ru": "Местная телестанция является <w>филиалом</w> крупной национальной сети."
                            }
                        },
                        {
                            "text": "As a marketing <w>affiliate</w>, the blogger earns a commission for every product sold through her links.",
                            "target_lang_translations": {
                                "ru": "Как маркетинговый <w>партнёр</w>, блогер получает комиссию за каждый продукт, проданный по её ссылкам."
                            }
                        }
                    ],
                    "synonyms": [
                        "associate",
                        "partner",
                        "branch",
                        "member"
                    ],
                    "common_phrases": [
                        "network affiliate",
                        "local affiliate",
                        "marketing affiliate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация, компания или лицо, официально связанные с более крупной структурой или сетью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "филиал",
                                "target_lang_sense_clarification": "отделение организации"
                            },
                            {
                                "target_lang_word": "партнёр",
                                "target_lang_sense_clarification": "в бизнесе или маркетинге"
                            },
                            {
                                "target_lang_word": "аффилированная компания",
                                "target_lang_sense_clarification": "связанная компания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b652aeb-d352-313c-8b0e-679f1a83e8f7",
                    "sense_definition": "To officially connect an organization or person to a larger body or group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "connection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small college decided to <w>affiliate</w> with the state university to expand its research capabilities.",
                            "target_lang_translations": {
                                "ru": "Небольшой колледж решил <w>присоединиться</w> к государственному университету, чтобы расширить свои исследовательские возможности."
                            }
                        },
                        {
                            "text": "Many local unions choose to <w>affiliate</w> with a national federation for better representation.",
                            "target_lang_translations": {
                                "ru": "Многие местные профсоюзы решают <w>присоединиться</w> к национальной федерации для лучшего представительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "associate",
                        "ally",
                        "partner",
                        "join",
                        "link"
                    ],
                    "antonyms": [
                        "dissociate",
                        "separate"
                    ],
                    "common_phrases": [
                        "affiliate with",
                        "affiliate to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально присоединить организацию или лицо к более крупной структуре или группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединяться",
                                "target_lang_sense_clarification": "вступать в организацию"
                            },
                            {
                                "target_lang_word": "объединяться",
                                "target_lang_sense_clarification": "создавать союз"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec7d1ee2-3db2-39a0-b89b-2733d73efe24",
                    "sense_definition": "Парная дугообразная плоская кость, идущая от позвоночника к грудине и составляющая часть грудной клетки.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После падения с велосипеда он почувствовал сильную боль и заподозрил, что сломал <w>ребро</w>."
                        },
                        {
                            "text": "На рентгеновском снимке врач внимательно осмотрел все <w>рёбра</w> пациента, чтобы исключить трещины."
                        }
                    ],
                    "common_phrases": [
                        "сломать ребро",
                        "нижние рёбра"
                    ]
                },
                {
                    "sense_id": "581c9f86-7271-3150-aaea-3d1d43f83178",
                    "sense_definition": "Узкий край плоского предмета или место пересечения двух его плоскостей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_edge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Монета упала на <w>ребро</w>, покатилась по столу и со звоном упала на пол."
                        },
                        {
                            "text": "Чтобы сэкономить место на полке, мы решили поставить все книги на <w>ребро</w>."
                        }
                    ],
                    "synonyms": [
                        "край",
                        "грань"
                    ],
                    "common_phrases": [
                        "поставить на ребро",
                        "вопрос ребром"
                    ]
                },
                {
                    "sense_id": "099d7c9c-247d-397e-8fa7-e68a4889160b",
                    "sense_definition": "Отрезок прямой, образованный пересечением двух смежных граней геометрического тела.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученики на уроке геометрии считали, сколько вершин, граней и <w>рёбер</w> имеет правильный куб."
                        },
                        {
                            "text": "Длина бокового <w>ребра</w> пирамиды была вычислена с помощью теоремы Пифагора."
                        }
                    ],
                    "common_phrases": [
                        "ребро куба",
                        "боковое ребро"
                    ]
                },
                {
                    "sense_id": "1ef52703-89d9-33a0-aba6-68ece1dbc0ef",
                    "sense_definition": "Линия или дуга, соединяющая две вершины в графе.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "math_graph",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В теории графов каждое <w>ребро</w> может иметь определённый вес или направление."
                        },
                        {
                            "text": "Если удалить это <w>ребро</w>, граф перестанет быть связным и распадётся на части."
                        }
                    ],
                    "common_phrases": [
                        "вес ребра",
                        "ориентированное ребро"
                    ]
                },
                {
                    "sense_id": "3ebe1730-e4a5-36ee-a10d-1fe4ed011496",
                    "sense_definition": "Выступ на поверхности детали или конструкции, служащий для увеличения прочности или теплоотдачи.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для лучшего охлаждения двигателя цилиндры снабжены специальными металлическими <w>рёбрами</w>."
                        },
                        {
                            "text": "Бетонная плита была усилена дополнительным <w>ребром</w> жёсткости для повышения надёжности конструкции."
                        }
                    ],
                    "synonyms": [
                        "выступ"
                    ],
                    "common_phrases": [
                        "ребро жёсткости",
                        "охлаждающие рёбра"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ]
                },
                {
                    "sense_id": "fbf83867-bd0a-38ef-833a-7eaf377cbc1f",
                    "sense_definition": "Самодельная грампластинка с записью музыки, изготовленная на использованном рентгеновском снимке.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В середине прошлого века запрещённый джаз и рок-н-ролл часто записывали на <w>рёбрах</w>."
                        },
                        {
                            "text": "Коллекционер показал нам старую запись на <w>рёбрах</w>, где сквозь музыку проглядывал снимок грудной клетки."
                        }
                    ],
                    "common_phrases": [
                        "музыка на рёбрах",
                        "джаз на рёбрах"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "historical"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "Soviet era"
                        }
                    ]
                }
            ]
        }
    ]

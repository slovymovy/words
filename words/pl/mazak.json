            "senses": [
                {
                    "sense_id": "ef10d5bf-bd4b-3c8e-97cc-f0e4b21138ff",
                    "sense_definition": "Przyrząd do pisania lub rysowania wyposażony w końcówkę z chłonnego materiału nasączonego kolorowym tuszem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "writing_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci w przedszkolu uwielbiają rysować kolorowymi <w>mazakami</w> na dużych kartkach.",
                            "target_lang_translations": {
                                "ru": "Дети в детском саду обожают рисовать цветными <w>фломастерами</w> на больших листах бумаги."
                            }
                        },
                        {
                            "text": "Muszę kupić czarny <w>mazak</w>, żeby wyraźnie podpisać te pudełka.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить черный <w>маркер</w>, чтобы четко подписать эти коробки."
                            }
                        }
                    ],
                    "synonyms": [
                        "flamaster",
                        "pisak",
                        "marker"
                    ],
                    "common_phrases": [
                        "kolorowy mazak",
                        "zestaw mazaków",
                        "pisać mazakiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент для письма или рисования с наконечником из пористого материала, пропитанного цветными чернилами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фломастер",
                                "target_lang_sense_clarification": "для рисования"
                            },
                            {
                                "target_lang_word": "маркер",
                                "target_lang_sense_clarification": "перманентный или для выделения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e780a0b-083d-314b-ba6b-0d004b7eb2c4",
                    "sense_definition": "Potoczne, często żartobliwe lub lekceważące określenie twarzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_parts_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli będziesz go dalej prowokować, możesz w końcu dostać w <w>mazak</w>.",
                            "target_lang_translations": {
                                "ru": "Если будешь продолжать его провоцировать, можешь в конце концов получить по <w>морде</w>."
                            }
                        },
                        {
                            "text": "Umyj swój brudny <w>mazak</w>, zanim usiądziesz z nami do stołu.",
                            "target_lang_translations": {
                                "ru": "Умой свою грязную <w>рожу</w>, прежде чем садиться с нами за стол."
                            }
                        }
                    ],
                    "synonyms": [
                        "twarz"
                    ],
                    "common_phrases": [
                        "dostać w mazak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {

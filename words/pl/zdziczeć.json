            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2e06fcb-b697-3827-b82d-2c9f43f37129",
                    "sense_definition": "Stracić ogładę towarzyską, stać się nieufnym i zacząć unikać kontaktów z ludźmi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedząc samotnie w domu przez miesiąc, czułem, że zaczynam <w>zdziczeć</w>.",
                            "target_lang_translations": {
                                "ru": "Сидя в одиночестве дома целый месяц, я чувствовал, что начинаю <w>дичать</w>."
                            }
                        },
                        {
                            "text": "Bez stałego kontaktu z rówieśnikami dziecko może <w>zdziczeć</w> i zamknąć się w sobie.",
                            "target_lang_translations": {
                                "ru": "Без постоянного общения со сверстниками ребёнок может <w>одичать</w> и замкнуться в себе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyalienować się",
                        "zdziczeć"
                    ],
                    "antonyms": [
                        "uspołecznić się",
                        "ucywilizować się"
                    ],
                    "common_phrases": [
                        "zdziczeć w domu",
                        "zdziczeć do reszty",
                        "zdziczeć na odludziu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to express going stir-crazy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать общаться с людьми, стать нелюдимым, утратить навыки жизни в обществе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одичать",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65bc0453-b922-3425-a065-34468d80f3fc",
                    "sense_definition": "Powrócić do stanu naturalnego, tracąc cechy nabyte przez hodowlę lub uprawę (o roślinach i zwierzętach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piękny niegdyś park został zaniedbany i przez lata całkowicie <w>zdziczał</w>.",
                            "target_lang_translations": {
                                "ru": "Этот некогда прекрасный парк был заброшен и за годы полностью <w>одичал</w>."
                            }
                        },
                        {
                            "text": "Uciekł nam kot, który po roku życia na wolności zupełnie <w>zdziczał</w>.",
                            "target_lang_translations": {
                                "ru": "У нас сбежал кот, который после года жизни на воле совершенно <w>одичал</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "oswoić się"
                    ],
                    "common_phrases": [
                        "zdziczały ogród",
                        "zdziczałe psy",
                        "szybko zdziczeć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуться в дикое состояние, утратив свойства, приобретённые в результате селекции или воспитания (о растениях и животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одичать",
                                "target_lang_sense_clarification": "о животных и растениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]

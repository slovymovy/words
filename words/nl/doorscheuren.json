                    "sense_id": "468fddfd-499f-3e9e-a2d0-213c9a078b94",
                    "sense_definition": "Iets volledig doormidden of in stukken scheuren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jason had alle liefdesbrieven uit woede <w>doorgescheurd</w>.",
                            "target_lang_translations": {
                                "ru": "Джейсон в ярости <w>разорвал</w> все любовные письма."
                            }
                        },
                        {
                            "text": "Je moet het papier precies op de stippellijn <w>doorscheuren</w>.",
                            "target_lang_translations": {
                                "ru": "Нужно <w>разорвать</w> бумагу точно по пунктирной линии."
                            }
                        }
                    ],
                    "synonyms": [
                        "verscheuren",
                        "kapotscheuren"
                    ],
                    "antonyms": [
                        "plakken",
                        "lijmen"
                    ],
                    "common_phrases": [
                        "een brief doorscheuren",
                        "papier doorscheuren",
                        "midden doorscheuren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разорвать что-либо полностью пополам или на куски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разорвать",
                                "target_lang_sense_clarification": "на части или пополам"
                            },
                            {
                                "target_lang_word": "порвать",
                                "target_lang_sense_clarification": "разделить силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2866a49a-ae51-3455-8774-32bb0e9b3e0f",
                    "sense_definition": "Met hoge snelheid blijven rijden zonder te stoppen of vaart te minderen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "driving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ondanks het risico op een botsing bleef de dronken man <w>doorscheuren</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на риск столкновения, пьяный мужчина продолжал <w>мчаться</w> дальше."
                            }
                        },
                        {
                            "text": "We moesten flink <w>doorscheuren</w> op de snelweg om op tijd te komen.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось изрядно <w>погнать</w> по шоссе, чтобы успеть вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorrijden",
                        "racen"
                    ],
                    "antonyms": [
                        "stoppen",
                        "afremmen"
                    ],
                    "common_phrases": [
                        "flink doorscheuren",
                        "blijven doorscheuren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать ехать на высокой скорости, не останавливаясь или не сбавляя темп."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мчаться дальше",
                                "target_lang_sense_clarification": "продолжать быструю езду"
                            },
                            {
                                "target_lang_word": "гнать",
                                "target_lang_sense_clarification": "ехать очень быстро (разг.)"
                            }
                        ]
                    }
                }

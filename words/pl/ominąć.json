    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e84fd00-42da-3442-a4aa-ee8f1d95db70",
                    "sense_definition": "Przemieścić się obok kogoś lub czegoś, zachowując dystans, aby uniknąć zderzenia lub spotkania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca musiał gwałtownie skręcić, aby <w>ominąć</w> psa na drodze.",
                            "target_lang_translations": {
                                "ru": "Водителю пришлось резко свернуть, чтобы <w>объехать</w> собаку на дороге."
                            }
                        },
                        {
                            "text": "Udało nam się <w>ominąć</w> największe korki, jadąc bocznymi ulicami.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <w>объехать</w> самые большие пробки, поехав по боковым улицам."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyminąć",
                        "okrążyć"
                    ],
                    "antonyms": [
                        "potrącić",
                        "najechać"
                    ],
                    "common_phrases": [
                        "ominąć szerokim łukiem",
                        "ominąć przeszkodę",
                        "ominąć korek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пройти или проехать мимо кого-то или чего-то, сохраняя дистанцию, чтобы избежать столкновения или встречи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объехать",
                                "target_lang_sense_clarification": "о транспорте или препятствии на дороге"
                            },
                            {
                                "target_lang_word": "обойти",
                                "target_lang_sense_clarification": "пешком или о препятствии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bb3d7e3-f76a-311d-a607-1564963f1eea",
                    "sense_definition": "Nie uwzględnić czegoś, zrezygnować z czegoś lub sprawić, że dane zdarzenie nie dotknie określonej osoby.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "avoidance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na szczęście w tym roku <w>ominęła</w> mnie grypa i czuję się świetnie.",
                            "target_lang_translations": {
                                "ru": "К счастью, в этом году грипп <w>обошёл</w> меня стороной, и я чувствую себя отлично."
                            }
                        },
                        {
                            "text": "Czy możemy <w>ominąć</w> ten trudny temat i porozmawiać o czymś innym?",
                            "target_lang_translations": {
                                "ru": "Можем ли мы <w>обойти</w> эту трудную тему и поговорить о чём-то другом?"
                            }
                        },
                        {
                            "text": "Boję się, że coś ważnego mnie <w>ominęło</w>, gdy wyszedłem z sali.",
                            "target_lang_translations": {
                                "ru": "Боюсь, что я <w>пропустил</w> что-то важное, когда выходил из зала."
                            }
                        }
                    ],
                    "synonyms": [
                        "uniknąć",
                        "pominąć",
                        "przeoczyć"
                    ],
                    "antonyms": [
                        "uwzględnić",
                        "doświadczyć"
                    ],
                    "common_phrases": [
                        "ominąć kolejkę",
                        "ominąć prawo",
                        "ominąć temat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избежать чего-либо, пропустить что-то или не затронуть кого-то (о событии или действии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "об опасности, болезни или событии"
                            },
                            {
                                "target_lang_word": "обойти",
                                "target_lang_sense_clarification": "избежать обсуждения или выполнения (тему, закон)"
                            },
                            {
                                "target_lang_word": "пропустить",
                                "target_lang_sense_clarification": "не принять во внимание или не участвовать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
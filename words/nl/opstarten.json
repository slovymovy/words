                    "sense_id": "a9fe4d22-9203-3503-a65d-86a6999908a0",
                    "sense_definition": "Een elektronisch apparaat of computerprogramma in werking zetten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "start_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet mijn computer even opnieuw <w>opstarten</w> omdat hij vastloopt.",
                            "target_lang_translations": {

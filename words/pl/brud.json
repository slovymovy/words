            "senses": [
                {
                    "sense_id": "cabd28da-4b1c-3bb0-894b-669d15c0f8fe",
                    "sense_definition": "Wszelkie zanieczyszczenia, takie jak kurz, błoto, tłuszcz lub inne substancje, które znajdują się na powierzchni czegoś i czynią to nieczystym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_dirt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę umyć ręce, ponieważ mam na nich dużo <w>brudu</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно помыть руки, потому что на них много <w>грязи</w>."
                            }
                        },
                        {
                            "text": "Ten środek czyszczący doskonale usuwa nawet stary <w>brud</w>.",
                            "target_lang_translations": {
                                "ru": "Это чистящее средство отлично удаляет даже старую <w>грязь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanieczyszczenie",
                        "nieczystość"
                    ],
                    "antonyms": [
                        "czystość"
                    ],
                    "common_phrases": [
                        "usunąć brud",
                        "warstwa brudu",
                        "pokryty brudem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любые загрязнения, такие как пыль, жир или другие вещества, которые находятся на поверхности чего-либо и делают это нечистым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "физическое загрязнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "badeb214-a894-3e78-a5dd-1975a2722bc7",
                    "sense_definition": "Niemoralne czyny, wstydliwe sprawy osobiste, konflikty lub sekrety, które są ukrywane przed opinią publiczną.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_dirt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinniśmy wyciągać rodzinnych <w>brudów</w> przy obcych ludziach.",
                            "target_lang_translations": {
                                "ru": "Мы не должны вытаскивать семейное <w>грязное бельё</w> при посторонних."
                            }
                        },
                        {
                            "text": "Politycy często szukają na siebie nawzajem <w>brudów</w> przed wyborami.",
                            "target_lang_translations": {
                                "ru": "Политики часто ищут <w>компромат</w> друг на друга перед выборами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prać brudy",
                        "wyciągać brudy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in plural form 'brudy'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аморальные поступки, постыдные личные дела, конфликты или секреты, которые скрываются от общественности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "перен. аморальность, сплетни"
                            },
                            {
                                "target_lang_word": "грязное бельё",
                                "target_lang_sense_clarification": "идиом. скрываемые скандалы, интимные подробности"
                            }
                        ]
                    }
                }
            ]
        },

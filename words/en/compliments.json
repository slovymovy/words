{
    "word_family": [
        "complimentary",
        "uncomplimentary"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab66cb55-7172-31f8-9c75-a06750ed3322",
                    "sense_definition": "Plural of <w>compliment</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "form_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She received many <w>compliments</w> on her presentation today.",
                            "target_lang_translations": {
                                "ru": "Она получила много <w>комплиментов</w> по поводу своей сегодняшней презентации."
                            }
                        },
                        {
                            "text": "He is not used to hearing such nice <w>compliments</w> from his boss.",
                            "target_lang_translations": {
                                "ru": "Он не привык слышать такие приятные <w>комплименты</w> от своего начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "praises",
                        "commendations",
                        "accolades"
                    ],
                    "antonyms": [
                        "insults",
                        "criticisms"
                    ],
                    "common_phrases": [
                        "fish for compliments",
                        "pay compliments",
                        "return the compliment"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"комплимент\"; лестные отзывы или похвала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплименты",
                                "target_lang_sense_clarification": "лестные слова, похвала"
                            },
                            {
                                "target_lang_word": "любезности",
                                "target_lang_sense_clarification": "приятные слова, вежливые высказывания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28cc9f63-5b75-3769-afd5-57b3bc405c4b",
                    "sense_definition": "Formal greetings, regards, or good wishes, often used to accompany a gift given free of charge.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "regards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please accept this bottle of wine with the <w>compliments</w> of the management.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, примите эту бутылку вина в качестве <w>комплимента</w> от администрации."
                            }
                        },
                        {
                            "text": "Give my <w>compliments</w> to the chef for this delicious meal.",
                            "target_lang_translations": {
                                "ru": "Передайте мои <w>комплименты</w> шеф-повару за это восхитительное блюдо."
                            }
                        }
                    ],
                    "synonyms": [
                        "regards",
                        "respects",
                        "best wishes"
                    ],
                    "common_phrases": [
                        "with our compliments",
                        "compliments of the house",
                        "give my compliments to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальные приветствия, выражение уважения или подарок (обычно бесплатное угощение от заведения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплимент",
                                "target_lang_sense_clarification": "бесплатное угощение или подарок от заведения"
                            },
                            {
                                "target_lang_word": "приветствия",
                                "target_lang_sense_clarification": "передача добрых пожеланий или уважения"
                            },
                            {
                                "target_lang_word": "почтение",
                                "target_lang_sense_clarification": "выражение глубокого уважения (устар./форм.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e580ddfb-b93c-3647-9d61-693b50740535",
                    "sense_definition": "Third-person singular simple present indicative of <w>compliment</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "form_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He always <w>compliments</w> his colleagues on their hard work.",
                            "target_lang_translations": {
                                "ru": "Он всегда <w>хвалит</w> своих коллег за их усердную работу."
                            }
                        },
                        {
                            "text": "She <w>compliments</w> everyone she meets, making them feel special.",
                            "target_lang_translations": {
                                "ru": "Она <w>делает комплименты</w> всем, кого встречает, заставляя их чувствовать себя особенными."
                            }
                        }
                    ],
                    "synonyms": [
                        "praises",
                        "commends",
                        "flatters"
                    ],
                    "antonyms": [
                        "insults",
                        "criticizes"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb-form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола \"делать комплимент\" или \"хвалить\" в третьем лице единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвалит",
                                "target_lang_sense_clarification": "выражает одобрение"
                            },
                            {
                                "target_lang_word": "делает комплименты",
                                "target_lang_sense_clarification": "говорит приятные слова"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
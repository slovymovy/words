{
    "word_family": [
        "zgoda",
        "zgodzić",
        "zgodność",
        "zgodnie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c59ca6ff-c656-3fe5-a961-62bce2ba9bd9",
                    "sense_definition": "Taki, który jest w sprzeczności z czymś, narusza ustalone zasady lub różni się od faktów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contradiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten produkt jest <w>niezgodny</w> z opisem zamieszczonym w sklepie internetowym.",
                            "target_lang_translations": {
                                "ru": "Этот товар <w>не соответствует</w> описанию, размещенному в интернет-магазине."
                            }
                        },
                        {
                            "text": "Jego zachowanie było całkowicie <w>niezgodne</w> z regulaminem szkoły.",
                            "target_lang_translations": {
                                "ru": "Его поведение полностью <w>противоречило</w> школьным правилам."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprzeczny",
                        "rozbieżny",
                        "niespójny",
                        "przeciwstawny",
                        "niekonsekwentny"
                    ],
                    "antonyms": [
                        "zgodny"
                    ],
                    "common_phrases": [
                        "niezgodny z prawem",
                        "niezgodny z prawdą",
                        "niezgodny z opisem",
                        "niezgodny z regulaminem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который противоречит чему-либо, нарушает установленные правила или отличается от фактов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не соответствующий",
                                "target_lang_sense_clarification": "отличающийся от описания, фактов или стандартов"
                            },
                            {
                                "target_lang_word": "противоречащий",
                                "target_lang_sense_clarification": "нарушающий правила или идущий вразрез с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a883573c-238e-39b6-9c6c-757a55bb9062",
                    "sense_definition": "Źle dobrany, niepasujący do innych elementów lub niezdolny do wspólnego działania (np. technicznie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "compatibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zainstalowany program okazał się <w>niezgodny</w> z moim systemem operacyjnym.",
                            "target_lang_translations": {
                                "ru": "Установленная программа оказалась <w>несовместимой</w> с моей операционной системой."
                            }
                        },
                        {
                            "text": "Te dwa kolory są estetycznie <w>niezgodne</w> i źle wyglądają razem.",
                            "target_lang_translations": {
                                "ru": "Эти два цвета эстетически <w>несочетаемы</w> и плохо смотрятся вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "niekompatybilny",
                        "nieharmonijny",
                        "nieprzystający",
                        "nieodpowiedni"
                    ],
                    "antonyms": [
                        "pasujący",
                        "zgodny"
                    ],
                    "common_phrases": [
                        "niezgodny typ",
                        "sprzęt niezgodny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохо подобранный, не подходящий к другим элементам или неспособный к совместной работе (например, технически)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несовместимый",
                                "target_lang_sense_clarification": "технически или логически не подходящий"
                            },
                            {
                                "target_lang_word": "несочетающийся",
                                "target_lang_sense_clarification": "эстетически не подходящий (о цветах, стилях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "508a9214-e3a6-3217-9572-02ff981b73ba",
                    "sense_definition": "O ludziach: skłonny do kłótni, żyjący w konflikcie z innymi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>niezgodne</w> rodzeństwo ciągle walczy ze sobą o zabawki.",
                            "target_lang_translations": {
                                "ru": "Эти <w>недружные</w> братья и сестры постоянно дерутся из-за игрушек."
                            }
                        },
                        {
                            "text": "Sąsiedzi byli <w>niezgodni</w> i często wzywali policję z byle powodu.",
                            "target_lang_translations": {
                                "ru": "Соседи были <w>неуживчивыми</w> и часто вызывали полицию по любому поводу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłótliwy",
                        "konfliktowy",
                        "nieustępliwy",
                        "skłócony"
                    ],
                    "antonyms": [
                        "zgodny",
                        "bezkonfliktowy",
                        "niekonfliktowy"
                    ],
                    "common_phrases": [
                        "niezgodne małżeństwo",
                        "niezgodni sąsiedzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "О людях: склонный к ссорам, живущий в конфликте с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуживчивый",
                                "target_lang_sense_clarification": "о характере: трудно ладящий с людьми"
                            },
                            {
                                "target_lang_word": "недружный",
                                "target_lang_sense_clarification": "об отношениях: находящийся в ссоре или разладе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
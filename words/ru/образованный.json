                    ],
                    "common_phrases": [
                        "образованный человек",
                        "быть образованным",
                        "высшее образование"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a good education, possessing versatile knowledge; enlightened."
                    },
                    "translations": {
                        "en": [

                    "frequency": "High",
                    "semantic_group_id": "banking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to find an ATM to <w>withdraw</w> some cash for the weekend."
                        },
                        {
                            "text": "She <w>withdrew</w> five hundred dollars from her savings account to pay the bill."
                        }
                    ],
                    "synonyms": [
                        "take back"
                    ],
                    "antonyms": [
                        "deposit#Verb"
                    ],
                    "common_phrases": [

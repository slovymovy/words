    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "883331fc-38fe-3263-bb4c-b38ab56ab441",
                    "sense_definition": "Iets fysiek voor iemand of iets houden zodat het zichtbaar is of als bescherming dient.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieagent <w>hield</w> de automobilist een stopbord <w>voor</w>.",
                            "target_lang_translations": {
                                "en": "The police officer <w>held up</w> a stop sign for the motorist.",
                                "ru": "Полицейский <w>показал</w> водителю знак «стоп»."
                            }
                        },
                        {
                            "text": "Hij <w>hield</w> zijn handen <w>voor</w> zijn gezicht toen de felle lamp aanging.",
                            "target_lang_translations": {
                                "en": "He <w>held</w> his hands <w>in front of</w> his face when the bright light turned on.",
                                "ru": "Он <w>держал</w> руки <w>перед</w> лицом, когда включился яркий свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "tonen",
                        "laten zien",
                        "presenteren"
                    ],
                    "antonyms": [
                        "verbergen",
                        "wegstoppen"
                    ],
                    "common_phrases": [
                        "de hand voorhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To physically hold something in front of someone or something so it is visible or serves as protection.",
                        "ru": "Держать что-то физически перед кем-то или чем-то, чтобы это было видно или служило защитой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold in front of",
                                "target_lang_sense_clarification": "physical position"
                            },
                            {
                                "target_lang_word": "hold up",
                                "target_lang_sense_clarification": "to show or display"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "держать перед",
                                "target_lang_sense_clarification": "физическое положение"
                            },
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "демонстрировать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8ac1ac7-1873-3632-8899-674e8b22c4a0",
                    "sense_definition": "Iets met nadruk aan iemand of zichzelf vertellen om te overtuigen, te waarschuwen of iets te doen geloven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>hield</w> zichzelf <w>voor</w> dat hij een geboren kunstenaar was.",
                            "target_lang_translations": {
                                "en": "He <w>told</w> himself that he was a born artist.",
                                "ru": "Он <w>внушал</w> себе, что он прирожденный художник."
                            }
                        },
                        {
                            "text": "Mijn ouders hebben me altijd <w>voorgehouden</w> dat eerlijkheid het allerbelangrijkste is.",
                            "target_lang_translations": {
                                "en": "My parents always <w>impressed upon</w> me that honesty is the most important thing.",
                                "ru": "Мои родители всегда <w>внушали</w> мне, что честность — это самое главное."
                            }
                        }
                    ],
                    "synonyms": [
                        "inprenten",
                        "wijsmaken",
                        "beweren",
                        "doen geloven"
                    ],
                    "common_phrases": [
                        "zichzelf voorhouden",
                        "iemand iets voorhouden",
                        "iemand een worst voorhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To emphatically tell someone (or oneself) something to convince, warn, or make them believe it.",
                        "ru": "Настойчиво говорить что-то кому-то (или себе), чтобы убедить, предупредить или заставить поверить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to state emphatically to oneself or others"
                            },
                            {
                                "target_lang_word": "impress upon",
                                "target_lang_sense_clarification": "to emphasize a value or truth to someone"
                            },
                            {
                                "target_lang_word": "convince",
                                "target_lang_sense_clarification": "to make oneself believe something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "внушать",
                                "target_lang_sense_clarification": "настойчиво прививать мысль"
                            },
                            {
                                "target_lang_word": "убеждать",
                                "target_lang_sense_clarification": "заставлять поверить"
                            },
                            {
                                "target_lang_word": "твердить",
                                "target_lang_sense_clarification": "постоянно повторять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57f211ab-aca7-3bdc-9968-2e652d2be7f9",
                    "sense_definition": "Iemand confronteren met de realiteit, zijn eigen gedrag of de gevolgen daarvan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_confront",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hield</w> hem een spiegel <w>voor</w>, zodat hij eindelijk begreep wat zijn gedrag teweegbracht.",
                            "target_lang_translations": {
                                "en": "I <w>held up</w> a mirror to him, so he finally understood what his behavior caused.",
                                "ru": "Я <w>поднес</w> ему зеркало, чтобы он наконец понял, к чему приводит его поведение."
                            }
                        },
                        {
                            "text": "De documentaire <w>houdt</w> de samenleving de harde feiten <w>voor</w>.",
                            "target_lang_translations": {
                                "en": "The documentary <w>presents</w> society with the hard facts.",
                                "ru": "Документальный фильм <w>предъявляет</w> обществу суровые факты."
                            }
                        }
                    ],
                    "synonyms": [
                        "confronteren"
                    ],
                    "common_phrases": [
                        "een spiegel voorhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To confront someone with reality, their own behavior, or the consequences thereof.",
                        "ru": "Заставить кого-то осознать реальность, собственное поведение или последствия (часто используется в переносном смысле)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold up",
                                "target_lang_sense_clarification": "metaphorical, often used with 'a mirror'"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "to show facts or reality"
                            },
                            {
                                "target_lang_word": "confront with",
                                "target_lang_sense_clarification": "to force someone to face facts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предъявлять",
                                "target_lang_sense_clarification": "факты или доказательства"
                            },
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "отражать реальность (как в зеркале)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

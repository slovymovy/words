        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d4685f0-86a1-3aaf-bcee-f9e2fa5ac2c2",
                    "sense_definition": "Gatunek muzyki rozrywkowej charakteryzujący się prostą, rytmiczną strukturą, melodyjnością i wykorzystaniem instrumentów elektronicznych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "music_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wolnych chwilach najchętniej słucham <w>popu</w>.",
                            "target_lang_translations": {
                                "ru": "В свободное время я охотнее всего слушаю <w>поп-музыку</w>."
                            }
                        },
                        {
                            "text": "W radiu leci teraz głośny <w>pop</w>, który wpada w ucho.",
                            "target_lang_translations": {
                                "ru": "По радио сейчас играет громкая <w>попса</w>, которая запоминается."
                            }
                        }
                    ],
                    "antonyms": [
                        "muzyka poważna",
                        "muzyka klasyczna"
                    ],
                    "common_phrases": [
                        "gwiazda popu",
                        "król popu",
                        "muzyka pop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр развлекательной музыки, характеризующийся простой, ритмичной структурой, мелодичностью и использованием электронных инструментов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поп-музыка",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            },
                            {
                                "target_lang_word": "поп",
                                "target_lang_sense_clarification": "разг. сокращение"
                            },
                            {
                                "target_lang_word": "попса",
                                "target_lang_sense_clarification": "разг. часто пренебр."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0cacc6f-39c2-37d5-98e6-40bde0cb41cb",
                    "sense_definition": "Kultura masowa, popularna, nastawiona na szeroki odbiór.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To, co tworzą, to czysty <w>pop</w> bez głębszego przesłania.",
                            "target_lang_translations": {
                                "ru": "То, что они создают, — это чистая <w>попса</w> без глубокого смысла."
                            }
                        },
                        {
                            "text": "Współczesny <w>pop</w> dominuje w mediach i reklamach.",
                            "target_lang_translations": {
                                "ru": "Современная <w>поп-культура</w> доминирует в СМИ и рекламе."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Массовая культура, популярная, рассчитанная на широкую аудиторию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поп-культура",
                                "target_lang_sense_clarification": "массовая культура"
                            },
                            {
                                "target_lang_word": "поп",
                                "target_lang_sense_clarification": "в значении массовой культуры"
                            }
                        ]
                    }
                }
            ]
        },
        {

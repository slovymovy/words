    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "682d2d22-34d4-3326-8810-86e2b503a8e3",
                    "sense_definition": "Внезапно появляющийся и быстро исчезающий свет, искра или отблеск.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "light_phenomena",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В полной темноте мы вдруг увидели слабый <проблеск> света.",
                            "target_lang_translations": {
                                "en": "In complete darkness, we suddenly saw a faint <gleam> of light."
                            }
                        },
                        {
                            "text": "Маяк посылал в море короткие яркие <проблески>.",
                            "target_lang_translations": {
                                "en": "The lighthouse sent short bright <flashes> into the sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "вспышка"
                    ],
                    "antonyms": [
                        "затемнение"
                    ],
                    "common_phrases": [
                        "яркий проблеск",
                        "проблеск света",
                        "первый проблеск",
                        "проблеск зари"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden, brief flash of light, a spark, or a faint gleam."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flash",
                                "target_lang_sense_clarification": "a sudden, bright burst of light"
                            },
                            {
                                "target_lang_word": "gleam",
                                "target_lang_sense_clarification": "a faint or brief beam of light"
                            },
                            {
                                "target_lang_word": "glimmer",
                                "target_lang_sense_clarification": "a faint, unsteady light"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad55db25-698d-3eea-8947-630ca49e139b",
                    "sense_definition": "Слабое, едва заметное или кратковременное проявление какого-либо чувства, способности или состояния.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_manifestation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Даже в самой сложной ситуации у нас оставался <проблеск> надежды.",
                            "target_lang_translations": {
                                "en": "Even in the most difficult situation, we still had a <glimmer> of hope."
                            }
                        },
                        {
                            "text": "Врачи с радостью заметили у больного <проблески> сознания.",
                            "target_lang_translations": {
                                "en": "The doctors happily noticed <flashes> of consciousness in the patient."
                            }
                        }
                    ],
                    "synonyms": [
                        "намёк",
                        "просвет"
                    ],
                    "common_phrases": [
                        "проблеск надежды",
                        "проблеск сознания",
                        "проблеск гениальности",
                        "проблеск радости",
                        "проблеск ума"
                    ],
                    "target_lang_definitions": {
                        "en": "A faint, barely noticeable, or brief sign of a feeling, quality, or state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "glimmer",
                                "target_lang_sense_clarification": "a faint sign, often used with 'hope'"
                            },
                            {
                                "target_lang_word": "ray",
                                "target_lang_sense_clarification": "a slight indication, often used with 'hope'"
                            },
                            {
                                "target_lang_word": "spark",
                                "target_lang_sense_clarification": "a brief intense sign, often of intelligence or genius"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "starheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6b2adbb9-ff3d-3864-adac-3988fbf726a0",
                    "sense_definition": "Niet in staat of bereid om zich aan te passen; stijf en onbuigzaam in denken, houding of fysieke toestand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rigid_inflexible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>starre</w> houding zorgde ervoor dat er geen compromis gesloten kon worden.",
                            "target_lang_translations": {
                                "ru": "Его <w>негибкая</w> позиция привела к тому, что компромисс не мог быть достигнут."
                            }
                        },
                        {
                            "text": "De regels binnen de organisatie zijn erg <w>star</w> en bieden weinig ruimte voor uitzonderingen.",
                            "target_lang_translations": {
                                "ru": "Правила в организации очень <w>жесткие</w> и оставляют мало места для исключений."
                            }
                        },
                        {
                            "text": "Door de extreme kou waren zijn vingers helemaal <w>star</w> geworden.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного холода его пальцы стали совершенно <w>окоченевшими</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "soepel",
                        "flexibel"
                    ],
                    "common_phrases": [
                        "een starre houding",
                        "starre regels",
                        "star vasthouden aan",
                        "een starre blik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный или не желающий приспосабливаться; жесткий и негибкий в мышлении, отношении или физическом состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негибкий",
                                "target_lang_sense_clarification": "о мышлении, отношении или правилах"
                            },
                            {
                                "target_lang_word": "жесткий",
                                "target_lang_sense_clarification": "строгий, не допускающий отклонений"
                            },
                            {
                                "target_lang_word": "застывший",
                                "target_lang_sense_clarification": "о взгляде или физическом состоянии"
                            },
                            {
                                "target_lang_word": "окоченевший",
                                "target_lang_sense_clarification": "физически твердый от холода или смерти"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8381eb64-191b-37c3-a58b-2661fb0d80e1",
                    "sense_definition": "Een zeer beroemd persoon, met name een acteur, actrice of zanger(es).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "celebrity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zoals het een echte <w>star</w> betaamt, kwam ze met een grote zonnebril de lobby binnen.",
                            "target_lang_translations": {
                                "ru": "Как и подобает настоящей <w>звезде</w>, она вошла в вестибюль в больших солнечных очках."
                            }
                        },
                        {
                            "text": "Het filmfestival nodigt elk jaar een internationale <w>star</w> uit om de opening bij te wonen.",
                            "target_lang_translations": {
                                "ru": "Кинофестиваль каждый год приглашает международную <w>звезду</w> посетить открытие."
                            }
                        }
                    ],
                    "synonyms": [
                        "ster",
                        "vedette"
                    ],
                    "common_phrases": [
                        "een internationale star",
                        "als een star"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень известная личность, особенно актер, актриса или певец (певица)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звезда",
                                "target_lang_sense_clarification": "знаменитость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f424140-b31a-3912-8532-0068fe0bead3",
                    "sense_definition": "Een hemellichaam dat licht geeft (verouderde of poëtische vorm van 'ster').",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "celestial_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het oude gedicht werd de zon beschreven als een gouden <w>star</w>.",
                            "target_lang_translations": {
                                "ru": "В старом стихотворении солнце описывалось как золотая <w>звезда</w>."
                            }
                        },
                        {
                            "text": "De eenzame <w>star</w> fonkelde helder in de donkere nachtelijke hemel.",
                            "target_lang_translations": {
                                "ru": "Одинокая <w>звезда</w> ярко сверкала в темном ночном небе."
                            }
                        }
                    ],
                    "synonyms": [
                        "ster"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Standard Dutch uses 'ster'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, излучающее свет (устаревшая или поэтическая форма слова 'ster')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звезда",
                                "target_lang_sense_clarification": "небесное тело"
                            },
                            {
                                "target_lang_word": "светило",
                                "target_lang_sense_clarification": "поэтическое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
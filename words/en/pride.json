        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da29c4db-a9b7-3212-86a6-ac8f811c9a49",
                    "sense_definition": "To feel satisfied and proud about an achievement, quality, or skill; usually used reflexively.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_of_pride",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>pride</w> myself on being punctual for every meeting.",
                            "target_lang_translations": {
                                "ru": "Я <w>горжусь</w> тем, что прихожу вовремя на каждую встречу."
                            }
                        },
                        {
                            "text": "She <w>prides</w> herself on her ability to solve complex problems.",
                            "target_lang_translations": {
                                "ru": "Она <w>гордится</w> своей способностью решать сложные задачи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pride oneself on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать удовлетворение и гордость за достижение, качество или навык."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гордиться",
                                "target_lang_sense_clarification": "испытывать гордость"
                            }
                        ]
                    }
                }
            ]
        },
        {

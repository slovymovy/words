            "senses": [
                {
                    "sense_id": "58f3b79c-73d4-3547-9256-c4c1c8411e20",
                    "sense_definition": "Made known publicly or admitted; not kept secret.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <disclosed> documents provided evidence of the agreement.",
                            "target_lang_translations": {
                                "ru": "<Обнародованные> документы послужили доказательством соглашения."
                            }
                        },
                        {
                            "text": "Investors were pleased with the <disclosed> profits for the quarter.",
                            "target_lang_translations": {
                                "ru": "Инвесторы были довольны <раскрытой> прибылью за квартал."
                            }
                        }

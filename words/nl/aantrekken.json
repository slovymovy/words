                },
                {
                    "sense_id": "10690836-2a43-34c7-abdd-fdc529db64b0",
                    "sense_definition": "Een kracht uitoefenen waardoor objecten naar elkaar toe bewegen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_attract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De magneet <w>trekt</w> alle ijzerdeeltjes <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "The magnet <w>attracts</w> all iron particles.",
                                "ru": "Магнит <w>притягивает</w> все частицы железа."
                            }
                        },
                        {
                            "text": "Volgens de wet van de zwaartekracht <w>trekken</w> twee massa's elkaar <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "According to the law of gravity, two masses <w>attract</w> each other.",
                                "ru": "Согласно закону всемирного тяготения, две массы <w>притягивают</w> друг друга."
                            }
                        }
                    ],
                    "antonyms": [

                                "target_lang_sense_clarification": "очень короткое время"
                            },
                            {
                                "target_lang_word": "момент",
                                "target_lang_sense_clarification": "короткий промежуток"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "second",
                                "target_lang_sense_clarification": "unit of time"
                            },
                            {
                                "target_lang_word": "moment",
                                "target_lang_sense_clarification": "short duration"
                            }
                        ]

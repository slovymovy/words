                {
                    "sense_id": "ce24280a-aaa1-358a-a7f5-28e9e5ff2e65",
                    "sense_definition": "Osłabienie lub całkowita utrata odporności psychicznej; stan silnego smutku, depresji",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po stracie pracy mężczyzna przeżył głębokie <w>załamanie</w>.",
                            "target_lang_translations": {
                                "ru": "После потери работы мужчина пережил глубокий <w>срыв</w>."
                            }
                        },
                        {
                            "text": "Jej nagłe <w>załamanie</w> nerwowe zaniepokoiło całą rodzinę.",
                            "target_lang_translations": {
                                "ru": "Её внезапный нервный <w>срыв</w> обеспокоил всю семью."
                            }
                        }
                    ],
                    "synonyms": [

                        {
                            "text": "Anger <w>welled</w> inside him when he saw the damage.",
                            "target_lang_translations": {
                                "ru": "Гнев <w>закипел</w> внутри него, когда он увидел ущерб."
                            }
                        }
                    ],
                    "common_phrases": [
                        "well up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подниматься на поверхность и вытекать (часто о жидкостях или эмоциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наворачиваться",
                                "target_lang_sense_clarification": "о слезах"
                            },
                            {
                                "target_lang_word": "подступать",
                                "target_lang_sense_clarification": "об эмоциях или жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {

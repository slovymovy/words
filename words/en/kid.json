        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9ba1dde3-14c1-3c4e-9fa4-273bcce00226",
                    "sense_definition": "To make jokes or say things that are not serious.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "deception_humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was only <w>kidding</w> when I said I didn't like the gift.",
                            "target_lang_translations": {

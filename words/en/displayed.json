{
    "word_family": [
        "display",
        "displayer",
        "redisplay",
        "undisplayed"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f33f4286-dc31-3562-b168-b1990f6d6611",
                    "sense_definition": "Simple past and past participle of <w>display</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The computer monitor <w>displayed</w> the image clearly.",
                            "target_lang_translations": {
                                "ru": "Монитор компьютера чётко <w>отображал</w> изображение."
                            }
                        },
                        {
                            "text": "She <w>displayed</w> her artwork at the local gallery last week.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе она <w>выставила</w> свои работы в местной галерее."
                            }
                        }
                    ],
                    "synonyms": [
                        "shown",
                        "exhibited",
                        "presented"
                    ],
                    "antonyms": [
                        "hidden",
                        "concealed"
                    ],
                    "common_phrases": [
                        "displayed on screen",
                        "displayed prominently",
                        "displayed in public"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола display (показывать, демонстрировать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "давать возможность увидеть"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "показывать наглядно или публично"
                            },
                            {
                                "target_lang_word": "отображать",
                                "target_lang_sense_clarification": "воспроизводить на экране или дисплее"
                            },
                            {
                                "target_lang_word": "выставлять",
                                "target_lang_sense_clarification": "помещать где-либо для обозрения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e37ef99d-b33b-3d2c-af5e-4b5122965ce3",
                    "sense_definition": "Spread open to view or arranged so that it can be easily seen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>displayed</w> items in the shop window attracted many customers.",
                            "target_lang_translations": {
                                "ru": "<w>Выставленные</w> в витрине товары привлекали много покупателей."
                            }
                        },
                        {
                            "text": "Please do not touch the <w>displayed</w> artifacts in the museum.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не трогайте <w>выставленные</w> в музее экспонаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "shown",
                        "exposed",
                        "visible"
                    ],
                    "antonyms": [
                        "hidden",
                        "concealed"
                    ],
                    "common_phrases": [
                        "displayed goods",
                        "displayed information",
                        "displayed data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выставленный на обозрение или расположенный так, чтобы его было легко увидеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выставленный",
                                "target_lang_sense_clarification": "расположенный для показа"
                            },
                            {
                                "target_lang_word": "демонстрируемый",
                                "target_lang_sense_clarification": "показываемый в данный момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "753875df-cd09-358a-af8a-2a704134b2a7",
                    "sense_definition": "Physically spread out, extended, or unfurled.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The eagle sat on the rock with its fully <w>displayed</w> wings drying in the sun.",
                            "target_lang_translations": {
                                "ru": "Орёл сидел на камне, суша на солнце свои полностью <w>расправленные</w> крылья."
                            }
                        },
                        {
                            "text": "We had to step carefully over the <w>displayed</w> roots of the ancient beech trees.",
                            "target_lang_translations": {
                                "ru": "Нам приходилось осторожно перешагивать через <w>распластанные</w> корни древних буков."
                            }
                        }
                    ],
                    "synonyms": [
                        "spread",
                        "unfurled",
                        "extended"
                    ],
                    "antonyms": [
                        "folded",
                        "closed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически расправленный, вытянутый или развёрнутый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расправленный",
                                "target_lang_sense_clarification": "о крыльях или ткани"
                            },
                            {
                                "target_lang_word": "развёрнутый",
                                "target_lang_sense_clarification": "раскрытый во всю ширину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d16a4b5-4502-3790-abb1-283623ea49e1",
                    "sense_definition": "Depicted with wings unfurled and spread out, typically in heraldry.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family coat of arms features a golden eagle <w>displayed</w> on a blue shield.",
                            "target_lang_translations": {
                                "ru": "На фамильном гербе изображён золотой орёл <w>с распростёртыми крыльями</w> на синем щите."
                            }
                        },
                        {
                            "text": "A lion <w>displayed</w> is a less common symbol than the rampant lion.",
                            "target_lang_translations": {
                                "ru": "<w>Распростёртый</w> лев — менее распространённый символ, чем восстающий лев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "eagle displayed"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "heraldry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображённый с развёрнутыми и расправленными крыльями (обычно в геральдике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с распростёртыми крыльями",
                                "target_lang_sense_clarification": "геральдический термин"
                            },
                            {
                                "target_lang_word": "распростёртый",
                                "target_lang_sense_clarification": "широко раскинутый (о фигуре на гербе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d4ca17c-7f9a-3659-b2e7-7c3636243ede",
                    "sense_definition": "Set with lines of prominent type interspersed to catch the eye.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The advertisement used <w>displayed</w> text to highlight the special offer.",
                            "target_lang_translations": {
                                "ru": "В рекламе использовался <w>выделенный</w> текст, чтобы подчеркнуть специальное предложение."
                            }
                        },
                        {
                            "text": "This page features <w>displayed</w> headings that make the document easier to scan.",
                            "target_lang_translations": {
                                "ru": "На этой странице есть <w>выделенные</w> заголовки, которые облегчают беглый просмотр документа."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "printing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набранный выделяющимся шрифтом или расположенный отдельно для привлечения внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделенный",
                                "target_lang_sense_clarification": "отличающийся шрифтом или расположением"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
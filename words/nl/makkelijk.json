    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "34d94551-a175-35cf-81b4-a6f2ac844385",
                    "sense_definition": "Niet moeilijk; iets wat weinig moeite en inspanning vereist.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "difficulty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze opdracht is erg <w>makkelijk</w> om te doen voor beginners.",
                            "target_lang_translations": {
                                "en": "This assignment is very <w>easy</w> to do for beginners.",
                                "ru": "Это задание очень <w>лёгкое</w> для начинающих."
                            }
                        },
                        {
                            "text": "Het is <w>makkelijk</w> praten als je zelf niets hoeft te doen.",
                            "target_lang_translations": {
                                "en": "It is <w>easy</w> to talk when you don't have to do anything yourself.",
                                "ru": "<w>Легко</w> говорить, когда самому ничего не нужно делать."
                            }
                        }
                    ],
                    "synonyms": [
                        "gemakkelijk"
                    ],
                    "antonyms": [
                        "moeilijk"
                    ],
                    "common_phrases": [
                        "makkelijk praten hebben",
                        "lekker makkelijk",
                        "niet zo makkelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Not difficult; something that requires little effort or exertion.",
                        "ru": "Не трудный; то, что требует мало усилий или напряжения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "easy",
                                "target_lang_sense_clarification": "requiring little effort"
                            },
                            {
                                "target_lang_word": "simple",
                                "target_lang_sense_clarification": "not complicated"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "не требующий усилий"
                            },
                            {
                                "target_lang_word": "простой",
                                "target_lang_sense_clarification": "не сложный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcaa61aa-487c-3a95-8b6d-5a80a3488c75",
                    "sense_definition": "Zelfgenoegzaam of gemakzuchtig; te snel tevreden met zichzelf of de situatie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een nogal <w>makkelijke</w> instelling en werkt niet hard.",
                            "target_lang_translations": {
                                "en": "He has a rather <w>lax</w> attitude and doesn't work hard.",
                                "ru": "У него довольно <w>беспечное</w> отношение к делу, и он не усердно работает."
                            }
                        },
                        {
                            "text": "Je moet niet te <w>makkelijk</w> denken over dit ernstige probleem.",
                            "target_lang_translations": {
                                "en": "You shouldn't think too <w>lightly</w> about this serious problem.",
                                "ru": "Не стоит относиться к этой серьезной проблеме слишком <w>легкомысленно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zelfgenoegzaam"
                    ],
                    "common_phrases": [
                        "zich er makkelijk van afmaken"
                    ],
                    "target_lang_definitions": {
                        "en": "Complacent or lazy; too quickly satisfied with oneself or the situation.",
                        "ru": "Самодовольный или ленивый; слишком быстро довольствующийся собой или ситуацией."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lax",
                                "target_lang_sense_clarification": "lacking strictness or care"
                            },
                            {
                                "target_lang_word": "complacent",
                                "target_lang_sense_clarification": "self-satisfied"
                            },
                            {
                                "target_lang_word": "easy-going",
                                "target_lang_sense_clarification": "relaxed, often implying laziness in this context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "беспечный",
                                "target_lang_sense_clarification": "небрежный, ищущий лёгких путей"
                            },
                            {
                                "target_lang_word": "легкомысленный",
                                "target_lang_sense_clarification": "несерьезный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
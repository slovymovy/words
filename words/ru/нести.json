    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e15e270-a817-335a-a14c-3b63c4a985fa",
                    "sense_definition": "Перемещать кого-либо или что-либо, держа в руках или на себе (обычно в одном направлении)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама <несёт> тяжёлые сумки из магазина домой.",
                            "target_lang_translations": {
                                "en": "Mom is <carrying> heavy bags home from the store."
                            }
                        },
                        {
                            "text": "Отец бережно <нёс> спящего ребёнка на руках.",
                            "target_lang_translations": {
                                "en": "The father was gently <carrying> the sleeping child in his arms."
                            }
                        }
                    ],
                    "synonyms": [
                        "транспортировать"
                    ],
                    "common_phrases": [
                        "нести сумку",
                        "нести на руках",
                        "нести чемодан"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something from one place to another while holding them in one's hands or on one's body"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to transport by lifting"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "to carry or convey (somewhat formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9262ca8f-f716-3a8f-8e3b-04dd699a93ed",
                    "sense_definition": "Принимать на себя и выдерживать что-либо трудное (ответственность, расходы, наказание)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_bearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый человек должен <нести> ответственность за свои поступки.",
                            "target_lang_translations": {
                                "en": "Every person must <bear> responsibility for their actions."
                            }
                        },
                        {
                            "text": "Компания <несёт> большие убытки в этом году.",
                            "target_lang_translations": {
                                "en": "The company is <bearing> heavy losses this year."
                            }
                        }
                    ],
                    "synonyms": [
                        "терпеть",
                        "переживать",
                        "испытывать"
                    ],
                    "common_phrases": [
                        "нести ответственность",
                        "нести потери",
                        "нести наказание",
                        "нести расходы"
                    ],
                    "target_lang_definitions": {
                        "en": "To accept and endure something difficult or unpleasant, such as responsibility, costs, or punishment"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "to sustain or be answerable for"
                            },
                            {
                                "target_lang_word": "shoulder",
                                "target_lang_sense_clarification": "to accept responsibility"
                            },
                            {
                                "target_lang_word": "sustain",
                                "target_lang_sense_clarification": "to undergo or suffer (losses/injuries)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f956ae1-9139-3f9b-9101-f602ae0a9cc3",
                    "sense_definition": "Откладывать яйца (о птицах)",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша курица каждый день <несёт> свежие яйца.",
                            "target_lang_translations": {
                                "en": "Our hen <lays> fresh eggs every day."
                            }
                        },
                        {
                            "text": "Птицы обычно <несут> яйца весной в гнёздах.",
                            "target_lang_translations": {
                                "en": "Birds usually <lay> eggs in nests in the spring."
                            }

{
    "word_family": [
        "lightproduct"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f3bd8413-9dfe-3947-8cb0-4b892de3ee9c",
                    "sense_definition": "Met minder calorieën, vet of suiker dan de gewone variant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij drinkt altijd cola <w>light</w> omdat ze niet te veel suiker wil binnenkrijgen.",
                            "target_lang_translations": {
                                "ru": "Она всегда пьет колу <w>лайт</w>, так как не хочет потреблять слишком много сахара."
                            }
                        },
                        {
                            "text": "In de supermarkt kun je kiezen uit veel verschillende <w>light</w> producten.",
                            "target_lang_translations": {
                                "ru": "В супермаркете можно выбрать из множества различных продуктов <w>лайт</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "caloriearm",
                        "mager"
                    ],
                    "antonyms": [
                        "vol"
                    ],
                    "common_phrases": [
                        "cola light",
                        "light producten",
                        "light frisdrank"
                    ],
                    "target_lang_definitions": {
                        "ru": "С меньшим содержанием калорий, жира или сахара по сравнению с обычным вариантом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лайт",
                                "target_lang_sense_clarification": "о продуктах питания и напитках"
                            },
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "низкокалорийный или обезжиренный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4ff9564-c9b9-32ec-97a2-d62cea06033c",
                    "sense_definition": "Met de claim minder schadelijke stoffen te bevatten (vooral gebruikt bij tabaksproducten).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel mensen dachten onterecht dat <w>light</w> sigaretten gezonder waren dan normale.",
                            "target_lang_translations": {
                                "ru": "Многие люди ошибочно думали, что <w>лёгкие</w> сигареты полезнее обычных."
                            }
                        },
                        {
                            "text": "De term <w>light</w> is bij tabaksproducten inmiddels verboden omdat het misleidend kan zijn.",
                            "target_lang_translations": {
                                "ru": "Термин <w>лайт</w> для табачных изделий теперь запрещен, так как он может вводить в заблуждение."
                            }
                        }
                    ],
                    "antonyms": [
                        "zwaar"
                    ],
                    "common_phrases": [
                        "light sigaretten"
                    ],
                    "target_lang_definitions": {
                        "ru": "С заявлением о меньшем содержании вредных веществ (используется в основном для табачных изделий)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "о сигаретах"
                            },
                            {
                                "target_lang_word": "лайт",
                                "target_lang_sense_clarification": "маркировка на упаковке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
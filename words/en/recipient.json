    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c9791cd-0667-3c6a-921e-1f67431f646c",
                    "sense_definition": "A person or entity that receives or is awarded something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My e-mail never reached the intended <w>recipient</w>.",
                            "target_lang_translations": {
                                "ru": "Мое электронное письмо так и не дошло до предполагаемого <w>получателя</w>."
                            }
                        },
                        {
                            "text": "She was the proud <w>recipient</w> of the prestigious award.",
                            "target_lang_translations": {
                                "ru": "Она стала гордым <w>получателем</w> престижной награды."
                            }
                        }
                    ],
                    "synonyms": [
                        "receiver",
                        "addressee"
                    ],
                    "antonyms": [
                        "giver"
                    ],
                    "common_phrases": [
                        "intended recipient",
                        "award recipient",
                        "grant recipient",
                        "recipient of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или организация, которые получают что-либо (почту, деньги, награду)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получатель",
                                "target_lang_sense_clarification": "общий смысл: тот, кто получает что-либо"
                            },
                            {
                                "target_lang_word": "адресат",
                                "target_lang_sense_clarification": "получатель письма или сообщения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b1c3638-e7fe-3dcf-ad4a-02d7497419ef",
                    "sense_definition": "A patient who receives a donated organ or tissue.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heart transplant <w>recipient</w> is recovering well in the hospital.",
                            "target_lang_translations": {
                                "ru": "<w>Реципиент</w> донорского сердца хорошо восстанавливается в больнице."
                            }
                        },
                        {
                            "text": "Doctors must ensure the donor matches the <w>recipient</w> to prevent rejection.",
                            "target_lang_translations": {
                                "ru": "Врачи должны убедиться, что донор подходит <w>реципиенту</w>, чтобы предотвратить отторжение."
                            }
                        }
                    ],
                    "synonyms": [
                        "receiver"
                    ],
                    "common_phrases": [
                        "transplant recipient",
                        "organ recipient"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пациент, которому пересаживают донорский орган или ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реципиент",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4abcdb57-229a-3d88-b4a3-2bc1d8d3e207",
                    "sense_definition": "A vessel used in distillation to collect the liquid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The distilled liquid slowly dripped into the glass <w>recipient</w>.",
                            "target_lang_translations": {
                                "ru": "Дистиллированная жидкость медленно капала в стеклянный <w>приёмник</w>."
                            }
                        },
                        {
                            "text": "He carefully attached the <w>recipient</w> to the apparatus.",
                            "target_lang_translations": {
                                "ru": "Он осторожно присоединил <w>приёмник</w> к аппарату."
                            }
                        }
                    ],
                    "synonyms": [
                        "receiver"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical chemistry context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосуд, используемый при дистилляции для сбора жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приёмник",
                                "target_lang_sense_clarification": "химический термин: сосуд для сбора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c06f4b01-f0da-3567-80d6-290142c5e50a",

                },
                {
                    "sense_id": "6a182514-7af1-3a2c-a16e-d3c50a5d9af2",
                    "sense_definition": "Relating to sexual intercourse.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made a humorous remark about the <w>horizontal</w> tango.",
                            "target_lang_translations": {
                                "ru": "Он отпустил шутливое замечание о <w>горизонтальном</w> танго."
                            }

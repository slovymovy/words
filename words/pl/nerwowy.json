    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7dc71a83-c995-370c-8f37-ea465804ea88",
                    "sense_definition": "O człowieku lub zwierzęciu: taki, który łatwo się denerwuje, jest nadpobudliwy i szybko wpada w złość lub irytację.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest bardzo <w>nerwowy</w> i często krzyczy bez powodu.",
                            "target_lang_translations": {
                                "ru": "Мой сосед очень <w>нервный</w> и часто кричит без причины."
                            }
                        },
                        {
                            "text": "Ten pies jest <w>nerwowy</w>, więc lepiej do niego nie podchodź.",
                            "target_lang_translations": {
                                "ru": "Эта собака <w>нервная</w>, так что лучше к ней не подходи."
                            }
                        }
                    ],
                    "synonyms": [
                        "drażliwy",
                        "pobudliwy",
                        "niecierpliwy",
                        "impulsywny",
                        "gwałtowny",
                        "przewrażliwiony"
                    ],
                    "antonyms": [
                        "spokojny",
                        "łagodny",
                        "cierpliwy"
                    ],
                    "common_phrases": [
                        "nerwowy człowiek",
                        "być nerwowym",
                        "nerwowy charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке или животном: легко возбудимый, раздражительный, склонный быстро выходить из себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "о характере или темпераменте"
                            },
                            {
                                "target_lang_word": "раздражительный",
                                "target_lang_sense_clarification": "склонный к раздражению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fe1ef38-9483-3ccc-b405-9cfb676341f2",
                    "sense_definition": "Związany z nerwami jako częścią organizmu, dotyczący fizjologicznego układu nerwowego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzki układ <w>nerwowy</w> odpowiada za przesyłanie sygnałów w całym ciele.",
                            "target_lang_translations": {
                                "ru": "Человеческая <w>нервная</w> система отвечает за передачу сигналов по всему телу."
                            }
                        },
                        {
                            "text": "Długotrwały stres może osłabić twój system <w>nerwowy</w>.",
                            "target_lang_translations": {
                                "ru": "Длительный стресс может ослабить твою <w>нервную</w> систему."
                            }
                        }
                    ],
                    "common_phrases": [
                        "układ nerwowy",
                        "system nerwowy",
                        "wstrząs nerwowy",
                        "załamanie nerwowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к нервам или нервной системе организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4885cf02-7943-3686-84b0-ac2eb96b3ecf",
                    "sense_definition": "Świadczący o czyimś zdenerwowaniu, napięciu lub niepokoju; charakterystyczny dla stresującej sytuacji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manifestation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W biurze panowała <w>nerwowa</w> atmosfera przed przyjazdem prezesa.",
                            "target_lang_translations": {
                                "ru": "В офисе царила <w>нервная</w> атмосфера перед приездом президента."
                            }
                        },
                        {
                            "text": "Jej <w>nerwowy</w> śmiech zdradzał, że nie czuje się pewnie w tym towarzystwie.",
                            "target_lang_translations": {
                                "ru": "Её <w>нервный</w> смех выдавал, что она не чувствует себя уверенно в этой компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "niespokojny"
                    ],
                    "antonyms": [
                        "spokojny"
                    ],
                    "common_phrases": [
                        "nerwowa atmosfera",
                        "nerwowy śmiech",
                        "nerwowe ruchy",
                        "nerwowe spojrzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий волнение, напряжение или беспокойство; характерный для стрессовой ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "выражающий беспокойство"
                            },
                            {
                                "target_lang_word": "напряжённый",
                                "target_lang_sense_clarification": "об атмосфере или ситуации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
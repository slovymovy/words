    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e62ae3a-911f-30f9-a49b-44834001001d",
                    "sense_definition": "Способность сознания запоминать, сохранять и воспроизводить информацию.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моей бабушки очень хорошая <память> на лица.",
                            "target_lang_translations": {
                                "en": "My grandmother has a very good <memory> for faces."
                            }
                        },
                        {
                            "text": "Студент прочитал стихотворение по <памяти>, ни разу не заглянув в книгу.",
                            "target_lang_translations": {
                                "en": "The student recited the poem from <memory> without looking at the book once."
                            }
                        }
                    ],
                    "synonyms": [
                        "памятливость"
                    ],
                    "antonyms": [
                        "беспамятство"
                    ],
                    "common_phrases": [
                        "хорошая память",
                        "плохая память",
                        "по памяти",
                        "без памяти"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability of the mind to store, retain, and recall information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "memory",
                                "target_lang_sense_clarification": "mental faculty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a663af85-e6d8-3f5f-9b12-0572da141ae9",
                    "sense_definition": "То, что сохранилось в сознании; воспоминание о ком-либо или чём-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны хранить вечную <память> о героях войны.",
                            "target_lang_translations": {
                                "en": "We must preserve the eternal <memory> of the war heroes."
                            }
                        },
                        {
                            "text": "Это радостное событие навсегда останется в моей <памяти>.",
                            "target_lang_translations": {
                                "en": "This joyful event will remain in my <memory> forever."
                            }
                        }
                    ],
                    "synonyms": [
                        "воспоминание"
                    ],
                    "common_phrases": [
                        "вечная память",
                        "светлая память",
                        "память о прошлом",
                        "врезаться в память"
                    ],
                    "target_lang_definitions": {
                        "en": "Something retained in the mind; a recollection of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "memory",
                                "target_lang_sense_clarification": "content of the mind"
                            },
                            {
                                "target_lang_word": "recollection",
                                "target_lang_sense_clarification": "specific instance of remembering"
                            },
                            {
                                "target_lang_word": "remembrance",
                                "target_lang_sense_clarification": "act of honoring or memorializing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad1fee5d-3c08-35b3-b7c0-ecdb181342fc",
                    "sense_definition": "Часть компьютера или другого электронного устройства, предназначенная для хранения данных.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно купить новую карту <памяти> для фотоаппарата.",
                            "target_lang_translations": {
                                "en": "I need to buy a new <memory> card for the camera."
                            }
                        },
                        {
                            "text": "Оперативная <память> компьютера была полностью загружена сложными задачами.",
                            "target_lang_translations": {
                                "en": "The computer's random access <memory> was fully loaded with complex tasks."
                            }
                        }
                    ],
                    "synonyms": [
                        "запоминающее устройство"
                    ],
                    "common_phrases": [
                        "оперативная память",
                        "карта памяти",
                        "не хватает памяти"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "В значении конкретного компонента"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A part of a computer or other electronic device designed for data storage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "memory",
                                "target_lang_sense_clarification": "RAM/temporary data"
                            },
                            {
                                "target_lang_word": "storage",
                                "target_lang_sense_clarification": "permanent data retention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "827bf3f1-8b54-3b70-8c16-62148af0ac5a",
                    "sense_definition": "Предмет, который хранят как воспоминание о человеке или событии.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я подарил другу книгу на долгую <память> перед его отъездом.",
                            "target_lang_translations": {
                                "en": "I gave my friend a book as a lasting <keepsake> before his departure."
                            }
                        },
                        {
                            "text": "Эта старинная ваза дорога мне как <память> о моей бабушке.",
                            "target_lang_translations": {
                                "en": "This antique vase is dear to me as a <memento> of my grandmother."
                            }
                        }
                    ],
                    "common_phrases": [
                        "на память",
                        "на долгую память",
                        "как память"
                    ],
                    "target_lang_definitions": {
                        "en": "An object kept as a reminder of a person or event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keepsake",
                                "target_lang_sense_clarification": "object kept for memory"
                            },
                            {
                                "target_lang_word": "memento",
                                "target_lang_sense_clarification": "souvenir"
                            },
                            {
                                "target_lang_word": "remembrance",
                                "target_lang_sense_clarification": "token or gift"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
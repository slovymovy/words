                {
                    "sense_id": "886b9508-c9e4-3767-ad28-1722fbe60aac",
                    "sense_definition": "słynny wodospad na rzece Niagara, położony na granicy Kanady i Stanów Zjednoczonych",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geografia_wody",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Każdego roku miliony turystów przyjeżdżają, aby podziwiać potężną <w>Niagarę</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый год миллионы туристов приезжают, чтобы полюбоваться мощной <w>Ниагарой</w>."
                            }
                        },
                        {
                            "text": "Huk wody spadającej z <w>Niagary</w> słychać z bardzo dużej odległości.",
                            "target_lang_translations": {
                                "ru": "Грохот воды, падающей с <w>Ниагары</w>, слышен с очень большого расстояния."
                            }
                        }
                    ],
                    "synonyms": [
                        "Wodospad Niagara"
                    ],
                    "common_phrases": [
                        "Wodospad Niagara",
                        "wycieczka nad Niagarę"
                    ],
                    "target_lang_definitions": {
                        "ru": "знаменитый водопад на реке Ниагара, расположенный на границе Канады и США"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ниагарский водопад",
                                "target_lang_sense_clarification": "полное название"
                            },
                            {
                                "target_lang_word": "Ниагара",
                                "target_lang_sense_clarification": "разговорное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ce15b13-ce7b-37ea-a8e0-3b3f4718650e",
                    "sense_definition": "rzeka w Ameryce Północnej, łącząca jeziora Erie i Ontario, wyznaczająca granicę między Kanadą a Stanami Zjednoczonymi",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geografia_wody",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Rzeka <w>Niagara</w> jest stosunkowo krótka, ale przepływa przez nią ogromna ilość wody.",
                            "target_lang_translations": {
                                "ru": "Река <w>Ниагара</w> относительно короткая, но через нее протекает огромное количество воды."
                            }
                        },
                        {
                            "text": "Na rzece <w>Niagara</w> zbudowano kilka dużych elektrowni wodnych.",
                            "target_lang_translations": {
                                "ru": "На реке <w>Ниагара</w> построено несколько крупных гидроэлектростанций."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzeka Niagara",
                        "nurt Niagary"
                    ],
                    "target_lang_definitions": {
                        "ru": "река в Северной Америке, соединяющая озера Эри и Онтарио и образующая границу между Канадой и США"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ниагара",

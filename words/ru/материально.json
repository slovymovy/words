                    ],
                    "common_phrases": [
                        "материально выраженный"
                    ],
                    "target_lang_definitions": {
                        "en": "In the form of substance, physically, or in a tangible form."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "materially",
                                "target_lang_sense_clarification": "in a physical or tangible manner"
                            },
                            {
                                "target_lang_word": "physically",
                                "target_lang_sense_clarification": "in a physical form"
                            }
                        ]
                    }
                }
            ]
        },

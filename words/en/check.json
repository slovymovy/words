    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "518ab3e6-ee5e-3d40-b7cc-c055682a9593",
                    "sense_definition": "To examine something in order to establish its state, accuracy, or safety.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>check</w> your answers before handing in the test.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>проверьте</w> свои ответы перед сдачей теста."
                            }
                        },
                        {
                            "text": "I need to <w>check</w> if we have enough milk for breakfast.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>проверить</w>, хватит ли нам молока на завтрак."
                            }
                        }
                    ],
                    "synonyms": [
                        "inspect",
                        "examine",
                        "verify"
                    ],
                    "common_phrases": [
                        "check it out",
                        "check for errors",
                        "double-check"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осматривать что-либо, чтобы убедиться в его состоянии, точности или безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "удостоверяться в правильности"
                            },
                            {
                                "target_lang_word": "осматривать",
                                "target_lang_sense_clarification": "инспектировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c04a099-c8ff-3dd6-b5d5-06872d5c97a0",
                    "sense_definition": "To put a mark next to an item on a list to show that it is correct, chosen, or has been dealt with.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>check</w> the box that corresponds to your age group.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>поставьте галочку</w> в поле, соответствующем вашей возрастной группе."
                            }
                        },
                        {
                            "text": "She <w>checked</w> off each task as she completed it.",
                            "target_lang_translations": {
                                "ru": "Она <w>отмечала</w> каждое задание по мере его выполнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "tick",
                        "mark"
                    ],
                    "antonyms": [
                        "uncheck"
                    ],
                    "common_phrases": [
                        "check off",
                        "check the box"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить отметку рядом с пунктом в списке, чтобы показать, что он правильный, выбран или выполнен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмечать",
                                "target_lang_sense_clarification": "ставить пометку"
                            },
                            {
                                "target_lang_word": "ставить галочку",
                                "target_lang_sense_clarification": "графический знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae453628-1ec6-3b74-80d5-d8afedf48c3b",
                    "sense_definition": "To leave luggage, coats, or other items with an official to be kept safe or transported.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>check</w> your large bags at the counter before boarding.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>сдать</w> большие сумки на стойке перед посадкой."
                            }
                        },
                        {
                            "text": "We can <w>check</w> our coats at the entrance of the museum.",
                            "target_lang_translations": {
                                "ru": "Мы можем <w>сдать</w> пальто при входе в музей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "check in luggage",
                        "check a bag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять багаж, верхнюю одежду или другие предметы официальному лицу для хранения или транспортировки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "в багаж или гардероб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3cba922-b22d-335c-95b0-233964d2a1e4",
                    "sense_definition": "To stop, slow down, or hold back the progress of something undesirable.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government introduced new laws to <w>check</w> the rise in inflation.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело новые законы, чтобы <w>сдержать</w> рост инфляции."
                            }
                        },
                        {
                            "text": "He struggled to <w>check</w> his anger when he heard the bad news.",
                            "target_lang_translations": {
                                "ru": "Он с трудом пытался <w>сдержать</w> гнев, когда услышал плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "curb",
                        "restrain",
                        "limit",
                        "control"
                    ],
                    "common_phrases": [
                        "check oneself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Останавливать, замедлять или сдерживать развитие чего-либо нежелательного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдерживать",
                                "target_lang_sense_clarification": "контролировать эмоции или процессы"
                            },
                            {
                                "target_lang_word": "останавливать",
                                "target_lang_sense_clarification": "прекращать движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3987f79-581e-39e8-8112-c3938bfc30ae",
                    "sense_definition": "To make a move in chess that directly threatens the opponent's king.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The grandmaster <w>checked</w> the king with her queen.",
                            "target_lang_translations": {
                                "ru": "Гроссмейстер <w>объявила шах</w> королю своим ферзем."
                            }
                        },
                        {
                            "text": "You cannot castle if your king is being <w>checked</w>.",
                            "target_lang_translations": {
                                "ru": "Нельзя делать рокировку, если вашему королю <w>объявлен шах</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "chess"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать ход в шахматах, создающий прямую угрозу королю противника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объявлять шах",
                                "target_lang_sense_clarification": "в шахматах"
                            },
                            {
                                "target_lang_word": "атаковать",
                                "target_lang_sense_clarification": "короля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96d3c93d-3ef8-3b8a-b37c-4f1344db58c9",
                    "sense_definition": "To choose not to bet in a card game, passing the action to the next player.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "6",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to <w>check</w> because his hand was not very strong.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>пропустить ход</w>, так как его карты были не очень сильными."
                            }
                        },
                        {
                            "text": "If everyone <w>checks</w>, we will see the next card for free.",
                            "target_lang_translations": {
                                "ru": "Если все <w>пропустят ход</w>, мы увидим следующую карту бесплатно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "poker"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить не делать ставку в карточной игре, передавая ход следующему игроку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропускать ход",
                                "target_lang_sense_clarification": "в картах"
                            },
                            {
                                "target_lang_word": "чекать",
                                "target_lang_sense_clarification": "сленг в покере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "496dbe28-5501-372c-89ca-e1656fdddc89",
                    "sense_definition": "To obstruct or hit an opponent in sports like hockey to gain possession of the puck or ball.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "7",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defender <w>checked</w> the forward into the boards.",
                            "target_lang_translations": {
                                "ru": "Защитник <w>применил силовой прием</w> против нападающего у борта."
                            }
                        },
                        {
                            "text": "It is a foul to <w>check</w> a player from behind.",
                            "target_lang_translations": {
                                "ru": "Запрещено <w>атаковать</w> игрока сзади."
                            }
                        }
                    ],
                    "common_phrases": [
                        "body check"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Блокировать или толкать противника в спорте (например, в хоккее), чтобы завладеть шайбой или мячом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "применять силовой прием",
                                "target_lang_sense_clarification": "в спорте"
                            },
                            {
                                "target_lang_word": "блокировать",
                                "target_lang_sense_clarification": "препятствовать движению"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca9b051e-968a-3bc0-b954-7f6129e40328",
                    "sense_definition": "An act of examining something to verify its condition, accuracy, or safety.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We perform a routine safety <w>check</w> on the equipment every morning.",
                            "target_lang_translations": {
                                "ru": "Мы проводим плановую <w>проверку</w> безопасности оборудования каждое утро."
                            }
                        },
                        {
                            "text": "Give your essay a final <w>check</w> for spelling mistakes.",
                            "target_lang_translations": {
                                "ru": "Выполните финальную <w>проверку</w> своего эссе на наличие орфографических ошибок."
                            }
                        }
                    ],
                    "synonyms": [
                        "inspection",
                        "examination"
                    ],
                    "common_phrases": [
                        "background check",
                        "spot check",
                        "reality check",
                        "security check"
                    ],
                    "target_lang_definitions": {

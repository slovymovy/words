    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9b1268c7-8dd4-3a7e-b28f-f963fae70524",
                    "sense_definition": "A state located in the southeastern United States.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geography_state",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "My family moved to <w>Alabama</w> last summer to enjoy the warmer weather.",
                            "target_lang_translations": {
                                "ru": "Прошлым летом моя семья переехала в <w>Алабаму</w>, чтобы насладиться более теплой погодой."
                            }
                        },
                        {
                            "text": "The laws in the state of <w>Alabama</w> are quite different from those in the north.",
                            "target_lang_translations": {
                                "ru": "Законы штата <w>Алабама</w> довольно сильно отличаются от законов на севере."
                            }
                        }
                    ],
                    "synonyms": [
                        "Bama"
                    ],
                    "common_phrases": [
                        "state of Alabama",
                        "Sweet Home Alabama"
                    ],
                    "target_lang_definitions": {
                        "ru": "Штат, расположенный на юго-востоке США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Алаба́ма",
                                "target_lang_sense_clarification": "штат США"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "432f77cd-92dc-3f0d-bf65-a776fad5506b",
                    "sense_definition": "The University of Alabama, often referred to in the context of collegiate sports.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "institution",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "He received a scholarship to play football for <w>Alabama</w> next season.",
                            "target_lang_translations": {
                                "ru": "Он получил стипендию, чтобы играть в футбол за <w>Алабаму</w> в следующем сезоне."
                            }
                        },
                        {
                            "text": "She is a proud graduate of <w>Alabama</w> and visits the campus often.",
                            "target_lang_translations": {
                                "ru": "Она гордая выпускница <w>Алабамы</w> и часто посещает кампус."
                            }
                        }
                    ],
                    "synonyms": [
                        "Bama"
                    ],
                    "target_lang_definitions": {
                        "ru": "Алабамский университет, часто упоминаемый в контексте студенческого спорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Алаба́ма",
                                "target_lang_sense_clarification": "университет"
                            },
                            {
                                "target_lang_word": "Алабамский университет",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55bd7fd6-f05c-3ccd-a1a4-ffdfcfdb7ff3",
                    "sense_definition": "A river that flows through the state of Alabama to the Mobile River.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography_river",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "The <w>Alabama</w> winds through the countryside before reaching the coast.",
                            "target_lang_translations": {
                                "ru": "<w>Алабама</w> петляет по сельской местности, прежде чем достичь побережья."
                            }
                        },
                        {
                            "text": "Fishermen often take their boats out on the <w>Alabama</w> during the weekend.",
                            "target_lang_translations": {
                                "ru": "Рыбаки часто выходят на лодках на <w>Алабаму</w> в выходные."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Река, протекающая через штат Алабама и впадающая в реку Мобил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Алаба́ма",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7194180-b134-3046-9df5-53ac574bc7c1",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c749662b-0a94-3434-bf56-bb9f6ba9c365",
                    "sense_definition": "An amorphous solid, often transparent substance, usually made by melting silica sand with various additives.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large window in the living room is made of clear <w>glass</w>.",
                            "target_lang_translations": {
                                "ru": "Большое окно в гостиной сделано из прозрачного <w>стекла</w>."
                            }
                        },
                        {
                            "text": "Please be very careful not to step on the broken <w>glass</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будь очень осторожен, не наступи на разбитое <w>стекло</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "broken glass",
                        "stained glass",
                        "safety glass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аморфное твердое вещество, часто прозрачное, обычно получаемое путем плавления кварцевого песка с различными добавками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стекло",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61f4a116-6eb7-34f4-9a72-e5ada30a6a77",
                    "sense_definition": "A vessel from which one drinks, especially one made of glass, plastic, or similar translucent material.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "container_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Would you like a <w>glass</w> of cold water with your meal?",
                            "target_lang_translations": {
                                "ru": "Хотите <w>стакан</w> холодной воды к еде?"
                            }
                        },
                        {
                            "text": "She raised her <w>glass</w> to propose a toast to the happy couple.",
                            "target_lang_translations": {
                                "ru": "Она подняла свой <w>бокал</w>, чтобы произнести тост за счастливую пару."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wine glass",
                        "water glass",
                        "empty glass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосуд, из которого пьют, особенно сделанный из стекла, пластика или подобного материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стакан",
                                "target_lang_sense_clarification": "обычный сосуд для питья"
                            },
                            {
                                "target_lang_word": "бокал",
                                "target_lang_sense_clarification": "для вина или коктейлей"
                            },
                            {
                                "target_lang_word": "рюмка",
                                "target_lang_sense_clarification": "для крепких напитков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0924f22c-e6ce-3a74-9aec-09d660964239",
                    "sense_definition": "The quantity of liquid contained in a drinking vessel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I try to drink at least one <w>glass</w> of milk every morning.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь выпивать хотя бы один <w>стакан</w> молока каждое утро."
                            }
                        },
                        {
                            "text": "He poured himself a <w>glass</w> of juice before leaving for work.",
                            "target_lang_translations": {
                                "ru": "Он налил себе <w>стакан</w> сока перед уходом на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "glass of water",
                        "glass of wine",
                        "glass of milk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество жидкости, вмещающееся в питьевой сосуд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стакан",
                                "target_lang_sense_clarification": "мера объема"

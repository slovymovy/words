    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fe34d066-12e6-3537-b933-89ed7bb9cead",
                    "sense_definition": "С участием сознания и разума; понимая смысл своих действий.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны <w>сознательно</w> подходить к выбору своей будущей профессии.",
                            "target_lang_translations": {
                                "en": "We must approach the choice of our future profession <w>consciously</w>."
                            }
                        },
                        {
                            "text": "Пациент пришёл в себя и <w>сознательно</w> отвечал на вопросы врача.",
                            "target_lang_translations": {
                                "en": "The patient regained consciousness and answered the doctor's questions <w>lucidly</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "осмысленно",
                        "осознанно"
                    ],
                    "antonyms": [
                        "бессознательно"
                    ],
                    "common_phrases": [
                        "действовать сознательно",
                        "выбирать сознательно",
                        "жить сознательно"
                    ],
                    "target_lang_definitions": {
                        "en": "With full awareness and understanding; in a state of consciousness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consciously",
                                "target_lang_sense_clarification": "with awareness"
                            },
                            {
                                "target_lang_word": "knowingly",
                                "target_lang_sense_clarification": "with understanding"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c9b6ff5-d407-3edd-919a-6f8a26059f45",
                    "sense_definition": "С определённой целью, по собственному намерению; не случайно.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>сознательно</w> пошёл на этот риск ради спасения команды.",
                            "target_lang_translations": {
                                "en": "He <w>deliberately</w> took this risk to save the team."
                            }
                        },
                        {
                            "text": "Автор <w>сознательно</w> не стал упоминать имена участников конфликта.",
                            "target_lang_translations": {
                                "en": "The author <w>intentionally</w> did not mention the names of the conflict participants."
                            }
                        }
                    ],
                    "synonyms": [
                        "преднамеренно",
                        "умышленно",
                        "нарочно"
                    ],
                    "common_phrases": [
                        "сознательно игнорировать",
                        "сознательно отказаться",
                        "сознательно затягивать"
                    ],
                    "target_lang_definitions": {
                        "en": "Done on purpose or with a specific intent; not accidentally."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deliberately",
                                "target_lang_sense_clarification": "on purpose"
                            },
                            {
                                "target_lang_word": "intentionally",
                                "target_lang_sense_clarification": "with intent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f3cd422-570d-3c2b-839e-3e999df6fde0",
                    "sense_definition": "С чувством ответственности и долга; честно и старательно.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый сотрудник должен <w>сознательно</w> относиться к своим обязанностям.",
                            "target_lang_translations": {
                                "en": "Every employee must treat their duties <w>conscientiously</w>."
                            }
                        },
                        {
                            "text": "Граждане страны <w>сознательно</w> соблюдают установленные законы и правила.",
                            "target_lang_translations": {
                                "en": "The country's citizens <w>responsibly</w> observe the established laws and rules."
                            }
                        }
                    ],
                    "synonyms": [
                        "добросовестно",
                        "на совесть"
                    ],
                    "common_phrases": [
                        "сознательно относиться",
                        "сознательно выполнять",
                        "сознательно трудиться"
                    ],
                    "target_lang_definitions": {
                        "en": "With a sense of duty, responsibility, and honesty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conscientiously",
                                "target_lang_sense_clarification": "with a sense of duty"
                            },
                            {
                                "target_lang_word": "responsibly",
                                "target_lang_sense_clarification": "in a responsible manner"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
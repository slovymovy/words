{
    "word_family": [
        "communicate",
        "communicative",
        "communicable",
        "communicator",
        "uncommunicative"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4c01ee9d-0d7f-3279-ae18-fa502354566d",
                    "sense_definition": "The process of exchanging information, ideas, or feelings between people or entities.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "info_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Effective <w>communication</w> is essential for a successful business relationship.",
                            "target_lang_translations": {
                                "ru": "Эффективная <w>коммуникация</w> необходима для успешных деловых отношений."
                            }
                        },
                        {
                            "text": "They remained in close <w>communication</w> despite living in different countries.",
                            "target_lang_translations": {
                                "ru": "Они поддерживали тесную <w>связь</w>, несмотря на то, что жили в разных странах."
                            }
                        }
                    ],
                    "synonyms": [
                        "interaction",
                        "contact",
                        "correspondence",
                        "exchange"
                    ],
                    "common_phrases": [
                        "means of communication",
                        "communication skills",
                        "lack of communication",
                        "verbal communication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс обмена информацией, идеями или чувствами между людьми или организациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общение",
                                "target_lang_sense_clarification": "взаимодействие между людьми"
                            },
                            {
                                "target_lang_word": "коммуникация",
                                "target_lang_sense_clarification": "обмен информацией, часто в деловом или формальном контексте"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "контакт, состояние соединения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bb2dfb7-cd26-3ba2-8e92-26723ebb5d72",
                    "sense_definition": "A specific message, letter, or piece of information that is sent or received.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "info_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general received an urgent <w>communication</w> from the front lines.",
                            "target_lang_translations": {
                                "ru": "Генерал получил срочное <w>сообщение</w> с передовой."
                            }
                        },
                        {
                            "text": "All official <w>communications</w> must be recorded and archived.",
                            "target_lang_translations": {
                                "ru": "Все официальные <w>сообщения</w> должны быть зарегистрированы и заархивированы."
                            }
                        }
                    ],
                    "synonyms": [
                        "message",
                        "dispatch",
                        "report",
                        "memo",
                        "note"
                    ],
                    "common_phrases": [
                        "official communication",
                        "written communication",
                        "privileged communication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное сообщение, письмо или информация, которая отправляется или получается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщение",
                                "target_lang_sense_clarification": "передаваемое сведение, известие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb45dc0c-92b2-3515-a71e-8aab94aaf7a9",
                    "sense_definition": "A physical passage, opening, or route that connects two places.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The secret door provided direct <w>communication</w> between the study and the garden.",
                            "target_lang_translations": {
                                "ru": "Потайная дверь обеспечивала прямое <w>сообщение</w> между кабинетом и садом."
                            }
                        },
                        {
                            "text": "Heavy snow blocked all lines of <w>communication</w> to the remote village.",
                            "target_lang_translations": {
                                "ru": "Сильный снегопад перекрыл все пути <w>сообщения</w> с отдалённой деревней."
                            }
                        }
                    ],
                    "synonyms": [
                        "passage",
                        "connection",
                        "link",
                        "access"
                    ],
                    "common_phrases": [
                        "direct communication",
                        "line of communication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический проход, отверстие или маршрут, соединяющий два места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщение",
                                "target_lang_sense_clarification": "путь или проход между местами"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "соединение между пунктами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54e2a1a8-8b64-3284-aaa7-bfcc092a2653",
                    "sense_definition": "A connection between two tissues, organs, or cavities in the body.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor identified an abnormal <w>communication</w> between the two blood vessels.",
                            "target_lang_translations": {
                                "ru": "Врач обнаружил аномальное <w>сообщение</w> между двумя кровеносными сосудами."
                            }
                        },
                        {
                            "text": "Surgery was required to close the <w>communication</w> between the stomach and the esophagus.",
                            "target_lang_translations": {
                                "ru": "Потребовалась операция, чтобы закрыть <w>сообщение</w> между желудком и пищеводом."
                            }
                        }
                    ],
                    "synonyms": [
                        "connection",
                        "fistula",
                        "shunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединение между двумя тканями, органами или полостями в теле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщение",
                                "target_lang_sense_clarification": "анатомическое соединение"
                            },
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "связь тканей или органов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
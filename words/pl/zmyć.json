    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "01e223f0-80ec-318f-bd74-62b9fe992bb1",
                    "sense_definition": "Usunąć brud, farbę, kosmetyki lub inną substancję z powierzchni za pomocą wody, często z dodatkiem detergentu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie <w>zmyć</w> makijaż przed pójściem spać.",
                            "target_lang_translations": {
                                "ru": "Ты должна тщательно <w>смыть</w> макияж перед сном."
                            }
                        },
                        {
                            "text": "Deszcz szybko <w>zmył</w> kurz z naszego samochodu.",
                            "target_lang_translations": {
                                "ru": "Дождь быстро <w>смыл</w> пыль с нашей машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmazać",
                        "usunąć",
                        "oczyścić"
                    ],
                    "antonyms": [
                        "pobrudzić",
                        "pomazać"
                    ],
                    "common_phrases": [
                        "zmyć makijaż",
                        "zmyć farbę",
                        "zmyć plamę",
                        "zmyć z siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить грязь, краску, косметику или другое вещество с поверхности с помощью воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смыть",
                                "target_lang_sense_clarification": "удалить с поверхности"
                            },
                            {
                                "target_lang_word": "отмыть",
                                "target_lang_sense_clarification": "очистить от грязи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a99a672e-8801-3104-a5db-0d9e3ef85808",
                    "sense_definition": "Umyć brudne naczynia i sztućce po posiłku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po obiedzie każdy musi <w>zmyć</w> swój talerz.",
                            "target_lang_translations": {
                                "ru": "После обеда каждый должен <w>помыть</w> свою тарелку."
                            }
                        },
                        {
                            "text": "Czy możesz dzisiaj <w>zmyć</w> naczynia zamiast mnie?",
                            "target_lang_translations": {
                                "ru": "Ты можешь сегодня <w>помыть</w> посуду вместо меня?"
                            }
                        }
                    ],
                    "synonyms": [
                        "pozmywać"
                    ],
                    "common_phrases": [
                        "zmyć naczynia",
                        "zmyć po obiedzie",
                        "zmyć gary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помыть грязную посуду и столовые приборы после еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помыть",
                                "target_lang_sense_clarification": "посуду"
                            },
                            {
                                "target_lang_word": "вымыть",
                                "target_lang_sense_clarification": "дочиста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d391147-67cf-3233-b4a7-39a79e1ed02c",
                    "sense_definition": "Zabrać coś lub usunąć z powierzchni ziemi siłą płynącej wody (np. przez falę, rzekę, deszcz).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nature_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielka fala <w>zmyła</w> turystę ze skały do morza.",
                            "target_lang_translations": {
                                "ru": "Огромная волна <w>смыла</w> туриста со скалы в море."
                            }
                        },
                        {
                            "text": "Gwałtowna powódź <w>zmyła</w> drewniany most w dolinie.",
                            "target_lang_translations": {
                                "ru": "Внезапное наводнение <w>смыло</w> деревянный мост в долине."
                            }
                        }
                    ],
                    "synonyms": [
                        "spłukać",
                        "znieść"
                    ],
                    "common_phrases": [
                        "zmyć z pokładu",
                        "zmyć z powierzchni",
                        "zmyć ślady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Унести или удалить с поверхности земли силой текущей воды (волной, рекой, дождем)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смыть",
                                "target_lang_sense_clarification": "унести потоком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f879a6c0-b8b0-3d26-8946-cbd9ac6220de",
                    "sense_definition": "Zejść z powierzchni, przestać być widocznym pod wpływem mycia lub wody (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że ta plama z wina się <w>zmyje</w>.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, это пятно от вина <w>отмоется</w>."
                            }
                        },
                        {
                            "text": "Ten tatuaż jest tymczasowy i <w>zmyje</w> się po kilku dniach.",
                            "target_lang_translations": {
                                "ru": "Эта татуировка временная и <w>смоется</w> через несколько дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "zejść",
                        "zniknąć"
                    ],
                    "common_phrases": [
                        "łatwo się zmyć",
                        "nie zmyć się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сойти, исчезнуть с поверхности под воздействием воды или мытья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смыться",
                                "target_lang_sense_clarification": "исчезнуть"
                            },
                            {
                                "target_lang_word": "отмыться",
                                "target_lang_sense_clarification": "стать чистым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e448476-f1d8-3fb5-ba47-25001742b52c",
                    "sense_definition": "Szybko opuścić jakieś miejsce, uciec lub wyjść niepostrzeżenie (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Impreza była nudna, więc postanowiłem się <w>zmyć</w> do domu.",
                            "target_lang_translations": {
                                "ru": "Вечеринка была скучной, поэтому я решил <w>смыться</w> домой."
                            }
                        },
                        {
                            "text": "Musimy się stąd <w>zmyć</w>, zanim ktoś nas zauważy.",
                            "target_lang_translations": {
                                "ru": "Нам нужно отсюда <w>смыться</w>, пока нас никто не заметил."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciec",
                        "zwiać",
                        "ulotnić się",
                        "wynieść się"
                    ],
                    "antonyms": [
                        "zostać"
                    ],
                    "common_phrases": [
                        "zmyć się stąd",
                        "muszę się zmyć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро покинуть место, сбежать или уйти незаметно (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смыться",
                                "target_lang_sense_clarification": "сбежать"
                            },
                            {
                                "target_lang_word": "улизнуть",
                                "target_lang_sense_clarification": "уйти тайком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f65c1ebb-bee8-311f-adf5-7f43a7f457ee",
                    "sense_definition": "Pozbyć się negatywnego uczucia, winy lub złej reputacji (znaczenie przenośne).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Próbował <w>zmyć</w> z siebie hańbę po przegranej bitwie.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>смыть</w> с себя позор после проигранной битвы."
                            }
                        },
                        {
                            "text": "Żadne słowa nie mogą <w>zmyć</w> winy za to, co zrobił.",
                            "target_lang_translations": {
                                "ru": "Никакие слова не могут <w>смыть</w> вину за то, что он сделал."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmazać",
                        "odkupić"
                    ],
                    "common_phrases": [
                        "zmyć hańbę",
                        "zmyć winę",
                        "zmyć grzechy"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51e0d75a-d527-34bb-8f1f-26fcaf058459",
                    "sense_definition": "Een voorgenomen reeks handelingen of een idee om een bepaald doel te bereiken",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>plan</w> is om morgenochtend vroeg te vertrekken.",
                            "target_lang_translations": {
                                "ru": "<w>План</w> состоит в том, чтобы уехать завтра рано утром.",
                                "en": "The <w>plan</w> is to leave early tomorrow morning."
                            }
                        },
                        {
                            "text": "Hij is van <w>plan</w> om volgend jaar een lange reis te maken.",
                            "target_lang_translations": {
                                "ru": "Он <w>планирует</w> отправиться в долгое путешествие в следующем году.",
                                "en": "He <w>plans</w> to take a long trip next year."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een plan maken",
                        "van plan zijn",
                        "volgens plan",
                        "een plan bedenken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задуманный порядок действий или идея для достижения определенной цели",
                        "en": "A set of intended actions or an idea to achieve a specific goal"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "замысел, программа действий"
                            },
                            {
                                "target_lang_word": "намерение",
                                "target_lang_sense_clarification": "желание что-то сделать (в выражении 'van plan zijn')"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "plan",
                                "target_lang_sense_clarification": "a detailed proposal for doing or achieving something"
                            },
                            {
                                "target_lang_word": "intention",
                                "target_lang_sense_clarification": "an aim or purpose (often in 'van plan zijn')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b79ad58b-c696-309d-ac68-ec1d0bbe7d17",
                    "sense_definition": "Een gedetailleerde tekening of kaart van een gebouw, stad of gebied",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "map",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het <w>plan</w> kun je zien waar de nooduitgangen zich bevinden.",
                            "target_lang_translations": {
                                "ru": "На <w>плане</w> можно увидеть, где находятся аварийные выходы.",
                                "en": "On the <w>plan</w>, you can see where the emergency exits are located."
                            }
                        },
                        {
                            "text": "De architect liet ons het <w>plan</w> van de nieuwe woning zien.",
                            "target_lang_translations": {
                                "ru": "Архитектор показал нам <w>план</w> нового дома.",
                                "en": "The architect showed us the <w>plan</w> of the new house."
                            }
                        }
                    ],
                    "synonyms": [
                        "plattegrond"
                    ],
                    "common_phrases": [
                        "plan van de stad",
                        "een plan tekenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подробный чертеж или карта здания, города или местности",
                        "en": "A detailed drawing or map of a building, city, or area"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "чертеж, схема"
                            },
                            {
                                "target_lang_word": "схема",
                                "target_lang_sense_clarification": "графическое изображение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "plan",
                                "target_lang_sense_clarification": "a technical drawing or diagram"
                            },
                            {
                                "target_lang_word": "map",
                                "target_lang_sense_clarification": "a visual representation of an area"
                            },
                            {
                                "target_lang_word": "blueprint",
                                "target_lang_sense_clarification": "a design plan or technical drawing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d632403-4531-35a8-b2b5-4a13b0acc098",
                    "sense_definition": "Een niveau of vlak waarop iets zich afspeelt of functioneert",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten proberen de discussie naar een hoger <w>plan</w> te tillen.",
                            "target_lang_translations": {
                                "ru": "Мы должны попытаться поднять дискуссию на более высокий <w>уровень</w>.",
                                "en": "We must try to lift the discussion to a higher <w>level</w>."
                            }
                        },
                        {
                            "text": "Op artistiek <w>plan</w> was de voorstelling een groot succes.",
                            "target_lang_translations": {
                                "ru": "В художественном <w>плане</w> представление имело большой успех.",
                                "en": "On an artistic <w>level</w>, the performance was a great success."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op een hoger plan",
                        "op artistiek plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень или сфера, в которой что-то происходит или функционирует",
                        "en": "A level or plane on which something takes place or functions"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "степень развития или качества"
                            },
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "аспект, точка зрения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "level",
                                "target_lang_sense_clarification": "a position on a scale of quantity, quality, or intensity"
                            },
                            {
                                "target_lang_word": "plane",
                                "target_lang_sense_clarification": "a level of existence, thought, or development"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

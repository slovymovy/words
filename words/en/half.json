                {
                    "sense_id": "01a30db9-d337-3916-bcbe-dc272ed65099",
                    "sense_definition": "Partial or incomplete; resembling a half but indefinite in amount.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "partial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician told a <w>half</w> truth to avoid the scandal.",
                            "target_lang_translations": {
                                "ru": "Политик сказал <w>полуправду</w>, чтобы избежать скандала."
                            }

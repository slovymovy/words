{
    "word_family": [
        "half",
        "weg",
        "helft",
        "halveren"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4d2a61c5-abc1-3b76-a990-fb4fbdd7cda1",
                    "sense_definition": "Op het middelpunt van een afstand, weg of tijdsduur; op de helft.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit huis staat ergens aan het begin van de straat en niet <w>halverwege</w>.",
                            "target_lang_translations": {
                                "en": "This house stands somewhere at the beginning of the street and not <w>halfway</w>.",
                                "ru": "Этот дом стоит где-то в начале улицы, а не <w>посередине</w>."
                            }
                        },
                        {
                            "text": "<w>Halverwege</w> kwam ons een boswachter tegemoet om de weg te wijzen.",
                            "target_lang_translations": {
                                "en": "<w>Halfway</w>, a forest ranger came towards us to show the way.",
                                "ru": "<w>На полпути</w> нас встретил лесник, чтобы показать дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "halfweg"
                    ],
                    "antonyms": [
                        "aan het begin",
                        "aan het einde"
                    ],
                    "common_phrases": [
                        "ergens halverwege"
                    ],
                    "target_lang_definitions": {
                        "en": "At the midpoint of a distance, path, or duration; at the halfway point.",
                        "ru": "На середине расстояния, пути или временного отрезка; на полпути."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "halfway",
                                "target_lang_sense_clarification": "at the middle point"
                            },
                            {
                                "target_lang_word": "midway",
                                "target_lang_sense_clarification": "in the middle of the way or distance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на полпути",
                                "target_lang_sense_clarification": "о пройденном расстоянии"
                            },
                            {
                                "target_lang_word": "посередине",
                                "target_lang_sense_clarification": "в центре чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "c113e1ae-583e-39ac-bd3a-73ee34bea68a",
                    "sense_definition": "In het midden van een periode, afstand of proces.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zijn nu <w>halverwege</w> het seizoen en de resultaten zijn uitstekend.",
                            "target_lang_translations": {
                                "en": "We are now <w>halfway through</w> the season and the results are excellent.",
                                "ru": "Мы сейчас <w>в середине</w> сезона, и результаты отличные."
                            }
                        },
                        {
                            "text": "De man was waarschijnlijk <w>halverwege</w> de twintig toen hij afstudeerde.",
                            "target_lang_translations": {
                                "en": "The man was probably <w>halfway through</w> his twenties when he graduated.",
                                "ru": "Мужчине было, вероятно, <w>около двадцати пяти</w>, когда он закончил учебу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "halverwege de maand",
                        "halverwege de middag",
                        "halverwege de rit"
                    ],
                    "target_lang_definitions": {
                        "en": "In the middle of a period, distance, or process.",
                        "ru": "В середине периода, расстояния или процесса."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "halfway through",
                                "target_lang_sense_clarification": "preposition indicating the middle of something"
                            },
                            {
                                "target_lang_word": "midway through",
                                "target_lang_sense_clarification": "in the middle of a process or period"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в середине",
                                "target_lang_sense_clarification": "внутри периода или процесса"
                            },
                            {
                                "target_lang_word": "на полпути",
                                "target_lang_sense_clarification": "в процессе выполнения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf5e0e16-c521-3930-b90b-93bf452d6a74",
                    "sense_definition": "A user-created file or software package that alters the appearance or behaviour of a video game.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "modification_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I installed a <mod> that improves the graphics of the game.",
                            "target_lang_translations": {
                                "ru": "Я установил <мод>, который улучшает графику в игре."
                            }
                        },
                        {
                            "text": "The community has created thousands of free <mods> for this title.",
                            "target_lang_translations": {
                                "ru": "Сообщество создало тысячи бесплатных <модов> для этой игры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "install a mod",
                        "game mod",
                        "mod support"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for modification"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Созданный пользователем файл или пакет программного обеспечения, который изменяет внешний вид или поведение видеоигры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мод",
                                "target_lang_sense_clarification": "видеоигры"
                            },
                            {
                                "target_lang_word": "модификация",
                                "target_lang_sense_clarification": "изменение игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28cbc96c-1366-37bf-814f-3580cb011a54",
                    "sense_definition": "A person who monitors and enforces rules in an online discussion forum or chat group.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "moderation_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you break the rules, the <mods> will ban you from the server.",
                            "target_lang_translations": {
                                "ru": "Если ты нарушишь правила, <модераторы> забанят тебя на сервере."
                            }
                        },
                        {
                            "text": "She volunteered to be a <mod> for the new discussion board.",
                            "target_lang_translations": {
                                "ru": "Она вызвалась быть <модератором> на новом форуме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "forum mod",
                        "chat mod"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for moderator"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который следит за соблюдением правил на онлайн-форуме или в чате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модератор",
                                "target_lang_sense_clarification": "роль"
                            },
                            {
                                "target_lang_word": "модер",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0ada0f4-e0c1-36b5-a1f6-c9d16a67617e",
                    "sense_definition": "A modification or change made to something, such as a vehicle or machine, to improve it.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "modification_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made several <mods> to his car engine to make it faster.",
                            "target_lang_translations": {
                                "ru": "Он внес несколько <доработок> в двигатель своей машины, чтобы сделать её быстрее."
                            }
                        },
                        {
                            "text": "The mechanic suggested a few simple <mods> for better fuel efficiency.",
                            "target_lang_translations": {
                                "ru": "Механик предложил несколько простых <модификаций> для экономии топлива."
                            }
                        }
                    ],
                    "synonyms": [
                        "modification",
                        "alteration"
                    ],
                    "common_phrases": [
                        "body mod",
                        "performance mod"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for modification"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменение, внесенное во что-либо, например, в транспортное средство или механизм, с целью его улучшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модификация",
                                "target_lang_sense_clarification": "техническая"
                            },
                            {
                                "target_lang_word": "доработка",
                                "target_lang_sense_clarification": "улучшение"
                            },
                            {
                                "target_lang_word": "тюнинг",
                                "target_lang_sense_clarification": "автомобильный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afc61358-13f3-34ee-b78b-d70f618eaa7c",
                    "sense_definition": "A member of a British youth subculture of the 1960s, characterized by stylish dress, soul music, and motor scooters.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "subculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The film depicts the famous clashes between the <mods> and the rockers.",
                            "target_lang_translations": {
                                "ru": "Фильм изображает знаменитые стычки между <модами> и рокерами."
                            }
                        },
                        {
                            "text": "He dressed like a <mod>, wearing a sharp suit and a parka.",
                            "target_lang_translations": {
                                "ru": "Он одевался как <мод>, нося строгий костюм и парку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mods and rockers"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "refers to 1960s culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель британской молодежной субкультуры 1960-х годов, отличающийся стильной одеждой, любовью к соул-музыке и мотороллерам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мод",
                                "target_lang_sense_clarification": "субкультура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "568f2e3f-0767-3ba7-bf5b-cb2c99f6e884",
                    "sense_definition": "The operation of finding the remainder when one integer is divided by another.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_modulus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this equation, ten <mod> three equals one.",
                            "target_lang_translations": {
                                "ru": "В этом уравнении десять по <модулю> три равно единице."
                            }
                        },
                        {
                            "text": "Students learned how to calculate the <mod> during the computer science lecture.",
                            "target_lang_translations": {
                                "ru": "Студенты учились вычислять <остаток от деления> на лекции по информатике."
                            }
                        }
                    ],
                    "synonyms": [
                        "modulus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Операция нахождения остатка от деления одного целого числа на другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модуль",
                                "target_lang_sense_clarification": "математика"
                            },
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "от деления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f8b4ed63-6ecf-3888-bd2b-514affeae7c4",
                    "sense_definition": "To modify or customize an object, especially hardware or software, to improve its performance or appearance.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "modification_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <modded> his computer case by adding neon lights and extra fans.",
                            "target_lang_translations": {
                                "ru": "Он <модифицировал> корпус своего компьютера, добавив неоновую подсветку и дополнительные вентиляторы."
                            }
                        },
                        {
                            "text": "Gamers often <mod> the game files to unlock hidden features.",
                            "target_lang_translations": {
                                "ru": "Геймеры часто <модифицируют> файлы игры, чтобы разблокировать скрытые функции."
                            }
                        }
                    ],
                    "synonyms": [
                        "customize",
                        "modify",
                        "trick out"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for modify"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Модифицировать или настраивать объект, особенно оборудование или программное обеспечение, для улучшения его характеристик или внешнего вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модифицировать",
                                "target_lang_sense_clarification": "изменять"
                            },
                            {
                                "target_lang_word": "моддить",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8aba9cf2-8692-3ef4-805c-8b0007792dd3",
                    "sense_definition": "To act as a moderator by managing users and content on an online forum.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "moderation_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been <modding> this community channel for over two years.",
                            "target_lang_translations": {
                                "ru": "Она <модерирует> этот канал сообщества уже более двух лет."
                            }
                        },
                        {
                            "text": "It takes a lot of patience to <mod> a large discussion group effectively.",
                            "target_lang_translations": {
                                "ru": "Требуется много терпения, чтобы эффективно <модерировать> большую дискуссионную группу."
                            }
                        }
                    ],
                    "synonyms": [
                        "moderate"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for moderate"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять функции модератора, управляя пользователями и контентом на онлайн-форуме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модерировать",
                                "target_lang_sense_clarification": "управлять форумом"
                            }
                        ]
                    }
                }
            ]
        },

        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f5fcbea3-9176-30cc-a20c-cc80ce6f773f",
                    "sense_definition": "Of a beam, column, or other device, carrying weight or load.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "structural_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot remove that wall because it is a <w>bearing</w> wall.",
                            "target_lang_translations": {
                                "ru": "Эту стену нельзя сносить, потому что она — <w>несущая</w>."
                            }
                        },
                        {
                            "text": "The architect identified the main <w>bearing</w> structures in the plan.",
                            "target_lang_translations": {
                                "ru": "Архитектор определил на плане основные <w>несущие</w> конструкции."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8da82f22-b847-328b-9281-dc4c21753c5e",
                    "sense_definition": "Zarodek ssaka, w tym człowieka, w późniejszym stadium rozwoju wewnątrzmacicznego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Badanie ultrasonograficzne pozwala sprawdzić, czy <w>płód</w> rozwija się prawidłowo.",
                            "target_lang_translations": {
                                "ru": "Ультразвуковое исследование позволяет проверить, правильно ли развивается <w>плод</w>."
                            }
                        },
                        {
                            "text": "Lekarz stwierdził, że <w>płód</w> jest ułożony główką w dół.",
                            "target_lang_translations": {
                                "ru": "Врач констатировал, что <w>плод</w> расположен головкой вниз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozwój płodu",
                        "ruchy płodu",
                        "uszkodzenie płodu",
                        "płeć płodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зародыш млекопитающего, в том числе человека, на поздней стадии внутриутробного развития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "эмбрион, зародыш"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b66b46f-c4b4-3ca8-b701-b1bc7b488a6f",
                    "sense_definition": "Roślinne bogactwa natury, które wydała ziemia, wykorzystywane przez ludzi głównie jako pożywienie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesienią rolnicy zbierają z pól liczne <w>płody</w> ziemi, by przygotować zapasy.",
                            "target_lang_translations": {
                                "ru": "Осенью фермеры собирают с полей многочисленные <w>плоды</w> земли, чтобы подготовить запасы."
                            }
                        },
                        {
                            "text": "Las oferuje nam swoje <w>płody</w>, takie jak grzyby i jagody.",
                            "target_lang_translations": {
                                "ru": "Лес предлагает нам свои <w>плоды</w>, такие как грибы и ягоды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płody ziemi",
                        "płody rolne",
                        "płody leśne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растительные дары природы, выращенные землей, используемые людьми в основном в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "дары природы, урожай"
                            },
                            {
                                "target_lang_word": "дары",
                                "target_lang_sense_clarification": "в контексте «дары земли/леса»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e288386c-4304-3463-bb51-9d3a9f2c831b",
                    "sense_definition": "Wynik czyjejś pracy umysłowej, talentu lub wyobraźni; dzieło.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta powieść to <w>płód</w> jego wieloletnich przemyśleń i doświadczeń.",
                            "target_lang_translations": {
                                "ru": "Этот роман — <w>плод</w> его многолетних размышлений и опыта."
                            }
                        },
                        {
                            "text": "Krytycy uznali ten utwór za nieudany <w>płód</w> chorej wyobraźni.",
                            "target_lang_translations": {
                                "ru": "Критики сочли это произведение неудачным <w>плодом</w> больного воображения."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytwór",
                        "dzieło"
                    ],
                    "common_phrases": [
                        "płód wyobraźni",
                        "płód literacki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат чьего-либо умственного труда, таланта или воображения; творение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "результат, творение"
                            },
                            {
                                "target_lang_word": "порождение",
                                "target_lang_sense_clarification": "продукт (часто воображения)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
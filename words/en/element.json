                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heating <w>element</w> in the toaster needs to be replaced.",
                            "target_lang_translations": {
                                "ru": "Нагревательный <w>элемент</w> в тостере необходимо заменить."
                            }

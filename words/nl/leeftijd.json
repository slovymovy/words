{
    "word_family": [
        "leeftijdsgrens",
        "leeftijdsgenoot",
        "leeftijdsgroep",
        "leeftijdsverschil"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc34a7f0-cc70-3a8a-a7e6-c76510706401",
                    "sense_definition": "De tijd dat iemand leeft of geleefd heeft, het totaal aantal levensjaren.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat is uw <w>leeftijd</w>?",
                            "target_lang_translations": {
                                "ru": "Каков ваш <w>возраст</w>?"
                            }
                        },
                        {
                            "text": "Op achttienjarige <w>leeftijd</w> mag je in Nederland stemmen.",
                            "target_lang_translations": {
                                "ru": "В восемнадцатилетнем <w>возрасте</w> в Нидерландах можно голосовать."
                            }
                        }
                    ],
                    "synonyms": [
                        "ouderdom"
                    ],
                    "common_phrases": [
                        "op jonge leeftijd",
                        "op hoge leeftijd",
                        "de wettelijke leeftijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время, которое кто-то прожил, или общее количество прожитых лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возраст",
                                "target_lang_sense_clarification": "количество прожитых лет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cf28b78-4791-3963-831f-c2eac26f1dce",
                    "sense_definition": "Een bepaald tijdstip of specifieke fase in iemands leven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "age_stage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die man daar is van middelbare <w>leeftijd</w>.",
                            "target_lang_translations": {
                                "ru": "Тот мужчина — среднего <w>возраста</w>."
                            }
                        },
                        {
                            "text": "Ik zoek een partner die ongeveer van mijn <w>leeftijd</w> is.",
                            "target_lang_translations": {
                                "ru": "Я ищу партнера примерно моего <w>возраста</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "levensfase"
                    ],
                    "common_phrases": [
                        "van middelbare leeftijd",
                        "van mijn leeftijd",
                        "van dezelfde leeftijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный момент или конкретная фаза в жизни человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возраст",
                                "target_lang_sense_clarification": "период жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e972f75-b9f8-3ceb-b361-6240e4864111",
                    "sense_definition": "De totale duur van het leven of het bestaan van iets.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "age_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemiddelde <w>leeftijd</w> van deze diersoort is erg kort.",
                            "target_lang_translations": {
                                "ru": "Средняя <w>продолжительность жизни</w> этого вида животных очень мала."
                            }
                        },
                        {
                            "text": "Wetenschappers onderzoeken de maximale <w>leeftijd</w> van bomen in dit bos.",
                            "target_lang_translations": {
                                "ru": "Ученые исследуют максимальный <w>возраст</w> деревьев в этом лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "levensduur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая продолжительность жизни или существования чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возраст",
                                "target_lang_sense_clarification": "срок существования"
                            },
                            {
                                "target_lang_word": "продолжительность жизни",
                                "target_lang_sense_clarification": "длительность жизни"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
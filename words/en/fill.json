    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a06d2558-dbc0-3198-bd82-ec06650c56dd",
                    "sense_definition": "To make something full by putting contents or substance into it.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "make_full",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>filled</w> the glass with cold water.",
                            "target_lang_translations": {
                                "ru": "Она <w>наполнила</w> стакан холодной водой."
                            }
                        },
                        {
                            "text": "Please <w>fill</w> the tank before you return the car.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>наполните</w> бак, прежде чем вернуть машину."
                            }
                        }
                    ],
                    "antonyms": [
                        "empty"
                    ],
                    "common_phrases": [
                        "fill with water",
                        "fill the bottle",
                        "fill the tank"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо полным, помещая внутрь содержимое или вещество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наполнять",
                                "target_lang_sense_clarification": "делать полным"
                            },
                            {
                                "target_lang_word": "заполнять",
                                "target_lang_sense_clarification": "занимать целиком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb98c0ba-90ca-3706-8031-c9fc14e96473",
                    "sense_definition": "To become full of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "make_full",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bucket <w>filled</w> with rain during the storm.",
                            "target_lang_translations": {
                                "ru": "Ведро <w>наполнилось</w> дождём во время грозы."
                            }
                        },
                        {
                            "text": "His eyes <w>filled</w> with tears when he heard the news.",
                            "target_lang_translations": {
                                "ru": "Его глаза <w>наполнились</w> слезами, когда он услышал новости."
                            }
                        }
                    ],
                    "antonyms": [
                        "empty"
                    ],
                    "common_phrases": [
                        "fill with tears",
                        "fill with smoke",
                        "fill up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться полным чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наполняться",
                                "target_lang_sense_clarification": "становиться полным"
                            },
                            {
                                "target_lang_word": "заполняться",
                                "target_lang_sense_clarification": "занимать пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a1298e4-ad95-31c8-8829-b99169c06aa3",
                    "sense_definition": "To spread throughout and occupy a space, time, or sense completely.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "make_full",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The smell of fresh coffee <w>filled</w> the kitchen.",
                            "target_lang_translations": {
                                "ru": "Запах свежего кофе <w>наполнил</w> кухню."
                            }
                        },
                        {
                            "text": "Loud music <w>filled</w> the air at the festival.",
                            "target_lang_translations": {
                                "ru": "Громкая музыка <w>наполнила</w> воздух на фестивале."
                            }
                        }
                    ],
                    "synonyms": [
                        "pervade"
                    ],
                    "common_phrases": [
                        "fill the air",
                        "fill the room",
                        "fill the silence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространяться и полностью занимать пространство, время или чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заполнять",
                                "target_lang_sense_clarification": "занимать пространство"
                            },
                            {
                                "target_lang_word": "наполнять",
                                "target_lang_sense_clarification": "пропитывать, насыщать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41561c66-4dad-31a9-b9de-23e295d1043f",

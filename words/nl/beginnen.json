{
    "word_family": [
        "begin",
        "beginner",
        "beginneling",
        "beginsel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "38838a39-1d5a-3ba9-a09d-3251aef80de7",
                    "sense_definition": "Een aanvang nemen; starten op een bepaald moment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De film <w>begint</w> om acht uur vanavond.",
                            "target_lang_translations": {
                                "ru": "Фильм <w>начинается</w> сегодня в восемь вечера.",
                                "en": "The movie <w>starts</w> at eight o'clock tonight."
                            }
                        },
                        {
                            "text": "Het <w>begon</w> hard te regenen toen we naar buiten gingen.",
                            "target_lang_translations": {
                                "ru": "<w>Начал</w> идти сильный дождь, когда мы вышли на улицу.",
                                "en": "It <w>started</w> raining heavily when we went outside."
                            }
                        },
                        {
                            "text": "De zomervakantie <w>begint</w> volgende week vrijdag.",
                            "target_lang_translations": {
                                "ru": "Летние каникулы <w>начинаются</w> в следующую пятницу.",
                                "en": "The summer holidays <w>start</w> next Friday."
                            }
                        }
                    ],
                    "synonyms": [
                        "starten"
                    ],
                    "antonyms": [
                        "eindigen",
                        "stoppen",
                        "ophouden"
                    ],
                    "common_phrases": [
                        "het begint te",
                        "opnieuw beginnen",
                        "de les begint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинаться; стартовать в определенный момент.",
                        "en": "To start; to commence at a specific moment."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начинаться",
                                "target_lang_sense_clarification": "о событиях, фильмах, периодах времени"
                            },
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "о времени года или суток"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "events, movies, time periods"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "general use"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bc7dc3f-0ade-30cd-9bb8-18e9521a74a6",
                    "sense_definition": "Voor het eerst iets gaan doen; een activiteit starten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag met pianoles <w>beginnen</w>.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>начать</w> брать уроки игры на фортепиано.",
                                "en": "I would like to <w>start</w> piano lessons."
                            }
                        },
                        {
                            "text": "Zij is gisteren <w>begonnen</w> met haar nieuwe baan.",
                            "target_lang_translations": {
                                "ru": "Вчера она <w>приступила</w> к своей новой работе.",
                                "en": "She <w>started</w> her new job yesterday."
                            }
                        },
                        {
                            "text": "Laten we <w>beginnen</w> met het opruimen van de kamer.",
                            "target_lang_translations": {
                                "ru": "Давайте <w>начнем</w> с уборки комнаты.",
                                "en": "Let's <w>start</w> by cleaning the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "starten"
                    ],
                    "antonyms": [
                        "stoppen",
                        "ophouden"
                    ],
                    "common_phrases": [
                        "beginnen met",
                        "beginnen aan",
                        "voor zichzelf beginnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать делать что-то; приступать к деятельности.",
                        "en": "To start doing something for the first time; to commence an activity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начинать",
                                "target_lang_sense_clarification": "делать что-либо"
                            },
                            {
                                "target_lang_word": "приступать",
                                "target_lang_sense_clarification": "переходить к делу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "an activity or habit"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "an action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe4d965-64b4-3e25-a92e-94725edcd7ea",
                    "sense_definition": "In gang zetten; iets nieuws opzetten of initiëren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn oom wil een eigen restaurant <w>beginnen</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дядя хочет <w>открыть</w> собственный ресторан.",
                                "en": "My uncle wants to <w>open</w> his own restaurant."
                            }
                        },
                        {
                            "text": "Het bedrijf is een groot project <w>begonnen</w> in het buitenland.",
                            "target_lang_translations": {
                                "ru": "Компания <w>запустила</w> крупный проект за границей.",
                                "en": "The company has <w>launched</w> a large project abroad."
                            }
                        }
                    ],
                    "synonyms": [
                        "starten"
                    ],
                    "antonyms": [
                        "eindigen"
                    ],
                    "common_phrases": [
                        "een zaak beginnen",
                        "een ruzie beginnen",
                        "een discussie beginnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запускать; основывать или инициировать что-то новое.",
                        "en": "To set in motion; to establish or initiate something new."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "бизнес, дело"
                            },
                            {
                                "target_lang_word": "затевать",
                                "target_lang_sense_clarification": "спор, ссору"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "проект, процесс"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "a business, a dispute"
                            },
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "a shop, restaurant"
                            },
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "a project, campaign"
                            },
                            {
                                "target_lang_word": "initiate",
                                "target_lang_sense_clarification": "formal contexts"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "575064c2-6c29-34e0-8974-fb94ef5d74d9",
                    "sense_definition": "The archaic or dialectal second-person singular subject pronoun, used to address one person (equivalent to modern 'you').",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pronoun_2nd_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Where art <w>thou</w> going at this hour?",
                            "target_lang_translations": {
                                "ru": "Куда <w>ты</w> идешь в этот час?"
                            }
                        },
                        {
                            "text": "<w>Thou</w> shalt not steal.",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w> не должен красть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "holier-than-thou",
                        "art thou"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "used in some regional dialects"
                        },
                        {
                            "trait_type": "form",
                            "comment": "singular subject"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или диалектное местоимение второго лица единственного числа (соответствует современному «ты»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc7d4829-a73e-3117-bb57-cdd220789b52",
                    "sense_definition": "A thousand, especially referring to an amount of money.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "number_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The repairs on the car will cost a couple of <w>thou</w>.",
                            "target_lang_translations": {
                                "ru": "Ремонт машины обойдется в пару <w>штук</w>."
                            }
                        },
                        {
                            "text": "He managed to save fifty <w>thou</w> for the deposit.",
                            "target_lang_translations": {
                                "ru": "Ему удалось накопить пятьдесят <w>штук</w> на первый взнос."
                            }
                        }
                    ],
                    "synonyms": [
                        "grand#Noun",
                        "K#Noun",
                        "large"
                    ],
                    "common_phrases": [
                        "a few thou",
                        "fifty thou"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тысяча, особенно при обозначении денежных сумм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штука",
                                "target_lang_sense_clarification": "разг., тысяча"
                            },
                            {
                                "target_lang_word": "косарь",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3abd8ca0-22ee-33b0-a0a2-abe40072ae8e",
                    "sense_definition": "A unit of length equal to one thousandth of an inch.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engine parts must be machined to within a few <w>thou</w>.",
                            "target_lang_translations": {
                                "ru": "Детали двигателя должны быть обработаны с точностью до нескольких <w>милов</w>."
                            }
                        },
                        {
                            "text": "We need to shave a <w>thou</w> off this metal sheet to make it fit.",
                            "target_lang_translations": {
                                "ru": "Нам нужно снять один <w>мил</w> с этого металлического листа, чтобы он подошел."
                            }
                        }
                    ],
                    "synonyms": [
                        "mil"
                    ],
                    "common_phrases": [
                        "a few thou"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for thousandth"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица длины, равная одной тысячной дюйма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мил",
                                "target_lang_sense_clarification": "единица измерения"
                            },
                            {
                                "target_lang_word": "тысячная дюйма",
                                "target_lang_sense_clarification": "мера длины"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5fe8dfc7-cbf6-35eb-af36-f3a808dfd3d1",
                    "sense_definition": "To address a person using the familiar pronoun 'thou', often to imply inferiority, contempt, or intimacy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "address_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do not <w>thou</w> me; I expect to be addressed with respect.",
                            "target_lang_translations": {
                                "ru": "Не <w>тычь</w> мне; я требую уважительного обращения."
                            }
                        },
                        {
                            "text": "He <w>thoued</w> the servant but spoke formally to the king.",
                            "target_lang_translations": {
                                "ru": "Он <w>тыкал</w> слуге, но с королем говорил официально."
                            }
                        }
                    ],
                    "synonyms": [
                        "thee"
                    ],
                    "antonyms": [
                        "you"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к человеку на «ты», часто подразумевая его низшее положение или панибратство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыкать",
                                "target_lang_sense_clarification": "обращаться на «ты»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
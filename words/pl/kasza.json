    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c589955-08a4-36e2-875b-e4cd6e6a743a",
                    "sense_definition": "Łuskane lub rozdrobnione ziarna zbóż (np. gryki, jęczmienia, prosa) przeznaczone do spożycia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_ingredient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić paczkę <kaszy> gryczanej na dzisiejszy obiad.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить пачку гречневой <крупы> на сегодняшний обед."
                            }
                        },
                        {
                            "text": "W szafce kuchennej przechowujemy ryż, makaron i różne rodzaje <kaszy>.",
                            "target_lang_translations": {
                                "ru": "В кухонном шкафу мы храним рис, макароны и разные виды <круп>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kasza gryczana",
                        "kasza jęczmienna",
                        "kasza manna",
                        "kasza jaglana",
                        "woreczek kaszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очищенные или дробленые зерна злаков (например, гречихи, ячменя, проса), предназначенные для употребления в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крупа",
                                "target_lang_sense_clarification": "сырой продукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eedda6ce-d1c2-30a1-9241-d72e73e21d19",
                    "sense_definition": "Gotowa potrawa przyrządzona z ugotowanych ziaren zbóż.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_dish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie dzieci często jedzą słodką <kaszę> na mleku.",
                            "target_lang_translations": {
                                "ru": "На завтрак дети часто едят сладкую <кашу> на молоке."
                            }
                        },
                        {
                            "text": "Do gulaszu wieprzowego najlepiej pasuje sypka <kasza> jęczmienna.",
                            "target_lang_translations": {
                                "ru": "К свиному гуляшу лучше всего подходит рассыпчатая ячневая <каша>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kasza ze skwarkami",
                        "kasza na mleku",
                        "talerz kaszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовое блюдо, приготовленное из сваренных зерен злаков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каша",
                                "target_lang_sense_clarification": "готовое блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfc37058-2cf0-38fc-9f5f-2537fc83a64e",
                    "sense_definition": "Sytuacja charakteryzująca się brakiem porządku, chaosem lub zamieszaniem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chaos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez błąd w systemie komputerowym zrobiła się niezła <kasza> w dokumentach.",
                            "target_lang_translations": {
                                "ru": "Из-за ошибки в компьютерной системе в документах получилась настоящая <каша>."
                            }
                        },
                        {
                            "text": "Nie lubię pracować w pośpiechu, bo wtedy zawsze wychodzi z tego jakaś <kasza>.",
                            "target_lang_translations": {
                                "ru": "Я не люблю работать в спешке, потому что тогда всегда получается какая-то <каша>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamęt",
                        "zamieszanie"
                    ],
                    "common_phrases": [
                        "niezła kasza",
                        "totalna kasza"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, характеризующаяся отсутствием порядка, хаосом или неразберихой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каша",
                                "target_lang_sense_clarification": "беспорядок"
                            },
                            {
                                "target_lang_word": "неразбериха",
                                "target_lang_sense_clarification": "путаница"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd33563c-02a5-35fd-ac6a-44d5197131f9",
                    "sense_definition": "Masa drobnych, sypkich lub półpłynnych kawałków, przypominająca strukturą rozdrobnione ziarno (często o śniegu lub lodzie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na chodnikach zalega mokra <kasza>, przez którą trudno przejść suchą stopą.",
                            "target_lang_translations": {
                                "ru": "На тротуарах лежит мокрая <каша>, по которой трудно пройти, не промочив ноги."
                            }
                        },
                        {
                            "text": "Z nieba zamiast puszystego śniegu padała zmrożona <kasza>.",
                            "target_lang_translations": {
                                "ru": "С неба вместо пушистого снега падала ледяная <крупа>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kasza śnieżna",
                        "błotnista kasza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Масса мелких, сыпучих или полужидких частиц, напоминающая по структуре дроблёное зерно (часто о снеге или льде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снежная каша",
                                "target_lang_sense_clarification": "слякоть на земле"
                            },
                            {
                                "target_lang_word": "крупа",
                                "target_lang_sense_clarification": "вид осадков"
                            }
                        ]
                    }
                }
            ]

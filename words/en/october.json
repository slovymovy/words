    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "95552511-66c2-3907-a48c-8f690666fcc4",
                    "sense_definition": "The tenth month of the Gregorian calendar, following September and preceding November.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "My birthday is in <w>October</w>.",
                            "target_lang_translations": {
                                "ru": "Мой день рождения в <w>октябре</w>."
                            }
                        },
                        {
                            "text": "The weather usually starts to get colder in <w>October</w>.",
                            "target_lang_translations": {
                                "ru": "В <w>октябре</w> погода обычно начинает становиться холоднее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in October",
                        "last October",
                        "next October",
                        "early October",
                        "late October"
                    ],
                    "target_lang_definitions": {
                        "ru": "Десятый месяц григорианского календаря, следующий за сентябрем и предшествующий ноябрю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "октябрь",
                                "target_lang_sense_clarification": "месяц года"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3abd8f0f-9304-345e-b980-444cc132e3c2",
                    "sense_definition": "A female given name transferred from the month name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Her parents named her <w>October</w> because she was born in the autumn.",
                            "target_lang_translations": {
                                "ru": "Родители назвали её <w>Октобер</w>, потому что она родилась осенью."
                            }
                        },
                        {
                            "text": "<w>October</w> is a unique and beautiful name for a girl.",
                            "target_lang_translations": {
                                "ru": "<w>Октобер</w> — уникальное и красивое имя для девочки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя, происходящее от названия месяца."
                    },
                    "translations": {
                        "ru": [
                            {

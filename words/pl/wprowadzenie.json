            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7f083549-7939-3420-9f57-f893e2db7a3a",
                    "sense_definition": "wstępna część tekstu, książki lub przemówienia, która przedstawia temat i przygotowuje odbiorcę do głównej treści",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "text_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor napisał ciekawe <w>wprowadzenie</w>, które wyjaśnia genezę całej powieści.",
                            "target_lang_translations": {
                                "ru": "Автор написал интересное <w>введение</w>, которое объясняет происхождение всего романа."
                            }
                        },
                        {
                            "text": "To krótkie <w>wprowadzenie</w> ułatwia zrozumienie dalszych, bardziej skomplikowanych rozdziałów.",
                            "target_lang_translations": {
                                "ru": "Это краткое <w>вступление</w> облегчает понимание дальнейших, более сложных глав."
                            }
                        }
                    ],
                    "synonyms": [

            "senses": [
                {
                    "sense_id": "690fbc92-6271-3211-91a8-13d004c7db8d",
                    "sense_definition": "Название Богоматери у католиков, а также её скульптурное или живописное изображение.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В центре зала висела прекрасная <w>мадонна</w> с младенцем на руках.",
                            "target_lang_translations": {
                                "en": "A beautiful <w>Madonna</w> with the Child in her arms hung in the center of the hall."
                            }
                        },
                        {
                            "text": "Художник потратил несколько лет, чтобы написать эту <w>мадонну</w>.",
                            "target_lang_translations": {
                                "en": "The artist spent several years painting this <w>Madonna</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Богоматерь",
                        "Дева Мария",
                        "Богородица"
                    ],
                    "common_phrases": [
                        "Сикстинская мадонна",
                        "мадонна с младенцем",
                        "лик мадонны"
                    ],
                    "target_lang_definitions": {
                        "en": "The name of the Virgin Mary in the Catholic tradition, as well as a painting or sculpture depicting her."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Madonna",
                                "target_lang_sense_clarification": "Virgin Mary or art depicting her"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cbd1880-fd94-3169-a54d-3119ba4e48c8",
                    "sense_definition": "О женщине, являющейся воплощением чистой красоты, целомудрия и кротости.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её спокойное и светлое лицо напоминало лик <w>мадонны</w>.",
                            "target_lang_translations": {
                                "en": "Her calm and radiant face resembled the face of a <w>Madonna</w>."
                            }
                        },
                        {
                            "text": "Он смотрел на неё с восхищением, как на земную <w>мадонну</w>.",
                            "target_lang_translations": {
                                "en": "He looked at her with admiration, as if at an earthly <w>Madonna</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Referring to a woman who is the embodiment of pure beauty, chastity, and meekness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Madonna",
                                "target_lang_sense_clarification": "symbol of purity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef6a9c3e-f4dc-37c4-b7a1-7eb315acb661",
                    "sense_definition": "Старинное вежливое обращение к женщине в Италии.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прекрасная <w>мадонна</w>, позвольте мне проводить вас до дома.",
                            "target_lang_translations": {
                                "en": "Beautiful <w>Madonna</w>, allow me to walk you home."
                            }
                        },
                        {
                            "text": "Слуга низко поклонился и открыл дверь перед знатной <w>мадонной</w>.",
                            "target_lang_translations": {
                                "en": "The servant bowed low and opened the door for the noble <w>Madonna</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "госпожа",
                        "синьора"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "старинное обращение"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "в Италии"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An archaic polite form of address to a woman in Italy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Madonna",
                                "target_lang_sense_clarification": "archaic Italian address"
                            },
                            {
                                "target_lang_word": "my lady",
                                "target_lang_sense_clarification": "polite address"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b0965bba-619b-373b-8d75-1a970432b623",
                    "sense_definition": "Сварливая, скандальная или неприятная женщина (грубое ругательство).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <w>сука</w> снова устроила скандал на пустом месте.",
                            "target_lang_translations": {
                                "en": "That <w>bitch</w> made a scene out of nothing again."
                            }
                        },
                        {
                            "text": "Он назвал её <w>сукой</w> и хлопнул дверью.",
                            "target_lang_translations": {
                                "en": "He called her a <w>bitch</w> and slammed the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "стерва"
                    ],
                    "common_phrases": [
                        "редкая сука",
                        "продажная сука"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "грубое, вульгарное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A quarrelsome, scandalous, or unpleasant woman (offensive slur)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bitch",
                                "target_lang_sense_clarification": "offensive term for a woman"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9aad45d0-d5b0-3076-8429-4500ad5c7d8f",
                    "sense_definition": "Подлый, негодный человек; негодяй (используется по отношению к любому полу).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верни мне деньги, <w>сука</w>!",
                            "target_lang_translations": {
                                "en": "Give me back my money, <w>bitch</w>!"
                            }
                        },
                        {
                            "text": "Какой же он <w>сука</w>, что так поступил с нами.",
                            "target_lang_translations": {
                                "en": "What a <w>scumbag</w> he is for treating us like that."
                            }
                        }
                    ],
                    "synonyms": [
                        "мерзавец",
                        "мразь",
                        "негодяй",
                        "сволочь",
                        "подлец"
                    ],
                    "common_phrases": [

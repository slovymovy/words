{
    "word_family": [
        "frontal",
        "frontage",
        "confront",
        "affront",
        "forefront"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3f6c9d36-f09c-32a5-8837-5cd18c74dbaf",
                    "sense_definition": "The foremost side of something or the end that faces the direction it normally moves.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>front</w> of the car was badly damaged in the accident.",
                            "target_lang_translations": {
                                "ru": "В аварии сильно пострадала <w>передняя часть</w> машины."
                            }
                        },
                        {
                            "text": "Please write your name on the <w>front</w> of the envelope.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите свое имя на <w>лицевой стороне</w> конверта."
                            }
                        }
                    ],
                    "synonyms": [
                        "fore"
                    ],
                    "antonyms": [
                        "back",
                        "rear"
                    ],
                    "common_phrases": [
                        "at the front",
                        "in front",
                        "front door",
                        "front seat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя сторона чего-либо или конец, обращенный в направлении движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передняя часть",
                                "target_lang_sense_clarification": "общая часть предмета"
                            },
                            {
                                "target_lang_word": "лицевая сторона",
                                "target_lang_sense_clarification": "плоских предметов, документов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3880ad8-dfd0-39e2-a9ae-5239ef40ca4d",
                    "sense_definition": "The side of a building where the main entrance is located.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We planted some beautiful flowers in the <w>front</w> of the house.",
                            "target_lang_translations": {
                                "ru": "Мы посадили красивые цветы в <w>передней части</w> двора дома."
                            }
                        },
                        {
                            "text": "There is a large park right at the <w>front</w> of the hotel.",
                            "target_lang_translations": {
                                "ru": "Прямо у <w>фасада</w> отеля есть большой парк."
                            }
                        }
                    ],
                    "antonyms": [
                        "back",
                        "rear"
                    ],
                    "common_phrases": [
                        "front garden",
                        "front yard",
                        "front gate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сторона здания, где расположен главный вход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фасад",
                                "target_lang_sense_clarification": "архитектурный термин"
                            },
                            {
                                "target_lang_word": "передняя часть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98bd2f55-5b5c-3a5b-b2f5-e9f67cf4cfc9",
                    "sense_definition": "The boundary between two air masses of different temperatures, often causing a change in weather.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "weather_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold <w>front</w> is moving in from the north this evening.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером с севера движется холодный <w>фронт</w>."
                            }
                        },
                        {
                            "text": "The weather forecast predicts a storm <w>front</w> will arrive tomorrow.",
                            "target_lang_translations": {
                                "ru": "Прогноз погоды предсказывает, что завтра придет штормовой <w>фронт</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cold front",
                        "warm front",
                        "weather front"
                    ],
                    "target_lang_definitions": {
                        "ru": "Граница между двумя воздушными массами разной температуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фронт",
                                "target_lang_sense_clarification": "метеорологический"
                            },
                            {
                                "target_lang_word": "атмосферный фронт",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c01f8e9-81d9-3bb3-b74f-634d9bb2f49b",
                    "sense_definition": "A road or promenade along the sea.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "spatial_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked along the <w>front</w> and enjoyed the sea breeze.",
                            "target_lang_translations": {
                                "ru": "Мы гуляли по <w>набережной</w> и наслаждались морским бризом."
                            }
                        },
                        {
                            "text": "The hotel is located right on the sea <w>front</w>.",
                            "target_lang_translations": {
                                "ru": "Отель расположен прямо на <w>набережной</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sea front",
                        "on the front"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорога или прогулочная зона вдоль моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набережная",
                                "target_lang_sense_clarification": "вдоль моря"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "712af57a-fdf4-3236-b7c6-480f13e2215e",
                    "sense_definition": "An area where armies are fighting each other in a war.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Soldiers were sent to the <w>front</w> to defend the border.",
                            "target_lang_translations": {
                                "ru": "Солдат отправили на <w>фронт</w> защищать границу."
                            }
                        },
                        {
                            "text": "News from the <w>front</w> was often delayed by several days.",
                            "target_lang_translations": {
                                "ru": "Новости с <w>фронта</w> часто задерживались на несколько дней."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the front",
                        "front line",
                        "western front"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, где армии сражаются друг с другом во время войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фронт",
                                "target_lang_sense_clarification": "военный"
                            },
                            {
                                "target_lang_word": "передовая",
                                "target_lang_sense_clarification": "линия огня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9f6617f-e071-35f4-a18b-a85b0ba7cbc3",
                    "sense_definition": "A person or organization that hides the illegal or secret activities of another group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "deception_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small shop was actually a <w>front</w> for a criminal gang.",
                            "target_lang_translations": {
                                "ru": "Маленький магазинчик на самом деле был <w>прикрытием</w> для преступной банды."
                            }
                        },
                        {
                            "text": "He used his business as a <w>front</w> to launder stolen money.",
                            "target_lang_translations": {
                                "ru": "Он использовал свой бизнес как <w>прикрытие</w> для отмывания украденных денег."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front for",
                        "mere front"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или организация, скрывающие незаконную или тайную деятельность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикрытие",
                                "target_lang_sense_clarification": "для незаконной деятельности"
                            },
                            {
                                "target_lang_word": "ширма",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "874ff4b0-2a77-30ad-9e41-9c965059078b",
                    "sense_definition": "A false appearance or behavior intended to hide one's true feelings or situation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "deception_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He put on a brave <w>front</w> so his children would not worry.",
                            "target_lang_translations": {
                                "ru": "Он напустил на себя храбрый <w>вид</w>, чтобы дети не волновались."
                            }
                        },
                        {
                            "text": "Her confidence is just a <w>front</w> to hide her insecurity.",
                            "target_lang_translations": {
                                "ru": "Ее уверенность — это лишь <w>маска</w>, скрывающая неуверенность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "put on a front",
                        "bold front"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ложная внешность или поведение, призванные скрыть истинные чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видимость",
                                "target_lang_sense_clarification": "внешнее впечатление"
                            },
                            {
                                "target_lang_word": "маска",
                                "target_lang_sense_clarification": "притворство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9795fbe4-1463-3072-84c0-9e85a2fe9d48",
                    "sense_definition": "Located at or near the most forward part of something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She likes to sit in the <w>front</w> row at the cinema.",
                            "target_lang_translations": {
                                "ru": "Она любит сидеть в <w>переднем</w> ряду в кинотеатре."
                            }
                        },
                        {
                            "text": "The <w>front</w> page of the newspaper had a picture of the president.",
                            "target_lang_translations": {
                                "ru": "На <w>первой</w> полосе газеты была фотография президента."
                            }
                        }
                    ],
                    "synonyms": [
                        "fore",
                        "first"
                    ],
                    "antonyms": [
                        "back",
                        "rear",
                        "last"
                    ],
                    "common_phrases": [
                        "front door",
                        "front page",
                        "front seat",
                        "front row"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный в самой передней части чего-либо или рядом с ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передний",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "первый",
                                "target_lang_sense_clarification": "в контексте рядов или страниц"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c5b84e21-5bc2-3e28-8cce-205e7c6fcfc4",
                    "sense_definition": "To face towards a particular direction or place.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel <w>fronts</w> the ocean, offering beautiful views.",
                            "target_lang_translations": {
                                "ru": "Отель <w>выходит на</w> океан, открывая прекрасные виды."
                            }
                        },
                        {
                            "text": "The house <w>fronts</w> onto a busy main street.",
                            "target_lang_translations": {
                                "ru": "Дом <w>выходит фасадом</w> на оживленную главную улицу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front onto",
                        "front the street"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть обращенным в определенную сторону или к определенному месту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходить на",
                                "target_lang_sense_clarification": "об окнах или фасаде"
                            },
                            {
                                "target_lang_word": "быть обращенным к",
                                "target_lang_sense_clarification": "смотреть в сторону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86c14ef7-d9ac-3a7a-83a1-1d96673b68b5",
                    "sense_definition": "To lead or be the main representative of a group or organization.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "leadership_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>fronts</w> a popular rock band that tours the world.",
                            "target_lang_translations": {
                                "ru": "Она <w>возглавляет</w> популярную рок-группу, которая гастролирует по миру."
                            }
                        },
                        {
                            "text": "He is currently <w>fronting</w> a campaign to protect the environment.",
                            "target_lang_translations": {
                                "ru": "В настоящее время он <w>возглавляет</w> кампанию по защите окружающей среды."
                            }
                        }
                    ],
                    "synonyms": [
                        "lead"
                    ],
                    "common_phrases": [
                        "front a band",
                        "front a campaign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководить или быть главным представителем группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возглавлять",
                                "target_lang_sense_clarification": "быть лидером"
                            },
                            {
                                "target_lang_word": "быть лицом",
                                "target_lang_sense_clarification": "представлять организацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f4ba6c6-4ec1-3713-8822-4721bba515d2",
                    "sense_definition": "To provide money for something in advance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial_front",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents agreed to <w>front</w> the money for my tuition.",
                            "target_lang_translations": {
                                "ru": "Родители согласились <w>внести</w> деньги за мое обучение."
                            }
                        },
                        {
                            "text": "The studio refused to <w>front</w> the budget for the new movie.",
                            "target_lang_translations": {
                                "ru": "Студия отказалась <w>финансировать</w> бюджет нового фильма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front the money",
                        "front the cost"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять деньги на что-либо заранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "финансировать",
                                "target_lang_sense_clarification": "выделять средства"
                            },
                            {
                                "target_lang_word": "оплачивать заранее",
                                "target_lang_sense_clarification": "вносить предоплату"
                            }
                        ]
                    }
                }
            ]
        }
    ]

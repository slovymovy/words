            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7949bc5-9395-3d43-897f-12be149eb68b",
                    "sense_definition": "A shop or place where medicines are prepared, sold, or dispensed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to go to the <w>pharmacy</w> to pick up my prescription.",
                            "target_lang_translations": {
                                "ru": "Мне нужно зайти в <w>аптеку</w>, чтобы забрать лекарство по рецепту."
                            }
                        },
                        {
                            "text": "The local <w>pharmacy</w> is open until late at night.",
                            "target_lang_translations": {
                                "ru": "Местная <w>аптека</w> открыта до поздней ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "drugstore",
                        "chemist's",
                        "dispensary",
                        "apothecary"
                    ],
                    "common_phrases": [
                        "local pharmacy",
                        "hospital pharmacy",
                        "go to the pharmacy",
                        "24-hour pharmacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Магазин или учреждение, где изготавливают, продают или выдают лекарства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аптека",
                                "target_lang_sense_clarification": "место продажи лекарств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5777542-fa9c-3c83-825c-85c9e8de91f9",
                    "sense_definition": "The scientific study and technique of preparing and dispensing drugs and medicines.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to study <w>pharmacy</w> at the university.",
                            "target_lang_translations": {
                                "ru": "Она решила изучать <w>фармацию</w> в университете."
                            }
                        },
                        {
                            "text": "Advances in <w>pharmacy</w> have led to more effective treatments for many diseases.",
                            "target_lang_translations": {
                                "ru": "Достижения в <w>фармацевтике</w> привели к появлению более эффективных методов лечения многих заболеваний."
                            }
                        }
                    ],
                    "common_phrases": [
                        "school of pharmacy",
                        "doctor of pharmacy",
                        "clinical pharmacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Научная дисциплина и методика изготовления и выдачи лекарственных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фармация",
                                "target_lang_sense_clarification": "научная дисциплина"
                            },
                            {
                                "target_lang_word": "фармацевтика",
                                "target_lang_sense_clarification": "отрасль медицины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47d1cd86-855d-3e6e-a512-503d6d68be37",
                    "sense_definition": "The profession or occupation of a pharmacist.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has been practicing <w>pharmacy</w> for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Он занимается <w>фармацевтикой</w> уже более двадцати лет."
                            }
                        },
                        {
                            "text": "Strict regulations govern the practice of <w>pharmacy</w> to ensure patient safety.",
                            "target_lang_translations": {
                                "ru": "Строгие правила регулируют сферу <w>фармации</w> для обеспечения безопасности пациентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "practice pharmacy",
                        "profession of pharmacy"
                    ],
                    "target_lang_definitions": {

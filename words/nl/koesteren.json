{
    "word_family": [
        "koestering"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57e9e495-bc58-31c5-8d35-7257cb570eca",
                    "sense_definition": "Iets of iemand met veel liefde en zorg behandelen, beschermen of nauw aan het hart houden.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De moeder <koesterde> haar pasgeboren baby in haar armen.",
                            "target_lang_translations": {
                                "en": "The mother <w>cherished</w> her newborn baby in her arms."
                            }
                        },
                        {
                            "text": "Wij <koesteren> de mooie herinneringen aan onze vakantie in Frankrijk.",
                            "target_lang_translations": {
                                "en": "We <w>cherish</w> the beautiful memories of our holiday in France."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertroetelen",
                        "verzorgen",
                        "liefhebben",
                        "omarmen"
                    ],
                    "antonyms": [
                        "verwaarlozen",
                        "verstoten"
                    ],
                    "common_phrases": [
                        "herinneringen koesteren",
                        "iemand koesteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To treat someone or something with great love and care, to protect them, or to hold them dear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cherish",
                                "target_lang_sense_clarification": "to love, protect, and care for tenderly"
                            },
                            {
                                "target_lang_word": "treasure",
                                "target_lang_sense_clarification": "to value highly, especially memories or objects"
                            },
                            {
                                "target_lang_word": "nurture",
                                "target_lang_sense_clarification": "to care for and encourage the growth of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bec5dc06-c481-347f-bb46-286c639fb45b",
                    "sense_definition": "Een bepaald gevoel, een wens of een gedachte langdurig bij zich dragen en blijven voelen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ondanks de moeilijke situatie bleef hij hoop <koesteren> op een goede afloop.",
                            "target_lang_translations": {
                                "en": "Despite the difficult situation, he continued to <w>cherish</w> hope for a good outcome."
                            }
                        },
                        {
                            "text": "Het is niet gezond om jarenlang wrok te <koesteren> tegen je oude buren.",
                            "target_lang_translations": {
                                "en": "It is not healthy to <w>harbor</w> a grudge against your old neighbors for years."
                            }
                        }
                    ],
                    "synonyms": [
                        "voelen",
                        "dragen",
                        "hebben"
                    ],
                    "antonyms": [
                        "loslaten",
                        "vergeten"
                    ],
                    "common_phrases": [
                        "hoop koesteren",
                        "wrok koesteren",
                        "verwachtingen koesteren",
                        "argwaan koesteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold onto a specific feeling, wish, or thought for a long time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "harbor",
                                "target_lang_sense_clarification": "to keep a negative thought or feeling (like a grudge) in one's mind"
                            },
                            {
                                "target_lang_word": "cherish",
                                "target_lang_sense_clarification": "to keep a hope, dream, or ambition in one's mind"
                            },
                            {
                                "target_lang_word": "hold onto",
                                "target_lang_sense_clarification": "to maintain or refuse to let go of a feeling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "822ce908-18e8-34e3-b113-9374b30e1a1d",
                    "sense_definition": "Zichzelf verwarmen aan een warmtebron of genieten van de warmte, vaak in de zon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kat <koesterde> zich urenlang in de warme zonnestralen op de vensterbank.",
                            "target_lang_translations": {
                                "en": "The cat <w>basked</w> in the warm sunbeams on the windowsill for hours."
                            }
                        },
                        {
                            "text": "Na de koude wandeling <koesterden> we ons bij het haardvuur.",
                            "target_lang_translations": {
                                "en": "After the cold walk, we <w>warmed ourselves</w> by the fire."
                            }
                        }
                    ],
                    "synonyms": [
                        "zonnen",
                        "warmen",
                        "baden"
                    ],
                    "common_phrases": [
                        "zich koesteren in"
                    ],
                    "target_lang_definitions": {
                        "en": "To warm oneself at a heat source or to enjoy the warmth, often in the sun."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bask",
                                "target_lang_sense_clarification": "to lie exposed to warmth and light, typically from the sun"
                            },
                            {
                                "target_lang_word": "warm oneself",
                                "target_lang_sense_clarification": "to get warm near a heat source"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0841a584-2e42-3ffa-bdb9-397dd401f036",
                    "sense_definition": "Vragend woord om te informeren naar het onderwerp van een gesprek, gedachte of handeling (over wat?).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Waarover</w> hebben jullie het tijdens de vergadering gehad?",
                            "target_lang_translations": {
                                "en": "<w>What</w> did you talk <w>about</w> during the meeting?",
                                "ru": "<w>О чём</w> вы говорили на собрании?"
                            }
                        },
                        {
                            "text": "Ik vraag me af <w>Waarover</w> hij zich zo druk maakt.",
                            "target_lang_translations": {
                                "en": "I wonder <w>what</w> he is so worried <w>about</w>.",
                                "ru": "Интересно, <w>о чём</w> он так беспокоится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waarover gaat het",
                        "waarover spreken"
                    ],
                    "target_lang_definitions": {
                        "en": "Question word used to inquire about the subject of a conversation, thought, or action.",
                        "ru": "Вопросительное слово, используемое для выяснения темы разговора, мысли или действия (о чём?)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "what ... about",
                                "target_lang_sense_clarification": "used in questions to ask for the topic"
                            },
                            {
                                "target_lang_word": "about what",
                                "target_lang_sense_clarification": "formal or emphatic interrogative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "о чём",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e57606-4ff5-389d-a9c1-aa96274795cf",
                    "sense_definition": "Verwijzend woord dat terugkoppelt naar iets dat eerder is genoemd (over hetwelk, over dat).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het boek <w>waarover</w> ik je gisteren vertelde.",
                            "target_lang_translations": {
                                "en": "This is the book <w>about which</w> I told you yesterday.",
                                "ru": "Это книга, <w>о которой</w> я тебе вчера рассказывал."
                            }
                        },
                        {
                            "text": "Er zijn weinig onderwerpen <w>waarover</w> wij het niet eens kunnen worden.",
                            "target_lang_translations": {
                                "en": "There are few subjects <w>about which</w> we cannot agree.",
                                "ru": "Есть мало тем, <w>о которых</w> мы не могли бы договориться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het punt waarover",
                        "de kwestie waarover"
                    ],
                    "target_lang_definitions": {
                        "en": "Reference word linking back to something previously mentioned.",
                        "ru": "Относительное местоимение, которое ссылается на что-то, упомянутое ранее (о котором, о чём)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "about which",
                                "target_lang_sense_clarification": "relative pronoun referring to a thing"
                            },
                            {
                                "target_lang_word": "that ... about",
                                "target_lang_sense_clarification": "informal relative clause"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "о котором",
                                "target_lang_sense_clarification": "относительное местоимение (изменяется по родам и числам)"
                            },
                            {
                                "target_lang_word": "о чём",
                                "target_lang_sense_clarification": "относительное местоимение (для абстрактных понятий)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
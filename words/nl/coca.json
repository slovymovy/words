            "senses": [
                {
                    "sense_id": "4fd0a728-9e81-3eeb-a32c-c206a631ac8b",
                    "sense_definition": "Een plant uit Zuid-Amerika die op grote hoogte groeit en waarvan de bladeren een stimulerende werking hebben.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>coca</w> wordt in de Andes al eeuwenlang verbouwd door lokale boeren.",
                            "target_lang_translations": {
                                "en": "<w>Coca</w> has been cultivated in the Andes for centuries by local farmers.",
                                "ru": "Местные фермеры в Андах выращивают <w>коку</w> уже на протяжении веков."
                            }
                        },
                        {
                            "text": "Vanwege de hoogteziekte is de <w>coca</w> een belangrijk gewas in dit gebied.",
                            "target_lang_translations": {
                                "en": "Because of altitude sickness, <w>coca</w> is an important crop in this area.",
                                "ru": "Из-за горной болезни <w>кока</w> является важной сельскохозяйственной культурой в этом регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "cocaplant",
                        "koka"
                    ],
                    "common_phrases": [
                        "coca verbouwen",
                        "plantage met coca"
                    ],
                    "target_lang_definitions": {
                        "en": "A plant from South America that grows at high altitudes and whose leaves have a stimulating effect.",
                        "ru": "Растение из Южной Америки, произрастающее на больших высотах, листья которого обладают стимулирующим эффектом."
                    },

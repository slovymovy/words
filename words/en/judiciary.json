    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b4e1d62-6f34-37f6-a758-0c256da99053",
                    "sense_definition": "The collective body of judges and justices who administer justice in a jurisdiction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An independent <w>judiciary</w> is a cornerstone of a functioning democracy.",
                            "target_lang_translations": {
                                "ru": "Независимая <w>судебная власть</w> является краеугольным камнем действующей демократии."
                            }
                        },
                        {
                            "text": "Members of the <w>judiciary</w> are expected to remain impartial in all legal matters.",
                            "target_lang_translations": {
                                "ru": "Члены <w>судейского корпуса</w> должны оставаться беспристрастными во всех правовых вопросах."
                            }
                        }
                    ],
                    "synonyms": [
                        "the judicial branch",
                        "judicature"
                    ],
                    "common_phrases": [
                        "independent judiciary",
                        "federal judiciary",
                        "members of the judiciary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность судей и должностных лиц, отправляющих правосудие в определенной юрисдикции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судейский корпус",
                                "target_lang_sense_clarification": "сообщество судей"
                            },
                            {
                                "target_lang_word": "судебная власть",
                                "target_lang_sense_clarification": "ветвь государственной власти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1068b4aa-756c-3122-99de-06afa90e5d7d",
                    "sense_definition": "The system of courts of justice in a country, including the administration and staff.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government announced new reforms to modernize the state <w>judiciary</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новых реформах по модернизации государственной <w>судебной системы</w>."
                            }
                        },
                        {
                            "text": "A lack of funding has caused significant delays within the <w>judiciary</w>.",
                            "target_lang_translations": {
                                "ru": "Нехватка финансирования привела к значительным задержкам в работе <w>судебной системы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "judicature"
                    ],
                    "common_phrases": [
                        "state judiciary",
                        "judiciary committee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система судов правосудия в стране, включая администрацию и персонал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судебная система",
                                "target_lang_sense_clarification": "совокупность судов"
                            },
                            {
                                "target_lang_word": "судоустройство",
                                "target_lang_sense_clarification": "организация судебной системы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "52439786-bc9f-3e87-a325-dfebf14b896e",
                    "sense_definition": "Relating to the administration of justice or the function of a judge.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The senator was appointed to the <w>judiciary</w> committee to review new laws.",
                            "target_lang_translations": {
                                "ru": "Сенатор был назначен в <w>судебный</w> комитет для рассмотрения новых законов."
                            }
                        },
                        {
                            "text": "They discussed various <w>judiciary</w> procedures during the legal conference.",
                            "target_lang_translations": {
                                "ru": "Они обсуждали различные <w>судебные</w> процедуры во время юридической конференции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "judiciary committee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к отправлению правосудия или функциям судьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судебный",
                                "target_lang_sense_clarification": "связанный с судом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

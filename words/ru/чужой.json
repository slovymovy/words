{
    "word_family": [
        "чужак",
        "чужбина",
        "чуждаться",
        "отчуждение",
        "отчуждённость"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e0dba979-d4f5-3439-947a-5c7a35c7cd42",
                    "sense_definition": "Принадлежащий другому лицу; не свой, не собственный.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не трогай <чужие> вещи без разрешения.",
                            "target_lang_translations": {
                                "en": "Don't touch <w>other people's</w> things without permission."
                            }
                        },
                        {
                            "text": "Я случайно взял <чужой> телефон со стола.",
                            "target_lang_translations": {
                                "en": "I accidentally took <w>someone else's</w> phone from the table."
                            }
                        }
                    ],
                    "antonyms": [
                        "свой"
                    ],
                    "common_phrases": [
                        "чужие вещи",
                        "чужое мнение",
                        "чужой карман",
                        "чужая жена"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to another person; not one's own."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "someone else's",
                                "target_lang_sense_clarification": "belonging to another person"
                            },
                            {
                                "target_lang_word": "other people's",
                                "target_lang_sense_clarification": "belonging to others (plural)"
                            },
                            {
                                "target_lang_word": "another's",
                                "target_lang_sense_clarification": "belonging to another (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "593788ee-6b2a-3acb-9cdf-5c0e19381560",
                    "sense_definition": "Не состоящий в родстве или близких отношениях; посторонний, незнакомый.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнате были только <чужие> люди.",
                            "target_lang_translations": {
                                "en": "There were only <w>unfamiliar</w> people in the room."
                            }
                        },
                        {
                            "text": "Ребёнок боится <чужих> взрослых.",
                            "target_lang_translations": {
                                "en": "The child is afraid of <w>strange</w> adults."
                            }
                        }
                    ],
                    "synonyms": [
                        "посторонний",
                        "незнакомый",
                        "неизвестный",
                        "неродной"
                    ],
                    "antonyms": [
                        "родственный",
                        "свой"
                    ],
                    "common_phrases": [
                        "чужой человек",
                        "чужие люди",
                        "чужой дядя"
                    ],
                    "target_lang_definitions": {
                        "en": "Not related or close; outsider, unfamiliar."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unfamiliar",
                                "target_lang_sense_clarification": "not known or recognized"
                            },
                            {
                                "target_lang_word": "strange",
                                "target_lang_sense_clarification": "unknown, not previously met"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d382a22-2ef5-3e33-9511-f41683b37bf4",
                    "sense_definition": "Относящийся к другой стране или местности; не являющийся родным.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему было одиноко в <чужой> стране.",
                            "target_lang_translations": {
                                "en": "He was lonely in a <w>foreign</w> country."
                            }
                        },
                        {
                            "text": "Мы услышали на улице <чужую> речь.",
                            "target_lang_translations": {
                                "en": "We heard <w>foreign</w> speech on the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "иноземный"
                    ],
                    "antonyms": [
                        "родственный"
                    ],
                    "common_phrases": [
                        "чужая страна",
                        "чужой край",
                        "чужая земля",
                        "чужой язык"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to another country or place; not native."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "foreign",
                                "target_lang_sense_clarification": "from another country"
                            },
                            {
                                "target_lang_word": "alien",
                                "target_lang_sense_clarification": "belonging to a foreign place (literary)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b55ca511-24a3-3f25-a3ab-d9aaa68ab60d",
                    "sense_definition": "Далёкий по духу, взглядам или интересам; внутренне не близкий.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "spirit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот мир мне совершенно <чужой>.",
                            "target_lang_translations": {
                                "en": "This world is completely <w>alien</w> to me."
                            }
                        },
                        {
                            "text": "Мы стали друг другу <чужими> людьми.",
                            "target_lang_translations": {
                                "en": "We became <w>strangers</w> to each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "чуждый",
                        "отчуждённый",
                        "отрешённый"
                    ],
                    "antonyms": [
                        "родственный"
                    ],
                    "common_phrases": [
                        "чужой по духу",
                        "чужая культура",
                        "чужой мир"
                    ],
                    "target_lang_definitions": {
                        "en": "Distant in spirit, views, or interests; internally not close."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "alien",
                                "target_lang_sense_clarification": "completely different or repugnant to one's nature"
                            },
                            {
                                "target_lang_word": "stranger",
                                "target_lang_sense_clarification": "one who is estranged or not close"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e891040-1e7b-38d2-aacb-b339f8d585d5",
                    "sense_definition": "Посторонний человек, не являющийся родственником или близким знакомым.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Собака громко лает на <чужих>.",
                            "target_lang_translations": {
                                "en": "The dog barks loudly at <w>strangers</w>."
                            }
                        },
                        {
                            "text": "Он никогда не открывает дверь <чужим>.",
                            "target_lang_translations": {
                                "en": "He never opens the door to <w>strangers</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "посторонний"
                    ],
                    "antonyms": [
                        "свой"
                    ],
                    "common_phrases": [
                        "свой среди чужих"
                    ],
                    "target_lang_definitions": {
                        "en": "An outsider, not a relative or close acquaintance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stranger",
                                "target_lang_sense_clarification": "a person one does not know"
                            },
                            {
                                "target_lang_word": "outsider",
                                "target_lang_sense_clarification": "one not belonging to a group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d03ba25b-e794-3a64-b059-eef7fa3a0db7",
                    "sense_definition": "Внеземное существо из одноимённой фантастической медиафраншизы.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fiction",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "В фильме герои сражаются против опасного <Чужого>.",
                            "target_lang_translations": {
                                "en": "In the movie, the heroes fight against the dangerous <w>Alien</w>."
                            }
                        },
                        {
                            "text": "Образ <Чужого> стал культовым в кино.",
                            "target_lang_translations": {
                                "en": "The image of the <w>Alien</w> became cult in cinema."
                            }
                        }
                    ],
                    "synonyms": [
                        "ксеноморф"
                    ],
                    "target_lang_definitions": {
                        "en": "Extraterrestrial creature from the sci-fi franchise of the same name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Alien",
                                "target_lang_sense_clarification": "the fictional extraterrestrial creature"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "30417a8f-54b4-3760-a738-7858f385d8ef",
                    "sense_definition": "The activity of buying and selling goods and services, especially on a large scale between different countries or regions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government is working to promote international <w>commerce</w> with neighboring countries.",
                            "target_lang_translations": {
                                "ru": "Правительство работает над развитием международной <w>торговли</w> с соседними странами."
                            }
                        },
                        {
                            "text": "The local chamber of <w>commerce</w> helps small businesses grow and network.",
                            "target_lang_translations": {
                                "ru": "Местная <w>торговая</w> палата помогает малому бизнесу расти и налаживать связи."
                            }
                        }
                    ],
                    "synonyms": [
                        "trade",
                        "traffic"
                    ],
                    "common_phrases": [
                        "chamber of commerce",
                        "international commerce",
                        "electronic commerce",
                        "online commerce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по покупке и продаже товаров и услуг, особенно в крупных масштабах между разными странами или регионами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговля",
                                "target_lang_sense_clarification": "обмен товарами"
                            },

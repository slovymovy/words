    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44571b4c-9306-307d-bdb8-fb620636178e",
                    "sense_definition": "Een vaste plaats waar openbaar vervoer, zoals een bus of tram, stopt om passagiers te laten in- en uitstappen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stop_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat hij vlak naast een <w>halte</w> woont, gaat hij vaak met de bus.",
                            "target_lang_translations": {
                                "ru": "Так как он живет прямо рядом с <w>остановкой</w>, он часто ездит на автобусе."
                            }
                        },
                        {
                            "text": "Ik stap bij de volgende <w>halte</w> uit om naar huis te lopen.",
                            "target_lang_translations": {
                                "ru": "Я выхожу на следующей <w>остановке</w>, чтобы пойти домой пешком."
                            }
                        }
                    ],
                    "synonyms": [
                        "bushalte",
                        "tramhalte"
                    ],
                    "common_phrases": [
                        "volgende halte",
                        "bij de halte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фиксированное место, где останавливается общественный транспорт, например автобус или трамвай, для посадки и высадки пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "общественного транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21bff12a-e960-365b-81a4-601e9046af29",
                    "sense_definition": "Een tijdelijke onderbreking van een reis of een plaats waar men stopt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "stop_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een korte <w>halte</w> gingen we verder met de reis.",
                            "target_lang_translations": {
                                "ru": "После короткой <w>остановки</w> мы продолжили путешествие."
                            }
                        },
                        {
                            "text": "We maakten een <w>halte</w> in een klein dorpje om te lunchen.",
                            "target_lang_translations": {
                                "ru": "Мы сделали <w>остановку</w> в маленькой деревушке, чтобы пообедать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een korte halte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временный перерыв в путешествии или место, где делают остановку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "пауза в пути"
                            },
                            {
                                "target_lang_word": "привал",
                                "target_lang_sense_clarification": "отдых во время похода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa37dd97-67c0-38dd-8e04-16c5a365e4ac",
                    "sense_definition": "Een rustpunt of tussenfase in een proces of ontwikkeling.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stop_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze periode was een nuttige <w>halte</w> in zijn leven om tot rust te komen.",
                            "target_lang_translations": {
                                "ru": "Этот период стал полезной <w>передышкой</w> в его жизни, чтобы успокоиться."
                            }
                        },
                        {
                            "text": "Het behalen van zijn diploma was een belangrijke <w>halte</w> in zijn carrière.",
                            "target_lang_translations": {
                                "ru": "Получение диплома стало важным <w>этапом</w> в его карьере."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент отдыха или промежуточная фаза в процессе или развитии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этап",
                                "target_lang_sense_clarification": "стадия развития"
                            },
                            {
                                "target_lang_word": "веха",
                                "target_lang_sense_clarification": "важный момент"
                            },
                            {
                                "target_lang_word": "передышка",
                                "target_lang_sense_clarification": "момент отдыха"
                            }
                        ]
                    }
                }
            ]
        },

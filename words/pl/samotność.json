    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7794ffc-9ea5-369d-90d8-c9264f854be1",
                    "sense_definition": "Uczucie smutku, pustki lub przygnębienia wynikające z braku towarzystwa lub bliskich relacji z innymi ludźmi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej <samotność> stawała się nie do zniesienia po wyjeździe dzieci z domu.",
                            "target_lang_translations": {
                                "ru": "Её <w>одиночество</w> становилось невыносимым после отъезда детей из дома."
                            }
                        },
                        {
                            "text": "W wielkim mieście często doskwierała mu <samotność>, mimo tłumu ludzi wokół.",
                            "target_lang_translations": {
                                "ru": "В большом городе его часто мучило <w>одиночество</w>, несмотря на толпу людей вокруг."
                            }
                        }
                    ],
                    "synonyms": [
                        "osamotnienie"
                    ],
                    "common_phrases": [
                        "poczucie samotności",
                        "strach przed samotnością",
                        "doskwierająca samotność",
                        "walczyć z samotnością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство грусти, пустоты или подавленности, возникающее из-за отсутствия общества или близких отношений с другими людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одиночество",
                                "target_lang_sense_clarification": "эмоциональное состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d436303-1840-3417-a512-5475bdd827f9",
                    "sense_definition": "Stan przebywania bez towarzystwa innych ludzi; życie w odosobnieniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czasami potrzebuję chwili <samotności>, żeby odpocząć od hałasu i pośpiechu.",
                            "target_lang_translations": {
                                "ru": "Иногда мне нужна минута <w>уединения</w>, чтобы отдохнуть от шума и суеты."
                            }
                        },
                        {
                            "text": "Mnich wybrał całkowitą <samotność>, aby móc się modlić w spokoju.",
                            "target_lang_translations": {
                                "ru": "Монах выбрал полное <w>уединение</w>, чтобы молиться в спокойствии."
                            }
                        }
                    ],
                    "synonyms": [
                        "odosobnienie",
                        "izolacja"
                    ],
                    "antonyms": [
                        "towarzystwo"
                    ],
                    "common_phrases": [
                        "wybrać samotność",
                        "życie w samotności",
                        "szukać samotności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние пребывания без общества других людей; жизнь в изоляции или уединении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уединение",
                                "target_lang_sense_clarification": "пребывание наедине с собой"
                            },
                            {
                                "target_lang_word": "одиночество",
                                "target_lang_sense_clarification": "физическая изоляция"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
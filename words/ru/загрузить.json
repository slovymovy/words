    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3762e16-5764-3c33-87e2-c8ac99ece8aa",
                    "sense_definition": "Наполнить что-либо грузом; поместить вещи или товары внутрь транспортного средства или контейнера.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "loading_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие помогли нам быстро <загрузить> мебель в грузовик.",
                            "target_lang_translations": {
                                "en": "The workers helped us quickly <load> the furniture into the truck."
                            }
                        },
                        {
                            "text": "Перед поездкой нужно <загрузить> все чемоданы в багажник.",
                            "target_lang_translations": {
                                "en": "Before the trip, we need to <load> all the suitcases into the trunk."
                            }
                        }
                    ],
                    "synonyms": [
                        "нагрузить"
                    ],
                    "antonyms": [
                        "разгрузить"
                    ],
                    "common_phrases": [
                        "загрузить вещи",
                        "загрузить машину",
                        "загрузить вагон",
                        "загрузить багаж"
                    ],
                    "target_lang_definitions": {
                        "en": "To fill something with cargo; to place things or goods inside a vehicle or container."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "load",
                                "target_lang_sense_clarification": "to place cargo into a vehicle or container"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b328881-ac91-39c3-b21a-4157c9946fa3",
                    "sense_definition": "Перенести данные или программы в память компьютера или другого электронного устройства; скачать файл из сети.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "loading_digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я пытаюсь <загрузить> новую игру на свой телефон.",
                            "target_lang_translations": {
                                "en": "I am trying to <download> a new game to my phone."
                            }
                        },
                        {
                            "text": "Вам необходимо <загрузить> обновление, чтобы программа работала корректно.",
                            "target_lang_translations": {
                                "en": "You need to <download> the update for the program to work correctly."
                            }
                        }
                    ],
                    "synonyms": [
                        "закачать"
                    ],
                    "common_phrases": [
                        "загрузить файл",
                        "загрузить фото",
                        "загрузить программу",
                        "загрузить данные"
                    ],
                    "target_lang_definitions": {
                        "en": "To transfer data or programs to the memory of a computer or another electronic device; to download a file from a network."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "download",
                                "target_lang_sense_clarification": "to transfer data from a network"
                            },
                            {
                                "target_lang_word": "load",
                                "target_lang_sense_clarification": "to transfer data into memory or software"
                            },
                            {
                                "target_lang_word": "upload",
                                "target_lang_sense_clarification": "to transfer data to a network"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1885cbf9-d737-33f5-bd02-ed149dc20d34",
                    "sense_definition": "Обеспечить кого-либо большим количеством работы, заданий или дел.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "loading_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник решил <загрузить> отдел дополнительными отчётами.",
                            "target_lang_translations": {
                                "en": "The boss decided to <load> the department with additional reports."
                            }
                        },
                        {
                            "text": "Не стоит так сильно <загрузить> себя делами перед отпуском.",
                            "target_lang_translations": {
                                "en": "You shouldn't <overload> yourself so much with tasks before vacation."
                            }
                        }
                    ],
                    "synonyms": [
                        "занять",
                        "нагрузить"
                    ],
                    "antonyms": [
                        "разгрузить"
                    ],
                    "common_phrases": [
                        "загрузить работой",
                        "загрузить делами",
                        "загрузить по полной"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide someone with a large amount of work, tasks, or business."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "load",
                                "target_lang_sense_clarification": "to assign a large amount of work"
                            },
                            {
                                "target_lang_word": "burden",
                                "target_lang_sense_clarification": "to weigh down with tasks or responsibilities"
                            },
                            {
                                "target_lang_word": "overload",
                                "target_lang_sense_clarification": "to give too much work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a9b4123-198a-3d4f-aeca-feb146fc2d99",
                    "sense_definition": "Утомить кого-либо сложной информацией, проблемами или долгими разговорами; запутать.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "loading_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он любит <загрузить> собеседника своими философскими теориями.",
                            "target_lang_translations": {
                                "en": "He likes to <overwhelm> his interlocutor with his philosophical theories."
                            }
                        },
                        {
                            "text": "Не пытайся <загрузить> меня своими проблемами, я и так устал.",
                            "target_lang_translations": {
                                "en": "Don't try to <burden> me with your problems, I'm already tired."
                            }
                        }
                    ],
                    "common_phrases": [
                        "загрузить мозги",
                        "загрузить проблемами"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To tire someone with complex information, problems, or long conversations; to confuse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overwhelm",
                                "target_lang_sense_clarification": "to overpower with information or emotion"
                            },
                            {
                                "target_lang_word": "burden",
                                "target_lang_sense_clarification": "to trouble someone with problems"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccd55ba6-9122-35b3-a4d0-bc24ae4e1e8e",
                    "sense_definition": "Поместить сырьё или топливо в специальное устройство (печь, топку) для переработки.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "loading_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В доменную печь нужно <загрузить> очередную партию руды.",
                            "target_lang_translations": {
                                "en": "We need to <load> the next batch of ore into the blast furnace."
                            }
                        },
                        {
                            "text": "Оператор должен вовремя <загрузить> топливо в котёл.",
                            "target_lang_translations": {
                                "en": "The operator must <load> fuel into the boiler on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "заложить"
                    ],
                    "common_phrases": [
                        "загрузить печь",
                        "загрузить топливо",
                        "загрузить сырьё"
                    ],
                    "target_lang_definitions": {
                        "en": "To place raw material or fuel into a special device (furnace, firebox) for processing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "load",
                                "target_lang_sense_clarification": "to fill a machine or device with material"
                            },
                            {
                                "target_lang_word": "charge",
                                "target_lang_sense_clarification": "technical: to fill a furnace or reactor"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
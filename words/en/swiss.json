    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b9c97904-3c31-350c-a722-b8332e968f91",
                    "sense_definition": "Of, from, or pertaining to Switzerland or the Swiss people.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought some delicious <w>Swiss</w> chocolate at the airport shop.",
                            "target_lang_translations": {
                                "ru": "Мы купили вкусный <w>швейцарский</w> шоколад в магазине в аэропорту."
                            }
                        },
                        {
                            "text": "He always carries a <w>Swiss</w> Army knife in his pocket.",
                            "target_lang_translations": {
                                "ru": "Он всегда носит в кармане <w>швейцарский</w> армейский нож."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Swiss cheese",
                        "Swiss chocolate",
                        "Swiss Army knife",
                        "Swiss watch",
                        "Swiss roll"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Швейцарии или швейцарцам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "швейцарский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbbcd43f-4c90-3ebd-b1f4-a7f82bc55cf8",
                    "sense_definition": "A person from Switzerland or of Swiss descent.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Swiss</w> are famous for their neutrality and banking system.",
                            "target_lang_translations": {
                                "ru": "<w>Швейцарцы</w> известны своим нейтралитетом и банковской системой."
                            }
                        },
                        {
                            "text": "She married a <w>Swiss</w> and moved to Zurich last year.",
                            "target_lang_translations": {
                                "ru": "Она вышла замуж за <w>швейцарца</w> и переехала в Цюрих в прошлом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "Switzer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Житель Швейцарии или человек швейцарского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {

{
    "word_family": [
        "odwracać",
        "odwrotny",
        "odwrotnie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6df2c76b-e0be-38be-bf52-18320bd821da",
                    "sense_definition": "Manewr wojskowy polegający na zorganizowanym wycofaniu się oddziałów z zajmowanych pozycji, zazwyczaj pod naporem wroga.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "military_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Generał, widząc przewagę wroga, natychmiast zarządził taktyczny <w>odwrót</w>.",
                            "target_lang_translations": {
                                "ru": "Генерал, видя превосходство врага, немедленно приказал совершить тактическое <w>отступление</w>."
                            }
                        },
                        {
                            "text": "Nasi żołnierze osłaniali <w>odwrót</w> cywilów z zagrożonego miasta.",
                            "target_lang_translations": {
                                "ru": "Наши солдаты прикрывали <w>отход</w> мирных жителей из города, находящегося под угрозой."
                            }
                        }
                    ],
                    "synonyms": [
                        "wycofanie się",
                        "ewakuacja",
                        "rejterada"
                    ],
                    "antonyms": [
                        "atak",
                        "natarcie"
                    ],
                    "common_phrases": [
                        "zarządzić odwrót",
                        "odciąć drogę odwrotu",
                        "odwrót taktyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военный маневр, заключающийся в организованном отходе войск с занимаемых позиций, обычно под натиском врага."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отступление",
                                "target_lang_sense_clarification": "военный маневр"
                            },
                            {
                                "target_lang_word": "отход",
                                "target_lang_sense_clarification": "организованное перемещение назад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "593181d1-e4e1-3c5f-9a63-e8fce6d65963",
                    "sense_definition": "Rezygnacja z podjętych wcześniej działań, decyzji lub poglądów; wycofanie się z trudnej sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podjęliśmy ostateczną decyzję i teraz nie ma już od niej <w>odwrotu</w>.",
                            "target_lang_translations": {
                                "ru": "Мы приняли окончательное решение, и теперь <w>пути назад</w> уже нет."
                            }
                        },
                        {
                            "text": "To była droga bez <w>odwrotu</w>, więc musieliśmy kontynuować naszą podróż.",
                            "target_lang_translations": {
                                "ru": "Это была дорога без <w>возврата</w>, поэтому нам пришлось продолжить наше путешествие."
                            }
                        }
                    ],
                    "synonyms": [
                        "wycofanie",
                        "odstąpienie",
                        "cofnięcie się"
                    ],
                    "common_phrases": [
                        "nie ma odwrotu",
                        "droga bez odwrotu",
                        "wziąć w odwrót"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказ от ранее предпринятых действий, решений или взглядов; невозможность вернуться к прежнему состоянию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возврат",
                                "target_lang_sense_clarification": "возвращение к прежнему"
                            },
                            {
                                "target_lang_word": "путь назад",
                                "target_lang_sense_clarification": "возможность отменить решение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3333be9b-c2ea-3bbb-9e05-c5629bbad23f",
                    "sense_definition": "Druga, tylna strona płaskiego przedmiotu, na przykład kartki papieru, dokumentu lub medalu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę podpisać się czytelnie na <w>odwrocie</w> tego formularza.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, распишитесь разборчиво на <w>обороте</w> этого бланка."
                            }
                        },
                        {
                            "text": "Na <w>odwrocie</w> starego zdjęcia znalazłem datę i miejsce wykonania fotografii.",
                            "target_lang_translations": {
                                "ru": "На <w>обороте</w> старой фотографии я нашел дату и место съемки."
                            }
                        }
                    ],
                    "synonyms": [
                        "rewers"
                    ],
                    "common_phrases": [
                        "na odwrocie",
                        "zobacz na odwrocie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вторая, задняя сторона плоского предмета, например, листа бумаги, документа или медали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборот",
                                "target_lang_sense_clarification": "обратная сторона листа или документа"
                            },
                            {
                                "target_lang_word": "обратная сторона",
                                "target_lang_sense_clarification": "тыльная часть предмета"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
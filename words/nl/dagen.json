                        "ru": "Официально вызывать для появления в суде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "summon",
                                "target_lang_sense_clarification": "legal order"
                            },
                            {
                                "target_lang_word": "take to court",
                                "target_lang_sense_clarification": "sue"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вызывать в суд",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "привлекать к суду",
                                "target_lang_sense_clarification": "требовать судебного разбирательства"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "822219d1-102b-3813-9ca0-7eaa2c315471",
                    "sense_definition": "Причинять вред самому себе, разрушать своё здоровье, жизнь или моральный облик.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "self_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты <w>губишься</w> на этой работе, работая без выходных и перерывов.",
                            "target_lang_translations": {
                                "en": "You are <w>ruining yourself</w> at this job, working without weekends or breaks."
                            }
                        },
                        {
                            "text": "Не стоит так <w>губиться</w> из-за проблем, которые можно легко решить.",
                            "target_lang_translations": {
                                "en": "It is not worth <w>killing yourself</w> over problems that can be easily solved."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрушать себя",
                        "убиваться"
                    ],
                    "antonyms": [
                        "беречь себя",
                        "спасаться"
                    ],
                    "common_phrases": [
                        "губиться понапрасну",
                        "зря губиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To harm oneself, to ruin one's health, life, or moral character."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ruin oneself",
                                "target_lang_sense_clarification": "to destroy one's own health or life"
                            },
                            {
                                "target_lang_word": "destroy oneself",
                                "target_lang_sense_clarification": "stronger form of self-ruin"
                            },
                            {
                                "target_lang_word": "kill oneself",
                                "target_lang_sense_clarification": "figurative; to work or worry excessively"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f13f22e7-0b3f-33f3-bcee-3bf1a40cd18a",
                    "sense_definition": "Страдательная форма от глагола <w>губить</w>; подвергаться уничтожению, порче или разрушению под воздействием внешних сил.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "passive_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за постоянных дождей весь урожай на полях <w>губится</w>.",
                            "target_lang_translations": {
                                "en": "Due to constant rains, the entire harvest in the fields is <w>being ruined</w>."
                            }
                        },
                        {
                            "text": "Талант художника <w>губится</w>, если он не находит поддержки и вдохновения.",
                            "target_lang_translations": {
                                "en": "An artist's talent is <w>wasted</w> if he does not find support and inspiration."
                            }
                        }
                    ],
                    "synonyms": [
                        "уничтожаться",
                        "разрушаться",
                        "портиться"
                    ],
                    "antonyms": [
                        "сохраняться",
                        "спасаться"
                    ],
                    "common_phrases": [
                        "урожай губится",
                        "природа губится"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive form of the verb 'губить'; to be destroyed, spoiled, or ruined by external forces."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be ruined",
                                "target_lang_sense_clarification": "to undergo ruin or spoilage"
                            },
                            {
                                "target_lang_word": "be destroyed",
                                "target_lang_sense_clarification": "to be completely wrecked"
                            },
                            {
                                "target_lang_word": "be wasted",
                                "target_lang_sense_clarification": "often used regarding talent or potential"
                            }
                        ]
                    }
                }
            ]
        }

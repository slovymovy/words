                            {
                                "target_lang_word": "unharmed",
                                "target_lang_sense_clarification": "not injured, usually referring to living beings"
                            },
                            {
                                "target_lang_word": "safe",
                                "target_lang_sense_clarification": "often used in the phrase 'safe and sound'"
                            }
                        ]
                    }
                },
                {

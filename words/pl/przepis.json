                {
                    "sense_id": "97dc1410-f824-35bf-bd4a-fcc2e6a14162",
                    "sense_definition": "Instrukcja przygotowania potrawy, zawierająca listę składników oraz opis kolejnych czynności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cooking_instruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znalazłam w internecie prosty <w>przepis</w> na ciasto czekoladowe.",
                            "target_lang_translations": {
                                "ru": "Я нашла в интернете простой <w>рецепт</w> шоколадного торта."
                            }
                        },
                        {
                            "text": "Moja babcia zawsze gotuje rosół według starego, rodzinnego <w>przepisu</w>.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда готовит бульон по старому семейному <w>рецепту</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przepis na ciasto",
                        "przepis kulinarny",
                        "według przepisu",
                        "prosty przepis"
                    ],
                    "target_lang_definitions": {

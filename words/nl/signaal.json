            "pos": "noun",
            "senses": [
                {
                    "sense_id": "469c068e-7cec-36ec-9a69-02e1f8a2d218",
                    "sense_definition": "Een afgesproken teken, gebaar of geluid waarmee een boodschap, bevel of waarschuwing wordt gegeven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter gaf het <w>signaal</w> voor de start van de wedstrijd.",
                            "target_lang_translations": {
                                "en": "The referee gave the <w>signal</w> for the start of the match.",
                                "ru": "Судья дал <w>сигнал</w> к началу матча."
                            }
                        },
                        {
                            "text": "Bij het <w>signaal</w> van de fluit moeten alle leerlingen naar binnen gaan.",
                            "target_lang_translations": {
                                "en": "At the whistle <w>signal</w>, all students must go inside.",
                                "ru": "По <w>сигналу</w> свистка все ученики должны зайти внутрь."
                            }
                        }
                    ],
                    "synonyms": [
                        "sein",
                        "teken",
                        "gebaar",
                        "wenk"
                    ],
                    "common_phrases": [
                        "een signaal geven",
                        "wachten op het signaal",
                        "het start-signaal"
                    ],
                    "target_lang_definitions": {
                        "en": "An agreed sign, gesture, or sound used to convey a message, command, or warning.",
                        "ru": "Условный знак, жест или звук, используемый для передачи сообщения, приказа или предупреждения."
                    },
                    "translations": {

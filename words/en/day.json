                {
                    "sense_id": "da94a50e-a3cc-3b5f-ae77-1a1a9918dcef",
                    "sense_definition": "A period of time equal or almost equal to a full day-night cycle, being 24 hours long.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are seven <w>days</w> in a week.",
                            "target_lang_translations": {
                                "ru": "В неделе семь <w>дней</w>."
                            }
                        },
                        {
                            "text": "I will finish the project in two <w>days</w>.",
                            "target_lang_translations": {
                                "ru": "Я закончу проект через два <w>дня</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "twenty-four hour day"
                    ],
                    "common_phrases": [
                        "every day",
                        "all day",
                        "one day",
                        "next day",
                        "day by day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, равный 24 часам; сутки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день",
                                "target_lang_sense_clarification": "календарная единица"
                            },
                            {
                                "target_lang_word": "сутки",
                                "target_lang_sense_clarification": "период в 24 часа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f573bcea-550c-3c58-ab55-6d2943fd38cb",
                    "sense_definition": "The time when the Sun is above the horizon and it lights the sky.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "55d92781-aa57-3262-acc4-eaa25c44d628",
                    "sense_definition": "Предпринять какие-либо действия, сделать что-либо для изменения ситуации (часто используется в отрицательных конструкциях или риторических вопросах, выражая невозможность изменить обстоятельства).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_undertake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К сожалению, с этим решением уже ничего не <поделаешь>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, there is nothing you can <do> about this decision anymore."
                            }
                        },
                        {
                            "text": "Что тут <поделаешь>, нам придётся смириться с погодой.",
                            "target_lang_translations": {
                                "en": "What can you <do>, we'll have to accept the weather."
                            }
                        }
                    ],
                    "synonyms": [
                        "предпринять",
                        "изменить",
                        "сделать"
                    ],
                    "common_phrases": [
                        "ничего не поделаешь",
                        "что тут поделаешь",
                        "что с ним поделаешь"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To take action to change a situation (often used in negative constructions implying impossibility)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "about a situation, often in 'nothing to do'"
                            },
                            {
                                "target_lang_word": "help",
                                "target_lang_sense_clarification": "in the sense of 'cannot help it'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "995b7e59-845f-32f3-aa98-e47daf838c65",
                    "sense_definition": "Заняться каким-либо делом, поработать в течение некоторого времени.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу немного <поделать> домашнее задание, пока есть свободное время.",
                            "target_lang_translations": {
                                "en": "I want to <do> some homework while I have free time."
                            }
                        },
                        {
                            "text": "Ему нужно было что-то <поделать> руками, чтобы успокоить нервы.",
                            "target_lang_translations": {
                                "en": "He needed to <do> something with his hands to calm his nerves."
                            }
                        }
                    ],
                    "synonyms": [
                        "позаниматься",
                        "поработать"
                    ],
                    "antonyms": [
                        "побездельничать"
                    ],
                    "common_phrases": [
                        "поделать дела",
                        "поделать уроки",
                        "поделать работу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To engage in an activity or work for a certain period of time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "perform an activity for a while"
                            },
                            {
                                "target_lang_word": "work on",
                                "target_lang_sense_clarification": "spend time on a task"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1018f47-0c83-3119-bba9-c8ada6916dd3",
                    "sense_definition": "Сделать, выполнить множество дел или совершить ряд действий одно за другим.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_cumulative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За утро она успела <поделать> кучу мелких домашних дел.",
                            "target_lang_translations": {
                                "en": "In the morning she managed to <get> a bunch of small household chores <done>."
                            }
                        },
                        {
                            "text": "Мы всё <поделали> в саду и теперь можем отдыхать.",
                            "target_lang_translations": {
                                "en": "We <did> everything in the garden and now we can rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "переделать",
                        "выполнить"
                    ],
                    "common_phrases": [
                        "всё поделать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To complete a number of tasks or perform a series of actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get done",
                                "target_lang_sense_clarification": "complete multiple tasks"
                            },
                            {
                                "target_lang_word": "finish",
                                "target_lang_sense_clarification": "complete a set of actions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5e0e113-c59d-3ad7-bf6e-f7f8d07e3ad2",
                    "sense_definition": "Изготовить, смастерить что-либо (обычно кустарным способом).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_make",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Местный мастер обещал <поделать> нам новые скамейки для двора.",
                            "target_lang_translations": {
                                "en": "The local craftsman promised to <make> us new benches for the yard."
                            }
                        },
                        {
                            "text": "Он умел <поделать> удивительные игрушки из простой глины.",
                            "target_lang_translations": {
                                "en": "He knew how to <craft> amazing toys out of simple clay."
                            }
                        }
                    ],
                    "synonyms": [
                        "смастерить",
                        "изготовить"
                    ],
                    "antonyms": [
                        "сломать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To manufacture or craft something (usually in a rustic or colloquial manner)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "construct something"
                            },
                            {
                                "target_lang_word": "craft",
                                "target_lang_sense_clarification": "create manually"
                            }
                        ]
                    }
                }
            ]
        }
    ]

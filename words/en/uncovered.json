    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e32a1495-a6bf-388b-9405-d112cc3bec26",
                    "sense_definition": "Simple past and past participle of <w>uncover</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investigation <w>uncovered</w> a major scandal within the company.",
                            "target_lang_translations": {
                                "ru": "Расследование <w>раскрыло</w> крупный скандал внутри компании."
                            }
                        },
                        {
                            "text": "She <w>uncovered</w> the old painting to see if it was damaged.",
                            "target_lang_translations": {
                                "ru": "Она <w>открыла</w> старую картину, чтобы проверить, не повреждена ли она."
                            }
                        }
                    ],
                    "synonyms": [
                        "revealed"
                    ],
                    "antonyms": [
                        "covered",
                        "hid",
                        "concealed"
                    ],
                    "common_phrases": [
                        "uncovered the truth",
                        "uncovered a plot",
                        "uncovered evidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола uncover (раскрывать, обнаруживать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрыл",
                                "target_lang_sense_clarification": "сделал тайное явным"
                            },
                            {
                                "target_lang_word": "обнаружил",
                                "target_lang_sense_clarification": "нашел то, что было скрыто"
                            },
                            {
                                "target_lang_word": "открыл",
                                "target_lang_sense_clarification": "снял крышку или покрытие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "282bb705-1eeb-3461-b4a9-9e5e8367e9a5",
                    "sense_definition": "Not covered or protected from the weather or elements.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>uncovered</w> patio furniture got soaked in the rain.",
                            "target_lang_translations": {
                                "ru": "<w>Ненакрытая</w> садовая мебель промокла под дождем."
                            }
                        },
                        {
                            "text": "Please do not leave the food <w>uncovered</w> on the counter.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не оставляй еду <w>открытой</w> на столе."
                            }
                        }
                    ],
                    "antonyms": [
                        "covered",
                        "protected",
                        "sheltered"
                    ],
                    "common_phrases": [
                        "uncovered ground",
                        "left uncovered",
                        "uncovered wagon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не накрытый крышкой, чехлом или не защищенный от воздействия стихии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непокрытый",
                                "target_lang_sense_clarification": "не имеющий покрытия"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "доступный воздействию внешней среды"
                            },
                            {
                                "target_lang_word": "неприкрытый",
                                "target_lang_sense_clarification": "лишенный защиты или маскировки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55afd878-5752-3919-8ea5-bedc229e006a",
                    "sense_definition": "Lacking insurance or financial security.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bank refused to accept the <w>uncovered</w> checks.",
                            "target_lang_translations": {
                                "ru": "Банк отказался принимать <w>непокрытые</w> чеки."
                            }
                        },
                        {
                            "text": "He was worried about the <w>uncovered</w> medical expenses.",
                            "target_lang_translations": {
                                "ru": "Он беспокоился о <w>непокрытых</w> медицинских расходах."
                            }
                        }

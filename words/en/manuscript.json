            "senses": [
                {
                    "sense_id": "67cc1a24-b536-333d-b5d7-c5e9be6519d1",
                    "sense_definition": "A copy of a book, article, or document submitted by an author for publication.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sent the <w>manuscript</w> to the publisher last week.",
                            "target_lang_translations": {
                                "ru": "Она отправила <w>рукопись</w> издателю на прошлой неделе."
                            }
                        },
                        {
                            "text": "The editor found a few errors in the original <w>manuscript</w>.",
                            "target_lang_translations": {
                                "ru": "Редактор нашел несколько ошибок в оригинальной <w>рукописи</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "submit a manuscript",
                        "unpublished manuscript",
                        "edit a manuscript"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копия книги, статьи или документа, представленная автором для публикации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рукопись",
                                "target_lang_sense_clarification": "черновик произведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1641a78-b0fc-3363-8d50-1b0eed28f71f",
                    "sense_definition": "A very old document or book written by hand before printing was invented.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displays a rare illuminated <w>manuscript</w> from the Middle Ages.",
                            "target_lang_translations": {
                                "ru": "В музее выставлен редкий иллюминированный <w>манускрипт</w> эпохи Средневековья."
                            }
                        },
                        {
                            "text": "Scholars studied the ancient <w>manuscript</w> to understand the history of the region.",
                            "target_lang_translations": {
                                "ru": "Ученые изучали древнюю <w>рукопись</w>, чтобы понять историю региона."
                            }
                        }
                    ],
                    "synonyms": [
                        "autograph"
                    ],
                    "common_phrases": [
                        "illuminated manuscript",
                        "ancient manuscript",
                        "medieval manuscript"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень старый документ или книга, написанные от руки до изобретения книгопечатания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манускрипт",
                                "target_lang_sense_clarification": "древний документ"
                            },
                            {
                                "target_lang_word": "рукопись",
                                "target_lang_sense_clarification": "старинная книга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "910ac61b-6d59-36a9-9e3a-3a9287cca33f",
                    "sense_definition": "Written by hand rather than printed or typed mechanically.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He discovered a <w>manuscript</w> letter hidden in the old desk.",
                            "target_lang_translations": {
                                "ru": "Он обнаружил <w>рукописное</w> письмо, спрятанное в старом письменном столе."

            "senses": [
                {
                    "sense_id": "aa7c7e81-c281-3f62-b893-a6ed7cdc9c6c",
                    "sense_definition": "Płyta optyczna o dużej pojemności, służąca do cyfrowego zapisu danych, w szczególności filmów i muzyki.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "media_storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem ulubiony film na <w>DVD</w>, aby móc go oglądać w dowolnej chwili.",
                            "target_lang_translations": {
                                "ru": "Я купил любимый фильм на <w>DVD</w>, чтобы иметь возможность посмотреть его в любой момент."
                            }
                        },
                        {
                            "text": "Ta drukarka posiada funkcję drukowania etykiet bezpośrednio na płytach <w>DVD</w>.",
                            "target_lang_translations": {
                                "ru": "У этого принтера есть функция печати этикеток непосредственно на <w>DVD</w>-дисках."
                            }
                        }
                    ],
                    "synonyms": [
                        "płyta",
                        "krążek",
                        "dysk"
                    ],
                    "common_phrases": [
                        "płyta DVD",
                        "film na DVD",
                        "nagrać na DVD",
                        "format DVD"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "nieodmienne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оптический диск большой емкости, используемый для цифровой записи данных, в частности фильмов и музыки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "DVD",
                                "target_lang_sense_clarification": "формат"
                            },
                            {
                                "target_lang_word": "DVD-диск",
                                "target_lang_sense_clarification": "носитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b36d5f40-d294-3d08-9125-b6c1f5b268c9",
                    "sense_definition": "Urządzenie elektroniczne (odtwarzacz lub napęd komputerowy) służące do odczytu i odtwarzania danych z płyt tego formatu.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "media_device",
                    "name_type": "no",
                    "examples": [

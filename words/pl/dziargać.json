    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "91c67bcf-a2f3-3eb1-8648-3e24aca9dacc",
                    "sense_definition": "Wykonywać robótki ręczne, takie jak robienie na drutach, szydełkowanie lub haftowanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "needlework_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia uwielbia siadać w fotelu i <w>dziargać</w> ciepłe skarpety dla całej rodziny.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка любит садиться в кресло и <w>вязать</w> теплые носки для всей семьи."
                            }
                        },
                        {
                            "text": "Zamiast kupować gotowy szalik, postanowiłam sama go <w>dziargać</w> na drutach.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы покупать готовый шарф, я решила сама его <w>связать</w> на спицах."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziergać"
                    ],
                    "common_phrases": [
                        "dziargać na drutach",
                        "dziargać sweter",
                        "dziargać szalik"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo potoczne, często używane w kontekście hobby"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заниматься рукоделием, таким как вязание на спицах, крючком или вышивание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вязать",
                                "target_lang_sense_clarification": "спицами или крючком"
                            },
                            {
                                "target_lang_word": "рукодельничать",
                                "target_lang_sense_clarification": "заниматься ручной работой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "284c6c8c-a169-37ab-bad0-832180564ae4",
                    "sense_definition": "Wykonywać tatuaż na skórze.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "tattoo_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój znajomy zaczął profesjonalnie <w>dziargać</w> w studiu tatuażu w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый начал профессионально <w>бить</w> татуировки в студии в центре города."
                            }
                        },
                        {
                            "text": "Chcę sobie <w>dziargać</w> wzór smoka na całym ramieniu.",
                            "target_lang_translations": {
                                "ru": "Я хочу <w>набить</w> себе узор дракона на всем плече."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dziargać tatuaż",
                        "dziargać rękaw"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "popularne określenie w kulturze młodzieżowej i subkulturach"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать татуировку на коже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "делать татуировку (сленг)"

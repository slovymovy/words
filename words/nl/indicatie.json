{
    "word_family": [
        "indiceren",
        "indicator",
        "indicatief"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e87fe8a9-20a8-33c7-b511-51970a5b8a17",
                    "sense_definition": "Een teken dat op iets wijst of iets aannemelijk maakt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is geen enkele <w>indicatie</w> dat hij bij de inbraak betrokken was.",
                            "target_lang_translations": {
                                "ru": "Нет ни единого <w>указания</w> на то, что он был причастен к взлому."
                            }
                        },
                        {
                            "text": "De donkere wolken zijn een duidelijke <w>indicatie</w> dat het gaat regenen.",
                            "target_lang_translations": {
                                "ru": "Темные тучи — это явный <w>признак</w> того, что пойдет дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwijzing",
                        "aanduiding"
                    ],
                    "common_phrases": [
                        "een duidelijke indicatie",
                        "ter indicatie",
                        "een indicatie geven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак, который указывает на что-то или делает что-то вероятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указание",
                                "target_lang_sense_clarification": "сведение, указывающее на что-либо"
                            },
                            {
                                "target_lang_word": "признак",
                                "target_lang_sense_clarification": "внешнее проявление или примета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1006294-6a15-3915-b059-27f71d604c6f",
                    "sense_definition": "Een medische reden om een bepaalde behandeling of medicijn voor te schrijven.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts schreef het zware medicijn alleen voor op strikte medische <w>indicatie</w>.",
                            "target_lang_translations": {
                                "ru": "Врач выписал это сильное лекарство только по строгим медицинским <w>показаниям</w>."
                            }
                        },
                        {
                            "text": "Zonder de juiste <w>indicatie</w> wordt deze operatie niet door de verzekering vergoed.",
                            "target_lang_translations": {
                                "ru": "Без соответствующих <w>показаний</w> эта операция не покрывается страховкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "medische indicatie",
                        "op indicatie van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинская причина для назначения определенного лечения или лекарства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показание",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a401ba87-9670-3ff1-ba0b-aec69c2c05b8",
                    "sense_definition": "Een grond of reden voor verdenking, vaak in een juridische context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "suspicion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft sterke <w>indicaties</w> dat er sprake is van fraude.",
                            "target_lang_translations": {
                                "ru": "У полиции есть веские <w>основания</w> полагать, что имеет место мошенничество."
                            }
                        },
                        {
                            "text": "Er is een <w>indicatie</w> van betrokkenheid bij de illegale handel.",
                            "target_lang_translations": {
                                "ru": "Есть <w>указание</w> на причастность к незаконной торговле."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwijzing"
                    ],
                    "common_phrases": [
                        "indicaties van fraude"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основание или причина для подозрения, часто в юридическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "причина для подозрений"
                            },
                            {
                                "target_lang_word": "указание",
                                "target_lang_sense_clarification": "свидетельство причастности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
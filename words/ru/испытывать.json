                        "тестировать",
                        "пробовать",
                        "опробовать"
                    ],
                    "common_phrases": [
                        "испытывать на прочность",
                        "испытывать судьбу",
                        "испытывать терпение",
                        "испытывать оружие"
                    ],
                    "target_lang_definitions": {
                        "en": "To check the quality, suitability, or performance of something or someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "test",
                                "target_lang_sense_clarification": "to verify performance or quality"
                            },
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "to strain or push to the limit (e.g., patience)"
                            }
                        ]

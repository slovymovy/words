    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1700d8d8-53b9-3c89-bdeb-fc7fa6acae0d",
                    "sense_definition": "Relating to or performing the act of walking on a tightrope.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The circus performer demonstrated amazing <w>funambulatory</w> skills high above the crowd.",
                            "target_lang_translations": {
                                "ru": "Цирковой артист продемонстрировал удивительные <w>канатоходные</w> навыки высоко над толпой."
                            }
                        },
                        {
                            "text": "Historians described the <w>funambulatory</w> elephants that entertained audiences in ancient Rome.",
                            "target_lang_translations": {
                                "ru": "Историки описывали <w>ходящих по канату</w> слонов, которые развлекали публику в Древнем Риме."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к хождению по канату или выполнению этого действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канатоходный",
                                "target_lang_sense_clarification": "относящийся к искусству хождения по канату"
                            },
                            {
                                "target_lang_word": "эквилибристический",
                                "target_lang_sense_clarification": "связанный с удержанием равновесия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed42cb5d-fde7-375f-90aa-7b0eb96b3afc",
                    "sense_definition": "Resembling a tightrope, particularly by being narrow or requiring great balance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hikers moved slowly along the <w>funambulatory</w> ridge of the mountain.",
                            "target_lang_translations": {
                                "ru": "Туристы медленно двигались по <w>узкому, как канат,</w> горному хребту."
                            }
                        },
                        {
                            "text": "Navigating the complex legal system felt like walking a <w>funambulatory</w> path.",
                            "target_lang_translations": {
                                "ru": "Навигация в сложной правовой системе напоминала движение по <w>зыбкому</w> пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "narrow",
                        "precarious"
                    ],
                    "antonyms": [
                        "wide",
                        "stable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий канат, особенно своей узостью или необходимостью удерживать равновесие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узкий",
                                "target_lang_sense_clarification": "очень тесный, требующий балансировки"
                            },
                            {
                                "target_lang_word": "зыбкий",
                                "target_lang_sense_clarification": "неустойчивый, ненадежный"
                            },
                            {
                                "target_lang_word": "рискованный",
                                "target_lang_sense_clarification": "опасный, требующий осторожности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

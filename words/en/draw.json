    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "249881a4-70bf-3219-b0e0-16e1d37f71a4",
                    "sense_definition": "To produce a picture or diagram by making lines and marks, especially with a pen, pencil, or crayon.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "creative_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child sat at the table and <drew> a picture of her family.",
                            "target_lang_translations": {
                                "ru": "Ребёнок сидел за столом и <w>рисова́л</w> картинку своей семьи."
                            }
                        },
                        {
                            "text": "He can <draw> cartoons very well using just a black pen.",
                            "target_lang_translations": {
                                "ru": "Он умеет очень хорошо <w>рисова́ть</w> карикатуры, используя только чёрную ручку."
                            }
                        }
                    ],
                    "synonyms": [
                        "stalemate",
                        "tie"
                    ],
                    "common_phrases": [
                        "draw a picture",
                        "draw a line",
                        "draw a circle",
                        "draw a map"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать изображение или схему, делая линии и отметки, особенно ручкой, карандашом или мелком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисова́ть",
                                "target_lang_sense_clarification": "создавать изображение"
                            },
                            {
                                "target_lang_word": "черти́ть",
                                "target_lang_sense_clarification": "создавать схемы или линии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e1c2670-aa5a-3f7b-8393-f0fd1956c69b",
                    "sense_definition": "To pull or move something or someone in a particular direction, often smoothly or steadily.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <drew> the chair closer to the fire to get warm.",
                            "target_lang_translations": {
                                "ru": "Он <w>придви́нул</w> стул поближе к огню, чтобы согреться."
                            }
                        },
                        {
                            "text": "She <drew> the papers out of her bag and placed them on the desk.",
                            "target_lang_translations": {
                                "ru": "Она <w>доста́ла</w> бумаги из сумки и положила их на стол."
                            }
                        }
                    ],
                    "synonyms": [
                        "stalemate",
                        "tie"
                    ],
                    "common_phrases": [
                        "draw a breath",
                        "draw aside",
                        "draw blood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тянуть или перемещать что-то или кого-то в определенном направлении, часто плавно или равномерно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придвига́ть",
                                "target_lang_sense_clarification": "приближать к себе"
                            },
                            {
                                "target_lang_word": "достава́ть",
                                "target_lang_sense_clarification": "извлекать наружу"
                            },
                            {
                                "target_lang_word": "тяну́ть",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a8495b2-6671-3626-84d8-c4e1f3d8e289",
                    "sense_definition": "To finish a game or competition with the score even, so that neither side wins.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two teams <drew> 1-1 in the final match of the season.",
                            "target_lang_translations": {
                                "ru": "Две команды <w>сыгра́ли вничью́</w> 1:1 в финальном матче сезона."
                            }
                        },
                        {
                            "text": "If we <draw> this game, we will still qualify for the next round.",
                            "target_lang_translations": {
                                "ru": "Если мы <w>сыгра́ем вничью́</w> в этой игре, мы всё равно пройдём в следующий раунд."
                            }
                        }
                    ],
                    "synonyms": [
                        "tie"
                    ],
                    "common_phrases": [
                        "draw a match",
                        "draw a game"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завершить игру или соревнование с равным счетом, так что ни одна из сторон не выигрывает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыгра́ть вничью́",
                                "target_lang_sense_clarification": "закончить игру с равным счетом"
                            },
                            {
                                "target_lang_word": "своди́ть вничью́",
                                "target_lang_sense_clarification": "добиваться ничейного результата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81908acd-49ab-39d6-95b9-6c777bbe0bab",
                    "sense_definition": "To pull curtains or blinds either open or closed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <draw> the curtains because it is getting dark outside.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>задёрни</w> шторы, потому что на улице темнеет."
                            }
                        },
                        {
                            "text": "She <drew> the blinds to let the bright morning sunlight into the room.",
                            "target_lang_translations": {
                                "ru": "Она <w>откры́ла</w> жалюзи, чтобы впустить в комнату яркий утренний солнечный свет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "draw the curtains",
                        "draw the blinds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открывать или закрывать шторы или жалюзи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задёргивать",
                                "target_lang_sense_clarification": "закрывать шторы"
                            },
                            {
                                "target_lang_word": "открыва́ть",
                                "target_lang_sense_clarification": "раздвигать шторы или жалюзи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9eb54376-55d5-3c6b-aac7-30f91a5bc3f9",
                    "sense_definition": "To attract attention, interest, or people to a particular place or thing.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attraction_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud noise in the street <drew> everyone's attention immediately.",
                            "target_lang_translations": {
                                "ru": "Громкий шум на улице немедленно <w>привлёк</w> всеобщее внимание."
                            }
                        },
                        {
                            "text": "The summer festival <draws> tourists from all over the country.",
                            "target_lang_translations": {
                                "ru": "Летний фестиваль <w>привлека́ет</w> туристов со всей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "stalemate",
                        "tie"
                    ],
                    "common_phrases": [
                        "draw attention",
                        "draw a crowd",
                        "draw interest",
                        "draw fire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекать внимание, интерес или людей к определенному месту или предмету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлека́ть",
                                "target_lang_sense_clarification": "вызывать интерес или притягивать людей"
                            },
                            {
                                "target_lang_word": "притя́гивать",
                                "target_lang_sense_clarification": "манить к себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf8f4f03-ae52-3ab4-bdeb-c3616735c81c",
                    "sense_definition": "To move steadily in a particular direction or into a specific position (intransitive).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As the train <drew> into the station, the passengers prepared to disembark.",
                            "target_lang_translations": {
                                "ru": "Когда поезд <w>подходи́л</w> к станции, пассажиры приготовились к выходу."
                            }
                        },
                        {
                            "text": "The long winter night finally <drew> to an end.",
                            "target_lang_translations": {
                                "ru": "Длинная зимняя ночь наконец <w>подошла́</w> к концу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "draw near",
                        "draw close",
                        "draw level",
                        "draw away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Равномерно двигаться в определенном направлении или занимать определенное положение (непереходный глагол)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходи́ть",
                                "target_lang_sense_clarification": "приближаться (о транспорте или времени)"
                            },
                            {
                                "target_lang_word": "приближа́ться",
                                "target_lang_sense_clarification": "сокращать расстояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9cee2fd-5821-3461-b0cc-9ba63b3bd1dd",
                    "sense_definition": "To take something out of a container, source, or holster (such as a weapon, money, or a card).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sheriff <drew> his gun when he saw the bandits approaching.",
                            "target_lang_translations": {
                                "ru": "Шериф <w>вы́хватил</w> пистолет, когда увидел приближающихся бандитов."
                            }
                        },
                        {
                            "text": "I need to go to the bank to <draw> some cash for the weekend.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в банк, чтобы <w>снять</w> немного наличных на выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "stalemate",
                        "tie"
                    ],
                    "common_phrases": [
                        "draw a weapon",
                        "draw a sword",
                        "draw money",
                        "draw a card"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставать что-то из контейнера, источника или кобуры (например, оружие, деньги или карту)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достава́ть",
                                "target_lang_sense_clarification": "извлекать предмет"
                            },
                            {
                                "target_lang_word": "выхва́тывать",
                                "target_lang_sense_clarification": "резко доставать (оружие)"
                            },
                            {
                                "target_lang_word": "снима́ть",
                                "target_lang_sense_clarification": "получать деньги со счета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59a6d6fd-1df2-319c-9ea6-0c51b5b19776",
                    "sense_definition": "To derive or infer a conclusion, comparison, or information from a source.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We can <draw> a conclusion from the evidence presented in the report.",
                            "target_lang_translations": {
                                "ru": "Мы можем <w>сде́лать</w> вывод из доказательств, представленных в отчёте."
                            }
                        },
                        {
                            "text": "She <drew> comfort from the fact that she was not alone in her struggle.",
                            "target_lang_translations": {
                                "ru": "Она <w>че́рпала</w> утешение в том факте, что была не одна в своей борьбе."
                            }
                        }
                    ],
                    "synonyms": [
                        "stalemate",
                        "tie"
                    ],
                    "common_phrases": [
                        "draw a conclusion",
                        "draw a lesson",
                        "draw a comparison",
                        "draw inspiration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать вывод, проводить сравнение или получать информацию из источника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "де́лать",
                                "target_lang_sense_clarification": "приходить к заключению (делать вывод)"
                            },
                            {
                                "target_lang_word": "че́рпать",
                                "target_lang_sense_clarification": "получать вдохновение или силы"
                            },
                            {
                                "target_lang_word": "проводи́ть",
                                "target_lang_sense_clarification": "делать сравнение (проводить параллель)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2a5c8bcf-02ee-38fb-ab8f-98918fad60e5",
                    "sense_definition": "The result of a game or contest in which neither side wins; a tie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The football match ended in a <draw> after ninety minutes of play.",
                            "target_lang_translations": {
                                "ru": "Футбольный матч закончился <w>ничьёй</w> после девяноста минут игры."
                            }
                        },
                        {
                            "text": "It was a goalless <draw>, which disappointed the fans.",
                            "target_lang_translations": {
                                "ru": "Это была нулевая <w>ничья́</w>, что разочаровало болельщиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "tie",
                        "stalemate"
                    ],
                    "common_phrases": [
                        "end in a draw",
                        "goalless draw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат игры или состязания, в котором ни одна из сторон не выигрывает; ничья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничья́",
                                "target_lang_sense_clarification": "равный счет в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ac385d5-0fc5-31ca-bcf3-41811e9bd251",
                    "sense_definition": "A competition or process where winners or choices are selected randomly, often by picking tickets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prize <draw> will take place on Saturday afternoon.",
                            "target_lang_translations": {
                                "ru": "<w>Ро́зыгрыш</w> призов состоится в субботу днём."
                            }
                        },
                        {
                            "text": "We entered a lucky <draw> to win a new car.",
                            "target_lang_translations": {
                                "ru": "Мы участвовали в <w>ро́зыгрыше</w>, чтобы выиграть новую машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "stalemate",
                        "tie"
                    ],
                    "common_phrases": [
                        "prize draw",
                        "lucky draw",
                        "lottery draw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соревнование или процесс, в котором победители или варианты выбираются случайным образом, часто путем вытягивания билетов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ро́зыгрыш",
                                "target_lang_sense_clarification": "лотерея или конкурс"
                            },
                            {
                                "target_lang_word": "жеребьёвка",
                                "target_lang_sense_clarification": "распределение по жребию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f072e557-dc2f-33de-8769-3cfd3cd12e39",
                    "sense_definition": "A person or thing that is very popular and attracts a large audience or interest.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attraction_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new museum has been a major tourist <draw> for the city.",
                            "target_lang_translations": {
                                "ru": "Новый музей стал главным <w>це́нтром притяже́ния</w> туристов для города."
                            }
                        },
                        {
                            "text": "She is a big box-office <draw> and her movies always make money.",
                            "target_lang_translations": {
                                "ru": "Она — <w>звезда́ ка́ссовых сбо́ров</w>, и её фильмы всегда приносят прибыль."
                            }
                        }
                    ],
                    "synonyms": [
                        "stalemate",
                        "tie"
                    ],
                    "common_phrases": [
                        "big draw",
                        "main draw",
                        "tourist draw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, которая очень популярна и привлекает большую аудиторию или интерес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "центр притяже́ния",
                                "target_lang_sense_clarification": "популярное место или объект"
                            },
                            {
                                "target_lang_word": "гвоздь програ́ммы",
                                "target_lang_sense_clarification": "главное событие или участник"
                            },
                            {
                                "target_lang_word": "прима́нка",
                                "target_lang_sense_clarification": "то, что привлекает (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2df5fc95-a3b0-32b6-8d75-775a51947380",
                    "sense_definition": "A dry stream bed or gully that drains surface water only during heavy rain.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cattle wandered up the shallow <draw> looking for shade.",
                            "target_lang_translations": {
                                "ru": "Скот брёл вверх по неглубокому <w>овра́гу</w> в поисках тени."
                            }
                        },
                        {
                            "text": "Flash floods can occur in the <draw> after a sudden storm.",
                            "target_lang_translations": {
                                "ru": "Внезапные наводнения могут происходить в <w>овра́ге</w> после неожиданного шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "arroyo",
                        "wadi",
                        "dry creek"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сухое русло ручья или овраг, по которому вода течет только во время сильных дождей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овра́г",
                                "target_lang_sense_clarification": "глубокая впадина"
                            },
                            {
                                "target_lang_word": "лощи́на",
                                "target_lang_sense_clarification": "долина с пологими склонами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "993830bf-cf39-30e0-8505-124aa481be37",
                    "sense_definition": "Cannabis.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent all his money on <draw> instead of food.",
                            "target_lang_translations": {
                                "ru": "Он потратил все свои деньги на <w>траву́</w> вместо еды."
                            }
                        },
                        {
                            "text": "They were smoking <draw> behind the building.",
                            "target_lang_translations": {
                                "ru": "Они курили <w>траву́</w> за зданием."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Каннабис, марихуана (слэнг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трава́",
                                "target_lang_sense_clarification": "слэнг"
                            },
                            {
                                "target_lang_word": "дурь",
                                "target_lang_sense_clarification": "слэнг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

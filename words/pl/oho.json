            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "401ac1d3-8596-3a9b-ba52-fec4af674ac8",
                    "sense_definition": "Słowo wyrażające zdziwienie, zaskoczenie, zachwyt lub uznanie dla kogoś lub czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oho</w>, widzę, że kupiłeś sobie całkiem nowy samochód!",
                            "target_lang_translations": {
                                "ru": "<w>Ого</w>, я вижу, ты купил себе совершенно новую машину!"
                            }
                        },
                        {
                            "text": "<w>Oho</w>, to naprawdę wspaniały wynik, gratuluję ci sukcesu.",
                            "target_lang_translations": {
                                "ru": "<w>Ого</w>, это действительно отличный результат, поздравляю с успехом."
                            }
                        }
                    ],
                    "synonyms": [
                        "ho, ho!",
                        "oho ho!"
                    ],
                    "common_phrases": [
                        "oho, zobacz kto to",
                        "oho, ale niespodzianka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее удивление, восхищение или признание по отношению к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ого",
                                "target_lang_sense_clarification": "выражение удивления"
                            },
                            {
                                "target_lang_word": "о-го-го",
                                "target_lang_sense_clarification": "сильное восхищение или удивление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "620d09a9-26b0-355e-8fbe-90177e80d5ab",
                    "sense_definition": "Słowo podkreślające nagłe dostrzeżenie czegoś lub uświadomienie sobie faktu, często ocenianego negatywnie lub zwiastującego kłopoty.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "realization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oho</w>, chyba zaczyna padać deszcz, musimy szybko uciekać.",
                            "target_lang_translations": {
                                "ru": "<w>Опа</w>, кажется, начинается дождь, нам нужно быстро убегать."
                            }
                        },
                        {
                            "text": "<w>Oho</w>, szef idzie korytarzem, lepiej wracajmy do pracy.",
                            "target_lang_translations": {
                                "ru": "<w>Та-ак</w>, шеф идет по коридору, лучше вернемся к работе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oho, zaczyna się",
                        "oho, to niedobrze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, подчеркивающее внезапное обнаружение чего-либо или осознание факта, часто предвещающего неприятности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опа",
                                "target_lang_sense_clarification": "внезапная догадка или неожиданность"
                            },
                            {
                                "target_lang_word": "та-ак",
                                "target_lang_sense_clarification": "настороженность или обнаружение проблемы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

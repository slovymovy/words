    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42db5aac-37f3-31b7-93db-249f8564f33f",
                    "sense_definition": "Zmusić kogoś lub coś do szybkiego przemieszczenia się w określonym kierunku; przepędzić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik musiał <pogonić> krowy, aby wróciły do zagrody przed nocą.",
                            "target_lang_translations": {
                                "ru": "Фермеру пришлось <погнать> коров, чтобы они вернулись в загон до ночи."
                            }
                        },
                        {
                            "text": "Jeśli ten pies znowu wejdzie do ogrodu, będziesz musiał go <pogonić>.",
                            "target_lang_translations": {
                                "ru": "Если эта собака снова зайдет в сад, тебе придется её <прогнать>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pognać",
                        "popędzić"
                    ],
                    "common_phrases": [
                        "pogonić kota",
                        "pogonić towarzystwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то или что-то быстро переместиться в определенном направлении; прогнать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погнать",
                                "target_lang_sense_clarification": "заставить двигаться, понукая (животных, людей)"
                            },
                            {
                                "target_lang_word": "прогнать",
                                "target_lang_sense_clarification": "заставить уйти, выгнать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "382323a8-5a40-3f72-a43f-db44ab25d180",
                    "sense_definition": "Udać się gdzieś bardzo szybko, zwykle biegiem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <pogonić> do sklepu, zanim go zamkną.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <побежать> в магазин, пока он не закрылся."
                            }
                        },
                        {
                            "text": "Chłopiec postanowił <pogonić> na przystanek, żeby zdążyć na autobus.",
                            "target_lang_translations": {
                                "ru": "Мальчик решил <побежать> на остановку, чтобы успеть на автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "pognać"
                    ],
                    "common_phrases": [
                        "pogonić do domu",
                        "pogonić na autobus"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Standard Polish prefers 'pobiec' in this context."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправиться куда-то очень быстро, обычно бегом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побежать",
                                "target_lang_sense_clarification": "начать бежать куда-либо"
                            },
                            {
                                "target_lang_word": "погнать",
                                "target_lang_sense_clarification": "разг. быстро отправиться, помчаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f532062-321a-35fa-8c0b-12352a34cf8a",
                    "sense_definition": "Wywrzeć presję na kogoś, aby szybciej wykonał jakąś czynność lub zadanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szef musiał nas trochę <pogonić>, żebyśmy zdążyli z projektem na czas.",
                            "target_lang_translations": {
                                "ru": "Начальнику пришлось нас немного <поторопить>, чтобы мы успели с проектом вовремя."
                            }
                        },
                        {
                            "text": "Mama ciągle musi mnie <pogonić> do sprzątania mojego pokoju.",
                            "target_lang_translations": {
                                "ru": "Маме постоянно приходится меня <подгонять> с уборкой моей комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "przynaglić",
                        "popędzić"
                    ],
                    "common_phrases": [
                        "pogonić do roboty",
                        "pogonić do nauki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказать давление на кого-то, чтобы он быстрее выполнил действие или задачу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поторопить",
                                "target_lang_sense_clarification": "побудить делать что-либо быстрее"
                            },
                            {
                                "target_lang_word": "подгонять",
                                "target_lang_sense_clarification": "разг. заставлять спешить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c60fe8e4-123c-35aa-bb89-b476c579a08b",
                    "sense_definition": "Zacząć biec za kimś lub za czymś, próbując to złapać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_chase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant bez wahania postanowił <pogonić> za uciekającym złodziejem.",
                            "target_lang_translations": {
                                "ru": "Полицейский без колебаний решил <погнаться> за убегающим вором."
                            }
                        },
                        {
                            "text": "Pies zerwał się ze smyczy i chciał <pogonić> za kotem sąsiada.",
                            "target_lang_translations": {
                                "ru": "Собака сорвалась с поводка и хотела <погнаться> за соседским котом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pogonić za kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побежать за кем-то или чем-то, пытаясь поймать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погнаться",
                                "target_lang_sense_clarification": "начать преследовать кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86bf64a4-f38e-39fd-b7b1-71dabbc29eed",
                    "sense_definition": "Sprzedać coś, często z zyskiem lub w celu szybkiego pozbycia się towaru.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "trade_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Udało mu się <pogonić> stary samochód za całkiem niezłą cenę.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <загнать> старую машину по вполне неплохой цене."
                            }
                        },
                        {
                            "text": "Handlarz chciał szybko <pogonić> towar, zanim zmienią się przepisy.",
                            "target_lang_translations": {
                                "ru": "Торговец хотел быстро <сплавить> товар, пока не изменились правила."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pogonić towar"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Продать что-то, часто с выгодой или с целью быстро избавиться от товара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загнать",
                                "target_lang_sense_clarification": "слэнг: продать, сбыть с рук"
                            },
                            {
                                "target_lang_word": "сплавить",
                                "target_lang_sense_clarification": "разг. избавиться, продав или отдав"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
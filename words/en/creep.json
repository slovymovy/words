    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "daa379bf-2253-3fd3-9ebe-cb68e5e7ef2c",
                    "sense_definition": "To move slowly, quietly, and carefully, usually to avoid being noticed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_stealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>creep</w> past the security guard without being seen."
                        },
                        {
                            "text": "She <w>crept</w> up the stairs late at night to avoid waking her parents."
                        }
                    ],
                    "common_phrases": [
                        "creep up on",
                        "creep into",
                        "creep out"
                    ]
                },
                {
                    "sense_id": "88b093fb-fe3a-362f-aeb7-d8ce1184a4fe",
                    "sense_definition": "To move with the body close to the ground or on hands and knees.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby began to <w>creep</w> across the rug towards the toys."
                        },
                        {
                            "text": "Lizards and snakes <w>crept</w> over the warm rocks in the sun."
                        }
                    ],
                    "synonyms": [
                        "crawl"
                    ]
                },
                {
                    "sense_id": "783543e2-0c44-3940-8d6d-4f2e9c5406d5",
                    "sense_definition": "To change, increase, or develop very slowly and gradually.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gradual_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gas prices have been <w>creeping</w> up steadily all year long."
                        },
                        {
                            "text": "Old age seems to <w>creep</w> upon us before we even realize it."
                        }
                    ],
                    "common_phrases": [
                        "creep up"
                    ]
                },
                {
                    "sense_id": "282038cc-cb64-325f-9142-86abea0957ef",
                    "sense_definition": "To cause a sensation of fear or disgust, feeling as if insects are crawling on the skin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sight of the accident made my flesh <w>creep</w> with horror."
                        },
                        {
                            "text": "It makes my skin <w>creep</w> just thinking about that haunted house."
                        }
                    ],
                    "common_phrases": [
                        "make one's flesh creep"
                    ]
                },
                {
                    "sense_id": "94712c22-13e9-3315-bd89-ac91c1a4b17a",
                    "sense_definition": "To secretly be unfaithful to a romantic partner.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "infidelity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was caught <w>creeping</w> with his ex-girlfriend while his wife was away."
                        },
                        {
                            "text": "If you are <w>creeping</w> around, you will eventually get caught."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ]
                }
            ]
        },
        {

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cda51423-2bf3-3eec-b75d-654cdd4758b6",
                    "sense_definition": "De verzameling voorwerpen, gereedschappen en benodigdheden die nodig zijn om een bepaalde taak uit te voeren of op reis te gaan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>uitrusting</w> van de expeditie was niet voldoende voor de zware tocht.",
                            "target_lang_translations": {
                                "ru": "<w>Снаряжение</w> экспедиции было недостаточным для тяжелого похода."
                            }
                        },
                        {
                            "text": "Voor onze kampeervakantie hebben we een complete <w>uitrusting</w> gekocht.",
                            "target_lang_translations": {
                                "ru": "Для нашего отпуска в кемпинге мы купили полное <w>снаряжение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toerusting",
                        "bepakking"
                    ],
                    "common_phrases": [
                        "militaire uitrusting",
                        "technische uitrusting",
                        "nieuwe uitrusting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор предметов, инструментов и принадлежностей, необходимых для выполнения определенной задачи или для путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снаряжение",
                                "target_lang_sense_clarification": "для походов, экспедиций, военных целей"
                            },
                            {
                                "target_lang_word": "оснащение",
                                "target_lang_sense_clarification": "техническое обеспечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f715561-9137-3e2b-ae18-e4e2d3c67cd8",
                    "sense_definition": "Speciale kleding en beschermende middelen die men draagt tijdens sport of gevaarlijk werk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protective_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ijshockeyspeler trok zijn beschermende <w>uitrusting</w> aan in de kleedkamer.",

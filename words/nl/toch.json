{
    "word_family": [],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0a70764f-7832-37a8-b71c-a01bdb56c8dd",
                    "sense_definition": "Wordt gebruikt in een vraagzin om bevestiging of instemming van de luisteraar te zoeken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "confirmation_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jij komt vanavond ook naar het feest, <toch>?",
                            "target_lang_translations": {
                                "en": "You're coming to the party tonight too, <right>?",
                                "ru": "Ты тоже придешь сегодня вечером на вечеринку, <не так ли>?"
                            }
                        },
                        {
                            "text": "We hebben afgesproken bij het station, <toch>?",
                            "target_lang_translations": {
                                "en": "We agreed to meet at the station, <didn't we>?",
                                "ru": "Мы договорились встретиться на вокзале, <не так ли>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nietwaar",
                        "hè"
                    ],
                    "common_phrases": [
                        "of toch niet"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in a question to seek confirmation or agreement from the listener.",
                        "ru": "Используется в вопросительном предложении для получения подтверждения или согласия слушателя."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right?",
                                "target_lang_sense_clarification": "universal tag question"
                            },
                            {
                                "target_lang_word": "isn't it?",
                                "target_lang_sense_clarification": "tag question depending on context"
                            },
                            {
                                "target_lang_word": "don't you?",
                                "target_lang_sense_clarification": "tag question depending on context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "вопросительная конструкция в конце предложения"
                            },
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "разговорный вариант запроса подтверждения"
                            },
                            {
                                "target_lang_word": "ведь",
                                "target_lang_sense_clarification": "частица, подразумевающая ожидание согласия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7d1661c-11a1-3b89-b7b2-274d7ec33aaf",
                    "sense_definition": "Duidt een tegenstelling aan ten opzichte van wat eerder gezegd of verwacht is; ondanks dat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regende heel hard, maar hij ging <toch> naar buiten.",
                            "target_lang_translations": {
                                "en": "It was raining very hard, but he went outside <anyway>.",
                                "ru": "Шел сильный дождь, но он <всё-таки> вышел на улицу."
                            }
                        },
                        {
                            "text": "Ik heb geen honger, maar ik eet <toch> een appel.",
                            "target_lang_translations": {
                                "en": "I'm not hungry, but I'm eating an apple <still>.",
                                "ru": "Я не голоден, но я <всё же> съем яблоко."
                            }
                        }
                    ],
                    "synonyms": [
                        "desondanks",
                        "evenwel",
                        "desalniettemin",
                        "echter"
                    ],
                    "common_phrases": [
                        "maar toch",
                        "en toch"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a contrast to what was previously said or expected; despite that.",
                        "ru": "Указывает на противопоставление тому, что было сказано или ожидалось ранее; несмотря на это."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "despite the previous statement"
                            },
                            {
                                "target_lang_word": "anyway",
                                "target_lang_sense_clarification": "regardless of the situation"
                            },
                            {
                                "target_lang_word": "nevertheless",
                                "target_lang_sense_clarification": "formal contrast"
                            },
                            {
                                "target_lang_word": "yet",
                                "target_lang_sense_clarification": "conjunction indicating contrast"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "всё-таки",
                                "target_lang_sense_clarification": "указывает на действие вопреки обстоятельствам"
                            },
                            {
                                "target_lang_word": "всё же",
                                "target_lang_sense_clarification": "тем не менее"
                            },
                            {
                                "target_lang_word": "тем не менее",
                                "target_lang_sense_clarification": "формальное выражение уступки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96541f3c-c027-3a13-8ef7-31600d1cbbde",
                    "sense_definition": "Wordt gebruikt om een ontkenning krachtig tegen te spreken; geeft aan dat iets wel degelijk het geval is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contradiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je denkt dat ik het niet kan, maar ik doe het <toch>!",
                            "target_lang_translations": {
                                "en": "You think I can't do it, but I'm doing it <anyway>!",
                                "ru": "Ты думаешь, я не могу этого сделать, но я <всё равно> сделаю это!"
                            }
                        },
                        {
                            "text": "Hij zei dat hij niet zou komen, maar hij is er <toch> wel.",
                            "target_lang_translations": {
                                "en": "He said he wouldn't come, but he is here <after all>.",
                                "ru": "Он сказал, что не придет, но он <всё-таки> здесь."
                            }
                        }
                    ],
                    "synonyms": [
                        "jawel",
                        "wel degelijk"
                    ],
                    "common_phrases": [
                        "toch wel"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to strongly contradict a denial; indicates that something is indeed the case.",
                        "ru": "Используется для решительного возражения на отрицание; указывает на то, что что-то действительно имеет место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anyway",
                                "target_lang_sense_clarification": "doing something despite opposition"
                            },
                            {
                                "target_lang_word": "actually",
                                "target_lang_sense_clarification": "emphasizing reality over denial"
                            },
                            {
                                "target_lang_word": "after all",
                                "target_lang_sense_clarification": "contrary to statement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "всё равно",
                                "target_lang_sense_clarification": "независимо от возражений"
                            },
                            {
                                "target_lang_word": "всё-таки",
                                "target_lang_sense_clarification": "настаивание на факте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "beb4a017-6864-3763-bf96-e65d98e37943",
                    "sense_definition": "Geeft aan dat de spreker vindt dat iets duidelijk, logisch of algemeen bekend zou moeten zijn voor de luisteraar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "modal_obviousness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je weet <toch> dat ik niet van kaas houd?",
                            "target_lang_translations": {
                                "en": "You know I don't like cheese, <right>?",
                                "ru": "Ты <же> знаешь, что я не люблю сыр?"
                            }
                        },
                        {
                            "text": "Dat is <toch> logisch als je erover nadenkt!",
                            "target_lang_translations": {
                                "en": "That is logical, <isn't it>, if you think about it!",
                                "ru": "Это <же> логично, если подумать!"
                            }
                        }
                    ],
                    "synonyms": [
                        "immers"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates that the speaker feels something should be clear, logical, or generally known to the listener.",
                        "ru": "Указывает на то, что, по мнению говорящего, что-то должно быть ясно, логично или общеизвестно слушателю."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surely",
                                "target_lang_sense_clarification": "appealing to shared knowledge"
                            },
                            {
                                "target_lang_word": "right?",
                                "target_lang_sense_clarification": "checking for agreement on obvious facts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "частица для усиления очевидности"
                            },
                            {
                                "target_lang_word": "ведь",
                                "target_lang_sense_clarification": "ссылка на известный факт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a9b6314-3891-3529-9397-d6640dba438b",
                    "sense_definition": "Geeft aan dat iets uiteindelijk gebeurt of waar blijkt te zijn, vaak ondanks eerdere twijfel, oponthoud of tegenverwachting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "outcome_surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lang twijfelen hebben ze <toch> besloten om te verhuizen.",
                            "target_lang_translations": {
                                "en": "After hesitating for a long time, they decided to move <after all>.",
                                "ru": "После долгих сомнений они <всё-таки> решили переехать."
                            }
                        },
                        {
                            "text": "Ach, je bent <toch> gekomen ondanks het slechte weer!",
                            "target_lang_translations": {
                                "en": "Oh, you came <after all> despite the bad weather!",
                                "ru": "Ах, ты <всё-таки> пришел, несмотря на плохую погоду!"
                            }
                        }
                    ],
                    "synonyms": [
                        "uiteindelijk",
                        "alsnog",
                        "ten slotte"
                    ],
                    "common_phrases": [
                        "dan toch"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates that something eventually happens or turns out to be true, often despite earlier doubt, delay, or contrary expectations.",
                        "ru": "Указывает на то, что что-то в конечном итоге происходит или оказывается правдой, часто вопреки прежним сомнениям."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "after all",
                                "target_lang_sense_clarification": "outcome despite expectations"
                            },
                            {
                                "target_lang_word": "in the end",
                                "target_lang_sense_clarification": "final outcome"
                            },
                            {
                                "target_lang_word": "eventually",
                                "target_lang_sense_clarification": "after a period of doubt"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "всё-таки",
                                "target_lang_sense_clarification": "в конечном счете"
                            },
                            {
                                "target_lang_word": "в конце концов",
                                "target_lang_sense_clarification": "по прошествии времени или сомнений"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "colorful",
        "colorless",
        "coloring",
        "coloration",
        "discolor",
        "discoloration",
        "multicolor",
        "colorist"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a872055-a93b-389a-83cf-97d8b611e587",
                    "sense_definition": "A particular set of visible spectral compositions, perceived or named as a class, such as red, blue, or yellow.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What <w>color</w> are your eyes?",
                            "target_lang_translations": {
                                "ru": "Какого <w>цвета</w> твои глаза?"
                            }
                        },
                        {
                            "text": "She loves bright <w>colors</w> like yellow and orange.",
                            "target_lang_translations": {
                                "ru": "Она любит яркие <w>цвета</w>, такие как желтый и оранжевый."
                            }
                        }
                    ],
                    "synonyms": [
                        "hue",
                        "shade",
                        "tint"
                    ],
                    "common_phrases": [
                        "primary colors",
                        "bright colors",
                        "dark colors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спектральный состав видимого света, воспринимаемый глазом (например, красный, синий или желтый)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "оттенок",
                                "target_lang_sense_clarification": "разновидность цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59922457-fe31-3858-ac75-8437a519bf46",
                    "sense_definition": "The quality of having hue as opposed to black, white, and gray.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most people dream in <w>color</w> rather than black and white.",
                            "target_lang_translations": {
                                "ru": "Большинство людей видят сны в <w>цвете</w>, а не черно-белые."
                            }
                        },
                        {
                            "text": "The photograph captures the vibrant <w>color</w> of the landscape.",
                            "target_lang_translations": {
                                "ru": "Фотография передает яркий <w>цвет</w> пейзажа."
                            }
                        }
                    ],
                    "synonyms": [
                        "hue"
                    ],
                    "common_phrases": [
                        "in color",
                        "full color"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство иметь окраску, в отличие от черно-белого изображения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "наличие красок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86864e68-368a-3d79-9f48-422265c079dc",
                    "sense_definition": "Human skin tone, especially as an indicator of race or ethnicity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "skin_complexion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People of every <w>color</w> and creed live in this city.",
                            "target_lang_translations": {
                                "ru": "Люди любого <w>цвета кожи</w> и вероисповедания живут в этом городе."
                            }
                        },
                        {
                            "text": "Discrimination based on <w>color</w> is illegal in many countries.",
                            "target_lang_translations": {
                                "ru": "Дискриминация по <w>цвету кожи</w> незаконна во многих странах."
                            }
                        }
                    ],
                    "synonyms": [
                        "complexion",
                        "race",
                        "ethnicity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оттенок кожи человека, особенно как показатель расовой или этнической принадлежности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет кожи",
                                "target_lang_sense_clarification": "расовая принадлежность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d20184c3-8cc3-39c0-82ba-3da70b056374",
                    "sense_definition": "A substance, such as a paint or dye, used to give color to something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pigment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist mixed the <w>colors</w> on her palette.",
                            "target_lang_translations": {
                                "ru": "Художница смешала <w>краски</w> на своей палитре."
                            }
                        },
                        {
                            "text": "He bought a new set of water <w>colors</w> for the class.",
                            "target_lang_translations": {
                                "ru": "Он купил новый набор акварельных <w>красок</w> для занятий."
                            }
                        }
                    ],
                    "synonyms": [
                        "paint",
                        "dye",
                        "stain",
                        "tincture"
                    ],
                    "common_phrases": [
                        "water colors",
                        "oil colors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, такое как краска или пигмент, используемое для придания цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "вещество"
                            },
                            {
                                "target_lang_word": "колер",
                                "target_lang_sense_clarification": "специальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "166651ab-fbc1-3efa-b991-6e09b9432b19",
                    "sense_definition": "A flushed appearance of blood in the face; redness of complexion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "skin_complexion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fresh air brought some <w>color</w> back to her cheeks.",
                            "target_lang_translations": {
                                "ru": "Свежий воздух вернул <w>румянец</w> на её щеки."
                            }
                        },
                        {
                            "text": "She had high <w>color</w> after running up the stairs.",
                            "target_lang_translations": {
                                "ru": "У неё был яркий <w>румянец</w> после того, как она взбежала по лестнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "complexion",
                        "blush"
                    ],
                    "common_phrases": [
                        "high color"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покраснение лица из-за прилива крови; здоровый цвет лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "румянец",
                                "target_lang_sense_clarification": "на щеках"
                            },
                            {
                                "target_lang_word": "цвет лица",
                                "target_lang_sense_clarification": "оттенок кожи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23cf5896-d8af-3728-ae96-c2da7f1fda3f",
                    "sense_definition": "Richness of expression, detail, or flavour that is likely to generate interest or enjoyment.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "influence_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The reporter added some local <w>color</w> to the story to make it more engaging.",
                            "target_lang_translations": {
                                "ru": "Репортер добавил немного местного <w>колорита</w> в историю, чтобы сделать её более увлекательной."
                            }
                        },
                        {
                            "text": "He provided <w>color</w> commentary for the football match.",
                            "target_lang_translations": {
                                "ru": "Он обеспечивал <w>живой</w> комментарий футбольного матча."
                            }
                        }
                    ],
                    "common_phrases": [
                        "local color",
                        "color commentary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Богатство выражения, деталей или атмосферы, вызывающее интерес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колорит",
                                "target_lang_sense_clarification": "атмосфера, своеобразие"
                            },
                            {
                                "target_lang_word": "живость",
                                "target_lang_sense_clarification": "яркость описания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4a4f852-f27d-3390-baaa-aef9a6bdfda1",
                    "sense_definition": "A standard, flag, or insignia of a nation or team.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers saluted the <w>colors</w> as they marched past.",
                            "target_lang_translations": {
                                "ru": "Солдаты отдали честь <w>знамени</w>, проходя мимо."
                            }
                        },
                        {
                            "text": "The regiment defended its <w>colors</w> bravely during the battle.",
                            "target_lang_translations": {
                                "ru": "Полк отважно защищал свои <w>знамена</w> во время битвы."
                            }
                        }
                    ],
                    "synonyms": [
                        "banner",
                        "standard"
                    ],
                    "common_phrases": [
                        "flying colors"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Штандарт, флаг или эмблема страны или воинского подразделения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знамя",
                                "target_lang_sense_clarification": "символ полка или страны"
                            },
                            {
                                "target_lang_word": "флаг",
                                "target_lang_sense_clarification": "полотнище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b6754fc-42ed-343d-8ec4-258a13b8a796",
                    "sense_definition": "An appearance of right or authority, often used in legal contexts.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_pretext",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He acted under <w>color</w> of law to enforce the new regulations.",
                            "target_lang_translations": {
                                "ru": "Он действовал под <w>видом</w> закона, чтобы обеспечить соблюдение новых правил."
                            }
                        },
                        {
                            "text": "They seized the property under the <w>color</w> of an old statute.",
                            "target_lang_translations": {
                                "ru": "Они захватили собственность под <w>предлогом</w> старого закона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "under color of"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "legal or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимость права или полномочий (юридический контекст)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "в выражении «под видом»"
                            },
                            {
                                "target_lang_word": "предлог",
                                "target_lang_sense_clarification": "формальное основание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd0f395b-c5f7-3032-8b3e-ab93489fadc2",
                    "sense_definition": "To apply colors to a surface, for example with markers, paint, or dye.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pigment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children love to <w>color</w> pictures of animals.",
                            "target_lang_translations": {
                                "ru": "Дети любят <w>раскрашивать</w> картинки с животными."
                            }
                        },
                        {
                            "text": "We decided to <w>color</w> the walls blue.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>покрасить</w> стены в синий цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "paint",
                        "dye",
                        "stain",
                        "tint",
                        "color in"
                    ],
                    "antonyms": [
                        "decolor"
                    ],
                    "common_phrases": [
                        "color in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить краску на поверхность (фломастерами, красками и т.д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрашивать",
                                "target_lang_sense_clarification": "картинки, рисунки"
                            },
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "стены, поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3df0cf0-1304-3cfc-acd9-7e04177bc42b",
                    "sense_definition": "To become red in the face, usually from embarrassment or shame.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skin_complexion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>colored</w> slightly when he complimented her work.",
                            "target_lang_translations": {
                                "ru": "Она слегка <w>покраснела</w>, когда он похвалил её работу."
                            }
                        },
                        {
                            "text": "His face <w>colored</w> with anger as he read the letter.",
                            "target_lang_translations": {
                                "ru": "Его лицо <w>побагровело</w> от гнева, когда он читал письмо."
                            }
                        }
                    ],
                    "synonyms": [
                        "blush"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краснеть лицом, обычно от смущения или стыда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краснеть",
                                "target_lang_sense_clarification": "меняться в лице"
                            },
                            {
                                "target_lang_word": "заливаться румянцем",
                                "target_lang_sense_clarification": "сильно краснеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a76c90c-011c-3cfa-9c61-640617a5583c",
                    "sense_definition": "To affect or influence something, often in a biased or negative way.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "influence_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do not let your personal feelings <w>color</w> your judgment.",
                            "target_lang_translations": {
                                "ru": "Не позволяй своим личным чувствам <w>влиять</w> на твое суждение."
                            }
                        },
                        {
                            "text": "His bad experience in the city <w>colored</w> his opinion of the entire country.",
                            "target_lang_translations": {
                                "ru": "Его неудачный опыт в городе <w>наложил отпечаток</w> на его мнение о всей стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "affect",
                        "influence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Влиять на что-то, часто предвзятым или негативным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влиять",
                                "target_lang_sense_clarification": "оказывать воздействие"
                            },
                            {
                                "target_lang_word": "окрашивать",
                                "target_lang_sense_clarification": "придавать оттенок (перен.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "59c191b9-b493-3735-8639-1d15ebb62690",
                    "sense_definition": "Conveying color, as opposed to shades of gray.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We watched the game on a large <w>color</w> television.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели игру по большому <w>цветному</w> телевизору."
                            }
                        },
                        {
                            "text": "I prefer <w>color</w> photography because it feels more real.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю <w>цветную</w> фотографию, потому что она кажется более реальной."
                            }
                        }
                    ],
                    "synonyms": [
                        "color television"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передающий цвета, цветной (в отличие от черно-белого)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветной",
                                "target_lang_sense_clarification": "не черно-белый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
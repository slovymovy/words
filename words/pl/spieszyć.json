                        "spieszyć się na autobus",
                        "nie spieszyć się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять действия очень быстро или передвигаться в быстром темпе, обычно из-за нехватки времени (чаще всего используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торопиться",
                                "target_lang_sense_clarification": "делать что-то или идти быстрее обычного"
                            },
                            {
                                "target_lang_word": "спешить",
                                "target_lang_sense_clarification": "стараться успеть, действовать быстро"
                            }
                        ]
                    }
                },

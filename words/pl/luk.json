{
    "word_family": [
        "luka",
        "lukowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "de363ea3-5aca-35bc-ad7c-60dcb3cbd350",
                    "sense_definition": "<w>luka</w> w dopełniaczu liczby mnogiej",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jego wiedzy nie było żadnych <w>luk</w>, znał odpowiedź na każde pytanie.",
                            "target_lang_translations": {
                                "ru": "В его знаниях не было никаких <w>пробелов</w>, он знал ответ на каждый вопрос."
                            }
                        },
                        {
                            "text": "Prawnicy szukali <w>luk</w> w nowych przepisach, aby ominąć podatki.",
                            "target_lang_translations": {
                                "ru": "Юристы искали <w>лазейки</w> в новых правилах, чтобы избежать налогов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа слова «luka» (пробел, лазейка)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробел",
                                "target_lang_sense_clarification": "недостаток, пропуск в знаниях или тексте"
                            },
                            {
                                "target_lang_word": "лазейка",
                                "target_lang_sense_clarification": "уловка, возможность обойти закон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c94db40-8b9f-3c97-8181-46781c22c0ad",
                    "sense_definition": "Wydzielona przestrzeń w samolocie, autobusie lub na statku przeznaczona do przewozu bagażu lub towarów",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "compartment_opening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasażerowie oddali duże walizki, które trafiły do <w>luku</w> bagażowego.",
                            "target_lang_translations": {
                                "ru": "Пассажиры сдали большие чемоданы, которые попали в багажный <w>отсек</w>."
                            }
                        },
                        {
                            "text": "Obsługa lotniska miała problem z zamknięciem <w>luku</w> przed startem.",
                            "target_lang_translations": {
                                "ru": "У персонала аэропорта возникла проблема с закрытием <w>люка</w> перед взлетом."
                            }
                        }
                    ],
                    "synonyms": [
                        "ładownia"
                    ],
                    "common_phrases": [
                        "luk bagażowy",
                        "luk towarowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное пространство в самолете, автобусе или на судне, предназначенное для перевозки багажа или грузов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отсек",
                                "target_lang_sense_clarification": "багажный или грузовой"
                            },
                            {
                                "target_lang_word": "трюм",
                                "target_lang_sense_clarification": "грузовое пространство на корабле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93d61315-a79d-3fed-b6e1-faf0b3f96e12",
                    "sense_definition": "Zamykany otwór w pokładzie statku, w czołgu lub na dachu pojazdu, służący do wchodzenia, wentylacji lub załadunku",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "compartment_opening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marynarz zabezpieczył <w>luk</w> przed nadchodzącym sztormem.",
                            "target_lang_translations": {
                                "ru": "Моряк закрепил <w>люк</w> перед надвигающимся штормом."
                            }
                        },
                        {
                            "text": "Czołgista wyjrzał przez otwarty <w>luk</w> wieżyczki, aby ocenić sytuację.",
                            "target_lang_translations": {
                                "ru": "Танкист выглянул через открытый <w>люк</w> башни, чтобы оценить обстановку."
                            }
                        }
                    ],
                    "synonyms": [
                        "właz"
                    ],
                    "common_phrases": [
                        "luk wejściowy",
                        "luk ewakuacyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Закрывающееся отверстие в палубе судна, в танке или на крыше транспортного средства"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "люк",
                                "target_lang_sense_clarification": "отверстие с крышкой для прохода или вентиляции"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
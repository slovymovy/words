            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8d21a64-8231-3b71-ac87-3eda7542f12e",
                    "sense_definition": "Начальник, руководитель предприятия или отдела (обычно в разговорной речи).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "boss_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой <w>шеф</w> сегодня в очень хорошем настроении.",
                            "target_lang_translations": {
                                "en": "My <w>boss</w> is in a very good mood today."
                            }
                        },
                        {
                            "text": "Секретарь сказала, что <w>шеф</w> сейчас занят и не может принять посетителей.",
                            "target_lang_translations": {
                                "en": "The secretary said that the <w>boss</w> is busy right now and cannot see visitors."
                            }
                        }
                    ],
                    "synonyms": [
                        "босс",
                        "руководитель",
                        "глава",
                        "командир"
                    ],
                    "antonyms": [
                        "подчинённый"
                    ],
                    "common_phrases": [
                        "строгий шеф",
                        "кабинет шефа",
                        "вызвать к шефу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A boss, head of an enterprise or department (usually colloquial)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "colloquial term for a supervisor"
                            },
                            {
                                "target_lang_word": "chief",
                                "target_lang_sense_clarification": "informal term for a leader"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e1bfb8d-ba86-3566-a84b-348019b415ee",
                    "sense_definition": "Главный повар в ресторане или столовой; то же, что <w>шеф-повар</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cook_profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом ресторане работает знаменитый французский <w>шеф</w>.",
                            "target_lang_translations": {
                                "en": "A famous French <w>chef</w> works at this restaurant."
                            }
                        },
                        {
                            "text": "Мы заказали фирменное блюдо от <w>шефа</w> и остались очень довольны.",
                            "target_lang_translations": {
                                "en": "We ordered the <w>chef</w>'s special and were very satisfied."
                            }
                        }
                    ],
                    "synonyms": [
                        "шеф-повар"
                    ],
                    "common_phrases": [
                        "блюдо от шефа",
                        "талантливый шеф",
                        "комплимент от шефа"
                    ],
                    "target_lang_definitions": {
                        "en": "Head cook in a restaurant or canteen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chef",
                                "target_lang_sense_clarification": "professional cook"
                            },
                            {
                                "target_lang_word": "head chef",
                                "target_lang_sense_clarification": "leader of the kitchen staff"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0806be89-b647-3799-90e4-9600ce22c882",
                    "sense_definition": "Дружеское или фамильярное обращение к водителю такси или частному извозчику.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "driver_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Шеф</w>, отвези меня на железнодорожный вокзал, пожалуйста.",
                            "target_lang_translations": {
                                "en": "<w>Chief</w>, take me to the train station, please."
                            }
                        },
                        {
                            "text": "Останови здесь, <w>шеф</w>, я дальше пешком дойду.",
                            "target_lang_translations": {
                                "en": "Stop here, <w>boss</w>, I'll walk the rest of the way."
                            }
                        }
                    ],
                    "synonyms": [
                        "шофёр"
                    ],
                    "common_phrases": [
                        "спасибо шеф",
                        "шеф свободен"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "form of address"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "familiar address"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Friendly or familiar form of address to a taxi driver."
                    },

            "senses": [
                {
                    "sense_id": "5a89fde8-3baa-3d40-b9ba-2142e73bf753",
                    "sense_definition": "dotyczący koguta, należący do koguta",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_attributes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdego ranka mieszkańców wsi budzi głośny <w>koguci</w> śpiew.",
                            "target_lang_translations": {
                                "ru": "Каждое утро жителей деревни будит громкое <w>петушиное</w> пение."
                            }
                        },
                        {
                            "text": "<w>Kogucią</w> główkę wieńczy okazały, czerwony grzebień.",
                            "target_lang_translations": {
                                "ru": "<w>Петушиную</w> головку венчает внушительный красный гребень."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waga kogucia",
                        "koguci grzebień",
                        "koguci śpiew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к петуху, принадлежащий петуху"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петушиный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "петуший",
                                "target_lang_sense_clarification": "вариант, часто о внешних признаках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecc5ce50-bf70-3574-aa61-f14830b9ec23",
                    "sense_definition": "mający cechy przypisywane kogutowi, takie jak zadziorność, dumna postawa lub skłonność do walki",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_attributes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>koguci</w> temperament często wpędzał go w niepotrzebne kłopoty.",
                            "target_lang_translations": {
                                "ru": "Его <w>петушиный</w> темперамент часто втягивал его в ненужные неприятности."
                            }
                        },
                        {
                            "text": "Mężczyzna wszedł do sali pewnym siebie, wręcz <w>kogucim</w> krokiem.",
                            "target_lang_translations": {
                                "ru": "Мужчина вошел в зал уверенным, прямо-таки <w>петушиным</w> шагом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zadziorny",
                        "bitny"
                    ],
                    "antonyms": [
                        "łagodny",
                        "spokojny"
                    ],
                    "common_phrases": [
                        "koguci temperament",
                        "koguci chód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий чертами, приписываемыми петуху: задиристостью, горделивостью или склонностью к дракам"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петушиный",
                                "target_lang_sense_clarification": "перен. задиристый, боевой"
                            }
                        ]
                    }
                }
            ]
        }
    ]

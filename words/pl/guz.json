            "senses": [
                {
                    "sense_id": "e30e8209-5583-3045-bdab-a72fb74ce439",
                    "sense_definition": "Bolesne zgrubienie pod skórą, które powstaje zwykle na skutek uderzenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "swelling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marysia nabiła sobie <guza> na placu zabaw.",
                            "target_lang_translations": {
                                "ru": "Марыся набила себе <шишку> на детской площадке."
                            }
                        },
                        {
                            "text": "Uważaj na niskie drzwi, żebyś nie nabawił się <guza> na czole.",
                            "target_lang_translations": {
                                "ru": "Осторожно, низкая дверь, не набей себе <шишку> на лбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gula",
                        "narośl"
                    ],
                    "common_phrases": [
                        "nabić sobie guza",
                        "szukać guza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезненная припухлость под кожей, возникающая обычно в результате ушиба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шишка",
                                "target_lang_sense_clarification": "от удара"
                            }

    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "1ad84378-1f66-378a-b07f-78c9063c270b",
                    "sense_definition": "Partykuła sygnalizująca, że mówiący traktuje daną informację jako pozorną, niepewną, rzekomą lub niezgodną z prawdą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "doubt_distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek <w>niby</w> poszedł do pracy, ale widziałem go przed chwilą w kawiarni.",
                            "target_lang_translations": {
                                "ru": "Марек <w>якобы</w> пошёл на работу, но я только что видел его в кафе."
                            }
                        },
                        {
                            "text": "To zadanie jest <w>niby</w> proste, a jednak sprawia uczniom wiele trudności.",
                            "target_lang_translations": {
                                "ru": "Это задание <w>вроде бы</w> простое, а всё же вызывает у учеников много трудностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzekomo",
                        "jakoby"
                    ],
                    "common_phrases": [
                        "niby tak",
                        "niby nic",
                        "niby wszystko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, указывающая на то, что говорящий воспринимает информацию как мнимую, недостоверную, предполагаемую или не соответствующую действительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "якобы",
                                "target_lang_sense_clarification": "указывает на недостоверность или чужое утверждение"
                            },
                            {
                                "target_lang_word": "как будто",
                                "target_lang_sense_clarification": "выражает видимость или сомнение"
                            },
                            {
                                "target_lang_word": "вроде",
                                "target_lang_sense_clarification": "разговорное; выражает неуверенность"
                            },
                            {
                                "target_lang_word": "будто бы",
                                "target_lang_sense_clarification": "сомнение в достоверности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94653a4f-1417-3980-b6a5-77ad3f5505fc",
                    "sense_definition": "Partykuła używana w pytaniach i wykrzyknieniach dla wyrażenia zdziwienia, niedowierzania, oburzenia lub ironii.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego <w>niby</w> miałbym ci pożyczać pieniądze po tym, co zrobiłeś?",
                            "target_lang_translations": {
                                "ru": "<w>С какой стати</w> я должен одалживать тебе деньги после того, что ты сделал?"
                            }
                        },
                        {
                            "text": "Twierdzisz, że to <w>niby</w> moja wina, chociaż mnie tam nie było?",
                            "target_lang_translations": {
                                "ru": "Утверждаешь, что это <w>якобы</w> моя вина, хотя меня там не было?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "niby ja",
                        "niby po co",
                        "niby dlaczego"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej do wyrażenia emocji"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, используемая в вопросах и восклицаниях для выражения удивления, недоверия, возмущения или иронии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с какой стати",
                                "target_lang_sense_clarification": "идиома; используется в вопросах типа «niby dlaczego»"
                            },
                            {
                                "target_lang_word": "якобы",
                                "target_lang_sense_clarification": "ироническое подчеркивание недоверия"
                            },
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "усилительная частица в вопросах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "36430d64-2b5e-357d-90d4-526d3bb6c548",
                    "sense_definition": "Spójnik wprowadzający porównanie, wskazujący na podobieństwo do kogoś lub czegoś; znaczenie zbliżone do „jak” lub „niczym”.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poczuł się w tym obcym miejscu bezpiecznie, <w>niby</w> u siebie w domu.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал себя в этом чужом месте безопасно, <w>словно</w> у себя дома."
                            }
                        },
                        {
                            "text": "Jej głos brzmiał cicho i delikatnie, <w>niby</w> szum letniego wiatru.",
                            "target_lang_translations": {
                                "ru": "Её голос звучал тихо и нежно, <w>как</w> шум летнего ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "jak",
                        "niczym"
                    ],
                    "common_phrases": [
                        "chudy niby szczapa",
                        "blady niby ściana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий сравнение; указывает на сходство с кем-либо или чем-либо, близок по значению к «как» или «словно»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "словно",
                                "target_lang_sense_clarification": "книжный/поэтический оттенок сравнения"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "общее сравнение"
                            },
                            {
                                "target_lang_word": "будто",
                                "target_lang_sense_clarification": "сравнительный союз"
                            },
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "указывает на полное сходство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

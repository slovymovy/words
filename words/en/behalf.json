            "senses": [
                {
                    "sense_id": "4e81dcf0-2667-3ba0-aa46-cad07b7c8c3f",
                    "sense_definition": "The position of acting for or representing someone else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am writing this letter on <w>behalf</w> of my manager.",
                            "target_lang_translations": {
                                "ru": "Я пишу это письмо от <w>имени</w> моего руководителя."
                            }
                        },
                        {
                            "text": "The lawyer acted on <w>behalf</w> of the client during the trial.",
                            "target_lang_translations": {
                                "ru": "Адвокат действовал от <w>лица</w> клиента во время судебного разбирательства."
                            }
                        }
                    ],
                    "synonyms": [
                        "stead",
                        "place",
                        "name"
                    ],
                    "common_phrases": [
                        "on behalf of",
                        "on my behalf",
                        "on someone's behalf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие от чьего-либо имени или представление чьих-либо интересов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "имя",
                                "target_lang_sense_clarification": "в выражении \"от имени\""
                            },
                            {
                                "target_lang_word": "лицо",
                                "target_lang_sense_clarification": "в выражении \"от лица\""
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d00481bf-3617-3925-80fc-960dfba2fa2c",
                    "sense_definition": "The interest, benefit, or wellbeing of a person or group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They organized a fundraiser in <w>behalf</w> of the local hospital.",
                            "target_lang_translations": {
                                "ru": "Они организовали сбор средств в <w>пользу</w> местной больницы."
                            }
                        },
                        {
                            "text": "She argued passionately in <w>behalf</w> of the proposed changes.",
                            "target_lang_translations": {
                                "ru": "Она страстно выступала в <w>интересах</w> предложенных изменений."
                            }
                        }
                    ],
                    "synonyms": [
                        "benefit",
                        "interest",
                        "sake",
                        "advantage"

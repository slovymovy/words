    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9517ab8d-499a-3640-94a7-b9f06f766762",
                    "sense_definition": "Eigenaar of beheerder van een café of herberg.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "horeca",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kastelein</w> goot mijn glas opnieuw vol met bier.",
                            "target_lang_translations": {
                                "en": "The <w>publican</w> refilled my glass with beer.",
                                "ru": "<w>Трактирщик</w> снова наполнил мой стакан пивом."
                            }
                        },
                        {
                            "text": "We vroegen de <w>kastelein</w> of er nog een kamer vrij was voor de nacht.",
                            "target_lang_translations": {
                                "en": "We asked the <w>innkeeper</w> if there was still a room available for the night.",
                                "ru": "Мы спросили <w>трактирщика</w>, есть ли свободная комната на ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "kroegbaas",
                        "herbergier",
                        "waard",
                        "barman",
                        "cafébaas",
                        "kroeghouder",
                        "caféhouder"
                    ],
                    "common_phrases": [
                        "de vrolijke kastelein",
                        "bij de kastelein"
                    ],
                    "target_lang_definitions": {
                        "en": "Owner or manager of a pub or inn.",
                        "ru": "Владелец или управляющий кафе, трактиром или постоялым двором."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "innkeeper",
                                "target_lang_sense_clarification": "manager of an inn"
                            },
                            {
                                "target_lang_word": "publican",
                                "target_lang_sense_clarification": "manager of a pub"
                            },
                            {
                                "target_lang_word": "landlord",
                                "target_lang_sense_clarification": "owner of a pub"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "трактирщик",
                                "target_lang_sense_clarification": "владелец трактира или постоялого двора"
                            },
                            {
                                "target_lang_word": "хозяин таверны",
                                "target_lang_sense_clarification": "владелец питейного заведения"
                            },
                            {
                                "target_lang_word": "кабатчик",
                                "target_lang_sense_clarification": "содержатель кабака (устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e238a4ee-6ce2-397e-bda5-6436d43b4440",
                    "sense_definition": "De beheerder of slotvoogd van een kasteel, vaak als plaatsvervanger van de kasteelheer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historisch_beroep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kastelein</w> was verantwoordelijk voor de verdediging van de burcht.",
                            "target_lang_translations": {
                                "en": "The <w>castellan</w> was responsible for the defense of the fortress.",
                                "ru": "<w>Кастелян</w> отвечал за оборону крепости."
                            }
                        },
                        {
                            "text": "In afwezigheid van de graaf nam de <w>kastelein</w> de dagelijkse leiding over het slot op zich.",
                            "target_lang_translations": {
                                "en": "In the absence of the count, the <w>castellan</w> took charge of the daily management of the castle.",
                                "ru": "В отсутствие графа <w>кастелян</w> брал на себя повседневное руководство замком."
                            }
                        }
                    ],
                    "synonyms": [
                        "slotvoogd"
                    ],
                    "common_phrases": [
                        "kastelein van het slot"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historische functie"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The administrator or governor of a castle, often as a deputy to the lord of the castle.",
                        "ru": "Управляющий или смотритель замка, часто замещающий владельца замка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "castellan",
                                "target_lang_sense_clarification": "governor of a castle"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кастелян",
                                "target_lang_sense_clarification": "смотритель замка"
                            },
                            {
                                "target_lang_word": "комендант крепости",
                                "target_lang_sense_clarification": "военный начальник крепости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f0765cd-4058-383f-9d69-a2241edb0c4a",
                    "sense_definition": "Een functionaris die namens de landheer bestuurlijke en rechterlijke taken uitvoerde.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historisch_beroep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hertog stelde een nieuwe <w>kastelein</w> aan om de belastingen in het gebied te innen.",
                            "target_lang_translations": {
                                "en": "The duke appointed a new <w>steward</w> to collect taxes in the area.",
                                "ru": "Герцог назначил нового <w>управителя</w> для сбора налогов в этой области."
                            }
                        },
                        {
                            "text": "Als <w>kastelein</w> sprak hij recht over de geschillen tussen de lokale boeren.",
                            "target_lang_translations": {
                                "en": "As <w>bailiff</w>, he adjudicated disputes between the local farmers.",
                                "ru": "Как <w>управитель</w>, он вершил суд в спорах между местными крестьянами."
                            }
                        }
                    ],
                    "synonyms": [
                        "drost",
                        "rentmeester"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historische functie"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An official who performed administrative and judicial tasks on behalf of the lord.",
                        "ru": "Должностное лицо, выполнявшее административные и судебные функции от имени феодала."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steward",
                                "target_lang_sense_clarification": "administrative official"
                            },
                            {
                                "target_lang_word": "bailiff",
                                "target_lang_sense_clarification": "judicial/administrative official"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "управитель",
                                "target_lang_sense_clarification": "администратор поместья"
                            },
                            {
                                "target_lang_word": "наместник",
                                "target_lang_sense_clarification": "представитель власти"
                            }
                        ]
                    }
                }
            ]
        }

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стояк",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2961934e-963f-3813-8e75-f2ed36044ecd",
                    "sense_definition": "A pejorative term for a white person, often used within the prison system.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoner was warned not to associate with the <w>wood</w> gang.",
                            "target_lang_translations": {
                                "ru": "Заключенного предупредили не связываться с бандой <w>белых</w>."
                            }
                        },
                        {
                            "text": "He took offense when someone called him a <w>wood</w>.",
                            "target_lang_translations": {
                                "ru": "Он обиделся, когда кто-то назвал его <w>белым</w>."
                            }

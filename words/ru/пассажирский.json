    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5cea1084-08cc-350f-ba7f-9e3af98e56f1",
                    "sense_definition": "Предназначенный для перевозки людей (пассажиров), а не грузов; связанный с обслуживанием пассажиров.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На третий путь прибывает скорый <w>пассажирский</w> поезд из Москвы.",
                            "target_lang_translations": {
                                "en": "A fast <w>passenger</w> train from Moscow is arriving on track three."
                            }
                        },
                        {
                            "text": "Крупный <w>пассажирский</w> самолёт готовился к взлёту в международном аэропорту.",
                            "target_lang_translations": {
                                "en": "A large <w>passenger</w> plane was preparing for takeoff at the international airport."
                            }
                        }
                    ],
                    "antonyms": [
                        "грузовой",
                        "товарный"
                    ],
                    "common_phrases": [
                        "пассажирский поезд",
                        "пассажирский транспорт",
                        "пассажирский самолёт",
                        "пассажирский вагон",
                        "пассажирский лифт"
                    ],
                    "target_lang_definitions": {
                        "en": "Designed for transporting people (passengers), rather than cargo; related to serving passengers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passenger",
                                "target_lang_sense_clarification": "adjective describing vehicles or transport systems"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "710bfa60-af2d-33be-83bd-15a3151efa32",
                    "sense_definition": "Принадлежащий пассажиру, свойственный ему или исходящий от него.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "belonging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Носильщик аккуратно погрузил <w>пассажирский</w> багаж в такси.",
                            "target_lang_translations": {
                                "en": "The porter carefully loaded the <w>passenger</w> luggage into the taxi."
                            }
                        },
                        {
                            "text": "В коридоре вагона слышались громкие <w>пассажирские</w> голоса и смех.",
                            "target_lang_translations": {
                                "en": "Loud <w>passenger</w> voices and laughter could be heard in the carriage corridor."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пассажирские вещи",
                        "пассажирский багаж",
                        "пассажирский билет"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to a passenger, characteristic of them, or originating from them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passenger",
                                "target_lang_sense_clarification": "attributive use, relating to passengers"
                            },
                            {
                                "target_lang_word": "passenger's",
                                "target_lang_sense_clarification": "possessive form"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80fa3989-e1b8-3155-aeba-94a3282f8d18",
                    "sense_definition": "Пассажирский поезд (обычно медленный, со всеми остановками, в отличие от скорого).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "transport_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы опоздали на экспресс, поэтому пришлось ехать обычным <w>пассажирским</w>.",
                            "target_lang_translations": {
                                "en": "We missed the express, so we had to take the ordinary <w>passenger train</w>."
                            }
                        },
                        {
                            "text": "Диспетчер объявил, что <w>пассажирский</w> задерживается на десять минут.",
                            "target_lang_translations": {
                                "en": "The dispatcher announced that the <w>passenger train</w> is delayed by ten minutes."
                            }
                        }
                    ],
                    "antonyms": [
                        "товарный",
                        "скорый"
                    ],
                    "common_phrases": [
                        "ехать пассажирским"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "substantivized adjective"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A passenger train (usually a slow one making all stops, unlike an express)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passenger train",
                                "target_lang_sense_clarification": "colloquial short form referring to the train itself"
                            },
                            {
                                "target_lang_word": "stopping train",
                                "target_lang_sense_clarification": "contextual equivalent for a slow train"
                            }
                        ]
                    }
                }
            ]
        }
    ]

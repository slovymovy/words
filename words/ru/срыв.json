    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7bdb5c3c-f255-326a-9370-15a5747803ce",
                    "sense_definition": "Внезапное нарушение, прекращение или неудача в осуществлении какого-либо запланированного действия, процесса или переговоров.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "failure_interruption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за болезни главного актёра произошёл <w>срыв</w> вечернего спектакля.",
                            "target_lang_translations": {
                                "en": "Due to the main actor's illness, there was a <w>cancellation</w> of the evening performance."
                            }
                        },
                        {
                            "text": "Любой <w>срыв</w> сроков поставки товара повлечёт за собой крупные штрафы.",
                            "target_lang_translations": {
                                "en": "Any <w>failure</w> to meet delivery deadlines will entail heavy fines."
                            }
                        }
                    ],
                    "synonyms": [
                        "провал",
                        "нарушение",
                        "неудача"
                    ],
                    "antonyms": [
                        "выполнение",
                        "осуществление",
                        "успех"
                    ],
                    "common_phrases": [
                        "срыв переговоров",
                        "срыв сроков",
                        "срыв плана",
                        "срыв работы"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden failure, disruption, or breakdown of a planned activity, process, or negotiation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disruption",
                                "target_lang_sense_clarification": "of plans or schedules"
                            },
                            {
                                "target_lang_word": "breakdown",
                                "target_lang_sense_clarification": "of negotiations"
                            },
                            {
                                "target_lang_word": "failure",
                                "target_lang_sense_clarification": "to meet deadlines"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f874002-d0fb-3000-82bc-1740c80e6cc2",
                    "sense_definition": "Острое нервное расстройство, потеря психического равновесия или возвращение к вредной привычке после воздержания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "failure_interruption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После месяца напряжённой работы без выходных у менеджера случился нервный <w>срыв</w>.",
                            "target_lang_translations": {
                                "en": "After a month of intense work without days off, the manager had a nervous <w>breakdown</w>."
                            }
                        },
                        {
                            "text": "Врачи делают всё возможное, чтобы предотвратить повторный <w>срыв</w> пациента.",
                            "target_lang_translations": {
                                "en": "Doctors are doing everything possible to prevent a repeated <w>relapse</w> of the patient."
                            }
                        }
                    ],
                    "synonyms": [
                        "расстройство",
                        "припадок",
                        "рецидив"
                    ],
                    "antonyms": [
                        "самоконтроль",
                        "равновесие"
                    ],
                    "common_phrases": [
                        "нервный срыв",
                        "эмоциональный срыв"
                    ],
                    "target_lang_definitions": {
                        "en": "An acute mental disorder, loss of emotional balance, or a relapse into a bad habit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breakdown",
                                "target_lang_sense_clarification": "nervous or emotional"
                            },
                            {
                                "target_lang_word": "relapse",
                                "target_lang_sense_clarification": "return to addiction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f98d046-1765-3a1a-9223-f2e34b44646d",
                    "sense_definition": "Резкое отрывание, снятие чего-либо закреплённого или повреждение резьбы при сильном вращении.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За незаконный <w>срыв</w> пломбы с газового счётчика владельцу грозит штраф.",
                            "target_lang_translations": {
                                "en": "The owner faces a fine for the illegal <w>tearing off</w> of the gas meter seal."
                            }
                        },
                        {
                            "text": "Если крутить гайку слишком сильно, может произойти <w>срыв</w> резьбы.",
                            "target_lang_translations": {
                                "en": "If you turn the nut too hard, <w>stripping</w> of the thread may occur."
                            }
                        }
                    ],
                    "synonyms": [
                        "срывание",
                        "отрывание"
                    ],
                    "common_phrases": [
                        "срыв пломбы",
                        "срыв резьбы"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of forcefully tearing off or removing something fixed, or damaging a screw thread."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tearing off",
                                "target_lang_sense_clarification": "of a seal or poster"
                            },
                            {
                                "target_lang_word": "stripping",
                                "target_lang_sense_clarification": "of a thread"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62986782-19f0-30a4-b1ed-3f7079edc2fb",
                    "sense_definition": "Потеря сцепления с поверхностью (колёс с дорогой, потока воздуха с крылом) или падение при альпинизме.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный водитель сумел предотвратить <w>срыв</w> автомобиля в неуправляемый занос.",
                            "target_lang_translations": {
                                "en": "The experienced driver managed to prevent the car's <w>loss of traction</w> leading to an uncontrolled skid."
                            }
                        },
                        {
                            "text": "Страховочная верёвка спасла жизнь спортсмену, когда произошёл <w>срыв</w> со скалы.",
                            "target_lang_translations": {
                                "en": "The safety rope saved the athlete's life when a <w>fall</w> from the cliff occurred."
                            }
                        }
                    ],
                    "synonyms": [
                        "соскальзывание",
                        "падение"
                    ],
                    "antonyms": [
                        "сцепление"
                    ],
                    "common_phrases": [
                        "срыв в штопор",
                        "срыв потока",
                        "срыв колёс"
                    ],
                    "target_lang_definitions": {
                        "en": "A loss of grip or adhesion to a surface, or a fall while climbing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stall",
                                "target_lang_sense_clarification": "aerodynamics/airflow"
                            },
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "mountaineering"
                            },
                            {
                                "target_lang_word": "loss of traction",
                                "target_lang_sense_clarification": "driving"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
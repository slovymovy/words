                {
                    "sense_id": "8395506c-477a-393b-affe-6a29b295f9d7",
                    "sense_definition": "The largest lake in Africa (Lake Victoria).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_name_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The Nile River begins its long journey at Lake <w>Victoria</w>.",
                            "target_lang_translations": {

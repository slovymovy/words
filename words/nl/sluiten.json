    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5f374d2c-8244-38e3-b7e2-0742e28aa271",
                    "sense_definition": "Iets dichtmaken zodat het niet meer open is, bijvoorbeeld een deur, raam of boek.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je alsjeblieft het raam <w>sluiten</w> omdat het koud is?",
                            "target_lang_translations": {
                                "en": "Would you please <w>close</w> the window because it is cold?",
                                "ru": "Ты не мог бы <w>закрыть</w> окно, пожалуйста, так как холодно?"
                            }
                        },
                        {
                            "text": "Hij <w>sloot</w> de deur zachtjes toen hij de kamer verliet.",
                            "target_lang_translations": {
                                "en": "He <w>closed</w> the door softly when he left the room.",
                                "ru": "Он тихо <w>закрыл</w> дверь, выходя из комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "dichtdoen",
                        "dichtmaken",
                        "toedoen"
                    ],
                    "antonyms": [
                        "openen"
                    ],
                    "common_phrases": [
                        "de deur sluiten",
                        "de ogen sluiten",
                        "ramen en deuren sluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something shut so that it is no longer open, for example a door, window, or book.",
                        "ru": "Закрыть что-либо, чтобы оно перестало быть открытым, например дверь, окно или книгу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "close",
                                "target_lang_sense_clarification": "to make shut"
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "synonym for close"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "физическое действие (несовершенный вид)"
                            },
                            {
                                "target_lang_word": "закрыть",
                                "target_lang_sense_clarification": "физическое действие (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d73ae650-9f4d-3be0-9e91-e54b0f7fc9a7",
                    "sense_definition": "Niet meer toegankelijk zijn voor publiek of stoppen met werken (van een winkel, gebouw of dienst).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "business_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De supermarkt <w>sluit</w> elke avond om acht uur.",
                            "target_lang_translations": {
                                "en": "The supermarket <w>closes</w> every evening at eight o'clock.",
                                "ru": "Супермаркет <w>закрывается</w> каждый вечер в восемь часов."
                            }
                        },
                        {
                            "text": "Het museum is op maandag de hele dag <w>gesloten</w>.",
                            "target_lang_translations": {
                                "en": "The museum is <w>closed</w> all day on Mondays.",
                                "ru": "По понедельникам музей весь день <w>закрыт</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "openen"
                    ],
                    "common_phrases": [
                        "de winkel sluit",
                        "gaan sluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop being open to the public or stop operating (of a shop, building, or service).",
                        "ru": "Перестать быть доступным для публики или прекратить работу (о магазине, здании или службе)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "close",
                                "target_lang_sense_clarification": "of a business or service"
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закрываться",
                                "target_lang_sense_clarification": "о магазинах, учреждениях"
                            },
                            {
                                "target_lang_word": "быть закрытым",
                                "target_lang_sense_clarification": "состояние неработы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09d0f554-7e31-3c46-8e96-e24b407fd4f0",
                    "sense_definition": "Dichtgaan of goed op elkaar passen (van een object).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze oude koffer <w>sluit</w> niet meer goed.",
                            "target_lang_translations": {
                                "en": "This old suitcase doesn't <w>close</w> properly anymore.",
                                "ru": "Этот старый чемодан больше не <w>закрывается</w> плотно."
                            }
                        },
                        {
                            "text": "De wond is na een paar dagen vanzelf weer <w>gesloten</w>.",
                            "target_lang_translations": {
                                "en": "The wound <w>closed</w> up by itself after a few days.",
                                "ru": "Через несколько дней рана сама <w>затянулась</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "goed sluiten",
                        "niet goed sluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "To shut properly or fit together well (of an object or wound).",
                        "ru": "Соединяться, смыкаться или плотно прилегать друг к другу (о предмете или ране)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "close",
                                "target_lang_sense_clarification": "to fit together or heal"
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "to fasten securely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "закрываться",
                                "target_lang_sense_clarification": "о механизме, замке, чемодане"
                            },
                            {
                                "target_lang_word": "затягиваться",
                                "target_lang_sense_clarification": "о ране, заживать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc8efdf1-3332-347f-b724-539d7002466f",
                    "sense_definition": "Een officiële overeenkomst, vriendschap of verbintenis tot stand brengen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twee landen hebben gisteren officieel vrede <w>gesloten</w>.",
                            "target_lang_translations": {
                                "en": "The two countries officially <w>made</w> peace yesterday.",
                                "ru": "Вчера две страны официально <w>заключили</w> мир."
                            }
                        },
                        {
                            "text": "Na lang onderhandelen hebben ze eindelijk een compromis <w>gesloten</w>.",
                            "target_lang_translations": {
                                "en": "After long negotiations, they finally <w>reached</w> a compromise.",
                                "ru": "После долгих переговоров они наконец <w>пришли</w> к компромиссу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een compromis sluiten",
                        "vriendschap sluiten",
                        "een akkoord sluiten",
                        "een huwelijk sluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish an official agreement, friendship, or commitment.",
                        "ru": "Заключить официальное соглашение, установить дружеские отношения или взять на себя обязательство."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conclude",
                                "target_lang_sense_clarification": "an agreement or treaty"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "peace or friends"
                            },
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "a compromise"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заключать",
                                "target_lang_sense_clarification": "договор, мир, брак, сделку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
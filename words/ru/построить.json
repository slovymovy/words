            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e7d5926-5c23-30be-8b45-5b51a83e3aac",
                    "sense_definition": "Возвести здание, сооружение или иную материальную конструкцию.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "creation_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка мечтал <построить> большой деревянный дом для всей семьи.",
                            "target_lang_translations": {
                                "en": "My grandfather dreamed of <building> a large wooden house for the whole family."
                            }
                        },
                        {
                            "text": "Власти города обещали <построить> новый мост через реку к концу года.",
                            "target_lang_translations": {
                                "en": "The city authorities promised to <build> a new bridge across the river by the end of the year."
                            }
                        }
                    ],
                    "synonyms": [
                        "соорудить",
                        "выстроить"
                    ],
                    "antonyms": [
                        "сломать",
                        "разрушить"
                    ],
                    "common_phrases": [
                        "построить дом",
                        "построить здание",
                        "построить мост",
                        "заново построить"
                    ],
                    "target_lang_definitions": {
                        "en": "To erect a building, structure, or other material construction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "build",
                                "target_lang_sense_clarification": "general term for creating structures like houses or bridges"
                            },
                            {
                                "target_lang_word": "construct",
                                "target_lang_sense_clarification": "more formal, often used for large or complex structures"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96e5ca9b-9aa4-3ca3-a111-0056fd35a2be",
                    "sense_definition": "Создать, организовать или спланировать что-либо нематериальное (систему, отношения, график).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ей удалось <построить> успешную карьеру в крупной международной компании.",
                            "target_lang_translations": {
                                "en": "She managed to <build> a successful career in a large international company."
                            }
                        },
                        {
                            "text": "Мы стараемся <построить> доверительные отношения с нашими партнёрами.",
                            "target_lang_translations": {
                                "en": "We are trying to <build> trusting relationships with our partners."
                            }
                        }
                    ],
                    "synonyms": [
                        "сформировать",
                        "организовать"
                    ],
                    "antonyms": [
                        "разрушить"
                    ],
                    "common_phrases": [
                        "построить планы",
                        "построить карьеру",
                        "построить отношения",
                        "построить график"
                    ],
                    "target_lang_definitions": {
                        "en": "To create, organize, or plan something non-material (a system, relationships, a schedule)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "build",
                                "target_lang_sense_clarification": "to develop something abstract over time, e.g., a career or trust"
                            },
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "to set up something firm or permanent, like relationships"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5b43679-13e7-3a15-8eca-a5021ad49d4a",
                    "sense_definition": "Расставить людей (обычно военных) в строй или определённый порядок.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир приказал <построить> солдат на плацу для утренней проверки.",
                            "target_lang_translations": {
                                "en": "The commander ordered the soldiers to <line up> on the parade ground for the morning inspection."
                            }
                        },
                        {
                            "text": "Учитель физкультуры помог <построить> учеников по росту перед началом урока.",
                            "target_lang_translations": {
                                "en": "The PE teacher helped <line up> the students by height before the lesson started."
                            }
                        }
                    ],
                    "synonyms": [

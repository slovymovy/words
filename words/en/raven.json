                },
                {
                    "sense_id": "cae95d38-f91f-38f8-baba-01168c28b50f",
                    "sense_definition": "A name used for various places or communities.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_concept",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They stopped in the small town of <w>Raven</w> during their road trip.",
                            "target_lang_translations": {
                                "ru": "Они остановились в маленьком городке <w>Рэйвен</w> во время своего автопутешествия."
                            }
                        },
                        {
                            "text": "The map shows a community called <w>Raven</w> near the river.",
                            "target_lang_translations": {
                                "ru": "На карте показано поселение под названием <w>Рэйвен</w> у реки."
                            }

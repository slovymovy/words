{
    "word_family": [
        "verwelkomen",
        "onwelkom"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "473c84d3-d8d6-347f-8213-86a393de7dfc",
                    "sense_definition": "Een begroeting die men uitspreekt wanneer iemand ergens arriveert.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Welkom</w> in onze prachtige stad.",
                            "target_lang_translations": {
                                "en": "<w>Welcome</w> to our beautiful city.",
                                "ru": "<w>Добро пожаловать</w> в наш прекрасный город."
                            }
                        },
                        {
                            "text": "Hallo allemaal en <w>welkom</w> bij deze presentatie.",
                            "target_lang_translations": {
                                "en": "Hello everyone and <w>welcome</w> to this presentation.",
                                "ru": "Всем привет и <w>добро пожаловать</w> на эту презентацию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van harte welkom",
                        "welkom thuis",
                        "welkom terug"
                    ],
                    "target_lang_definitions": {
                        "en": "A greeting used when someone arrives at a place.",
                        "ru": "Приветствие, используемое при прибытии кого-либо в какое-то место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "welcome",
                                "target_lang_sense_clarification": "greeting upon arrival"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "добро пожаловать",
                                "target_lang_sense_clarification": "приветствие при прибытии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ad007b06-fd6f-311f-8c85-4dd8389c3e5f",
                    "sense_definition": "Met plezier ontvangen of gewenst; aangenaam omdat het er is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange wandeling was de rust zeer <w>welkom</w>.",
                            "target_lang_translations": {
                                "en": "After the long walk, the rest was very <w>welcome</w>.",
                                "ru": "После долгой прогулки отдых был очень <w>желанным</w>."
                            }
                        },
                        {
                            "text": "Hij is altijd een <w>welkome</w> gast op onze feesten.",
                            "target_lang_translations": {
                                "en": "He is always a <w>welcome</w> guest at our parties.",
                                "ru": "Он всегда <w>желанный</w> гость на наших вечеринках."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewenst",
                        "aangenaam"
                    ],
                    "antonyms": [
                        "onwelkom"
                    ],
                    "common_phrases": [
                        "van harte welkom zijn",
                        "een welkome afwisseling",
                        "niet welkom zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Gladly received, pleasing, or freely permitted.",
                        "ru": "Принятый с радостью или желаемый; приятный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "welcome",
                                "target_lang_sense_clarification": "adjective: pleasing or gladly received"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "желанный",
                                "target_lang_sense_clarification": "прилагательное: ожидаемый с удовольствием"
                            },
                            {
                                "target_lang_word": "приятный",
                                "target_lang_sense_clarification": "доставляющий удовольствие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8211dd8c-6a57-3007-87fb-33adf14fd1eb",
                    "sense_definition": "De handeling van het begroeten of ontvangen van iemand.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe directeur kreeg een warm <w>welkom</w> van het personeel.",
                            "target_lang_translations": {
                                "en": "The new director received a warm <w>welcome</w> from the staff.",
                                "ru": "Новый директор получил тёплый <w>приём</w> от персонала."
                            }
                        },
                        {
                            "text": "Het <w>welkom</w> op het vliegveld was erg emotioneel.",
                            "target_lang_translations": {
                                "en": "The <w>welcome</w> at the airport was very emotional.",
                                "ru": "<w>Встреча</w> в аэропорту была очень эмоциональной."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontvangst",
                        "onthaal",
                        "begroeting"
                    ],
                    "common_phrases": [
                        "een warm welkom",
                        "een hartelijk welkom"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of greeting or receiving someone.",
                        "ru": "Действие по приветствию или встрече кого-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "welcome",
                                "target_lang_sense_clarification": "noun: act of greeting"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "существительное: встреча гостей"
                            },
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "акт приветствия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
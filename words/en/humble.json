    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "92b4e3bf-71f8-34e1-b7b2-202dffa6d634",
                    "sense_definition": "Having a low opinion of oneself; not proud, arrogant, or assuming; modest.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "character_modesty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a <w>humble</w> man who never brags about his achievements.",
                            "target_lang_translations": {
                                "ru": "Он <w>скромный</w> человек, который никогда не хвастается своими достижениями."
                            }
                        },
                        {
                            "text": "In my <w>humble</w> opinion, we should wait for more information before deciding.",
                            "target_lang_translations": {
                                "ru": "По моему <w>скромному</w> мнению, нам следует подождать дополнительной информации, прежде чем принимать решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "modest",
                        "unassuming",
                        "meek",
                        "self-effacing",
                        "diffident"
                    ],
                    "antonyms": [
                        "arrogant",
                        "presumptuous",
                        "smug",
                        "snobby",
                        "immodest"
                    ],
                    "common_phrases": [
                        "in my humble opinion",
                        "stay humble",
                        "be humble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий невысокое мнение о себе; не гордый и не высокомерный; скромный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скромный",
                                "target_lang_sense_clarification": "не хвастливый, сдержанный в оценке своих достоинств"
                            },
                            {
                                "target_lang_word": "смиренный",
                                "target_lang_sense_clarification": "кроткий, лишенный гордыни, часто в духовном контексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46245d54-b7bb-3791-b41a-0cd0c4afae0b",
                    "sense_definition": "Not pretentious or magnificent; unpretending; unassuming.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "object_simplicity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They lived in a <w>humble</w> cottage at the edge of the forest.",
                            "target_lang_translations": {
                                "ru": "Они жили в <w>скромном</w> домике на опушке леса."
                            }
                        },
                        {
                            "text": "Despite his <w>humble</w> beginnings, he rose to become a successful leader.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свое <w>простое</w> происхождение, он стал успешным лидером."
                            }
                        }
                    ],
                    "synonyms": [
                        "unpretentious",
                        "modest",
                        "low",
                        "unassuming"
                    ],
                    "common_phrases": [
                        "humble beginnings",
                        "humble abode",
                        "humble origin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не вычурный и не роскошный; простой; непритязательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скромный",
                                "target_lang_sense_clarification": "простой, небогатый, без излишеств"
                            },
                            {
                                "target_lang_word": "непритязательный",
                                "target_lang_sense_clarification": "не требующий многого, простой на вид"
                            },
                            {
                                "target_lang_word": "простой",
                                "target_lang_sense_clarification": "обычный, низкого происхождения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6dbede45-44c0-31df-89ce-5b7e02646894",
                    "sense_definition": "To make humble or lowly; to make less proud or arrogant; to make meek and submissive.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unexpected defeat <w>humbled</w> the champion and made him train harder.",
                            "target_lang_translations": {
                                "ru": "Неожиданное поражение <w>усмирило</w> чемпиона и заставило его тренироваться усерднее."
                            }
                        },
                        {
                            "text": "I was <w>humbled</w> by the kindness and generosity of the strangers.",
                            "target_lang_translations": {
                                "ru": "Я был <w>смущен</w> добротой и щедростью незнакомцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "abase",
                        "humiliate",
                        "degrade",
                        "lower",
                        "mortify"
                    ],
                    "common_phrases": [
                        "humbled by",
                        "humble someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-либо чувствовать себя менее важным или гордым; сбивать спесь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смирять",
                                "target_lang_sense_clarification": "делать покорным, подавлять чью-то гордость"
                            },
                            {
                                "target_lang_word": "сбивать спесь",
                                "target_lang_sense_clarification": "лишать высокомерия, ставить на место"
                            },
                            {
                                "target_lang_word": "смущать",
                                "target_lang_sense_clarification": "заставлять чувствовать неловкость (например, от чьей-то доброты)"
                            }
                        ]
                    }
                }
            ]
        },

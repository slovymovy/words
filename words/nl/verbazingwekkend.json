            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "029185be-dd4d-374b-8c3d-e190632a2b66",
                    "sense_definition": "Wat verbazing opwekt of buitengewoon is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die circusartiesten lieten <w>verbazingwekkende</w> dingen zien.",
                            "target_lang_translations": {
                                "en": "Those circus artists showed <w>astonishing</w> things.",
                                "ru": "Эти цирковые артисты показывали <w>удивительные</w> вещи."
                            }
                        },
                        {
                            "text": "Het is <w>verbazingwekkend</w> dat hij na al die jaren nog steeds zo fit is.",
                            "target_lang_translations": {
                                "en": "It is <w>amazing</w> that he is still so fit after all these years.",
                                "ru": "<w>Удивительно</w>, что спустя столько лет он всё ещё в такой хорошей форме."
                            }
                        }
                    ],
                    "synonyms": [
                        "wonderbaarlijk",
                        "verbluffend",
                        "spectaculair",
                        "ongelooflijk"
                    ],
                    "antonyms": [
                        "gewoon",
                        "alledaags",
                        "normaal"
                    ],
                    "common_phrases": [
                        "een verbazingwekkend verhaal",
                        "verbazingwekkende resultaten",
                        "een verbazingwekkende ontdekking"
                    ],
                    "target_lang_definitions": {
                        "en": "Causing surprise or wonder; extraordinary.",
                        "ru": "То, что вызывает удивление или является необычайным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "astonishing",
                                "target_lang_sense_clarification": "causing great surprise or wonder"
                            },
                            {
                                "target_lang_word": "amazing",
                                "target_lang_sense_clarification": "causing great surprise or wonder, often positive"
                            },
                            {
                                "target_lang_word": "surprising",
                                "target_lang_sense_clarification": "unexpected"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "удивительный",
                                "target_lang_sense_clarification": "вызывающий удивление"
                            },
                            {
                                "target_lang_word": "поразительный",
                                "target_lang_sense_clarification": "производящий сильное впечатление"
                            },
                            {
                                "target_lang_word": "изумительный",
                                "target_lang_sense_clarification": "восхитительный, необыкновенный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a3b1f127-4368-33a2-b13f-71492c589bed",
                    "sense_definition": "Op een manier die verbazing wekt; in zeer hoge mate.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Verbazingwekkend</w> genoeg vermelden de annalen slechts negen tot zestien slachtoffers.",
                            "target_lang_translations": {
                                "en": "<w>Surprisingly</w> enough, the annals mention only nine to sixteen victims.",
                                "ru": "<w>Удивительно</w>, но летописи упоминают лишь от девяти до шестнадцати жертв."
                            }
                        },
                        {
                            "text": "De nieuwe computer werkt <w>verbazingwekkend</w> snel vergeleken met de oude.",
                            "target_lang_translations": {
                                "en": "The new computer works <w>astonishingly</w> fast compared to the old one.",
                                "ru": "Новый компьютер работает <w>поразительно</w> быстро по сравнению со старым."
                            }
                        }
                    ],
                    "synonyms": [
                        "buitengewoon",
                        "ongelooflijk",
                        "uiterst"
                    ],
                    "common_phrases": [
                        "verbazingwekkend genoeg",
                        "verbazingwekkend goed",
                        "verbazingwekkend snel"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that causes surprise; to a very high degree.",
                        "ru": "Образом, вызывающим удивление; в очень высокой степени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surprisingly",
                                "target_lang_sense_clarification": "used to express that something is unexpected"
                            },
                            {
                                "target_lang_word": "amazingly",
                                "target_lang_sense_clarification": "in a way that causes wonder"
                            },
                            {
                                "target_lang_word": "astonishingly",
                                "target_lang_sense_clarification": "in a way that causes great surprise"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "удивительно",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "поразительно",
                                "target_lang_sense_clarification": "в высшей степени"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            },
                            {
                                "target_lang_word": "лейки",
                                "target_lang_sense_clarification": "для душа"
                            },
                            {
                                "target_lang_word": "стетоскопы",
                                "target_lang_sense_clarification": "медицинские"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdd0e6b1-7967-3e8e-8902-1a8666d8dec9",
                    "sense_definition": "Forma dopełniacza liczby pojedynczej od: <w>słuchawka</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę znaleźć <w>słuchawki</w> od domofonu.",

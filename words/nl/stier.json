            "pos": "noun",
            "senses": [
                {
                    "sense_id": "882a620c-db43-3882-9f38-6b1e62de583d",
                    "sense_definition": "Een mannelijk rund dat niet gecastreerd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer bracht de <w>stier</w> naar een andere wei om te grazen.",
                            "target_lang_translations": {
                                "en": "The farmer brought the <w>bull</w> to another pasture to graze."
                            }
                        },
                        {
                            "text": "Pas op als je door het weiland loopt, want er staat een <w>stier</w>.",
                            "target_lang_translations": {
                                "en": "Watch out when walking through the pasture, because there is a <w>bull</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bul"
                    ],
                    "common_phrases": [
                        "een wilde stier",
                        "jonge stier"
                    ],
                    "target_lang_definitions": {
                        "en": "An uncastrated male bovine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bull",
                                "target_lang_sense_clarification": "male bovine"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "971f18c4-bb91-3a16-a665-0d6e6fcc4cc1",
                    "sense_definition": "Iemand die is geboren in de periode waarin de zon in het sterrenbeeld Stier staat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "astrologie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer is een <w>Stier</w> en hij kan soms erg koppig zijn.",
                            "target_lang_translations": {
                                "en": "My brother is a <w>Taurus</w> and he can sometimes be very stubborn."
                            }
                        },
                        {
                            "text": "Zij leest elke ochtend haar horoscoop omdat ze een echte <w>Stier</w> is.",
                            "target_lang_translations": {
                                "en": "She reads her horoscope every morning because she is a true <w>Taurus</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een echte Stier"
                    ],
                    "target_lang_definitions": {
                        "en": "A person born under the zodiac sign Taurus."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Taurus",
                                "target_lang_sense_clarification": "person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eafbe128-0ebb-310e-8004-5fa4435257c3",
                    "sense_definition": "Het mannetje van bepaalde grote zoogdieren, zoals de walvis, de olifant en de eland.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De enorme <w>stier</w> beschermde de kudde olifanten tegen het naderende gevaar.",
                            "target_lang_translations": {
                                "en": "The huge <w>bull</w> protected the herd of elephants against the approaching danger."
                            }
                        },
                        {
                            "text": "Tijdens de safari zagen we een grote <w>stier</w> bij de rivier drinken.",
                            "target_lang_translations": {
                                "en": "During the safari, we saw a large <w>bull</w> drinking at the river."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The male of certain large mammals, such as whales, elephants, and moose."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bull",
                                "target_lang_sense_clarification": "male of large mammals"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0a491bca-1923-3e4d-9d14-4c7ed19f761a",
                    "sense_definition": "Het teken van de dierenriem waar de zon in staat van ca. 20 april tot ca. 21 mei.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "astrologie",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Mensen die onder het teken <w>Stier</w> geboren zijn, houden vaak van lekker eten.",
                            "target_lang_translations": {
                                "en": "People born under the sign <w>Taurus</w> often love good food."
                            }
                        },
                        {
                            "text": "In de astrologie wordt <w>Stier</w> geassocieerd met het element aarde.",
                            "target_lang_translations": {
                                "en": "In astrology, <w>Taurus</w> is associated with the element earth."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het teken Stier"
                    ],
                    "target_lang_definitions": {
                        "en": "The zodiac sign that the sun occupies from approximately April 20 to May 21."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Taurus",
                                "target_lang_sense_clarification": "zodiac sign"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c95aabf-feea-3bc3-a3e2-74a9855f26ac",
                    "sense_definition": "Een sterrenbeeld van de dierenriem aan de noordelijke hemel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astrologie",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Op een heldere winteravond kun je het sterrenbeeld <w>Stier</w> goed zien.",
                            "target_lang_translations": {
                                "en": "On a clear winter evening, you can see the constellation <w>Taurus</w> clearly."
                            }
                        },
                        {
                            "text": "De helderste ster in <w>Stier</w> is de rode reus Aldebaran.",
                            "target_lang_translations": {
                                "en": "The brightest star in <w>Taurus</w> is the red giant Aldebaran."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sterrenbeeld Stier"
                    ],
                    "target_lang_definitions": {
                        "en": "A constellation of the zodiac in the northern sky."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Taurus",
                                "target_lang_sense_clarification": "constellation"
                            }
                        ]
                    }
                }
            ]
        },

                    }
                },
                {
                    "sense_id": "397bb9a6-8740-3bc2-b0bb-40df14437df9",
                    "sense_definition": "Gwałtowne i trudne wydarzenie w życiu osobistym, społecznym lub w historii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_event",
                    "name_type": "no",
                    "examples": [

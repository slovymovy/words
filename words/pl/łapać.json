    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d6d1435d-8313-3d21-944b-7f63d25809e7",
                    "sense_definition": "Chwytać coś, co jest w ruchu lub spada; brać coś szybko do ręki",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz musi pewnie <łapać> piłkę, aby obronić bramkę.",
                            "target_lang_translations": {
                                "ru": "Вратарь должен уверенно <ловить> мяч, чтобы защитить ворота."
                            }
                        },
                        {
                            "text": "Dziecko biega po łące i <łapie> kolorowe bańki mydlane.",
                            "target_lang_translations": {
                                "ru": "Ребенок бегает по лугу и <ловит> разноцветные мыльные пузыри."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwytać",
                        "ujmować"
                    ],
                    "antonyms": [
                        "puszczać",
                        "upuszczać"
                    ],
                    "common_phrases": [
                        "łapać piłkę",
                        "łapać w lot",
                        "łapać równowagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ловить что-то движущееся или падающее; быстро брать что-то в руки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лови́ть",
                                "target_lang_sense_clarification": "хватать движущийся объект (мяч, насекомое)"
                            },
                            {
                                "target_lang_word": "хвата́ть",
                                "target_lang_sense_clarification": "быстро брать рукой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd0a14ac-d2cd-34ae-bff7-26bb833f2e15",
                    "sense_definition": "Zdążyć wsiąść do środka transportu lub zatrzymać pojazd",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <łapię> autobus do pracy o tej samej godzinie.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <успеваю на> автобус на работу в одно и то же время."
                            }
                        },
                        {
                            "text": "Turystka stała przy ulicy i <łapała> taksówkę na lotnisko.",
                            "target_lang_translations": {
                                "ru": "Туристка стояла у дороги и <ловила> такси в аэропорт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łapać autobus",
                        "łapać taksówkę",
                        "łapać okazję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Успеть сесть в транспорт или остановить машину"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лови́ть",
                                "target_lang_sense_clarification": "останавливать машину (такси, попутку)"
                            },
                            {
                                "target_lang_word": "успева́ть на",
                                "target_lang_sense_clarification": "приходить вовремя к отправлению транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ddbb5d5-35fe-325a-893f-6707344c9790",
                    "sense_definition": "Zarażać się chorobą",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W okresie jesiennym uczniowie często <łapią> przeziębienie.",
                            "target_lang_translations": {
                                "ru": "В осенний период ученики часто <подхватывают> простуду."
                            }
                        },
                        {
                            "text": "Nie chcę <złapać> grypy, więc biorę witaminy.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <подхватить> грипп, поэтому принимаю витамины."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabierać się"
                    ],
                    "common_phrases": [
                        "łapać katar",
                        "łapać grypę",
                        "łapać wirusa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заражаться болезнью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подхва́тывать",
                                "target_lang_sense_clarification": "разг. заражаться (простудой, вирусом)"
                            },
                            {
                                "target_lang_word": "подцепля́ть",
                                "target_lang_sense_clarification": "разг. получать заболевание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "051aecd8-a07a-3398-b494-ee7a89bcc20f",
                    "sense_definition": "Rozumieć coś, pojmować sens wypowiedzi",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On jest bardzo bystry i szybko <łapie> nowe zasady gry.",
                            "target_lang_translations": {
                                "ru": "Он очень сообразительный и быстро <схватывает> новые правила игры."
                            }
                        },
                        {
                            "text": "Przepraszam, ale kompletnie nie <łapię>, o co ci chodzi w tym zadaniu.",
                            "target_lang_translations": {
                                "ru": "Извини, но я совершенно не <улавливаю>, о чем ты говоришь в этом задании."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojmać",
                        "orientować się",
                        "załapywać"
                    ],
                    "common_phrases": [
                        "szybko łapać",
                        "nie łapać żartu",
                        "łapać aluzję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать что-то, улавливать смысл сказанного"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "схва́тывать",
                                "target_lang_sense_clarification": "быстро понимать, усваивать"
                            },
                            {
                                "target_lang_word": "ула́вливать",
                                "target_lang_sense_clarification": "понимать смысл, намек"
                            },
                            {
                                "target_lang_word": "вруба́ться",
                                "target_lang_sense_clarification": "жарг. понимать, разбираться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62cd8b5d-3dec-37b4-9042-63ec27cb55bd",
                    "sense_definition": "Polować na zwierzęta, chwytać je w pułapkę lub na wędkę",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kot uwielbia <łapać> myszy w stodole.",
                            "target_lang_translations": {
                                "ru": "Мой кот обожает <ловить> мышей в сарае."
                            }
                        },
                        {
                            "text": "Wędkarze siedzą nad rzeką i <łapią> ryby na kolację.",
                            "target_lang_translations": {
                                "ru": "Рыбаки сидят у реки и <ловят> рыбу на ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "łowić"
                    ],
                    "antonyms": [
                        "wypuszczać"
                    ],
                    "common_phrases": [
                        "łapać ryby",
                        "łapać myszy",
                        "łapać muchy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охотиться на животных, ловить их в ловушку или на удочку"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лови́ть",
                                "target_lang_sense_clarification": "охотиться, рыбачить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c544c433-1e21-321e-a3c4-c5002dec837d",
                    "sense_definition": "Zatrzymywać kogoś, kto ucieka lub zrobił coś złego; przyłapywać kogoś",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pursuit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja stara się skutecznie <łapać> przestępców w naszej dzielnicy.",
                            "target_lang_translations": {
                                "ru": "Полиция старается эффективно <ловить> преступников в нашем районе."
                            }
                        },
                        {
                            "text": "Nauczyciel <złapał> ucznia na ściąganiu podczas ważnego testu.",
                            "target_lang_translations": {
                                "ru": "Учитель <поймал> ученика на списывании во время важного теста."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojmać",
                        "dopadać",
                        "przyłapywać się"
                    ],
                    "common_phrases": [
                        "łapać złodzieja",
                        "łapać na kłamstwie",
                        "łapać za rękę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задерживать кого-то убегающего или уличать в проступке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лови́ть",
                                "target_lang_sense_clarification": "преследовать и задерживать"
                            },
                            {
                                "target_lang_word": "пойма́ть",
                                "target_lang_sense_clarification": "схватить, уличить (на месте преступления)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e9e12b0-9d11-362f-aa34-9d7524775b5b",
                    "sense_definition": "Odbierać sygnał (np. radiowy, telefoniczny) lub wykorzystywać krótką chwilę",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary telefon słabo <łapie> zasięg w górach.",
                            "target_lang_translations": {
                                "ru": "Мой старый телефон плохо <ловит> сеть в горах."
                            }
                        },
                        {
                            "text": "Po szybkim biegu muszę chwilę odpocząć i <złapać> oddech.",
                            "target_lang_translations": {
                                "ru": "После быстрого бега мне нужно немного отдохнуть и <перевести> дух."
                            }
                        }
                    ],
                    "antonyms": [
                        "gubić"
                    ],
                    "common_phrases": [
                        "łapać zasięg",
                        "łapać oddech",
                        "łapać słońce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать сигнал (радио, телефон) или использовать момент"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лови́ть",
                                "target_lang_sense_clarification": "принимать сигнал, сеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6b5b915-6664-3701-9a6a-afaf44fba2ee",
                    "sense_definition": "Chwytać się czegoś rękami, aby nie upaść lub utrzymać równowagę",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasażerowie muszą <łapać się> uchwytów, gdy tramwaj hamuje.",
                            "target_lang_translations": {
                                "ru": "Пассажиры должны <держаться> за поручни, когда трамвай тормозит."
                            }
                        },
                        {
                            "text": "Kiedy usłyszał tę straszną cenę, aż <złapał się> za głowę.",
                            "target_lang_translations": {
                                "ru": "Когда он услышал эту страшную цену, то аж <схватился> за голову."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwytać się"
                    ],
                    "common_phrases": [
                        "łapać się poręczy",
                        "łapać się za głowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держаться за что-то руками, чтобы не упасть"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвата́ться",
                                "target_lang_sense_clarification": "резко браться за что-то"
                            },
                            {
                                "target_lang_word": "держа́ться",
                                "target_lang_sense_clarification": "сохранять равновесие, держась за опору"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc466226-6860-36bc-9b7b-617508dffacd",
                    "sense_definition": "Uświadamiać sobie nagle, że się coś robi lub myśli w określony sposób",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Często <łapię się> na tym, że zapominam o ważnych datach urodzin.",
                            "target_lang_translations": {
                                "ru": "Я часто <ловлю себя> на том, что забываю о важных датах дней рождения."
                            }
                        },
                        {
                            "text": "Ona <złapała się> na nuceniu piosenki, której właściwie nie lubi.",
                            "target_lang_translations": {
                                "ru": "Она <поймала себя> на том, что напевает песню, которую на самом деле не любит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łapać się na myśli",
                        "łapać się na tym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно осознавать свои действия или мысли"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лови́ть себя́ (на)",
                                "target_lang_sense_clarification": "замечать за собой (мысль, действие)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

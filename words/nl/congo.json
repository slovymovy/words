                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Конго",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f880387-991e-3122-92d4-e189e3db47ee",
                    "sense_definition": "Een land in Centraal-Afrika, officieel de Republiek Congo, met Brazzaville als hoofdstad.",

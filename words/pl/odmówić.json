            "senses": [
                {
                    "sense_id": "22a86f56-4ccd-31f0-98b0-7fdd140e05aa",
                    "sense_definition": "Nie zgodzić się na zrobienie czegoś, nie przyjąć propozycji lub zaproszenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem niestety <odmówić> przyjścia na twoje urodziny, ponieważ wyjeżdżam.",
                            "target_lang_translations": {
                                "ru": "К сожалению, мне пришлось <отказаться> прийти на твой день рождения, так как я уезжаю."
                            }
                        },
                        {
                            "text": "Dyrektor postanowił stanowczo <odmówić> podpisania niekorzystnej umowy.",
                            "target_lang_translations": {
                                "ru": "Директор решил решительно <отказаться> от подписания невыгодного договора."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpowiedzieć",
                        "odpowiedzieć"
                    ],
                    "antonyms": [
                        "przyznać"
                    ],
                    "common_phrases": [
                        "grzecznie odmówić",
                        "stanowczo odmówić",
                        "odmówić współpracy",
                        "odmówić udziału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не согласиться сделать что-либо, не принять предложение или приглашение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказаться",
                                "target_lang_sense_clarification": "не согласиться на предложение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e893d68-8f90-3223-bb7e-03644aea72ea",
                    "sense_definition": "Nie udzielić komuś tego, o co prosi, czego żąda lub czego oczekuje.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz nie ma prawa <odmówić> pomocy osobie w stanie zagrożenia życia.",
                            "target_lang_translations": {
                                "ru": "Врач не имеет права <отказать> в помощи человеку, чья жизнь находится в опасности."
                            }
                        },
                        {
                            "text": "Sąd musiał <odmówić> oskarżonemu prawa do wcześniejszego wyjścia na wolność.",
                            "target_lang_translations": {
                                "ru": "Суд был вынужден <отказать> обвиняемому в праве на досрочное освобождение."
                            }
                        }
                    ],
                    "antonyms": [
                        "przyznać",
                        "umówić"
                    ],
                    "common_phrases": [
                        "odmówić pomocy",
                        "odmówić prawa",
                        "odmówić posłuszeństwa",
                        "odmówić wstępu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не предоставить кому-либо того, о чём он просит, чего требует или ожидает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказать",
                                "target_lang_sense_clarification": "не удовлетворить просьбу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "571b83d6-9715-3326-9bf1-61d843131dbe",
                    "sense_definition": "Wypowiedzieć na głos tekst, którego nauczyło się na pamięć, najczęściej modlitwę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "recitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia poprosiła wnuczka, aby pomógł jej <odmówić> wieczorny pacierz.",
                            "target_lang_translations": {
                                "ru": "Бабушка попросила внука помочь ей <прочитать> вечернюю молитву."
                            }
                        },
                        {
                            "text": "Każde dziecko musiało <odmówić> krótki wierszyk przed całą klasą.",
                            "target_lang_translations": {
                                "ru": "Каждый ребёнок должен был <рассказать> короткий стишок перед всем классом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmówić"
                    ],
                    "common_phrases": [
                        "odmówić modlitwę",
                        "odmówić różaniec",
                        "odmówić pacierz",
                        "odmówić wiersz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произнести вслух заученный наизусть текст, чаще всего молитву."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочитать",
                                "target_lang_sense_clarification": "молитву или текст"
                            },
                            {
                                "target_lang_word": "произнести",
                                "target_lang_sense_clarification": "сказать вслух"
                            }
                        ]
                    }
                }
            ]
        }
    ]

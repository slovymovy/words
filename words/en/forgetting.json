            "pos": "verb",
            "senses": [
                {
                    "sense_id": "784e582b-3303-35f1-b67b-939bf69fe259",
                    "sense_definition": "Present participle and gerund of <w>forget</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I keep <w>forgetting</w> where I put my car keys.",
                            "target_lang_translations": {
                                "ru": "Я постоянно <w>забываю</w>, куда положил ключи от машины."
                            }
                        },
                        {
                            "text": "<w>Forgetting</w> to set the alarm caused him to be late for work.",
                            "target_lang_translations": {
                                "ru": "<w>Забыв</w> поставить будильник, он опоздал на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlooking",
                        "neglecting"
                    ],
                    "antonyms": [
                        "remembering",
                        "recalling"
                    ],
                    "common_phrases": [
                        "keep forgetting",
                        "start forgetting"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle and gerund"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола <w>forget</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забывание",
                                "target_lang_sense_clarification": "процесс утраты информации из памяти"
                            },
                            {
                                "target_lang_word": "забывая",
                                "target_lang_sense_clarification": "деепричастие; действие, происходящее одновременно с другим"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4cf1e116-5e86-3797-ba09-5490dfd30550",
                    "sense_definition": "The mental act by which something is forgotten.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The history of the region is marked by deliberate <w>forgettings</w> of past conflicts.",
                            "target_lang_translations": {
                                "ru": "История региона отмечена преднамеренным <w>забвением</w> прошлых конфликтов."
                            }
                        },
                        {
                            "text": "Such <w>forgettings</w> are dangerous because they prevent us from learning from our mistakes.",
                            "target_lang_translations": {
                                "ru": "Подобное <w>забвение</w> опасно, так как мешает нам учиться на своих ошибках."
                            }
                        }
                    ],
                    "synonyms": [
                        "lapses",
                        "memory losses"
                    ],
                    "antonyms": [
                        "recollections",
                        "rememberings"
                    ],
                    "common_phrases": [
                        "collective forgetting",
                        "cultural forgetting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психический акт или процесс, посредством которого что-либо забывается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забвение",
                                "target_lang_sense_clarification": "состояние, когда что-то забыто (часто в культурном или историческом контексте)"
                            },
                            {
                                "target_lang_word": "забывание",
                                "target_lang_sense_clarification": "конкретные случаи или акты утраты памяти"
                            }
                        ]
                    }
                }
            ]
        }
    ]

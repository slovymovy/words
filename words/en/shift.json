    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "73bd7d9f-b6c0-3918-920c-07c10cc713b3",
                    "sense_definition": "A modifier key on a computer keyboard used to switch between character sets, such as typing capital letters or special symbols.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computing_key",
                    "name_type": "no",

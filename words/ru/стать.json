{
    "word_family": [
        "встать",
        "перестать",
        "устать",
        "отстать",
        "достать",
        "застать",
        "остаться",
        "восстать",
        "предстать",
        "статный",
        "становление",
        "станок",
        "остановка"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07039c03-c94f-3142-8aae-3f13fa8dc3d1",
                    "sense_definition": "Превратиться, сделаться кем-либо или каким-либо, перейти в новое состояние.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После дождя дорога <стала> очень грязной.",
                            "target_lang_translations": {
                                "en": "After the rain, the road <became> very dirty."
                            }
                        },
                        {
                            "text": "Мой брат <стал> врачом после окончания университета.",
                            "target_lang_translations": {
                                "en": "My brother <became> a doctor after graduating from university."
                            }
                        }
                    ],
                    "synonyms": [
                        "сделаться",
                        "превратиться"
                    ],
                    "common_phrases": [
                        "стало быть",
                        "стало лучше",
                        "стало известно"
                    ],
                    "target_lang_definitions": {
                        "en": "To turn into, become someone or something, or change state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "become",
                                "target_lang_sense_clarification": "to come into existence or begin to be"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to reach a certain state or condition (colloquial)"
                            },
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "to change condition or nature"
                            },
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "to become gradually"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e723ad2-da9d-3bb5-bddf-098644341c2d",
                    "sense_definition": "Начать совершать какое-либо действие (используется как вспомогательный глагол с инфинитивом несовершенного вида).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда стемнело, мы <стали> собираться домой.",
                            "target_lang_translations": {
                                "en": "When it got dark, we <started> getting ready to go home."
                            }
                        },
                        {
                            "text": "Он <стал> писать письмо, но не закончил его.",
                            "target_lang_translations": {
                                "en": "He <started> writing a letter but didn't finish it."
                            }
                        }
                    ],
                    "antonyms": [
                        "закончить",
                        "продолжить",
                        "недоставать"
                    ],
                    "common_phrases": [
                        "не стал делать",
                        "стал думать"
                    ],
                    "target_lang_definitions": {
                        "en": "To start doing something (used as an auxiliary verb)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "to begin an activity or movement"
                            },
                            {
                                "target_lang_word": "begin",
                                "target_lang_sense_clarification": "to perform the first part of an action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be714762-cf2a-3d81-868d-a085e7aaf424",
                    "sense_definition": "Принять вертикальное положение, встать на ноги или занять определённое место.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Стань> прямо и не двигайся, пока я тебя фотографирую.",
                            "target_lang_translations": {
                                "en": "<Stand> straight and don't move while I take your picture."
                            }
                        },
                        {
                            "text": "Я <стал> в очередь за билетами.",
                            "target_lang_translations": {
                                "en": "I <got> in line for tickets."
                            }
                        }
                    ],
                    "synonyms": [
                        "встать"
                    ],
                    "antonyms": [
                        "лечь",
                        "согнуться"
                    ],
                    "common_phrases": [
                        "стать на колени",
                        "стать в очередь"
                    ],
                    "target_lang_definitions": {
                        "en": "To take a standing position, stand up, or take a specific place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "to be in an upright position"
                            },
                            {
                                "target_lang_word": "stand up",
                                "target_lang_sense_clarification": "to rise to one's feet"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to move into a position (e.g., in line)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "653f4a52-dcef-3350-a021-3e2796c6ece6",
                    "sense_definition": "Прекратить движение или работу; остановиться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мои часы внезапно <стали>, и я опоздал на встречу.",
                            "target_lang_translations": {
                                "en": "My watch suddenly <stopped>, and I was late for the meeting."
                            }
                        },
                        {
                            "text": "Машина <стала> посреди дороги из-за поломки двигателя.",
                            "target_lang_translations": {
                                "en": "The car <stopped> in the middle of the road due to engine failure."
                            }
                        }
                    ],
                    "synonyms": [
                        "остановиться",
                        "застопориться"
                    ],
                    "antonyms": [
                        "двигаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop moving or working; to come to a halt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "halt",
                                "target_lang_sense_clarification": "to come to a stop"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fec842ec-f0b6-31ba-8d0e-1005c4bb8dfe",
                    "sense_definition": "Перестать существовать, исчезнуть или умереть (используется в безличных предложениях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его не <стало> после долгой и тяжёлой болезни.",
                            "target_lang_translations": {
                                "en": "He was <gone> after a long and severe illness."
                            }
                        },
                        {
                            "text": "В магазине совсем не <стало> свежего хлеба.",
                            "target_lang_translations": {
                                "en": "There was no fresh bread <left> in the store at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "наличествовать",
                        "существовать"
                    ],
                    "antonyms": [
                        "отсутствовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To cease to exist, disappear, or die (used in impersonal constructions)."
                    },
                    "translations": {
                        "en": []
                    }
                },
                {
                    "sense_id": "cc615532-557c-3c4e-bdc7-661ccf45e5fd",
                    "sense_definition": "Обойтись в какую-либо сумму денег.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во что мне <станет> ремонт этой квартиры?",
                            "target_lang_translations": {
                                "en": "How much will the renovation of this apartment <cost> me?"
                            }
                        },
                        {
                            "text": "Эта поездка <стала> нам в копеечку.",
                            "target_lang_translations": {
                                "en": "This trip <cost> us a pretty penny."
                            }
                        }
                    ],
                    "synonyms": [
                        "стоить",
                        "обойтись"
                    ],
                    "common_phrases": [
                        "во что бы то ни стало"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cost a certain amount of money."
                    },
                    "translations": {
                        "en": []
                    }
                },
                {
                    "sense_id": "37271607-6f72-3d67-b0b8-a72bd4f2912d",
                    "sense_definition": "Покрыться льдом, замёрзнуть (о реке или водоёме).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "freeze",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зима была холодной, и река <стала> уже в ноябре.",
                            "target_lang_translations": {
                                "en": "Winter was cold, and the river <froze> as early as November."
                            }
                        },
                        {
                            "text": "Лёд на озере <стал> достаточно крепким для катания.",
                            "target_lang_translations": {
                                "en": "The ice on the lake <became> strong enough for skating."
                            }
                        }
                    ],
                    "synonyms": [
                        "замёрзнуть",
                        "установиться"
                    ],
                    "antonyms": [
                        "растаять"
                    ],
                    "target_lang_definitions": {
                        "en": "To freeze over or become covered with ice (of a body of water)."
                    },
                    "translations": {
                        "en": []
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc970d15-6c0e-346e-8f4d-1673893a4029",
                    "sense_definition": "Основание, причина или нужда (в современном языке употребляется почти исключительно в вопросительных конструкциях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "need",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С какой <стати> я должен выполнять твою работу?",
                            "target_lang_translations": {
                                "en": "For what <reason> should I do your work?"
                            }
                        },
                        {
                            "text": "Нет никакой <стати> ехать туда в такую погоду.",
                            "target_lang_translations": {
                                "en": "There is no <need> to go there in such weather."
                            }
                        }
                    ],
                    "synonyms": [
                        "нужда",
                        "надобность",
                        "необходимость"
                    ],
                    "common_phrases": [
                        "с какой стати"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used primarily in fixed phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Reason, need, or cause (archaic, mostly used in fixed phrases)."
                    },
                    "translations": {
                        "en": []
                    }
                },
                {
                    "sense_id": "bfeefdad-5563-37da-9b4f-3025279d2ed3",
                    "sense_definition": "Соответствие кому-либо или чему-либо по характеру, свойствам или положению (обычно в значении «подходящая пара» или «ровня»).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "match",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта умная и красивая девушка ему под <стать>.",
                            "target_lang_translations": {
                                "en": "This smart and beautiful girl is a <match> for him."
                            }
                        },
                        {
                            "text": "Они нашли друг друга, оба упрямые и гордые — под <стать>.",
                            "target_lang_translations": {
                                "en": "They found each other, both stubborn and proud — a <match>."
                            }
                        }
                    ],
                    "synonyms": [
                        "характер",
                        "суть"
                    ],
                    "common_phrases": [
                        "под стать"
                    ],
                    "target_lang_definitions": {
                        "en": "A match or equal in character or status."
                    },
                    "translations": {
                        "en": []
                    }
                },
                {
                    "sense_id": "6942da9a-e6ec-3b16-97d8-aab2578b7eda",
                    "sense_definition": "Внешний вид, телосложение, осанка (обычно о красивой, стройной фигуре).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "build",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Военная <стать> выдавала в нём бывшего офицера.",
                            "target_lang_translations": {
                                "en": "His military <bearing> gave him away as a former officer."
                            }
                        },
                        {
                            "text": "Все любовались её гордой <статью> и плавными движениями.",
                            "target_lang_translations": {
                                "en": "Everyone admired her proud <stature> and smooth movements."
                            }
                        }
                    ],
                    "synonyms": [
                        "телосложение",
                        "осанка",
                        "фигура",
                        "стан"
                    ],
                    "target_lang_definitions": {
                        "en": "Physical appearance, build, or posture (usually implying stateliness)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "build",
                                "target_lang_sense_clarification": "physical structure or physique"
                            },
                            {
                                "target_lang_word": "figure",
                                "target_lang_sense_clarification": "bodily shape or form"
                            },
                            {
                                "target_lang_word": "appearance",
                                "target_lang_sense_clarification": "the way someone looks"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a6c42e3-fb60-377b-aa87-c494070c7d40",
                    "sense_definition": "Прикоснуться губами к кому-либо или чему-либо в знак любви, дружбы, уважения или ласки.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kissing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама наклонилась, чтобы <поцеловать> ребёнка перед сном.",
                            "target_lang_translations": {
                                "en": "Mom leaned down to <kiss> the child before bed."
                            }
                        },
                        {
                            "text": "Он хотел <поцеловать> её руку в знак глубокого уважения.",
                            "target_lang_translations": {
                                "en": "He wanted to <kiss> her hand as a sign of deep respect."
                            }
                        }
                    ],
                    "synonyms": [
                        "чмокнуть"
                    ],
                    "common_phrases": [
                        "поцеловать в щёку",
                        "поцеловать руку",
                        "крепко поцеловать",
                        "поцеловать на прощание"
                    ],
                    "target_lang_definitions": {
                        "en": "To touch someone or something with one's lips as a sign of love, friendship, respect, or affection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kiss",
                                "target_lang_sense_clarification": "touch with lips"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4adc3e79-1fba-3a74-af39-a04df1454519",
                    "sense_definition": "Слегка столкнуться с другим транспортным средством или препятствием, нанеся незначительные повреждения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На тесной парковке я случайно <поцеловал> бампер соседней машины.",
                            "target_lang_translations": {
                                "en": "In the tight parking lot, I accidentally <kissed> the bumper of the car next to me."
                            }
                        },
                        {
                            "text": "Автобус при повороте слегка <поцеловал> фонарный столб.",
                            "target_lang_translations": {
                                "en": "The bus lightly <grazed> the lamppost while turning."
                            }
                        }
                    ],
                    "synonyms": [
                        "задеть",
                        "стукнуть"
                    ],
                    "common_phrases": [

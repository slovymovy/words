    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "939909db-89e1-3f1a-a754-d3e9b466f065",
                    "sense_definition": "Czyścić lub osuszać mokrą lub zakurzoną powierzchnię, pocierając ją czymś (np. ścierką, ręcznikiem)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_drying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po umyciu naczyń musisz je dokładnie <w>wycierać</w> suchą ścierką.",
                            "target_lang_translations": {
                                "ru": "После мытья посуды ты должен тщательно <w>вытирать</w> её сухой тряпкой."
                            }
                        },
                        {
                            "text": "W każdą sobotę lubię <w>wycierać</w> kurze z mebli w salonie.",
                            "target_lang_translations": {
                                "ru": "Каждую субботу я люблю <w>вытирать</w> пыль с мебели в гостиной."
                            }
                        }
                    ],
                    "antonyms": [
                        "brudzić",
                        "moczyć"
                    ],
                    "common_phrases": [
                        "wycierać naczynia",
                        "wycierać kurze",
                        "wycierać buty",
                        "wycierać podłogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чистить или сушить мокрую или пыльную поверхность, протирая её чем-либо (например, тряпкой, полотенцем)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытирать",
                                "target_lang_sense_clarification": "делать сухим или чистым"
                            },
                            {
                                "target_lang_word": "протирать",
                                "target_lang_sense_clarification": "удалять пыль или влагу с поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc6e2c08-2cef-341f-9b1f-938694f50126",
                    "sense_definition": "Osuszać swoje ciało, zwykle za pomocą ręcznika po kąpieli lub myciu",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_drying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wyjściu z basenu trzeba szybko <w>wycierać</w> się ręcznikiem, żeby nie zmarznąć.",
                            "target_lang_translations": {
                                "ru": "Выйдя из бассейна, нужно быстро <w>вытираться</w> полотенцем, чтобы не замёрзнуть."
                            }
                        },
                        {
                            "text": "Zawsze pamiętaj, aby dokładnie <w>wycierać</w> ręce po wyjściu z toalety.",
                            "target_lang_translations": {
                                "ru": "Всегда помни, что нужно тщательно <w>вытирать</w> руки после туалета."
                            }
                        }
                    ],
                    "antonyms": [
                        "moczyć się"
                    ],
                    "common_phrases": [
                        "wycierać się ręcznikiem",
                        "wycierać ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высушивать своё тело, обычно с помощью полотенца после купания или мытья"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытирать",
                                "target_lang_sense_clarification": "руки, лицо или части тела"
                            },
                            {
                                "target_lang_word": "вытираться",
                                "target_lang_sense_clarification": "высушивать себя полностью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7bdd399-9c27-3b13-a727-d97b90cb5993",
                    "sense_definition": "Usuwać coś niepożądanego (np. brud, plamę, napis) z powierzchni poprzez pocieranie",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "removing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby zaczął <w>wycierać</w> tablicę po lekcji.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика начать <w>вытирать</w> доску после урока."
                            }
                        },
                        {
                            "text": "Dziecko próbowało <w>wycierać</w> plamę z bluzki mokrą chusteczką.",
                            "target_lang_translations": {
                                "ru": "Ребёнок пытался <w>оттирать</w> пятно с блузки влажной салфеткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścierać",
                        "ścierać",
                        "ścierać"
                    ],
                    "common_phrases": [
                        "wycierać tablicę",
                        "wycierać nos",
                        "wycierać plamę",
                        "wycierać łzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять что-то нежелательное (например, грязь, пятно, надпись) с поверхности путем трения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стирать",
                                "target_lang_sense_clarification": "удалять написанное или нарисованное"
                            },
                            {
                                "target_lang_word": "оттирать",
                                "target_lang_sense_clarification": "удалять пятна с усилием"
                            },
                            {
                                "target_lang_word": "вытирать",
                                "target_lang_sense_clarification": "очищать поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecf84c96-c32d-37f3-8a74-3bf3aa7f597e",
                    "sense_definition": "Ulegać zniszczeniu, stawać się cieńszym lub tracić powierzchnię na skutek długotrwałego tarcia i używania",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deterioration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tanie dżinsy mają tendencję, by szybko <w>wycierać</w> się na kolanach.",
                            "target_lang_translations": {
                                "ru": "Дешёвые джинсы имеют тенденцию быстро <w>протираться</w> на коленях."
                            }
                        },
                        {
                            "text": "Ten materiał jest delikatny i może się brzydko <w>wycierać</w> przy częstym noszeniu.",
                            "target_lang_translations": {
                                "ru": "Этот материал деликатный и может некрасиво <w>вытираться</w> при частой носке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "materiał się wyciera",
                        "spodnie się wycierają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушаться, становиться тоньше или терять поверхность в результате длительного трения и использования"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протираться",
                                "target_lang_sense_clarification": "становиться дырявым от носки"
                            },
                            {
                                "target_lang_word": "изнашиваться",
                                "target_lang_sense_clarification": "приходить в негодность"
                            },
                            {
                                "target_lang_word": "вытираться",
                                "target_lang_sense_clarification": "терять ворс или цвет"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "ec84b728-fb8d-3748-b2af-f2a26aeafae6",
                    "sense_definition": "Geeft aan dat iets of iemand van een bepaalde plaats, tijd of bron afkomstig is, of zich van binnen naar buiten beweegt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "origin_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman komt oorspronkelijk <w>uit</w> Frankrijk.",
                            "target_lang_translations": {
                                "en": "My neighbor is originally <w>from</w> France."
                            }
                        },
                        {
                            "text": "Zij haalde haar telefoon <w>uit</w> haar tas om te bellen.",
                            "target_lang_translations": {
                                "en": "She took her phone <w>out of</w> her bag to make a call."
                            }
                        }
                    ],
                    "antonyms": [
                        "in"
                    ],
                    "common_phrases": [
                        "uit bed",
                        "uit school",
                        "uit het hoofd",
                        "uit liefde"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates that something or someone originates from a certain place, time, or source, or moves from the inside to the outside."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating origin or source"
                            },
                            {
                                "target_lang_word": "out of",
                                "target_lang_sense_clarification": "movement from inside to outside"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7f148245-80f6-329c-af0e-538c0a2fcbd6",
                    "sense_definition": "Naar buiten toe; weg van een binnenruimte naar een andere plaats.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan vanavond gezellig samen <w>uit</w> in de stad.",
                            "target_lang_translations": {
                                "en": "We are going <w>out</w> together in the city tonight."
                            }
                        },
                        {
                            "text": "Hij liep boos de deur <w>uit</w> zonder iets te zeggen.",
                            "target_lang_translations": {
                                "en": "He walked <w>out</w> the door angrily without saying anything."
                            }
                        }
                    ],
                    "antonyms": [
                        "in"
                    ],
                    "common_phrases": [
                        "erop uit",
                        "dagje uit",
                        "uit eten"
                    ],
                    "target_lang_definitions": {
                        "en": "Towards the outside; away from an inner space to another place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "direction or movement away from inside"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f7de8d3a-729b-3c70-a445-70a138156c43",
                    "sense_definition": "Niet in werking of niet meer brandend; uitgeschakeld.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_off",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeet niet het licht <w>uit</w> te doen als je weggaat.",
                            "target_lang_translations": {
                                "en": "Don't forget to turn the light <w>off</w> when you leave."
                            }
                        },
                        {
                            "text": "De televisie is <w>uit</w> omdat er niemand kijkt.",
                            "target_lang_translations": {
                                "en": "The television is <w>off</w> because no one is watching."
                            }
                        }
                    ],
                    "antonyms": [
                        "aan"
                    ],
                    "common_phrases": [
                        "licht uit",
                        "tv uit",
                        "kachel uit"
                    ],
                    "target_lang_definitions": {
                        "en": "Not functioning or no longer burning; switched off."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "off",
                                "target_lang_sense_clarification": "state of a device or light"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "809da4b6-e94b-3d8a-a235-1dd549d218c5",
                    "sense_definition": "Afgelopen, voorbij of volledig klaar.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_finished",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De film is <w>uit</w>, dus we gaan nu naar huis.",
                            "target_lang_translations": {
                                "en": "The movie is <w>over</w>, so we are going home now."
                            }
                        },
                        {
                            "text": "Het is <w>uit</w> tussen die twee, ze zijn niet meer verliefd.",
                            "target_lang_translations": {
                                "en": "It is <w>over</w> between those two, they are no longer in love."
                            }
                        }
                    ],
                    "synonyms": [
                        "afgelopen",
                        "voorbij",
                        "gedaan"
                    ],
                    "common_phrases": [
                        "het is uit",
                        "school is uit",
                        "boek uit"
                    ],
                    "target_lang_definitions": {
                        "en": "Finished, past, or completely done."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "finished (event, relationship)"
                            },
                            {
                                "target_lang_word": "finished",
                                "target_lang_sense_clarification": "completed"
                            },
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "school or time period ended"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe63dc01-6023-3e2e-8d80-78dd369eb008",
                    "sense_definition": "Buiten de lijnen van het speelveld (bij sport).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter floot omdat de bal duidelijk <w>uit</w> was.",
                            "target_lang_translations": {
                                "en": "The referee blew the whistle because the ball was clearly <w>out</w>."
                            }
                        },
                        {
                            "text": "Hij sloeg de tennisbal te hard, waardoor deze <w>uit</w> ging.",
                            "target_lang_translations": {
                                "en": "He hit the tennis ball too hard, causing it to go <w>out</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "in"
                    ],
                    "common_phrases": [
                        "bal is uit",
                        "slaan uit"
                    ],
                    "target_lang_definitions": {
                        "en": "Outside the lines of the playing field (in sports)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "beyond the boundaries of the field"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f527565e-82b9-3163-a44d-487e92fa0cc6",
                    "sense_definition": "Eerste, tweede en derde persoon enkelvoud tegenwoordige tijd van <w>uiten</w>, of de gebiedende wijs.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>uit</w> zijn mening vaak op een heel directe manier.",
                            "target_lang_translations": {
                                "en": "He often <w>expresses</w> his opinion in a very direct way."
                            }
                        },
                        {
                            "text": "<w>Uit</w> je gevoelens, want dat lucht enorm op.",
                            "target_lang_translations": {
                                "en": "<w>Express</w> your feelings, because that is a huge relief."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection of uiten"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugated form of 'uiten' (to express): first, second, and third person singular present tense, or imperative."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expresses",
                                "target_lang_sense_clarification": "states an opinion or feeling"
                            },
                            {
                                "target_lang_word": "voices",
                                "target_lang_sense_clarification": "gives voice to"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
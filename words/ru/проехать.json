    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17001e7d-a710-32b9-9b4b-bf5f2434e090",
                    "sense_definition": "Проследовать мимо чего-либо или через что-либо на транспортном средстве.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы быстро <w>проехали</w> через центр города, так как не было пробок.",
                            "target_lang_translations": {
                                "en": "We quickly <w>drove through</w> the city center since there was no traffic."
                            }
                        },
                        {
                            "text": "Автобус <w>проехал</w> мимо остановки, не сбавляя скорости.",
                            "target_lang_translations": {
                                "en": "The bus <w>drove past</w> the stop without slowing down."
                            }
                        }
                    ],
                    "synonyms": [
                        "объехать"
                    ],
                    "common_phrases": [
                        "проехать мимо",
                        "проехать через мост",
                        "проехать на красный"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass by or go through something in a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive past",
                                "target_lang_sense_clarification": "movement past a specific point"
                            },
                            {
                                "target_lang_word": "drive through",
                                "target_lang_sense_clarification": "movement across an area"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "general movement past something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e742f3c-78d5-351b-b49a-c7daa99dd38e",
                    "sense_definition": "Преодолеть определённое расстояние, передвигаясь на транспорте.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За сегодня мы <w>проехали</w> пятьсот километров и очень устали.",
                            "target_lang_translations": {
                                "en": "We <w>covered</w> five hundred kilometers today and were very tired."
                            }
                        },
                        {
                            "text": "Нам осталось <w>проехать</w> совсем немного до дома.",
                            "target_lang_translations": {
                                "en": "We have very little left to <w>drive</w> to get home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "проехать сто километров",
                        "проехать полпути",
                        "проехать много"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a certain distance while traveling by vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "operating a vehicle over a distance"
                            },
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "completing a specific distance"
                            },
                            {
                                "target_lang_word": "travel",
                                "target_lang_sense_clarification": "general movement over distance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1450a353-c4d4-3f56-bde2-12f312cec72e",
                    "sense_definition": "Случайно пропустить нужное место остановки или назначения во время поездки.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я зачитался интересной книгой и <w>проехал</w> свою станцию метро.",
                            "target_lang_translations": {
                                "en": "I got absorbed in an interesting book and <w>missed</w> my subway station."
                            }
                        },
                        {
                            "text": "Водитель такси случайно <w>проехал</w> нужный поворот.",
                            "target_lang_translations": {
                                "en": "The taxi driver accidentally <w>missed</w> the right turn."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропустить"
                    ],
                    "common_phrases": [
                        "проехать остановку",
                        "проехать поворот",
                        "проехать мимо цели"
                    ],
                    "target_lang_definitions": {
                        "en": "To accidentally miss a stop or destination while traveling."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "fail to stop at intended location"
                            },
                            {
                                "target_lang_word": "go past",
                                "target_lang_sense_clarification": "physically pass the destination"
                            },
                            {
                                "target_lang_word": "overshoot",
                                "target_lang_sense_clarification": "go beyond the target"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a37161e1-1e2b-3173-8f30-4187882c3b49",
                    "sense_definition": "Перестать обсуждать какую-либо тему; забыть о сказанном или сделанном (обычно используется как призыв прекратить разговор).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давай не будем снова начинать этот спор, <w>проехали</w>.",
                            "target_lang_translations": {
                                "en": "Let's not start this argument again, <w>forget it</w>."
                            }
                        },
                        {
                            "text": "Ты извинился, так что всё нормально, <w>проехали</w> эту тему.",
                            "target_lang_translations": {
                                "en": "You apologized, so everything is fine, let's <w>drop</w> this subject."
                            }
                        }
                    ],
                    "synonyms": [
                        "забить"
                    ],
                    "common_phrases": [
                        "ладно проехали",
                        "всё проехали",
                        "проехали тему"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop discussing a topic; to forget about something said or done."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forget it",
                                "target_lang_sense_clarification": "colloquial, urging to stop discussing"
                            },
                            {
                                "target_lang_word": "drop it",
                                "target_lang_sense_clarification": "stop talking about a subject"
                            },
                            {
                                "target_lang_word": "move on",
                                "target_lang_sense_clarification": "proceed to the next topic"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d19bbd44-57c8-3573-a1ad-3fc97f0a9d96",
                    "sense_definition": "Тот, кто занимал какое-либо место, должность или пост перед кем-либо другим.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "role_holder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый директор сразу начал критиковать работу своего <w>предшественника</w>.",
                            "target_lang_translations": {
                                "en": "The new director immediately began criticizing the work of his <w>predecessor</w>."
                            }
                        },
                        {
                            "text": "Мой <w>предшественник</w> оставил все документы в полном порядке, что облегчило мне задачу.",
                            "target_lang_translations": {
                                "en": "My <w>predecessor</w> left all the documents in perfect order, which made my task easier."
                            }
                        }
                    ],
                    "antonyms": [
                        "последователь"
                    ],
                    "common_phrases": [
                        "достойный предшественник",
                        "предшественник на посту",
                        "ошибки предшественника"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who held a job, office, or position before the current holder."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "predecessor",
                                "target_lang_sense_clarification": "person who held a position previously"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eed64728-79ce-3409-bb50-4761e3496f66",
                    "sense_definition": "Лицо, явление или предмет, которые существовали ранее и подготовили условия для появления или развития чего-либо нового.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "precursor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Алхимия была историческим <w>предшественником</w> современной научной химии.",
                            "target_lang_translations": {
                                "en": "Alchemy was a historical <w>precursor</w> of modern scientific chemistry."
                            }
                        },
                        {
                            "text": "Этот писатель считается <w>предшественником</w> символизма в русской литературе.",
                            "target_lang_translations": {
                                "en": "This writer is considered a <w>forerunner</w> of symbolism in Russian literature."
                            }
                        }
                    ],
                    "synonyms": [
                        "предтеча"
                    ],
                    "antonyms": [
                        "последователь",
                        "эпигон",
                        "подражатель"
                    ],
                    "common_phrases": [
                        "великие предшественники",
                        "прямой предшественник",
                        "являться предшественником"
                    ],
                    "target_lang_definitions": {
                        "en": "A person, thing, or event that existed earlier and paved the way for something new."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "precursor",
                                "target_lang_sense_clarification": "something that leads to the development of something else"
                            },
                            {
                                "target_lang_word": "forerunner",
                                "target_lang_sense_clarification": "an early example or ancestor"
                            },
                            {
                                "target_lang_word": "predecessor",
                                "target_lang_sense_clarification": "an earlier version or model"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06157a8c-2529-3fae-85b6-b508e1ff1545",
                    "sense_definition": "Сельскохозяйственная культура, которая выращивалась на поле до посева следующей культуры в севообороте.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Озимая пшеница — это отличный <w>предшественник</w> для многих яровых культур.",
                            "target_lang_translations": {
                                "en": "Winter wheat is an excellent <w>predecessor</w> for many spring crops."
                            }
                        },
                        {
                            "text": "Агроном тщательно выбирал <w>предшественник</w> для посева сахарной свеклы.",
                            "target_lang_translations": {
                                "en": "The agronomist carefully chose the <w>predecessor</w> for sowing sugar beet."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лучший предшественник",
                        "выбор предшественника"
                    ],
                    "target_lang_definitions": {
                        "en": "An agricultural crop grown on a field before the next crop in the rotation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "predecessor",
                                "target_lang_sense_clarification": "crop grown previously in rotation"
                            },
                            {
                                "target_lang_word": "preceding crop",
                                "target_lang_sense_clarification": "agricultural term"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
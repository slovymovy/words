{
    "word_family": [
        "spillage",
        "spiller",
        "overspill"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "50d1191c-c72e-338c-ba26-0a5516087a95",
                    "sense_definition": "To accidentally cause a liquid or substance to flow over the edge of its container or boundaries.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>spill</w> the hot soup on yourself.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не <w>пролей</w> на себя горячий суп."
                            }
                        },
                        {
                            "text": "He accidentally <w>spilled</w> his drink all over the table.",
                            "target_lang_translations": {
                                "ru": "Он случайно <w>разлил</w> свой напиток по всему столу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pour",
                        "slop"
                    ],
                    "antonyms": [
                        "contain"
                    ],
                    "common_phrases": [
                        "spill water",
                        "spill coffee",
                        "spill the milk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно вылить жидкость или высыпать вещество через край контейнера или границы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пролить",
                                "target_lang_sense_clarification": "о жидкости; случайно вылить"
                            },
                            {
                                "target_lang_word": "разлить",
                                "target_lang_sense_clarification": "о жидкости; вылить на поверхность"
                            },
                            {
                                "target_lang_word": "просыпать",
                                "target_lang_sense_clarification": "о сыпучих веществах (соль, крупа и т.д.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbd79e72-65b2-386f-83bf-6b30dc849c84",
                    "sense_definition": "To flow over the edge of a container or spread out uncontrollably.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water <w>spilled</w> over the edge of the bathtub.",
                            "target_lang_translations": {
                                "ru": "Вода <w>перелилась</w> через край ванны."
                            }
                        },
                        {
                            "text": "Light from the hallway <w>spilled</w> into the dark bedroom.",
                            "target_lang_translations": {
                                "ru": "Свет из коридора <w>проникал</w> в темную спальню."
                            }
                        }
                    ],
                    "synonyms": [
                        "overflow",
                        "stream",
                        "run out"
                    ],
                    "common_phrases": [
                        "spill over",
                        "spill out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переливаться через край или бесконтрольно распространяться (о жидкости, свете и т.д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разливаться",
                                "target_lang_sense_clarification": "распространяться по поверхности"
                            },
                            {
                                "target_lang_word": "переливаться",
                                "target_lang_sense_clarification": "течь через край"
                            },
                            {
                                "target_lang_word": "проникать",
                                "target_lang_sense_clarification": "о свете; попадать внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "150444be-3cf0-3823-bef0-06d33b6ac82a",
                    "sense_definition": "To reveal secret or private information to someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She finally <w>spilled</w> the secret about the surprise party.",
                            "target_lang_translations": {
                                "ru": "Она наконец <w>раскрыла</w> секрет о вечеринке-сюрпризе."
                            }
                        },
                        {
                            "text": "I won't tell anyone, so you can <w>spill</w> the beans to me.",
                            "target_lang_translations": {
                                "ru": "Я никому не скажу, так что можешь всё мне <w>выложить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "reveal",
                        "disclose",
                        "divulge"
                    ],
                    "antonyms": [
                        "conceal",
                        "hide",
                        "suppress"
                    ],
                    "common_phrases": [
                        "spill the beans",
                        "spill the tea",
                        "spill it"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрыть секретную или личную информацию кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрыть",
                                "target_lang_sense_clarification": "тайну или секрет"
                            },
                            {
                                "target_lang_word": "разболтать",
                                "target_lang_sense_clarification": "разг.; выдать секрет"
                            },
                            {
                                "target_lang_word": "проговориться",
                                "target_lang_sense_clarification": "случайно выдать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "add5cd9a-d682-3593-aebc-b92a1ee95965",
                    "sense_definition": "To move or spread out of a place in large numbers, like a liquid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_crowd",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the game, fans <w>spilled</w> out of the stadium.",
                            "target_lang_translations": {
                                "ru": "После игры болельщики <w>высыпали</w> со стадиона."
                            }
                        },
                        {
                            "text": "The protest <w>spilled</w> into the neighboring streets.",
                            "target_lang_translations": {
                                "ru": "Протест <w>перекинулся</w> на соседние улицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "pour",
                        "stream",
                        "flood",
                        "swarm"
                    ],
                    "common_phrases": [
                        "spill into",
                        "spill out of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выходить или распространяться откуда-либо в большом количестве (о толпе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высыпать",
                                "target_lang_sense_clarification": "о толпе людей"
                            },
                            {
                                "target_lang_word": "хлынуть",
                                "target_lang_sense_clarification": "двинуться потоком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "880e6e02-f661-3b39-9225-95d00c74840c",
                    "sense_definition": "To cause blood to flow by wounding or killing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_blood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers were reluctant to <w>spill</w> more blood.",
                            "target_lang_translations": {
                                "ru": "Солдаты не хотели больше <w>проливать</w> кровь."
                            }
                        },
                        {
                            "text": "He felt guilty for the innocent blood he had <w>spilt</w>.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал вину за невинную кровь, которую <w>пролил</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "shed"
                    ],
                    "common_phrases": [
                        "spill blood"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in the phrase 'spill blood'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать кровотечение, раня или убивая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проливать",
                                "target_lang_sense_clarification": "обычно в сочетании «проливать кровь»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "824b0371-6759-38eb-9c5a-8b1b68d0f786",
                    "sense_definition": "To cause someone to fall from a vehicle, horse, or saddle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horse got spooked and <w>spilled</w> the rider.",
                            "target_lang_translations": {
                                "ru": "Лошадь испугалась и <w>сбросила</w> всадника."
                            }
                        },
                        {
                            "text": "The rough terrain <w>spilled</w> him from his bike.",
                            "target_lang_translations": {
                                "ru": "Из-за неровной дороги он <w>упал</w> с велосипеда."
                            }
                        }
                    ],
                    "synonyms": [
                        "throw",
                        "unseat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбросить кого-либо с транспортного средства или лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбросить",
                                "target_lang_sense_clarification": "заставить упасть"
                            },
                            {
                                "target_lang_word": "скинуть",
                                "target_lang_sense_clarification": "разг.; сбросить"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ae990a3-1445-35c0-adda-d0fbb949cafd",
                    "sense_definition": "A quantity of liquid or other substance that has been dropped or has flowed out of its container.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The janitor came to clean up the <w>spill</w> in the hallway.",
                            "target_lang_translations": {
                                "ru": "Уборщик пришел, чтобы вытереть <w>лужу</w> в коридоре."
                            }
                        },
                        {
                            "text": "Environmentalists are worried about the oil <w>spill</w>.",
                            "target_lang_translations": {
                                "ru": "Экологи обеспокоены <w>разливом</w> нефти."
                            }
                        }
                    ],
                    "synonyms": [
                        "mess",
                        "leakage"
                    ],
                    "common_phrases": [
                        "oil spill",
                        "chemical spill",
                        "clean up a spill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество жидкости или другого вещества, которое пролилось или вытекло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лужа",
                                "target_lang_sense_clarification": "пролитая жидкость на полу/столе"
                            },
                            {
                                "target_lang_word": "разлив",
                                "target_lang_sense_clarification": "масштабное вытекание (напр. нефти)"
                            },
                            {
                                "target_lang_word": "пятно",
                                "target_lang_sense_clarification": "след от пролитого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4dbfd12-64e6-34ba-9fca-a2484817b395",
                    "sense_definition": "A fall from a standing position or from a vehicle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grandpa took a bad <w>spill</w> on the icy sidewalk.",
                            "target_lang_translations": {
                                "ru": "Дедушка сильно <w>упал</w> на обледенелом тротуаре."
                            }
                        },
                        {
                            "text": "She had a nasty <w>spill</w> while riding her bike.",
                            "target_lang_translations": {
                                "ru": "Она скверно <w>упала</w>, катаясь на велосипеде."
                            }
                        }
                    ],
                    "synonyms": [
                        "fall",
                        "tumble"
                    ],
                    "common_phrases": [
                        "take a spill",
                        "bad spill"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Падение из положения стоя или с транспортного средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "резкое падение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93326546-b24f-315e-a01e-eaf5ca95ac08",
                    "sense_definition": "A declaration that a leadership position in a political party is vacant and open for re-election.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The party announced a leadership <w>spill</w> this morning.",
                            "target_lang_translations": {
                                "ru": "Партия объявила о <w>перевыборах</w> руководства сегодня утром."
                            }
                        },
                        {
                            "text": "He lost his position after the sudden <w>spill</w>.",
                            "target_lang_translations": {
                                "ru": "Он потерял свою должность после внезапных <w>перевыборов</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leadership spill"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly Australia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объявление руководящих постов в политической партии вакантными для перевыборов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевыборы руководства",
                                "target_lang_sense_clarification": "политический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54d4a941-58ab-369a-a888-8155ff9c7275",
                    "sense_definition": "A thin strip of wood or paper used to transfer flame from a fire to a candle or pipe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_stick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used a <w>spill</w> to light the candle from the fire.",
                            "target_lang_translations": {
                                "ru": "Он использовал <w>лучину</w>, чтобы зажечь свечу от огня."
                            }
                        },
                        {
                            "text": "She rolled a piece of paper into a <w>spill</w>.",
                            "target_lang_translations": {
                                "ru": "Она свернула кусок бумаги в <w>трубочку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "taper"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкая щепка или скрученная бумага для переноса огня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лучина",
                                "target_lang_sense_clarification": "тонкая щепка"
                            },
                            {
                                "target_lang_word": "бумажная трубочка",
                                "target_lang_sense_clarification": "для розжига"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
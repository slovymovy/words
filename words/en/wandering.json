        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "98d61a83-9d89-34c2-8bf9-cd7216ffb20c",
                    "sense_definition": "Travelling from place to place without a fixed home or specific destination.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "travel_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <wandering> tribe set up their camp near the river for the summer.",
                            "target_lang_translations": {
                                "ru": "<Кочевое> племя разбило лагерь у реки на лето."
                            }
                        },
                        {
                            "text": "He lived the life of a <wandering> musician, playing songs in every town he visited.",
                            "target_lang_translations": {
                                "ru": "Он жил жизнью <странствующего> музыканта, играя песни в каждом городе, который посещал."
                            }
                        }
                    ],
                    "synonyms": [
                        "roaming",
                        "nomadic",
                        "itinerant",
                        "wayfaring"
                    ],
                    "antonyms": [
                        "settled",
                        "stationary"
                    ],
                    "common_phrases": [
                        "wandering eye",
                        "wandering star",
                        "wandering minstrel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествующий с места на место без постоянного дома или конкретного пункта назначения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стра́нствующий",
                                "target_lang_sense_clarification": "traveling"
                            },

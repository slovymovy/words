{
    "word_family": [
        "copyrightable",
        "uncopyrightable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "be0e8606-1cd0-3928-898f-9d4efe2d9b8a",
                    "sense_definition": "The legal right that allows an author or creator to control how their work is published, copied, or distributed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_right",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author retained the <w>copyright</w> to her manuscript to ensure no one else could print it."
                        },
                        {
                            "text": "Under current <w>copyright</w> law, the protection lasts for the life of the creator plus seventy years."
                        }
                    ],
                    "synonyms": [
                        "intellectual property"
                    ],
                    "antonyms": [
                        "public domain"
                    ],
                    "common_phrases": [
                        "copyright law",
                        "copyright protection",
                        "under copyright",
                        "copyright holder"
                    ]
                },
                {
                    "sense_id": "c2d4c236-d1b2-35e3-a609-3a77259b4772",
                    "sense_definition": "A specific instance of exclusive legal control held over a particular creative work.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_right",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The music studio purchased the <w>copyrights</w> to the entire catalog of the band."
                        },
                        {
                            "text": "She lost the <w>copyright</w> to her early paintings when she signed the unfair contract."
                        }
                    ],
                    "common_phrases": [
                        "own the copyright",
                        "buy the copyright"
                    ]
                },
                {
                    "sense_id": "0923008d-f62f-3eaa-b9a6-afe02ffbc12b",
                    "sense_definition": "A violation of copyright law, often used as a shorthand for infringement.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The video was taken down immediately due to <w>copyright</w>."
                        },
                        {
                            "text": "He added a disclaimer saying no <w>copyright</w> intended to avoid getting banned."
                        }
                    ],
                    "synonyms": [
                        "infringement",
                        "piracy"
                    ],
                    "common_phrases": [
                        "no copyright intended",
                        "accused of copyright"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used elliptically for 'copyright infringement'"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42790f67-50d4-30c4-8223-3c30b89b3f9e",
                    "sense_definition": "To legally secure the exclusive right to publish and distribute a creative work.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>copyright</w> your software before releasing it to the public."
                        },
                        {
                            "text": "The company decided to <w>copyright</w> the new logo to prevent competitors from using it."
                        }
                    ],
                    "synonyms": [
                        "protect",
                        "license"
                    ],
                    "common_phrases": [
                        "copyright a book",
                        "copyright a name"
                    ]
                },
                {
                    "sense_id": "de9cc8e0-8c17-3b28-b0ce-ada76ee89c74",
                    "sense_definition": "To report digital content for violating copyright laws, often leading to its removal or demonetization.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not want to get <w>copyrighted</w> for playing popular music on my stream."
                        },
                        {
                            "text": "The platform <w>copyrighted</w> the video because it contained clips from a movie."
                        }
                    ],
                    "synonyms": [
                        "copystrike",
                        "flag"
                    ],
                    "common_phrases": [
                        "get copyrighted"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ]
                }
            ]
        }
    ]
}
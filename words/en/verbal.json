    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b7472322-3acb-3994-93d9-9735a8c10230",
                    "sense_definition": "Expressly spoken rather than written.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spoken_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager gave the employee a <w>verbal</w> warning before taking further action.",
                            "target_lang_translations": {
                                "ru": "Менеджер сделал сотруднику <w>устное</w> предупреждение, прежде чем принимать дальнейшие меры."
                            }
                        },
                        {
                            "text": "They made a <w>verbal</w> agreement to sell the house, but nothing was signed yet.",
                            "target_lang_translations": {
                                "ru": "Они заключили <w>устное</w> соглашение о продаже дома, но ничего еще не было подписано."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-verbal",
                        "unsaid",
                        "implied"
                    ],
                    "common_phrases": [
                        "verbal agreement",
                        "verbal warning",
                        "verbal communication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устный, выраженный словами, а не письменно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "у́стный",
                                "target_lang_sense_clarification": "spoken, not written"
                            },
                            {
                                "target_lang_word": "слове́сный",
                                "target_lang_sense_clarification": "expressed in words"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddb7e19a-14d6-3f68-94ce-1fac8800fb3f",
                    "sense_definition": "Relating to words or the use of words.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relating_to_words",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has excellent <w>verbal</w> skills and can explain complex ideas clearly.",
                            "target_lang_translations": {
                                "ru": "У нее отличные <w>вербальные</w> навыки, и она может ясно объяснять сложные идеи."
                            }
                        },
                        {
                            "text": "The test evaluates both mathematical and <w>verbal</w> reasoning abilities.",
                            "target_lang_translations": {
                                "ru": "Тест оценивает способности как к математическому, так и к <w>вербальному</w> мышлению."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-verbal"
                    ],
                    "common_phrases": [
                        "verbal skills",
                        "verbal reasoning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный со словами или использованием слов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верба́льный",
                                "target_lang_sense_clarification": "relating to words or skills"
                            },
                            {
                                "target_lang_word": "слове́сный",
                                "target_lang_sense_clarification": "consisting of words"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fc644bf-470f-3a2d-8064-7c5d6a417e5a",
                    "sense_definition": "Capable of using speech.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ability_to_speak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The therapist worked hard to help the non-verbal child become <w>verbal</w>.",
                            "target_lang_translations": {
                                "ru": "Терапевт усердно работал, чтобы помочь невербальному ребенку стать <w>говорящим</w>."
                            }
                        },
                        {
                            "text": "Although he is <w>verbal</w>, he struggles to communicate his emotions effectively.",
                            "target_lang_translations": {
                                "ru": "Хотя он <w>владеет речью</w>, ему трудно эффективно выражать свои эмоции."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-verbal"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "60fb0e7e-24c5-33f2-b2e2-84dfa48cf521",
                    "sense_definition": "Vloeistof in kleine druppels laten rondvliegen of uiteen laten vallen, vaak gezegd van heet vet of vallende regen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "splatter_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op, de hete boter kan flink <w>spetten</w> als je het vlees in de pan legt.",
                            "target_lang_translations": {
                                "en": "Watch out, the hot butter can really <w>splatter</w> when you put the meat in the pan.",
                                "ru": "Осторожно, горячее масло может сильно <w>брызгаться</w>, когда кладешь мясо на сковороду."
                            }
                        },
                        {
                            "text": "De regen <w>spet</w> tegen de ruiten door de harde wind.",
                            "target_lang_translations": {
                                "en": "The rain <w>splatters</w> against the windows due to the strong wind.",
                                "ru": "Дождь <w>брызжет</w> в окна из-за сильного ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "spatten",
                        "spetteren"
                    ],
                    "common_phrases": [
                        "boter spet"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause liquid to fly around in small drops, often said of hot grease or falling rain.",
                        "ru": "Разбрызгивать жидкость мелкими каплями или распадаться на капли; часто говорится о горячем жире или дожде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "splatter",
                                "target_lang_sense_clarification": "to scatter liquid in small drops"
                            },
                            {
                                "target_lang_word": "spit",
                                "target_lang_sense_clarification": "specifically for hot fat or grease making a popping sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "брызгать",
                                "target_lang_sense_clarification": "разлетаться каплями"
                            },
                            {
                                "target_lang_word": "стрелять",
                                "target_lang_sense_clarification": "о горячем жире"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46656bfd-bb15-3fe9-85e1-eeaab8649225",
                    "sense_definition": "Meervoud van <w>spet</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zaten vieze <w>spetten</w> modder op zijn nieuwe broek.",
                            "target_lang_translations": {
                                "en": "There were dirty mud <w>splatters</w> on his new trousers.",
                                "ru": "На его новых брюках были грязные <w>брызги</w> грязи."
                            }
                        },
                        {
                            "text": "De schilder probeerde de <w>spetten</w> verf van de vloer te verwijderen.",
                            "target_lang_translations": {
                                "en": "The painter tried to remove the paint <w>spots</w> from the floor.",
                                "ru": "Маляр пытался удалить <w>капли</w> краски с пола."
                            }
                        }
                    ],

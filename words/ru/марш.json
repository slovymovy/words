    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2d4f7f30-242b-3e5e-80b1-af239a2f6a6a",
                    "sense_definition": "Музыкальное произведение с чётким, энергичным ритмом, предназначенное для сопровождения строевого шага или шествия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оркестр заиграл торжественный <w>марш</w>, и солдаты начали движение по главной площади."
                        },
                        {
                            "text": "В начале церемонии мы услышали знаменитый свадебный <w>марш</w> Мендельсона."
                        }
                    ],
                    "common_phrases": [
                        "похоронный марш",
                        "свадебный марш",
                        "играть марш",
                        "звуки марша"
                    ]
                },
                {
                    "sense_id": "27b3b55e-6d7d-3ff6-b92b-c8261aa0d871",
                    "sense_definition": "Организованное передвижение войск или больших групп людей из одного места в другое.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полк совершил длительный ночной <w>марш</w> и к утру достиг места назначения."
                        },
                        {
                            "text": "Во время <w>марша</w> солдатам строго запрещалось разговаривать и нарушать строй."
                        }
                    ],
                    "synonyms": [
                        "поход",
                        "прохождение"
                    ],
                    "common_phrases": [
                        "марш-бросок",
                        "на марше",
                        "совершить марш"
                    ]
                },
                {
                    "sense_id": "50587557-f799-325d-9f69-6f0300afe59d",
                    "sense_definition": "Массовое шествие людей с целью привлечения внимания к какой-либо проблеме или выражения протеста.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тысячи неравнодушных граждан вышли на <w>марш</w> в защиту экологии родного города."
                        },
                        {
                            "text": "Вчера в центре столицы прошёл <w>марш</w> несогласных с новой экономической реформой."
                        }
                    ],
                    "synonyms": [
                        "демонстрация"
                    ],
                    "common_phrases": [
                        "марш протеста",
                        "марш мира",
                        "участвовать в марше"
                    ]
                },
                {
                    "sense_id": "b554bd7a-28bb-3041-b362-629200b8f556",
                    "sense_definition": "Часть лестницы, представляющая собой непрерывный ряд ступеней между двумя площадками.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители закончили бетонировать первый лестничный <w>марш</w> в новом жилом доме."
                        },
                        {
                            "text": "Чтобы подняться на чердак, нам пришлось преодолеть ещё один крутой <w>марш</w>."
                        }
                    ],
                    "synonyms": [
                        "пролёт"
                    ],
                    "common_phrases": [
                        "лестничный марш",
                        "нижний марш"
                    ]
                }
            ]
        },
        {

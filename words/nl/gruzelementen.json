            "pos": "noun",
            "senses": [
                {
                    "sense_id": "474ecc76-0612-37e8-844c-64eb3f307e8c",
                    "sense_definition": "Kleine stukken of scherven die ontstaan wanneer een voorwerp breekt.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In zijn woede smeet hij die prachtige vaas in <w>gruzelementen</w>.",
                            "target_lang_translations": {
                                "en": "In his anger, he smashed that beautiful vase to <w>smithereens</w>.",
                                "ru": "В ярости он разбил эту прекрасную вазу на мелкие <w>осколки</w>."
                            }
                        },
                        {
                            "text": "Toen de spiegel van de muur viel, lag hij in duizend <w>gruzelementen</w> op de vloer.",
                            "target_lang_translations": {
                                "en": "When the mirror fell off the wall, it lay in a thousand <w>pieces</w> on the floor.",
                                "ru": "Когда зеркало упало со стены, оно лежало на полу, разбитое на тысячу <w>осколков</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "scherven",
                        "brokstukken",
                        "splinters"
                    ],
                    "common_phrases": [
                        "in gruzelementen vallen",
                        "in gruzelementen slaan",
                        "in gruzelementen liggen"
                    ],
                    "target_lang_definitions": {
                        "en": "Small pieces or shards created when an object breaks.",
                        "ru": "Мелкие обломки или осколки, которые образуются, когда предмет разбивается."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smithereens",
                                "target_lang_sense_clarification": "tiny pieces resulting from smashing (idiomatic)"
                            },
                            {
                                "target_lang_word": "shards",
                                "target_lang_sense_clarification": "sharp broken pieces, typically of glass or pottery"
                            },
                            {
                                "target_lang_word": "fragments",
                                "target_lang_sense_clarification": "general broken parts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осколки",
                                "target_lang_sense_clarification": "острые куски разбитого стекла, керамики и т.д."
                            },
                            {
                                "target_lang_word": "дребезги",
                                "target_lang_sense_clarification": "мельчайшие частицы (обычно в выражении «вдребезги»)"
                            },
                            {
                                "target_lang_word": "обломки",
                                "target_lang_sense_clarification": "куски разрушенного предмета"
                            }
                        ]
                    }
                }
            ]
        }

                    "synonyms": [
                        "монархия"
                    ],
                    "antonyms": [
                        "республика"
                    ],
                    "common_phrases": [
                        "британская корона",
                        "служить короне",
                        "интересы короны"
                    ],
                    "target_lang_definitions": {
                        "en": "Monarchical power, the state, or the government headed by a monarch."
                    },
                    "translations": {

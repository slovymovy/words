    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ca2451e-81c1-35bf-837b-d60834a3952e",
                    "sense_definition": "Поединок с применением оружия между двумя противниками по вызову одного из них.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой офицер решил вызвать своего обидчика на <w>дуэль</w>.",
                            "target_lang_translations": {
                                "en": "The young officer decided to challenge his offender to a <w>duel</w>."
                            }
                        },
                        {
                            "text": "Великий поэт был смертельно ранен на <w>дуэли</w>.",
                            "target_lang_translations": {
                                "en": "The great poet was mortally wounded in a <w>duel</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "поединок",
                        "единоборство"
                    ],
                    "antonyms": [
                        "примирение"
                    ],
                    "common_phrases": [
                        "вызвать на дуэль",
                        "драться на дуэли",
                        "убит на дуэли"
                    ],
                    "target_lang_definitions": {
                        "en": "A combat with weapons between two opponents, usually initiated by a challenge from one of them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duel",
                                "target_lang_sense_clarification": "combat with weapons"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b62390f2-8551-3959-94d9-cf3fcfbf4f58",
                    "sense_definition": "Состязание, острая борьба двух сторон в каком-либо деле.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Между кандидатами в президенты состоялась словесная <w>дуэль</w>.",
                            "target_lang_translations": {
                                "en": "A verbal <w>duel</w> took place between the presidential candidates."
                            }
                        },
                        {
                            "text": "Эта шахматная <w>дуэль</w> привлекла внимание всего мира.",
                            "target_lang_translations": {
                                "en": "This chess <w>duel</w> attracted the attention of the whole world."
                            }
                        }
                    ],
                    "synonyms": [
                        "состязание",
                        "борьба"
                    ],
                    "common_phrases": [
                        "словесная дуэль"
                    ],
                    "target_lang_definitions": {
                        "en": "A competition or intense struggle between two sides in a particular field or matter."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duel",
                                "target_lang_sense_clarification": "metaphorical contest"
                            },
                            {
                                "target_lang_word": "battle",
                                "target_lang_sense_clarification": "intense struggle"
                            }
                        ]
                    }
                }
            ]
        }

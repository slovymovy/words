    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b1eeb27-409e-3b77-907c-70f50033183c",
                    "sense_definition": "To mend, to repair.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "repair_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That heater will start a fire if you don't <w>fix</w> it.",
                            "target_lang_translations": {
                                "ru": "Этот обогреватель вызовет пожар, если ты его не <w>починишь</w>."
                            }
                        },
                        {
                            "text": "I need to <w>fix</w> my bike before we can go for a ride.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>починить</w> велосипед, прежде чем мы сможем поехать кататься."
                            }
                        }
                    ],
                    "synonyms": [
                        "repair",
                        "patch",
                        "rectify"
                    ],
                    "common_phrases": [
                        "fix it",
                        "fix up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чинить, ремонтировать или исправлять что-либо сломанное или неисправное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чинить",
                                "target_lang_sense_clarification": "исправлять поломку (обычно мелкую)"
                            },
                            {
                                "target_lang_word": "ремонтировать",
                                "target_lang_sense_clarification": "производить ремонт (более формально или масштабно)"
                            },
                            {
                                "target_lang_word": "исправлять",
                                "target_lang_sense_clarification": "устранять неполадки или ошибки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43626d59-7aae-3a0b-8a50-53a641da671c",
                    "sense_definition": "To prepare (food or drink).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>fixed</w> dinner for the kids while they played outside.",
                            "target_lang_translations": {
                                "ru": "Она <w>приготовила</w> ужин для детей, пока они играли на улице."
                            }
                        },
                        {
                            "text": "I will <w>fix</w> you a sandwich if you are hungry.",
                            "target_lang_translations": {
                                "ru": "Я <w>сделаю</w> тебе бутерброд, если ты голоден."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fix dinner",
                        "fix a drink"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in US English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовить еду или напитки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовить",
                                "target_lang_sense_clarification": "заниматься приготовлением пищи"
                            },
                            {
                                "target_lang_word": "приготовить",
                                "target_lang_sense_clarification": "сделать готовым к употреблению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce30bb8d-8602-3143-876b-959578ce0c94",
                    "sense_definition": "To attach; to affix; to hold in place or at a particular time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attach_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A dab of chewing gum will <w>fix</w> your note to the bulletin board.",
                            "target_lang_translations": {
                                "ru": "Капелька жвачки <w>прикрепит</w> твою записку к доске объявлений."
                            }
                        },
                        {
                            "text": "The committee will <w>fix</w> the date for the next meeting.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>назначит</w> дату следующего собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "join",
                        "unite"
                    ],
                    "antonyms": [
                        "move",
                        "change"
                    ],
                    "common_phrases": [
                        "fix to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять что-либо к чему-либо; назначать или устанавливать (время, дату)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закреплять",
                                "target_lang_sense_clarification": "фиксировать в определенном положении"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "определять время или дату"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "утверждать или монтировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641e3d3f-8f05-365a-9b37-a4b5d3646740",
                    "sense_definition": "To make a contest, vote, or gamble unfair; to privilege one contestant or group.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rig_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A majority of voters believed the election was <w>fixed</w> in favor of the incumbent.",
                            "target_lang_translations": {
                                "ru": "Большинство избирателей считало, что выборы были <w>сфальсифицированы</w> в пользу действующего кандидата."
                            }
                        },
                        {
                            "text": "They tried to <w>fix</w> the match by bribing the referee.",
                            "target_lang_translations": {
                                "ru": "Они пытались <w>подстроить</w> исход матча, подкупив судью."
                            }
                        }
                    ],
                    "synonyms": [
                        "rig",
                        "doctor"
                    ],
                    "common_phrases": [
                        "fix the match",
                        "fix the election"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подстраивать результат состязания или выборов; фальсифицировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстраивать",
                                "target_lang_sense_clarification": "заранее договариваться о результате"
                            },
                            {
                                "target_lang_word": "фальсифицировать",
                                "target_lang_sense_clarification": "подделывать результаты (выборов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37f9b6fb-3915-3edc-a55e-a860f24563fe",
                    "sense_definition": "To surgically render an animal, especially a pet, infertile.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "neuter_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rover stopped digging under the fence after we had the vet <w>fix</w> him.",
                            "target_lang_translations": {
                                "ru": "Ровер перестал копать под забором после того, как ветеринар его <w>кастрировал</w>."
                            }
                        },
                        {
                            "text": "It is responsible to get your cat <w>fixed</w> to prevent unwanted kittens.",
                            "target_lang_translations": {
                                "ru": "Ответственно <w>стерилизовать</w> свою кошку, чтобы предотвратить появление нежелательных котят."
                            }
                        }
                    ],
                    "synonyms": [
                        "neuter",
                        "spay",
                        "castrate",
                        "desex"
                    ],
                    "common_phrases": [
                        "get fixed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стерилизовать или кастрировать животное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кастрировать",
                                "target_lang_sense_clarification": "удалять половые железы (обычно у самцов)"
                            },
                            {
                                "target_lang_word": "стерилизовать",
                                "target_lang_sense_clarification": "лишать способности к размножению (обычно самок)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab444369-2691-3dd7-879a-cb63772c6d22",
                    "sense_definition": "To focus or determine oneself on a concept; to fixate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attach_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She's <w>fixed</w> on the idea of becoming a doctor.",
                            "target_lang_translations": {
                                "ru": "Она <w>зациклилась</w> на идее стать врачом."
                            }
                        },
                        {
                            "text": "He <w>fixed</w> his attention on the small detail in the painting.",
                            "target_lang_translations": {
                                "ru": "Он <w>сосредоточил</w> свое внимание на мелкой детали картины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fix on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточиваться на чем-либо; зацикливаться на идее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зацикливаться",
                                "target_lang_sense_clarification": "навязчиво думать о чем-то"
                            },
                            {
                                "target_lang_word": "сосредоточиваться",
                                "target_lang_sense_clarification": "фокусировать внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04114a93-d74d-36ba-a311-141f9f2b403d",
                    "sense_definition": "To convert into a stable or available form (e.g. nitrogen).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "attach_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Legumes are valued in crop rotation for their ability to <w>fix</w> nitrogen.",
                            "target_lang_translations": {
                                "ru": "Бобовые ценятся в севообороте за их способность <w>фиксировать</w> азот."
                            }
                        },
                        {
                            "text": "Certain bacteria can <w>fix</w> atmospheric nitrogen into the soil.",
                            "target_lang_translations": {
                                "ru": "Определенные бактерии могут <w>связывать</w> атмосферный азот в почве."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобразовывать химический элемент в устойчивую форму (например, азот)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фиксировать",
                                "target_lang_sense_clarification": "усваивать или закреплять химически"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "образовывать химическое соединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f1ec627-52dc-3e5c-9686-264d11fd035b",
                    "sense_definition": "To shoot; to inject a drug.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "drug_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The junkies would often <w>fix</w> in the abandoned building.",
                            "target_lang_translations": {
                                "ru": "Наркоманы часто <w>кололись</w> в заброшенном здании."
                            }
                        },
                        {
                            "text": "He went to the bathroom to <w>fix</w> before the party started.",
                            "target_lang_translations": {
                                "ru": "Он пошел в ванную, чтобы <w>уколоться</w> перед началом вечеринки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "drug culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать наркотик (обычно внутривенно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колоться",
                                "target_lang_sense_clarification": "сленг: делать инъекцию наркотика"
                            },
                            {
                                "target_lang_word": "уколоться",
                                "target_lang_sense_clarification": "сленг: принять дозу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "578bb86c-5da5-3783-b887-99f0ce6f82a9",
                    "sense_definition": "A repair or corrective action.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "repair_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That plumber's <w>fix</w> is much better than the first one's.",
                            "target_lang_translations": {
                                "ru": "<w>Починка</w>, выполненная этим сантехником, намного лучше, чем у первого."
                            }
                        },
                        {
                            "text": "We need a quick <w>fix</w> for this software bug.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстрое <w>решение</w> для этой программной ошибки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "quick fix",
                        "easy fix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решение проблемы, исправление или починка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решение",
                                "target_lang_sense_clarification": "способ устранения проблемы"
                            },
                            {
                                "target_lang_word": "починка",
                                "target_lang_sense_clarification": "ремонт"
                            },
                            {
                                "target_lang_word": "исправление",
                                "target_lang_sense_clarification": "корректировка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a64c474d-04a3-3bbe-bdbf-35f6c1f13b0c",
                    "sense_definition": "A difficult situation; a quandary or dilemma; a predicament.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dilemma_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It rained before we repaired the roof, and were we in a <w>fix</w>!",
                            "target_lang_translations": {
                                "ru": "Дождь пошел до того, как мы починили крышу, и мы попали в <w>переплет</w>!"
                            }
                        },
                        {
                            "text": "He is in a real <w>fix</w> because he lost his wallet and keys.",
                            "target_lang_translations": {
                                "ru": "Он в настоящей <w>беде</w>, потому что потерял кошелек и ключи."
                            }
                        }
                    ],
                    "synonyms": [
                        "dilemma",
                        "predicament",
                        "jam",
                        "pickle",
                        "hole",
                        "spot",
                        "plight",
                        "quandary",
                        "difficulty",
                        "trouble"
                    ],
                    "common_phrases": [
                        "in a fix"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Затруднительное положение, дилемма или неприятная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переплет",
                                "target_lang_sense_clarification": "разг.: трудная ситуация"
                            },
                            {
                                "target_lang_word": "затруднительное положение",
                                "target_lang_sense_clarification": "сложная ситуация"
                            },
                            {
                                "target_lang_word": "беда",
                                "target_lang_sense_clarification": "неприятность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8177a5ee-6be1-3840-b4e4-74eab305de79",
                    "sense_definition": "A determination of location.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attach_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have a <w>fix</w> on your position and help is on the way.",
                            "target_lang_translations": {
                                "ru": "Мы определили ваше <w>местоположение</w>, и помощь уже в пути."
                            }
                        },
                        {
                            "text": "The navigator tried to get a <w>fix</w> using the stars.",
                            "target_lang_translations": {
                                "ru": "Штурман пытался взять <w>пеленг</w> по звездам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get a fix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определение точного местоположения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местоположение",
                                "target_lang_sense_clarification": "координаты"
                            },
                            {
                                "target_lang_word": "пеленг",
                                "target_lang_sense_clarification": "навигационное определение позиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6b2ca21-7f34-3d97-92bb-d68fe8bdd15b",
                    "sense_definition": "A single dose of a narcotic drug, especially when injected.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "drug_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The addict was desperate to find money for his next <w>fix</w>.",
                            "target_lang_translations": {
                                "ru": "Наркоман отчаянно искал деньги на свою следующую <w>дозу</w>."
                            }
                        },
                        {
                            "text": "He needs a <w>fix</w> to stop the withdrawal symptoms.",
                            "target_lang_translations": {
                                "ru": "Ему нужна <w>доза</w>, чтобы прекратить ломку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "need a fix"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "drug culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разовая доза наркотика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доза",
                                "target_lang_sense_clarification": "порция наркотического вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6af9f4e0-cf32-36b5-b584-53507ff1b3ee",
                    "sense_definition": "A prearrangement of the outcome of a supposedly competitive process.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rig_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can tell by the way the case is handled that the <w>fix</w> is in.",
                            "target_lang_translations": {
                                "ru": "По тому, как ведется дело, видно, что имеет место <w>сговор</w>."
                            }
                        },
                        {
                            "text": "The fans suspected a <w>fix</w> when the referee made that call.",
                            "target_lang_translations": {
                                "ru": "Болельщики заподозрили <w>подтасовку</w>, когда судья принял это решение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the fix is in"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сговор или подтасовка результатов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сговор",
                                "target_lang_sense_clarification": "тайное соглашение"
                            },
                            {
                                "target_lang_word": "подтасовка",
                                "target_lang_sense_clarification": "фальсификация"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "fd60f10c-b298-3b00-97af-e2325f7926b6",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Fix</w> is a character in the famous adventure novel.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Фикс</w> — персонаж известного приключенческого романа."
                            }
                        },
                        {
                            "text": "I met a man named John <w>Fix</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил человека по имени Джон <w>Фикс</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "surname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фикс",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
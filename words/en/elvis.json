    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ffef277f-553b-3ccb-bced-1488672405b2",
                    "sense_definition": "Elvis Presley (1935–1977), a highly popular and influential American rock and roll singer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_1",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My grandmother still listens to the songs of <w>Elvis</w> every day.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка до сих пор слушает песни <w>Элвиса</w> каждый день."
                            }
                        },
                        {
                            "text": "Many people believe that <w>Elvis</w> changed the history of music forever.",
                            "target_lang_translations": {
                                "ru": "Многие считают, что <w>Элвис</w> навсегда изменил историю музыки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Elvis has left the building"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элвис Пресли (1935–1977), очень популярный и влиятельный американский певец рок-н-ролла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Элвис",
                                "target_lang_sense_clarification": "певец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0ebe0cb-5f8c-3b5b-9933-5b098e89ad65",
                    "sense_definition": "A male given name.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "name_1",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "They decided to name their newborn son <w>Elvis</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Элвисом</w>."
                            }
                        },
                        {
                            "text": "Is <w>Elvis</w> a common name in your country?",
                            "target_lang_translations": {
                                "ru": "<w>Элвис</w> — распространенное имя в вашей стране?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Элвис",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d112b6b4-b152-3009-95f4-166196867eee",
                    "sense_definition": "An entertainer who impersonates the singer Elvis Presley.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impersonator_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hired a professional <w>Elvis</w> to sing at the wedding reception.",
                            "target_lang_translations": {
                                "ru": "Мы наняли профессионального <w>двойника Элвиса</w>, чтобы он спел на свадебном приеме."
                            }
                        },
                        {
                            "text": "The streets of Las Vegas are often filled with tourists and an occasional <w>Elvis</w>.",
                            "target_lang_translations": {
                                "ru": "Улицы Лас-Вегаса часто заполнены туристами, и иногда встречается <w>Элвис</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "impersonator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Артист, который подражает певцу Элвису Пресли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двойник Элвиса",
                                "target_lang_sense_clarification": "артист-подражатель"
                            },
                            {
                                "target_lang_word": "Элвис",
                                "target_lang_sense_clarification": "в значении подражатель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3e71c1c-0f89-3e27-a6c4-d4caa460e8ae",
                    "sense_definition": "A specific operator in computer programming (often written as ?:) used to handle null values.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tech_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can simplify the code significantly by using an <w>Elvis</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете значительно упростить код, используя <w>элвис-оператор</w>."
                            }
                        },
                        {
                            "text": "The <w>Elvis</w> returns the default value if the variable is null.",
                            "target_lang_translations": {
                                "ru": "<w>Элвис-оператор</w> возвращает значение по умолчанию, если переменная равна null."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Elvis operator"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфический оператор в программировании (часто записывается как ?:), используемый для обработки значений null."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "элвис-оператор",
                                "target_lang_sense_clarification": "программирование"
                            },
                            {
                                "target_lang_word": "оператор Элвис",
                                "target_lang_sense_clarification": "программирование"
                            }
                        ]
                    }
                }
            ]
        }
    ]

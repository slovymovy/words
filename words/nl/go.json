    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1d700f6c-34c9-39ee-b166-4780be438ec0",
                    "sense_definition": "Een uitroep die het startsein geeft voor een wedstrijd of handeling.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "start_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klaar voor de start, <w>go</w>!",
                            "target_lang_translations": {
                                "ru": "На старт, внимание, <w>марш</w>!",
                                "en": "Ready, set, <w>go</w>!"
                            }
                        },
                        {
                            "text": "De scheidsrechter riep <w>go</w> en iedereen begon te rennen.",
                            "target_lang_translations": {
                                "ru": "Судья крикнул «<w>марш</w>», и все побежали.",
                                "en": "The referee shouted <w>go</w> and everyone started running."
                            }
                        }
                    ],
                    "synonyms": [
                        "af",
                        "start"
                    ],
                    "common_phrases": [
                        "klaar voor de start go"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, служащий сигналом к началу соревнования или действия.",
                        "en": "An exclamation serving as the starting signal for a competition or action."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "команда на старте"
                            },
                            {
                                "target_lang_word": "поехали",
                                "target_lang_sense_clarification": "разговорное начало действия"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "start signal"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4694363d-c937-3c95-b563-41e27ea5fccb",
                    "sense_definition": "Toestemming of goedkeuring om met een plan of activiteit te starten.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "approval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We wachten nog op een <w>go</w> van de manager voordat we beginnen.",
                            "target_lang_translations": {
                                "ru": "Мы всё ещё ждем <w>добра</w> от менеджера, прежде чем начать.",
                                "en": "We are still waiting for the <w>go-ahead</w> from the manager before we start."
                            }
                        },
                        {
                            "text": "Zodra we een <w>go</w> hebben, kunnen we het project lanceren.",
                            "target_lang_translations": {
                                "ru": "Как только мы получим <w>добро</w>, мы сможем запустить проект.",
                                "en": "As soon as we get the <w>green light</w>, we can launch the project."
                            }
                        }
                    ],
                    "synonyms": [
                        "groen licht"
                    ],
                    "common_phrases": [
                        "een go geven",
                        "een go krijgen",
                        "no go"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in business contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешение или одобрение на запуск плана или деятельности.",
                        "en": "Permission or approval to start a plan or activity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добро",
                                "target_lang_sense_clarification": "разрешение, одобрение"
                            },
                            {
                                "target_lang_word": "отмашка",
                                "target_lang_sense_clarification": "сигнал к началу"
                            },
                            {
                                "target_lang_word": "зелёный свет",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "go-ahead",
                                "target_lang_sense_clarification": "permission to proceed"
                            },
                            {
                                "target_lang_word": "green light",
                                "target_lang_sense_clarification": "approval"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48c10a56-6e11-3354-b3b0-56eaaa7af6b6",
                    "sense_definition": "Een strategisch bordspel van Oost-Aziatische oorsprong dat gespeeld wordt met zwarte en witte stenen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "board_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij speelt elke week een potje <w>go</w> met zijn vrienden.",
                            "target_lang_translations": {
                                "ru": "Каждую неделю он играет партию в <w>го</w> с друзьями.",
                                "en": "He plays a game of <w>Go</w> with his friends every week."
                            }
                        },
                        {
                            "text": "De regels van <w>go</w> zijn eenvoudig, maar het spel vereist veel inzicht.",
                            "target_lang_translations": {
                                "ru": "Правила <w>го</w> просты, но игра требует глубокого понимания.",
                                "en": "The rules of <w>Go</w> are simple, but the game requires a lot of insight."
                            }
                        }
                    ],
                    "common_phrases": [
                        "potje go"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стратегическая настольная игра восточноазиатского происхождения, в которую играют черными и белыми камнями.",
                        "en": "A strategic board game of East Asian origin played with black and white stones."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "го",
                                "target_lang_sense_clarification": "настольная игра"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "Go",
                                "target_lang_sense_clarification": "board game"
                            }
                        ]
                    }
                }
            ]
        }
    ]

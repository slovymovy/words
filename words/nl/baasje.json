    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd628bcc-1ecf-3db8-9509-a776ee3e7f83",
                    "sense_definition": "Eigenaar van een huisdier, meestal een hond of kat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pet_owner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond was zo blij toen zijn <w>baasje</w> eindelijk thuiskwam.",
                            "target_lang_translations": {
                                "ru": "Собака была так рада, когда её <w>хозяин</w> наконец вернулся домой."
                            }
                        },
                        {
                            "text": "Wie is het <w>baasje</w> van deze mopshond?",
                            "target_lang_translations": {
                                "ru": "Кто <w>хозяин</w> этого мопса?"
                            }
                        }
                    ],
                    "synonyms": [
                        "eigenaar"
                    ],
                    "common_phrases": [
                        "nieuw baasje",
                        "lief baasje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владелец домашнего животного, обычно собаки или кошки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "владелец питомца"
                            },
                            {
                                "target_lang_word": "владелец",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "698ac49d-fb10-3bbd-a2d6-421d309e00e3",
                    "sense_definition": "Een jongen of man, vaak gebruikt om iemand met een opvallend karakter te beschrijven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mark is een vrolijk <w>baasje</w> met rode wangen.",
                            "target_lang_translations": {
                                "ru": "Марк — веселый <w>малый</w> с румяными щеками."
                            }
                        },
                        {
                            "text": "Dat buurjongetje is een pienter <w>baasje</w> dat alles wil weten.",
                            "target_lang_translations": {
                                "ru": "Этот соседский мальчик — смышленый <w>паренёк</w>, который хочет всё знать."
                            }
                        }
                    ],
                    "synonyms": [
                        "ventje",
                        "kereltje",
                        "knul"
                    ],
                    "common_phrases": [
                        "raar baasje",
                        "vrolijk baasje",
                        "vreemd baasje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мальчик или мужчина; часто используется для описания человека с характерными чертами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "малый",
                                "target_lang_sense_clarification": "разговорное, о мужчине или парне"
                            },
                            {
                                "target_lang_word": "паренёк",
                                "target_lang_sense_clarification": "о мальчике или юноше"
                            },
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "субъект, часто с оттенком странности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4d6c478-49d0-30e8-9e84-f84789230acd",
                    "sense_definition": "Verkleinwoord van <w>baas</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gedraagt zich als een klein <w>baasje</w> tegenover zijn vrienden.",
                            "target_lang_translations": {
                                "ru": "Он ведет себя как маленький <w>начальник</w> со своими друзьями."
                            }
                        },
                        {
                            "text": "Zelfs als klein <w>baasje</w> had hij al veel invloed op het schoolplein.",
                            "target_lang_translations": {
                                "ru": "Даже будучи маленьким <w>командиром</w>, он уже имел большое влияние на школьном дворе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова \"baas\" (начальник), часто используется иронично или для описания властного поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начальничек",
                                "target_lang_sense_clarification": "ироничное уменьшительное"
                            },
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "о том, кто любит командовать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

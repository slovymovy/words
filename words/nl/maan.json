    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3495934e-bdad-35de-825d-67bd7a341ebb",
                    "sense_definition": "Het hemellichaam dat als natuurlijke satelliet in een baan om de aarde draait.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "moon_earth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk naar boven, de <w>maan</w> schijnt helder vannacht.",
                            "target_lang_translations": {
                                "ru": "Посмотри наверх, <w>луна</w> ярко светит сегодня ночью.",
                                "en": "Look up, the <w>moon</w> is shining brightly tonight."
                            }
                        },
                        {
                            "text": "Bij volle <w>maan</w> is het buiten licht genoeg om te wandelen.",
                            "target_lang_translations": {
                                "ru": "При полной <w>луне</w> на улице достаточно светло, чтобы гулять.",
                                "en": "During a full <w>moon</w>, it is light enough outside to walk."
                            }
                        }
                    ],
                    "synonyms": [
                        "Luna"
                    ],
                    "common_phrases": [
                        "volle maan",
                        "halve maan",
                        "nieuwe maan",
                        "naar de maan",
                        "de man in de maan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, которое вращается вокруг Земли как естественный спутник.",
                        "en": "The celestial body that orbits the Earth as a natural satellite."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "естественный спутник Земли"
                            },
                            {
                                "target_lang_word": "месяц",
                                "target_lang_sense_clarification": "серп луны"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "moon",
                                "target_lang_sense_clarification": "Earth's natural satellite"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2814d559-2028-3aa2-a483-87328f66bdec",
                    "sense_definition": "Een hemellichaam dat in een baan rond een andere planeet draait.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "moon_generic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De planeet Jupiter heeft tientallen <w>manen</w> in zijn zwaartekrachtveld.",
                            "target_lang_translations": {
                                "ru": "Планета Юпитер удерживает десятки <w>лун</w> в своем гравитационном поле.",
                                "en": "The planet Jupiter has dozens of <w>moons</w> in its gravitational field."
                            }
                        },
                        {
                            "text": "Titan is de grootste <w>maan</w> van Saturnus.",
                            "target_lang_translations": {
                                "ru": "Титан — самая большая <w>луна</w> Сатурна.",
                                "en": "Titan is Saturn's largest <w>moon</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "satelliet",
                        "wachter"
                    ],
                    "common_phrases": [
                        "de manen van Jupiter",
                        "natuurlijke maan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небесное тело, вращающееся по орбите вокруг другой планеты.",
                        "en": "A celestial body that orbits around another planet."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спутник",
                                "target_lang_sense_clarification": "небесное тело"
                            },
                            {
                                "target_lang_word": "луна",
                                "target_lang_sense_clarification": "спутник планеты"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "moon",
                                "target_lang_sense_clarification": "natural satellite of a planet"
                            },
                            {
                                "target_lang_word": "satellite",
                                "target_lang_sense_clarification": "celestial body"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3647a031-3d3b-3e17-9284-1cdfd6d2a5b8",
                    "sense_definition": "De lange haren die op de nek van bepaalde dieren groeien, zoals bij paarden en leeuwen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mane_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het paard had een prachtig gevlochten <w>maan</w> voor de wedstrijd.",
                            "target_lang_translations": {
                                "ru": "У лошади была красиво заплетенная <w>грива</w> перед соревнованиями.",
                                "en": "The horse had a beautifully braided <w>mane</w> for the competition."
                            }
                        },
                        {
                            "text": "De leeuw schudde zijn kop en zijn wilde <w>manen</w> bewogen mee.",
                            "target_lang_translations": {
                                "ru": "Лев тряхнул головой, и его дикая <w>грива</w> пришла в движение.",
                                "en": "The lion shook its head and its wild <w>mane</w> moved along with it."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wapperende manen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинные волосы, растущие на шее некоторых животных, например, лошадей и львов.",
                        "en": "The long hair growing on the neck of certain animals, such as horses and lions."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грива",
                                "target_lang_sense_clarification": "шерсть на шее животного"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "mane",
                                "target_lang_sense_clarification": "hair on an animal's neck"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ecd5ceb-7802-3296-812c-90e8528db89f",
                    "sense_definition": "*Eerste persoon enkelvoud tegenwoordige tijd van* <w>manen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>maan</w> je om voorzichtig te zijn met dat breekbare glas.",

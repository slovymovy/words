{
    "word_family": [
        "traplopen",
        "loper",
        "trap"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "37e6b633-550f-3342-a884-a126c22a9026",
                    "sense_definition": "Een langwerpig tapijt dat in het midden over de treden van een trap ligt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "interior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rode <w>traploper</w> geeft de hal een klassieke en chique uitstraling.",
                            "target_lang_translations": {
                                "ru": "Красная <w>ковровая дорожка</w> придает холлу классический и шикарный вид."
                            }
                        },
                        {
                            "text": "We moeten voorzichtig zijn, want de oude <w>traploper</w> zit een beetje los.",
                            "target_lang_translations": {
                                "ru": "Мы должны быть осторожны, так как старая <w>ковровая дорожка</w> немного отходит."
                            }
                        }
                    ],
                    "synonyms": [
                        "trapbekleding"
                    ],
                    "common_phrases": [
                        "rode traploper",
                        "versleten traploper",
                        "nieuwe traploper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный узкий ковер, который стелется посередине ступеней лестницы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковровая дорожка",
                                "target_lang_sense_clarification": "покрытие на лестнице"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eb6e725-1a0b-3f79-8549-9e43b6b52bf7",
                    "sense_definition": "Iemand die een trap oploopt, vaak in de context van een sportieve wedstrijd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De snelste <w>traploper</w> bereikte de bovenste verdieping in recordtijd.",
                            "target_lang_translations": {
                                "ru": "Самый быстрый <w>бегун по лестнице</w> достиг верхнего этажа за рекордное время."
                            }
                        },
                        {
                            "text": "Als fanatieke <w>traploper</w> traint hij wekelijks in hoge kantoorgebouwen.",
                            "target_lang_translations": {
                                "ru": "Как фанатичный <w>бегун по лестнице</w>, он еженедельно тренируется в высоких офисных зданиях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "snelste traploper",
                        "fanatieke traploper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, бегущий вверх по лестнице, часто в контексте спортивных соревнований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегун по лестнице",
                                "target_lang_sense_clarification": "спортсмен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a97ddb3-b5cc-3bcc-94ce-0a955260e8ef",
                    "sense_definition": "Spiraalvormig speelgoed, meestal van metaal of plastic, dat zelfstandig van de treden van een trap kan afdalen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "toy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen lieten de kleurrijke <w>traploper</w> keer op keer naar beneden gaan.",
                            "target_lang_translations": {
                                "ru": "Дети снова и снова запускали разноцветную <w>пружинку</w> вниз."
                            }
                        },
                        {
                            "text": "Mijn metalen <w>traploper</w> is helaas helemaal in de knoop geraakt.",
                            "target_lang_translations": {
                                "ru": "Моя металлическая <w>пружинка</w>, к сожалению, полностью запуталась."
                            }
                        }
                    ],
                    "synonyms": [
                        "slinky"
                    ],
                    "common_phrases": [
                        "metalen traploper",
                        "plastic traploper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрушка в форме спирали, которая может самостоятельно спускаться по ступеням."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пружинка",
                                "target_lang_sense_clarification": "игрушка"
                            },
                            {
                                "target_lang_word": "слинки",
                                "target_lang_sense_clarification": "название игрушки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "cd2a1e5c-e791-3245-b096-b7a719a12407",
                    "sense_definition": "Uitroep om iemand of een dier aan te sporen om weg te gaan, door te lopen of op te schieten.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"<Vort>!\" riep de boer tegen de hond die in de weg liep.",
                            "target_lang_translations": {
                                "ru": "\"<Прочь>!\" — крикнул фермер собаке, которая путалась под ногами."
                            }
                        },
                        {
                            "text": "<Vort>, ga nu maar snel naar school voordat je te laat komt.",
                            "target_lang_translations": {
                                "ru": "<Марш>, иди скорее в школу, пока не опоздал."
                            }
                        }
                    ],
                    "synonyms": [
                        "vooruit",
                        "hup",
                        "wegwezen"
                    ],
                    "antonyms": [
                        "halt",
                        "stop"
                    ],
                    "common_phrases": [
                        "vort met de geit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый, чтобы прогнать кого-то (человека или животное), или побудить идти быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "приказ уйти"
                            },
                            {
                                "target_lang_word": "брысь",
                                "target_lang_sense_clarification": "команда животному (обычно кошке, но используется шире)"
                            },
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "команда идти или уходить"
                            },
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "побуждение к действию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ab2f2238-6465-3eac-b6c2-ffca4294bcf5",
                    "sense_definition": "Niet meer aanwezig op de verwachte plek; verdwenen of zoekgeraakt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb overal gezocht, maar mijn bril is <vort>.",
                            "target_lang_translations": {
                                "ru": "Я везде искал, но мои очки <пропали>."
                            }
                        },
                        {
                            "text": "Toen de politie eindelijk arriveerde, was de dader al <vort>.",
                            "target_lang_translations": {
                                "ru": "Когда полиция наконец прибыла, преступник уже <исчез>."
                            }
                        }
                    ],
                    "synonyms": [
                        "weg",
                        "kwijt",
                        "verdwenen",
                        "foetsie"
                    ],
                    "antonyms": [
                        "aanwezig",
                        "terug",
                        "gevonden"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in Southern Netherlands and Flanders"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствующий на ожидаемом месте; исчезнувший или потерянный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропавший",
                                "target_lang_sense_clarification": "потерянный"
                            },
                            {
                                "target_lang_word": "исчезнувший",
                                "target_lang_sense_clarification": "скрывшийся из виду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92688856-d2b9-376e-8ba5-1c90c4670f9f",
                    "sense_definition": "In slechte staat van ontbinding verkerend; bedorven of aangetast.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rotten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gooi die <vorte> appels maar op de composthoop.",
                            "target_lang_translations": {
                                "ru": "Выбрось эти <гнилые> яблоки в компостную кучу."
                            }
                        },
                        {
                            "text": "Het kozijn is helemaal <vort> en moet dringend vervangen worden.",
                            "target_lang_translations": {
                                "ru": "Оконная рама совсем <сгнила> и требует срочной замены."
                            }
                        }
                    ],
                    "synonyms": [
                        "rot",
                        "verrot",
                        "bedorven"
                    ],
                    "antonyms": [
                        "vers",
                        "gaaf"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "mainly used in specific dialects"
                        },
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в состоянии разложения; гнилой или испорченный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнилой",
                                "target_lang_sense_clarification": "разложившийся"
                            },
                            {
                                "target_lang_word": "испорченный",
                                "target_lang_sense_clarification": "негодный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f7daae35-6305-3056-b498-f619d295de90",
                    "sense_definition": "Informele variant van <w>voort</w>: verder in tijd of ruimte, of vooruit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "forward",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten <vort> maken als we voor het donker thuis willen zijn.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <поторапливаться>, если мы хотим быть дома до темноты."
                            }
                        },
                        {
                            "text": "En zo leefden ze <vort> zonder zich ergens zorgen over te maken.",
                            "target_lang_translations": {
                                "ru": "И так они жили <дальше>, ни о чём не беспокоясь."
                            }
                        }
                    ],
                    "synonyms": [
                        "verder",
                        "vooruit"
                    ],
                    "antonyms": [
                        "terug",
                        "achteruit"
                    ],
                    "common_phrases": [
                        "vort maken"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальный вариант слова 'voort': дальше во времени или пространстве, или вперёд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальше",
                                "target_lang_sense_clarification": "продолжение действия"
                            },
                            {
                                "target_lang_word": "вперёд",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

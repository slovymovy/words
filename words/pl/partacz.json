            "senses": [
                {
                    "sense_id": "9aef821a-4b14-3ec3-9bbb-ad581bb5b44b",
                    "sense_definition": "Osoba wykonująca swoją pracę niefachowo, niedbale, niechlujnie i bez wymaganych umiejętności.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mechanik to zwykły <w>partacz</w>, bo po jego naprawie samochód wciąż nie działa.",
                            "target_lang_translations": {
                                "ru": "Этот механик — обыкновенный <w>халтурщик</w>, потому что после его ремонта машина всё ещё не работает."
                            }
                        },
                        {
                            "text": "Nie chcę, żeby jakiś <w>partacz</w> remontował mój dom, wolę zapłacić więcej profesjonaliście.",
                            "target_lang_translations": {
                                "ru": "Я не хочу, чтобы какой-то <w>бракодел</w> ремонтировал мой дом, я предпочитаю заплатить больше профессионалу."
                            }
                        }
                    ],
                    "synonyms": [
                        "fuszer",
                        "brakorób",
                        "tandeciarz",
                        "chałturnik",
                        "niedbaluch"
                    ],
                    "antonyms": [
                        "fachowiec",
                        "profesjonalista",
                        "mistrz"
                    ],
                    "common_phrases": [
                        "zwykły partacz",
                        "straszny partacz",
                        "robota partacza"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, выполняющий свою работу непрофессионально, небрежно, халтурно и без необходимых навыков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "халтурщик",
                                "target_lang_sense_clarification": "недобросовестный работник"
                            },
                            {
                                "target_lang_word": "бракодел",
                                "target_lang_sense_clarification": "тот, кто производит некачественную продукцию"
                            },
                            {
                                "target_lang_word": "горе-мастер",
                                "target_lang_sense_clarification": "ироничное название плохого специалиста"
                            },
                            {
                                "target_lang_word": "портач",
                                "target_lang_sense_clarification": "разг. неумелый работник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8b83ffa-80ae-35d1-af6d-f694aa83eef5",
                    "sense_definition": "W dawnym rzemiośle: rzemieślnik nienależący do cechu, wykonujący pracę nielegalnie i zwalczany przez organizacje cechowe.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>partacz</w> przyłapany na nielegalnej produkcji tracił swoje narzędzia i towary.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>внецеховой ремесленник</w>, пойманный на нелегальном производстве, лишался своих инструментов и товаров."
                            }
                        },
                        {
                            "text": "Cechy rzemieślnicze dbały o to, by żaden <w>partacz</w> nie psuł rynku tanimi wyrobami.",
                            "target_lang_translations": {
                                "ru": "Ремесленные цехи заботились о том, чтобы никакой <w>внецеховой мастер</w> не портил рынок дешёвыми изделиями."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeszkodnik",
                        "szturarz"
                    ],
                    "antonyms": [
                        "mistrz cechowy"
                    ],
                    "common_phrases": [
                        "walka z partaczami",
                        "miejski partacz"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В старинном ремесле: ремесленник, не принадлежащий к цеху, работающий нелегально и преследуемый цеховыми организациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внецеховой ремесленник",
                                "target_lang_sense_clarification": "исторический термин"
                            },
                            {
                                "target_lang_word": "партач",
                                "target_lang_sense_clarification": "ремесленник, не входящий в цех"
                            }
                        ]
                    }
                }
            ]

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствовать",
                                "target_lang_sense_clarification": "стандартам или ожиданиям"
                            },
                            {
                                "target_lang_word": "подчиняться",
                                "target_lang_sense_clarification": "правилам или нормам"
                            },
                            {
                                "target_lang_word": "приспосабливаться",
                                "target_lang_sense_clarification": "адаптироваться к условиям"

                    "sense_id": "f1462cf8-c47b-3332-a7fc-a11c89fda60e",
                    "sense_definition": "Het aanhoudend ploeteren en zwoegen om iets voor elkaar te krijgen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lang <w>geworstel</w> met de ingewikkelde handleiding zat de kast eindelijk in elkaar.",
                            "target_lang_translations": {
                                "en": "After a long <w>struggle</w> with the complicated manual, the cabinet was finally assembled.",
                                "ru": "После долгих <w>мучений</w> со сложной инструкцией шкаф был наконец собран."
                            }
                        },
                        {
                            "text": "Het <w>geworstel</w> met de bureaucratie kostte de ondernemer veel tijd en energie.",
                            "target_lang_translations": {
                                "en": "The <w>struggle</w> with bureaucracy cost the entrepreneur a lot of time and energy.",
                                "ru": "<w>Борьба</w> с бюрократией отняла у предпринимателя много времени и сил."
                            }
                        }
                    ],
                    "synonyms": [
                        "geploeter",
                        "gezwoeg",
                        "tobberij"
                    ],
                    "antonyms": [
                        "gemak"
                    ],
                    "common_phrases": [
                        "geworstel met",
                        "eeuwig geworstel"
                    ],
                    "target_lang_definitions": {

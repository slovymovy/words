                {
                    "sense_id": "f7760b22-58b7-3ce3-b220-f6874def5a9e",
                    "sense_definition": "A country located primarily in Western Europe, with Paris as its capital.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "country_sense",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning a trip to <w>France</w> next summer to see the Eiffel Tower.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку во <w>Францию</w> следующим летом, чтобы увидеть Эйфелеву башню."
                            }
                        },
                        {
                            "text": "Paris is the capital city of <w>France</w> and a major cultural center.",
                            "target_lang_translations": {
                                "ru": "Париж — столица <w>Франции</w> и крупный культурный центр."
                            }
                        }
                    ],
                    "common_phrases": [
                        "South of France",
                        "Tour de France",
                        "Vive la France",
                        "Made in France"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна, расположенная преимущественно в Западной Европе, со столицей в Париже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Франция",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03834550-0983-37c2-9d26-4238baa8d1b7",
                    "sense_definition": "The European part of the French Republic, as distinguished from its overseas territories.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "country_sense",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Goods are often shipped from French Guiana to mainland <w>France</w> for distribution.",
                            "target_lang_translations": {
                                "ru": "Товары часто отправляют из Французской Гвианы в материковую <w>Францию</w> для распределения."
                            }
                        },
                        {
                            "text": "The laws apply differently in metropolitan <w>France</w> compared to the overseas regions.",
                            "target_lang_translations": {
                                "ru": "Законы применяются иначе в метропольной <w>Франции</w> по сравнению с заморскими регионами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "metropolitan France",
                        "mainland France"
                    ],
                    "target_lang_definitions": {
                        "ru": "Европейская часть Французской Республики, в отличие от её заморских территорий."

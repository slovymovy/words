            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8a750cc-4cc3-3822-8277-af28e3dc3fbe",
                    "sense_definition": "Het verkeerd begrijpen van wat iemand anders bedoelt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gelukkig is dit <w>misverstand</w> nu uit de weg geruimd.",
                            "target_lang_translations": {
                                "en": "Fortunately, this <w>misunderstanding</w> has now been cleared up.",
                                "ru": "К счастью, это <w>недоразумение</w> теперь улажено."
                            }
                        },
                        {
                            "text": "Door een vervelend <w>misverstand</w> hebben we elkaar gisteren misgelopen.",
                            "target_lang_translations": {
                                "en": "Due to an annoying <w>misunderstanding</w>, we missed each other yesterday.",
                                "ru": "Из-за досадного <w>недоразумения</w> мы вчера разминулись."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een misverstand uit de weg ruimen",
                        "op een misverstand berusten",
                        "een misverstand voorkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of wrongly understanding what someone else means.",
                        "ru": "Неправильное понимание того, что имеет в виду другой человек; ошибка в коммуникации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "misunderstanding",
                                "target_lang_sense_clarification": "failure to understand correctly"
                            },
                            {
                                "target_lang_word": "confusion",
                                "target_lang_sense_clarification": "lack of clarity resulting in a mistake"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "недоразумение",
                                "target_lang_sense_clarification": "ошибка в понимании, неловкая ситуация"
                            },
                            {
                                "target_lang_word": "непонимание",
                                "target_lang_sense_clarification": "отсутствие взаимопонимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92cdb3e1-7b33-3df6-8054-6a8ae2fe71e9",
                    "sense_definition": "Een verkeerde opvatting of een idee dat niet klopt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een <w>misverstand</w> dat vleermuizen blind zijn.",
                            "target_lang_translations": {
                                "en": "It is a <w>misconception</w> that bats are blind.",
                                "ru": "Это <w>заблуждение</w>, что летучие мыши слепы."
                            }
                        },
                        {
                            "text": "Er moet sprake zijn van een <w>misverstand</w>!",
                            "target_lang_translations": {
                                "en": "There must be a <w>misunderstanding</w>!",
                                "ru": "Должно быть, произошло <w>недоразумение</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "misvatting"
                    ],
                    "common_phrases": [
                        "een wijdverbreid misverstand",
                        "een groot misverstand"
                    ],
                    "target_lang_definitions": {
                        "en": "A wrong opinion or an idea that is incorrect.",
                        "ru": "Ошибочное мнение или неверное представление о чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "misconception",
                                "target_lang_sense_clarification": "incorrect belief or view"
                            },
                            {
                                "target_lang_word": "misunderstanding",
                                "target_lang_sense_clarification": "mistaken belief"
                            },
                            {
                                "target_lang_word": "fallacy",
                                "target_lang_sense_clarification": "mistaken belief based on unsound argument"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заблуждение",
                                "target_lang_sense_clarification": "ложное убеждение, миф"
                            },
                            {
                                "target_lang_word": "ошибочное мнение",
                                "target_lang_sense_clarification": "неверный взгляд на вещи"
                            }
                        ]
                    }
                }
            ]
        }

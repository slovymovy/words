            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0cb0134e-69b4-3c52-9be8-23f6a01846df",
                    "sense_definition": "Osoba zatrudniona do pilnowania budynku, terenu lub mienia, dbająca o porządek i bezpieczeństwo.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "guarding_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nocny <stróż> zauważył otwartą bramę i natychmiast powiadomił właściciela.",
                            "target_lang_translations": {
                                "ru": "Ночной <сторож> заметил открытые ворота и немедленно сообщил владельцу."
                            }
                        },
                        {
                            "text": "Mieszkańcy osiedla postanowili zatrudnić <stróża>, aby pilnował parkingu.",
                            "target_lang_translations": {
                                "ru": "Жители жилого комплекса решили нанять <сторожа>, чтобы он охранял парковку."
                            }
                        }
                    ],
                    "synonyms": [
                        "dozorca",
                        "strażnik",
                        "cieć"
                    ],
                    "common_phrases": [
                        "stróż nocny",
                        "stróż budynku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, нанятый для охраны здания, территории или имущества, следящий за порядком и безопасностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сторож",
                                "target_lang_sense_clarification": "охранник имущества или территории"
                            },
                            {
                                "target_lang_word": "вахтёр",
                                "target_lang_sense_clarification": "дежурный на входе в учреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb39da32-b325-3323-ab0f-954c3caf4e69",
                    "sense_definition": "Osoba czuwająca nad czyimś bezpieczeństwem lub dbająca o przestrzeganie zasad i wartości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "guarding_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierzyła, że jej anioł <stróż> uchronił ją przed wypadkiem.",
                            "target_lang_translations": {
                                "ru": "Она верила, что её ангел-<хранитель> уберёг её от несчастного случая."
                            }
                        },
                        {
                            "text": "Policjant powinien być nieskazitelnym <stróżem> prawa i porządku publicznego.",
                            "target_lang_translations": {
                                "ru": "Полицейский должен быть безупречным <стражем> закона и общественного порядка."
                            }
                        }
                    ],
                    "synonyms": [
                        "opiekun",
                        "strażnik"
                    ],
                    "common_phrases": [
                        "anioł stróż",
                        "stróż prawa",
                        "stróż moralności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, оберегающий чью-то безопасность или следящий за соблюдением правил и ценностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранитель",
                                "target_lang_sense_clarification": "в контексте «ангел-хранитель»"
                            },
                            {
                                "target_lang_word": "страж",
                                "target_lang_sense_clarification": "защитник закона или порядка (высокий стиль)"
                            }
                        ]
                    }
                }
            ]
        },

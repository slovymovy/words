    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05106f3d-3834-3854-b3c1-b7c66ebd343d",
                    "sense_definition": "A sport where two opponents punch each other with gloved fists to the head and torso, aiming to score points or achieve a knockout.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He started <w>boxing</w> at a local gym to improve his fitness.",
                            "target_lang_translations": {
                                "ru": "Он начал заниматься <w>боксом</w> в местном спортзале, чтобы улучшить свою физическую форму."
                            }
                        },
                        {
                            "text": "The <w>boxing</w> match attracted a large crowd of spectators.",
                            "target_lang_translations": {
                                "ru": "Матч по <w>боксу</w> привлек большую толпу зрителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pugilism"
                    ],
                    "common_phrases": [
                        "boxing gloves",
                        "boxing match",
                        "boxing ring",
                        "kick-boxing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид спорта, в котором два противника наносят друг другу удары кулаками в перчатках в голову и корпус с целью набрать очки или отправить соперника в нокаут."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бокс",
                                "target_lang_sense_clarification": "вид спорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba82ca3d-6397-3284-adcf-5bd642e1909a",
                    "sense_definition": "Automatic conversion of value types to objects by wrapping them within a heap-allocated reference type in computer programming.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Automatic <w>boxing</w> allows primitive types to be treated as objects.",
                            "target_lang_translations": {
                                "ru": "Автоматическая <w>упаковка</w> позволяет рассматривать примитивные типы как объекты."
                            }
                        },
                        {
                            "text": "Excessive <w>boxing</w> and unboxing can negatively impact application performance.",
                            "target_lang_translations": {
                                "ru": "Чрезмерная <w>упаковка</w> и распаковка могут негативно сказаться на производительности приложения."
                            }
                        }
                    ],
                    "antonyms": [
                        "unboxing"
                    ],
                    "common_phrases": [
                        "boxing and unboxing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Автоматическое преобразование типов значений в объекты путем их обертывания в ссылочный тип в программировании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упаковка",
                                "target_lang_sense_clarification": "программирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37ece21e-262f-311b-a360-fda25c9dc5fd",
                    "sense_definition": "Material used for making boxes or protective casings, often around pipes or machinery.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter installed wooden <w>boxing</w> to hide the unsightly pipes.",
                            "target_lang_translations": {
                                "ru": "Плотник установил деревянный <w>короб</w>, чтобы скрыть неприглядные трубы."
                            }
                        },
                        {
                            "text": "We need to repair the <w>boxing</w> around the electrical wires.",
                            "target_lang_translations": {
                                "ru": "Нам нужно починить <w>обшивку</w> вокруг электрических проводов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pipe boxing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал или конструкция, используемые для создания коробов или защитных оболочек, часто вокруг труб или механизмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "короб",
                                "target_lang_sense_clarification": "конструкция"
                            },
                            {
                                "target_lang_word": "обшивка",
                                "target_lang_sense_clarification": "защитное покрытие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "222fb11d-c485-33df-a388-c5f63a72613e",
                    "sense_definition": "Present participle and gerund of <w>box</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is currently <w>boxing</w> up her books for the move.",
                            "target_lang_translations": {

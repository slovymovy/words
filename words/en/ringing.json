        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b1f568b2-4949-345d-b5a5-c8c725a84372",
                    "sense_definition": "The sound of something that rings.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I woke up to the loud <w>ringing</w> of the alarm clock.",
                            "target_lang_translations": {
                                "ru": "Я проснулся от громкого <w>звонка</w> будильника."
                            }
                        },
                        {
                            "text": "The constant <w>ringing</w> in his ears made it hard to concentrate.",
                            "target_lang_translations": {
                                "ru": "Постоянный <w>звон</w> в ушах мешал ему сосредоточиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ears ringing",
                        "bell ringing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, издаваемый чем-то звенящим или звонящим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звон",
                                "target_lang_sense_clarification": "звучание"
                            },
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "сигнал телефона или будильника"
                            },
                            {
                                "target_lang_word": "гул",
                                "target_lang_sense_clarification": "шум в ушах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87629fb2-7bf1-3aa9-8eef-14163ad07631",
                    "sense_definition": "A technique used in the study of wild birds, by attaching a small, individually numbered, metal or plastic tag to their legs or wings.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bird <w>ringing</w> helps scientists track migration patterns across continents.",
                            "target_lang_translations": {
                                "ru": "<w>Кольцевание</w> птиц помогает ученым отслеживать пути миграции через континенты."
                            }
                        },
                        {
                            "text": "We spent the morning <w>ringing</w> young swallows near the lake.",
                            "target_lang_translations": {
                                "ru": "Мы провели утро, <w>кольцуя</w> молодых ласточек у озера."
                            }
                        }
                    ],
                    "synonyms": [
                        "banding"
                    ],
                    "common_phrases": [
                        "bird ringing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод изучения диких птиц путем прикрепления метки к их лапам или крыльям."
                    },
                    "translations": {
                        "ru": [
                            {

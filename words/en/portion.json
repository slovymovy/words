    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a254db5-30df-3762-8ba7-b44045517bb7",
                    "sense_definition": "A part of a whole that is separated or distinguished from the rest.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A significant <w>portion</w> of the budget is spent on education.",
                            "target_lang_translations": {
                                "ru": "Значительная <w>часть</w> бюджета тратится на образование."
                            }
                        },
                        {
                            "text": "Only a small <w>portion</w> of the ancient wall remains standing today.",
                            "target_lang_translations": {
                                "ru": "Лишь небольшая <w>часть</w> древней стены сохранилась до наших дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "part",
                        "piece",
                        "fragment",
                        "section",
                        "segment"
                    ],
                    "antonyms": [
                        "whole",
                        "entirety"
                    ],
                    "common_phrases": [
                        "significant portion",
                        "large portion",
                        "major portion",
                        "substantial portion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть целого, отделенная или отличающаяся от остального."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часть",
                                "target_lang_sense_clarification": "общего целого"
                            },
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "процент или выделенный объем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b89f60fa-cf2f-395f-aeaf-d1a9f6bfc3ca",
                    "sense_definition": "An amount of food served to one person at a meal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "allocated_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant serves a very generous <w>portion</w> of pasta for lunch.",
                            "target_lang_translations": {
                                "ru": "В ресторане подают очень щедрую <w>порцию</w> пасты на обед."
                            }
                        },
                        {
                            "text": "You should try to eat a smaller <w>portion</w> if you want to lose weight.",
                            "target_lang_translations": {
                                "ru": "Тебе следует стараться съедать меньшую <w>порцию</w>, если хочешь похудеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "serving",
                        "helping"
                    ],
                    "common_phrases": [
                        "generous portion",
                        "small portion",
                        "double portion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество еды, подаваемое одному человеку за один прием пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порция",
                                "target_lang_sense_clarification": "еды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a724775a-5aa0-3007-9954-52afb2826a07",
                    "sense_definition": "A person's destiny or lot in life.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He accepted that suffering was his <w>portion</w> and did not complain.",
                            "target_lang_translations": {
                                "ru": "Он принял то, что страдание было его <w>долей</w>, и не жаловался."
                            }
                        },
                        {
                            "text": "Happiness is not the <w>portion</w> of every person in this story.",
                            "target_lang_translations": {
                                "ru": "Счастье — это не <w>доля</w> каждого человека в этой истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "fate",
                        "destiny",
                        "lot",
                        "fortune",
                        "doom"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or biblical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Судьба или участь человека в жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "судьба"
                            },
                            {
                                "target_lang_word": "участь",
                                "target_lang_sense_clarification": "рок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

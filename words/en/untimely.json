{
    "word_family": [
        "time",
        "timely",
        "timeliness",
        "untimeliness",
        "timing"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "477a0d0b-a39d-3fab-a46c-40d1d59be412",
                    "sense_definition": "Happening or done sooner than expected or usual.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "premature_timing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous actor met a tragic and <w>untimely</w> death at a very young age.",
                            "target_lang_translations": {
                                "ru": "Известный актер встретил трагическую и <w>безвременную</w> смерть в очень молодом возрасте."
                            }
                        },
                        {
                            "text": "An <w>untimely</w> frost destroyed the crops before the harvest could begin.",
                            "target_lang_translations": {
                                "ru": "<w>Преждевременные</w> заморозки уничтожили посевы до того, как мог начаться сбор урожая."
                            }
                        }
                    ],
                    "synonyms": [
                        "premature",
                        "early",
                        "hasty"
                    ],
                    "antonyms": [
                        "late",
                        "overdue"
                    ],
                    "common_phrases": [
                        "untimely death",
                        "untimely end",
                        "untimely demise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий или совершенный раньше, чем ожидалось или обычно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преждевременный",
                                "target_lang_sense_clarification": "наступающий слишком рано"
                            },
                            {
                                "target_lang_word": "безвременный",
                                "target_lang_sense_clarification": "обычно о смерти или кончине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "833ffd2e-503d-3ac8-a995-a190eb2b9635",
                    "sense_definition": "Occurring at a time that is not suitable, convenient, or appropriate.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "bad_timing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>untimely</w> joke offended everyone during the serious meeting.",
                            "target_lang_translations": {
                                "ru": "Его <w>неуместная</w> шутка оскорбила всех во время серьезной встречи."
                            }
                        },
                        {
                            "text": "Please forgive this <w>untimely</w> interruption to your work.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, простите это <w>несвоевременное</w> вмешательство в вашу работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "inopportune",
                        "ill-timed",
                        "mistimed",
                        "unseasonable"
                    ],
                    "antonyms": [
                        "timely",
                        "opportune"
                    ],
                    "common_phrases": [
                        "untimely interruption",
                        "untimely remark",
                        "untimely arrival"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий в неподходящее, неудобное или несоответствующее время"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несвоевременный",
                                "target_lang_sense_clarification": "сделанный не вовремя"
                            },
                            {
                                "target_lang_word": "неуместный",
                                "target_lang_sense_clarification": "не подходящий к ситуации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8ad51616-9769-3eb2-9c46-a3b9eca7dbc5",
                    "sense_definition": "Before the natural or proper time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "premature_timing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blossoms fell <w>untimely</w> to the ground during the heavy storm.",
                            "target_lang_translations": {
                                "ru": "Цветы <w>преждевременно</w> упали на землю во время сильной бури."
                            }
                        },
                        {
                            "text": "He felt that his youth had perished <w>untimely</w> due to the war.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что его молодость <w>безвременно</w> погибла из-за войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "early"
                    ],
                    "antonyms": [
                        "late"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раньше естественного или надлежащего времени"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преждевременно",
                                "target_lang_sense_clarification": "слишком рано"
                            },
                            {
                                "target_lang_word": "безвременно",
                                "target_lang_sense_clarification": "трагически рано, обычно о гибели"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
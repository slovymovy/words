    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "763482e1-9810-3241-868f-be6355ebd99b",
                    "sense_definition": "Something that devours or absorbs lots of money.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "money_drain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restoration of the crumbling castle proved to be a <w>lickpenny</w>.",
                            "target_lang_translations": {
                                "ru": "Реставрация разрушающегося замка оказалась настоящей <w>бездонной бочкой</w>."
                            }
                        },
                        {
                            "text": "He realized too late that the lawsuit was a <w>lickpenny</w> that would ruin him.",
                            "target_lang_translations": {
                                "ru": "Он слишком поздно понял, что этот судебный процесс — <w>разорение</w>, которое его погубит."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "describing a thing or situation that consumes money"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что требует огромных финансовых затрат или поглощает много денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездонная бочка",
                                "target_lang_sense_clarification": "о деле или проекте, требующем бесконечных вложений"
                            },
                            {
                                "target_lang_word": "разорение",
                                "target_lang_sense_clarification": "источник больших расходов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "716fa940-d077-3ad7-96b7-6f05105ae26f",
                    "sense_definition": "A miserly person.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "miser",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>lickpenny</w> refused to spend a single coin on heating his home.",
                            "target_lang_translations": {
                                "ru": "Старый <w>скряга</w> отказывался потратить хоть монету на отопление своего дома."
                            }
                        },
                        {
                            "text": "Don't be such a <w>lickpenny</w> when your family is in need.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>скупердяем</w>, когда твоя семья нуждается."
                            }
                        }
                    ],
                    "antonyms": [
                        "spendthrift"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "referring to a stingy person"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скупой человек, который неохотно расстается с деньгами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скряга",
                                "target_lang_sense_clarification": "жадный человек"
                            },
                            {
                                "target_lang_word": "скупердяй",
                                "target_lang_sense_clarification": "разговорное, неодобрительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "053e4af0-431a-3d0c-a9ba-860eb2204104",
                    "sense_definition": "Expensive.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "expensive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Life in <w>lickpenny</w> London depleted his savings rapidly.",
                            "target_lang_translations": {
                                "ru": "Жизнь в <w>разорительном</w> Лондоне быстро истощила его сбережения."
                            }
                        },
                        {
                            "text": "They could not sustain such a <w>lickpenny</w> lifestyle for long.",
                            "target_lang_translations": {
                                "ru": "Они не могли долго поддерживать такой <w>дорогостоящий</w> образ жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "expensive"
                    ],
                    "antonyms": [
                        "cheap",
                        "inexpensive"
                    ],
                    "common_phrases": [
                        "lickpenny London"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used to describe expensive places or habits"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорогой, требующий больших расходов (обычно о месте или образе жизни)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разорительный",
                                "target_lang_sense_clarification": "ведущий к большим тратам"
                            },
                            {
                                "target_lang_word": "дорогостоящий",
                                "target_lang_sense_clarification": "требующий много денег"
                            }
                        ]
                    }
                }
            ]
        }
    ]

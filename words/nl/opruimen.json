    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "50371530-2876-3bd4-af23-579f0aa21b28",
                    "sense_definition": "Spullen op de juiste plaats leggen en een ruimte netjes maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je kamer <opruimen> voordat je naar buiten mag.",
                            "target_lang_translations": {
                                "en": "You have to <tidy up> your room before you are allowed outside.",
                                "ru": "Ты должен <убрать> свою комнату, прежде чем тебе разрешат выйти на улицу."
                            }
                        },
                        {
                            "text": "Kun jij al het speelgoed van de vloer <opruimen>?",
                            "target_lang_translations": {
                                "en": "Can you <put away> all the toys from the floor?",
                                "ru": "Ты можешь <убрать> все игрушки с пола?"
                            }
                        }
                    ],
                    "synonyms": [
                        "uitschakelen",
                        "elimineren"
                    ],
                    "antonyms": [
                        "vervuilen"
                    ],
                    "common_phrases": [
                        "kamer opruimen",
                        "rommel opruimen",
                        "tafel opruimen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put items in the right place and make a room neat.",
                        "ru": "Класть вещи на свои места и приводить помещение в порядок."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tidy up",
                                "target_lang_sense_clarification": "to make a room or area neat by organizing things"
                            },
                            {
                                "target_lang_word": "clean up",
                                "target_lang_sense_clarification": "general term for making a space neat and clean"
                            },
                            {
                                "target_lang_word": "put away",
                                "target_lang_sense_clarification": "specifically regarding items like toys or clothes"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "убирать",
                                "target_lang_sense_clarification": "наводить чистоту"
                            },
                            {
                                "target_lang_word": "прибирать",
                                "target_lang_sense_clarification": "наводить порядок"
                            },
                            {
                                "target_lang_word": "наводить порядок",
                                "target_lang_sense_clarification": "упорядочивать вещи"
                            }
                        ]

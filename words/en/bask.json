    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2191ffaf-ef04-369d-8ead-b2ac932fa838",
                    "sense_definition": "To lie or sit exposed to pleasant warmth, especially from the sun.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "warmth_pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lizards like to <w>bask</w> on the warm rocks during the afternoon.",
                            "target_lang_translations": {
                                "ru": "Ящерицы любят <w>греться</w> на теплых камнях днем."
                            }
                        },
                        {
                            "text": "We decided to <w>bask</w> in the sunshine while waiting for the bus.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>понежиться</w> на солнышке, пока ждали автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "sunbathe",
                        "lounge",
                        "relax"
                    ],
                    "antonyms": [
                        "shiver",
                        "freeze"
                    ],
                    "common_phrases": [
                        "bask in the sun",
                        "bask in the warmth",
                        "bask in the sunshine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лежать или сидеть, наслаждаясь приятным теплом, особенно солнечным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "греться",
                                "target_lang_sense_clarification": "на солнце или у источника тепла"
                            },
                            {
                                "target_lang_word": "нежиться",
                                "target_lang_sense_clarification": "наслаждаться теплом и комфортом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70482988-bbf9-329d-bf67-a5fce30b4ae1",
                    "sense_definition": "To take great pleasure or satisfaction in something, such as praise, attention, or success.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "warmth_pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The winning team stayed on the field to <w>bask</w> in the applause of the crowd.",
                            "target_lang_translations": {
                                "ru": "Победившая команда осталась на поле, чтобы <w>искупаться</w> в аплодисментах толпы."
                            }
                        },
                        {
                            "text": "She paused for a moment to <w>bask</w> in the glory of her recent achievement.",
                            "target_lang_translations": {
                                "ru": "Она остановилась на мгновение, чтобы <w>насладиться</w> славой своего недавнего достижения."
                            }
                        }
                    ],
                    "synonyms": [
                        "revel",
                        "delight",
                        "savor",
                        "enjoy",
                        "wallow"
                    ],
                    "common_phrases": [
                        "bask in glory",
                        "bask in the attention",
                        "bask in praise",
                        "bask in the glow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать огромное удовольствие или удовлетворение от чего-либо, например, от похвалы или внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купаться",
                                "target_lang_sense_clarification": "в лучах славы, во внимании"
                            },
                            {
                                "target_lang_word": "наслаждаться",
                                "target_lang_sense_clarification": "успехом или моментом"
                            }
                        ]
                    }
                }
            ]
        },
        {

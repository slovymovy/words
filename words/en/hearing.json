{
    "word_family": [
        "hear",
        "hearer",
        "hearsay",
        "overhear",
        "mishear",
        "rehear",
        "unheard"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d0bbbffe-ddc9-34b2-bfd7-518d3f96c4f5",
                    "sense_definition": "The present participle or gerund of <w>hear</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I remember <w>hearing</w> that song on the radio yesterday.",
                            "target_lang_translations": {
                                "ru": "Я помню, как <w>слышал</w> эту песню вчера по радио."
                            }
                        },
                        {
                            "text": "Are you <w>hearing</w> a strange noise coming from the engine?",
                            "target_lang_translations": {
                                "ru": "Ты <w>слышишь</w> странный шум, исходящий от двигателя?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени или герундий от глагола <w>hear</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слышать",
                                "target_lang_sense_clarification": "глагол"
                            },
                            {
                                "target_lang_word": "слышащий",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21d0d288-4d5c-30f6-9ea1-6a10f7d1f932",
                    "sense_definition": "The physical ability or sense used to perceive sound.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather uses a device to help with his poor <w>hearing</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка использует устройство для улучшения своего плохого <w>слуха</w>."
                            }
                        },
                        {
                            "text": "Dogs have a very acute sense of <w>hearing</w>.",
                            "target_lang_translations": {
                                "ru": "У собак очень острый <w>слух</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sense of hearing",
                        "hard of hearing",
                        "hearing loss",
                        "hearing aid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая способность или чувство, используемое для восприятия звука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слух",
                                "target_lang_sense_clarification": "способность слышать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "244ae827-a68c-3e78-b4ee-44b29885c2b5",
                    "sense_definition": "An official meeting where evidence, arguments, or opinions are presented to a judge or committee.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee will hold a public <w>hearing</w> to discuss the new traffic laws.",
                            "target_lang_translations": {
                                "ru": "Комитет проведет публичное <w>слушание</w> для обсуждения новых правил дорожного движения."
                            }
                        },
                        {
                            "text": "She had to testify at a disciplinary <w>hearing</w> last week.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе ей пришлось давать показания на дисциплинарном <w>слушании</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "public hearing",
                        "court hearing",
                        "disciplinary hearing",
                        "preliminary hearing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное заседание, на котором доказательства, аргументы или мнения представляются судье или комитету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушание",
                                "target_lang_sense_clarification": "судебное или официальное заседание"
                            },
                            {
                                "target_lang_word": "разбирательство",
                                "target_lang_sense_clarification": "рассмотрение дела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f553b05-7d8d-3ba7-a684-323213daaec7",
                    "sense_definition": "The distance within which a sound can be heard.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "range",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not discuss confidential matters within <w>hearing</w> of the guests.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не обсуждайте конфиденциальные вопросы в пределах <w>слышимости</w> гостей."
                            }
                        },
                        {
                            "text": "He made sure to stay out of <w>hearing</w> while they argued.",
                            "target_lang_translations": {
                                "ru": "Он старался держаться вне пределов <w>слышимости</w>, пока они спорили."
                            }
                        }
                    ],
                    "synonyms": [
                        "earshot"
                    ],
                    "common_phrases": [
                        "within hearing",
                        "out of hearing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние, в пределах которого можно услышать звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слышимость",
                                "target_lang_sense_clarification": "пределы восприятия звука"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9801352a-f61c-3e95-beac-ecc4db2c44a8",
                    "sense_definition": "Capable of perceiving sound; not deaf.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sign language interpreters help bridge the gap between deaf and <w>hearing</w> people.",
                            "target_lang_translations": {
                                "ru": "Переводчики жестового языка помогают преодолеть разрыв между глухими и <w>слышащими</w> людьми."
                            }
                        },
                        {
                            "text": "The <w>hearing</w> population often misunderstands the challenges faced by the deaf community.",
                            "target_lang_translations": {
                                "ru": "<w>Слышащее</w> население часто неправильно понимает проблемы, с которыми сталкивается сообщество глухих."
                            }
                        }
                    ],
                    "antonyms": [
                        "deaf"
                    ],
                    "common_phrases": [
                        "hearing person",
                        "hearing world",
                        "hearing impaired"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный воспринимать звук; не глухой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слышащий",
                                "target_lang_sense_clarification": "антоним к слову глухой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a11feb85-5925-325f-9117-3312f368ec1e",
                    "sense_definition": "Wyrażenie algebraiczne wskazujące, że jedna wartość jest większa, mniejsza lub różna od drugiej (połączone znakami <, >, ≤, ≥ lub ≠).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby rozwiązał trudną <nierówność> przy tablicy.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика решить сложное <неравенство> у доски."
                            }
                        },
                        {
                            "text": "Ta <nierówność> jest prawdziwa dla wszystkich liczb dodatnich.",
                            "target_lang_translations": {
                                "ru": "Это <неравенство> верно для всех положительных чисел."
                            }
                        }
                    ],
                    "antonyms": [
                        "równanie",
                        "równość"
                    ],
                    "common_phrases": [
                        "rozwiązać nierówność",
                        "nierówność kwadratowa",
                        "znak nierówności",
                        "układ nierówności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Алгебраическое выражение, указывающее на то, что одна величина больше, меньше или не равна другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неравенство",
                                "target_lang_sense_clarification": "математическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "293e0945-2e04-3fae-ad4e-4ac4bc2e55c0",
                    "sense_definition": "Brak równości, sprawiedliwości lub równowagi w społeczeństwie, prawie, szansach lub podziale dóbr.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd wprowadza nowe przepisy, aby zmniejszyć <nierówność> dochodów w społeczeństwie.",
                            "target_lang_translations": {
                                "ru": "Правительство вводит новые правила, чтобы уменьшить <неравенство> доходов в обществе."
                            }
                        },
                        {
                            "text": "Nierówności społeczne często prowadzą do napięć i protestów.",
                            "target_lang_translations": {
                                "ru": "Социальное <неравенство> часто приводит к напряженности и протестам."
                            }
                        }
                    ],
                    "synonyms": [
                        "antyegalitaryzm",
                        "antyegalitarność"
                    ],
                    "antonyms": [
                        "równość",
                        "egalitaryzm",
                        "egalitarność"
                    ],
                    "common_phrases": [
                        "nierówności społeczne",
                        "nierówność płacowa",
                        "wzrost nierówności",
                        "walka z nierównościami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие равенства, справедливости или равновесия в обществе, правах или распределении благ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неравенство",
                                "target_lang_sense_clarification": "социальное, экономическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc849ac5-c96c-3398-a1bf-4f93177ceb0c",
                    "sense_definition": "Brak gładkości powierzchni; miejsce, gdzie teren jest wyboisty, pofałdowany lub posiada zagłębienia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód podskoczył gwałtownie, gdy wjechał na dużą <nierówność> drogi.",
                            "target_lang_translations": {
                                "ru": "Автомобиль резко подпрыгнул, наехав на большую <неровность> на дороге."
                            }
                        },
                        {
                            "text": "Musisz uważać na wszelkie <nierówności> terenu, biegając po lesie.",
                            "target_lang_translations": {
                                "ru": "Нужно быть внимательным к любым <неровностям> рельефа во время бега по лесу."
                            }
                        }
                    ],
                    "antonyms": [
                        "gładkość",
                        "równość"
                    ],
                    "common_phrases": [
                        "nierówność terenu",
                        "nierówności na drodze",
                        "niwelować nierówności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие гладкости поверхности; неровный участок местности, дороги или предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неровность",
                                "target_lang_sense_clarification": "поверхности, рельефа"
                            },
                            {
                                "target_lang_word": "ухаб",
                                "target_lang_sense_clarification": "на дороге"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
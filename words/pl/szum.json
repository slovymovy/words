    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e5ad081-5ce2-333b-862f-a199a3eab320",
                    "sense_definition": "Jednostajny, ciągły dźwięk o niskim natężeniu, przypominający odgłos wiatru, płynącej wody lub pracującego urządzenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uspokajał mnie cichy <w>szum</w> fal uderzających o piaszczysty brzeg.",
                            "target_lang_translations": {
                                "ru": "Меня успокаивал тихий <w>шум</w> волн, ударяющихся о песчаный берег."
                            }
                        },
                        {
                            "text": "W całym biurze słychać było monotonny <w>szum</w> pracujących komputerów i klimatyzacji.",
                            "target_lang_translations": {
                                "ru": "Во всем офисе был слышен монотонный <w>шум</w> работающих компьютеров и кондиционера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szum morza",
                        "szum wiatru",
                        "szum w uszach",
                        "jednostajny szum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монотонный, непрерывный звук низкой интенсивности, напоминающий звук ветра, текущей воды или работающего устройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шум",
                                "target_lang_sense_clarification": "звук"
                            },
                            {
                                "target_lang_word": "гул",
                                "target_lang_sense_clarification": "низкий, монотонный звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d467609-e180-3388-9094-8e18c29d694e",
                    "sense_definition": "Duże zainteresowanie, ożywienie, głośna dyskusja lub zamieszanie wokół jakiejś sprawy, osoby lub wydarzenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commotion_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Premiera nowego filmu wywołała ogromny <w>szum</w> w mediach społecznościowych.",
                            "target_lang_translations": {
                                "ru": "Премьера нового фильма вызвала огромную <w>шумиху</w> в социальных сетях."
                            }
                        },
                        {
                            "text": "Nie rozumiem, dlaczego robisz tyle <w>szumu</w> o taką błahą sprawę.",
                            "target_lang_translations": {
                                "ru": "Я не понимаю, почему ты поднимаешь столько <w>шума</w> из-за такого пустякового дела."
                            }
                        }
                    ],
                    "synonyms": [
                        "ferment",
                        "rejwach"
                    ],
                    "common_phrases": [
                        "robić dużo szumu",
                        "szum medialny",
                        "narobić szumu",
                        "wielki szum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой интерес, оживление, громкое обсуждение или суматоха вокруг какого-либо дела, человека или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шумиха",
                                "target_lang_sense_clarification": "ажиотаж, оживленное обсуждение"
                            },
                            {
                                "target_lang_word": "шум",
                                "target_lang_sense_clarification": "суматоха, переполох"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9eedef8b-5143-31ce-ad56-492c920fadff",
                    "sense_definition": "Zakłócenia sygnału dźwiękowego, radiowego lub wizualnego, utrudniające prawidłowy odbiór informacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare radio gubiło zasięg i wydawało z siebie tylko głośny <w>szum</w>.",
                            "target_lang_translations": {
                                "ru": "Старое радио теряло сигнал и издавало только громкий <w>шум</w>."
                            }
                        },
                        {
                            "text": "Na zdjęciu zrobionym w słabym świetle widać wyraźny cyfrowy <w>szum</w>.",
                            "target_lang_translations": {
                                "ru": "На фотографии, сделанной при слабом освещении, виден отчетливый цифровой <w>шум</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trzaski"
                    ],
                    "common_phrases": [
                        "szum informacyjny",
                        "szumy w eterze",
                        "szum cyfrowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помехи звукового, радио- или визуального сигнала, затрудняющие правильный прием информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шум",
                                "target_lang_sense_clarification": "помехи сигнала"
                            },
                            {
                                "target_lang_word": "помехи",
                                "target_lang_sense_clarification": "искажения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee40e70a-bf25-31e1-a962-313a5e0fdee9",
                    "sense_definition": "Piana z zanieczyszczeniami zbierająca się na powierzchni gotującego się płynu, na przykład rosołu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substance_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie zebrać łyżką <w>szum</w>, aby zupa była idealnie klarowna.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно собрать ложкой <w>накипь</w>, чтобы суп был идеально прозрачным."
                            }
                        },
                        {
                            "text": "Podczas gotowania mięsa na wierzchu wody pojawił się gęsty, szary <w>szum</w>.",
                            "target_lang_translations": {
                                "ru": "Во время варки мяса на поверхности воды появилась густая серая <w>пена</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "szumowina",
                        "piana"
                    ],
                    "common_phrases": [
                        "zebrać szum",
                        "szum na rosole"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пена с загрязнениями, собирающаяся на поверхности кипящей жидкости, например бульона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накипь",
                                "target_lang_sense_clarification": "пена на бульоне"
                            },
                            {
                                "target_lang_word": "пена",
                                "target_lang_sense_clarification": "на поверхности жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6beb0fe1-dff3-3266-8acf-17f8fd539dc3",
                    "sense_definition": "Forma trybu rozkazującego (druga osoba liczby pojedynczej) czasownika <w>szumieć</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>szum</w> tak głośno papierami, bo próbuję się skupić na pracy.",
                            "target_lang_translations": {
                                "ru": "Не <w>шуми</w> так громко бумагами, потому что я пытаюсь сосредоточиться на работе."
                            }
                        },
                        {
                            "text": "Rzeko, <w>szum</w> spokojnie i nie wylewaj wody ze swojego koryta.",
                            "target_lang_translations": {
                                "ru": "Река, <w>шуми</w> спокойно и не выливай воду из своего русла."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative of szumieć"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения (второе лицо единственного числа) глагола \"szumieć\" (шуметь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шуми",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7a79ae87-c73d-3479-ae75-f6ad0e09c6e4",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Szum</w> został wybrany na nowego przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Шум</w> был избран новым председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Na liście obecności widnieje nazwisko <w>Szum</w>, ale nikogo takiego nie ma.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих значится фамилия <w>Шум</w>, но никого такого нет."
                            }
                        }
                    ],
                    "traits": [

            "senses": [
                {
                    "sense_id": "2be615ef-63a0-32d6-ad10-125c3a24b9c4",
                    "sense_definition": "Actually being, existing, or occurring; not fictitious or imaginary.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "existence_truth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer reading stories about <w>real</w> people rather than fantasy characters.",

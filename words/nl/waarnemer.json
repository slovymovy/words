{
    "word_family": [
        "waarnemen",
        "waarneming",
        "waarneembaar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b4e93e8-85b2-330a-bf43-7c15850760c0",
                    "sense_definition": "Iemand die aandachtig kijkt naar wat er gebeurt of iets vaststelt",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "observer_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>waarnemer</w> noteerde nauwkeurig alle details van het experiment.",
                            "target_lang_translations": {
                                "en": "The <w>observer</w> accurately noted all details of the experiment.",
                                "ru": "<w>Наблюдатель</w> точно записал все детали эксперимента."
                            }
                        },
                        {
                            "text": "Als stille <w>waarnemer</w> bleef hij op de achtergrond en zei niets.",
                            "target_lang_translations": {
                                "en": "As a silent <w>observer</w>, he stayed in the background and said nothing.",
                                "ru": "Как молчаливый <w>наблюдатель</w>, он оставался на заднем плане и ничего не говорил."
                            }
                        }
                    ],
                    "synonyms": [
                        "observator",
                        "toeschouwer"
                    ],
                    "common_phrases": [
                        "stille waarnemer",
                        "objectieve waarnemer"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who watches carefully what happens or establishes something",
                        "ru": "Тот, кто внимательно наблюдает за происходящим или фиксирует факты"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "observer",
                                "target_lang_sense_clarification": "someone who watches or notices details"
                            },
                            {
                                "target_lang_word": "onlooker",
                                "target_lang_sense_clarification": "spectator"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наблюдатель",
                                "target_lang_sense_clarification": "человек, следящий за событиями или явлениями"
                            },
                            {
                                "target_lang_word": "очевидец",
                                "target_lang_sense_clarification": "свидетель события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfbea1db-8517-38ec-bedf-542bd8f22ec7",
                    "sense_definition": "Een persoon die officieel is aangesteld om toezicht te houden op een proces of gebeurtenis",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "observer_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Verenigde Naties stuurden een <w>waarnemer</w> om de verkiezingen te controleren.",
                            "target_lang_translations": {
                                "en": "The United Nations sent an <w>observer</w> to monitor the elections.",
                                "ru": "Организация Объединенных Наций направила <w>наблюдателя</w> для контроля за выборами."
                            }
                        },
                        {
                            "text": "Elke internationale <w>waarnemer</w> moest een rapport schrijven over de situatie.",
                            "target_lang_translations": {
                                "en": "Every international <w>observer</w> had to write a report on the situation.",
                                "ru": "Каждый международный <w>наблюдатель</w> должен был написать отчет о ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "toezichthouder",
                        "inspecteur"
                    ],
                    "common_phrases": [
                        "internationale waarnemer",
                        "onafhankelijke waarnemer"
                    ],
                    "target_lang_definitions": {
                        "en": "A person officially appointed to supervise a process or event",
                        "ru": "Лицо, официально назначенное для надзора за процессом или событием"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "observer",
                                "target_lang_sense_clarification": "official monitor, e.g. for elections"
                            },
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "person who checks progress or compliance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наблюдатель",
                                "target_lang_sense_clarification": "официальный представитель для контроля (на выборах, в миссиях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f270ef11-ebe2-39c1-a46b-5c85aa8e2142",
                    "sense_definition": "Iemand die tijdelijk de taken van een ander overneemt",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "substitute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de ziekte van de burgemeester trad hij op als <w>waarnemer</w>.",
                            "target_lang_translations": {
                                "en": "During the mayor's illness, he acted as a <w>substitute</w>.",
                                "ru": "Во время болезни мэра он выступал в качестве <w>исполняющего обязанности</w>."
                            }
                        },
                        {
                            "text": "De fungerend <w>waarnemer</w> heeft de vergadering gisteren geleid.",
                            "target_lang_translations": {
                                "en": "The acting <w>substitute</w> led the meeting yesterday.",
                                "ru": "Действующий <w>заместитель</w> провел вчерашнее собрание."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervanger",
                        "plaatsvervanger"
                    ],
                    "common_phrases": [
                        "fungerend waarnemer"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who temporarily takes over the duties of another",
                        "ru": "Тот, кто временно выполняет обязанности другого лица"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "substitute",
                                "target_lang_sense_clarification": "temporary replacement"
                            },
                            {
                                "target_lang_word": "acting official",
                                "target_lang_sense_clarification": "person temporarily holding a position"
                            },
                            {
                                "target_lang_word": "replacement",
                                "target_lang_sense_clarification": "person taking another's place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "исполняющий обязанности",
                                "target_lang_sense_clarification": "временный руководитель или должностное лицо"
                            },
                            {
                                "target_lang_word": "заместитель",
                                "target_lang_sense_clarification": "лицо, замещающее начальника"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
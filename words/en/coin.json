    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59ba1ff0-22b1-393e-a37c-1162871be69b",
                    "sense_definition": "A flat, usually round piece of metal used as money.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I found a shiny gold <w>coin</w> on the sidewalk.",
                            "target_lang_translations": {
                                "ru": "Я нашел блестящую золотую <w>монету</w> на тротуаре."
                            }
                        },
                        {
                            "text": "He flipped a <w>coin</w> to decide who would go first.",
                            "target_lang_translations": {
                                "ru": "Он подбросил <w>монету</w>, чтобы решить, кто пойдет первым."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flip a coin",
                        "gold coin",
                        "silver coin",
                        "two sides of the same coin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский, обычно круглый кусок металла, используемый в качестве денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монета",
                                "target_lang_sense_clarification": "денежный знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12e4d6c2-3c47-3af2-95cd-51b03798c32e",
                    "sense_definition": "A digital currency or a unit of cryptocurrency.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "money_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bitcoin is the most famous <w>coin</w> on the market.",
                            "target_lang_translations": {
                                "ru": "Биткойн — самая известная <w>монета</w> на рынке."
                            }
                        },
                        {
                            "text": "Investors are looking for the next stable <w>coin</w> to buy.",
                            "target_lang_translations": {
                                "ru": "Инвесторы ищут следующую стабильную <w>монету</w> для покупки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stable coin",
                        "meme coin",
                        "buy coin"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for cryptocurrency or cryptocoin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровая валюта или единица криптовалюты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монета",
                                "target_lang_sense_clarification": "криптовалюта"
                            },
                            {
                                "target_lang_word": "коин",
                                "target_lang_sense_clarification": "сленг, криптовалюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fa19f6f-fe95-3428-8275-cbf1984f6ff5",
                    "sense_definition": "A flat disc used like money in machines or games, often in a specific establishment.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "money_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need a special <w>coin</w> to operate the laundry machine.",
                            "target_lang_translations": {
                                "ru": "Вам нужен специальный <w>жетон</w>, чтобы запустить стиральную машину."
                            }
                        },
                        {
                            "text": "The casino gave him a <w>coin</w> to play the slot machine.",
                            "target_lang_translations": {
                                "ru": "В казино ему дали <w>жетон</w>, чтобы сыграть на игровом автомате."
                            }
                        }
                    ],
                    "synonyms": [
                        "chip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский диск, используемый вместо денег в автоматах или играх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жетон",
                                "target_lang_sense_clarification": "для автоматов"
                            },
                            {
                                "target_lang_word": "фишка",
                                "target_lang_sense_clarification": "для игр, казино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ca556db-2c39-31c3-8b70-b44fdddec7e4",
                    "sense_definition": "Money in general, especially implying a significant amount.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "money_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spent some serious <w>coin</w> on that new luxury car.",
                            "target_lang_translations": {
                                "ru": "Она потратила серьезные <w>бабки</w> на эту новую роскошную машину."
                            }
                        },
                        {
                            "text": "They are making a lot of <w>coin</w> from their new business venture.",
                            "target_lang_translations": {
                                "ru": "Они делают кучу <w>денег</w> на своем новом бизнес-проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "money",
                        "dough",
                        "bread",
                        "moola",
                        "dosh"
                    ],
                    "common_phrases": [
                        "make coin",
                        "serious coin"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги в целом, особенно подразумевая значительную сумму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабки",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "деньги",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a20beef-042b-3238-8186-0585dfc91453",
                    "sense_definition": "A small, round slice of food, such as a vegetable.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cut the carrots into thin <w>coins</w> before boiling them.",
                            "target_lang_translations": {
                                "ru": "Нарежьте морковь тонкими <w>кружочками</w> перед варкой."
                            }
                        },
                        {
                            "text": "The chef garnished the dish with <w>coins</w> of radish.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар украсил блюдо <w>кружочками</w> редиса."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой круглый ломтик еды, например, овоща."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружочек",
                                "target_lang_sense_clarification": "форма нарезки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "788e6dbb-0d8b-3a8e-9aa4-6a4b94e0f2b5",
                    "sense_definition": "To invent or create a new word or phrase.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Shakespeare is known to <w>coin</w> many words we use today.",
                            "target_lang_translations": {
                                "ru": "Шекспир известен тем, что <w>придумал</w> много слов, которые мы используем сегодня."
                            }
                        },
                        {
                            "text": "The scientist <w>coined</w> a term to describe the new species.",
                            "target_lang_translations": {
                                "ru": "Ученый <w>придумал</w> термин для описания нового вида."
                            }
                        }
                    ],
                    "synonyms": [
                        "invent",
                        "originate"
                    ],
                    "common_phrases": [
                        "coin a phrase",
                        "coin a term",
                        "coin a word"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изобрести или создать новое слово или фразу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придумать",
                                "target_lang_sense_clarification": "сочинить слово"
                            },
                            {
                                "target_lang_word": "ввести",
                                "target_lang_sense_clarification": "ввести в употребление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00cdeb5b-4cfd-3d34-b60c-5323a6dc8e1c",
                    "sense_definition": "To manufacture coins from metal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manufacturing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to <w>coin</w> more silver dollars this year.",
                            "target_lang_translations": {
                                "ru": "Правительство решило <w>отчеканить</w> больше серебряных долларов в этом году."
                            }
                        },
                        {
                            "text": "They used a heavy press to <w>coin</w> the commemorative medals.",
                            "target_lang_translations": {
                                "ru": "Они использовали тяжелый пресс, чтобы <w>отчеканить</w> памятные медали."
                            }
                        }
                    ],
                    "synonyms": [
                        "mint",
                        "manufacture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изготавливать монеты из металла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чеканить",
                                "target_lang_sense_clarification": "изготавливать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "422162ff-fbe8-3285-b990-551da004721b",
                    "sense_definition": "To earn or acquire money rapidly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "earning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With sales booming, the company is really <w>coining</w> it in.",
                            "target_lang_translations": {
                                "ru": "С ростом продаж компания реально <w>гребет</w> деньги лопатой."
                            }
                        },
                        {
                            "text": "He thought he would <w>coin</w> money with his new invention.",
                            "target_lang_translations": {
                                "ru": "Он думал, что будет <w>делать</w> деньги на своем новом изобретении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coin it in"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро зарабатывать или получать деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загребать",
                                "target_lang_sense_clarification": "деньги"
                            },
                            {
                                "target_lang_word": "делать",
                                "target_lang_sense_clarification": "деньги"
                            }
                        ]
                    }
                }
            ]
        },

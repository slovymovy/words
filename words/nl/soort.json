                    "sense_id": "e80edd29-98b4-3fe4-b17e-e7ded39a1e02",
                    "sense_definition": "Een groep voorwerpen, mensen of andere entiteiten die bepaalde kenmerken gemeenschappelijk hebben en zich daardoor onderscheiden van andere groepen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hou niet van dit <w>soort</w> films omdat ze te eng zijn.",
                            "target_lang_translations": {
                                "ru": "Я не люблю этот <w>тип</w> фильмов, потому что они слишком страшные."
                            }
                        },
                        {
                            "text": "Heeft u nog meer van dit <w>soort</w> boeken in de bibliotheek?",
                            "target_lang_translations": {
                                "ru": "У вас есть еще книги такого <w>типа</w> в библиотеке?"
                            }
                        },
                        {
                            "text": "Het smaakt naar een <w>soort</w> van zoete appel.",
                            "target_lang_translations": {
                                "ru": "Это по вкусу напоминает какой-то <w>сорт</w> сладкого яблока."
                            }
                        }
                    ],
                    "synonyms": [
                        "type",
                        "categorie",
                        "genre",
                        "aard"
                    ],
                    "common_phrases": [

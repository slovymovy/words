    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66c14b5b-b7e4-3b4e-9633-ab2228020301",
                    "sense_definition": "A commercial business or enterprise that produces goods or services to make a profit.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "business_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works for a large insurance <w>company</w> in the city center.",
                            "target_lang_translations": {
                                "ru": "Она работает в крупной страховой <w>компании</w> в центре города."
                            }
                        },
                        {
                            "text": "The <w>company</w> announced a significant increase in profits this year.",
                            "target_lang_translations": {
                                "ru": "<w>Компания</w> объявила о значительном росте прибыли в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "business",
                        "firm",
                        "enterprise",
                        "corporation"
                    ],
                    "common_phrases": [
                        "software company",
                        "parent company",
                        "limited company",
                        "start a company"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коммерческое предприятие или организация, производящая товары или услуги для получения прибыли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компа́ния",
                                "target_lang_sense_clarification": "организация, бизнес"
                            },
                            {
                                "target_lang_word": "фи́рма",
                                "target_lang_sense_clarification": "коммерческое предприятие"
                            },
                            {
                                "target_lang_word": "предприя́тие",
                                "target_lang_sense_clarification": "завод, фабрика или организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb4a4f6a-e36d-374a-bf83-d23f1bde6fda",
                    "sense_definition": "The state of being with someone else; the feeling of fellowship or friendship.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really enjoy your <w>company</w> when we go for walks.",
                            "target_lang_translations": {
                                "ru": "Мне очень приятно твое <w>общество</w>, когда мы гуляем."
                            }
                        },
                        {
                            "text": "He kept me <w>company</w> while I waited for the bus to arrive.",
                            "target_lang_translations": {
                                "ru": "Он составил мне <w>компанию</w>, пока я ждал прибытия автобуса."

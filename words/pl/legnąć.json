    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83ecddb1-1828-3071-a648-17aba513ce78",
                    "sense_definition": "Położyć się, przyjąć pozycję leżącą, zazwyczaj w celu odpoczynku lub snu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_rest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony wędrowiec postanowił <legnąć> pod drzewem, aby chwilę odpocząć.",
                            "target_lang_translations": {
                                "ru": "Уставший путник решил <прилечь> под деревом, чтобы немного отдохнуть."
                            }
                        },
                        {
                            "text": "Pies zaszczekał cicho i posłusznie <legł> u stóp swojego pana.",
                            "target_lang_translations": {
                                "ru": "Собака тихо залаяла и послушно <легла> у ног своего хозяина."
                            }
                        }
                    ],
                    "synonyms": [
                        "położyć się",
                        "spocząć"
                    ],
                    "common_phrases": [
                        "legnąć do snu",
                        "legnąć na trawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лечь, принять лежачее положение, обычно для отдыха или сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечь",
                                "target_lang_sense_clarification": "принять горизонтальное положение"
                            },
                            {
                                "target_lang_word": "прилечь",
                                "target_lang_sense_clarification": "лечь ненадолго, чтобы отдохнуть"
                            },
                            {
                                "target_lang_word": "улечься",
                                "target_lang_sense_clarification": "устроиться лежать поудобнее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e1e9fb2-3f4a-39d0-9a4f-4b59a3598bc7",
                    "sense_definition": "Ponieść śmierć, zginąć, zazwyczaj podczas walki lub wojny.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "death_battle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu dzielnych żołnierzy <legło> w obronie niepodległości kraju.",
                            "target_lang_translations": {
                                "ru": "Много храбрых солдат <полегло> при защите независимости страны."
                            }
                        },
                        {
                            "text": "Rycerz <legł> w boju, trafiony strzałą nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Рыцарь <пал> в бою, сраженный вражеской стрелой."
                            }
                        }
                    ],
                    "synonyms": [
                        "paść"
                    ],
                    "common_phrases": [
                        "legnąć w boju",
                        "legnąć na polu chwały"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Погибнуть, умереть, обычно в бою или на войне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "погибнуть в бою (высокий стиль)"
                            },
                            {
                                "target_lang_word": "полечь",
                                "target_lang_sense_clarification": "погибнуть во множестве (о солдатах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd8a860a-cbee-3571-9209-6520c70dd1bf",
                    "sense_definition": "Przewrócić się, upaść na ziemię z dużą siłą lub ulec zniszczeniu (często o budynkach lub drzewach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fall_collapse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary dąb z wielkim hukiem <legł> na ziemię podczas wichury.",
                            "target_lang_translations": {
                                "ru": "Старый дуб с сильным грохотом <повалился> на землю во время бури."
                            }
                        },
                        {
                            "text": "Po trzęsieniu ziemi całe miasto <legło> w gruzach.",
                            "target_lang_translations": {
                                "ru": "После землетрясения весь город <рухнул>, превратившись в руины."
                            }
                        }
                    ],
                    "synonyms": [
                        "upaść"
                    ],
                    "common_phrases": [
                        "legnąć w gruzach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упасть на землю с силой или разрушиться (часто о зданиях или деревьях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рухнуть",
                                "target_lang_sense_clarification": "тяжело упасть или обвалиться"
                            },
                            {
                                "target_lang_word": "повалиться",
                                "target_lang_sense_clarification": "упасть на бок (о деревьях, предметах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
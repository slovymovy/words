                {
                    "sense_id": "dbf09feb-abb5-36a3-bff5-203307bddfae",
                    "sense_definition": "Mieć na sobie ubranie, ozdoby lub inne elementy stroju",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą muszę <nosić> ciepłą czapkę i wełniany szalik.",
                            "target_lang_translations": {
                                "ru": "Зимой я должен <носить> теплую шапку и шерстяной шарф."
                            }
                        },
                        {
                            "text": "Mój starszy brat lubi <nosić> sportowe buty do pracy.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат любит <носить> спортивную обувь на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieć na sobie",
                        "ubierać"
                    ],
                    "common_phrases": [
                        "nosić okulary",
                        "nosić spodnie",
                        "nosić biżuterię",
                        "nosić żałobę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть одетым в одежду, носить украшения или другие элементы наряда"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носить",
                                "target_lang_sense_clarification": "одежду, аксессуары"

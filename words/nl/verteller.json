{
    "word_family": [
        "vertellen",
        "vertelling",
        "vertelsel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "847efd5a-4f83-30bd-9c0e-ab52791959ec",
                    "sense_definition": "Iemand die een verhaal vertelt aan een publiek",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "storyteller",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen in de bibliotheek luisterden ademloos naar de <w>verteller</w>.",
                            "target_lang_translations": {
                                "ru": "Дети в библиотеке, затаив дыхание, слушали <w>рассказчика</w>."
                            }
                        },
                        {
                            "text": "Mijn opa is een geboren <w>verteller</w> en kent honderden grappige anekdotes.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка — прирожденный <w>рассказчик</w> и знает сотни забавных анекдотов."
                            }
                        }
                    ],
                    "synonyms": [
                        "verhalenverteller"
                    ],
                    "antonyms": [
                        "luisteraar"
                    ],
                    "common_phrases": [
                        "een geboren verteller"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто рассказывает историю публике"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассказчик",
                                "target_lang_sense_clarification": "человек, рассказывающий истории"
                            },
                            {
                                "target_lang_word": "сказочник",
                                "target_lang_sense_clarification": "тот, кто рассказывает сказки или небылицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4e90548-9214-304f-a05f-36b04f08cbeb",
                    "sense_definition": "De stem of figuur die in een boek, film of toneelstuk de gebeurtenissen beschrijft",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "narrator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het toneelstuk praatte de <w>verteller</w> de delen van het verhaal aan elkaar.",
                            "target_lang_translations": {
                                "ru": "Во время спектакля <w>рассказчик</w> связывал части истории воедино."
                            }
                        },
                        {
                            "text": "De alwetende <w>verteller</w> geeft de lezer inzicht in de gedachten van alle personages.",
                            "target_lang_translations": {
                                "ru": "Всеведущий <w>повествователь</w> дает читателю представление о мыслях всех персонажей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de alwetende verteller",
                        "de ik-verteller"
                    ],
                    "target_lang_definitions": {
                        "ru": "Голос или персонаж, описывающий события в книге, фильме или пьесе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повествователь",
                                "target_lang_sense_clarification": "литературный термин"
                            },
                            {
                                "target_lang_word": "рассказчик",
                                "target_lang_sense_clarification": "голос за кадром или в тексте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_given",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Her middle name is <w>Bell</w>, named after her grandmother.",
                            "target_lang_translations": {
                                "ru": "Ее второе имя — <w>Белл</w>, в честь бабушки."
                            }
                        },
                        {
                            "text": "Little <w>Bell</w> played happily in the garden.",
                            "target_lang_translations": {
                                "ru": "Маленькая <w>Белл</w> весело играла в саду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly 19th century"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя, часто вариант имени Белль или сокращение от Изабелла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Белл",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "230c7f01-dd7e-336a-8ec9-96b133993790",
                    "sense_definition": "Umieścić coś bezpośrednio na powierzchni czegoś innego lub bardzo blisko, tak aby przedmioty się stykały.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <przyłożyć> kartę do czytnika, żeby otworzyć drzwi.",
                            "target_lang_translations": {
                                "ru": "Ты должен <приложить> карту к считывателю, чтобы открыть дверь."
                            }
                        },
                        {
                            "text": "Lekarz kazał mi <przyłożyć> zimny okład do opuchniętej nogi.",
                            "target_lang_translations": {
                                "ru": "Врач велел мне <приложить> холодный компресс к опухшей ноге."
                            }
                        }
                    ],
                    "synonyms": [
                        "przytknąć",
                        "przycisnąć"
                    ],
                    "antonyms": [
                        "odsunąć",
                        "zabrać"
                    ],
                    "common_phrases": [
                        "przyłożyć ucho",
                        "przyłożyć rękę",
                        "przyłożyć lód",
                        "przyłożyć palec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить что-то непосредственно на поверхность чего-то другого или очень близко, чтобы предметы соприкасались."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приложить",
                                "target_lang_sense_clarification": "поместить вплотную к чему-либо"
                            },
                            {
                                "target_lang_word": "прислонить",
                                "target_lang_sense_clarification": "приставить, опирая на что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fbf1737-fd6d-3386-b032-f8f887ce4185",
                    "sense_definition": "Wykonać jakąś pracę lub zadanie z dużym zaangażowaniem i starannością (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli chcesz zdać ten trudny egzamin, musisz się bardziej <przyłożyć>.",
                            "target_lang_translations": {
                                "ru": "Если хочешь сдать этот трудный экзамен, ты должен больше <постараться>."
                            }
                        },
                        {
                            "text": "Uczeń postanowił <przyłożyć> się do nauki matematyki w tym semestrze.",
                            "target_lang_translations": {
                                "ru": "Ученик решил <налечь> на математику в этом семестре."
                            }
                        }
                    ],
                    "synonyms": [
                        "postarać się",
                        "skupić się"
                    ],
                    "antonyms": [
                        "zaniedbać",
                        "olać"
                    ],
                    "common_phrases": [
                        "przyłożyć się do nauki",
                        "przyłożyć się do pracy",
                        "przyłożyć się do obowiązków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнить работу или задачу с большим усердием и старанием (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постараться",
                                "target_lang_sense_clarification": "приложить усилия, сделать качественно"
                            },
                            {
                                "target_lang_word": "налечь",
                                "target_lang_sense_clarification": "разг. усердно заняться чем-либо"
                            },
                            {
                                "target_lang_word": "взяться",
                                "target_lang_sense_clarification": "приступить к делу с энергией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07c1a009-576a-39dc-b37c-882d00538fa9",
                    "sense_definition": "Mocno kogoś uderzyć.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chuligan chciał <przyłożyć> przechodniowi, ale na szczęście się potknął.",
                            "target_lang_translations": {
                                "ru": "Хулиган хотел <врезать> прохожему, но, к счастью, споткнулся."
                            }

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вьющийся",
                                "target_lang_sense_clarification": "о растениях"
                            },
                            {
                                "target_lang_word": "плетистый",
                                "target_lang_sense_clarification": "о розах и кустарниках"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c30810dc-34c2-3e86-80e5-79029ded7211",
                    "sense_definition": "Ziek zijn of een medische aandoening hebben.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_suffering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader <w>lijdt</w> al jaren aan reuma.",
                            "target_lang_translations": {
                                "en": "My grandfather has been <w>suffering</w> from rheumatism for years.",
                                "ru": "Мой дедушка уже много лет <w>страдает</w> от ревматизма."
                            }
                        },
                        {
                            "text": "Hij <w>lijdt</w> aan een zeldzame ziekte die moeilijk te behandelen is.",
                            "target_lang_translations": {
                                "en": "He <w>suffers</w> from a rare disease that is difficult to treat.",
                                "ru": "Он <w>страдает</w> редким заболеванием, которое трудно поддается лечению."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lijden aan",
                        "lijden onder"
                    ],
                    "target_lang_definitions": {
                        "en": "To be sick or have a medical condition.",
                        "ru": "Болеть или иметь медицинское заболевание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "from an illness or condition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "страдать",
                                "target_lang_sense_clarification": "от болезни или недуга"
                            },
                            {
                                "target_lang_word": "болеть",
                                "target_lang_sense_clarification": "иметь заболевание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "280e3ab6-6db9-3770-91e9-cb75ce59c852",
                    "sense_definition": "Pijn, verdriet of ellende ervaren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_suffering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vluchtelingen moesten vreselijke kou <w>lijden</w> tijdens hun tocht.",
                            "target_lang_translations": {
                                "en": "The refugees had to <w>suffer</w> terrible cold during their journey.",
                                "ru": "Беженцам пришлось <w>терпеть</w> ужасный холод во время своего путешествия."
                            }
                        },
                        {
                            "text": "Zij heeft veel moeten <w>lijden</w> na het verlies van haar man.",
                            "target_lang_translations": {
                                "en": "She had to <w>suffer</w> a lot after the loss of her husband.",
                                "ru": "Ей пришлось много <w>страдать</w> после потери мужа."
                            }
                        }
                    ],
                    "synonyms": [
                        "leed",
                        "lijdenis"
                    ],
                    "common_phrases": [
                        "honger lijden",
                        "kou lijden",
                        "pijn lijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience pain, grief, or misery.",
                        "ru": "Испытывать боль, горе или страдания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "pain or hardship"
                            },
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "hardship or difficulty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "страдать",
                                "target_lang_sense_clarification": "мучиться"
                            },
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "переносить трудности или боль"
                            },
                            {
                                "target_lang_word": "мучиться",
                                "target_lang_sense_clarification": "испытывать муки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "258c6e35-56c7-3081-808c-0c438585f832",
                    "sense_definition": "Schade, verlies of nadeel ondervinden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf heeft vorig jaar een groot verlies <w>geleden</w>.",
                            "target_lang_translations": {
                                "en": "The company <w>suffered</w> a major loss last year.",
                                "ru": "В прошлом году компания <w>понесла</w> большие убытки."
                            }
                        },
                        {
                            "text": "Door de zware storm hebben veel huizen schade <w>geleden</w>.",
                            "target_lang_translations": {
                                "en": "Many houses <w>suffered</w> damage due to the heavy storm.",
                                "ru": "Из-за сильного шторма многие дома <w>пострадали</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verlies lijden",
                        "schade lijden",
                        "nederlaag lijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience damage, loss, or disadvantage.",
                        "ru": "Нести ущерб, потери или убытки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "loss or damage"
                            },
                            {
                                "target_lang_word": "sustain",
                                "target_lang_sense_clarification": "damage or injury"
                            },
                            {
                                "target_lang_word": "incur",
                                "target_lang_sense_clarification": "financial loss"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "убытки или потери"
                            },
                            {
                                "target_lang_word": "потерпеть",
                                "target_lang_sense_clarification": "поражение или неудачу"
                            },
                            {
                                "target_lang_word": "пострадать",
                                "target_lang_sense_clarification": "получить повреждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25028ae6-c834-3562-b068-2764129c1cdb",
                    "sense_definition": "Iemand of iets kunnen verdragen of aardig vinden (meestal gebruikt in ontkenningen of met 'mogen').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_tolerate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan die arrogante man echt niet <w>lijden</w>.",
                            "target_lang_translations": {
                                "en": "I really can't <w>stand</w> that arrogant man.",
                                "ru": "Я правда не <w>выношу</w> этого высокомерного мужчину."
                            }
                        },
                        {
                            "text": "Zij mag haar nieuwe collega wel <w>lijden</w>.",
                            "target_lang_translations": {
                                "en": "She quite <w>likes</w> her new colleague.",
                                "ru": "Ей вполне <w>нравится</w> её новый коллега."
                            }
                        }
                    ],
                    "synonyms": [
                        "leed"
                    ],
                    "common_phrases": [
                        "niet kunnen lijden",
                        "wel mogen lijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to tolerate or like someone or something (usually used in negatives or with 'mogen').",
                        "ru": "Быть в состоянии выносить или любить кого-то (обычно используется в отрицаниях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "tolerate (usually negative)"
                            },
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "put up with"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "endure someone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "терпеть кого-то"
                            },
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "мириться с кем-то"
                            },
                            {
                                "target_lang_word": "переваривать",
                                "target_lang_sense_clarification": "разг. выносить"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3787acc5-a824-39d6-9bfc-9e52ec1453f6",
                    "sense_definition": "Het ervaren van pijn, verdriet of ellende.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_noun_suffering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oorlog veroorzaakte veel <w>lijden</w> onder de bevolking.",
                            "target_lang_translations": {
                                "en": "The war caused much <w>suffering</w> among the population.",
                                "ru": "Война причинила много <w>страданий</w> населению."
                            }
                        },
                        {
                            "text": "De arts probeerde het <w>lijden</w> van de patiënt te verzachten.",
                            "target_lang_translations": {
                                "en": "The doctor tried to alleviate the patient's <w>suffering</w>.",
                                "ru": "Врач пытался облегчить <w>страдания</w> пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "leed"
                    ],
                    "common_phrases": [
                        "menselijk lijden",
                        "onnodig lijden"
                    ],
                    "target_lang_definitions": {
                        "en": "The experience of pain, grief, or misery.",
                        "ru": "Переживание боли, горя или мучений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffering",
                                "target_lang_sense_clarification": "noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "страдание",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "мучение",
                                "target_lang_sense_clarification": "сильная боль или переживание"
                            }
                        ]
                    }
                }
            ]
        }
    ]

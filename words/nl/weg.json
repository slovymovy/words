                {
                    "sense_id": "9f4e7db6-fdb7-309b-8075-aef553351706",
                    "sense_definition": "Naar een andere plaats toe; verwijderd van de huidige locatie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adv_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep boos van huis <w>weg</w>.",
                            "target_lang_translations": {

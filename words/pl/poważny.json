                {
                    "sense_id": "faa5f32c-0052-3b9c-b8dc-97179c7a6110",
                    "sense_definition": "Dotyczący twórczości artystycznej o wysokich walorach estetycznych i głębokiej treści (zazwyczaj w odniesieniu do muzyki klasycznej).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra uwielbia słuchać muzyki <w>poważnej</w>, zwłaszcza utworów Chopina.",
                            "target_lang_translations": {

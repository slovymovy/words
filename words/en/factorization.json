    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a08a06b1-a8b7-327e-b014-6775684e0d50",
                    "sense_definition": "The mathematical process of finding the factors of a number or algebraic expression.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students practiced prime <w>factorization</w> to solve the math problem.",
                            "target_lang_translations": {
                                "ru": "Студенты практиковали <w>разложение</w> на простые множители для решения математической задачи."
                            }
                        },
                        {
                            "text": "Computer security often relies on the difficulty of the <w>factorization</w> of very large numbers.",
                            "target_lang_translations": {
                                "ru": "Компьютерная безопасность часто полагается на сложность <w>факторизации</w> очень больших чисел."
                            }
                        }
                    ],
                    "synonyms": [
                        "factoring"
                    ],
                    "antonyms": [
                        "multiplication",
                        "expansion"
                    ],
                    "common_phrases": [
                        "prime factorization",
                        "unique factorization",
                        "integer factorization"
                    ],
                    "target_lang_definitions": {
                        "ru": "Математический процесс нахождения множителей числа или алгебраического выражения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разложение на множители",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "факторизация",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55f608e2-a9d6-3f62-8830-64f077b43198",
                    "sense_definition": "An expression or list showing the factors that multiply together to produce a specific quantity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "math_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor wrote the correct <w>factorization</w> of the polynomial on the board.",
                            "target_lang_translations": {
                                "ru": "Профессор написал на доске правильное <w>разложение</w> многочлена."
                            }
                        },
                        {
                            "text": "We checked the <w>factorization</w> by multiplying the terms back together.",
                            "target_lang_translations": {
                                "ru": "Мы проверили <w>разложение</w>, перемножив члены обратно."
                            }
                        }
                    ],
                    "antonyms": [
                        "product"
                    ],
                    "common_phrases": [
                        "matrix factorization"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение или список множителей, которые при перемножении дают исходную величину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разложение",
                                "target_lang_sense_clarification": "результат"
                            }
                        ]
                    }
                }
            ]
        }
    ]

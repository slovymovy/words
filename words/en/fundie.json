                {
                    "sense_id": "5d8c9893-95ee-3efc-8811-d9885131db24",
                    "sense_definition": "An informal term for a <w>fundamentalist</w> Christian.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_fundamentalist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>fundies</w> are picketing outside the city hall to protest the new law.",
                            "target_lang_translations": {
                                "ru": "<w>Фундаменталисты</w> пикетируют здание мэрии в знак протеста против нового закона."
                            }
                        },
                        {
                            "text": "He was raised by <w>fundies</w> but eventually decided to leave the church.",
                            "target_lang_translations": {
                                "ru": "Его воспитали <w>религиозные фанатики</w>, но в конце концов он решил уйти из церкви."
                            }
                        }
                    ],
                    "synonyms": [
                        "fundamentalist"
                    ],
                    "antonyms": [
                        "liberal",
                        "progressive"
                    ],
                    "common_phrases": [
                        "right-wing fundie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное название христианского фундаменталиста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фундаменталист",
                                "target_lang_sense_clarification": "сторонник консервативных религиозных взглядов"
                            },
                            {
                                "target_lang_word": "религиозный фанатик",
                                "target_lang_sense_clarification": "пренебрежительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5faab673-99bf-3077-95f2-d6ea3394210e",
                    "sense_definition": "An informal term for a religious <w>fundamentalist</w> of any faith.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_fundamentalist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a Jewish <w>fundie</w> who follows every dietary rule strictly.",
                            "target_lang_translations": {
                                "ru": "Он иудейский <w>фундаменталист</w>, который строго соблюдает все правила питания."
                            }
                        },
                        {
                            "text": "The term <w>fundie</w> can apply to extremists of any religion, not just Christianity.",
                            "target_lang_translations": {
                                "ru": "Термин <w>фундаменталист</w> может применяться к экстремистам любой религии, а не только христианства."
                            }
                        }
                    ],
                    "synonyms": [

                            {
                                "target_lang_word": "забрать",
                                "target_lang_sense_clarification": "выиграть, получить себе"
                            },
                            {
                                "target_lang_word": "отхватить",
                                "target_lang_sense_clarification": "разг. получить что-то ценное"

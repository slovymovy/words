    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1f1883ec-50d6-3d2d-807c-b1dd1d5bb14f",
                    "sense_definition": "Przestarzały piec węglowy, który emituje dużo dymu i zanieczyszczeń, często niespełniający norm ekologicznych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "smog_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy jak najszybciej wymienić ten stary <w>kopciuch</w> na ekologiczną pompę ciepła.",
                            "target_lang_translations": {
                                "ru": "Мы должны как можно скорее заменить эту старую <w>коптилку</w> на экологичный тепловой насос."
                            }
                        },
                        {
                            "text": "Sąsiad znowu rozpalił w swoim <w>kopciuchu</w>, przez co na całej ulicy czuć gryzący dym.",
                            "target_lang_translations": {
                                "ru": "Сосед снова растопил свою <w>коптилку</w>, из-за чего на всей улице стоит едкий дым."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wymiana kopciucha",
                        "stary kopciuch",
                        "likwidacja kopciuchów"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "powszechnie używane w kontekście smogu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшая угольная печь, выделяющая много дыма и загрязнений, часто не соответствующая экологическим нормам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коптилка",
                                "target_lang_sense_clarification": "разговорное, о дымящей печи"
                            },
                            {
                                "target_lang_word": "старая угольная печь",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "735e8b29-323b-3d25-841c-39ec828d4d2f",
                    "sense_definition": "Osoba brudna, zaniedbana lub ubrana niechlujnie; ktoś, kto się ubrudził sadzą lub brudem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dirty_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko wróciło z podwórka wyglądając jak mały <w>kopciuch</w>, całe w błocie i kurzu.",
                            "target_lang_translations": {
                                "ru": "Ребенок вернулся со двора, выглядя как маленький <w>чумазый</w>, весь в грязи и пыли."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>kopciuchem</w> i idź wreszcie uprać swoje ubrania.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>грязнулей</w> и иди наконец постирай свою одежду."
                            }
                        }
                    ],
                    "synonyms": [
                        "brudas",
                        "niechluj",
                        "fleja",
                        "flejtuch",
                        "kocmołuch",
                        "smoluch"
                    ],
                    "common_phrases": [
                        "straszny kopciuch",
                        "wyglądać jak kopciuch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często o dzieciach lub żartobliwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грязный, неопрятный человек; тот, кто испачкался сажей или грязью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязнуля",
                                "target_lang_sense_clarification": "неопрятный человек"
                            },
                            {
                                "target_lang_word": "замарашка",
                                "target_lang_sense_clarification": "испачканный человек"
                            },
                            {
                                "target_lang_word": "чумазый",
                                "target_lang_sense_clarification": "разговорное, испачканный сажей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9441d167-2998-3eb6-835f-0a5502d04fce",
                    "sense_definition": "Papieros niskiej jakości, często tani lub własnoręcznie skręcany, wydzielający nieprzyjemny zapach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "smoking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek siedział na ławce i palił śmierdzącego <w>kopciucha</w>.",
                            "target_lang_translations": {
                                "ru": "Дедушка сидел на скамейке и курил вонючую <w>цигарку</w>."
                            }
                        },
                        {
                            "text": "Nie wiem, jak możesz palić te tanie <w>kopciuchy</w>, one strasznie gryzą w gardło.",
                            "target_lang_translations": {
                                "ru": "Не знаю, как ты можешь курить эти дешевые <w>вонючие сигареты</w>, они ужасно дерут горло."
                            }
                        }
                    ],
                    "common_phrases": [
                        "palić kopciucha",
                        "śmierdzący kopciuch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сигарета низкого качества, часто дешевая или самокрутка, издающая неприятный запах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цигарка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "вонючая сигарета",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "deff8036-1f34-3417-bf2d-680adaef41c5",
                    "sense_definition": "Pojazd lub maszyna w złym stanie technicznym, która wydziela nadmierną ilość spalin.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "smog_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary autobus to straszny <w>kopciuch</w>, zostawia za sobą czarną chmurę dymu.",
                            "target_lang_translations": {
                                "ru": "Этот старый автобус — жуткая <w>коптилка</w>, он оставляет за собой черное облако дыма."
                            }
                        },
                        {
                            "text": "Policja zatrzymała tego <w>kopciucha</w> do kontroli z powodu nadmiernej emisji spalin.",
                            "target_lang_translations": {
                                "ru": "Полиция остановила эту <w>коптилку</w> для проверки из-за чрезмерного выброса выхлопных газов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство или машина в плохом техническом состоянии, выделяющая чрезмерное количество выхлопных газов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коптилка",
                                "target_lang_sense_clarification": "разговорное, о дымящем транспорте"
                            },
                            {
                                "target_lang_word": "дымовуха",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9efee910-9936-33e5-90d5-ee43b35a4233",
                    "sense_definition": "Zgrubienie od słowa <w>kopciuszek</w> (nazwa ptaka lub postaci z bajki).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To nie był delikatny kopciuszek, ale wielki, szary <w>kopciuch</w>.",
                            "target_lang_translations": {
                                "ru": "Это была не изящная горихвостка, а огромная серая <w>птица</w>."
                            }
                        },
                        {
                            "text": "Autor użył słowa <w>kopciuch</w>, aby podkreślić brzydotę postaci w tej wersji opowieści.",
                            "target_lang_translations": {
                                "ru": "Автор использовал слово <w>замарашка</w>, чтобы подчеркнуть уродство персонажа в этой версии рассказа."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zgrubienie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличительная форма от слова «kopciuszek» (название птицы или персонажа сказки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страшилище",
                                "target_lang_sense_clarification": "в контексте уродства"
                            },
                            {
                                "target_lang_word": "замарашка",
                                "target_lang_sense_clarification": "грубая форма"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "overeenkomst",
        "overeenkomen",
        "overeenstemmen",
        "overeenkomstig",
        "overeenstemming"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a0084cce-097f-3eb5-b3a4-44fec553ff8f",
                    "sense_definition": "Op een manier die gelijk is aan of past bij iets anders; in harmonie of overeenstemming.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "matching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleur van de nieuwe gordijnen komt mooi <w>overeen</w> met het tapijt.",
                            "target_lang_translations": {
                                "en": "The color of the new curtains <w>matches</w> the carpet beautifully.",
                                "ru": "Цвет новых штор прекрасно <w>сочетается</w> с ковром."
                            }
                        },
                        {
                            "text": "Komt een liter water exact <w>overeen</w> met een kilogram water?",
                            "target_lang_translations": {
                                "en": "Does a liter of water <w>correspond</w> exactly to a kilogram of water?",
                                "ru": "Литр воды точно <w>соответствует</w> килограмму воды?"
                            }
                        }
                    ],
                    "synonyms": [
                        "gelijk",
                        "conform"
                    ],
                    "antonyms": [
                        "afwijkend",
                        "verschillend"
                    ],
                    "common_phrases": [
                        "overeenkomen met",
                        "overeenstemmen met"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that is equal to or fits with something else; in harmony or accordance.",
                        "ru": "В соответствии, гармонии или совпадении с чем-либо; аналогично чему-то другому."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "match",
                                "target_lang_sense_clarification": "to fit or be similar to"
                            },
                            {
                                "target_lang_word": "correspond",
                                "target_lang_sense_clarification": "to be equivalent to"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "соответствовать",
                                "target_lang_sense_clarification": "быть аналогичным или эквивалентным"
                            },
                            {
                                "target_lang_word": "совпадать",
                                "target_lang_sense_clarification": "быть одинаковым, накладываться"
                            },
                            {
                                "target_lang_word": "сочетаться",
                                "target_lang_sense_clarification": "гармонировать (о цветах, стиле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44a479af-e87c-348d-9e01-4f59631b3869",
                    "sense_definition": "Tot een wederzijdse afspraak, besluit of goedkeuring (voornamelijk gebruikt in combinatie met werkwoorden).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directie en de vakbond kwamen gisteren <w>overeen</w> over de nieuwe arbeidsvoorwaarden.",
                            "target_lang_translations": {
                                "en": "The management and the union <w>agreed</w> on the new employment conditions yesterday.",
                                "ru": "Руководство и профсоюз вчера <w>договорились</w> о новых условиях труда."
                            }
                        },
                        {
                            "text": "Zij kwamen <w>overeen</w> hier gezamenlijk een eind aan te maken.",
                            "target_lang_translations": {
                                "en": "They <w>agreed</w> to put an end to this together.",
                                "ru": "Они <w>договорились</w> вместе положить этому конец."
                            }
                        }
                    ],
                    "synonyms": [
                        "akkoord",
                        "eens"
                    ],
                    "antonyms": [
                        "oneens"
                    ],
                    "common_phrases": [
                        "iets overeenkomen",
                        "overeen te komen"
                    ],
                    "target_lang_definitions": {
                        "en": "Towards a mutual arrangement, decision, or approval (mainly used in combination with verbs).",
                        "ru": "Касательно взаимного соглашения, решения или договоренности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agree",
                                "target_lang_sense_clarification": "to reach a consensus"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "договориться",
                                "target_lang_sense_clarification": "прийти к общему решению"
                            },
                            {
                                "target_lang_word": "согласовать",
                                "target_lang_sense_clarification": "утвердить совместно"
                            },
                            {
                                "target_lang_word": "сойтись",
                                "target_lang_sense_clarification": "прийти к единому мнению"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
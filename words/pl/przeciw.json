                {
                    "sense_id": "a62730df-8c32-3cc6-93f8-c07dee93b424",
                    "sense_definition": "Używany do wyrażenia braku zgody, protestu lub wrogiego nastawienia wobec kogoś lub czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "opposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem zdecydowanie <w>przeciw</w> wprowadzaniu takich zmian w regulaminie.",
                            "target_lang_translations": {
                                "ru": "Я решительно <w>против</w> внесения таких изменений в регламент."
                            }
                        },
                        {
                            "text": "Większość mieszkańców głosowała <w>przeciw</w> budowie nowej fabryki.",
                            "target_lang_translations": {
                                "ru": "Большинство жителей проголосовало <w>против</w> строительства новой фабрики."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwko",
                        "wbrew"
                    ],
                    "antonyms": [
                        "za"
                    ],
                    "common_phrases": [
                        "głosować przeciw",
                        "być przeciw",
                        "mieć coś przeciw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения несогласия, протеста или враждебного отношения к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "выражение несогласия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df2c70c2-2ae0-3ea1-aff5-57b4898a253a",
                    "sense_definition": "Wskazuje na przeciwnika w walce, sporze, grze lub rywalizacji sportowej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna narodowa zagra jutro mecz <w>przeciw</w> mistrzom świata.",
                            "target_lang_translations": {
                                "ru": "Наша сборная сыграет завтра матч <w>против</w> чемпионов мира."
                            }
                        },
                        {
                            "text": "W dawnych czasach rycerze często walczyli <w>przeciw</w> sobie w turniejach.",
                            "target_lang_translations": {
                                "ru": "В давние времена рыцари часто сражались <w>против</w> друг друга на турнирах."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwko"
                    ],
                    "common_phrases": [
                        "grać przeciw komuś",
                        "walczyć przeciw wrogom",
                        "wystąpić przeciw komuś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на противника в борьбе, споре, игре или спортивном состязании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "в противостоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e1a2b56-f0a8-3970-bd09-8296d6e2ba62",
                    "sense_definition": "Wskazuje na coś, co chce się zwalczyć, czemu się zapobiega lub przed czym się chroni (np. chorobę, ból, zagrożenie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "remedy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz przepisał pacjentowi silny antybiotyk <w>przeciw</w> zapaleniu płuc.",
                            "target_lang_translations": {
                                "ru": "Врач выписал пациенту сильный антибиотик <w>против</w> воспаления легких."
                            }
                        },
                        {
                            "text": "To jest bardzo skuteczny środek <w>przeciw</w> komarom i kleszczom.",
                            "target_lang_translations": {
                                "ru": "Это очень эффективное средство <w>против</w> комаров и клещей."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwko"
                    ],
                    "common_phrases": [
                        "lek przeciw bólowi",
                        "szczepionka przeciw grypie",
                        "środek przeciw owadom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на то, с чем борются, что предотвращают или от чего защищаются (например, болезнь, боль, угроза)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "для борьбы с чем-либо"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "лекарство или средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0eac019-46c7-3953-be52-f9024a35baa1",
                    "sense_definition": "Wskazuje kierunek ruchu odwrotny do działania jakiejś siły, np. wiatru lub nurtu wody.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo trudno jest płynąć łódką <w>przeciw</w> silnemu prądowi rzeki.",
                            "target_lang_translations": {
                                "ru": "Очень трудно плыть на лодке <w>против</w> сильного течения реки."
                            }
                        },
                        {
                            "text": "Szliśmy <w>przeciw</w> wiatrowi, więc odczuwaliśmy zimno znacznie bardziej.",
                            "target_lang_translations": {
                                "ru": "Мы шли <w>против</w> ветра, поэтому чувствовали холод гораздо сильнее."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwko",
                        "wbrew"
                    ],
                    "common_phrases": [
                        "płynąć przeciw prądowi",
                        "iść przeciw wiatrowi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает направление движения, противоположное действию какой-либо силы, например, ветра или течения воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "против",

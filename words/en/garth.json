                },
                {
                    "sense_id": "f41e68e0-da28-3a6e-a27c-126100ed16d3",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Garth</w> has been teaching history at the high school for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Гарт</w> преподает историю в старшей школе уже двадцать лет."
                            }
                        },
                        {
                            "text": "The <w>Garth</w> family estate is located just outside the village.",
                            "target_lang_translations": {
                                "ru": "Родовое поместье семьи <w>Гарт</w> находится сразу за деревней."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""

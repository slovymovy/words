{
    "word_family": [
        "napar",
        "naparzyć",
        "parzyć",
        "zaparzać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb1c25da-b3c9-32e7-86fb-6632e6bbd241",
                    "sense_definition": "Zalewać wrzątkiem zioła, herbatę lub inne składniki roślinne w celu przygotowania napoju leczniczego lub aromatycznego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "brewing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia zawsze <w>naparzała</w> miętę, gdy bolał mnie brzuch.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда <w>заваривала</w> мяту, когда у меня болел живот."
                            }
                        },
                        {
                            "text": "Należy <w>naparzać</w> te zioła pod przykryciem przez około dziesięć minut.",
                            "target_lang_translations": {
                                "ru": "Эти травы нужно <w>настаивать</w> под крышкой около десяти минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "parzyć",
                        "zaparzać"
                    ],
                    "common_phrases": [
                        "naparzać zioła",
                        "naparzać herbatę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заливать кипятком травы, чай или другие растительные ингредиенты для приготовления лечебного или ароматного напитка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заваривать",
                                "target_lang_sense_clarification": "чай, травы"
                            },
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "делать настой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86cd1675-40ea-3b5e-b091-d9e5638f81a6",
                    "sense_definition": "Uderzać kogoś lub coś z dużą siłą i częstotliwością; bić intensywnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bokser <w>naparzał</w> w worek treningowy, przygotowując się do ważnej walki.",
                            "target_lang_translations": {
                                "ru": "Боксер <w>колотил</w> по боксерской груше, готовясь к важному бою."
                            }
                        },
                        {
                            "text": "Nie możesz tak mocno <w>naparzać</w> w klawiaturę, bo ją zniszczysz.",
                            "target_lang_translations": {
                                "ru": "Нельзя так сильно <w>долбить</w> по клавиатуре, иначе ты ее сломаешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "parzyć",
                        "zaparzać"
                    ],
                    "common_phrases": [
                        "naparzać pięściami",
                        "naparzać w drzwi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить кого-то или что-то с большой силой и частотой; интенсивно избивать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лупить",
                                "target_lang_sense_clarification": "сильно бить (разг.)"
                            },
                            {
                                "target_lang_word": "колотить",
                                "target_lang_sense_clarification": "ударять многократно"
                            },
                            {
                                "target_lang_word": "дубасить",
                                "target_lang_sense_clarification": "сильно бить (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4618dee5-fbf2-3748-b405-2189fe045d45",
                    "sense_definition": "Bić się z kimś wzajemnie; uczestniczyć w bójce (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopaki z sąsiedztwa często <w>naparzali</w> się na boisku po szkole.",
                            "target_lang_translations": {
                                "ru": "Местные парни часто <w>дрались</w> на площадке после школы."
                            }
                        },
                        {
                            "text": "Dwa dzikie koty <w>naparzały</w> się głośno na dachu garażu.",
                            "target_lang_translations": {
                                "ru": "Два диких кота громко <w>дрались</w> на крыше гаража."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naparzać się z kimś",
                        "naparzać się na pięści"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Драться с кем-то; участвовать в потасовке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "драться",
                                "target_lang_sense_clarification": "вступать в бой"
                            },
                            {
                                "target_lang_word": "махаться",
                                "target_lang_sense_clarification": "драться на кулаках (сленг)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
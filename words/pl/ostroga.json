{
    "word_family": [
        "ostry",
        "ostrzyć",
        "ostrość",
        "ostrogowiec"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "062552ab-bef8-3e54-b88c-289fab25a457",
                    "sense_definition": "Metalowy przedmiot w kształcie łuku, często zakończony kolcem lub kółkiem, mocowany do buta jeźdźca i służący do popędzania konia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "horse_riding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeździec lekko dotknął boków konia <ostrogami>, aby przyspieszyć.",
                            "target_lang_translations": {
                                "ru": "Всадник слегка коснулся боков лошади <шпорами>, чтобы ускориться."
                            }
                        },
                        {
                            "text": "Dźwięk jego <ostróg> roznosił się echem po kamiennej posadzce.",
                            "target_lang_translations": {
                                "ru": "Звон его <шпор> разносился эхом по каменному полу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "złota ostroga",
                        "przypiąć ostrogi",
                        "brzęczeć ostrogami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический предмет в форме дужки, часто с колесиком или шипом, прикрепляемый к сапогу всадника для управления лошадью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпора",
                                "target_lang_sense_clarification": "для верховой езды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6204d28-e090-3ab5-a394-dd5524e98bea",
                    "sense_definition": "Czynnik, który pobudza do działania, motywuje lub inspiruje kogoś do zrobienia czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "motivation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krytyka ze strony szefa była dla niego <ostrogą> do dalszego rozwoju.",
                            "target_lang_translations": {
                                "ru": "Критика со стороны начальника стала для него <стимулом> к дальнейшему развитию."
                            }
                        },
                        {
                            "text": "Potrzebował silnej <ostrogi>, aby w końcu zrealizować swoje marzenia.",
                            "target_lang_translations": {
                                "ru": "Ему нужен был сильный <толчок>, чтобы наконец осуществить свои мечты."
                            }
                        }
                    ],
                    "synonyms": [
                        "bodziec"
                    ],
                    "common_phrases": [
                        "być ostrogą",
                        "stanowić ostrogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фактор, который побуждает к действию, мотивирует или вдохновляет кого-либо на поступки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стимул",
                                "target_lang_sense_clarification": "побуждение"
                            },
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "импульс к действию"
                            },
                            {
                                "target_lang_word": "побуждение",
                                "target_lang_sense_clarification": "мотивация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0d484a7-f1ea-382a-a34b-8c0386b278d6",
                    "sense_definition": "Bolesna narośl kostna tworząca się w obrębie kości piętowej, utrudniająca chodzenie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_pathology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił specjalne wkładki ortopedyczne na <ostrogę> piętową.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал специальные ортопедические стельки от пяточной <шпоры>."
                            }
                        },
                        {
                            "text": "Moja babcia skarży się na ból stopy spowodowany przez <ostrogę>.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка жалуется на боль в стопе, вызванную <шпорой>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ostroga piętowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезненный костный нарост на пяточной кости, затрудняющий ходьбу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпора",
                                "target_lang_sense_clarification": "медицина, пяточная"
                            },
                            {
                                "target_lang_word": "пяточная шпора",
                                "target_lang_sense_clarification": "заболевание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc7a3ca7-18ed-3127-8091-40adfe678d4c",
                    "sense_definition": "Ostry, zrogowaciały wyrostek występujący na tylnej części nogi u niektórych ptaków, zwłaszcza samców kuraków.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "zoology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kogut bronił swojego stada, atakując przeciwnika ostrymi <ostrogami>.",
                            "target_lang_translations": {
                                "ru": "Петух защищал свое стадо, атакуя противника острыми <шпорами>."
                            }
                        },
                        {
                            "text": "Długość <ostrogi> pozwala określić przybliżony wiek bażanta.",
                            "target_lang_translations": {
                                "ru": "Длина <шпоры> позволяет определить приблизительный возраст фазана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ostrogi koguta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый роговой вырост на ногах некоторых птиц, особенно самцов куриных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпора",
                                "target_lang_sense_clarification": "у птиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e5f0d7c-30c6-3eeb-897b-7877dc5bfa92",
                    "sense_definition": "Budowla wodna wysunięta poprzecznie w głąb rzeki lub morza, służąca do ochrony brzegu przed wymywaniem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fale rozbijały się o drewnianą <ostrogę> wybiegającą w morze.",
                            "target_lang_translations": {
                                "ru": "Волны разбивались о деревянную <шпору>, уходящую в море."
                            }
                        },
                        {
                            "text": "Inżynierowie zaprojektowali system <ostróg>, aby zatrzymać erozję plaży.",
                            "target_lang_translations": {
                                "ru": "Инженеры спроектировали систему <шпор>, чтобы остановить эрозию пляжа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ostroga brzegowa",
                        "ostroga rzeczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гидротехническое сооружение, выступающее поперек течения реки или в море для защиты берега от размыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпора",
                                "target_lang_sense_clarification": "гидротехника"
                            },
                            {
                                "target_lang_word": "буна",
                                "target_lang_sense_clarification": "берегозащитная"
                            },
                            {
                                "target_lang_word": "полузапруда",
                                "target_lang_sense_clarification": "речная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ab0d365-ee9f-3191-94fb-46ead33a261d",
                    "sense_definition": "Wydłużona, rurkowata część płatka korony lub działki kielicha kwiatu, w której zazwyczaj gromadzi się nektar.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niektóre storczyki posiadają bardzo długie <ostrogi> wypełnione nektarem.",
                            "target_lang_translations": {
                                "ru": "У некоторых орхидей есть очень длинные <шпорцы>, наполненные нектаром."
                            }
                        },
                        {
                            "text": "Owad musi mieć długi aparat gębowy, aby sięgnąć do dna <ostrogi>.",
                            "target_lang_translations": {
                                "ru": "Насекомое должно иметь длинный ротовой аппарат, чтобы достать до дна <шпорца>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удлиненная трубчатая часть лепестка или чашелистика цветка, где обычно скапливается нектар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпорец",
                                "target_lang_sense_clarification": "ботаника"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "12acab79-e3c2-3e3e-a94d-31d357e8a48f",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Ostroga> jest znanym lokalnym działaczem społecznym.",
                            "target_lang_translations": {
                                "ru": "Господин <Острога> — известный местный общественный деятель."
                            }
                        },
                        {
                            "text": "Na liście obecności widnieje nazwisko Anny <Ostrogi>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих значится фамилия Анны <Остроги>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, используемая в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Острога",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
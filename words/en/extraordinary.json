        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "db0dfa86-7831-3744-b30e-0fbb136bd2d0",
                    "sense_definition": "Not ordinary; exceptional or unusual in a way that attracts attention.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality_unusual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police took <w>extraordinary</w> measures to ensure safety during the parade.",
                            "target_lang_translations": {
                                "ru": "Полиция приняла <w>чрезвычайные</w> меры для обеспечения безопасности во время парада."
                            }
                        },
                        {
                            "text": "It is quite <w>extraordinary</w> that they managed to finish the project so quickly.",
                            "target_lang_translations": {
                                "ru": "Довольно <w>необычно</w>, что им удалось закончить проект так быстро."
                            }
                        }
                    ],
                    "synonyms": [
                        "exceptional",
                        "noteworthy"
                    ],
                    "antonyms": [
                        "ordinary",
                        "normal",
                        "usual",
                        "everyday",
                        "regular"
                    ],
                    "common_phrases": [
                        "extraordinary circumstances",
                        "extraordinary meeting",
                        "extraordinary measures",
                        "extraordinary event"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обычный; исключительный или необычный, привлекающий внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необычный",
                                "target_lang_sense_clarification": "общий смысл; отличающийся от привычного"
                            },
                            {
                                "target_lang_word": "чрезвычайный",
                                "target_lang_sense_clarification": "используется с мерами, обстоятельствами или событиями"

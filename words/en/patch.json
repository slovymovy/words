{
    "word_family": [
        "patchy",
        "patchwork",
        "dispatcher"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a1de1b8-570d-3e88-a67c-7c3415d995e2",
                    "sense_definition": "A piece of cloth or other material used to mend a hole or strengthen a worn area on a garment or object.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "repair_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His mother sewed a colorful <w>patch</w> onto the knee of his jeans.",
                            "target_lang_translations": {
                                "ru": "Его мама пришила цветную <w>заплатку</w> на колено его джинсов."
                            }
                        },
                        {
                            "text": "The mechanic put a rubber <w>patch</w> on the tire to stop the leak.",
                            "target_lang_translations": {
                                "ru": "Механик поставил резиновую <w>заплату</w> на шину, чтобы устранить утечку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sew a patch",
                        "leather patch",
                        "elbow patch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок ткани или другого материала, используемый для починки дыры или укрепления изношенного места на одежде или предмете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заплата",
                                "target_lang_sense_clarification": "кусок ткани"
                            },
                            {
                                "target_lang_word": "заплатка",
                                "target_lang_sense_clarification": "уменьшительное, часто используется в разговорной речи"
                            },
                            {
                                "target_lang_word": "латка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f882ffb-d394-3840-ad3c-7d18964e2fe5",
                    "sense_definition": "A small, distinct area or spot that is different in color, texture, or appearance from its surroundings.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distinct_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to slip on that <w>patch</w> of ice near the door.",
                            "target_lang_translations": {
                                "ru": "Осторожно, не поскользнитесь на том <w>пятне</w> льда у двери."
                            }
                        },
                        {
                            "text": "We could see a small <w>patch</w> of blue sky through the clouds.",
                            "target_lang_translations": {
                                "ru": "Сквозь облака мы видели маленький <w>клочок</w> голубого неба."
                            }
                        }
                    ],
                    "synonyms": [
                        "spot",
                        "area",
                        "section",
                        "blotch"
                    ],
                    "common_phrases": [
                        "patch of ice",
                        "patch of grass",
                        "bald patch",
                        "damp patch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный период времени, часто характеризующийся специфическими трудностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "период времени, часто в идиомах"
                            },
                            {
                                "target_lang_word": "период",
                                "target_lang_sense_clarification": "отрезок времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07f01bef-fe42-33c5-a3ba-7708a39a9985",
                    "sense_definition": "A small piece of ground used for gardening or growing specific plants.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "land_plot",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They spent the afternoon picking strawberries in the <w>patch</w> behind the house.",
                            "target_lang_translations": {
                                "ru": "Они провели день, собирая клубнику на <w>грядке</w> за домом."
                            }
                        },
                        {
                            "text": "My grandfather grows carrots and onions in his vegetable <w>patch</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка выращивает морковь и лук на своем <w>огороде</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tract"
                    ],
                    "common_phrases": [
                        "vegetable patch",
                        "pumpkin patch",
                        "garden patch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой участок земли, используемый для садоводства или выращивания определенных растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грядка",
                                "target_lang_sense_clarification": "для выращивания овощей"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "земли"
                            },
                            {
                                "target_lang_word": "делянка",
                                "target_lang_sense_clarification": "выделенный участок земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "032f8a5f-0f2f-351a-accb-edd4de63b15f",
                    "sense_definition": "A piece of code or data used to fix a bug or update a computer program.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should download the latest security <w>patch</w> to protect your computer.",
                            "target_lang_translations": {
                                "ru": "Вам следует скачать последний <w>патч</w> безопасности, чтобы защитить свой компьютер."
                            }
                        },
                        {
                            "text": "The developers released a <w>patch</w> to fix the crashing issue.",
                            "target_lang_translations": {
                                "ru": "Разработчики выпустили <w>патч</w> для устранения проблемы с вылетом программы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "security patch",
                        "software patch",
                        "release a patch"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фрагмент кода или данных, используемый для исправления ошибки или обновления компьютерной программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патч",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "заплатка",
                                "target_lang_sense_clarification": "компьютерный жаргон"
                            },
                            {
                                "target_lang_word": "исправление",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ae117f8-9cba-34cb-8685-b6405e5ad85b",
                    "sense_definition": "An adhesive pad containing medication that is absorbed through the skin over time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He managed to quit smoking by using a nicotine <w>patch</w>.",
                            "target_lang_translations": {
                                "ru": "Ему удалось бросить курить с помощью никотинового <w>пластыря</w>."
                            }
                        },
                        {
                            "text": "The doctor prescribed a pain relief <w>patch</w> for her back.",
                            "target_lang_translations": {
                                "ru": "Врач прописал обезболивающий <w>пластырь</w> для ее спины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nicotine patch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Клейкая накладка, содержащая лекарство, которое впитывается через кожу с течением времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пластырь",
                                "target_lang_sense_clarification": "медицинский"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f882ffb-d394-3840-ad3c-7d18964e2fe5",
                    "sense_definition": "A distinct period of time, often characterized by specific difficulties.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Their marriage went through a rough <w>patch</w> last year, but they are happy now.",
                            "target_lang_translations": {
                                "ru": "В прошлом году в их браке была черная <w>полоса</w>, но сейчас они счастливы."
                            }
                        },
                        {
                            "text": "The business hit a bad <w>patch</w> during the economic recession.",
                            "target_lang_translations": {
                                "ru": "Бизнес переживал неудачную <w>полосу</w> во время экономического спада."
                            }
                        }
                    ],
                    "synonyms": [
                        "spell",
                        "stretch",
                        "period of time"
                    ],
                    "common_phrases": [
                        "rough patch",
                        "bad patch",
                        "purple patch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный период времени, часто характеризующийся специфическими трудностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "период времени, часто в идиомах"
                            },
                            {
                                "target_lang_word": "период",
                                "target_lang_sense_clarification": "отрезок времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25495399-bfce-37fd-891b-9e1f9681e66d",
                    "sense_definition": "A cover worn over an injured or missing eye.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "eye_cover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pirate in the movie wore a black <w>patch</w> over his left eye.",
                            "target_lang_translations": {
                                "ru": "Пират в фильме носил черную <w>повязку</w> на левом глазу."
                            }
                        },
                        {
                            "text": "After the surgery, she had to wear an eye <w>patch</w> for a week.",
                            "target_lang_translations": {
                                "ru": "После операции ей пришлось неделю носить глазную <w>повязку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "eye patch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повязка, которую носят поверх поврежденного или отсутствующего глаза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повязка",
                                "target_lang_sense_clarification": "на глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9d6e53e-277b-32aa-aa90-7abfea68584c",
                    "sense_definition": "A specific area or region assigned to someone for professional responsibility, such as a police officer or salesperson.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "responsibility_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new officer was getting to know the people on his <w>patch</w>.",
                            "target_lang_translations": {
                                "ru": "Новый офицер знакомился с людьми на своем <w>участке</w>."
                            }
                        },
                        {
                            "text": "She knows every client in her sales <w>patch</w> personally.",
                            "target_lang_translations": {
                                "ru": "Она знает каждого клиента на своей торговой <w>территории</w> лично."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on my patch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная территория или регион, закрепленные за кем-либо для выполнения профессиональных обязанностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "зона ответственности"
                            },
                            {
                                "target_lang_word": "территория",
                                "target_lang_sense_clarification": "район деятельности"
                            }
                        ]
                    }
                }

{
    "word_family": [
        "mask",
        "unmask",
        "masker",
        "maskless"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8227bcb7-0d75-3a62-91b3-b5beaa84f90a",
                    "sense_definition": "Wearing a mask or masks to cover the face.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <masked> intruder entered the building through a back door.",
                            "target_lang_translations": {
                                "ru": "Злоумышленник <в маске> вошел в здание через заднюю дверь."
                            }
                        },
                        {
                            "text": "Guests at the <masked> ball wore elaborate costumes and disguises.",
                            "target_lang_translations": {
                                "ru": "Гости на <маскарадном> балу были одеты в сложные костюмы и маски."
                            }
                        }
                    ],
                    "antonyms": [
                        "unmasked",
                        "maskless"
                    ],
                    "common_phrases": [
                        "masked man",
                        "masked ball",
                        "heavily masked"
                    ],
                    "target_lang_definitions": {
                        "ru": "Носящий маску или маски, закрывающие лицо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в маске",
                                "target_lang_sense_clarification": "о человеке, скрывающем лицо"
                            },
                            {
                                "target_lang_word": "маскарадный",
                                "target_lang_sense_clarification": "относящийся к балу или костюму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1987efe-7c7f-3b9c-83ac-2a95171a12bb",
                    "sense_definition": "Kept hidden or concealed; not easily noticed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her true feelings were <masked> by a polite smile.",
                            "target_lang_translations": {
                                "ru": "Ее истинные чувства были <скрыты> за вежливой улыбкой."
                            }
                        },
                        {
                            "text": "The bitter taste of the medicine was <masked> by the sweet syrup.",
                            "target_lang_translations": {
                                "ru": "Горький вкус лекарства был <замаскирован> сладким сиропом."
                            }
                        }
                    ],
                    "antonyms": [
                        "unmasked"
                    ],
                    "common_phrases": [
                        "masked by",
                        "masked depression"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрытый или замаскированный; нелегко заметить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрытый",
                                "target_lang_sense_clarification": "неявный, спрятанный"
                            },
                            {
                                "target_lang_word": "замаскированный",
                                "target_lang_sense_clarification": "спрятанный под видом чего-то другого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "619473a4-8d13-37e4-a664-b4df40087ee8",
                    "sense_definition": "Having a distinct patch of color on the head or face that resembles a mask, especially in birds.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <masked> weaver is known for the black markings on its face.",
                            "target_lang_translations": {
                                "ru": "<Масковый> ткач известен черными отметинами на лице."
                            }
                        },
                        {
                            "text": "We spotted a small <masked> bird in the dense foliage.",
                            "target_lang_translations": {
                                "ru": "Мы заметили маленькую <масковую> птицу в густой листве."
                            }
                        }
                    ],
                    "common_phrases": [
                        "masked owl",
                        "masked booby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий характерное пятно цвета на голове или лице, напоминающее маску (обычно о птицах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масковый",
                                "target_lang_sense_clarification": "в биологических названиях видов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54710535-65e7-3700-9dc4-96c303bf15e4",
                    "sense_definition": "Simple past and past participle of <w>mask</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <masked> his identity with a fake beard and glasses.",
                            "target_lang_translations": {
                                "ru": "Он <замаскировал> свою личность с помощью накладной бороды и очков."
                            }
                        },
                        {
                            "text": "The loud music <masked> the sound of the approaching car.",
                            "target_lang_translations": {
                                "ru": "Громкая музыка <заглушила> звук приближающейся машины."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past simple and past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола mask (маскировать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замаскировал",
                                "target_lang_sense_clarification": "спрятал, скрыл вид"
                            },
                            {
                                "target_lang_word": "скрыл",
                                "target_lang_sense_clarification": "утаил"
                            },
                            {
                                "target_lang_word": "заглушил",
                                "target_lang_sense_clarification": "о звуке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "bribery",
        "briber",
        "bribable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4439b9c0-4d4f-3333-a79e-044edcc98a24",
                    "sense_definition": "To give money or a gift to someone to persuade them to do something, often illegally or dishonestly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bribe_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The criminal tried to <w>bribe</w> the police officer to let him go."
                        },
                        {
                            "text": "They attempted to <w>bribe</w> the judge into dropping the charges."
                        }
                    ],
                    "synonyms": [
                        "subornation"
                    ],
                    "common_phrases": [
                        "bribe someone with",
                        "bribe into doing"
                    ]
                },
                {
                    "sense_id": "11d09284-a10e-344a-a522-5214e7b4b8e0",
                    "sense_definition": "To offer a treat or reward to someone, especially a child, to encourage them to behave or do a task.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bribe_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had to <w>bribe</w> my son with ice cream to get him to clean his room."
                        },
                        {
                            "text": "She used chocolate to <w>bribe</w> the children into sitting quietly."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "figurative",
                            "comment": "used in a lighter, non-criminal context"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "42af7552-308f-35c6-a5d1-1c4cd1e4c6cb",
                    "sense_definition": "Money or a gift given to someone in exchange for using their influence or doing something dishonest.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bribe_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The official was arrested for accepting a large <w>bribe</w> from a construction company."
                        },
                        {
                            "text": "He offered a cash <w>bribe</w> to avoid getting a speeding ticket."
                        }
                    ],
                    "synonyms": [
                        "inducement",
                        "pay-off",
                        "backhander",
                        "bung",
                        "sweetener"
                    ],
                    "common_phrases": [
                        "accept a bribe",
                        "take a bribe",
                        "offer a bribe"
                    ]
                },
                {
                    "sense_id": "a80b36db-c047-3b6f-90c7-b82587204e81",
                    "sense_definition": "Something attractive offered to persuade someone to do something, often used playfully.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bribe_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The promise of extra vacation time was a <w>bribe</w> to keep the employees happy."
                        },
                        {
                            "text": "I used a cookie as a <w>bribe</w> to stop the toddler from crying."
                        }
                    ],
                    "synonyms": [
                        "inducement",
                        "sweetener"
                    ],
                    "traits": [
                        {
                            "trait_type": "figurative",
                            "comment": "used in non-criminal contexts"
                        }
                    ]
                }
            ]
        }
    ]

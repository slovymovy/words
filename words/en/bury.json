                            },
                            {
                                "target_lang_word": "забы́ть",
                                "target_lang_sense_clarification": "to forget (differences)"
                            },
                            {
                                "target_lang_word": "зары́ть",
                                "target_lang_sense_clarification": "used in idiom 'bury the hatchet'"
                            }
                        ]
                    }

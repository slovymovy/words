    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c1121480-8fa9-3612-bddb-42d5f65c11c9",
                    "sense_definition": "Zmienić położenie przedmiotu lub osoby, kierując je w inną stronę lub wykonując ruch dookoła własnej osi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz delikatnie <obrócić> klucz w zamku, aby otworzyć drzwi.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно осторожно <повернуть> ключ в замке, чтобы открыть дверь."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił uczniów, aby <obrócić> kartkę na drugą stronę.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников <перевернуть> листок на другую сторону."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekręcić",
                        "odwrócić",
                        "skierować"
                    ],
                    "common_phrases": [
                        "obrócić głowę",
                        "obrócić klucz",
                        "obrócić kartkę",
                        "obrócić się na pięcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить положение предмета или человека, направив его в другую сторону или совершив движение вокруг собственной оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повернуть",
                                "target_lang_sense_clarification": "изменить направление или положение"
                            },
                            {
                                "target_lang_word": "перевернуть",
                                "target_lang_sense_clarification": "положить другой стороной вверх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9504e55-acd2-39d9-ab59-5746233bc144",
                    "sense_definition": "Sprawić, że coś zmieni swoją postać, charakter lub stan na inny; przekształcić coś w coś innego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego niefortunna uwaga może <obrócić> całą dyskusję w kłótnię.",
                            "target_lang_translations": {
                                "ru": "Его неудачное замечание может <превратить> всю дискуссию в ссору."
                            }
                        },
                        {
                            "text": "Próbował <obrócić> tę niezręczną sytuację w niewinny żart.",
                            "target_lang_translations": {
                                "ru": "Он пытался <обратить> эту неловкую ситуацию в невинную шутку."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekształcić",
                        "zmienić",
                        "przeistoczyć",
                        "zamienić"
                    ],
                    "common_phrases": [
                        "obrócić w żart",
                        "obrócić w niwecz",
                        "obrócić w ruinę",
                        "obrócić w perzynę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то изменить свою форму, характер или состояние; превратить что-то во что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превратить",
                                "target_lang_sense_clarification": "полностью изменить сущность или вид"
                            },
                            {
                                "target_lang_word": "обратить",
                                "target_lang_sense_clarification": "перевести в иное состояние (часто абстрактное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39ce8ed5-999c-385a-a9c2-3a4478716071",
                    "sense_definition": "Wykorzystać posiadane środki, pieniądze lub zasoby w określonym celu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zarząd firmy zdecydował <obrócić> nadwyżkę budżetową na inwestycje.",
                            "target_lang_translations": {
                                "ru": "Правление компании решило <направить> бюджетный излишек на инвестиции."
                            }
                        },
                        {
                            "text": "Mężczyzna postanowił <obrócić> cały swój majątek na cele charytatywne.",
                            "target_lang_translations": {
                                "ru": "Мужчина решил <пустить> всё своё состояние на благотворительные цели."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeznaczyć",
                        "wykorzystać",
                        "spożytkować"
                    ],
                    "antonyms": [
                        "zmarnować",
                        "roztrwonić"
                    ],
                    "common_phrases": [
                        "obrócić pieniądze",
                        "obrócić zysk",
                        "obrócić majątek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать имеющиеся средства, деньги или ресурсы для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направить",
                                "target_lang_sense_clarification": "использовать средства для цели"
                            },
                            {
                                "target_lang_word": "пустить",
                                "target_lang_sense_clarification": "вложить в дело, пустить в оборот"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "8e46191a-ea83-3360-b03f-c6beef4c5aed",
                    "sense_definition": "Число 1; количество, равное единице.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "number_val",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня есть только <w>один</w> вопрос к тебе.",
                            "target_lang_translations": {
                                "en": "I have only <w>one</w> question for you."
                            }
                        },
                        {
                            "text": "На столе лежит <w>одна</w> интересная книга.",
                            "target_lang_translations": {
                                "en": "There is <w>one</w> interesting book on the table."
                            }
                        },
                        {
                            "text": "В нашем классе двадцать <w>один</w> ученик.",
                            "target_lang_translations": {
                                "en": "There are twenty-<w>one</w> students in our class."
                            }
                        }
                    ],
                    "synonyms": [
                        "едини́ца",
                        "раз"
                    ],
                    "common_phrases": [
                        "один раз",
                        "номер один",
                        "один из нас",
                        "ни один"
                    ],
                    "target_lang_definitions": {

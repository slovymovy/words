{
    "word_family": [
        "przyznawać",
        "przyznanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f780732-6b00-3792-bfce-f22b1ba601b3",
                    "sense_definition": "Stwierdzić, że coś jest prawdą, często pomimo wcześniejszych wątpliwości lub niechęci; zgodzić się z czyimś zdaniem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "admit_acknowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <przyznać>, że twój pomysł jest znacznie lepszy od mojego.",
                            "target_lang_translations": {
                                "ru": "Я должен <признать>, что твоя идея значительно лучше моей."
                            }
                        },
                        {
                            "text": "Mimo długiej dyskusji, ojciec w końcu <przyznał> rację synowi.",
                            "target_lang_translations": {
                                "ru": "Несмотря на долгую дискуссию, отец в конце концов <признал> правоту сына."
                            }
                        }
                    ],
                    "synonyms": [
                        "potwierdzić",
                        "zgodzić się"
                    ],
                    "antonyms": [
                        "zaprzeczyć"
                    ],
                    "common_phrases": [
                        "przyznać rację",
                        "trzeba przyznać",
                        "szczerze przyznać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждать, что что-то является правдой, часто вопреки прежним сомнениям или нежеланию; согласиться с чьим-то мнением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признать",
                                "target_lang_sense_clarification": "согласиться с фактом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f318c066-31e2-3645-ae22-ccb8c250de01",
                    "sense_definition": "Potwierdzić fakt zrobienia czegoś, zazwyczaj złego lub wstydliwego; używane z zaimkiem zwrotnym „się”.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "admit_acknowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podejrzany o kradzież mężczyzna <przyznał> się do winy.",
                            "target_lang_translations": {
                                "ru": "Мужчина, подозреваемый в краже, <признал> свою вину."
                            }
                        },
                        {
                            "text": "Bardzo trudno jest <przyznać> się do błędu przed całą grupą.",
                            "target_lang_translations": {
                                "ru": "Очень трудно <признать> свою ошибку перед всей группой."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyznać"
                    ],
                    "antonyms": [
                        "wyprzeć się"
                    ],
                    "common_phrases": [
                        "przyznać się do winy",
                        "przyznać się do błędu",
                        "przyznać się bez bicia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подтвердить факт совершения чего-либо, обычно плохого или постыдного; используется с возвратным местоимением «się»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признаться",
                                "target_lang_sense_clarification": "рассказать о своем поступке"
                            },
                            {
                                "target_lang_word": "сознаться",
                                "target_lang_sense_clarification": "в преступлении"
                            },
                            {
                                "target_lang_word": "признать",
                                "target_lang_sense_clarification": "вину или ошибку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19653695-3410-3fbc-9bd7-f25be0581f7d",
                    "sense_definition": "Oficjalnie przydzielić coś komuś, na przykład nagrodę, pieniądze lub uprawnienia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komisja postanowiła <przyznać> główną nagrodę młodemu artyście.",
                            "target_lang_translations": {
                                "ru": "Комиссия решила <присудить> главную награду молодому художнику."
                            }
                        },
                        {
                            "text": "Sąd <przyznał> matce wyłączną opiekę nad dziećmi.",
                            "target_lang_translations": {
                                "ru": "Суд <предоставил> матери единоличную опеку над детьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "przydzielić",
                        "nadać",
                        "udzielić"
                    ],
                    "antonyms": [
                        "odebrać",
                        "odmówić"
                    ],
                    "common_phrases": [
                        "przyznać nagrodę",
                        "przyznać stypendium",
                        "przyznać dotację",
                        "przyznać obywatelstwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально выделить что-то кому-то, например, награду, деньги или права."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присудить",
                                "target_lang_sense_clarification": "о награде"
                            },
                            {
                                "target_lang_word": "предоставить",
                                "target_lang_sense_clarification": "о правах"
                            },
                            {
                                "target_lang_word": "назначить",
                                "target_lang_sense_clarification": "о выплатах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
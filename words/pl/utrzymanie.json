{
    "word_family": [
        "utrzymać",
        "utrzymywać",
        "utrzymanek",
        "utrzymanka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fce23206-2060-3ef6-851a-c05dced87657",
                    "sense_definition": "Środki finansowe lub materialne niezbędne do życia i zaspokojenia podstawowych potrzeb.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "livelihood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta praca jest jedynym źródłem <w>utrzymania</w> całej naszej rodziny.",
                            "target_lang_translations": {
                                "ru": "Эта работа — единственный источник <w>средств к существованию</w> для всей нашей семьи."
                            }
                        },
                        {
                            "text": "Były mąż daje mi na <w>utrzymanie</w> zaledwie trzysta złotych.",
                            "target_lang_translations": {
                                "ru": "Бывший муж дает мне на <w>содержание</w> всего триста злотых."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koszty utrzymania",
                        "środki na utrzymanie",
                        "źródło utrzymania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовые или материальные средства, необходимые для жизни и удовлетворения основных потребностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средства к существованию",
                                "target_lang_sense_clarification": "деньги или ресурсы, необходимые для жизни"
                            },
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "финансовое обеспечение кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d735f975-7e5a-3f55-bd5c-ef148f70af66",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>utrzymać</w>; czynność polegająca na zachowaniu czegoś w niezmienionym stanie, pozycji lub posiadaniu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Utrzymanie</w> tego ciężaru okazało się dla niego niemożliwe.",
                            "target_lang_translations": {
                                "ru": "<w>Удержание</w> этого веса оказалось для него невозможным."
                            }
                        },
                        {
                            "text": "<w>Utrzymanie</w> porządku w tak dużym domu wymaga wiele pracy.",
                            "target_lang_translations": {
                                "ru": "<w>Поддержание</w> порядка в таком большом доме требует много труда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "utrzymanie porządku",
                        "utrzymanie równowagi",
                        "utrzymanie tempa"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verbal noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в сохранении чего-либо в неизменном состоянии, положении или владении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удержание",
                                "target_lang_sense_clarification": "физическое удерживание веса, позиции или власти"
                            },
                            {
                                "target_lang_word": "поддержание",
                                "target_lang_sense_clarification": "сохранение определенного состояния (порядка, разговора, темпа)"
                            },
                            {
                                "target_lang_word": "сохранение",
                                "target_lang_sense_clarification": "сбережение чего-либо в целости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d86e2f4-e447-3bf6-9f20-8319756bc5bf",
                    "sense_definition": "Regularne posiłki (wyżywienie) oraz opieka zapewniane w miejscu pobytu, często w zamian za opłatę lub pracę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "board",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Opiekował się staruszką w zamian za <w>utrzymanie</w>.",
                            "target_lang_translations": {
                                "ru": "Он ухаживал за старушкой в обмен на <w>содержание</w>."
                            }
                        },
                        {
                            "text": "W cenie pokoju hotelowego wliczone jest całodzienne <w>utrzymanie</w>.",
                            "target_lang_translations": {
                                "ru": "В стоимость гостиничного номера включен полный <w>пансион</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyżywienie",
                        "wikt"
                    ],
                    "common_phrases": [
                        "wikt i utrzymanie",
                        "całodzienne utrzymanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярное питание и обслуживание, предоставляемые в месте пребывания, часто за плату или в обмен на работу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "полное обеспечение жильем и едой"
                            },
                            {
                                "target_lang_word": "пансион",
                                "target_lang_sense_clarification": "питание и обслуживание (обычно в гостиницах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
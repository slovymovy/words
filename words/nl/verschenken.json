                {
                    "sense_id": "3fa7d013-0417-381f-b43c-6e1ab916b290",
                    "sense_definition": "De kwaliteit van een drank verminderen door er te veel van een andere vloeistof bij te gieten.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pouring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je hebt de goede wijn <w>verschonken</w> door er water bij te doen.",
                            "target_lang_translations": {
                                "en": "You <w>ruined</w> the good wine by adding water to it.",
                                "ru": "Ты <w>испортил</w> хорошее вино, добавив в него воды."
                            }
                        },
                        {
                            "text": "Pas op dat je de smaak niet <w>verschenkt</w> met te veel ijs.",
                            "target_lang_translations": {
                                "en": "Be careful not to <w>spoil</w> the taste with too much ice.",
                                "ru": "Смотри не <w>испорть</w> вкус слишком большим количеством льда."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdunnen",
                        "verwateren"
                    ],
                    "target_lang_definitions": {
                        "en": "To reduce the quality of a drink by pouring too much of another liquid into it.",
                        "ru": "Ухудшать качество напитка, добавляя в него слишком много другой жидкости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spoil",
                                "target_lang_sense_clarification": "to ruin the quality of a drink"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "to destroy the taste by mixing"
                            },
                            {
                                "target_lang_word": "water down",
                                "target_lang_sense_clarification": "to dilute excessively"
                            }
                        ],
                        "ru": [

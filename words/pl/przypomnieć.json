{
    "word_family": [
        "pamięć",
        "pamiętać",
        "zapomnieć",
        "wspomnieć",
        "upomnieć",
        "wypomnieć",
        "zapominać",
        "wspominać",
        "upominać",
        "wypominać",
        "przypomnienie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cddf8744-7711-3fd6-ae31-2d34bc301da0",
                    "sense_definition": "Odtworzyć w swojej pamięci informację, zdarzenie lub fakt, o którym się zapomniało (używane z zaimkiem zwrotnym „sobie”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "recall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle sobie <przypomniałam>, że zostawiłam włączony piecyk.",
                            "target_lang_translations": {
                                "ru": "Вдруг я <w>вспомнила</w>, что оставила включенным обогреватель."
                            }
                        },
                        {
                            "text": "Nie mogę sobie <przypomnieć>, gdzie położyłem klucze od mieszkania.",
                            "target_lang_translations": {
                                "ru": "Не могу <w>вспомнить</w>, куда я положил ключи от квартиры."
                            }
                        }
                    ],
                    "antonyms": [
                        "zapomnieć"
                    ],
                    "common_phrases": [
                        "przypomnieć sobie coś",
                        "nagle sobie przypomnieć",
                        "nie móc sobie przypomnieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстановить в памяти информацию, событие или факт, о котором забыли (обычно используется с возвратным местоимением «sobie»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспомнить",
                                "target_lang_sense_clarification": "восстановить в памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baa46f49-79b2-3861-ac44-0a1f16b5767a",
                    "sense_definition": "Zwrócić komuś uwagę na coś, aby o tym nie zapomniał; sprawić, że ktoś wykona zaplanowaną czynność.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "remind_task",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Przypomniałem> jej o wizycie u lekarza, żeby się nie spóźniła.",
                            "target_lang_translations": {
                                "ru": "Я <w>напомнил</w> ей о визите к врачу, чтобы она не опоздала."
                            }
                        },
                        {
                            "text": "Proszę mi jutro <przypomnieć> o wysłaniu tego ważnego listu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>напомните</w> мне завтра отправить это важное письмо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przypomnieć o spotkaniu",
                        "przypomnieć o obowiązku",
                        "przypomnieć komuś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обратить чье-то внимание на что-то, чтобы человек не забыл; побудить кого-то выполнить запланированное действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напомнить",
                                "target_lang_sense_clarification": "указать на необходимость сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3a19179-0891-3738-9c4e-7ab574d9b92f",
                    "sense_definition": "Sprawić, że w czyjejś pamięci pojawia się wspomnienie osoby, rzeczy lub sytuacji z przeszłości (często przez skojarzenie).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "evoke_memory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zapach <przypomniał> mi wakacje spędzane u babci na wsi.",
                            "target_lang_translations": {
                                "ru": "Этот запах <w>напомнил</w> мне каникулы, проведенные у бабушки в деревне."
                            }
                        },
                        {
                            "text": "Jego zachowanie bardzo <przypomniało> nam o dawnych problemach w firmie.",
                            "target_lang_translations": {
                                "ru": "Его поведение очень <w>напомнило</w> нам о давних проблемах в фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywołać się"
                    ],
                    "common_phrases": [
                        "przypomnieć stare czasy",
                        "przypomnieć komuś kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать в чьей-то памяти воспоминание о человеке, вещи или ситуации из прошлого (часто через ассоциацию)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напомнить",
                                "target_lang_sense_clarification": "вызвать воспоминания или ассоциации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "eb25ae4a-7430-3776-ab5c-37f40c8078e1",
                    "sense_definition": "W sposób odnoszący się do ciała i organizmu, często w przeciwstawieniu do psychiki lub umysłu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat pracuje <w>fizycznie</w> na budowie przez osiem godzin dziennie.",
                            "target_lang_translations": {
                                "ru": "Мой брат работает <w>физически</w> на стройке по восемь часов в день."
                            }
                        },
                        {
                            "text": "Po całym tygodniu nauki czuję się <w>fizycznie</w> i psychicznie wyczerpany.",
                            "target_lang_translations": {
                                "ru": "После целой недели учебы я чувствую себя <w>физически</w> и морально истощенным."
                            }
                        }
                    ],
                    "synonyms": [
                        "cieleśnie"
                    ],
                    "common_phrases": [
                        "pracować fizycznie",
                        "zmęczony fizycznie",
                        "sprawny fizycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительно тела и организма, часто в противопоставлении психике или разуму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физически",
                                "target_lang_sense_clarification": "телесно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8c7b19a-d2e2-37e2-a9eb-aeae3e340c5b",
                    "sense_definition": "W sposób rzeczywisty, materialny i namacalny; oznaczający faktyczną obecność w danym miejscu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie musisz być <w>fizycznie</w> w biurze, żeby wziąć udział w tym spotkaniu.",
                            "target_lang_translations": {
                                "ru": "Тебе не обязательно <w>физически</w> находиться в офисе, чтобы принять участие в этой встрече."
                            }
                        },
                        {
                            "text": "To rozwiązanie jest teoretycznie dobre, ale <w>fizycznie</w> trudne do wykonania.",
                            "target_lang_translations": {
                                "ru": "Это решение теоретически хорошее, но <w>физически</w> трудновыполнимое."
                            }
                        }
                    ],
                    "synonyms": [
                        "materialnie",
                        "namacalnie"
                    ],
                    "common_phrases": [
                        "fizycznie obecny",
                        "fizycznie niemożliwe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реально, материально и осязаемо; означающее фактическое присутствие в данном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физически",
                                "target_lang_sense_clarification": "реально, фактически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71b50b51-9fd8-33db-a0d4-26568d90b399",
                    "sense_definition": "W sposób odnoszący się do praw fizyki jako nauki lub zjawisk przyrodniczych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zjawisko to można wyjaśnić <w>fizycznie</w> poprzez analizę ruchu cząsteczek.",
                            "target_lang_translations": {
                                "ru": "Это явление можно объяснить <w>физически</w> посредством анализа движения частиц."
                            }
                        },
                        {
                            "text": "Wszechświat jest <w>fizycznie</w> ograniczony pewnymi stałymi uniwersalnymi.",
                            "target_lang_translations": {
                                "ru": "Вселенная <w>физически</w> ограничена определенными универсальными константами."
                            }
                        }
                    ],
                    "synonyms": [
                        "fizykalnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительно законов физики как науки или природных явлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физически",
                                "target_lang_sense_clarification": "с точки зрения физики"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d554158d-3941-31b5-930b-e79a355e2026",
                    "sense_definition": "Grupa gór lub wzniesień ciągnąca się w jednym kierunku, tworząca wydłużony łańcuch.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>pasmo</w> górskie jest chętnie odwiedzane przez turystów.",
                            "target_lang_translations": {
                                "ru": "Этот горный <w>хребет</w> часто посещают туристы."
                            }
                        },
                        {
                            "text": "Wędrowaliśmy przez całe <w>pasmo</w>, podziwiając piękne widoki.",
                            "target_lang_translations": {
                                "ru": "Мы шли через весь <w>хребет</w>, любуясь прекрасными видами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasmo górskie",
                        "główne pasmo",
                        "pasmo Tatr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа гор или возвышенностей, тянущаяся в одном направлении и образующая вытянутую цепь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хребет",
                                "target_lang_sense_clarification": "горы"
                            },
                            {
                                "target_lang_word": "горная цепь",
                                "target_lang_sense_clarification": "география"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e51b7aa5-e438-37e9-b9df-d43cd4ca44cd",
                    "sense_definition": "Wydzielona wiązka włosów, nici lub włókien ułożonych obok siebie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_strand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fryzjer rozjaśnił jej jedno <w>pasmo</w> włosów na blond.",
                            "target_lang_translations": {
                                "ru": "Парикмахер осветлил ей одну <w>прядь</w> волос до блонда."
                            }
                        },
                        {
                            "text": "Z koka wysunęło się luźne <w>pasmo</w>, opadając na czoło.",
                            "target_lang_translations": {
                                "ru": "Из пучка выбилась свободная <w>прядь</w>, упав на лоб."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasmo włosów",
                        "farbowane pasmo",
                        "niesforne pasmo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный пучок волос, нитей или волокон, расположенных рядом друг с другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прядь",
                                "target_lang_sense_clarification": "волосы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39f073f5-4b32-3fc4-ba2e-c4a01d895413",
                    "sense_definition": "Ciąg następujących po sobie zdarzeń, zazwyczaj o podobnym, często negatywnym lub pozytywnym charakterze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego kariera to nieustające <w>pasmo</w> sukcesów zawodowych.",
                            "target_lang_translations": {
                                "ru": "Его карьера — это непрерывная <w>череда</w> профессиональных успехов."
                            }
                        },
                        {
                            "text": "Mamy nadzieję, że to <w>pasmo</w> pecha wreszcie się skończy.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся, что эта <w>полоса</w> неудач наконец закончится."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciąg"
                    ],
                    "common_phrases": [
                        "pasmo sukcesów",
                        "pasmo nieszczęść",
                        "pasmo porażek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Череда последовательных событий, обычно схожего, часто негативного или позитивного характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "череда",
                                "target_lang_sense_clarification": "событий"
                            },
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "период времени"
                            },
                            {
                                "target_lang_word": "серия",
                                "target_lang_sense_clarification": "последовательность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d3b50be-04e5-3646-b7de-9de48ea850fe",
                    "sense_definition": "Długi, wąski fragment powierzchni lub przestrzeni, odróżniający się wyglądem od otoczenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_strip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez szczelinę wpadło do pokoju wąskie <w>pasmo</w> światła.",
                            "target_lang_translations": {
                                "ru": "Через щель в комнату упала узкая <w>полоса</w> света."
                            }
                        },
                        {
                            "text": "Na horyzoncie widać było ciemne <w>pasmo</w> lasu.",
                            "target_lang_translations": {
                                "ru": "На горизонте была видна тёмная <w>полоса</w> леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "smuga",
                        "pas",
                        "pręga"
                    ],
                    "common_phrases": [
                        "pasmo światła",
                        "pasmo ziemi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный, узкий фрагмент поверхности или пространства, отличающийся внешним видом от окружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e53260e0-362d-364c-937c-1f63f4a5a496",
                    "sense_definition": "Zakres częstotliwości fal, energii lub czasu wykorzystywany w technice i fizyce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_range",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To radio działa w <w>paśmie</w> fal krótkich.",
                            "target_lang_translations": {
                                "ru": "Это радио работает в <w>диапазоне</w> коротких волн."
                            }
                        },
                        {
                            "text": "Operator zapewnia szerokie <w>pasmo</w> internetowe dla firm.",
                            "target_lang_translations": {
                                "ru": "Оператор предоставляет широкую <w>полосу</w> пропускания интернета для компаний."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasmo przenoszenia",
                        "szerokie pasmo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Диапазон частот волн, энергии или времени, используемый в технике и физике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диапазон",
                                "target_lang_sense_clarification": "частот"
                            },
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "пропускания, частот"
                            }
                        ]
                    }
                }
            ]
        }

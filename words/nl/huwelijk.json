            "senses": [
                {
                    "sense_id": "aee96484-8e8c-3524-999f-86260a12add8",
                    "sense_definition": "De wettelijke en officiële verbintenis tussen twee personen om samen te leven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "union_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hun <w>huwelijk</w> duurde meer dan vijftig jaar en was erg gelukkig.",
                            "target_lang_translations": {
                                "ru": "Их <w>брак</w> продлился более пятидесяти лет и был очень счастливым.",
                                "en": "Their <w>marriage</w> lasted more than fifty years and was very happy."
                            }
                        },
                        {
                            "text": "In Nederland is het burgerlijk <w>huwelijk</w> voor iedereen toegankelijk.",
                            "target_lang_translations": {
                                "ru": "В Нидерландах гражданский <w>брак</w> доступен для всех.",
                                "en": "In the Netherlands, civil <w>marriage</w> is accessible to everyone."
                            }
                        }
                    ],
                    "synonyms": [
                        "echt"
                    ],
                    "common_phrases": [
                        "in het huwelijk treden",
                        "een burgerlijk huwelijk",
                        "vragen ten huwelijk",
                        "een gelukkig huwelijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Законный и официальный союз двух людей для совместной жизни.",
                        "en": "The legal and official union between two people to live together."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брак",
                                "target_lang_sense_clarification": "официальный союз"
                            },
                            {
                                "target_lang_word": "супружество",
                                "target_lang_sense_clarification": "состояние в браке"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "marriage",
                                "target_lang_sense_clarification": "legal union"
                            },
                            {
                                "target_lang_word": "matrimony",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09b21dd0-e34a-3f08-9730-917514273231",
                    "sense_definition": "Een religieuze verbintenis tussen partners, vaak voltrokken in een kerk of gebedshuis.",

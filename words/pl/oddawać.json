    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "28a03216-aeba-3f89-bf86-31fba14ea69d",
                    "sense_definition": "Zwracać coś właścicielowi; dawać z powrotem rzecz, która była pożyczona lub wzięta.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "return_item",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę dzisiaj iść do biblioteki i <oddawać> wszystkie przeczytane książki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сегодня пойти в библиотеку и <сдать> все прочитанные книги."
                            }
                        },
                        {
                            "text": "Nie lubię pożyczać pieniędzy, bo potem ciężko jest je <oddawać> w terminie.",
                            "target_lang_translations": {
                                "ru": "Я не люблю одалживать деньги, потому что потом их трудно <отдавать> в срок."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwracać",
                        "odnosić"
                    ],
                    "antonyms": [
                        "zabierać",
                        "kraść"
                    ],
                    "common_phrases": [
                        "oddawać długi",
                        "oddawać pieniądze",
                        "oddawać książkę",
                        "oddawać resztę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвращать что-либо владельцу; отдавать обратно вещь, которая была одолжена или взята."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращать",
                                "target_lang_sense_clarification": "отдавать обратно взятое"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "давать обратно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ceeb85a-bca7-3157-b990-5e6c4f28f80d",
                    "sense_definition": "Przekazywać coś komuś, często bezpłatnie lub w celu pomocy; ofiarowywać coś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "give_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu ludzi decyduje się regularnie <oddawać> krew, aby ratować życie innych.",
                            "target_lang_translations": {
                                "ru": "Многие люди решают регулярно <сдавать> кровь, чтобы спасать жизни других."
                            }
                        },
                        {
                            "text": "Zamiast wyrzucać stare ubrania, wolę <oddawać> je potrzebującym osobom.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы выбрасывать старую одежду, я предпочитаю <отдавать> её нуждающимся."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawać",
                        "ofiarować",
                        "ofiarowywać",
                        "przedstawiać"
                    ],
                    "antonyms": [
                        "zabierać"
                    ],
                    "common_phrases": [
                        "oddawać krew",
                        "oddawać głos",
                        "oddawać za darmo",
                        "oddawać życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать что-либо кому-либо, часто бесплатно или в целях помощи; жертвовать чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "предоставлять безвозмездно"
                            },
                            {
                                "target_lang_word": "жертвовать",
                                "target_lang_sense_clarification": "давать в целях помощи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "315e3c6b-fb3f-32d6-8277-e6578db09620",
                    "sense_definition": "Umieszczać coś lub kogoś w określonym miejscu lub instytucji w konkretnym celu (np. do naprawy, do oceny, pod opiekę).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "submit_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój samochód się zepsuł, więc muszę go jutro <oddawać> do warsztatu.",
                            "target_lang_translations": {
                                "ru": "Моя машина сломалась, поэтому завтра мне нужно <отдать> её в мастерскую."
                            }
                        },
                        {
                            "text": "Rodzice bali się <oddawać> swoje małe dziecko pod opiekę obcej niani.",
                            "target_lang_translations": {
                                "ru": "Родители боялись <отдавать> своего маленького ребенка под опеку чужой няне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oddawać do naprawy",
                        "oddawać do druku",
                        "oddawać pod opiekę",
                        "oddawać pod sąd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо или кого-либо в определенное место или учреждение с конкретной целью (например, в ремонт, на оценку, под опеку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "помещать в учреждение или ремонт"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "поручать или передавать куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2b1e0e3-cc8b-3a53-9992-9ea9c832411c",
                    "sense_definition": "Wiernie przedstawiać, obrazować lub wyrażać cechy, atmosferę lub charakter czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflect_portray",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piękny portret idealnie <oddaje> smutek w oczach modelki.",
                            "target_lang_translations": {
                                "ru": "Этот прекрасный портрет идеально <передает> грусть в глазах модели."
                            }
                        },
                        {
                            "text": "Słowa nie potrafią w pełni <oddawać> tego, co czułem w tamtej chwili.",
                            "target_lang_translations": {
                                "ru": "Слова не могут в полной мере <передать> то, что я чувствовал в тот момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "odzwierciedlać",
                        "obrazować",
                        "wyrażać",
                        "charakteryzować",
                        "opisywać"
                    ],
                    "common_phrases": [
                        "oddawać atmosferę",
                        "oddawać rzeczywistość",
                        "oddawać sens",
                        "oddawać charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верно передавать, изображать или выражать черты, атмосферу или характер чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "выражать или воспроизводить"
                            },
                            {
                                "target_lang_word": "отражать",
                                "target_lang_sense_clarification": "показывать свойства или атмосферу"
                            }
                        ]

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "7ae28464-75b9-348d-9df6-f608eac0c0d5",
                    "sense_definition": "Zelfstandig gebruikt bezittelijk voornaamwoord dat verwijst naar iets dat van haar is; wordt gebruikt zonder zelfstandig naamwoord.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possessive_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die auto is de <w>hare</w>.",
                            "target_lang_translations": {
                                "en": "That car is <w>hers</w>.",
                                "ru": "Эта машина — <w>её</w>."
                            }
                        },
                        {
                            "text": "Zij heeft haar mening gegeven, en dit is de <w>hare</w>.",
                            "target_lang_translations": {
                                "en": "She gave her opinion, and this is <w>hers</w>.",
                                "ru": "Она высказала своё мнение, а это — <w>её</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "van haar"
                    ],
                    "common_phrases": [
                        "de hare",
                        "het hare"
                    ],
                    "target_lang_definitions": {
                        "en": "Independent possessive pronoun referring to something belonging to a female person; used without a following noun.",
                        "ru": "Самостоятельное притяжательное местоимение, указывающее на принадлежность женщине; используется без существительного."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hers",
                                "target_lang_sense_clarification": "possessive pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "её",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "75c8de95-0b49-365d-a6f6-0cb904dcda1c",
                    "sense_definition": "Verbogen vorm van <w>haar</w>, voornamelijk gebruikt in formele titels en vaste uitdrukkingen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "possessive_determiner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hare</w> Majesteit de Koningin was aanwezig bij de ceremonie.",
                            "target_lang_translations": {
                                "en": "<w>Her</w> Majesty the Queen was present at the ceremony.",
                                "ru": "<w>Её</w> Величество Королева присутствовала на церемонии."
                            }
                        },
                        {
                            "text": "Wij hebben <w>Hare</w> Koninklijke Hoogheid mogen ontmoeten.",
                            "target_lang_translations": {
                                "en": "We were allowed to meet <w>Her</w> Royal Highness.",
                                "ru": "Нам выпала честь встретиться с <w>Её</w> Королевским Высочеством."
                            }
                        }
                    ],
                    "synonyms": [
                        "haar"
                    ],
                    "common_phrases": [
                        "Hare Majesteit",
                        "Hare Koninklijke Hoogheid",
                        "Hare Excellentie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "of haar"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "outside of titles"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Flanders"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Formal inflected form of 'her', primarily used in royal or noble titles.",
                        "ru": "Формальная форма местоимения 'haar', используемая преимущественно в королевских или дворянских титулах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Her",
                                "target_lang_sense_clarification": "in formal titles"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Её",
                                "target_lang_sense_clarification": "в титулах (напр. Её Величество)"
                            }
                        ]
                    }
                }
            ]
        },
        {

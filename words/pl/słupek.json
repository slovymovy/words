{
    "word_family": [
        "słup",
        "słupowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a914079d-e53c-364c-b506-55526f825958",
                    "sense_definition": "Niewielki, pionowy element, często drewniany, metalowy lub betonowy, wbity w ziemię lub przymocowany do podłoża; zdrobnienie od <w>słup</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_post",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód uderzył w metalowy <słupek> przy chodniku.",
                            "target_lang_translations": {
                                "ru": "Машина врезалась в металлический <w>столбик</w> у тротуара."
                            }
                        },
                        {
                            "text": "Robotnicy malują biało-czerwone <słupki> wzdłuż drogi.",
                            "target_lang_translations": {
                                "ru": "Рабочие красят бело-красные <w>столбики</w> вдоль дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "balasek",
                        "balasek"
                    ],
                    "common_phrases": [
                        "słupek graniczny",
                        "słupek drogowy",
                        "słupek parkingowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой вертикальный элемент, часто деревянный, металлический или бетонный, вбитый в землю или прикрепленный к основанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбик",
                                "target_lang_sense_clarification": "небольшой столб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "931b235b-3377-3068-9212-e95ead24e04c",
                    "sense_definition": "Pionowy, boczny element konstrukcji bramki w grach zespołowych, takich jak piłka nożna.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_post",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piłka trafiła w <słupek> i nie wpadła do bramki.",
                            "target_lang_translations": {
                                "ru": "Мяч попал в <w>штангу</w> и не влетел в ворота."
                            }
                        },
                        {
                            "text": "Bramkarz oparł się o <słupek>, czekając na wznowienie gry.",
                            "target_lang_translations": {
                                "ru": "Вратарь прислонился к <w>штанге</w>, ожидая возобновления игры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strzelić w słupek",
                        "odbić się od słupka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальная боковая стойка ворот в командных видах спорта, таких как футбол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штанга",
                                "target_lang_sense_clarification": "часть ворот"
                            },
                            {
                                "target_lang_word": "стойка",
                                "target_lang_sense_clarification": "опора ворот"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d8c0e43-cc6e-31cb-9bb4-643687c60803",
                    "sense_definition": "Pionowy rząd liczb zapisanych jedna pod drugą, zazwyczaj w celu wykonania działania matematycznego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vertical_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel kazał nam dodać te duże liczby w <słupku>.",
                            "target_lang_translations": {
                                "ru": "Учитель велел нам сложить эти большие числа в <w>столбик</w>."
                            }
                        },
                        {
                            "text": "Zapisz wydatki w <słupku>, żeby łatwiej było je zsumować.",
                            "target_lang_translations": {
                                "ru": "Запиши расходы в <w>столбик</w>, чтобы легче было их суммировать."
                            }
                        }
                    ],
                    "synonyms": [
                        "balasek"
                    ],
                    "common_phrases": [
                        "liczyć w słupku",
                        "dodawanie w słupku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный ряд чисел, записанных друг под другом, обычно для выполнения математических действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбик",
                                "target_lang_sense_clarification": "способ записи вычислений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61cf4eab-2bd0-3a13-b156-69bb40c70398",
                    "sense_definition": "Prostokątny, pionowy element wykresu, którego wysokość reprezentuje określoną wartość liczbową.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vertical_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysokie <słupki> na wykresie oznaczają duży wzrost sprzedaży.",
                            "target_lang_translations": {
                                "ru": "Высокие <w>столбики</w> на графике означают большой рост продаж."
                            }
                        },
                        {
                            "text": "Czerwony <słupek> pokazuje spadek temperatury w nocy.",
                            "target_lang_translations": {
                                "ru": "Красный <w>столбик</w> показывает падение температуры ночью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wykres słupkowy",
                        "słupki poparcia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямоугольный вертикальный элемент графика, высота которого представляет определенное числовое значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбик",
                                "target_lang_sense_clarification": "элемент диаграммы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8db483fd-c1f3-34dd-990a-539a2ca2cd7e",
                    "sense_definition": "Widoczna wewnątrz rurki termometru ciecz (np. rtęć lub alkohol), której poziom wskazuje temperaturę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vertical_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalne dni <słupek> termometru szybko pnie się w górę.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни <w>столбик</w> термометра быстро ползет вверх."
                            }
                        },
                        {
                            "text": "Zimą <słupek> rtęci często spada poniżej zera.",
                            "target_lang_translations": {
                                "ru": "Зимой <w>столбик</w> ртути часто падает ниже нуля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "słupek rtęci",
                        "słupek termometru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимая жидкость внутри трубки термометра (например, ртуть или спирт), уровень которой указывает температуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбик",
                                "target_lang_sense_clarification": "термометра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25f2c5d0-9f2e-3c09-912e-61aa42183081",
                    "sense_definition": "Pozycja przyjmowana przez niektóre zwierzęta czworonożne, polegająca na staniu na tylnych łapach z wyprostowanym tułowiem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_pose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój pies robi <słupka>, kiedy chce dostać smakołyk.",
                            "target_lang_translations": {
                                "ru": "Моя собака делает <w>столбик</w>, когда хочет получить лакомство."
                            }
                        },
                        {
                            "text": "Zając stanął <słupka> na łące, aby rozejrzeć się po okolicy.",
                            "target_lang_translations": {
                                "ru": "Заяц встал <w>столбиком</w> на лугу, чтобы осмотреться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "robić słupka",
                        "stanąć słupka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поза, принимаемая некоторыми четвероногими животными, состоящая в стоянии на задних лапах с выпрямленным туловищем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбик",
                                "target_lang_sense_clarification": "поза животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf12bc2e-c996-3c49-9761-928fc5e26fde",
                    "sense_definition": "Rodzaj szerokiego, stabilnego obcasa w damskim obuwiu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fashion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiła sobie wygodne sandały na niskim <słupku>.",
                            "target_lang_translations": {
                                "ru": "Она купила себе удобные босоножки на низком <w>каблуке-столбике</w>."
                            }
                        },
                        {
                            "text": "Buty na <słupku> są zazwyczaj stabilniejsze niż szpilki.",
                            "target_lang_translations": {
                                "ru": "Туфли на <w>столбике</w> обычно устойчивее, чем шпильки."
                            }
                        }
                    ],
                    "antonyms": [
                        "szpilka"
                    ],
                    "common_phrases": [
                        "buty na słupku",
                        "obcas słupek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид широкого, устойчивого каблука в женской обуви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбик",
                                "target_lang_sense_clarification": "вид каблука"
                            },
                            {
                                "target_lang_word": "каблук-столбик",
                                "target_lang_sense_clarification": "вид каблука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81e088bc-07b2-3a4c-ae07-eda66befe893",
                    "sense_definition": "Żeński organ rozrodczy w kwiatach roślin okrytonasiennych, z którego po zapyleniu powstaje owoc.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pszczoła przenosi pyłek na <słupek> kwiatu.",
                            "target_lang_translations": {
                                "ru": "Пчела переносит пыльцу на <w>пестик</w> цветка."
                            }
                        },
                        {
                            "text": "Na lekcji biologii oglądaliśmy budowę <słupka> pod mikroskopem.",
                            "target_lang_translations": {
                                "ru": "На уроке биологии мы рассматривали строение <w>пестика</w> под микроскопом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znamię słupka",
                        "szyjka słupka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "botanical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женский репродуктивный орган в цветках растений, из которого после опыления образуется плод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пестик",
                                "target_lang_sense_clarification": "ботаника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd067b4b-991c-3fb2-ad94-e15c5b1970e7",
                    "sense_definition": "Jeden z podstawowych splotów w technice szydełkowania.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "crafts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten szalik jest zrobiony z samych <słupków>.",
                            "target_lang_translations": {
                                "ru": "Этот шарф связан из одних <w>столбиков</w>."
                            }
                        },
                        {
                            "text": "Musisz nawinąć nitkę na szydełko, aby zrobić <słupek>.",
                            "target_lang_translations": {
                                "ru": "Нужно накинуть нить на крючок, чтобы сделать <w>столбик</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "słupek pojedynczy",
                        "słupek podwójny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из основных видов петель в технике вязания крючком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столбик",
                                "target_lang_sense_clarification": "вязание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71d63ab6-9a2b-3e4e-aaec-3080b9e14a61",
                    "sense_definition": "A medical procedure in which a doctor cuts into a patient's body to repair or remove a damaged part.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She had to have an <w>operation</w> on her knee after the accident.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось сделать <w>операцию</w> на колене после аварии."
                            }
                        },
                        {
                            "text": "The doctor said the <w>operation</w> was successful and he will recover soon.",
                            "target_lang_translations": {
                                "ru": "Врач сказал, что <w>операция</w> прошла успешно, и он скоро поправится."
                            }
                        }
                    ],
                    "synonyms": [
                        "transformation",
                        "function"
                    ],
                    "common_phrases": [
                        "have an operation",
                        "major operation",
                        "emergency operation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинская процедура, при которой врач делает разрез на теле пациента для восстановления или удаления поврежденной части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "операция",
                                "target_lang_sense_clarification": "хирургическое вмешательство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "093dff86-7b98-30ae-8bfb-d6f9857ad546",
                    "sense_definition": "An organized activity or planned undertaking involving many people, often for a specific purpose like military, police, or rescue work.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police launched a major <w>operation</w> to catch the thieves.",
                            "target_lang_translations": {
                                "ru": "Полиция начала крупную <w>операцию</w> по поимке воров."
                            }
                        },
                        {
                            "text": "The rescue <w>operation</w> saved the lives of five people trapped in the mountains.",
                            "target_lang_translations": {
                                "ru": "Спасательная <w>операция</w> спасла жизни пяти человек, застрявших в горах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "military operation",
                        "rescue operation",
                        "police operation",
                        "covert operation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованная деятельность или спланированное мероприятие с участием многих людей, часто для конкретной цели, например, военной, полицейской или спасательной работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "операция",
                                "target_lang_sense_clarification": "спланированное действие (военное, полицейское)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb4f56ef-593c-36e0-82ba-2d22c83231e4",
                    "sense_definition": "The condition of functioning, working, or being active.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new factory will be in full <w>operation</w> by the end of the year.",

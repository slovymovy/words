                {
                    "sense_id": "587e4fe2-0194-3138-9363-5e108ca9de1d",
                    "sense_definition": "Surowo lub negatywnie oceniający kogoś lub coś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "evaluation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>krytyczne</w> uwagi sprawiły mi dużą przykrość.",
                            "target_lang_translations": {
                                "ru": "Его <w>критические</w> замечания меня очень огорчили."
                            }
                        },
                        {
                            "text": "Nauczyciel miał bardzo <w>krytyczny</w> stosunek do mojej pracy domowej.",
                            "target_lang_translations": {
                                "ru": "У учителя было очень <w>критическое</w> отношение к моей домашней работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "niepochlebny"

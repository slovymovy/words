    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed9992ab-2a5b-3916-a8ce-9bc24ba5434a",
                    "sense_definition": "Gęsta masa z przetartych owoców, często z dodatkiem cukru.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kazio zjadł cały słoik <w>musu</w> i zrobiło mu się niedobrze.",
                            "target_lang_translations": {
                                "ru": "Казик съел целую банку <w>пюре</w>, и ему стало плохо."
                            }
                        },
                        {
                            "text": "Na śniadanie często jem owsiankę z <w>musem</w> jabłkowym.",
                            "target_lang_translations": {
                                "ru": "На завтрак я часто ем овсянку с яблочным <w>пюре</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mus jabłkowy",
                        "mus owocowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густая масса из протертых фруктов, часто с добавлением сахара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пюре",
                                "target_lang_sense_clarification": "фруктовое"
                            },
                            {
                                "target_lang_word": "мусс",
                                "target_lang_sense_clarification": "фруктовый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cd4404c-7c43-37f2-849c-e392dc86a5b0",
                    "sense_definition": "Lekki, puszysty deser przygotowywany zwykle z jajek, śmietany i dodatków smakowych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia robi bardzo smaczny <w>mus</w> czekoladowy.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка делает очень вкусный шоколадный <w>мусс</w>."
                            }
                        },
                        {
                            "text": "W tej restauracji podają wyśmienity <w>mus</w> truskawkowy na deser.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают отличный клубничный <w>мусс</w> на десерт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mus czekoladowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий, воздушный десерт, обычно приготовленный из яиц, сливок и вкусовых добавок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мусс",
                                "target_lang_sense_clarification": "десерт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f487ce-cf4e-3597-8b79-70ba52aa148b",
                    "sense_definition": "Konieczność zrobienia czegoś, przymus; sytuacja bez wyjścia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trudno, <w>mus</w> to <w>mus</w>, muszę iść dzisiaj do pracy.",
                            "target_lang_translations": {
                                "ru": "Ничего не поделаешь, <w>надо</w> так <w>надо</w>, я должен идти сегодня на работу."
                            }
                        },
                        {
                            "text": "To nie jest żaden <w>mus</w>, możesz odmówić, jeśli nie chcesz tam iść.",
                            "target_lang_translations": {
                                "ru": "Это вовсе не <w>обязаловка</w>, ты можешь отказаться, если не хочешь туда идти."
                            }
                        }
                    ],
                    "synonyms": [
                        "przymus"
                    ],
                    "common_phrases": [
                        "mus to mus"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Необходимость сделать что-то, принуждение; безвыходная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необходимость",
                                "target_lang_sense_clarification": "потребность"
                            },
                            {
                                "target_lang_word": "обязаловка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "давление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "fair",
        "fairness",
        "unfair",
        "unfairly"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b47b1ece-999f-357f-b288-57cdcf86b09f",
                    "sense_definition": "To a moderate extent or degree; more than a little but less than very.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather this weekend will be <w>fairly</w> dry.",
                            "target_lang_translations": {
                                "ru": "Погода в эти выходные будет <w>довольно</w> сухой."
                            }
                        },
                        {
                            "text": "It is a <w>fairly</w> common problem among young people starting a new job.",
                            "target_lang_translations": {
                                "ru": "Это <w>довольно</w> распространенная проблема среди молодых людей, начинающих новую работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "somewhat",
                        "pretty",
                        "quite"
                    ],
                    "common_phrases": [
                        "fairly well",
                        "fairly good",
                        "fairly easy",
                        "fairly common",
                        "fairly certain"
                    ],
                    "target_lang_definitions": {
                        "ru": "В умеренной степени; довольно, но не слишком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "в значительной степени, достаточно"
                            },
                            {
                                "target_lang_word": "сравнительно",
                                "target_lang_sense_clarification": "относительно, по сравнению с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30c1376f-6026-3f07-bd86-9b64673464cb",
                    "sense_definition": "In a fair, honest, and impartial manner; without bias or injustice.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A jury is expected to ensure that the accused is <w>fairly</w> judged.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что присяжные обеспечат, чтобы обвиняемого судили <w>справедливо</w>."
                            }
                        },
                        {
                            "text": "Consumer laws aim to ensure that all customers are <w>fairly</w> treated by businesses.",
                            "target_lang_translations": {
                                "ru": "Законы о защите прав потребителей направлены на то, чтобы компании обращались со всеми клиентами <w>честно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "justly"
                    ],
                    "common_phrases": [
                        "treat fairly",
                        "act fairly",
                        "play fairly",
                        "judge fairly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справедливо, честно и беспристрастно; без предубеждений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справедливо",
                                "target_lang_sense_clarification": "беспристрастно, в соответствии с истиной"
                            },
                            {
                                "target_lang_word": "честно",
                                "target_lang_sense_clarification": "добросовестно, без обмана"
                            },
                            {
                                "target_lang_word": "беспристрастно",
                                "target_lang_sense_clarification": "без предвзятости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b30da7c4-0533-3b02-a381-4491efcb3915",
                    "sense_definition": "Used to emphasize a verb, suggesting that the action happened completely or intensely; practically.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The audience was <w>fairly</w> dazzled by the brilliant performance on stage.",
                            "target_lang_translations": {
                                "ru": "Публика была <w>просто</w> ослеплена блестящим выступлением на сцене."
                            }
                        },
                        {
                            "text": "She <w>fairly</w> ran out of the room when she heard the good news.",
                            "target_lang_translations": {
                                "ru": "Она <w>прямо-таки</w> выбежала из комнаты, когда услышала хорошие новости."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления глагола, указывая на то, что действие произошло полностью или интенсивно; буквально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо-таки",
                                "target_lang_sense_clarification": "усилительная частица, подчеркивающая интенсивность действия"
                            },
                            {
                                "target_lang_word": "просто",
                                "target_lang_sense_clarification": "в значении «совершенно» или «буквально»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
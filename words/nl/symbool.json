    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab678f3d-d113-3690-98d4-fe33314dbf15",
                    "sense_definition": "Een figuur, karakter of teken dat een bepaald idee, concept of object representeert.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De witte duif is een bekend <w>symbool</w> voor vrede.",
                            "target_lang_translations": {
                                "en": "The white dove is a well-known <w>symbol</w> of peace.",
                                "ru": "Белый голубь — известный <w>символ</w> мира."
                            }
                        },
                        {
                            "text": "Dit standbeeld staat <w>symbool</w> voor de onafhankelijkheid van het land.",
                            "target_lang_translations": {
                                "en": "This statue is a <w>symbol</w> of the country's independence.",
                                "ru": "Эта статуя является <w>символом</w> независимости страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "teken"
                    ],
                    "common_phrases": [
                        "symbool staan voor",
                        "nationaal symbool",
                        "belangrijk symbool"
                    ],
                    "target_lang_definitions": {
                        "en": "A figure, character, or sign that represents a specific idea, concept, or object.",
                        "ru": "Фигура, знак или изображение, представляющие определенную идею, понятие или объект."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "symbol",
                                "target_lang_sense_clarification": "representation of an idea"
                            },
                            {
                                "target_lang_word": "emblem",
                                "target_lang_sense_clarification": "distinctive badge or sign"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "символ",
                                "target_lang_sense_clarification": "знак, олицетворение идеи"
                            },
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "условное обозначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01f724b3-0be1-3905-a201-936e5d282f7e",
                    "sense_definition": "Een genormeerd teken om een technisch of wetenschappelijk begrip aan te duiden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de natuurkunde is de letter F het <w>symbool</w> voor kracht.",
                            "target_lang_translations": {
                                "en": "In physics, the letter F is the <w>symbol</w> for force.",
                                "ru": "В физике буква F — это <w>символ</w> силы."
                            }
                        },
                        {
                            "text": "Je moet elk chemisch <w>symbool</w> uit het periodiek systeem leren.",
                            "target_lang_translations": {
                                "en": "You have to learn every chemical <w>symbol</w> from the periodic table.",
                                "ru": "Тебе нужно выучить каждый химический <w>символ</w> из периодической системы."
                            }
                        }
                    ],
                    "synonyms": [
                        "teken"
                    ],
                    "common_phrases": [
                        "chemisch symbool",
                        "wiskundig symbool"
                    ],
                    "target_lang_definitions": {
                        "en": "A standardized sign used to denote a technical or scientific concept.",
                        "ru": "Стандартизированный знак для обозначения технического или научного понятия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "symbol",
                                "target_lang_sense_clarification": "scientific or technical character"
                            },
                            {
                                "target_lang_word": "sign",
                                "target_lang_sense_clarification": "notation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "символ",
                                "target_lang_sense_clarification": "научное или техническое обозначение"
                            },
                            {
                                "target_lang_word": "обозначение",
                                "target_lang_sense_clarification": "маркировка, знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21e67b8a-9371-3fb0-83a0-8ce6c640fc67",
                    "sense_definition": "Een geloofsbelijdenis, met name in de christelijke traditie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "creed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het Apostolisch <w>Symbool</w> vat de kern van het christelijk geloof samen.",
                            "target_lang_translations": {
                                "en": "The Apostles' <w>Creed</w> summarizes the core of the Christian faith.",
                                "ru": "Апостольский <w>символ</w> веры обобщает суть христианской веры."
                            }
                        },
                        {
                            "text": "Tijdens de dienst werd het oude <w>symbool</w> gezamenlijk uitgesproken.",
                            "target_lang_translations": {
                                "en": "During the service, the old <w>creed</w> was recited together.",
                                "ru": "Во время службы совместно произносили старый <w>символ</w> веры."
                            }
                        }
                    ],
                    "synonyms": [
                        "geloofsbelijdenis",
                        "credo",
                        "symbolum"
                    ],
                    "common_phrases": [
                        "Apostolisch Symbool"
                    ],
                    "target_lang_definitions": {
                        "en": "A confession of faith, particularly in the Christian tradition.",
                        "ru": "Исповедание веры, особенно в христианской традиции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "creed",
                                "target_lang_sense_clarification": "confession of faith"
                            },
                            {
                                "target_lang_word": "symbol",
                                "target_lang_sense_clarification": "theological summary of faith"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "символ веры",
                                "target_lang_sense_clarification": "христианское исповедание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            }
                        },
                        {
                            "text": "De politie droeg een zware <w>uitrusting</w> tijdens de demonstratie.",
                            "target_lang_translations": {
                                "ru": "Полиция была в тяжелой <w>экипировке</w> во время демонстрации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beschermende uitrusting",
                        "volledige uitrusting"
                    ],
                    "target_lang_definitions": {

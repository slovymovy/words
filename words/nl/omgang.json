    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d147ff97-a894-38c8-be1b-0efe9c55b97b",
                    "sense_definition": "De manier waarop mensen met elkaar omgaan of contact hebben.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De chagrijnige man was moeilijk in de <w>omgang</w>.",
                            "target_lang_translations": {
                                "en": "The grumpy man was difficult to <w>deal with</w>.",
                                "ru": "Этот угрюмый мужчина был тяжелым в <w>общении</w>."
                            }
                        },
                        {
                            "text": "Zij heeft veel ervaring in de <w>omgang</w> met lastige klanten.",
                            "target_lang_translations": {
                                "en": "She has a lot of experience in <w>dealing</w> with difficult customers.",
                                "ru": "У нее большой опыт <w>общения</w> с трудными клиентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "contact",
                        "verkeer"
                    ],
                    "common_phrases": [
                        "in de omgang",
                        "omgang met iemand"
                    ],
                    "target_lang_definitions": {
                        "en": "The way people interact or deal with one another.",
                        "ru": "Способ взаимодействия или общения людей друг с другом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interaction",
                                "target_lang_sense_clarification": "general social contact"
                            },
                            {
                                "target_lang_word": "dealings",
                                "target_lang_sense_clarification": "interactions, often professional or specific"
                            },
                            {
                                "target_lang_word": "relations",
                                "target_lang_sense_clarification": "formal or established connections between people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "общение",
                                "target_lang_sense_clarification": "социальный контакт, взаимодействие"
                            },
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "способ обходиться с кем-либо или чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "405b949f-90b6-36b3-a4d0-c7558937d0f0",
                    "sense_definition": "Een plechtige optocht, vaak met een religieus karakter.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "procession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jaarlijkse <w>omgang</w> trok veel bekijks in het dorp.",
                            "target_lang_translations": {
                                "en": "The annual <w>procession</w> attracted a lot of attention in the village.",
                                "ru": "Ежегодная <w>процессия</w> привлекла много внимания в деревне."
                            }
                        },
                        {
                            "text": "Tijdens de stille <w>omgang</w> liepen de mensen zwijgend door de straten.",
                            "target_lang_translations": {
                                "en": "During the silent <w>procession</w>, people walked silently through the streets.",
                                "ru": "Во время молчаливого <w>шествия</w> люди шли по улицам молча."
                            }
                        }
                    ],
                    "synonyms": [
                        "processie",
                        "optocht",
                        "stoet"
                    ],
                    "common_phrases": [
                        "stille omgang"
                    ],
                    "target_lang_definitions": {
                        "en": "A solemn group movement, often religious in nature.",
                        "ru": "Торжественное шествие, часто религиозного характера."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "procession",
                                "target_lang_sense_clarification": "ceremonial parade"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "процессия",
                                "target_lang_sense_clarification": "церемониальное шествие"
                            },
                            {
                                "target_lang_word": "шествие",
                                "target_lang_sense_clarification": "организованное движение группы людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ef80af2-618c-3c80-a372-49475b59c388",
                    "sense_definition": "Een galerij of wandelgang aan de buitenzijde van een toren of gebouw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanaf de <w>omgang</w> van de kerktoren heb je een prachtig uitzicht over de stad.",
                            "target_lang_translations": {
                                "en": "From the <w>gallery</w> of the church tower, you have a beautiful view of the city.",
                                "ru": "С <w>галереи</w> церковной башни открывается прекрасный вид на город."
                            }
                        },
                        {
                            "text": "De toeristen liepen voorzichtig over de smalle <w>omgang</w>.",
                            "target_lang_translations": {
                                "en": "The tourists walked carefully over the narrow <w>walkway</w>.",
                                "ru": "Туристы осторожно шли по узкому <w>обходу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trans",
                        "galerij"
                    ],
                    "target_lang_definitions": {
                        "en": "A gallery or walkway on the outside of a building or tower.",
                        "ru": "Галерея или проход с внешней стороны башни или здания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gallery",
                                "target_lang_sense_clarification": "architectural platform or corridor"
                            },
                            {
                                "target_lang_word": "walkway",
                                "target_lang_sense_clarification": "path for walking, often elevated"
                            },
                            {
                                "target_lang_word": "ambulatory",
                                "target_lang_sense_clarification": "walking place, specifically in church architecture"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "архитектурный элемент, балкон или коридор"
                            },
                            {
                                "target_lang_word": "обход",
                                "target_lang_sense_clarification": "проход вокруг чего-либо, например, башни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba2714e8-913f-3500-969d-d06bade78499",
                    "sense_definition": "Een volledige rondgang of omwenteling.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De machine maakte nog één laatste <w>omgang</w> voordat hij stopte.",
                            "target_lang_translations": {
                                "en": "The machine made one last <w>revolution</w> before it stopped.",
                                "ru": "Машина сделала еще один последний <w>оборот</w>, прежде чем остановилась."
                            }
                        },
                        {
                            "text": "Na een snelle <w>omgang</w> was het touw stevig vastgebonden.",
                            "target_lang_translations": {
                                "en": "After a quick <w>turn</w>, the rope was tied securely.",
                                "ru": "После быстрого <w>витка</w> веревка была крепко завязана."
                            }
                        }
                    ],
                    "synonyms": [

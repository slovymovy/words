            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0d41f080-b7f1-39dc-9dbe-53ab369386d6",
                    "sense_definition": "Het slopen of met de grond gelijk maken van gebouwen of constructies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vernietigen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente heeft besloten de oude fabriek te <w>platgooien</w> voor nieuwe woningen.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет решил <w>снести</w> старую фабрику ради строительства новых домов.",
                                "en": "The municipality has decided to <w>demolish</w> the old factory for new homes."
                            }
                        },
                        {
                            "text": "Ze gaan de hele wijk <w>platgooien</w> omdat de huizen onveilig zijn.",
                            "target_lang_translations": {
                                "ru": "Они собираются <w>снести</w> весь район, так как дома небезопасны.",
                                "en": "They are going to <w>tear down</w> the whole neighborhood because the houses are unsafe."
                            }
                        }
                    ],
                    "synonyms": [
                        "slopen",
                        "vernietigen"
                    ],
                    "common_phrases": [
                        "een gebouw platgooien",
                        "de wijk platgooien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снос или полное разрушение зданий или конструкций до основания.",
                        "en": "To demolish or level buildings or structures to the ground."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сносить",
                                "target_lang_sense_clarification": "разрушать здания"
                            },
                            {
                                "target_lang_word": "сравнивать с землёй",
                                "target_lang_sense_clarification": "полностью разрушать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "buildings or structures"
                            },
                            {
                                "target_lang_word": "tear down",
                                "target_lang_sense_clarification": "buildings"
                            },
                            {
                                "target_lang_word": "flatten",
                                "target_lang_sense_clarification": "level to the ground"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50a9fd16-706f-3275-bb9d-0fa3432f3a73",
                    "sense_definition": "Het werk of een organisatie volledig stilleggen, vaak als actiemiddel tijdens een staking.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "stilleggen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vakbonden dreigen het openbaar vervoer morgen te <w>platgooien</w>.",
                            "target_lang_translations": {
                                "ru": "Профсоюзы угрожают завтра <w>парализовать</w> работу общественного транспорта.",
                                "en": "The unions are threatening to <w>shut down</w> public transport tomorrow."
                            }
                        },
                        {
                            "text": "Als er niet naar ons geluisterd wordt, gaan we de boel <w>platgooien</w>.",
                            "target_lang_translations": {
                                "ru": "Если к нам не прислушаются, мы <w>остановим</w> всю работу.",
                                "en": "If we are not listened to, we will <w>shut</w> the whole thing <w>down</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stilleggen",
                        "staken"
                    ],
                    "common_phrases": [
                        "de boel platgooien",
                        "het werk platgooien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полная остановка работы или деятельности организации, часто как метод забастовки.",
                        "en": "To completely stop work or an organization's operations, often as a strike tactic."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парализовать",
                                "target_lang_sense_clarification": "останавливать деятельность"
                            },
                            {
                                "target_lang_word": "останавливать",
                                "target_lang_sense_clarification": "прекращать работу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "shut down",
                                "target_lang_sense_clarification": "operations or work"
                            },
                            {
                                "target_lang_word": "paralyze",
                                "target_lang_sense_clarification": "bring to a standstill"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9088a8b5-dda3-3955-9233-12b029b6d811",
                    "sense_definition": "Iets volledig verwoesten door het te bombarderen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vernietigen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De generaal gaf het bevel om de vijandelijke basis te <w>platgooien</w>.",
                            "target_lang_translations": {
                                "ru": "Генерал отдал приказ <w>разбомбить</w> вражескую базу.",
                                "en": "The general gave the order to <w>flatten</w> the enemy base."
                            }
                        },
                        {
                            "text": "In de oorlog dreigden ze de hele stad te <w>platgooien</w>.",
                            "target_lang_translations": {
                                "ru": "Во время войны они угрожали <w>стереть</w> весь город <w>с лица земли</w>.",
                                "en": "During the war, they threatened to <w>level</w> the whole city."
                            }
                        }
                    ],
                    "synonyms": [
                        "vernietigen",
                        "ruïneren"
                    ],
                    "common_phrases": [
                        "de stad platgooien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полное уничтожение объекта, обычно путем бомбардировки.",
                        "en": "To destroy something completely, usually by bombing."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбомбить",
                                "target_lang_sense_clarification": "уничтожить бомбами"
                            },
                            {
                                "target_lang_word": "стереть с лица земли",
                                "target_lang_sense_clarification": "полностью уничтожить"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "flatten",
                                "target_lang_sense_clarification": "by bombing"
                            },
                            {
                                "target_lang_word": "level",
                                "target_lang_sense_clarification": "destroy completely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52f0e153-8465-3eab-b70a-093245efc4de",
                    "sense_definition": "Zorgen dat een computersysteem, server of website niet meer functioneert.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stilleggen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hackers probeerden de website van de bank te <w>platgooien</w>.",
                            "target_lang_translations": {
                                "ru": "Хакеры пытались <w>обрушить</w> сайт банка.",
                                "en": "Hackers tried to <w>take down</w> the bank's website."
                            }
                        },
                        {
                            "text": "Het is strafbaar om een netwerk met een aanval te <w>platgooien</w>.",
                            "target_lang_translations": {
                                "ru": "Уголовно наказуемо <w>выводить из строя</w> сеть с помощью атаки.",
                                "en": "It is a criminal offense to <w>crash</w> a network with an attack."
                            }
                        }
                    ],
                    "synonyms": [

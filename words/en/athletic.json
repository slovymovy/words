                    "frequency": "Middle",
                    "semantic_group_id": "sports_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university offers an <w>athletic</w> scholarship to talented young runners.",
                            "target_lang_translations": {
                                "ru": "Университет предлагает <w>спортивную</w> стипендию талантливым молодым бегунам."
                            }
                        },
                        {
                            "text": "She joined the local <w>athletic</w> club to meet people and stay in shape.",
                            "target_lang_translations": {
                                "ru": "Она вступила в местный <w>спортивный</w> клуб, чтобы познакомиться с людьми и поддерживать форму."
                            }
                        }
                    ],
                    "synonyms": [
                        "sporting",
                        "sports"
                    ],
                    "common_phrases": [

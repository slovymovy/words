    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d45565e-254f-35cc-85f0-25f45f38e20a",
                    "sense_definition": "Iets of iemand verplaatsen door ertegen te drukken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moesten de auto naar de kant van de weg <w>duwen</w>.",
                            "target_lang_translations": {
                                "en": "We had to <w>push</w> the car to the side of the road.",
                                "ru": "Нам пришлось <w>толкать</w> машину к обочине."
                            }
                        },
                        {
                            "text": "Hij probeerde de zware deur open te <w>duwen</w>.",
                            "target_lang_translations": {
                                "en": "He tried to <w>push</w> the heavy door open.",
                                "ru": "Он пытался <w>толкнуть</w> тяжелую дверь, чтобы открыть её."
                            }
                        }
                    ],
                    "synonyms": [
                        "douwen"
                    ],
                    "antonyms": [
                        "trekken"
                    ],
                    "common_phrases": [
                        "de auto duwen",
                        "opzij duwen",
                        "hard duwen",
                        "vooruit duwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something or someone by pressing against them.",
                        "ru": "Перемещать что-либо или кого-либо, нажимая на это."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "push",
                                "target_lang_sense_clarification": "to apply force to move something away"
                            },
                            {
                                "target_lang_word": "shove",
                                "target_lang_sense_clarification": "to push roughly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "перемещать силой от себя"
                            },
                            {
                                "target_lang_word": "двигать",
                                "target_lang_sense_clarification": "приводить в движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cf73808-2f90-353b-bd8a-08f5af770b20",
                    "sense_definition": "Iets met kracht of onbeleefdheid ergens in stoppen of aan iemand geven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "force_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De man probeerde snel een folder in mijn handen te <w>duwen</w>.",
                            "target_lang_translations": {
                                "en": "The man tried to quickly <w>thrust</w> a flyer into my hands.",
                                "ru": "Мужчина пытался быстро <w>сунуть</w> мне в руки листовку."
                            }
                        },
                        {
                            "text": "Je kunt niet zomaar al je kleren in die kleine koffer <w>duwen</w>.",
                            "target_lang_translations": {
                                "en": "You can't just <w>stuff</w> all your clothes into that small suitcase.",
                                "ru": "Нельзя просто так <w>запихнуть</w> всю одежду в этот маленький чемодан."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de handen duwen",
                        "in een hoek duwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put something somewhere or give something to someone with force, haste, or rudeness.",
                        "ru": "С силой или грубостью помещать что-то внутрь чего-либо или вручать кому-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shove",
                                "target_lang_sense_clarification": "to push or put something somewhere roughly"
                            },
                            {
                                "target_lang_word": "thrust",
                                "target_lang_sense_clarification": "to force something upon someone or into something"
                            },
                            {
                                "target_lang_word": "stuff",
                                "target_lang_sense_clarification": "to push something into a small space"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "совать",
                                "target_lang_sense_clarification": "небрежно или поспешно давать/класть"
                            },
                            {
                                "target_lang_word": "запихивать",
                                "target_lang_sense_clarification": "с трудом помещать внутрь"
                            },
                            {
                                "target_lang_word": "пихать",
                                "target_lang_sense_clarification": "разг. грубо толкать или засовывать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bc5effd-2f69-3dc5-9788-c10f769ce0c8",
                    "sense_definition": "De meervoudsvorm van <w>duw</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spelers gaven elkaar enkele harde <w>duwen</w> tijdens de wedstrijd.",
                            "target_lang_translations": {
                                "en": "The players gave each other a few hard <w>shoves</w> during the match.",
                                "ru": "Во время матча игроки обменялись несколькими сильными <w>толчками</w>."
                            }
                        },
                        {
                            "text": "Door de <w>duwen</w> van de menigte viel ik bijna op de grond.",
                            "target_lang_translations": {
                                "en": "Because of the <w>pushes</w> from the crowd, I almost fell to the ground.",
                                "ru": "Из-за <w>толчков</w> толпы я чуть не упал на землю."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Acts of pushing or shoving; the plural of 'push'.",
                        "ru": "Множественное число слова «толчок»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pushes",
                                "target_lang_sense_clarification": "acts of applying force"
                            },
                            {
                                "target_lang_word": "shoves",
                                "target_lang_sense_clarification": "rough pushes"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "толчки",
                                "target_lang_sense_clarification": "резкие удары или движения от себя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
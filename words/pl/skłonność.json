    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "72077b20-8529-324a-98a1-e79bad838db4",
                    "sense_definition": "Wrodzona lub nabyta cecha charakteru lub organizmu, powodująca częste występowanie określonych zachowań, reakcji lub stanów.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent wykazuje silną <w>skłonność</w> do infekcji górnych dróg oddechowych w okresie jesiennym.",
                            "target_lang_translations": {
                                "ru": "Пациент проявляет сильную <w>склонность</w> к инфекциям верхних дыхательных путей в осенний период."
                            }
                        },
                        {
                            "text": "Jej <w>skłonność</w> do ryzyka sprawia, że często podejmuje niebezpieczne decyzje biznesowe.",
                            "target_lang_translations": {
                                "ru": "Ее <w>склонность</w> к риску приводит к тому, что она часто принимает опасные бизнес-решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "tendencja",
                        "predyspozycja",
                        "podatność",
                        "inklinacja"
                    ],
                    "antonyms": [
                        "brak skłonności"
                    ],
                    "common_phrases": [
                        "skłonność do tycia",
                        "skłonność do agresji",
                        "wrodzona skłonność",
                        "skłonność do kompromisów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Врожденная или приобретенная черта характера или организма, вызывающая частое проявление определенного поведения, реакций или состояний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склонность",
                                "target_lang_sense_clarification": "предрасположенность"
                            },
                            {
                                "target_lang_word": "предрасположенность",
                                "target_lang_sense_clarification": "организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaa70485-afea-3148-afbd-f9e66c7db5e9",
                    "sense_definition": "Silne upodobanie do czegoś, nawyk lub słabość do wykonywania określonych czynności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo diety, wciąż mam <w>skłonność</w> do podjadania słodyczy późnym wieczorem.",
                            "target_lang_translations": {
                                "ru": "Несмотря на диету, у меня все еще есть <w>склонность</w> перекусывать сладостями поздно вечером."
                            }
                        },
                        {
                            "text": "Marek ma niebezpieczną <w>skłonność</w> do hazardu, przez którą stracił wiele pieniędzy.",
                            "target_lang_translations": {
                                "ru": "У Марека опасная <w>склонность</w> к азартным играм, из-за которой он потерял много денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "upodobanie",
                        "słabość",
                        "ciągoty",
                        "pociąg",
                        "ciągotki"
                    ],
                    "antonyms": [

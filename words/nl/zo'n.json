    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "d9034d92-3b69-34cd-8e02-cb87962dc91c",
                    "sense_definition": "Gebruikt om een bepaalde eigenschap, soort of graad aan te duiden; verwijst naar iets dat eerder genoemd is of bekend wordt verondersteld.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil niet <w>zo'n</w> grote biefstuk.",
                            "target_lang_translations": {
                                "en": "I don't want <w>such a</w> big steak.",
                                "ru": "Я не хочу <w>такой</w> большой стейк."
                            }
                        },
                        {
                            "text": "Ik heb nog nooit <w>zo'n</w> mooie auto gezien.",
                            "target_lang_translations": {
                                "en": "I have never seen <w>such a</w> beautiful car.",
                                "ru": "Я никогда не видел <w>такой</w> красивой машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "zulk",
                        "dergelijk"
                    ],
                    "common_phrases": [
                        "zo'n ding",
                        "zo'n geval"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verkorting van 'zo een'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate a certain quality, type, or degree; refers to something mentioned earlier or assumed to be known.",
                        "ru": "Используется для указания на определенное качество, вид или степень; ссылается на что-то, упомянутое ранее или подразумеваемое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "such a",
                                "target_lang_sense_clarification": "used before a singular noun to emphasize a quality"
                            },
                            {
                                "target_lang_word": "that kind of",
                                "target_lang_sense_clarification": "referring to a specific type or sort"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "такой",
                                "target_lang_sense_clarification": "указание на качество или степень"
                            },
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "схожий по свойствам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a1613f3-95af-30f8-98e9-06700bf8c1f3",
                    "sense_definition": "Gebruikt in uitroepen om verbazing, bewondering of afkeuring over een eigenschap uit te drukken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "demonstrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zo'n</w> groot huis!",
                            "target_lang_translations": {
                                "en": "<w>Such a</w> big house!",
                                "ru": "<w>Такой</w> большой дом!"
                            }
                        },
                        {
                            "text": "Het was <w>zo'n</w> leuke verrassing voor ons allemaal.",
                            "target_lang_translations": {
                                "en": "It was <w>such a</w> nice surprise for all of us.",
                                "ru": "Это был <w>такой</w> приятный сюрприз для всех нас."
                            }
                        }
                    ],
                    "synonyms": [
                        "wat een"
                    ],
                    "common_phrases": [
                        "zo'n onzin",
                        "zo'n geluk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verkorting van 'zo een'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used in exclamations to express surprise, admiration, or disapproval regarding a quality.",
                        "ru": "Используется в восклицаниях для выражения удивления, восхищения или неодобрения по поводу какого-либо качества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "such a",
                                "target_lang_sense_clarification": "emphatic use in exclamations"
                            },
                            {
                                "target_lang_word": "what a",
                                "target_lang_sense_clarification": "used in exclamatory sentences"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "такой",
                                "target_lang_sense_clarification": "для эмоционального усиления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d921b1f-9b9f-33d8-98b3-c401b0ebd75a",
                    "sense_definition": "Geeft een schatting van een hoeveelheid of getal aan; niet precies.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantitative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hoeveel wilt u? <w>Zo'n</w> twee ons.",
                            "target_lang_translations": {
                                "en": "How much do you want? <w>About</w> two hundred grams.",
                                "ru": "Сколько вы хотите? <w>Примерно</w> двести грамм."
                            }
                        },
                        {
                            "text": "De reis naar Parijs duurt <w>zo'n</w> drie uur met de trein.",
                            "target_lang_translations": {
                                "en": "The trip to Paris takes <w>about</w> three hours by train.",
                                "ru": "Поездка в Париж на поезде занимает <w>около</w> трех часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "zulk"
                    ],
                    "common_phrases": [
                        "zo'n beetje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verkorting van 'zo een'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an estimate of a quantity or number; not precise.",
                        "ru": "Указывает на приблизительное количество или число; не точно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "about",
                                "target_lang_sense_clarification": "indicating an approximate amount"
                            },
                            {
                                "target_lang_word": "around",
                                "target_lang_sense_clarification": "indicating an approximate number or time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "приблизительно"
                            },
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "приблизительно"
                            },
                            {
                                "target_lang_word": "где-то",
                                "target_lang_sense_clarification": "разговорное, приблизительно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
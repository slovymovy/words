    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "312e384f-77a1-3f28-9213-23dfe0198d60",
                    "sense_definition": "postać biblijna ze Starego Testamentu, symbol cierpienia i niezachwianej wiary; w polskiej tradycji częściej znana jako Hiob",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biblical_job",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Według przekazów biblijnych <w>Job</w> stracił cały majątek i rodzinę, lecz nie wyrzekł się Boga.",
                            "target_lang_translations": {
                                "ru": "Согласно библейским преданиям, <w>Иов</w> потерял всё имущество и семью, но не отрекся от Бога."
                            }
                        },
                        {
                            "text": "Cierpienie, którego doświadczył <w>Job</w>, jest tematem wielu rozważań teologicznych.",
                            "target_lang_translations": {
                                "ru": "Страдание, которое испытал <w>Иов</w>, является темой многих богословских размышлений."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hiob"
                    ],
                    "common_phrases": [
                        "Księga Joba",
                        "cierpienie Joba"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "form less common than Hiob"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский персонаж из Ветхого Завета, символ страдания и непоколебимой веры; в русской традиции известен как Иов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иов",
                                "target_lang_sense_clarification": "библейский персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35a0e66f-36f0-3372-a516-627c93d266e9",
                    "sense_definition": "rzadkie imię męskie pochodzenia biblijnego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biblical_job",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "W starych księgach parafialnych odnaleziono zapis, że jego pradziadek nosił imię <w>Job</w>.",
                            "target_lang_translations": {
                                "ru": "В старых приходских книгах была найдена запись, что его прадед носил имя <w>Иов</w>."
                            }
                        },
                        {
                            "text": "Współcześnie imię <w>Job</w> jest nadawane dzieciom niezwykle rzadko.",
                            "target_lang_translations": {
                                "ru": "В наше время имя <w>Иов</w> дают детям крайне редко."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Редкое мужское имя библейского происхождения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иов",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff9d4a87-f5af-38ba-acb0-f86d244f6170",
                    "sense_definition": "skrót oznaczający biblijną Księgę Joba (Hioba), stosowany w odsyłaczach i przypisach",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biblical_job",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "Werset ten oznaczony jest w przypisie jako <w>Job</w> 1,1.",
                            "target_lang_translations": {
                                "ru": "Этот стих обозначен в сноске как <w>Иов</w> 1,1."
                            }
                        },
                        {
                            "text": "Podczas studium biblijnego sprawdziliśmy fragment opisany skrótem <w>Job</w>.",
                            "target_lang_translations": {
                                "ru": "Во время изучения Библии мы проверили отрывок, обозначенный сокращением <w>Иов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hi"
                    ],
                    "common_phrases": [
                        "Księga Joba"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение, обозначающее библейскую Книгу Иова, используемое в ссылках и сносках"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иов",
                                "target_lang_sense_clarification": "сокращение названия книги Библии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
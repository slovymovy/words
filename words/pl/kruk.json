    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4182f476-d13b-37b9-9870-d1e1b7dc03bd",
                    "sense_definition": "Duży, czarny ptak z rodziny krukowatych, wydający charakterystyczne, chrapliwe dźwięki, często występujący w legendach i literaturze.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na szczycie starego dębu siedział wielki, czarny <w>kruk</w>.",
                            "target_lang_translations": {
                                "ru": "На вершине старого дуба сидел большой черный <w>ворон</w>."
                            }
                        },
                        {
                            "text": "W wielu kulturach <w>kruk</w> jest symbolem mądrości lub tajemnicy.",
                            "target_lang_translations": {
                                "ru": "Во многих культурах <w>ворон</w> является символом мудрости или тайны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biały kruk",
                        "kruk krukowi oka nie wykole"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная черная птица семейства врановых, издающая характерные хриплые звуки, часто встречающаяся в легендах и литературе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворон",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6f53236-6be7-322b-916b-56b0c9c5c416",
                    "sense_definition": "Metalowy pręt, często zakrzywiony na końcu, służący do poruszania węgla lub drewna w piecu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek wziął żelazny <w>kruk</w>, aby poprawić ogień w kominku.",
                            "target_lang_translations": {
                                "ru": "Дедушка взял железную <w>кочергу</w>, чтобы поправить огонь в камине."
                            }
                        },
                        {
                            "text": "Przy piecu stał oparty stary, zardzewiały <w>kruk</w>.",
                            "target_lang_translations": {
                                "ru": "У печи стояла старая, ржавая <w>кочерга</w>."
                            }
                        },
                        {
                            "text": "Kowal wykuł nowy <w>kruk</w> do obsługi pieca chlebowego.",
                            "target_lang_translations": {
                                "ru": "Кузнец выковал новую <w>кочергу</w> для обслуживания хлебной печи."
                            }
                        }
                    ],
                    "synonyms": [
                        "pogrzebacz"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie częściej używa się słowa pogrzebacz"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический прут, часто загнутый на конце, служащий для перемешивания угля или дров в печи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кочерга",
                                "target_lang_sense_clarification": "инструмент для печи"
                            },
                            {
                                "target_lang_word": "крюк",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43fcde4f-2544-3cd7-b597-811301eb1aae",
                    "sense_definition": "Osoba bezrobotna, nieposiadająca stałego zatrudnienia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "people_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tamtych czasach niejeden <w>kruk</w> szukał dorywczej pracy na budowie.",
                            "target_lang_translations": {
                                "ru": "В те времена не один <w>безработный</w> искал подработку на стройке."
                            }
                        },
                        {
                            "text": "Mówili na niego <w>kruk</w>, bo od miesięcy nie mógł znaleźć posady.",
                            "target_lang_translations": {
                                "ru": "Его называли <w>безработным</w>, так как он месяцами не мог найти место."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "gwara warszawska"
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Безработный человек, не имеющий постоянного трудоустройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безработный",
                                "target_lang_sense_clarification": "человек"
                            }
                        ]
                    }
                }
            ]
        },

                    "sense_id": "2483d279-0672-3e80-80d8-9688ea74d3bd",
                    "sense_definition": "Kleding of textiel gladmaken met een heet strijkijzer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "household",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet vanavond nog vijf overhemden <w>strijken</w> voor mijn werk.",
                            "target_lang_translations": {

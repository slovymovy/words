                            },
                            {
                                "target_lang_word": "получаться",
                                "target_lang_sense_clarification": "выходить (у кого-либо)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to be able to do something difficult"
                            },
                            {
                                "target_lang_word": "succeed",
                                "target_lang_sense_clarification": "to achieve a goal or complete a task"
                            }
                        ]
                    }

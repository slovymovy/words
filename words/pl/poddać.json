{
    "word_family": [
        "poddanie",
        "poddawać",
        "poddany"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c4ef3ac-ea8d-331e-b1e4-d520531ea924",
                    "sense_definition": "Uznać swoją porażkę, zrezygnować z dalszej walki, oporu lub wysiłku (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "surrender_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo zmęczenia biegacz nie zamierzał się <w>poddać</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на усталость, бегун не собирался <w>сдаваться</w>."
                            }
                        },
                        {
                            "text": "Po długim oblężeniu żołnierze postanowili się <w>poddać</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой осады солдаты решили <w>сдаться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skapitulować",
                        "dać za wygraną"
                    ],
                    "common_phrases": [
                        "poddać się bez walki",
                        "nigdy się nie poddać",
                        "poddać się karze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Признать своё поражение, отказаться от дальнейшей борьбы, сопротивления или усилий (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдаться",
                                "target_lang_sense_clarification": "прекратить сопротивление, признать поражение"
                            },
                            {
                                "target_lang_word": "уступить",
                                "target_lang_sense_clarification": "поддаться давлению, перестать спорить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22c93a91-a8fa-3b28-9044-94b9061be0ce",
                    "sense_definition": "Sprawić, że ktoś lub coś stanie się obiektem jakiegoś działania, procesu, badania lub oceny.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "subject_to_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zdecydował <w>poddać</w> pacjenta szczegółowym badaniom.",
                            "target_lang_translations": {
                                "ru": "Врач решил <w>подвергнуть</w> пациента детальному обследованию."
                            }
                        },
                        {
                            "text": "Każdą nową teorię należy <w>poddać</w> w wątpliwość i sprawdzić.",
                            "target_lang_translations": {
                                "ru": "Каждую новую теорию следует <w>подвергнуть</w> сомнению и проверить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poddać krytyce",
                        "poddać analizie",
                        "poddać głosowaniu",
                        "poddać w wątpliwość",
                        "poddać dyskusji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать кого-то или что-то объектом какого-либо действия, процесса, исследования или оценки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвергнуть",
                                "target_lang_sense_clarification": "воздействию, испытанию, критике или анализу"
                            },
                            {
                                "target_lang_word": "предать",
                                "target_lang_sense_clarification": "в устойчивых сочетаниях (напр. суду, гласности)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0d4054b-385e-30cc-bf35-ac5f242c76fa",
                    "sense_definition": "Oddać coś (np. terytorium, mecz) przeciwnikowi, uznając jego przewagę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "surrender_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Generał rozkazał <w>poddać</w> miasto, aby uniknąć ofiar wśród cywilów.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал <w>сдать</w> город, чтобы избежать жертв среди мирного населения."
                            }
                        },
                        {
                            "text": "Szachista widząc brak szans na wygraną, wolał <w>poddać</w> partię.",
                            "target_lang_translations": {
                                "ru": "Шахматист, видя отсутствие шансов на победу, предпочел <w>сдать</w> партию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poddać mecz",
                        "poddać miasto",
                        "poddać partię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдать что-то (например, территорию, матч) противнику, признавая его превосходство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "отдать врагу или противнику (город, партию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cafa8cc8-2467-38f9-9677-d0a1d1a6543d",
                    "sense_definition": "Zasugerować coś, podsunąć komuś myśl lub propozycję do rozważenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "suggest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <w>poddać</w> wam pewien pomysł pod rozwagę.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>предложить</w> вам одну идею на рассмотрение."
                            }
                        },
                        {
                            "text": "To wydarzenie <w>poddało</w> mi temat do nowej książki.",
                            "target_lang_translations": {
                                "ru": "Это событие <w>подкинуло</w> мне тему для новой книги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poddać myśl",
                        "poddać pod rozwagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложить что-то, навести на мысль или выдвинуть предложение для рассмотрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложить",
                                "target_lang_sense_clarification": "идею или тему для обсуждения"
                            },
                            {
                                "target_lang_word": "подкинуть",
                                "target_lang_sense_clarification": "разг. навести на мысль, подсказать"
                            },
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "на рассмотрение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
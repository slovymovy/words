                    "semantic_group_id": "save_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De handelaar had na jaren werken een aanzienlijk vermogen <w>overgewonnen</w>.",
                            "target_lang_translations": {
                                "en": "The merchant had <w>saved</w> a considerable fortune after years of work.",
                                "ru": "Торговец <w>скопил</w> значительное состояние после многих лет работы."
                            }
                        },
                        {
                            "text": "Hij probeerde elk jaar wat geld <w>over</w> te <w>winnen</w> voor zijn oude dag.",
                            "target_lang_translations": {

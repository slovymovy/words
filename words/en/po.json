                {
                    "sense_id": "f951c159-7f55-30d5-bcf5-824795b04799",
                    "sense_definition": "An abbreviation for post office, a facility for sending and receiving mail.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abbreviations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to stop by the <w>PO</w> to mail this package before it closes.",
                            "target_lang_translations": {
                                "ru": "Мне нужно зайти на <w>почту</w>, чтобы отправить эту посылку до закрытия."
                            }
                        },
                        {
                            "text": "The local <w>PO</w> is always busy on Monday mornings.",
                            "target_lang_translations": {
                                "ru": "В местном <w>почтовом отделении</w> всегда много народу по утрам в понедельник."
                            }
                        }
                    ],
                    "common_phrases": [
                        "local PO",
                        "go to the PO"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"post office\" (почтовое отделение), учреждения для отправки и получения почты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почта",
                                "target_lang_sense_clarification": "учреждение"
                            },
                            {
                                "target_lang_word": "почтовое отделение",
                                "target_lang_sense_clarification": "офис"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7196f8b3-8a20-3111-9580-f0ce750903b3",
                    "sense_definition": "An abbreviation for purchase order, a commercial document issued by a buyer to a seller.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abbreviations",
                    "name_type": "no",

{
    "word_family": [
        "swojski",
        "swojak",
        "oswoić",
        "przyswoić",
        "swoisty"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "a669fbc1-e0ba-36b3-a83e-525d1964ab3b",
                    "sense_definition": "Należący do podmiotu zdania; zaimek zastępujący słowa: mój, twój, jego, jej, nasz, wasz, ich, gdy odnoszą się one do wykonawcy czynności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czerwony Kapturek codziennie odwiedzała <swoją> babcię.",
                            "target_lang_translations": {
                                "ru": "Красная Шапочка каждый день навещала <свою> бабушку."
                            }
                        },
                        {
                            "text": "Każdy powinien pilnować <swoich> spraw i nie wtrącać się w życie innych.",
                            "target_lang_translations": {
                                "ru": "Каждый должен заниматься <своими> делами и не вмешиваться в жизнь других."
                            }
                        }
                    ],
                    "synonyms": [
                        "własny"
                    ],
                    "common_phrases": [
                        "wszystko w swoim czasie",
                        "robić swoje",
                        "wyjść na swoje",
                        "nie w swoim sosie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий подлежащему; местоимение, заменяющее слова: мой, твой, его, её, наш, ваш, их, когда они относятся к исполнителю действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            },
                            {
                                "target_lang_word": "собственный",
                                "target_lang_sense_clarification": "принадлежащий себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b7a2358-e271-321b-a93c-3a9388cb0aff",
                    "sense_definition": "Osoba bliska, znajoma, należąca do tej samej rodziny, grupy społecznej lub środowiska; ktoś, kto nie jest obcy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wpuśćcie go, ja go znam, on jest <swój>.",
                            "target_lang_translations": {
                                "ru": "Впустите его, я его знаю, он <свой>."
                            }
                        },
                        {
                            "text": "Wśród <swoich> zawsze czuję się bezpiecznie i mogę być sobą.",
                            "target_lang_translations": {
                                "ru": "Среди <своих> я всегда чувствую себя в безопасности и могу быть собой."
                            }
                        }
                    ],
                    "synonyms": [
                        "swojak"
                    ],
                    "antonyms": [
                        "obcy"
                    ],
                    "common_phrases": [
                        "swój człowiek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Близкий, знакомый человек, принадлежащий к той же семье, социальной группе или среде; кто-то не чужой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "близкий, родной, не чужой"
                            },
                            {
                                "target_lang_word": "наш",
                                "target_lang_sense_clarification": "из нашей группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b364fa5-6f4e-3945-9c36-e0841ec83995",
                    "sense_definition": "O człowieku: sympatyczny, życzliwy, bezpośredni w obejściu, taki, na którym można polegać.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jerzy znów stawia piwo, to jest <swój> chłop!",
                            "target_lang_translations": {
                                "ru": "Ежи снова ставит пиво, это <свой> парень!"
                            }
                        },
                        {
                            "text": "Marek to naprawdę <swój> gość, na pewno ci pomoże w potrzebie.",
                            "target_lang_translations": {
                                "ru": "Марек — действительно <свой> человек, он точно поможет тебе в беде."
                            }
                        }
                    ],
                    "synonyms": [
                        "równy",
                        "fajny"
                    ],
                    "common_phrases": [
                        "swój chłop"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: симпатичный, доброжелательный, простой в общении, на которого можно положиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "простой, надежный (о человеке)"
                            },
                            {
                                "target_lang_word": "свой парень",
                                "target_lang_sense_clarification": "разговорное выражение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
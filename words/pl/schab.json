    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0a11fda0-12ad-332e-910e-c1bd6162e2b5",
                    "sense_definition": "Rodzaj mięsa wieprzowego wycinany z okolic kręgosłupa na odcinku piersiowo-lędźwiowym tuszy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_meat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić kilogram <w>schabu</w> w sklepie mięsnym.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить килограмм <w>корейки</w> в мясном магазине."
                            }
                        },
                        {
                            "text": "Ten <w>schab</w> wygląda na bardzo świeży i chudy.",
                            "target_lang_translations": {
                                "ru": "Эта <w>корейка</w> выглядит очень свежей и постной."
                            }
                        }
                    ],
                    "synonyms": [
                        "wieprzowina"
                    ],
                    "common_phrases": [
                        "kotlet schabowy",
                        "schab bez kości",
                        "schab z kością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть свиной туши, вырезаемая вдоль позвоночника (спинная часть); корейка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корейка",
                                "target_lang_sense_clarification": "часть туши"
                            },
                            {
                                "target_lang_word": "карбонад",
                                "target_lang_sense_clarification": "мясо без кости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "395a824c-8cef-39b0-9d2d-85c75ef4a61d",
                    "sense_definition": "Potrawa przygotowana z mięsa schabowego, często pieczona, duszona lub smażona.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_dish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niedzielny obiad mama podała pyszny pieczony <w>schab</w>.",
                            "target_lang_translations": {
                                "ru": "На воскресный обед мама подала вкусную запеченную <w>корейку</w>."
                            }
                        },
                        {
                            "text": "Ten <w>schab</w> ze śliwką jest specjalnością naszej restauracji.",
                            "target_lang_translations": {
                                "ru": "Эта <w>корейка</w> с черносливом — фирменное блюдо нашего ресторана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schab pieczony",
                        "schab ze śliwką",
                        "schab w sosie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блюдо, приготовленное из свиной корейки (часто запеченное или жареное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корейка",
                                "target_lang_sense_clarification": "блюдо"
                            },
                            {
                                "target_lang_word": "буженина",
                                "target_lang_sense_clarification": "запеченное мясо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbbab72b-b7cc-34b1-bce0-64b7f4f33651",
                    "sense_definition": "Młody, bardzo umięśniony i postawny mężczyzna, często kojarzony z siłownią i agresywnym wyglądem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wejściem do klubu stał wielki <w>schab</w> i sprawdzał bilety.",
                            "target_lang_translations": {
                                "ru": "Перед входом в клуб стоял огромный <w>амбал</w> и проверял билеты."
                            }
                        },
                        {
                            "text": "Nie zaczepiaj go, to miejscowy <w>schab</w>, z którym lepiej nie zadzierać.",
                            "target_lang_translations": {
                                "ru": "Не цепляйся к нему, это местный <w>амбал</w>, с которым лучше не связываться."
                            }
                        }
                    ],
                    "synonyms": [
                        "mięśniak",
                        "kark",
                        "paker"
                    ],
                    "antonyms": [
                        "chuderlak"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used pejoratively or descriptively in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный, мускулистый мужчина, часто с агрессивной внешностью (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "амбал",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "качок",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "шкаф",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]
        }

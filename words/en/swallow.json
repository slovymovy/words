                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "African contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид блюда на углеводной основе, распространенный в африканской кухне; мягкая масса, которую глотают, почти не пережевывая."
                    },
                    "translations": {
                        "ru": [

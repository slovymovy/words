                        ],
                        "ru": [
                            {
                                "target_lang_word": "зяблик",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4ae6f93-04e2-376f-9fa0-3bf0121b26bd",

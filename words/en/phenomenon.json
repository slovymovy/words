    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d684ded2-ee11-306f-a37e-2640f4104011",
                    "sense_definition": "A fact, event, or situation that can be observed to exist or happen, especially one that is unusual or interesting.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hurricanes are a powerful meteorological <w>phenomenon</w> that can cause great destruction."
                        },
                        {
                            "text": "The rapid rise of social media is a global <w>phenomenon</w> of the twenty-first century."
                        }
                    ],
                    "synonyms": [
                        "event"
                    ],
                    "common_phrases": [
                        "natural phenomenon",
                        "social phenomenon",
                        "common phenomenon",
                        "strange phenomenon"
                    ]
                },
                {
                    "sense_id": "4d37661f-0cd0-3034-ba51-45afbb9b24e7",
                    "sense_definition": "A person or thing that is very remarkable, impressive, or successful.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "marvel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young pianist was considered a <w>phenomenon</w> by critics around the world."
                        },
                        {
                            "text": "This new software is a <w>phenomenon</w> that has completely changed the industry."
                        }
                    ],
                    "synonyms": [
                        "prodigy",
                        "marvel",
                        "wonder",
                        "phenom"
                    ]
                },
                {
                    "sense_id": "8d2b4400-476d-3357-9599-f4feac22ae63",
                    "sense_definition": "An object or event as it is perceived by the senses, as distinct from how it exists in reality.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "philosophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kant argued that we can only know the <w>phenomenon</w>, not the thing-in-itself."
                        },
                        {
                            "text": "Every <w>phenomenon</w> is shaped by the mind's perception of the external world."
                        }
                    ],
                    "antonyms": [
                        "noumenon"
                    ]
                }
            ]
        }
    ]

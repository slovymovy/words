                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан приказал сбросить <w>балласт</w>, чтобы воздушный шар смог подняться выше."
                        },
                        {
                            "text": "В трюмы корабля закачали водяной <w>балласт</w> для обеспечения остойчивости в штормовом море."
                        }
                    ],
                    "common_phrases": [
                        "сбросить балласт",
                        "водяной балласт",
                        "набрать балласт"
                    ],
                    "traits": [
                        {

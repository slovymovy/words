    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ab55c474-fc28-395c-b30e-64b797ae70be",
                    "sense_definition": "Słowo używane, aby przypomnieć rozmówcy o czymś, co jest oczywiste, znane lub zrozumiałe.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie musisz mi tego tłumaczyć, <w>przecież</w> doskonale to rozumiem.",
                            "target_lang_translations": {
                                "ru": "Не нужно мне это объяснять, я <w>ведь</w> прекрасно это понимаю."
                            }
                        },
                        {
                            "text": "<w>Przecież</w> mówiłem ci, żebyś zabrał ze sobą parasol.",
                            "target_lang_translations": {
                                "ru": "Я <w>же</w> говорил тебе взять с собой зонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "wszak",
                        "przecie",
                        "toż"
                    ],
                    "common_phrases": [
                        "no przecież",
                        "przecież wiesz",
                        "przecież to jasne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что факт является известным, очевидным или понятным собеседнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведь",
                                "target_lang_sense_clarification": "указывает на причину или известный факт"
                            },
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "усилительная частица для напоминания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1d3a5a3-77ed-3a3c-8ce9-d3a57097727a",
                    "sense_definition": "Słowo wyrażające sprzeciw, zdziwienie lub oburzenie w nawiązaniu do zaistniałej sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Przecież</w> obiecałeś, że pomożesz mi w ten weekend!",
                            "target_lang_translations": {
                                "ru": "Ты <w>же</w> обещал, что поможешь мне в эти выходные!"
                            }
                        },
                        {
                            "text": "Jak to nie ma biletów? <w>Przecież</w> wczoraj jeszcze były dostępne.",
                            "target_lang_translations": {
                                "ru": "Как это нет билетов? <w>Ведь</w> вчера ещё были доступны."
                            }
                        }
                    ],
                    "synonyms": [
                        "przecie"
                    ],
                    "common_phrases": [
                        "ale przecież",
                        "przecież to niemożliwe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает возражение, удивление или негодование по поводу сложившейся ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "выражает эмоциональное усиление, упрек"
                            },
                            {
                                "target_lang_word": "ведь",
                                "target_lang_sense_clarification": "используется в эмоциональных возражениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]

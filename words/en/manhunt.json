    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b966c2c5-58d6-33f3-9557-2529c5887cfd",
                    "sense_definition": "An organized search for a person, usually a criminal or a fugitive.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police launched a massive <w>manhunt</w> to find the escaped prisoner.",
                            "target_lang_translations": {
                                "ru": "Полиция организовала масштабную <w>облаву</w> для поимки сбежавшего заключенного."
                            }
                        },
                        {
                            "text": "After the robbery, a citywide <w>manhunt</w> began immediately.",
                            "target_lang_translations": {
                                "ru": "После ограбления немедленно начался общегородской <w>розыск</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "search",
                        "pursuit"
                    ],
                    "common_phrases": [
                        "massive manhunt",
                        "launch a manhunt",
                        "police manhunt",
                        "nationwide manhunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованные поиски человека, обычно преступника или беглеца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розыск",
                                "target_lang_sense_clarification": "поиск преступника"
                            },
                            {
                                "target_lang_word": "облава",
                                "target_lang_sense_clarification": "операция по окружению и поимке"
                            },
                            {
                                "target_lang_word": "преследование",
                                "target_lang_sense_clarification": "погоня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c554d543-2c12-3b64-ad19-43010d4a70ea",
                    "sense_definition": "A children's game similar to hide-and-seek where players who are caught join the team of seekers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kids went outside to play <w>manhunt</w> in the park until it got dark.",
                            "target_lang_translations": {
                                "ru": "Дети пошли в парк играть в <w>казаки-разбойники</w>, пока не стемнело."
                            }
                        },
                        {
                            "text": "In <w>manhunt</w>, the game becomes more difficult as more people start looking for you.",
                            "target_lang_translations": {
                                "ru": "В <w>казаках-разбойниках</w> игра становится сложнее, когда всё больше людей начинают тебя искать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play manhunt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детская игра, похожая на прятки, в которой пойманные игроки присоединяются к команде ищущих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казаки-разбойники",
                                "target_lang_sense_clarification": "командная дворовая игра"
                            }
                        ]
                    }
                }
            ]
        }
    ]

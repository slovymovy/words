    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "65c124df-c5b9-399f-99cf-f75ac8d483b2",
                    "sense_definition": "A set of intended actions, usually mutually related, through which one expects to achieve a goal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "strategy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you have a <w>plan</w> for the weekend?",
                            "target_lang_translations": {
                                "ru": "У тебя есть <w>план</w> на выходные?"
                            }
                        },
                        {
                            "text": "Everything went according to <w>plan</w> during the event.",
                            "target_lang_translations": {
                                "ru": "Во время мероприятия всё шло по <w>плану</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheme",
                        "proposal",
                        "project",
                        "design"
                    ],
                    "common_phrases": [
                        "make a plan",
                        "have a plan",
                        "according to plan",
                        "change of plan",
                        "action plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заранее намеченная последовательность действий для достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "замысел, порядок действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "993a5f3e-39b6-32ce-9fa0-b8fed93e557d",
                    "sense_definition": "A drawing showing technical details of a building, machine, or area, often using symbols to represent specific features.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "drawing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect showed us the <w>plans</w> for the new house.",
                            "target_lang_translations": {
                                "ru": "Архитектор показал нам <w>планы</w> нового дома."
                            }
                        },
                        {
                            "text": "We looked at the floor <w>plan</w> to find the emergency exits.",
                            "target_lang_translations": {
                                "ru": "Мы посмотрели на <w>план</w> этажа, чтобы найти аварийные выходы."
                            }
                        }
                    ],
                    "synonyms": [
                        "blueprint",
                        "diagram",
                        "floor plan",
                        "chart",
                        "schematic",
                        "design",
                        "map"
                    ],
                    "common_phrases": [
                        "floor plan",
                        "building plan",
                        "seating plan",
                        "city plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чертёж, показывающий технические детали здания, машины или местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "схема местности или помещения"
                            },
                            {
                                "target_lang_word": "чертёж",
                                "target_lang_sense_clarification": "технический рисунок"
                            },
                            {
                                "target_lang_word": "схема",
                                "target_lang_sense_clarification": "графическое изображение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cec1b71-0147-30aa-b3b4-564f7d3aa62a",
                    "sense_definition": "A subscription to a service, often specifying the cost and available features.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "subscription",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to upgrade my mobile phone <w>plan</w> to get more data.",
                            "target_lang_translations": {
                                "ru": "Мне нужно обновить мой <w>тариф</w> мобильной связи, чтобы получить больше интернета."
                            }
                        },
                        {
                            "text": "The gym offers a monthly payment <w>plan</w> for members.",
                            "target_lang_translations": {
                                "ru": "Спортзал предлагает ежемесячный <w>план</w> оплаты для клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheme"
                    ],
                    "common_phrases": [
                        "phone plan",
                        "data plan",
                        "payment plan",
                        "retirement plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подписка на услугу с определённой стоимостью и набором функций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тариф",
                                "target_lang_sense_clarification": "условия оплаты услуг"

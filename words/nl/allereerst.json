{
    "word_family": [
        "eerst",
        "eerste",
        "allerlaatst"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ad0c2ee9-fb78-3696-b1d6-543b1422f1d2",
                    "sense_definition": "Helemaal in de eerste plaats; om mee te beginnen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Allereerst</w> wil ik iedereen bedanken voor de goede samenwerking.",
                            "target_lang_translations": {
                                "en": "<w>First of all</w>, I want to thank everyone for the good cooperation.",
                                "ru": "<w>Прежде всего</w> я хочу поблагодарить всех за хорошее сотрудничество."
                            }
                        },
                        {
                            "text": "We moeten <w>allereerst</w> uitzoeken wat er precies is gebeurd.",
                            "target_lang_translations": {
                                "en": "We must <w>first of all</w> find out exactly what happened.",
                                "ru": "Мы должны <w>в первую очередь</w> выяснить, что именно произошло."
                            }
                        }
                    ],
                    "synonyms": [
                        "ten eerste",
                        "om te beginnen",
                        "in de eerste plaats"
                    ],
                    "antonyms": [
                        "ten laatste",
                        "tot slot",
                        "als laatste"
                    ],
                    "common_phrases": [
                        "allereerst wil ik",
                        "maar allereerst"
                    ],
                    "target_lang_definitions": {
                        "en": "First of all; to begin with.",
                        "ru": "В первую очередь; для начала."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "first of all",
                                "target_lang_sense_clarification": "used to introduce the first topic or point"
                            },
                            {
                                "target_lang_word": "first and foremost",
                                "target_lang_sense_clarification": "emphasizing priority or importance"
                            },
                            {
                                "target_lang_word": "firstly",
                                "target_lang_sense_clarification": "formal way to start a list"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прежде всего",
                                "target_lang_sense_clarification": "наречие: сперва, в начале всего"
                            },
                            {
                                "target_lang_word": "в первую очередь",
                                "target_lang_sense_clarification": "наречие: первым делом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c99f0423-cac0-32dc-8a97-d8d95bff9364",
                    "sense_definition": "De absolute eerste; degene die vóór alle anderen komt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de <w>allereerste</w> keer dat ik in een vliegtuig zit.",
                            "target_lang_translations": {
                                "en": "This is the <w>very first</w> time I am on a plane.",
                                "ru": "Это <w>самый первый</w> раз, когда я лечу на самолете."
                            }
                        },
                        {
                            "text": "De <w>allereerste</w> versie van het boek was heel anders dan de huidige.",
                            "target_lang_translations": {
                                "en": "The <w>very first</w> version of the book was very different from the current one.",
                                "ru": "<w>Самая первая</w> версия книги сильно отличалась от нынешней."
                            }
                        }
                    ],
                    "antonyms": [
                        "allerlaatste"
                    ],
                    "common_phrases": [
                        "de allereerste keer",
                        "het allereerste begin"
                    ],
                    "target_lang_definitions": {
                        "en": "The absolute first; the one preceding all others.",
                        "ru": "Самый первый; предшествующий всем остальным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "very first",
                                "target_lang_sense_clarification": "emphasizing the initial position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "самый первый",
                                "target_lang_sense_clarification": "прилагательное: первый из всех"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "fe3362eb-5a7f-37f5-97f2-e870f2a1cec3",
                    "sense_definition": "Перевозить или переводить людей, грузы через какое-либо препятствие (например, через реку) или из одного места в другое.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Паром начинает <переправлять> пассажиров на другой берег рано утром.",
                            "target_lang_translations": {
                                "en": "The ferry begins to <w>transport</w> passengers to the other bank early in the morning."
                            }
                        },
                        {
                            "text": "Военные помогали <переправлять> технику через широкую реку.",
                            "target_lang_translations": {
                                "en": "The military helped to <w>transport</w> the equipment across the wide river."
                            }
                        }
                    ],
                    "synonyms": [
                        "перевозить",
                        "переводить",
                        "перемещать"
                    ],
                    "common_phrases": [
                        "переправлять через реку",
                        "переправлять на другой берег",
                        "переправлять груз",
                        "переправлять людей"
                    ],
                    "target_lang_definitions": {
                        "en": "To transport or move people or cargo across an obstacle (e.g., a river) or from one place to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "general term for moving people or goods"
                            },
                            {
                                "target_lang_word": "ferry",
                                "target_lang_sense_clarification": "specifically to transport across water"
                            },
                            {
                                "target_lang_word": "take across",
                                "target_lang_sense_clarification": "to move someone or something from one side to another"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b028ddd-617c-3144-8c18-0d65e982cfb5",
                    "sense_definition": "Отправлять полученное письмо, документы или посылку дальше по назначению или другому адресату.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь будет <переправлять> все важные письма директору.",
                            "target_lang_translations": {
                                "en": "The secretary will <w>forward</w> all important letters to the director."
                            }
                        },
                        {
                            "text": "Вам нужно срочно <переправлять> эти документы в главный офис.",
                            "target_lang_translations": {
                                "en": "You need to urgently <w>forward</w> these documents to the head office."
                            }
                        }
                    ],
                    "synonyms": [
                        "пересылать"
                    ],
                    "common_phrases": [
                        "переправлять почту",
                        "переправлять письмо",
                        "переправлять документы",
                        "переправлять по адресу"
                    ],
                    "target_lang_definitions": {
                        "en": "To send a received letter, document, or parcel further to its final destination or to another recipient."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forward",
                                "target_lang_sense_clarification": "to send mail, documents, or messages on to a further destination"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "128c8426-1f0b-3da9-b2c8-42d7a9031298",
                    "sense_definition": "Вносить изменения в написанное, исправлять ошибки или менять цифры/буквы в тексте.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученик пытался незаметно <переправлять> плохую оценку в дневнике.",
                            "target_lang_translations": {
                                "en": "The student tried to unnoticedly <w>alter</w> the bad grade in the diary."
                            }
                        },
                        {
                            "text": "Редактор устал постоянно <переправлять> ошибки в статьях этого автора.",
                            "target_lang_translations": {
                                "en": "The editor got tired of constantly <w>correcting</w> mistakes in this author's articles."
                            }

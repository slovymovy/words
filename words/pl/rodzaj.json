                {
                    "sense_id": "29beafe1-c8c9-3bd7-95a1-3ac09bf3619f",
                    "sense_definition": "Kategoria gramatyczna rzeczownika (męski, żeński lub nijaki), która decyduje o formie innych wyrazów w zdaniu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słowo „okno” ma w języku polskim <w>rodzaj</w> nijaki.",
                            "target_lang_translations": {
                                "ru": "Слово «okno» в польском языке имеет средний <w>род</w>."
                            }
                        },
                        {
                            "text": "Dla obcokrajowców polski <w>rodzaj</w> gramatyczny bywa trudny do nauki.",
                            "target_lang_translations": {
                                "ru": "Для иностранцев польский грамматический <w>род</w> бывает труден для изучения."
                            }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bcb681fb-26bb-3c17-b86b-32959c24716d",
                    "sense_definition": "transportować kogoś lub coś, trzymając to w rękach lub na sobie, jednocześnie przemieszczając się w określonym kierunku",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz mi pomóc <nieść> te ciężkie zakupy do kuchni?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне <донести> эти тяжелые покупки на кухню?"
                            }
                        },
                        {
                            "text": "Ojciec <niósł> zmęczoną córkę na rękach przez całą drogę powrotną.",
                            "target_lang_translations": {
                                "ru": "Отец <нёс> уставшую дочь на руках всю обратную дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "dźwigać",
                        "transportować"
                    ],
                    "antonyms": [
                        "upuszczać",
                        "stawiać"
                    ],
                    "common_phrases": [
                        "nieść na rękach",
                        "nieść na plecach",
                        "nieść bagaż",
                        "nieść walizkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "перемещать кого-либо или что-либо, держа в руках или на себе, одновременно двигаясь в определенном направлении"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "физическое перемещение"
                            },
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "разг. нести что-то тяжелое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57f504b6-8b2d-3c4f-b5fa-dd66b85e7f82",
                    "sense_definition": "powodować określone skutki, wiązać się z czymś lub dostarczać coś niematerialnego (np. pomoc, wieści)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda decyzja finansowa <niesie> ze sobą pewne ryzyko straty.",
                            "target_lang_translations": {
                                "ru": "Каждое финансовое решение <несёт> в себе определенный риск убытков."
                            }
                        },
                        {
                            "text": "Wolontariusze pojechali na miejsce katastrofy, aby <nieść> pomoc poszkodowanym.",
                            "target_lang_translations": {
                                "ru": "Волонтеры поехали на место катастрофы, чтобы <оказать> помощь пострадавшим."
                            }
                        }
                    ],
                    "synonyms": [
                        "powodować",
                        "przynosić",
                        "sprawiać"
                    ],
                    "common_phrases": [
                        "nieść pomoc",
                        "nieść ryzyko",
                        "nieść nadzieję",
                        "nieść ze sobą"
                    ],
                    "target_lang_definitions": {
                        "ru": "вызывать определенные последствия, быть связанным с чем-то или приносить что-то нематериальное (например, помощь, вести)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "абстрактное: ответственность, риск, помощь"
                            },
                            {
                                "target_lang_word": "влечь за собой",
                                "target_lang_sense_clarification": "о последствиях"
                            },
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "доставлять (вести, облегчение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d561f509-a950-392b-932e-9ec4ffc8a8c8",
                    "sense_definition": "rozprzestrzeniać się w przestrzeni, być słyszalnym lub wyczuwalnym w dużej odległości (zwykle o dźwięku lub zapachu)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spreading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głośne echo <niosło się> po całej górskiej dolinie.",
                            "target_lang_translations": {
                                "ru": "Громкое эхо <разносилось> по всей горной долине."
                            }
                        },
                        {
                            "text": "Zapach świeżo pieczonego chleba <niesie się> po całym domu.",
                            "target_lang_translations": {
                                "ru": "Запах свежеиспеченного хлеба <разносится> по всему дому."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozchodzić się",
                        "rozbrzmiewać",
                        "rozprzestrzeniać się"
                    ],
                    "common_phrases": [
                        "głos się niesie",
                        "echo się niesie",
                        "zapach się niesie"
                    ],
                    "traits": [

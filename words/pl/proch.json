{
    "word_family": [
        "proszek",
        "prochowy",
        "prochownia"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f3de3ee1-207d-3083-8341-69ef42455235",
                    "sense_definition": "Mieszanina substancji chemicznych, która gwałtownie spala się i wybucha, używana głównie jako ładunek miotający w broni palnej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gunpowder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chińczycy wynaleźli <proch> wiele wieków temu.",
                            "target_lang_translations": {
                                "ru": "Китайцы изобрели <w>порох</w> много веков назад."
                            }
                        },
                        {
                            "text": "Żołnierze musieli chronić <proch> przed wilgocią.",
                            "target_lang_translations": {
                                "ru": "Солдаты должны были беречь <w>порох</w> от влаги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "proch strzelniczy",
                        "wymyślić proch",
                        "beczka prochu",
                        "zapach prochu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смесь химических веществ, которая быстро сгорает и взрывается, используемая в основном как метательный заряд в огнестрельном оружии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порох",
                                "target_lang_sense_clarification": "взрывчатое вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e405a9eb-f75e-3bd3-b2fc-10f223226ea9",
                    "sense_definition": "Bardzo drobna, sypka substancja powstała z rozkruszenia lub spalenia czegoś; pył.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dust_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara księga rozsypała się w <proch> przy dotknięciu.",
                            "target_lang_translations": {
                                "ru": "Старая книга рассыпалась в <w>прах</w> при прикосновении."
                            }
                        },
                        {
                            "text": "Wiatr unosił gęsty <proch> z polnej drogi.",
                            "target_lang_translations": {
                                "ru": "Ветер поднимал густую <w>пыль</w> с проселочной дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "pył",
                        "popiół"
                    ],
                    "common_phrases": [
                        "zetrzeć w proch",
                        "obrócić w proch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень мелкое, сыпучее вещество, образовавшееся в результате раздробления или сжигания чего-либо; пыль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прах",
                                "target_lang_sense_clarification": "останки, тлен"
                            },
                            {
                                "target_lang_word": "пыль",
                                "target_lang_sense_clarification": "мелкие частицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc1f5343-dec1-3e86-ab2f-55121b9c5d03",
                    "sense_definition": "Symbol nicości, przemijania, śmierci lub czegoś bezwartościowego (często w kontekście biblijnym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_dust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pamiętaj, że jesteś tylko marnym <prochem>.",
                            "target_lang_translations": {
                                "ru": "Помни, что ты всего лишь жалкий <w>прах</w>."
                            }
                        },
                        {
                            "text": "Wszystkie jego bogactwa obróciły się w <proch>.",
                            "target_lang_translations": {
                                "ru": "Все его богатства обратились в <w>прах</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "z prochu powstałeś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or biblical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ небытия, бренности, смерти или чего-то ничтожного (часто в библейском контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прах",
                                "target_lang_sense_clarification": "символ бренности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "01b0631c-06f7-32b8-b2a7-5f884294d833",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Proch> mieszka na parterze naszego bloku.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Прох</w> живет на первом этаже нашего дома."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <Proch>.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Прох</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

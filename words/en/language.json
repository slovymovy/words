                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "компьютерный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f0bf399-42b3-3b27-8ee2-16bdc2e1a3f6",

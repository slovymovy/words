                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Trap</w> niet op de plantjes in de tuin!",
                            "target_lang_translations": {
                                "en": "Do not <w>step</w> on the plants in the garden!",
                                "ru": "Не <w>наступай</w> на растения в саду!"

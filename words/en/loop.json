    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "56583c3b-72ec-39dd-a018-2364c5fcf737",
                    "sense_definition": "A shape produced by a curve that bends around and crosses itself, or a length of thread, line, or rope arranged in this way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tied a small <w>loop</w> in the rope to hang the picture frame.",
                            "target_lang_translations": {
                                "ru": "Он завязал на веревке маленькую <w>петлю</w>, чтобы повесить рамку для картины."
                            }
                        },
                        {
                            "text": "The roller coaster features a giant vertical <w>loop</w> that scares many riders.",
                            "target_lang_translations": {
                                "ru": "На этих американских горках есть гигантская вертикальная <w>петля</w>, которая пугает многих пассажиров."
                            }
                        }
                    ],
                    "synonyms": [
                        "circle",
                        "coil",
                        "ring",
                        "curl"
                    ],
                    "common_phrases": [
                        "belt loop",
                        "rope loop",
                        "make a loop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма, образованная кривой, которая изгибается и пересекает саму себя, или кусок нити, лески или веревки, уложенный таким образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "форма или узел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "870af732-178f-35e1-9489-eece369073d7",
                    "sense_definition": "A programmed sequence of instructions in a computer program that is repeated until a specific condition is satisfied.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program got stuck in an infinite <w>loop</w> and froze the computer.",
                            "target_lang_translations": {
                                "ru": "Программа застряла в бесконечном <w>цикле</w>, и компьютер завис."
                            }
                        },
                        {
                            "text": "You need to write a <w>loop</w> to process every name in the list.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно написать <w>цикл</w>, чтобы обработать каждое имя в списке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "infinite loop",
                        "feedback loop",
                        "nested loop"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Запрограммированная последовательность инструкций в компьютерной программе, которая повторяется до выполнения определенного условия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цикл",
                                "target_lang_sense_clarification": "в программировании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34b0b96c-efe0-3b86-85fc-08ae93ae1134",
                    "sense_definition": "A road, railway, or path that curves around to form a complete circle or starts and ends at the same place.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked the three-mile <w>loop</w> around the lake this morning.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром мы прошли трехмильный <w>кольцевой маршрут</w> вокруг озера."
                            }
                        },
                        {
                            "text": "The bus drives a continuous <w>loop</w> through the city center.",
                            "target_lang_translations": {
                                "ru": "Автобус ездит по непрерывному <w>кольцу</w> через центр города."
                            }
                        }
                    ],
                    "synonyms": [
                        "circuit",
                        "ring road"
                    ],
                    "common_phrases": [
                        "loop road",
                        "bus loop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорога, путь или маршрут, который изгибается, образуя полный круг, или начинается и заканчивается в одном и том же месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольцевой маршрут",
                                "target_lang_sense_clarification": "дорога или путь"
                            },
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "форма пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d448d858-109c-3697-950a-92affba00d31",
                    "sense_definition": "An endless strip of tape, film, or digital audio allowing sound or video to be played continuously.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The DJ created a catchy drum <w>loop</w> for the new song.",
                            "target_lang_translations": {
                                "ru": "Диджей создал запоминающийся барабанный <w>луп</w> для новой песни."
                            }
                        },
                        {
                            "text": "The museum exhibit played a short video <w>loop</w> about the history of the castle.",
                            "target_lang_translations": {
                                "ru": "На выставке в музее крутили короткую видео-<w>петлю</w> об истории замка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drum loop",
                        "tape loop",
                        "video loop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бесконечная лента или цифровая запись, позволяющая непрерывно воспроизводить звук или видео."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "повторяющийся фрагмент"
                            },
                            {
                                "target_lang_word": "луп",
                                "target_lang_sense_clarification": "музыкальный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e5c4b59-c4d4-3e63-9106-b3fb68332fed",
                    "sense_definition": "A complete circuit through which an electric current can flow.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The electrician checked the ground <w>loop</w> to ensure safety.",
                            "target_lang_translations": {
                                "ru": "Электрик проверил <w>контур</w> заземления, чтобы обеспечить безопасность."
                            }
                        },
                        {
                            "text": "A break in the <w>loop</w> stopped the signal from reaching the device.",
                            "target_lang_translations": {
                                "ru": "Разрыв в <w>цепи</w> остановил прохождение сигнала к устройству."
                            }
                        }
                    ],
                    "synonyms": [
                        "circuit"
                    ],
                    "common_phrases": [
                        "current loop",
                        "ground loop"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Замкнутый контур, через который может протекать электрический ток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контур",
                                "target_lang_sense_clarification": "электрический"
                            },
                            {
                                "target_lang_word": "цепь",
                                "target_lang_sense_clarification": "электрическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "843e92cd-f271-356b-b0b4-0509fab3c43b",
                    "sense_definition": "An aerobatic maneuver in which an aircraft flies a circular path in a vertical plane.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "aviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot performed a perfect <w>loop</w> at the air show.",
                            "target_lang_translations": {
                                "ru": "Пилот выполнил идеальную <w>мёртвую петлю</w> на авиашоу."
                            }
                        },
                        {
                            "text": "He pulled the plane up into a tight <w>loop</w> above the clouds.",
                            "target_lang_translations": {
                                "ru": "Он поднял самолет в крутую <w>петлю</w> над облаками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "loop the loop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фигура высшего пилотажа, при которой самолет описывает замкнутую кривую в вертикальной плоскости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мёртвая петля",
                                "target_lang_sense_clarification": "авиация"
                            },
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "фигура пилотажа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b7b6cdb-dfa3-32a5-9224-92f5bcd08137",
                    "sense_definition": "A sports league or conference.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team dominated the local soccer <w>loop</w> for three years.",
                            "target_lang_translations": {
                                "ru": "Команда доминировала в местной футбольной <w>лиге</w> в течение трех лет."
                            }
                        },
                        {
                            "text": "Several colleges joined the new athletic <w>loop</w> this season.",
                            "target_lang_translations": {
                                "ru": "Несколько колледжей присоединились к новой спортивной <w>лиге</w> в этом сезоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "league",
                        "conference"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "journalistic or informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная лига или конференция."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лига",
                                "target_lang_sense_clarification": "спортивное объединение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "99b9389f-a67f-34c2-9f12-fcbd2fa236b2",
                    "sense_definition": "To form something into the shape of a loop or to fasten something by encircling it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shape_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>looped</w> the long scarf around her neck twice.",
                            "target_lang_translations": {
                                "ru": "Она дважды <w>обернула</w> длинный шарф вокруг шеи."
                            }
                        },
                        {
                            "text": "He <w>looped</w> the rope over the post to secure the boat.",
                            "target_lang_translations": {
                                "ru": "Он <w>накинул</w> веревку <w>петлей</w> на столб, чтобы закрепить лодку."
                            }
                        }
                    ],
                    "synonyms": [
                        "coil",
                        "encircle",
                        "twist",
                        "wrap"
                    ],
                    "common_phrases": [
                        "loop around",
                        "loop over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать чему-либо форму петли или закреплять что-либо, обернув вокруг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обматывать",
                                "target_lang_sense_clarification": "вокруг чего-либо"
                            },
                            {
                                "target_lang_word": "делать петлю",
                                "target_lang_sense_clarification": "формировать узел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cb92e9b-d6a5-35c7-9244-8d16efd9fcc9",
                    "sense_definition": "To play a sound or video repeatedly, or to cause a computer program to repeat a sequence.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>looped</w> the song because I like the melody so much.",
                            "target_lang_translations": {
                                "ru": "Я <w>поставил</w> песню <w>на повтор</w>, потому что мне очень нравится мелодия."
                            }
                        },
                        {
                            "text": "The software <w>loops</w> through the data until it finds a match.",
                            "target_lang_translations": {
                                "ru": "Программа <w>проходит в цикле</w> по данным, пока не найдет совпадение."
                            }
                        }
                    ],
                    "synonyms": [
                        "repeat",
                        "cycle"
                    ],
                    "common_phrases": [
                        "loop a video",
                        "loop the track"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воспроизводить звук или видео повторно или заставлять компьютерную программу повторять последовательность действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закольцовывать",
                                "target_lang_sense_clarification": "аудио/видео"
                            },
                            {
                                "target_lang_word": "выполнять цикл",
                                "target_lang_sense_clarification": "программирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc66cbab-d32e-3f6c-b9c7-0e3547c3cbad",
                    "sense_definition": "To move in a path that resembles a loop or curve.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ball <w>looped</w> high over the goalkeeper's head and into the net.",
                            "target_lang_translations": {
                                "ru": "Мяч <w>описал высокую дугу</w> над головой вратаря и влетел в сетку."
                            }
                        },
                        {
                            "text": "The plane <w>looped</w> through the sky during the performance.",
                            "target_lang_translations": {
                                "ru": "Самолет <w>делал петли</w> в небе во время выступления."
                            }
                        }
                    ],
                    "synonyms": [
                        "curve",
                        "arc",
                        "spiral"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться по траектории, напоминающей петлю или дугу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описывать дугу",
                                "target_lang_sense_clarification": "о движении объекта"
                            },
                            {
                                "target_lang_word": "делать петлю",
                                "target_lang_sense_clarification": "о траектории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8280f825-8e22-3fdd-b6ab-9fa8e90affa6",
                    "sense_definition": "To fly an aircraft in a vertical circle.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "aviation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stunt pilot <w>looped</w> his biplane to the delight of the crowd.",
                            "target_lang_translations": {
                                "ru": "Пилот-каскадер <w>выполнил мёртвую петлю</w> на своем биплане на радость толпе."
                            }
                        },
                        {
                            "text": "He <w>looped</w> the aircraft effortlessly despite the strong wind.",
                            "target_lang_translations": {
                                "ru": "Он без усилий <w>сделал петлю</w> на самолете, несмотря на сильный ветер."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять самолетом, выполняя вертикальный круг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать мёртвую петлю",
                                "target_lang_sense_clarification": "авиация"
                            }
                        ]
                    }
                }
            ]
        },

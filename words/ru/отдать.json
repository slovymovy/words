    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3390f1dd-e5db-3138-9ddd-0a1c6c34eb7c",
                    "sense_definition": "Вернуть то, что было взято или получено ранее.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "return_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещаю <w>отдать</w> тебе книгу завтра.",
                            "target_lang_translations": {
                                "en": "I promise to <w>give back</w> the book to you tomorrow."
                            }
                        },
                        {
                            "text": "Он <w>отдал</w> долг своему другу.",
                            "target_lang_translations": {
                                "en": "He <w>repaid</w> the debt to his friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "вернуть"
                    ],
                    "antonyms": [
                        "взять",
                        "забрать"
                    ],
                    "common_phrases": [
                        "отдать долг",
                        "отдать деньги"
                    ],
                    "target_lang_definitions": {
                        "en": "To return what was taken or received earlier."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to give back something taken"
                            },
                            {
                                "target_lang_word": "give back",
                                "target_lang_sense_clarification": "to restore to the owner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36f2b510-cd07-3f2d-aa46-e67d67e161d3",
                    "sense_definition": "Передать что-либо кому-либо во владение или распоряжение.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "give_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама <w>отдала</w> старые вещи бедным.",
                            "target_lang_translations": {
                                "en": "Mom <w>gave away</w> the old things to the poor."
                            }
                        },
                        {
                            "text": "Пожалуйста, <w>отдай</w> мне этот документ.",
                            "target_lang_translations": {
                                "en": "Please <w>give</w> me this document."
                            }
                        }
                    ],
                    "synonyms": [
                        "подарить"
                    ],
                    "antonyms": [
                        "взять",
                        "получить",
                        "принять"
                    ],
                    "common_phrases": [
                        "отдать в руки",
                        "отдать даром"
                    ],
                    "target_lang_definitions": {
                        "en": "To transfer something to someone for possession or disposal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "to transfer possession"
                            },
                            {
                                "target_lang_word": "hand over",
                                "target_lang_sense_clarification": "to physically pass something to someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e339b1df-e124-3acc-9e03-9fd98e8f7690",
                    "sense_definition": "Поместить куда-либо или поручить кому-либо для ухода, обучения или ремонта.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <w>отдать</w> ребёнка в музыкальную школу.",
                            "target_lang_translations": {
                                "en": "We decided to <w>send</w> the child to music school."
                            }
                        },
                        {
                            "text": "Мне нужно <w>отдать</w> часы в мастерскую.",
                            "target_lang_translations": {
                                "en": "I need to <w>take</w> the watch to the repair shop."
                            }
                        }
                    ],
                    "synonyms": [
                        "поместить"
                    ],
                    "antonyms": [
                        "забрать"
                    ],
                    "common_phrases": [
                        "отдать в школу",
                        "отдать в ремонт"
                    ],
                    "target_lang_definitions": {
                        "en": "To place somewhere or entrust to someone for care, education, or repair."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "send",
                                "target_lang_sense_clarification": "to dispatch to an institution (e.g., school)"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "to bring somewhere for service (e.g., repair)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7110c859-6298-3fdb-950d-d94341a4b29d",
                    "sense_definition": "Совершить действие, выраженное существительным (приказ, голос, честь).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "light_verb_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генерал <w>отдал</w> приказ наступать.",
                            "target_lang_translations": {
                                "en": "The general <w>gave</w> the order to advance."
                            }
                        },
                        {
                            "text": "На выборах я <w>отдал</w> свой голос за этого кандидата.",
                            "target_lang_translations": {
                                "en": "In the election, I <w>cast</w> my vote for this candidate."
                            }
                        }
                    ],
                    "common_phrases": [
                        "отдать приказ",
                        "отдать честь",
                        "отдать голос",
                        "отдать предпочтение"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform an action expressed by a noun (order, vote, salute)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "used with 'order' or 'command'"
                            },
                            {
                                "target_lang_word": "cast",
                                "target_lang_sense_clarification": "used with 'vote'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ab551f0-569a-3ebe-89e3-0440a50135a7",
                    "sense_definition": "Пожертвовать чем-либо или посвятить время и силы ради какой-то цели.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "devote_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдат был готов <w>отдать</w> жизнь за свою страну.",
                            "target_lang_translations": {
                                "en": "The soldier was ready to <w>give</w> his life for his country."
                            }
                        },
                        {
                            "text": "Она <w>отдала</w> работе все свои силы.",
                            "target_lang_translations": {
                                "en": "She <w>gave</w> all her strength to her work."
                            }
                        }
                    ],
                    "synonyms": [
                        "посвятить"
                    ],
                    "common_phrases": [
                        "отдать жизнь",
                        "отдать всего себя"
                    ],
                    "target_lang_definitions": {
                        "en": "To sacrifice something or devote time and energy for a goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "to devote or sacrifice (e.g., one's life)"
                            },
                            {
                                "target_lang_word": "sacrifice",
                                "target_lang_sense_clarification": "to give up something valuable"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c33674a5-fe67-3475-ac98-ba60e72ef521",
                    "sense_definition": "Продать товар или заплатить деньги за что-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "trade_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>отдал</w> за этот телефон слишком много денег.",
                            "target_lang_translations": {
                                "en": "I <w>paid</w> too much money for this phone."
                            }
                        },
                        {
                            "text": "Продавец согласился <w>отдать</w> товар со скидкой.",
                            "target_lang_translations": {
                                "en": "The seller agreed to <w>give</w> the item at a discount."
                            }
                        }
                    ],
                    "synonyms": [
                        "продать",
                        "заплатить"
                    ],
                    "antonyms": [
                        "купить",
                        "получить"
                    ],
                    "target_lang_definitions": {
                        "en": "To sell goods or pay money for something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "to give money for goods"
                            },
                            {
                                "target_lang_word": "sell",
                                "target_lang_sense_clarification": "to give goods in exchange for money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9a9b6f1-35d5-315a-9162-183692b1b9f4",
                    "sense_definition": "Уступить территорию или позицию противнику.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "surrender_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия была вынуждена <w>отдать</w> город врагу.",
                            "target_lang_translations": {
                                "en": "The army was forced to <w>give up</w> the city to the enemy."
                            }
                        },
                        {
                            "text": "Мы не собираемся <w>отдать</w> ни пяди земли.",
                            "target_lang_translations": {
                                "en": "We are not going to <w>give up</w> a single inch of land."
                            }
                        }
                    ],
                    "synonyms": [
                        "потерять"
                    ],
                    "antonyms": [
                        "взять"
                    ],
                    "target_lang_definitions": {
                        "en": "To yield territory or a position to an enemy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give up",
                                "target_lang_sense_clarification": "to surrender possession of"
                            },
                            {
                                "target_lang_word": "surrender",
                                "target_lang_sense_clarification": "to yield to an enemy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51b995c4-f778-3c1b-a2d2-67857e08b068",
                    "sense_definition": "Резко оттолкнуть назад (об отдаче оружия или механизма).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "recoil_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ружьё сильно <w>отдало</w> в плечо при выстреле.",
                            "target_lang_translations": {
                                "en": "The gun <w>recoiled</w> strongly into the shoulder when fired."
                            }
                        },
                        {
                            "text": "При ударе руль <w>отдал</w> в руки водителю.",
                            "target_lang_translations": {
                                "en": "Upon impact, the steering wheel <w>kicked back</w> in the driver's hands."
                            }
                        }
                    ],
                    "synonyms": [
                        "откатиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To push back sharply (regarding the recoil of a weapon or mechanism)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recoil",
                                "target_lang_sense_clarification": "to spring back (of a gun)"
                            },
                            {
                                "target_lang_word": "kick",
                                "target_lang_sense_clarification": "to strike back (of a mechanism or gun)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d0ee96e-d55b-3470-89e3-a082d4e67106",
                    "sense_definition": "Отвязать, отпустить (трос, якорь).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан скомандовал <w>отдать</w> швартовы.",
                            "target_lang_translations": {
                                "en": "The captain commanded to <w>cast off</w> the mooring lines."
                            }
                        },
                        {
                            "text": "Корабль <w>отдал</w> якорь в бухте.",
                            "target_lang_translations": {
                                "en": "The ship <w>dropped</w> anchor in the bay."
                            }
                        }
                    ],
                    "synonyms": [
                        "отпустить"
                    ],
                    "common_phrases": [
                        "отдать швартовы",
                        "отдать якорь"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To untie, release (a cable, anchor)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let go",
                                "target_lang_sense_clarification": "to release a line"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "to lower (an anchor)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
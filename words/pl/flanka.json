            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cf323fa8-33c9-3088-aa23-a58c80066475",
                    "sense_definition": "Skrzydło szyku wojskowego lub ugrupowania bojowego; boczna strona oddziału.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "side_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwie dywizje wyszły na naszą <w>flankę</w>.",
                            "target_lang_translations": {
                                "ru": "Две дивизии вышли на наш <w>фланг</w>."
                            }
                        },
                        {
                            "text": "Generał rozkazał kawalerii uderzyć w prawą <w>flankę</w> przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал кавалерии ударить в правый <w>фланг</w> противника."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrzydło"
                    ],
                    "common_phrases": [
                        "atakować z flanki",
                        "odsłonić flankę",
                        "zachodzić od flanki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правая или левая оконечность строя, боевого порядка войск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фланг",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be93db86-7163-3afb-9221-cd4b6f40ed07",
                    "sense_definition": "Boczna część boiska w grach zespołowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "side_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik przeprowadził szybki rajd lewą <w>flanką</w>.",
                            "target_lang_translations": {
                                "ru": "Нападающий совершил быстрый проход по левому <w>флангу</w>."
                            }
                        },
                        {
                            "text": "Trener nakazał zawodnikom częściej wykorzystywać obie <w>flanki</w>.",
                            "target_lang_translations": {
                                "ru": "Тренер велел игрокам чаще использовать оба <w>фланга</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrzydło"
                    ],
                    "common_phrases": [
                        "grać na flance",
                        "rajd flanką",
                        "dośrodkowanie z flanki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайняя боковая часть игрового поля в командных видах спорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фланг",
                                "target_lang_sense_clarification": "часть поля"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09bfb1f1-9b80-3804-92bf-e2f621af1992",
                    "sense_definition": "Część bastionu lub fortyfikacji, która łączy czoło z kurtyną i służy do obrony rowu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artyleria ustawiona na <w>flance</w> bastionu miała doskonałe pole ostrzału.",
                            "target_lang_translations": {
                                "ru": "Артиллерия, установленная на <w>фланке</w> бастиона, имела превосходный сектор обстрела."
                            }
                        },
                        {
                            "text": "Stare plany twierdzy ukazywały zniszczone <w>flanki</w> murów obronnych.",
                            "target_lang_translations": {
                                "ru": "Старые планы крепости показывали разрушенные <w>фланки</w> оборонительных стен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ogień z flanki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть крепостного бастиона, служащая для прикрытия куртины и обстрела рва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фланк",
                                "target_lang_sense_clarification": "фортификация"

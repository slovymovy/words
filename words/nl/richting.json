                    "sense_id": "1f8818d3-623e-37dc-9581-f7cef71b3741",
                    "sense_definition": "De kant die men opgaat of moet opgaan om een bepaalde bestemming te bereiken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto reed in de <w>richting</w> van het centrum.",
                            "target_lang_translations": {
                                "en": "The car drove in the <w>direction</w> of the center.",
                                "ru": "Машина ехала в <w>направлении</w> центра."
                            }
                        },
                        {
                            "text": "Weet jij welke <w>richting</w> we op moeten lopen?",
                            "target_lang_translations": {
                                "en": "Do you know which <w>direction</w> we need to walk?",
                                "ru": "Ты знаешь, в каком <w>направлении</w> нам нужно идти?"
                            }

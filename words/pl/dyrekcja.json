{
    "word_family": [
        "dyrektor",
        "dyrektorka",
        "wicedyrektor",
        "dyrektorski",
        "dyrygować",
        "dyrygent"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "39f23aad-b27e-3a0c-84f2-c7c48b2a7de9",
                    "sense_definition": "Grupa osób zarządzających instytucją, przedsiębiorstwem lub szkołą, na której czele stoi dyrektor.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "management_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa <w>dyrekcja</w> wprowadziła zmiany w regulaminie pracy.",
                            "target_lang_translations": {
                                "ru": "Новая <w>дирекция</w> внесла изменения в правила трудового распорядка."
                            }
                        },
                        {
                            "text": "Rodzice uczniów chcą spotkać się z <w>dyrekcją</w> szkoły w sprawie wycieczki.",
                            "target_lang_translations": {
                                "ru": "Родители учеников хотят встретиться с <w>дирекцией</w> школы по поводу экскурсии."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarząd",
                        "kierownictwo",
                        "szefostwo",
                        "władze"
                    ],
                    "antonyms": [
                        "podwładni",
                        "pracownicy",
                        "personel",
                        "załoga"
                    ],
                    "common_phrases": [
                        "dyrekcja szkoły",
                        "decyzja dyrekcji",
                        "spotkanie z dyrekcją",
                        "naczelna dyrekcja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа лиц, управляющих учреждением, предприятием или школой, во главе которой стоит директор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дирекция",
                                "target_lang_sense_clarification": "руководящий орган"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "начальство"
                            },
                            {
                                "target_lang_word": "администрация",
                                "target_lang_sense_clarification": "управленческий персонал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd10a05d-a3c5-33ca-9996-c423cbc8f2ed",
                    "sense_definition": "Budynek lub zespół pomieszczeń biurowych, w których urzędują osoby zarządzające daną instytucją.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie podania należy składać w sekretariacie, który mieści się w <w>dyrekcji</w>.",
                            "target_lang_translations": {
                                "ru": "Все заявления следует подавать в секретариат, который находится в <w>дирекции</w>."
                            }
                        },
                        {
                            "text": "Nad głównym wejściem do <w>dyrekcji</w> zawieszono nową tablicę informacyjną.",
                            "target_lang_translations": {
                                "ru": "Над главным входом в <w>дирекцию</w> повесили новую информационную доску."
                            }
                        }
                    ],
                    "synonyms": [
                        "biuro",
                        "siedziba",
                        "gabinet"
                    ],
                    "common_phrases": [
                        "budynek dyrekcji",
                        "iść do dyrekcji",
                        "siedziba dyrekcji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или комплекс офисных помещений, где работают лица, управляющие данным учреждением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дирекция",
                                "target_lang_sense_clarification": "место/здание"
                            },
                            {
                                "target_lang_word": "офис руководства",
                                "target_lang_sense_clarification": "помещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51d1ab97-6e6b-3ed7-9ce9-4a9d34174810",
                    "sense_definition": "Czynność kierowania czymś lub sprawowania władzy zwierzchniej; często używane w kontekście artystycznym (np. dyrygowanie orkiestrą).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_of_directing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Orkiestra symfoniczna wystąpiła pod <w>dyrekcją</w> wybitnego gościa z zagranicy.",
                            "target_lang_translations": {
                                "ru": "Симфонический оркестр выступил под <w>управлением</w> выдающегося гостя из-за границы."
                            }
                        },
                        {
                            "text": "Po wielu latach pracy w teatrze wreszcie objął jego <w>dyrekcję</w>.",
                            "target_lang_translations": {
                                "ru": "После многих лет работы в театре он наконец взял на себя <w>руководство</w> им."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierownictwo",
                        "przewodnictwo",
                        "dowodzenie",
                        "zarządzanie"
                    ],
                    "common_phrases": [
                        "pod dyrekcją",
                        "objąć dyrekcję",
                        "sprawować dyrekcję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс руководства чем-либо или осуществления верховной власти; часто используется в художественном контексте (например, дирижирование оркестром)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "процесс руководства (напр. оркестром)"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "возглавление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4255d4ca-1875-36e7-adf1-c4e030b54779",
                    "sense_definition": "Jednostka administracyjna lub oddział terytorialny dużej organizacji państwowej lub przedsiębiorstwa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "administrative_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regionalna <w>dyrekcja</w> lasów państwowych ogłosiła przetarg na sadzonki drzew.",
                            "target_lang_translations": {
                                "ru": "Региональная <w>дирекция</w> государственных лесов объявила тендер на саженцы деревьев."
                            }
                        },
                        {
                            "text": "Ten odcinek drogi podlega pod inną <w>dyrekcję</w> dróg krajowych i autostrad.",
                            "target_lang_translations": {
                                "ru": "Этот участок дороги подчиняется другой <w>дирекции</w> национальных дорог и автомагистралей."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddział",
                        "okręg",
                        "jednostka"
                    ],
                    "common_phrases": [
                        "dyrekcja okręgowa",
                        "dyrekcja generalna",
                        "dyrekcja regionalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административная единица или территориальное отделение крупной государственной организации или предприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дирекция",
                                "target_lang_sense_clarification": "административная единица"
                            },
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "ведомство/отдел"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
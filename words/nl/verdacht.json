{
    "word_family": [
        "verdenken",
        "verdenking"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fbe28c99-91e0-3953-bcaa-3f2dadc1a955",
                    "sense_definition": "Iets of iemand die wantrouwen oproept of onbetrouwbaar lijkt; waar men twijfels bij heeft.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "suspicious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie vond een <w>verdacht</w> pakketje bij het station.",
                            "target_lang_translations": {
                                "en": "The police found a <w>suspicious</w> package at the station.",
                                "ru": "Полиция обнаружила <w>подозрительный</w> пакет на станции."
                            }
                        },
                        {
                            "text": "Ik vind het erg <w>verdacht</w> dat hij niet op kwam dagen.",
                            "target_lang_translations": {
                                "en": "I find it very <w>suspicious</w> that he didn't show up.",
                                "ru": "Мне кажется очень <w>подозрительным</w>, что он не появился."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdenking"
                    ],
                    "antonyms": [
                        "onverdacht"
                    ],
                    "common_phrases": [
                        "een verdacht overlijden",
                        "zich verdacht maken",
                        "verdachte omstandigheden"
                    ],
                    "target_lang_definitions": {
                        "en": "Something or someone that arouses distrust or seems unreliable; causing doubt.",
                        "ru": "Тот, кто или что вызывает недоверие или кажется ненадежным; вызывающий сомнения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspicious",
                                "target_lang_sense_clarification": "arousing distrust or suspicion"
                            },
                            {
                                "target_lang_word": "suspect",
                                "target_lang_sense_clarification": "potentially dangerous or false"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подозрительный",
                                "target_lang_sense_clarification": "вызывающий недоверие"
                            },
                            {
                                "target_lang_word": "сомнительный",
                                "target_lang_sense_clarification": "ненадежный, вызывающий сомнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab8d4891-37ef-3414-9666-60b89a2df6c5",
                    "sense_definition": "Voorbereid en alert zijn op iets dat kan gebeuren; ergens rekening mee houden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet in deze buurt altijd <w>verdacht</w> zijn op zakkenrollers.",
                            "target_lang_translations": {
                                "en": "You must always be <w>wary</w> of pickpockets in this neighborhood.",
                                "ru": "В этом районе нужно всегда быть <w>настороже</w> из-за карманников."
                            }
                        },
                        {
                            "text": "Hij luisterde gespannen, <w>verdacht</w> op elk vreemd geluid.",
                            "target_lang_translations": {
                                "en": "He listened tensely, <w>alert</w> to every strange sound.",
                                "ru": "Он напряженно слушал, <w>готовый</w> к любому странному звуку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verdacht zijn op"
                    ],
                    "target_lang_definitions": {
                        "en": "Being prepared and alert for something that might happen; taking something into account.",
                        "ru": "Быть подготовленным и бдительным по отношению к тому, что может произойти; учитывать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wary",
                                "target_lang_sense_clarification": "cautious of possible dangers"
                            },
                            {
                                "target_lang_word": "alert",
                                "target_lang_sense_clarification": "watchful and ready to respond"
                            },
                            {
                                "target_lang_word": "prepared",
                                "target_lang_sense_clarification": "ready to deal with something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "настороженный",
                                "target_lang_sense_clarification": "бдительный, ожидающий опасности"
                            },
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "подготовленный к чему-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15092413-8dfb-3264-9de6-2c19f151c682",
                    "sense_definition": "Voltooid deelwoord van <w>verdenken</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De man wordt ervan <w>verdacht</w> de auto te hebben gestolen.",
                            "target_lang_translations": {
                                "en": "The man is <w>suspected</w> of having stolen the car.",
                                "ru": "Мужчина <w>подозревается</w> в угоне автомобиля."
                            }
                        },
                        {
                            "text": "Ze hebben hem nooit ergens van <w>verdacht</w>.",
                            "target_lang_translations": {
                                "en": "They never <w>suspected</w> him of anything.",
                                "ru": "Они никогда ни в чем его не <w>подозревали</w>."
                            }
                        }
                    ],

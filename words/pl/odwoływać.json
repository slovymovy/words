    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1816214c-e8a7-3477-a6d6-fa021b7e2396",
                    "sense_definition": "Oznajmiać, że zaplanowane wcześniej wydarzenie lub spotkanie się nie odbędzie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cancel_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z powodu nagłej choroby artysty organizatorzy musieli <odwołać> koncert.",
                            "target_lang_translations": {
                                "ru": "Из-за внезапной болезни артиста организаторам пришлось <w>отменить</w> концерт."
                            }
                        },
                        {
                            "text": "Nauczyciel często <odwołuje> zajęcia, gdy ma ważne spotkania w dyrekcji.",
                            "target_lang_translations": {
                                "ru": "Учитель часто <w>отменяет</w> занятия, когда у него важные встречи в дирекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "anulować"
                    ],
                    "common_phrases": [
                        "odwołać spotkanie",
                        "odwołać wizytę",
                        "odwołać lot",
                        "odwołać rezerwację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объявлять, что запланированное событие или встреча не состоится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отменять",
                                "target_lang_sense_clarification": "событие, встрече, планы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bfe2775-e018-3041-93f1-39f261ce532d",
                    "sense_definition": "Nawiązywać do czegoś w swojej wypowiedzi, traktując to jako źródło, dowód lub przykład (używane z „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "refer_to",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W swojej pracy magisterskiej student często <odwołuje> się do najnowszych badań.",
                            "target_lang_translations": {
                                "ru": "В своей магистерской диссертации студент часто <w>ссылается</w> на новейшие исследования."
                            }
                        },
                        {
                            "text": "Podczas dyskusji warto <odwoływać> się do sprawdzonych faktów, a nie plotek.",
                            "target_lang_translations": {
                                "ru": "Во время дискуссии стоит <w>ссылаться</w> на проверенные факты, а не на слухи."
                            }
                        }
                    ],
                    "synonyms": [
                        "powoływać się",
                        "nawiązywać",
                        "opierać się",
                        "bazować"
                    ],
                    "common_phrases": [
                        "odwoływać się do faktów",
                        "odwoływać się do tradycji",
                        "odwoływać się do autorytetu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упоминать что-либо в своей речи, используя это как источник, доказательство или пример (используется с «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссылаться",
                                "target_lang_sense_clarification": "на источник, факты или авторитет"

            "senses": [
                {
                    "sense_id": "74666da9-51ee-3225-9654-0aee53399fd0",
                    "sense_definition": "Gwałtownie wyrzucać z ust ślinę, flegmę lub inną substancję.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niegrzeczny chłopiec zaczął ostentacyjnie <w>pluć</w> na chodnik przed sklepem.",
                            "target_lang_translations": {
                                "ru": "Невоспитанный мальчик начал демонстративно <w>плевать</w> на тротуар перед магазином."
                            }
                        },
                        {
                            "text": "Podczas jedzenia arbuza musieliśmy ciągle <w>pluć</w> czarnymi pestkami do miseczki.",
                            "target_lang_translations": {
                                "ru": "Когда мы ели арбуз, нам приходилось постоянно <w>плевать</w> черными косточками в миску."
                            }
                        }
                    ],
                    "synonyms": [
                        "plwać",
                        "charkać"
                    ],
                    "antonyms": [
                        "połykać"
                    ],
                    "common_phrases": [
                        "pluć na podłogę",
                        "pluć krwią",
                        "pluć pestkami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко выбрасывать изо рта слюну, мокроту или другую субстанцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плевать",
                                "target_lang_sense_clarification": "выбрасывать слюну"
                            },
                            {
                                "target_lang_word": "сплёвывать",
                                "target_lang_sense_clarification": "выбрасывать что-то изо рта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3b5e194-7c48-3af1-9a6b-fbf527c28395",
                    "sense_definition": "Okazywać komuś lub czemuś głęboką pogardę, lekceważyć coś całkowicie i bez szacunku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten człowiek zdaje się <w>pluć</w> na wszelkie zasady dobrego wychowania.",
                            "target_lang_translations": {
                                "ru": "Этот человек, похоже, <w>плюет</w> на все правила хорошего тона."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>pluć</w> na tradycje, które są ważne dla twoich dziadków.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>плевать</w> на традиции, которые важны для твоих дедушки и бабушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "gardzić",
                        "lekceważyć",
                        "znieważać",
                        "szydzić",
                        "urągać"
                    ],
                    "antonyms": [
                        "szanować",
                        "poważać",
                        "czcić"
                    ],
                    "common_phrases": [
                        "pluć na wszystko",
                        "pluć jadem",
                        "pluć na konwenanse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрировать полное пренебрежение или неуважение к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плевать",
                                "target_lang_sense_clarification": "относиться с пренебрежением"
                            },
                            {
                                "target_lang_word": "чихать",
                                "target_lang_sense_clarification": "разг. игнорировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81382ca7-ab6b-3b70-8c26-4ceb7580fff6",
                    "sense_definition": "Wyrzucać z siebie coś gwałtownie i w dużej ilości, na przykład ogień, dym lub pociski.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki smok w bajce potrafił latać i <w>pluć</w> gorącym ogniem.",
                            "target_lang_translations": {
                                "ru": "Огромный дракон в сказке умел летать и <w>плевать</w> горячим огнем."
                            }
                        },
                        {
                            "text": "Karabin maszynowy zaczął <w>pluć</w> ołowiem w stronę nacierającego wroga.",
                            "target_lang_translations": {
                                "ru": "Пулемет начал <w>плевать</w> свинцом в сторону наступающего врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "strzelać",
                        "buchać",
                        "zionąć"
                    ],
                    "common_phrases": [
                        "pluć ogniem",
                        "pluć dymem",
                        "pluć ołowiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбрасывать из себя что-то резко и в большом количестве (например, огонь, дым или пули)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извергать",

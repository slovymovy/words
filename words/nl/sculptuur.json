{
    "word_family": [
        "sculpteren",
        "sculpturaal"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bd77fd87-5ec6-37ad-a62a-be82d98ab3f7",
                    "sense_definition": "Een ruimtelijk kunstwerk, zoals een beeldhouwwerk, dat is ontstaan door materiaal te bewerken of samen te voegen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "kunst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar werkte maandenlang aan de marmeren <w>sculptuur</w>.",
                            "target_lang_translations": {
                                "ru": "Художник месяцами работал над мраморной <w>скульптурой</w>."
                            }
                        },
                        {
                            "text": "In de museumtuin staat een grote abstracte <w>sculptuur</w> die veel bezoekers trekt.",
                            "target_lang_translations": {
                                "ru": "В саду музея стоит большая абстрактная <w>скульптура</w>, которая привлекает много посетителей."
                            }
                        },
                        {
                            "text": "Hij kocht een antieke <w>sculptuur</w> van brons op de veiling.",
                            "target_lang_translations": {
                                "ru": "Он купил антикварную бронзовую <w>скульптуру</w> на аукционе."
                            }
                        }
                    ],
                    "synonyms": [
                        "beeldhouwwerk",
                        "beeld"
                    ],
                    "common_phrases": [
                        "bronzen sculptuur",
                        "moderne sculptuur",
                        "marmeren sculptuur",
                        "monumentale sculptuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространственное произведение искусства, созданное путем обработки твердых материалов или лепки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скульптура",
                                "target_lang_sense_clarification": "произведение искусства"
                            },
                            {
                                "target_lang_word": "изваяние",
                                "target_lang_sense_clarification": "книжное или поэтическое слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a661e89b-44ab-301d-bd6f-a7fa00faeff2",
                    "sense_definition": "Het patroon van ribbels, groeven of stekels op de buitenkant van een schelp of ander biologisch oppervlak.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biologie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De determinatie van de schelp hangt af van de specifieke <w>sculptuur</w> op het oppervlak.",
                            "target_lang_translations": {
                                "ru": "Определение вида раковины зависит от специфической <w>скульптуры</w> на её поверхности."
                            }
                        },
                        {
                            "text": "Onder de loep wordt de fijne <w>sculptuur</w> van het slakkenhuisje pas echt goed zichtbaar.",
                            "target_lang_translations": {
                                "ru": "Под лупой становится по-настоящему хорошо видна тонкая <w>скульптура</w> раковины улитки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рельефный узор на поверхности раковины или другого биологического объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скульптура",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "рельеф",
                                "target_lang_sense_clarification": "структура поверхности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
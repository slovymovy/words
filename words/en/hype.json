        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80fc5e62-35f8-3212-b17b-67d4824a0d96",
                    "sense_definition": "To promote heavily; to advertise or build up.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "promotion_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is <w>hyping</w> its new smartphone as a revolutionary device.",
                            "target_lang_translations": {
                                "ru": "Компания <w>раскручивает</w> свой новый смартфон как революционное устройство."
                            }
                        },
                        {
                            "text": "They <w>hyped</w> the event for weeks, but attendance was surprisingly low.",
                            "target_lang_translations": {
                                "ru": "Они <w>пиарили</w> мероприятие неделями, но посещаемость была удивительно низкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "promote",
                        "advertise",
                        "publicize",
                        "plug"
                    ],
                    "common_phrases": [
                        "hype up",
                        "hype the product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Активно рекламировать, создавать шумиху вокруг чего-либо."
                    },

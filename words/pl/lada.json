{
    "word_family": [
        "ladaco"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b55d2564-afce-33f8-bea5-223287efbf42",
                    "sense_definition": "Długi stół w sklepie, barze lub urzędzie, oddzielający sprzedawcę lub urzędnika od klientów, służący do obsługi i prezentacji towaru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ekspedientka położyła zakupione towary na <w>ladzie</w>.",
                            "target_lang_translations": {
                                "ru": "Продавщица положила купленные товары на <w>прилавок</w>."
                            }
                        },
                        {
                            "text": "Klient oparł się o <w>ladę</w> i czekał na obsługę.",
                            "target_lang_translations": {
                                "ru": "Клиент опёрся о <w>прилавок</w> и ждал обслуживания."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontuar"
                    ],
                    "common_phrases": [
                        "zza lady",
                        "podać przez ladę",
                        "wyłożyć na ladę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный стол в магазине, баре или учреждении, отделяющий продавца от покупателей и служащий для выкладки товара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прилавок",
                                "target_lang_sense_clarification": "в магазине"
                            },
                            {
                                "target_lang_word": "стойка",
                                "target_lang_sense_clarification": "в баре или рецепции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd923da1-e843-342c-9115-62cae05dc61f",
                    "sense_definition": "Maszyna rolnicza służąca do cięcia słomy lub siana na sieczkę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik naprawiał starą <w>ladę</w> w stodole.",
                            "target_lang_translations": {
                                "ru": "Фермер чинил старую <w>соломорезку</w> в сарае."
                            }
                        },
                        {
                            "text": "Do cięcia słomy używano kiedyś ręcznej <w>lady</w>.",
                            "target_lang_translations": {
                                "ru": "Для резки соломы когда-то использовали ручную <w>соломорезку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sieczkarnia"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "określenie spotykane w gwarach i języku dawnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сельскохозяйственная машина для резки соломы или сена на корм скоту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соломорезка",
                                "target_lang_sense_clarification": "машина"
                            },
                            {
                                "target_lang_word": "сечкарня",
                                "target_lang_sense_clarification": "устаревшее/региональное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d80351cd-3754-3189-80c6-a21cf6558445",
                    "sense_definition": "Gruba belka lub drąg pełniący funkcję podpory.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciężka <w>lada</w> podtrzymywała strop starej chaty.",
                            "target_lang_translations": {
                                "ru": "Тяжёлая <w>балка</w> поддерживала потолок старой хаты."
                            }
                        },
                        {
                            "text": "Cieśla wymienił spróchniałą <w>ladę</w> na nową.",
                            "target_lang_translations": {
                                "ru": "Плотник заменил трухлявую <w>балку</w> на новую."
                            }
                        }
                    ],
                    "synonyms": [
                        "belka",
                        "drąg"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rzadko używane we współczesnym języku ogólnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстая балка или шест, выполняющие функцию опоры в конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балка",
                                "target_lang_sense_clarification": "строительная"
                            },
                            {
                                "target_lang_word": "брус",
                                "target_lang_sense_clarification": "деревянный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35498f54-709e-3b8c-9311-28b9a2bed842",
                    "sense_definition": "Duża skrzynia, zazwyczaj drewniana, służąca do przechowywania przedmiotów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej <w>ladzie</w> na strychu znaleźliśmy rodzinne pamiątki.",
                            "target_lang_translations": {
                                "ru": "В старом <w>ларе</w> на чердаке мы нашли семейные реликвии."
                            }
                        },
                        {
                            "text": "Babcia trzymała pościel w malowanej <w>ladzie</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка держала постельное бельё в расписном <w>сундуке</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo wychodzące z użycia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой ящик, обычно деревянный, используемый для хранения вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ларь",
                                "target_lang_sense_clarification": "для хранения"
                            },
                            {
                                "target_lang_word": "сундук",
                                "target_lang_sense_clarification": "ящик с крышкой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "3a140fb5-a9f8-388f-a1c9-a63692250038",
                    "sense_definition": "Wyraz łączący się z określeniami czasu, wskazujący na to, że coś wydarzy się w bardzo bliskiej przyszłości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goście powinni przyjechać <w>lada</w> chwila.",
                            "target_lang_translations": {
                                "ru": "Гости должны приехать <w>с минуты на минуту</w>."
                            }
                        },
                        {
                            "text": "Uważaj, bo <w>lada</w> moment zacznie padać deszcz.",
                            "target_lang_translations": {
                                "ru": "Осторожно, <w>вот-вот</w> пойдёт дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lada dzień",
                        "lada chwila",
                        "lada moment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служебное слово, указывающее на то, что событие произойдет в самом ближайшем будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вот-вот",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "с минуты на минуту",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "31db16c4-bc9c-339d-bf66-5f882023e19d",
                    "sense_definition": "Słowo oznaczające dowolność, przypadkowość lub niską wartość, używane w połączeniu z zaimkami lub rzeczownikami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie możesz wpuścić do domu <w>lada</w> kogo.",
                            "target_lang_translations": {
                                "ru": "Нельзя пускать в дом <w>кого попало</w>."
                            }
                        },
                        {
                            "text": "To nie jest <w>lada</w> wyzwanie dla początkującego sportowca.",
                            "target_lang_translations": {
                                "ru": "Это <w>немалое</w> испытание для начинающего спортсмена."
                            }
                        }
                    ],
                    "synonyms": [
                        "byle"
                    ],
                    "common_phrases": [
                        "lada jaki",
                        "lada kto",
                        "lada co",
                        "nie lada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, обозначающее произвольность, случайность или низкое качество (в сочетаниях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любой",
                                "target_lang_sense_clarification": "всякий"
                            },
                            {
                                "target_lang_word": "какой попало",
                                "target_lang_sense_clarification": "пренебрежительно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "610da624-3083-370c-9acc-072d4f4c460d",
                    "sense_definition": "Grupa gór mająca podobną rzeźbę, stanowiąca wyodrębnioną całość.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geography_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Płaskowyż zewsząd otaczały łańcuchy górskie, które na północy tworzyły <w>masyw</w> spiętrzonych szczytów.",
                            "target_lang_translations": {
                                "ru": "Плато со всех сторон окружали горные хребты, которые на севере образовывали <w>массив</w> нагроможденных вершин."
                            }
                        },
                        {
                            "text": "Turyści planują przejść przez cały <w>masyw</w> górski w ciągu trzech dni.",
                            "target_lang_translations": {
                                "ru": "Туристы планируют пройти через весь горный <w>массив</w> за три дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "masyw górski",
                        "góra"
                    ],
                    "common_phrases": [
                        "masyw górski",
                        "masyw skalny",
                        "masyw centralny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа гор, имеющая сходный рельеф и образующая обособленное целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массив",
                                "target_lang_sense_clarification": "горный"
                            },
                            {
                                "target_lang_word": "горный массив",
                                "target_lang_sense_clarification": "география"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5870725-393f-3755-a1fc-a36abe4ceb6f",
                    "sense_definition": "Wielka, potężna bryła lub zwarta masa czegoś zajmująca znaczny obszar w przestrzeni.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z daleka widzieliśmy ciemny <w>masyw</w> lasu, który oddzielał nas od miasta.",
                            "target_lang_translations": {
                                "ru": "Издалека мы видели темный <w>массив</w> леса, который отделял нас от города."
                            }
                        },
                        {
                            "text": "Architektura tego zamku to potężny <w>masyw</w> cegieł i kamienia.",
                            "target_lang_translations": {
                                "ru": "Архитектура этого замка представляет собой мощный <w>массив</w> кирпича и камня."
                            }
                        }
                    ],
                    "synonyms": [
                        "bryła"
                    ],
                    "common_phrases": [
                        "masyw leśny",
                        "masyw budynku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая, мощная глыба или плотная масса чего-либо, занимающая значительное пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массив",
                                "target_lang_sense_clarification": "леса, застройки и т.п."
                            },
                            {
                                "target_lang_word": "громада",
                                "target_lang_sense_clarification": "о чем-то огромном и тяжелом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eba52b32-a87a-3f15-9440-96799acaaa20",
                    "sense_definition": "Pierścień gumowy zamocowany na stałe na obręczy koła pojazdu, stanowiący ogumienie pełne.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wózki widłowe często posiadają <w>masywy</w> zamiast opon pneumatycznych.",
                            "target_lang_translations": {
                                "ru": "Вилочные погрузчики часто имеют <w>цельнолитые шины</w> вместо пневматических."
                            }
                        },
                        {
                            "text": "Ten pojazd wojskowy porusza się na <w>masywach</w>, co zwiększa jego wytrzymałość na przebicia.",
                            "target_lang_translations": {
                                "ru": "Это военное транспортное средство передвигается на <w>цельнолитых шинах</w>, что повышает его устойчивость к проколам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сплошное резиновое кольцо, закрепленное на ободе колеса; непневматическая шина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цельнолитая шина",
                                "target_lang_sense_clarification": "тип шины"
                            },
                            {
                                "target_lang_word": "массивная шина",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

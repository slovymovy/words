    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7ee6d2f0-7228-3292-b0ec-c8aad442a321",
                    "sense_definition": "To indicate or be a sign of a future outcome, usually describing whether it will be good or bad.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "prediction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The recent improvements in sales <w>bode</w> well for the company's financial future.",
                            "target_lang_translations": {
                                "ru": "Недавний рост продаж <w>сулит</w> хорошее финансовое будущее для компании."
                            }
                        },
                        {
                            "text": "Dark clouds gathering on the horizon often <w>bode</w> a severe storm.",
                            "target_lang_translations": {
                                "ru": "Темные тучи, собирающиеся на горизонте, часто <w>предвещают</w> сильную бурю."
                            }
                        }
                    ],
                    "synonyms": [
                        "portend",
                        "presage",
                        "foreshow"
                    ],
                    "common_phrases": [
                        "bode well",
                        "bode ill",
                        "bode poorly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить признаком или предзнаменованием будущего исхода (обычно хорошего или плохого)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предвещать",
                                "target_lang_sense_clarification": "указывать на приближение чего-либо"
                            },
                            {
                                "target_lang_word": "сулить",
                                "target_lang_sense_clarification": "обещать, подавать надежды (часто в сочетании с 'хорошее' или 'плохое')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40f2c93a-ee9d-3cd2-9743-ee22e3f85d85",
                    "sense_definition": "The simple past form of <w>bide</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>bode</w> his time in the shadows until the guards passed.",
                            "target_lang_translations": {
                                "ru": "Он <w>выжидал</w> время в тени, пока не прошла стража."
                            }
                        },
                        {
                            "text": "She <w>bode</w> at the cottage for three winters before moving on.",
                            "target_lang_translations": {
                                "ru": "Она <w>оставалась</w> в коттедже три зимы, прежде чем отправиться дальше."
                            }
                        }

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "драться",
                                "target_lang_sense_clarification": "участвовать в драке"
                            },
                            {
                                "target_lang_word": "биться",
                                "target_lang_sense_clarification": "сражаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30a650a8-1802-3b86-b192-a8cba660a23e",
                    "sense_definition": "O zegarze lub dzwonie: wydawać głośne, rytmiczne dźwięki sygnalizujące czas lub alarm.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słyszę, jak dzwony w kościele <w>biją</w> na południe.",
                            "target_lang_translations": {
                                "ru": "Я слышу, как колокола в церкви <w>бьют</w> полдень."
                            }
                        },
                        {
                            "text": "Stary zegar w salonie głośno <w>bije</w> co godzinę.",
                            "target_lang_translations": {
                                "ru": "Старые часы в гостиной громко <w>бьют</w> каждый час."
                            }

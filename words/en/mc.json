                    ],
                    "synonyms": [
                        "emcee"
                    ],
                    "common_phrases": [
                        "battle MC",
                        "hip-hop MC",
                        "underground MC"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальный исполнитель, который пишет и исполняет рэп; рэпер."

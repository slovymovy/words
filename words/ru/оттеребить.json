    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ff4629df-1f5c-311c-9b7d-958327215bf4",
                    "sense_definition": "Сильно потянуть, подёргать или оттаскать за что-либо (часто за уши или волосы), причиняя боль, или оторвать что-то, теребя.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За такие грубые слова отец вполне мог <оттеребить> сыну уши.",
                            "target_lang_translations": {
                                "en": "For such rude words, the father could well have <pulled> his son's ears."
                            }
                        },
                        {
                            "text": "Щенок играл с подушкой и в конце концов <оттеребил> от неё кисточку.",
                            "target_lang_translations": {
                                "en": "The puppy was playing with the pillow and eventually <tore off> a tassel from it."
                            }
                        }
                    ],
                    "synonyms": [
                        "оттаскать",
                        "надрать",
                        "отодрать"
                    ],
                    "common_phrases": [
                        "оттеребить уши"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech, often referring to punishment"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pull or tug hard at something (often ears or hair) causing pain, or to tear something off by tugging."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "ears or hair, often as punishment"
                            },
                            {
                                "target_lang_word": "tear off",
                                "target_lang_sense_clarification": "by tugging or worrying"
                            },
                            {
                                "target_lang_word": "tug",
                                "target_lang_sense_clarification": "hard or roughly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b7dc5e6-5e4b-3f22-b40c-cd4e9c2b1644",
                    "sense_definition": "Закончить <теребить>; завершить процесс дёрганья, щипания или обработки волокнистых растений.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крестьянки <оттеребили> лён ещё до захода солнца и пошли домой.",
                            "target_lang_translations": {
                                "en": "The peasant women <finished pulling> the flax before sunset and went home."
                            }
                        },
                        {
                            "text": "Когда мы <оттеребили> всю шерсть, её сложили в большие мешки.",
                            "target_lang_translations": {
                                "en": "When we had <finished teasing> all the wool, it was put into large sacks."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To finish the process of pulling, plucking, or processing fibrous plants (like flax) or wool."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "finish pulling",
                                "target_lang_sense_clarification": "flax or plants"
                            },
                            {
                                "target_lang_word": "finish teasing",
                                "target_lang_sense_clarification": "wool"
                            },
                            {
                                "target_lang_word": "finish scutching",
                                "target_lang_sense_clarification": "technical term for flax processing"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
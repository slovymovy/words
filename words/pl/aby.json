                    ],
                    "synonyms": [
                        "by",
                        "przynajmniej"
                    ],
                    "common_phrases": [
                        "aby do wiosny",
                        "aby tylko",
                        "aby zdrowie było"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in fixed expressions of hope or minimal requirement"
                        }
                    ]
                }
            ]
        }
    ]

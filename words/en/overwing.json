            "senses": [
                {
                    "sense_id": "f373d673-e0f5-3b09-9726-ac41079431e6",
                    "sense_definition": "To move past the flank or side of an enemy force; to outflank.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general attempted to <w>overwing</w> the opposing army by moving his cavalry to the right.",
                            "target_lang_translations": {
                                "ru": "Генерал попытался <w>обойти с фланга</w> армию противника, переместив кавалерию вправо."
                            }
                        },
                        {
                            "text": "They feared the enemy would <w>overwing</w> their position and attack from the rear.",
                            "target_lang_translations": {
                                "ru": "Они боялись, что враг <w>охватит с фланга</w> их позицию и атакует с тыла."
                            }
                        }
                    ],
                    "synonyms": [
                        "outflank",
                        "outmaneuver"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "military strategy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обойти фланг или сторону вражеских сил; совершить обходной маневр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойти с фланга",
                                "target_lang_sense_clarification": "военный маневр"
                            },
                            {
                                "target_lang_word": "охватить",
                                "target_lang_sense_clarification": "окружить с флангов"

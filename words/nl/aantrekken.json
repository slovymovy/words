            "senses": [
                {
                    "sense_id": "b511f535-f562-3803-b59a-5deb00ec92c1",
                    "sense_definition": "Kledingstukken om het lichaam doen",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga even iets anders <w>aantrekken</w>.",
                            "target_lang_translations": {
                                "en": "I'm going to <w>put on</w> something else.",
                                "ru": "Я пойду <w>надену</w> что-нибудь другое."
                            }
                        },
                        {
                            "text": "Na het douchen moet je schone kleren <w>aantrekken</w>.",
                            "target_lang_translations": {
                                "en": "After showering you must <w>put on</w> clean clothes.",
                                "ru": "После душа нужно <w>надеть</w> чистую одежду."
                            }
                        }
                    ],
                    "synonyms": [
                        "aandoen"
                    ],
                    "antonyms": [
                        "uittrekken"
                    ],
                    "common_phrases": [
                        "kleren aantrekken",
                        "jas aantrekken",
                        "schoenen aantrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To put clothes on one's body",
                        "ru": "Надевать одежду на тело"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "clothing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "надевать",
                                "target_lang_sense_clarification": "одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d798e843-4af8-32f9-9e59-b29f61275864",
                    "sense_definition": "De aandacht of interesse van iemand wekken; aanlokken",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_attract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zoete geur <w>trok</w> veel wespen <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "The sweet scent <w>attracted</w> many wasps.",
                                "ru": "Сладкий запах <w>привлек</w> много ос."
                            }
                        },
                        {
                            "text": "Werken in een ziekenhuis <w>trekt</w> me niet <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "Working in a hospital does not <w>appeal</w> to me.",
                                "ru": "Работа в больнице меня не <w>привлекает</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "afstoten"
                    ],
                    "common_phrases": [
                        "aandacht aantrekken",
                        "toeristen aantrekken",
                        "klanten aantrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To arouse someone's attention or interest",
                        "ru": "Вызывать чей-то интерес или внимание; манить"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "attention or interest"
                            },
                            {
                                "target_lang_word": "appeal to",
                                "target_lang_sense_clarification": "be interesting to"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "внимание, интерес"
                            },
                            {
                                "target_lang_word": "манить",
                                "target_lang_sense_clarification": "притягивать к себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12308c1b-2833-328f-a395-06d00941ffa7",
                    "sense_definition": "Zich zorgen maken om iets of zich iets persoonlijk nemen",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Trek</w> je maar niets <w>aan</w> van hun geschreeuw.",
                            "target_lang_translations": {
                                "en": "Don't <w>take</w> their shouting to <w>heart</w>.",
                                "ru": "Не <w>обращай внимания</w> на их крики."
                            }
                        },
                        {
                            "text": "Hij <w>trok</w> zich niets <w>aan</w> van de hoge boete.",
                            "target_lang_translations": {
                                "en": "He didn't <w>care</w> about the high fine.",
                                "ru": "Он не <w>обратил внимания</w> на высокий штраф."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich niets aantrekken",
                        "zich veel aantrekken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To take something personally or be bothered by it",
                        "ru": "Беспокоиться о чем-то или принимать на свой счет"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take to heart",
                                "target_lang_sense_clarification": "be affected by"
                            },
                            {
                                "target_lang_word": "care about",
                                "target_lang_sense_clarification": "be concerned with"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "принимать близко к сердцу",
                                "target_lang_sense_clarification": "переживать"
                            },
                            {
                                "target_lang_word": "обращать внимание",
                                "target_lang_sense_clarification": "обычно с отрицанием: не обращать внимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2814048-1678-361b-b604-af1858c2d5e3",
                    "sense_definition": "Iets vaster maken door eraan te trekken",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>trok</w> de handrem goed <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "He <w>applied</w> the handbrake firmly.",
                                "ru": "Он хорошо <w>затянул</w> ручной тормоз."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c2abcd6-af8a-342b-a046-4c8700f1047c",
                    "sense_definition": "An emergency vehicle designed for transporting seriously ill or injured people to a hospital.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We called an <w>ambulance</w> immediately after the accident occurred.",
                            "target_lang_translations": {
                                "ru": "Мы вызвали <w>скорую</w> сразу после аварии."
                            }
                        },
                        {
                            "text": "The <w>ambulance</w> rushed the patient to the emergency room with its sirens wailing.",
                            "target_lang_translations": {
                                "ru": "<w>Скорая помощь</w> с воем сирен помчала пациента в отделение неотложной помощи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "call an ambulance",
                        "ambulance driver",
                        "ambulance siren",
                        "air ambulance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина экстренной службы, предназначенная для перевозки тяжелобольных или раненых людей в больницу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорая помощь",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "скорая",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            },
                            {
                                "target_lang_word": "машина скорой помощи",
                                "target_lang_sense_clarification": "транспортное средство"
                            },
                            {
                                "target_lang_word": "неотложка",
                                "target_lang_sense_clarification": "разговорное, служба неотложной помощи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87805281-684e-3bc9-96f9-2af6b2808c42",
                    "sense_definition": "A vehicle used for the transportation of dead people, typically to a mortuary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The deceased was quietly removed from the home in a private <w>ambulance</w>.",
                            "target_lang_translations": {
                                "ru": "Покойного тихо увезли из дома на частном <w>спецтранспорте</w>."
                            }
                        },
                        {
                            "text": "A black <w>ambulance</w> arrived to transport the body to the funeral home.",
                            "target_lang_translations": {
                                "ru": "Черный <w>катафалк</w> прибыл, чтобы перевезти тело в похоронное бюро."
                            }
                        }
                    ],
                    "synonyms": [
                        "private ambulance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство, используемое для перевозки умерших, обычно в морг."
                    },
                    "translations": {
                        "ru": [
                            {

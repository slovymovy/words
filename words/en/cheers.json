        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "90a43f3d-2dcc-3eeb-92dc-32ef10125d42",
                    "sense_definition": "A common toast used when drinking in company.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "toast_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We raised our glasses high and shouted <w>cheers</w>.",
                            "target_lang_translations": {
                                "ru": "Мы высоко подняли бокалы и крикнули: «<w>За здоровье</w>!»"
                            }
                        },
                        {
                            "text": "He smiled at his friends and said <w>cheers</w> before taking a sip.",
                            "target_lang_translations": {
                                "ru": "Он улыбнулся друзьям и сказал: «<w>Будем здоровы</w>», прежде чем сделать глоток."
                            }
                        }
                    ],
                    "synonyms": [
                        "bottoms up",
                        "chin-chin",
                        "here's to",
                        "prosit"
                    ],
                    "common_phrases": [
                        "cheers to us",
                        "three cheers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенный тост, произносимый во время выпивки в компании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за здоро́вье",
                                "target_lang_sense_clarification": "общий тост"
                            },

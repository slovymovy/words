    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4956fab0-3222-338d-8f4e-b6d9574f12e9",
                    "sense_definition": "Письменная принадлежность в виде палочки со стержнем, наполненным чернилами или пастой.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "writing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент забыл дома пенал, и у него не было <w>ручки</w>.",
                            "target_lang_translations": {
                                "en": "The student forgot his pencil case at home, and he didn't have a <w>pen</w>."
                            }
                        },
                        {
                            "text": "Для заполнения официальных документов нужна синяя шариковая <w>ручка</w>.",
                            "target_lang_translations": {
                                "en": "A blue ballpoint <w>pen</w> is needed to fill out official documents."
                            }
                        }
                    ],
                    "synonyms": [
                        "авторучка",
                        "перо"
                    ],
                    "common_phrases": [
                        "шариковая ручка",
                        "гелевая ручка",
                        "писать ручкой"
                    ],
                    "target_lang_definitions": {
                        "en": "A writing instrument consisting of a rod with a reservoir of ink."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pen",
                                "target_lang_sense_clarification": "writing tool"
                            },
                            {
                                "target_lang_word": "ballpoint",
                                "target_lang_sense_clarification": "type of pen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5eb4a39a-cb30-3a06-9a40-483aa8e6cc0b",
                    "sense_definition": "Выступающая часть предмета, за которую его держат, открывают или поднимают.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дверная <w>ручка</w> сломалась, и мы не могли войти в комнату.",
                            "target_lang_translations": {
                                "en": "The door <w>handle</w> broke, and we couldn't enter the room."
                            }
                        },
                        {
                            "text": "Он крепко взялся за <w>ручку</w> тяжелого чемодана.",
                            "target_lang_translations": {
                                "en": "He gripped the <w>handle</w> of the heavy suitcase tightly."
                            }
                        }
                    ],
                    "synonyms": [
                        "рукоятка",
                        "рукоять"
                    ],
                    "common_phrases": [
                        "дверная ручка",
                        "ручка двери",
                        "дернуть за ручку"
                    ],
                    "target_lang_definitions": {
                        "en": "A protruding part of an object designed to be held, pulled, or lifted."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "part of an object"
                            },
                            {
                                "target_lang_word": "grip",
                                "target_lang_sense_clarification": "part to hold"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc6e7f69-d329-372b-825f-5ab256895585",
                    "sense_definition": "Уменьшительная форма слова <w>рука</w>; маленькая рука ребёнка или изящная женская рука.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький ребёнок с улыбкой протянул свои <w>ручки</w> к маме.",
                            "target_lang_translations": {
                                "en": "The small child smiled and reached his <w>little hands</w> out to his mom."
                            }
                        },
                        {
                            "text": "Молодая пара медленно шла по осеннему парку под <w>ручку</w>.",
                            "target_lang_translations": {
                                "en": "A young couple walked slowly through the autumn park <w>arm</w> in arm."
                            }
                        }
                    ],
                    "synonyms": [
                        "рука",
                        "рукоятка"
                    ],
                    "common_phrases": [
                        "взять на ручки",
                        "под ручку",
                        "махать ручкой"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Diminutive form of 'hand'; a small hand, especially of a child."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "little hand",
                                "target_lang_sense_clarification": "diminutive"
                            },
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "body part"
                            },
                            {
                                "target_lang_word": "arm",
                                "target_lang_sense_clarification": "in idioms like 'arm in arm'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4bcf231-ff28-36ca-bd23-005ceb167a0b",
                    "sense_definition": "Деталь механизма, которую вращают или перемещают для управления устройством.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanism_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы прибавить звук, нужно повернуть <w>ручку</w> громкости вправо.",
                            "target_lang_translations": {
                                "en": "To increase the sound, you need to turn the volume <w>knob</w> to the right."
                            }
                        },
                        {
                            "text": "Старая <w>ручка</w> настройки радиоприёмника вращалась с большим трудом.",
                            "target_lang_translations": {
                                "en": "The old radio tuning <w>knob</w> turned with great difficulty."
                            }
                        }
                    ],
                    "synonyms": [
                        "рукоятка",
                        "маховик"
                    ],
                    "common_phrases": [
                        "ручка громкости",
                        "ручка настройки",
                        "крутить ручку"
                    ],
                    "target_lang_definitions": {
                        "en": "A part of a mechanism that is turned or moved to control a device."
                    },
                    "translations": {

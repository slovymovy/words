            "senses": [
                {
                    "sense_id": "71f378af-f546-3fca-8a76-ce01da9646fa",
                    "sense_definition": "Słowo oznaczające, że do pełnego osiągnięcia jakiegoś stanu, ilości lub czasu brakuje bardzo niewiele.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem <prawie> pewien, że zostawiłem klucze na stole.",
                            "target_lang_translations": {
                                "ru": "Я <почти> уверен, что оставил ключи на столе."
                            }
                        },
                        {
                            "text": "Film był tak nudny, że <prawie> zasnąłem w kinie.",
                            "target_lang_translations": {
                                "ru": "Фильм был таким скучным, что я <почти> уснул в кинотеатре."
                            }
                        },
                        {
                            "text": "Moja siostra jest <prawie> tak wysoka jak ja.",
                            "target_lang_translations": {
                                "ru": "Моя сестра <почти> такого же роста, как я."
                            }
                        }
                    ],
                    "synonyms": [
                        "niemal",
                        "niemalże",
                        "nieomal",
                        "bez mała"
                    ],
                    "antonyms": [
                        "całkiem",

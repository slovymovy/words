    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "252f07f9-4e15-32ef-a0a9-903e7305c3a1",
                    "sense_definition": "Обращаться к Богу, святым или высшим силам с просьбой, благодарностью или славословием.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religious_practice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верующие люди ходят в церковь, чтобы <молиться> Богу.",
                            "target_lang_translations": {
                                "en": "Believers go to church to <pray> to God."
                            }
                        },
                        {
                            "text": "Бабушка каждый вечер тихо <молилась> за здоровье всей семьи.",
                            "target_lang_translations": {
                                "en": "Grandma <prayed> quietly every evening for the health of the whole family."
                            }
                        }
                    ],
                    "common_phrases": [
                        "молиться Богу",
                        "молиться за здоровье",
                        "молиться в храме",
                        "молиться перед иконой"
                    ],
                    "target_lang_definitions": {
                        "en": "To address God, saints, or higher powers with a request, gratitude, or praise."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pray",
                                "target_lang_sense_clarification": "religious practice"
                            },
                            {
                                "target_lang_word": "say prayers",
                                "target_lang_sense_clarification": "act of praying"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "522e0fe9-5d19-34dc-a72a-445f6e67587c",
                    "sense_definition": "Сильно любить, боготворить кого-либо, испытывать слепое обожание (обычно используется с предлогом «на»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_worship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он так сильно любит свою жену, что готов <молиться> на неё.",
                            "target_lang_translations": {
                                "en": "He loves his wife so much that he is ready to <worship> her."
                            }
                        },
                        {
                            "text": "Студенты просто <молились> на этого профессора за его доброту и знания.",
                            "target_lang_translations": {
                                "en": "The students simply <worshipped> this professor for his kindness and knowledge."
                            }
                        }
                    ],
                    "synonyms": [
                        "боготворить",
                        "обожать"
                    ],
                    "common_phrases": [
                        "молиться на кого-то",
                        "готов молиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To love someone intensely, to idolize, or to experience blind adoration (usually used with the preposition 'on')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worship",
                                "target_lang_sense_clarification": "adore or idolize someone"
                            },
                            {
                                "target_lang_word": "idolize",
                                "target_lang_sense_clarification": "admire intensely"
                            },
                            {
                                "target_lang_word": "dote on",
                                "target_lang_sense_clarification": "show excessive fondness"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
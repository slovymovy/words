    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac91875a-65fd-31af-9393-e98613720551",
                    "sense_definition": "Een beleefde aanspreekvorm of titel voor een volwassen vrouw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goedemorgen <w>mevrouw</w>, kan ik u ergens mee helpen?",
                            "target_lang_translations": {
                                "en": "Good morning <w>ma'am</w>, can I help you with anything?",
                                "ru": "Доброе утро, <w>мадам</w>, могу я вам чем-нибудь помочь?"
                            }
                        },
                        {
                            "text": "Dit is <w>mevrouw</w> Jansen, de directeur van de school.",
                            "target_lang_translations": {
                                "en": "This is <w>Ms.</w> Jansen, the school principal.",
                                "ru": "Это <w>госпожа</w> Янсен, директор школы."
                            }
                        }
                    ],
                    "antonyms": [
                        "meneer"
                    ],
                    "common_phrases": [
                        "dag mevrouw",
                        "geachte mevrouw",
                        "mevrouw de voorzitter"
                    ],
                    "target_lang_definitions": {
                        "en": "A polite form of address or title for an adult woman.",
                        "ru": "Вежливая форма обращения или титул для взрослой женщины."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "madam",
                                "target_lang_sense_clarification": "formal address without a name"
                            },
                            {
                                "target_lang_word": "ma'am",
                                "target_lang_sense_clarification": "polite address"
                            },
                            {
                                "target_lang_word": "Ms.",
                                "target_lang_sense_clarification": "title used before a surname"
                            },
                            {
                                "target_lang_word": "Mrs.",
                                "target_lang_sense_clarification": "title for a married woman"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "госпожа",
                                "target_lang_sense_clarification": "титул, используемый перед фамилией"
                            },
                            {
                                "target_lang_word": "мадам",
                                "target_lang_sense_clarification": "вежливое обращение (формальное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1786e3be-fa2f-3f55-b201-67c7bdba5c37",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>mevrouwen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>mevrouw</w> haar altijd omdat ik dat beleefd vind.",
                            "target_lang_translations": {
                                "en": "I always <w>address</w> her <w>as 'madam'</w> because I find that polite.",
                                "ru": "Я всегда <w>называю</w> её <w>«госпожой»</w>, потому что считаю это вежливым."
                            }
                        },
                        {
                            "text": "Zolang zij mij meneert, <w>mevrouw</w> ik haar terug.",
                            "target_lang_translations": {
                                "en": "As long as she calls me 'sir', I <w>call</w> her <w>'madam'</w> in return.",
                                "ru": "Пока она называет меня «господином», я <w>называю</w> её <w>«госпожой»</w> в ответ."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'mevrouwen' (to address someone as 'mevrouw').",
                        "ru": "Первое лицо единственного числа настоящего времени глагола 'mevrouwen' (называть кого-либо «госпожой»)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "address as 'madam'",
                                "target_lang_sense_clarification": "action of using the title"
                            },
                            {
                                "target_lang_word": "call 'Ms.'",
                                "target_lang_sense_clarification": "action of using the title"
                            }
                        ],

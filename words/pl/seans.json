                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym domu zorganizowano tajemniczy <w>seans</w> spirytystyczny.",
                            "target_lang_translations": {
                                "ru": "В старом доме организовали таинственный спиритический <w>сеанс</w>."
                            }

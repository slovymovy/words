                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break up",
                                "target_lang_sense_clarification": "end a romantic relationship"
                            },
                            {
                                "target_lang_word": "split up",
                                "target_lang_sense_clarification": "end a partnership or relationship"
                            },
                            {
                                "target_lang_word": "part ways",
                                "target_lang_sense_clarification": "formal or metaphorical ending of association"

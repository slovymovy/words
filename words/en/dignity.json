                },
                {
                    "sense_id": "978cd10a-729c-3488-9014-ec37f601606e",
                    "sense_definition": "A fundamental principle, axiom, or maxim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "principle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The philosopher based his arguments on ancient <w>dignities</w>.",
                            "target_lang_translations": {
                                "ru": "Философ основывал свои аргументы на древних <w>аксиомах</w>."
                            }

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2416a169-ac94-36e2-ba18-74b987032acc",
                    "sense_definition": "To prevent something from being successful; to put an end to a plan or rumor.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden storm <w>scotched</w> our plans for a weekend camping trip.",
                            "target_lang_translations": {
                                "ru": "Внезапный шторм <w>сорвал</w> наши планы на поход в выходные."
                            }
                        },
                        {
                            "text": "The CEO quickly <w>scotched</w> the rumors about the company merging with a competitor.",
                            "target_lang_translations": {
                                "ru": "Генеральный директор быстро <w>пресек</w> слухи о слиянии компании с конкурентом."
                            }
                        }
                    ],
                    "synonyms": [
                        "thwart",
                        "foil",
                        "put the kibosh on"
                    ],
                    "common_phrases": [
                        "scotch a rumor",
                        "scotch a plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положить конец чему-либо; пресечь (планы, слухи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пресечь",
                                "target_lang_sense_clarification": "остановить развитие чего-либо (слухов, действий)"
                            },
                            {
                                "target_lang_word": "сорвать",
                                "target_lang_sense_clarification": "нарушить планы"
                            },
                            {
                                "target_lang_word": "положить конец",
                                "target_lang_sense_clarification": "прекратить что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d54dfbdf-34d6-3188-b9a5-2901d3395178",
                    "sense_definition": "To block a wheel or other object with a wedge to prevent it from moving.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_block",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers <w>scotched</w> the wheels of the wagon so it wouldn't roll away.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>застопорили</w> колеса вагона, чтобы он не покатился."
                            }
                        },
                        {
                            "text": "Make sure to <w>scotch</w> the vehicle securely when parking on such a steep incline.",
                            "target_lang_translations": {
                                "ru": "Обязательно надежно <w>зафиксируйте</w> автомобиль при парковке на таком крутом склоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "chock",
                        "block"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заблокировать колесо клином или упором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застопорить",
                                "target_lang_sense_clarification": "зафиксировать от движения"
                            },
                            {
                                "target_lang_word": "заблокировать",
                                "target_lang_sense_clarification": "предотвратить движение"
                            }
                        ]
                    }
                }
            ]
        },
        {

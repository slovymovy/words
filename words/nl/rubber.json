                    ],
                    "common_phrases": [
                        "natuurlijk rubber",
                        "synthetisch rubber",
                        "geur van rubber"
                    ],
                    "target_lang_definitions": {
                        "en": "Elastic material originally manufactured from the sap of the rubber tree.",
                        "ru": "Эластичный материал, первоначально изготавливаемый из сока каучукового дерева."
                    },

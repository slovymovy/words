                {
                    "sense_id": "d10293ec-cc63-3cc9-8739-298912a2b47f",
                    "sense_definition": "Martin Luther (1483–1546), a German monk and theologian whose teachings inspired the Protestant Reformation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "historical_figure",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "The ninety-five theses written by <w>Luther</w> challenged the practices of the Catholic Church.",
                            "target_lang_translations": {
                                "ru": "Девяносто пять тезисов, написанных <w>Лютером</w>, бросили вызов практике католической церкви."
                            }
                        },
                        {
                            "text": "Many historians consider Martin <w>Luther</w> to be one of the most influential figures in Western history.",
                            "target_lang_translations": {
                                "ru": "Многие историки считают Мартина <w>Лютера</w> одной из самых влиятельных фигур в западной истории."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Martin Luther"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мартин Лютер (1483–1546), немецкий монах и богослов, чье учение вдохновило Реформацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лютер",
                                "target_lang_sense_clarification": "историческая личность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "834d320b-2668-3182-a93f-dee539f3b65e",
                    "sense_definition": "A surname of German origin.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Luther</w> has been teaching mathematics at the high school for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Лютер</w> преподает математику в старшей школе уже более двадцати лет."
                            }
                        },
                        {
                            "text": "We received an invitation to the wedding of Sarah <w>Luther</w> next month.",
                            "target_lang_translations": {
                                "ru": "Мы получили приглашение на свадьбу Сары <w>Лютер</w> в следующем месяце."
                            }

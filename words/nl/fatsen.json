    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a423322e-fdde-3ea3-8719-a7d81469f641",
                    "sense_definition": "Zonder toestemming wegblijven van school.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verzuim",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jongens besloten te <w>fatsen</w> en de stad in te gaan.",
                            "target_lang_translations": {
                                "en": "The boys decided to <w>skip school</w> and go into town.",
                                "ru": "Мальчики решили <w>прогулять</w> и пойти в город."
                            }
                        },
                        {
                            "text": "Als je te vaak gaat <w>fatsen</w>, krijg je problemen met de directie.",
                            "target_lang_translations": {
                                "en": "If you <w>play truant</w> too often, you will get into trouble with the administration.",
                                "ru": "Если ты будешь слишком часто <w>прогуливать</w>, у тебя будут проблемы с администрацией."
                            }
                        }
                    ],
                    "synonyms": [
                        "spijbelen"
                    ],
                    "common_phrases": [
                        "gaan fatsen"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in Antwerpen/Vlaanderen"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stay away from school without permission.",
                        "ru": "Пропускать занятия в школе без разрешения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "skip school",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "play truant",
                                "target_lang_sense_clarification": "standard"
                            },
                            {
                                "target_lang_word": "play hooky",
                                "target_lang_sense_clarification": "informal, US"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прогуливать",
                                "target_lang_sense_clarification": "пропускать занятия"
                            },
                            {
                                "target_lang_word": "сачковать",
                                "target_lang_sense_clarification": "разг. уклоняться от обязанностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c73b1a7-283f-343d-a542-2c6fd58361f4",
                    "sense_definition": "Zich langzaam of op het gemak te voet verplaatsen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bewegen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>fatsen</w> rustig door de straten van het oude centrum.",
                            "target_lang_translations": {
                                "en": "We <w>stroll</w> quietly through the streets of the old center.",
                                "ru": "Мы спокойно <w>прогуливаемся</w> по улицам старого центра."
                            }
                        },
                        {
                            "text": "Na het eten gingen ze nog wat <w>fatsen</w> in het park.",
                            "target_lang_translations": {
                                "en": "After dinner, they went <w>strolling</w> in the park for a bit.",
                                "ru": "После еды они пошли немного <w>побродить</w> в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "kuieren",
                        "wandelen"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Vlaams"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To walk slowly or in a relaxed manner.",
                        "ru": "Медленно или расслабленно передвигаться пешком."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stroll",
                                "target_lang_sense_clarification": "walk in a leisurely way"
                            },
                            {
                                "target_lang_word": "saunter",
                                "target_lang_sense_clarification": "walk in a slow, relaxed manner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прогуливаться",
                                "target_lang_sense_clarification": "гулять не спеша"
                            },
                            {
                                "target_lang_word": "бродить",
                                "target_lang_sense_clarification": "ходить без определенной цели"
                            },
                            {
                                "target_lang_word": "слоняться",
                                "target_lang_sense_clarification": "разг. ходить без дела"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "958a44dc-8688-3302-996a-4c7fbd29f697",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>fats</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die ijdele <w>fatsen</w> denken alleen maar aan hun uiterlijk.",
                            "target_lang_translations": {
                                "en": "Those vain <w>dandies</w> only think about their appearance.",
                                "ru": "Эти тщеславные <w>пижоны</w> думают только о своей внешности."
                            }
                        },
                        {
                            "text": "Hij had een hekel aan de arrogante <w>fatsen</w> in de club.",
                            "target_lang_translations": {
                                "en": "He hated the arrogant <w>fops</w> in the club.",
                                "ru": "Он терпеть не мог высокомерных <w>хлыщей</w> в клубе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "meervoud"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'fats' (a vain, stylishly dressed man).",
                        "ru": "Множественное число существительного 'fats' (пижон, хлыщ)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dandies",
                                "target_lang_sense_clarification": "men unduly devoted to style and appearance"
                            },
                            {
                                "target_lang_word": "fops",
                                "target_lang_sense_clarification": "men concerned with their clothes and appearance in an affected way"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пижоны",
                                "target_lang_sense_clarification": "люди, чрезмерно заботящиеся о внешности"
                            },
                            {
                                "target_lang_word": "хлыщи",
                                "target_lang_sense_clarification": "пренебр. пустые щеголи"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3b46deaa-12b3-329b-bc3b-c1ab64303308",
                    "sense_definition": "A diminutive of the male given name <w>Walter</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Walt</w> tells the best stories at family gatherings.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Уолт</w> рассказывает лучшие истории на семейных посиделках."
                            }
                        },
                        {
                            "text": "We decided to name the puppy <w>Walt</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили назвать щенка <w>Уолт</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужского имени Уолтер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уолт",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c959249f-45b6-3c86-af68-c53dd65e6991",
                    "sense_definition": "A person who falsely claims to be a military veteran.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was exposed as a <w>Walt</w> when he couldn't answer basic questions about his regiment.",
                            "target_lang_translations": {
                                "ru": "Его разоблачили как <w>ряженого</w>, когда он не смог ответить на элементарные вопросы о своем полку."
                            }
                        },
                        {
                            "text": "Real soldiers have no respect for a <w>Walt</w> wearing medals he didn't earn.",
                            "target_lang_translations": {
                                "ru": "Настоящие солдаты не уважают <w>самозванца</w>, носящего медали, которые он не заслужил."
                            }
                        }
                    ],
                    "synonyms": [
                        "imposter",
                        "fraud",
                        "pretender"
                    ],
                    "common_phrases": [
                        "bloody Walt"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ложно выдающий себя за ветерана боевых действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ряженый",
                                "target_lang_sense_clarification": "ложный ветеран"
                            },
                            {
                                "target_lang_word": "самозванец",
                                "target_lang_sense_clarification": "обманщик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26208c78-84e1-31c6-9736-bc5bc2b3867d",
                    "sense_definition": "An acronym for 'We Are Learning To', used in schools to outline the learning objective of a lesson.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher wrote the <w>WALT</w> on the whiteboard at the start of the class.",
                            "target_lang_translations": {
                                "ru": "Учитель написал <w>цель урока</w> на доске в начале занятия."
                            }
                        },
                        {
                            "text": "Please copy the <w>WALT</w> into your exercise books.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, перепишите <w>цель урока</w> в свои тетради."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "acronym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акроним фразы 'We Are Learning To' (Мы учимся...), используемый в школах для обозначения цели урока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цель урока",
                                "target_lang_sense_clarification": "педагогический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

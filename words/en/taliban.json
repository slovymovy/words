                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тали́б",
                                "target_lang_sense_clarification": "участник движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83c9f9d3-05e9-3392-9ddf-47e18fe3a296",
                    "sense_definition": "A specific militia group affiliated with or similar to the Taliban movement.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Analysts argue that there are actually two distinct <w>Talibans</w> operating in the region.",
                            "target_lang_translations": {

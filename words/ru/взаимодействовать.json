{
    "word_family": [
        "взаимодействие"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "076fa0a7-dc9e-3994-b0bb-a19f27b93e99",
                    "sense_definition": "Оказывать взаимное влияние друг на друга; находиться в связи или контакте с кем-либо или чем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Различные химические вещества могут активно <w>взаимодействовать</w> при смешивании.",
                            "target_lang_translations": {
                                "en": "Various chemical substances can actively <w>interact</w> when mixed."
                            }
                        },
                        {
                            "text": "Пользователь должен легко <w>взаимодействовать</w> с интерфейсом новой программы.",
                            "target_lang_translations": {
                                "en": "The user must be able to easily <w>interact</w> with the new program's interface."
                            }
                        }
                    ],
                    "synonyms": [
                        "контактировать"
                    ],
                    "antonyms": [
                        "противодействовать"
                    ],
                    "common_phrases": [
                        "взаимодействовать с системой",
                        "взаимодействовать друг с другом",
                        "активно взаимодействовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To exert mutual influence on each other; to be in connection or contact with someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interact",
                                "target_lang_sense_clarification": "to act in such a way as to have an effect on another"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9418858f-9d0a-3228-a5ad-a2c1598c4fcb",
                    "sense_definition": "Действовать согласованно, сообща при выполнении какой-либо задачи.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cooperation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для успеха проекта все отделы компании должны тесно <w>взаимодействовать</w>.",
                            "target_lang_translations": {
                                "en": "For the project's success, all company departments must closely <w>collaborate</w>."
                            }
                        },
                        {
                            "text": "Спасательные службы начали <w>взаимодействовать</w> с полицией для поиска пропавших людей.",
                            "target_lang_translations": {
                                "en": "Rescue services began to <w>cooperate</w> with the police to search for the missing people."
                            }
                        }
                    ],
                    "synonyms": [
                        "сотрудничать",
                        "кооперироваться"
                    ],
                    "antonyms": [
                        "противодействовать"
                    ],
                    "common_phrases": [
                        "взаимодействовать в команде",
                        "тесно взаимодействовать",
                        "эффективно взаимодействовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To act in coordination or together when performing a task."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cooperate",
                                "target_lang_sense_clarification": "to work together towards a common end"
                            },
                            {
                                "target_lang_word": "collaborate",
                                "target_lang_sense_clarification": "to work jointly on an activity"
                            },
                            {
                                "target_lang_word": "interact",
                                "target_lang_sense_clarification": "to engage with others to achieve a goal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
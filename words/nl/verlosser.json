    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f343bbf7-998a-32cb-bf25-05960d8272ed",
                    "sense_definition": "Iemand die anderen bevrijdt uit gevaar, nood of een moeilijke situatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Johan Cruijff werd door de fans gezien als de <w>verlosser</w> van de voetbalclub.",
                            "target_lang_translations": {
                                "en": "Johan Cruijff was seen by the fans as the <w>savior</w> of the football club.",
                                "ru": "Йохан Кройф воспринимался фанатами как <w>спаситель</w> футбольного клуба."
                            }
                        },
                        {
                            "text": "Na de overstroming kwamen de hulpverleners als een <w>verlosser</w> voor de bewoners.",
                            "target_lang_translations": {
                                "en": "After the flood, the rescue workers came as a <w>savior</w> to the residents.",
                                "ru": "После наводнения спасатели явились для жителей словно <w>избавитель</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "redder",
                        "bevrijder"
                    ],
                    "antonyms": [
                        "onderdrukker"
                    ],
                    "common_phrases": [
                        "binnengehaald als verlosser",
                        "de grote verlosser"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who frees others from danger, distress, or a difficult situation.",
                        "ru": "Тот, кто освобождает других от опасности, нужды или трудной ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "savior",
                                "target_lang_sense_clarification": "general term for someone who saves"
                            },
                            {
                                "target_lang_word": "liberator",
                                "target_lang_sense_clarification": "someone who sets others free"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спаситель",
                                "target_lang_sense_clarification": "общий смысл; тот, кто спасает"
                            },
                            {
                                "target_lang_word": "освободитель",
                                "target_lang_sense_clarification": "тот, кто дарует свободу"
                            },
                            {
                                "target_lang_word": "избавитель",
                                "target_lang_sense_clarification": "тот, кто избавляет от беды или мучений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3391a19c-6c29-3ab9-911e-f6ce38c69db4",
                    "sense_definition": "Een eretitel voor Jezus Christus als degene die de mensheid redt van zonden.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Tijdens de kerkdienst zongen de gelovigen een lied voor hun <w>Verlosser</w>.",
                            "target_lang_translations": {
                                "en": "During the church service, the believers sang a song for their <w>Redeemer</w>.",
                                "ru": "Во время церковной службы верующие пели песню своему <w>Спасителю</w>."
                            }
                        },
                        {
                            "text": "In het christendom wordt Jezus beschouwd als de <w>Verlosser</w> van de wereld.",
                            "target_lang_translations": {
                                "en": "In Christianity, Jesus is considered the <w>Savior</w> of the world.",
                                "ru": "В христианстве Иисус почитается как <w>Спаситель</w> мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "Heiland",
                        "Messias",
                        "Christus"
                    ],
                    "common_phrases": [
                        "onze Verlosser"
                    ],
                    "target_lang_definitions": {
                        "en": "An honorific title for Jesus Christ as the one who saves humanity from sins.",
                        "ru": "Почетный титул Иисуса Христа как спасителя человечества от грехов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Redeemer",
                                "target_lang_sense_clarification": "religious title"
                            },
                            {
                                "target_lang_word": "Savior",
                                "target_lang_sense_clarification": "religious title"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Спаситель",
                                "target_lang_sense_clarification": "религиозный титул"
                            },
                            {
                                "target_lang_word": "Искупитель",
                                "target_lang_sense_clarification": "тот, кто искупает грехи"
                            }
                        ]

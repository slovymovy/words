            "senses": [
                {
                    "sense_id": "173b5ef4-6d9e-3da8-aefd-404d36862c4a",
                    "sense_definition": "Wynik rywalizacji sportowej lub gry, w którym żadna ze stron nie odniosła zwycięstwa; gra nierozstrzygnięta.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aby przejść do następnej rundy, muszą wywalczyć w tym meczu przynajmniej <w>remis</w>.",
                            "target_lang_translations": {
                                "ru": "Чтобы пройти в следующий раунд, они должны добиться в этом матче хотя бы <w>ничьей</w>."
                            }
                        },
                        {
                            "text": "Mecz zakończył się niewygodnym dla obu stron <w>remisem</w> 1:1.",
                            "target_lang_translations": {
                                "ru": "Матч закончился неудобной для обеих сторон <w>ничьей</w> со счетом 1:1."
                            }
                        },
                        {
                            "text": "Mimo zaciętej walki, spotkanie zakończyło się bezbramkowym <w>remisem</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на упорную борьбу, встреча завершилась безголевой <w>ничьей</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "wygrana",
                        "przegrana",
                        "zwycięstwo",
                        "porażka"
                    ],
                    "common_phrases": [
                        "bezbramkowy remis",
                        "uratować remis",
                        "walczyć o remis",
                        "zakończyć się remisem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат спортивного состязания или игры, в котором ни одна из сторон не одержала победу; нерешенный исход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничья",
                                "target_lang_sense_clarification": "результат игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2638964-e1f7-373a-86e6-053a27beed6a",
                    "sense_definition": "Odłożenie sprawy sądowej do innego sądu lub na inne posiedzenie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_postponement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia zarządził <w>remis</w> sprawy z powodu niestawienia się głównego świadka.",
                            "target_lang_translations": {
                                "ru": "Судья распорядился об <w>отсрочке</w> дела из-за неявки главного свидетеля."
                            }
                        },
                        {
                            "text": "Adwokat złożył wniosek o <w>remis</w> postępowania do czasu uzupełnienia materiału dowodowego.",
                            "target_lang_translations": {
                                "ru": "Адвокат подал ходатайство об <w>отсрочке</w> разбирательства до дополнения доказательной базы."
                            }

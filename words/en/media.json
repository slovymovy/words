    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d96421f2-8a79-3e57-9d80-ccd6b188022d",
                    "sense_definition": "Means and institutions for publishing and broadcasting information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mass_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Social media has completely changed how we communicate with each other.",
                            "target_lang_translations": {
                                "ru": "Социальные <w>медиа</w> полностью изменили то, как мы общаемся друг с другом."
                            }
                        },
                        {
                            "text": "The president held a conference to address the questions from the news media.",
                            "target_lang_translations": {
                                "ru": "Президент провел конференцию, чтобы ответить на вопросы новостных <w>СМИ</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cuarto"
                    ],
                    "common_phrases": [
                        "social media",
                        "mass media",
                        "news media",
                        "media coverage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средства и учреждения для публикации и вещания информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "СМИ",
                                "target_lang_sense_clarification": "аббревиатура: средства массовой информации"
                            },
                            {
                                "target_lang_word": "медиа",
                                "target_lang_sense_clarification": "современный термин, часто в сочетании с \"социальные\""
                            },
                            {
                                "target_lang_word": "средства массовой информации",
                                "target_lang_sense_clarification": "полный официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "694cf204-cee5-3357-a57b-1af2e5db556a",
                    "sense_definition": "The journalists and other professionals who comprise the mass communication industry.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mass_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The celebrity tried to avoid the media by leaving through the back door.",
                            "target_lang_translations": {
                                "ru": "Знаменитость пыталась избежать <w>СМИ</w>, выйдя через черный ход."
                            }
                        },
                        {
                            "text": "Members of the media were invited to preview the new exhibition.",
                            "target_lang_translations": {
                                "ru": "Представители <w>СМИ</w> были приглашены на предварительный просмотр новой выставки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the media",
                        "local media"
                    ],
                    "target_lang_definitions": {
                        "ru": "Журналисты и другие профессионалы, составляющие индустрию массовых коммуникаций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "СМИ",
                                "target_lang_sense_clarification": "собирательное значение: журналисты и репортеры"
                            },
                            {
                                "target_lang_word": "пресса",
                                "target_lang_sense_clarification": "печатные издания и их представители"
                            },
                            {
                                "target_lang_word": "журналисты",
                                "target_lang_sense_clarification": "работники медиасферы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cd6709e-225b-36f9-8478-3daac80434d7",
                    "sense_definition": "The totality of content items (television shows, films, books, photographs, etc.) which are broadcast or published.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mass_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parents should monitor the media that their children consume daily.",
                            "target_lang_translations": {
                                "ru": "Родители должны контролировать <w>медиаконтент</w>, который их дети потребляют ежедневно."
                            }
                        },
                        {
                            "text": "The library has a large collection of digital media available for loan.",
                            "target_lang_translations": {
                                "ru": "В библиотеке есть большая коллекция цифровых <w>медиа</w>, доступных для абонемента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "digital media",
                        "popular media"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность контента (телепередачи, фильмы, книги, фотографии и т.д.), который транслируется или публикуется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медиаконтент",
                                "target_lang_sense_clarification": "содержание информационных ресурсов"
                            },
                            {
                                "target_lang_word": "медиа",
                                "target_lang_sense_clarification": "цифровые материалы"
                            },
                            {
                                "target_lang_word": "медиапродукция",
                                "target_lang_sense_clarification": "произведенный контент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e313c42-f622-32ee-8b07-cdf38a6343b7",
                    "sense_definition": "Files and data comprising material viewable by humans, but usually not plain text; audiovisual material.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should back up your photos and other media to an external drive.",
                            "target_lang_translations": {
                                "ru": "Вам следует создать резервную копию фотографий и других <w>медиафайлов</w> на внешний диск."
                            }
                        },
                        {
                            "text": "The website supports various forms of rich media to engage users.",
                            "target_lang_translations": {
                                "ru": "Веб-сайт поддерживает различные формы обогащенных <w>медиа</w> для вовлечения пользователей."
                            }
                        }
                    ],
                    "synonyms": [
                        "M"
                    ],
                    "common_phrases": [
                        "rich media",
                        "media file"
                    ],
                    "target_lang_definitions": {
                        "ru": "Файлы и данные, содержащие материалы для просмотра (обычно не простой текст); аудиовизуальные материалы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медиафайлы",
                                "target_lang_sense_clarification": "файлы с аудио, видео или изображениями"
                            },
                            {
                                "target_lang_word": "мультимедиа",
                                "target_lang_sense_clarification": "комбинированные форматы данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2388771-ca94-382c-afab-1c9f57b46cfb",
                    "sense_definition": "Plural of <w>medium</w> (only in certain senses).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "plural_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Air and water are the physical media through which sound waves travel.",
                            "target_lang_translations": {
                                "ru": "Воздух и вода — это физические <w>среды</w>, через которые распространяются звуковые волны."
                            }
                        },
                        {
                            "text": "The artist works with mixed media to create unique textures in her paintings.",
                            "target_lang_translations": {
                                "ru": "Художница работает со смешанными <w>материалами</w>, чтобы создать уникальные текстуры на своих картинах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mixed media"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of medium"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова medium (среда, средство, носитель)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среды",
                                "target_lang_sense_clarification": "физические субстанции"
                            },
                            {
                                "target_lang_word": "средства",
                                "target_lang_sense_clarification": "способы или материалы (в искусстве)"
                            },
                            {
                                "target_lang_word": "носители",
                                "target_lang_sense_clarification": "физические объекты для передачи чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bfac5f1-7354-300b-ab03-f342f3d49701",
                    "sense_definition": "Plural of <w>medium</w> (“someone who supposedly conveys information from the spirit world”).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "plural_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During the nineteenth century, many famous media claimed to speak with the dead.",
                            "target_lang_translations": {
                                "ru": "В девятнадцатом веке многие известные <w>медиумы</w> утверждали, что говорят с мертвыми."
                            }
                        },
                        {
                            "text": "The researcher studied how these media entered a trance state during the seance.",
                            "target_lang_translations": {
                                "ru": "Исследователь изучал, как эти <w>медиумы</w> входили в состояние транса во время сеанса."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of medium"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова medium (медиум - посредник между миром живых и мертвых)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медиумы",
                                "target_lang_sense_clarification": "люди, общающиеся с духами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e37ab165-4d1f-344a-869f-a2e018e3410c",
                    "sense_definition": "The middle layer of the wall of a blood vessel or lymph vessel which is composed of connective and muscular tissue.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Damage to the tunica media can weaken the structural integrity of the artery.",
                            "target_lang_translations": {
                                "ru": "Повреждение tunica <w>media</w> может ослабить структурную целостность артерии."
                            }
                        },
                        {
                            "text": "The media contains smooth muscle cells that help regulate blood pressure.",
                            "target_lang_translations": {
                                "ru": "<w>Медиа</w> содержит гладкомышечные клетки, которые помогают регулировать кровяное давление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tunica media"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical anatomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Средняя оболочка стенки кровеносного или лимфатического сосуда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медиа",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "средняя оболочка",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7199ae36-d945-3645-8813-f5400795dd12",
                    "sense_definition": "A voiced stop consonant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Ancient Greek, the media consonants were voiced stops like beta and delta.",
                            "target_lang_translations": {
                                "ru": "В древнегреческом языке согласные <w>медиа</w> были звонкими смычными, как бета и дельта."
                            }
                        },
                        {
                            "text": "The shift from a tenuis to a media is a common phonological process.",
                            "target_lang_translations": {
                                "ru": "Переход от tenuis к <w>media</w> является распространенным фонологическим процессом."
                            }
                        }
                    ],
                    "antonyms": [
                        "tenuis"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical linguistics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звонкий смычный согласный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медиа",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "звонкий смычный",
                                "target_lang_sense_clarification": "описание звука"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0c22bcb7-8e0b-3119-beaa-62fd63074aa2",
                    "sense_definition": "Clipping of <w>multimedia</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to install a new media player to watch this video format.",
                            "target_lang_translations": {
                                "ru": "Мне нужно установить новый <w>медиа</w>плеер, чтобы смотреть этот видеоформат."
                            }

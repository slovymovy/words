    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e11b702-3480-3ee7-b5be-f2ed3bb22403",
                    "sense_definition": "Издавать, выпускать в свет произведения печати (книги, статьи, журналы).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publishing_works",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Известный писатель каждый год <публикует> новый роман.",
                            "target_lang_translations": {
                                "en": "The famous writer <publishes> a new novel every year."
                            }
                        },
                        {
                            "text": "Этот научный журнал <публикует> статьи на разных языках.",
                            "target_lang_translations": {
                                "en": "This scientific journal <publishes> articles in different languages."
                            }
                        }
                    ],
                    "synonyms": [
                        "издавать"
                    ],
                    "common_phrases": [
                        "публиковать статью",
                        "публиковать книгу",
                        "публиковать журнал",
                        "публиковать роман"
                    ],
                    "target_lang_definitions": {
                        "en": "To issue or release printed works such as books, articles, or magazines."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "publish",
                                "target_lang_sense_clarification": "to produce and sell a book, magazine, or newspaper"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19d0224d-d067-341a-8578-f3dfebf63e31",
                    "sense_definition": "Делать информацию доступной для всеобщего сведения через печать или интернет.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "making_public",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство регулярно <публикует> отчеты о состоянии экономики.",
                            "target_lang_translations": {
                                "en": "The government regularly <publishes> reports on the state of the economy."
                            }
                        },
                        {
                            "text": "Газеты не имеют права <публиковать> непроверенные слухи.",
                            "target_lang_translations": {
                                "en": "Newspapers do not have the right to <publish> unverified rumors."
                            }
                        }
                    ],
                    "antonyms": [
                        "утаивать",
                        "замалчивать"
                    ],
                    "common_phrases": [
                        "публиковать отчет",
                        "публиковать данные",
                        "публиковать список",
                        "публиковать результаты"
                    ],
                    "target_lang_definitions": {
                        "en": "To make information available to the general public via print or the internet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "publish",
                                "target_lang_sense_clarification": "to make information available to people"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "to make a report or data public"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "was",
        "doek",
        "wassen",
        "wasbaar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a830461-a7d7-3b75-8653-9b7287ac014e",
                    "sense_definition": "Een weefsel van katoen of linnen dat waterdicht is gemaakt met een laagje vernis of kunststof, vaak gebruikt als tafelkleed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We leggen een <w>wasdoek</w> op de tafel als de kinderen gaan verven.",
                            "target_lang_translations": {
                                "en": "We put an <w>oilcloth</w> on the table when the children are going to paint.",
                                "ru": "Мы стелим <w>клеёнку</w> на стол, когда дети собираются рисовать красками."
                            }
                        },
                        {
                            "text": "Dit <w>wasdoek</w> is heel makkelijk schoon te maken met een natte spons.",
                            "target_lang_translations": {
                                "en": "This <w>oilcloth</w> is very easy to clean with a wet sponge.",
                                "ru": "Эту <w>клеёнку</w> очень легко чистить влажной губкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeil",
                        "tafelzeil"
                    ],
                    "common_phrases": [
                        "tafel met wasdoek",
                        "afneembaar wasdoek"
                    ],
                    "target_lang_definitions": {
                        "en": "A cotton or linen fabric made waterproof with a layer of varnish or plastic, often used as a tablecloth.",
                        "ru": "Хлопчатобумажная или льняная ткань, пропитанная водонепроницаемым составом, часто используемая как скатерть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oilcloth",
                                "target_lang_sense_clarification": "waterproof fabric for tables"
                            },
                            {
                                "target_lang_word": "oilskin",
                                "target_lang_sense_clarification": "treated waterproof fabric"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "клеёнка",
                                "target_lang_sense_clarification": "водонепроницаемая ткань для стола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a41a874-b0f6-307e-8114-027209357d98",
                    "sense_definition": "Een doek of lapje stof dat gebruikt wordt om het lichaam mee te wassen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "hygiene",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij waste zijn gezicht met koud water en een <w>wasdoek</w>.",
                            "target_lang_translations": {
                                "en": "He washed his face with cold water and a <w>washcloth</w>.",
                                "ru": "Он умыл лицо холодной водой и <w>салфеткой для умывания</w>."
                            }
                        },
                        {
                            "text": "In de badkamer liggen schone handdoeken en een <w>wasdoek</w> klaar.",
                            "target_lang_translations": {
                                "en": "Clean towels and a <w>washcloth</w> are ready in the bathroom.",
                                "ru": "В ванной приготовлены чистые полотенца и <w>салфетка для мытья</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "natte wasdoek"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België gebruikt, in Nederland spreekt men vaker van een washandje"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A cloth or piece of fabric used to wash the body.",
                        "ru": "Ткань или лоскут материи, используемый для мытья тела."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "washcloth",
                                "target_lang_sense_clarification": "for personal hygiene"
                            },
                            {
                                "target_lang_word": "flannel",
                                "target_lang_sense_clarification": "UK term"
                            },
                            {
                                "target_lang_word": "face cloth",
                                "target_lang_sense_clarification": "small cloth for the face"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "салфетка для мытья",
                                "target_lang_sense_clarification": "для личной гигиены"
                            },
                            {
                                "target_lang_word": "мочалка",
                                "target_lang_sense_clarification": "предмет для мытья тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d16301bb-8d47-3e16-bc0d-7a69f23ada8d",
                    "sense_definition": "Een doek die gebruikt wordt om een vloer of meubel in de was te zetten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cleaning_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schoonmaker gebruikte een oude <w>wasdoek</w> voor het parket.",
                            "target_lang_translations": {
                                "en": "The cleaner used an old <w>waxing cloth</w> for the parquet.",
                                "ru": "Уборщик использовал старую <w>тряпку для вощения</w> для паркета."
                            }
                        },
                        {
                            "text": "Na het boenen rook de <w>wasdoek</w> sterk naar bijenwas.",
                            "target_lang_translations": {
                                "en": "After polishing, the <w>waxing cloth</w> smelled strongly of beeswax.",
                                "ru": "После натирания <w>тряпка для вощения</w> сильно пахла пчелиным воском."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A cloth used to apply wax to a floor or piece of furniture.",
                        "ru": "Тряпка, используемая для натирания пола или мебели воском."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waxing cloth",
                                "target_lang_sense_clarification": "for applying wax"
                            },
                            {
                                "target_lang_word": "polishing cloth",
                                "target_lang_sense_clarification": "for cleaning and shining"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тряпка для вощения",
                                "target_lang_sense_clarification": "для нанесения воска"
                            },
                            {
                                "target_lang_word": "полировочная тряпка",
                                "target_lang_sense_clarification": "для полировки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "damnation",
        "damnable"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "98f1cdc6-7ff4-305c-b493-dce215ce53f9",
                    "sense_definition": "Used to express anger, irritation, disappointment, annoyance, contempt, or surprise.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oh <w>damn</w>, I completely forgot to bring my wallet with me.",
                            "target_lang_translations": {
                                "ru": "О <w>чёрт</w>, я совсем забыл взять с собой кошелек."
                            }
                        },
                        {
                            "text": "<w>Damn</w> it, I cannot believe we missed the last train home.",
                            "target_lang_translations": {
                                "ru": "<w>Чёрт побери</w>, не могу поверить, что мы опоздали на последнюю электричку домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "blast",
                        "drat",
                        "hell",
                        "shit",
                        "fuck",
                        "shoot"
                    ],
                    "common_phrases": [
                        "damn it",
                        "god damn"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения гнева, раздражения, разочарования или удивления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрт побери",
                                "target_lang_sense_clarification": "выражение досады или гнева"
                            },
                            {
                                "target_lang_word": "проклятие",
                                "target_lang_sense_clarification": "эмоциональное восклицание"
                            },
                            {
                                "target_lang_word": "блин",
                                "target_lang_sense_clarification": "разговорный эвфемизм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "568102cf-5aa5-32b2-aa2f-785968a640a7",
                    "sense_definition": "Used as an intensifier to express annoyance, anger, or emphasis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot find a single <w>damn</w> thing in this messy room.",
                            "target_lang_translations": {
                                "ru": "Я не могу найти ни одной <w>чёртовой</w> вещи в этой грязной комнате."
                            }
                        },
                        {
                            "text": "Turn off that <w>damn</w> music before I call the police.",
                            "target_lang_translations": {
                                "ru": "Выключи эту <w>проклятую</w> музыку, пока я не вызвал полицию."
                            }
                        }
                    ],
                    "synonyms": [
                        "bloody",
                        "blasted",
                        "cursed",
                        "damned",
                        "fucking",
                        "goddamn"
                    ],
                    "common_phrases": [
                        "damn fool",
                        "damn thing",
                        "whole damn time"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как усилитель для выражения раздражения, гнева или эмфазы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёртов",
                                "target_lang_sense_clarification": "разговорное, выражает недовольство"
                            },
                            {
                                "target_lang_word": "проклятый",
                                "target_lang_sense_clarification": "более сильное выражение ненависти или раздражения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ee49b845-4994-39b3-8e75-b65fa5b88fe3",
                    "sense_definition": "Very or extremely; used to emphasize an adjective or adverb.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You know <w>damn</w> well that I did not say that to him.",
                            "target_lang_translations": {
                                "ru": "Ты <w>чертовски</w> хорошо знаешь, что я не говорил ему этого."
                            }
                        },
                        {
                            "text": "That was a <w>damn</w> good movie we watched at the cinema yesterday.",
                            "target_lang_translations": {
                                "ru": "Это был <w>чертовски</w> хороший фильм, который мы смотрели вчера в кино."
                            }
                        }
                    ],
                    "synonyms": [
                        "awfully",
                        "extremely",
                        "terribly",
                        "incredibly",
                        "bloody"
                    ],
                    "common_phrases": [
                        "damn well",
                        "damn near",
                        "damn good"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень или чрезвычайно; используется для усиления прилагательного или наречия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чертовски",
                                "target_lang_sense_clarification": "разговорное, в значении «очень»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07b5ee10-d020-3d97-8fe3-430c1432469d",
                    "sense_definition": "To condemn someone to eternal punishment or a terrible fate.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "condemn_curse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient texts say the gods will <w>damn</w> the wicked for eternity.",
                            "target_lang_translations": {
                                "ru": "Древние тексты гласят, что боги <w>проклянут</w> грешников навечно."
                            }
                        },
                        {
                            "text": "He felt that his past mistakes would <w>damn</w> him to a life of regret.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что прошлые ошибки <w>обрекут</w> его на жизнь, полную сожалений."
                            }
                        }
                    ],
                    "synonyms": [
                        "condemn",
                        "doom",
                        "curse"
                    ],
                    "antonyms": [
                        "save",
                        "bless"
                    ],
                    "common_phrases": [
                        "damn to hell",
                        "be damned"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрекать кого-либо на вечные муки или ужасную участь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проклинать",
                                "target_lang_sense_clarification": "предавать анафеме, желать вечных мук"
                            },
                            {
                                "target_lang_word": "обрекать",
                                "target_lang_sense_clarification": "на страдания или гибель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e53af48-71a6-3e2f-a75e-f44245caaafc",
                    "sense_definition": "To curse or express great anger toward someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "condemn_curse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>damn</w> the day I ever met that dishonest man.",
                            "target_lang_translations": {
                                "ru": "Я <w>проклинаю</w> тот день, когда встретил этого лживого человека."
                            }
                        },
                        {
                            "text": "The farmer started to <w>damn</w> the weather when the heavy rain destroyed his crops.",
                            "target_lang_translations": {
                                "ru": "Фермер начал <w>клясть</w> погоду, когда сильный дождь уничтожил его урожай."
                            }
                        }
                    ],
                    "synonyms": [
                        "curse",
                        "execrate"
                    ],
                    "antonyms": [
                        "bless"
                    ],
                    "common_phrases": [
                        "damn you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проклинать или выражать сильный гнев по отношению к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проклинать",
                                "target_lang_sense_clarification": "желать зла, сильно ругать"
                            },
                            {
                                "target_lang_word": "клясть",
                                "target_lang_sense_clarification": "бранить, ругать судьбу или обстоятельства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5997a55a-b85c-3515-90c4-94731692b2ab",
                    "sense_definition": "To criticize strongly or declare something as unfit or invalid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "condemn_curse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The critics were quick to <w>damn</w> the new play as boring and uninspired.",
                            "target_lang_translations": {
                                "ru": "Критики поспешили <w>раскритиковать</w> новую пьесу как скучную и бездарную."
                            }
                        },
                        {
                            "text": "Do not <w>damn</w> the entire project just because of one small mistake.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>осуждать</w> весь проект только из-за одной маленькой ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "condemn",
                        "criticize",
                        "censure"
                    ],
                    "antonyms": [
                        "praise",
                        "acclaim"
                    ],
                    "common_phrases": [
                        "damn with faint praise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко критиковать или признавать что-либо негодным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осуждать",
                                "target_lang_sense_clarification": "выражать неодобрение"
                            },
                            {
                                "target_lang_word": "порицать",
                                "target_lang_sense_clarification": "книжное, критиковать"
                            },
                            {
                                "target_lang_word": "критиковать",
                                "target_lang_sense_clarification": "давать негативную оценку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b67a7a8-48de-3245-a847-25b9c7425c83",
                    "sense_definition": "The smallest amount of concern, consideration, or value.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "negligible_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Frankly, my dear, I do not give a <w>damn</w> about your problems.",
                            "target_lang_translations": {
                                "ru": "Честно говоря, дорогая, мне <w>плевать</w> на твои проблемы."
                            }
                        },
                        {
                            "text": "This old broken radio is not worth a <w>damn</w> anymore.",
                            "target_lang_translations": {
                                "ru": "Это старое сломанное радио больше не стоит и <w>гроша</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jot",
                        "whit",
                        "rap",
                        "hoot"
                    ],
                    "common_phrases": [
                        "give a damn",
                        "worth a damn"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in negative idioms"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Малейшая доля беспокойства, внимания или ценности (обычно в отрицательных идиомах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плевать",
                                "target_lang_sense_clarification": "в значении «все равно» (give a damn)"
                            },
                            {
                                "target_lang_word": "грош",
                                "target_lang_sense_clarification": "в значении «ничего не стоит» (worth a damn)"
                            },
                            {
                                "target_lang_word": "чёрт",
                                "target_lang_sense_clarification": "в идиомах (ни черта)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
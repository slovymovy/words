    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5dd089df-be02-3783-bb1f-b0a8b63e1219",
                    "sense_definition": "Największe miasto Syrii, położone w północno-zachodniej części kraju, blisko granicy z Turcją.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "aleppo_geo",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W odpowiedzi na te działania terroryści zrzucają pociski na <w>Aleppo</w>.",
                            "target_lang_translations": {
                                "ru": "В ответ на эти действия террористы сбрасывают снаряды на <w>Алеппо</w>."
                            }
                        },
                        {
                            "text": "Zabytkowe stare miasto <w>Aleppo</w> zostało wpisane na listę światowego dziedzictwa UNESCO.",
                            "target_lang_translations": {
                                "ru": "Древний старый город <w>Алеппо</w> был внесен в список всемирного наследия ЮНЕСКО."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bitwa o Aleppo",
                        "starożytne Aleppo",
                        "mieszkańcy Aleppo",
                        "centrum Aleppo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупнейший город Сирии, расположенный на северо-западе страны, недалеко от границы с Турцией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Алеппо",
                                "target_lang_sense_clarification": "город"
                            },
                            {
                                "target_lang_word": "Халеб",
                                "target_lang_sense_clarification": "вариант названия города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b504dab5-3caa-37ac-9748-a97bab6701af",
                    "sense_definition": "Jednostka administracyjna (muhafaza) w Syrii ze stolicą w mieście o tej samej nazwie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "aleppo_geo",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Gubernator odwiedził zniszczone wioski leżące w prowincji <w>Aleppo</w>.",
                            "target_lang_translations": {
                                "ru": "Губернатор посетил разрушенные деревни, находящиеся в провинции <w>Алеппо</w>."
                            }
                        },
                        {
                            "text": "Walki toczyły się na terenie całej muhafazy <w>Aleppo</w>, zmuszając ludność do ucieczki.",
                            "target_lang_translations": {
                                "ru": "Бои шли на территории всей мухафазы <w>Алеппо</w>, вынуждая население бежать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prowincja Aleppo",
                        "muhafaza Aleppo",
                        "gubernatorstwo Aleppo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административная единица (мухафаза) в Сирии со столицей в одноименном городе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Алеппо",
                                "target_lang_sense_clarification": "мухафаза / провинция"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dba5c31d-8221-388d-8637-56ddc83ce9cd",
                    "sense_definition": "The killing of one person by another, whether premeditated or unintentional.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police charged the suspect with <w>homicide</w> after reviewing the evidence.",
                            "target_lang_translations": {
                                "ru": "Полиция предъявила подозреваемому обвинение в <w>убийстве</w> после изучения улик."
                            }
                        },
                        {
                            "text": "The city has seen a significant decrease in the <w>homicide</w> rate this year.",
                            "target_lang_translations": {
                                "ru": "В этом году в городе наблюдается значительное снижение уровня <w>убийств</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "killing",
                        "murder",
                        "manslaughter"
                    ],
                    "common_phrases": [
                        "commit homicide",
                        "homicide rate",
                        "justifiable homicide",
                        "negligent homicide",
                        "ruled a homicide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишение жизни одного человека другим, умышленное или непреднамеренное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убийство",
                                "target_lang_sense_clarification": "преступление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ec15ab1-d100-3f2e-bb02-8e9647d9d153",
                    "sense_definition": "The department within a police force that investigates cases of homicide.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has worked in <w>homicide</w> for over ten years.",
                            "target_lang_translations": {
                                "ru": "Она проработала в <w>убойном отделе</w> более десяти лет."
                            }
                        },
                        {
                            "text": "The case was immediately transferred to the <w>homicide</w> division.",
                            "target_lang_translations": {
                                "ru": "Дело было немедленно передано в отдел <w>по расследованию убийств</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "homicide detective",
                        "homicide division",
                        "chief of homicide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдел полиции, занимающийся расследованием убийств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убойный отдел",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "отдел по расследованию убийств",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c905dfb-80e3-3355-bddc-be92693f5b37",
                    "sense_definition": "A person who kills another human being.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>homicide</w> was eventually captured and brought to trial.",
                            "target_lang_translations": {
                                "ru": "<w>Убийца</w> был в конце концов пойман и предан суду."
                            }
                        },
                        {
                            "text": "In ancient times, a <w>homicide</w> was often banished from the tribe.",
                            "target_lang_translations": {
                                "ru": "В древние времена <w>убийцу</w> часто изгоняли из племени."
                            }
                        }
                    ],
                    "synonyms": [
                        "killer"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, совершивший убийство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убийца",
                                "target_lang_sense_clarification": "человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4757f6e3-3a9a-37f1-9552-e05f3e8d0f86",
                    "sense_definition": "A victim of homicide; a person who has been unlawfully killed.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective confirmed that the body found in the alley was a <w>homicide</w>.",
                            "target_lang_translations": {
                                "ru": "Детектив подтвердил, что тело, найденное в переулке, принадлежит <w>жертве убийства</w>."
                            }
                        },
                        {
                            "text": "We treated the unidentified woman as a <w>homicide</w> from the start of the investigation.",
                            "target_lang_translations": {
                                "ru": "Мы с самого начала расследования рассматривали дело неизвестной женщины как <w>убийство</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "murder victim"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "police jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жертва убийства; случай насильственной смерти (на полицейском жаргоне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жертва убийства",
                                "target_lang_sense_clarification": "погибший"
                            },
                            {
                                "target_lang_word": "убийство",
                                "target_lang_sense_clarification": "классификация случая"
                            }
                        ]
                    }
                }
            ]
        }
    ]

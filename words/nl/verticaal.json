{
    "word_family": [
        "verticaliteit",
        "verticalisme"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "12446f67-c0b4-3d1e-bb67-0d73de6bcaea",
                    "sense_definition": "Loodrecht op het vlak van de horizon; van boven naar beneden gericht.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de <w>verticale</w> as van de grafiek staan de temperaturen aangegeven.",
                            "target_lang_translations": {
                                "en": "The temperatures are indicated on the <w>vertical</w> axis of the graph.",
                                "ru": "На <w>вертикальной</w> оси графика указаны температуры."
                            }
                        },
                        {
                            "text": "Teken een <w>verticale</w> lijn van de bovenkant naar de onderkant van het papier.",
                            "target_lang_translations": {
                                "en": "Draw a <w>vertical</w> line from the top to the bottom of the paper.",
                                "ru": "Нарисуйте <w>вертикальную</w> линию от верхнего края бумаги к нижнему."
                            }
                        }
                    ],
                    "synonyms": [
                        "loodrecht"
                    ],
                    "antonyms": [
                        "horizontaal"
                    ],
                    "common_phrases": [
                        "verticale as",
                        "verticale lijn",
                        "verticale richting",
                        "verticale streep"
                    ],
                    "target_lang_definitions": {
                        "en": "Perpendicular to the plane of the horizon; directed from top to bottom.",
                        "ru": "Перпендикулярный плоскости горизонта; направленный сверху вниз."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vertical",
                                "target_lang_sense_clarification": "perpendicular to the horizon"
                            },
                            {
                                "target_lang_word": "upright",
                                "target_lang_sense_clarification": "standing straight up"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вертикальный",
                                "target_lang_sense_clarification": "расположенный под прямым углом к горизонту"
                            },
                            {
                                "target_lang_word": "отвесный",
                                "target_lang_sense_clarification": "очень крутой, почти вертикальный (о склонах, стенах)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8e7f3cad-ea2c-32e8-bccc-77dd900eea65",
                    "sense_definition": "In een richting die loodrecht op de horizon staat; recht omhoog of omlaag.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een misstap zou fataal zijn, omdat je dan <w>verticaal</w> naar beneden zou storten.",
                            "target_lang_translations": {
                                "en": "A misstep would be fatal, because you would then fall <w>vertically</w> down.",
                                "ru": "Оступиться было бы фатально, так как вы бы рухнули <w>вертикально</w> вниз."
                            }
                        },
                        {
                            "text": "De moderne straaljager kan <w>verticaal</w> opstijgen zonder een lange startbaan te gebruiken.",
                            "target_lang_translations": {
                                "en": "The modern fighter jet can take off <w>vertically</w> without using a long runway.",
                                "ru": "Современный истребитель может взлетать <w>вертикально</w>, не используя длинную взлетно-посадочную полосу."
                            }
                        }
                    ],
                    "synonyms": [
                        "loodrecht"
                    ],
                    "antonyms": [
                        "horizontaal"
                    ],
                    "common_phrases": [
                        "verticaal opstijgen",
                        "verticaal plaatsen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a direction perpendicular to the horizon; straight up or down.",
                        "ru": "В направлении, перпендикулярном горизонту; прямо вверх или вниз."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vertically",
                                "target_lang_sense_clarification": "in a vertical direction"
                            },
                            {
                                "target_lang_word": "straight up and down",
                                "target_lang_sense_clarification": "informal phrase"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вертикально",
                                "target_lang_sense_clarification": "направление движения или положения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
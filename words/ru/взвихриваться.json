{
    "word_family": [
        "вихрь",
        "вихриться",
        "взвихрить",
        "взвихрение"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1a824b48-b085-38b6-b0cd-c5f2341d7212",
                    "sense_definition": "Подниматься вверх и кружиться вихрем (о пыли, снеге, ветре и т. п.)",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дорожная пыль густо <w>взвихривается</w> за проезжающей машиной.",
                            "target_lang_translations": {
                                "en": "Road dust thickly <w>swirls up</w> behind the passing car."
                            }
                        },
                        {
                            "text": "Опавшие листья <w>взвихривались</w> на ветру и летели вдоль улицы.",
                            "target_lang_translations": {
                                "en": "Fallen leaves <w>whirled up</w> in the wind and flew along the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "вихриться",
                        "клубиться",
                        "кружиться"
                    ],
                    "antonyms": [
                        "оседать",
                        "укладываться"
                    ],
                    "common_phrases": [
                        "пыль взвихривается",
                        "снег взвихривается"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise up and whirl like a vortex (of dust, snow, wind, etc.)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whirl up",
                                "target_lang_sense_clarification": "movement of particles rising in the air"
                            },
                            {
                                "target_lang_word": "swirl up",
                                "target_lang_sense_clarification": "circular rising motion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a384a711-4919-37a8-9c36-eb02ff959672",
                    "sense_definition": "Становиться взлохмаченным, торчать в разные стороны (о волосах)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его непослушные волосы смешно <w>взвихривались</w> на макушке.",
                            "target_lang_translations": {
                                "en": "His unruly hair funnily <w>stuck up</w> on the crown of his head."
                            }
                        },
                        {
                            "text": "От сильного ветра причёска <w>взвихривалась</w> и теряла форму.",
                            "target_lang_translations": {
                                "en": "From the strong wind, the hairstyle <w>became disheveled</w> and lost its shape."
                            }
                        }
                    ],
                    "synonyms": [
                        "взлохмачиваться",
                        "ерошиться",
                        "растрёпываться"
                    ],
                    "antonyms": [
                        "приглаживаться"
                    ],
                    "common_phrases": [
                        "волосы взвихриваются"
                    ],
                    "target_lang_definitions": {
                        "en": "To become disheveled or stick out in different directions (of hair)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick up",
                                "target_lang_sense_clarification": "of hair standing on end"
                            },
                            {
                                "target_lang_word": "become disheveled",
                                "target_lang_sense_clarification": "losing neatness due to wind or movement"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
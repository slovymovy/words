{
    "word_family": [
        "sanktuaryjny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "662c994a-803f-3f58-b008-df30464cb0a4",
                    "sense_definition": "Otoczony czcią obiekt religijny, miejsce przechowywania relikwii lub cudownych wizerunków, często cel pielgrzymek.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "religious_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierni tłumnie przybywają do tego <w>sanktuarium</w> na coroczne uroczystości.",
                            "target_lang_translations": {
                                "ru": "Верующие толпами прибывают в это <w>святилище</w> на ежегодные торжества."
                            }
                        },
                        {
                            "text": "To słynne <w>sanktuarium</w> maryjne znajduje się na szczycie góry.",
                            "target_lang_translations": {
                                "ru": "Это знаменитое марианское <w>святилище</w> находится на вершине горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "świątynia"
                    ],
                    "common_phrases": [
                        "sanktuarium maryjne",
                        "sanktuarium narodowe",
                        "odwiedzić sanktuarium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почитаемый религиозный объект, место хранения реликвий или чудотворных изображений, часто являющееся целью паломничества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "святилище",
                                "target_lang_sense_clarification": "место поклонения"
                            },
                            {
                                "target_lang_word": "святыня",
                                "target_lang_sense_clarification": "почитаемое место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2503d493-04f8-3436-9d18-aee106944832",
                    "sense_definition": "Miejsce bezpieczne, dające schronienie przed zagrożeniem, problemami lub zgiełkiem świata; także w sensie przenośnym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "refuge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego dom stał się dla niego prawdziwym <w>sanktuarium</w> spokoju.",
                            "target_lang_translations": {
                                "ru": "Его дом стал для него настоящим <w>убежищем</w> спокойствия."
                            }
                        },
                        {
                            "text": "Biblioteka była jej <w>sanktuarium</w>, gdzie mogła uciec od hałasu miasta.",
                            "target_lang_translations": {
                                "ru": "Библиотека была её <w>убежищем</w>, где она могла укрыться от городского шума."
                            }
                        }
                    ],
                    "synonyms": [
                        "azyl"
                    ],
                    "common_phrases": [
                        "znaleźć sanktuarium",
                        "prywatne sanktuarium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Безопасное место, дающее укрытие от угроз, проблем или мирской суеты; также в переносном смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убежище",
                                "target_lang_sense_clarification": "безопасное место"
                            },
                            {
                                "target_lang_word": "пристанище",
                                "target_lang_sense_clarification": "укрытие, приют"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "469e9f09-c900-3f95-a0b0-e15493ea3f95",
                    "sense_definition": "Wydzielony obszar lub ośrodek zapewniający dożywotnią opiekę i ochronę zwierzętom, często uratowanym z trudnych warunków.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_shelter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uratowane z cyrku lwy trafiły do specjalnego <w>sanktuarium</w> w Afryce.",
                            "target_lang_translations": {
                                "ru": "Спасенные из цирка львы попали в специальный <w>заповедник</w> в Африке."
                            }
                        },
                        {
                            "text": "To <w>sanktuarium</w> przyrody chroni rzadkie gatunki ptaków przed wyginięciem.",
                            "target_lang_translations": {
                                "ru": "Этот природный <w>заповедник</w> защищает редкие виды птиц от вымирания."
                            }
                        }
                    ],
                    "synonyms": [
                        "azyl",
                        "schronisko"
                    ],
                    "common_phrases": [
                        "sanktuarium przyrody",
                        "sanktuarium dla zwierząt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделенная территория или центр, обеспечивающий пожизненный уход и защиту животным, часто спасенным из трудных условий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заповедник",
                                "target_lang_sense_clarification": "охраняемая природная территория"
                            },
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "для животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
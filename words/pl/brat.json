{
    "word_family": [
        "braterstwo",
        "braterski",
        "zbratać się",
        "bratowa",
        "bratanek",
        "bratanica"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "576a72cb-38f4-31c8-b370-7400b943bbd3",
                    "sense_definition": "Mężczyzna lub chłopiec mający tych samych rodziców (lub jednego z nich) co inna osoba.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy <brat> uczy mnie grać w piłkę nożną.",
                            "target_lang_translations": {
                                "ru": "Мой старший <брат> учит меня играть в футбол."
                            }
                        },
                        {
                            "text": "Ona ma przyrodniego <brata>, który mieszka za granicą.",
                            "target_lang_translations": {
                                "ru": "У неё есть сводный <брат>, который живёт за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "braciszek",
                        "brachol",
                        "braciak"
                    ],
                    "antonyms": [
                        "siostra"
                    ],
                    "common_phrases": [
                        "starszy brat",
                        "młodszy brat",
                        "brat przyrodni",
                        "brat bliźniak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина или мальчик, имеющий тех же родителей (или одного из них), что и другой человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "родственник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c34b0ac-b919-3197-8c89-ee48b4ed776d",
                    "sense_definition": "Mężczyzna bliski komuś ze względu na przyjaźń, wspólne cele lub przynależność do tej samej grupy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "friend_associate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesteś dla mnie jak <brat>, mogę ci o wszystkim powiedzieć.",
                            "target_lang_translations": {
                                "ru": "Ты для меня как <брат>, я могу тебе обо всём рассказать."
                            }
                        },
                        {
                            "text": "W trudnych chwilach poznał, kto jest jego prawdziwym <bratem>, a kto wrogiem.",
                            "target_lang_translations": {
                                "ru": "В трудные минуты он узнал, кто его настоящий <брат>, а кто враг."
                            }
                        }
                    ],
                    "antonyms": [
                        "wróg"
                    ],
                    "common_phrases": [
                        "brat łata",
                        "za pan brat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, близкий кому-либо в силу дружбы, общих целей или принадлежности к той же группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "друг, близкий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2bb7587-66da-3f70-8c8e-75f3fff8b82f",
                    "sense_definition": "Członek męskiego zgromadzenia zakonnego, który zazwyczaj nie posiada święceń kapłańskich.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pokorny <brat> zakonny pracował w ogrodzie od wschodu słońca.",
                            "target_lang_translations": {
                                "ru": "Смиренный <брат> работал в саду с восхода солнца."
                            }
                        },
                        {
                            "text": "Spotkaliśmy <brata> Jana przy bramie klasztoru.",
                            "target_lang_translations": {
                                "ru": "Мы встретили <брата> Яна у ворот монастыря."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakonnik",
                        "frater"
                    ],
                    "antonyms": [
                        "siostra",
                        "ojciec"
                    ],
                    "common_phrases": [
                        "brat zakonny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член мужского монашеского ордена, обычно не имеющий сана священника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "монах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc520b44-05ab-3f44-af93-6e0ee387a8d3",
                    "sense_definition": "Dawne, honorowe określenie innego szlachcica używane dla podkreślenia równości stanu.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_noble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Witaj, mości <bracie>, napijmy się wina za pomyślność ojczyzny!",
                            "target_lang_translations": {
                                "ru": "Здравствуй, милостивый <брат>, выпьем вина за благополучие родины!"
                            }
                        },
                        {
                            "text": "Każdy szlachcic na sejmiku był dla drugiego panem <bratem>.",
                            "target_lang_translations": {
                                "ru": "Каждый шляхтич на сеймике был для другого паном <братом>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pan brat"
                    ],
                    "common_phrases": [
                        "pan brat",
                        "szlachta-bracia"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane w dawnej Polsce w odniesieniu do szlachty"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное, почетное именование другого дворянина, используемое для подчеркивания сословного равенства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брат",
                                "target_lang_sense_clarification": "обращение к дворянину"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
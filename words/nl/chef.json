            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b6003e98-f705-30c2-b0ad-42801e50b2d3",
                    "sense_definition": "Iemand die de leiding heeft over een groep werknemers of een afdeling.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>chef</w> heeft me toestemming gegeven om vandaag eerder naar huis te gaan.",
                            "target_lang_translations": {
                                "en": "My <w>boss</w> gave me permission to go home early today.",
                                "ru": "Мой <w>начальник</w> разрешил мне сегодня уйти домой пораньше."
                            }
                        },
                        {
                            "text": "Hij is onlangs gepromoveerd tot <w>chef</w> van de afdeling verkoop.",
                            "target_lang_translations": {
                                "en": "He was recently promoted to <w>head</w> of the sales department.",
                                "ru": "Его недавно повысили до <w>начальника</w> отдела продаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "baas"
                    ],
                    "common_phrases": [
                        "mijn directe chef",
                        "chef van de afdeling"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is in charge of a group of employees or a department.",
                        "ru": "Человек, который руководит группой сотрудников или отделом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "general term for a person in charge"
                            },
                            {
                                "target_lang_word": "manager",
                                "target_lang_sense_clarification": "person responsible for administering a department or group"
                            },
                            {
                                "target_lang_word": "head",
                                "target_lang_sense_clarification": "person leading a specific department or unit"
                            },
                            {
                                "target_lang_word": "chief",
                                "target_lang_sense_clarification": "leader of a specific group or rank"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "руководитель"
                            },
                            {
                                "target_lang_word": "шеф",
                                "target_lang_sense_clarification": "разговорное название начальника"
                            },
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ce52a3d-7dcd-3633-b60c-931f38079f31",
                    "sense_definition": "De belangrijkste kok in een restaurant die de leiding heeft over de keuken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>chef</w> heeft een speciaal menu samengesteld voor de feestdagen.",
                            "target_lang_translations": {
                                "en": "The <w>chef</w> has put together a special menu for the holidays.",
                                "ru": "<w>Шеф-повар</w> составил специальное меню к праздникам."
                            }
                        },
                        {
                            "text": "Als <w>chef</w> is hij verantwoordelijk voor de kwaliteit van alle gerechten die de keuken verlaten.",
                            "target_lang_translations": {
                                "en": "As <w>chef</w>, he is responsible for the quality of all dishes leaving the kitchen.",
                                "ru": "Как <w>шеф-повар</w>, он отвечает за качество всех блюд, покидающих кухню."
                            }
                        }
                    ],
                    "synonyms": [
                        "chef-kok"
                    ],
                    "common_phrases": [
                        "de chef in de keuken",
                        "chef de cuisine"
                    ],
                    "target_lang_definitions": {
                        "en": "The main cook in a restaurant who is in charge of the kitchen.",
                        "ru": "Главный повар в ресторане, отвечающий за работу кухни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chef",
                                "target_lang_sense_clarification": "professional cook, especially the head cook"
                            },
                            {
                                "target_lang_word": "head chef",
                                "target_lang_sense_clarification": "the person in charge of a professional kitchen"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шеф-повар",
                                "target_lang_sense_clarification": "главный повар"
                            },
                            {
                                "target_lang_word": "шеф",
                                "target_lang_sense_clarification": "краткая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba66c956-b9b4-39e5-8045-bdf6ef28ffd3",
                    "sense_definition": "Een informele manier om een man aan te spreken, vaak een onbekende of iemand in een dienstverlenend beroep.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hé <w>chef</w>, weet jij misschien hoe laat de laatste bus vertrekt?",
                            "target_lang_translations": {
                                "en": "Hey <w>chief</w>, do you happen to know what time the last bus leaves?",
                                "ru": "Эй, <w>шеф</w>, не знаешь, во сколько уходит последний автобус?"
                            }
                        },
                        {
                            "text": "Bedankt <w>chef</w>, dat is precies wat ik nodig had.",
                            "target_lang_translations": {
                                "en": "Thanks <w>boss</w>, that is exactly what I needed.",
                                "ru": "Спасибо, <w>шеф</w>, это именно то, что мне было нужно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hé chef",
                        "zeg chef"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An informal way to address a man, often a stranger or someone providing a service.",
                        "ru": "Неформальное обращение к мужчине, часто к незнакомцу или работнику сферы услуг."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chief",
                                "target_lang_sense_clarification": "informal term of address"
                            },
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "informal, friendly term of address"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шеф",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "разговорное обращение"
                            },
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "слэнг"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a28a7f93-3231-3818-856f-044c42b60661",
                    "sense_definition": "Nieudane uderzenie lub kopnięcie piłki w sporcie, niezgodne z zamiarem zawodnika.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obrońca popełnił <w>kiks</w>, przez co przeciwnicy zdobyli łatwego gola.",
                            "target_lang_translations": {
                                "ru": "Защитник допустил <w>кикс</w>, из-за чего противники забили легкий гол."
                            }
                        },
                        {
                            "text": "Ten niefortunny <w>kiks</w> przekreślił szanse drużyny na zwycięstwo w meczu.",
                            "target_lang_translations": {
                                "ru": "Этот досадный <w>кикс</w> перечеркнул шансы команды на победу в матче."
                            }
                        }
                    ],
                    "synonyms": [
                        "pudło",
                        "skucha"
                    ],
                    "common_phrases": [
                        "kiks obrońcy",
                        "kiks bramkarza",
                        "fatalny kiks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неудачный удар по мячу в спорте, когда он срывается с ноги или летит не туда, куда планировал игрок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кикс",
                                "target_lang_sense_clarification": "срезка мяча, неудачный удар"
                            },
                            {
                                "target_lang_word": "срезка",
                                "target_lang_sense_clarification": "удар по касательной"
                            },
                            {
                                "target_lang_word": "ляп",
                                "target_lang_sense_clarification": "грубая ошибка (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05676551-75b0-3168-a997-b669cde7cf68",
                    "sense_definition": "Niestosowne, nieoczekiwane lub błędne zachowanie, często popełnione w sytuacji towarzyskiej.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior_mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego niestosowny żart okazał się wielkim <w>kiksem</w> podczas oficjalnej kolacji.",
                            "target_lang_translations": {
                                "ru": "Его неуместная шутка оказалась большим <w>ляпом</w> во время официального ужина."
                            }
                        },
                        {
                            "text": "Polityk zaliczył wizerunkowy <w>kiks</w>, myląc nazwiska ważnych gości.",
                            "target_lang_translations": {
                                "ru": "Политик допустил имиджевый <w>промах</w>, перепутав фамилии важных гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "gafa",
                        "wpadka",
                        "nietakt",
                        "pomyłka"
                    ],
                    "common_phrases": [
                        "towarzyski kiks",
                        "popełnić kiks",
                        "zaliczyć kiks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неуместное действие, оплошность или бестактность, допущенная в социальной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оплошность",
                                "target_lang_sense_clarification": "ошибка в поведении"
                            },
                            {
                                "target_lang_word": "ляп",
                                "target_lang_sense_clarification": "нелепая ошибка, промах"
                            },
                            {
                                "target_lang_word": "промах",
                                "target_lang_sense_clarification": "неудача, ошибка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96930c60-8e19-36eb-ba76-4391cbf23929",
                    "sense_definition": "Fałszywy, nieczysty dźwięk wydobyty z instrumentu lub głosu podczas występu muzycznego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas solówki saksofonisty zdarzył się wyraźny <w>kiks</w>, który zauważyła publiczność.",
                            "target_lang_translations": {
                                "ru": "Во время соло саксофониста случился явный <w>кикс</w>, который заметила публика."
                            }
                        },
                        {
                            "text": "Młody muzyk bardzo przeżywał każdy <w>kiks</w> zrobiony w trakcie koncertu.",
                            "target_lang_translations": {
                                "ru": "Молодой музыкант очень переживал за каждый <w>кикс</w>, сделанный во время концерта."
                            }
                        }
                    ],
                    "synonyms": [
                        "fałsz"
                    ],
                    "common_phrases": [
                        "słyszalny kiks",
                        "kiks trębacza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Срыв звука, фальшивая нота при игре на духовом инструменте или пении."
                    },
                    "translations": {

{
    "word_family": [
        "information",
        "informative",
        "informant",
        "informer",
        "misinform",
        "disinformation"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "82b98c71-e57a-3d3f-ae60-24bb1b332e4c",
                    "sense_definition": "To give information, facts, or knowledge to someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communicate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>inform</w> us immediately if your contact details change.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, немедленно <w>сообщите</w> нам, если ваши контактные данные изменятся."
                            }
                        },
                        {
                            "text": "The police <w>informed</w> the family about the accident late last night.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>сообщила</w> семье о происшествии поздно ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "tell",
                        "notify",
                        "let know",
                        "advise",
                        "apprise",
                        "acquaint"
                    ],
                    "antonyms": [
                        "misinform",
                        "withhold"
                    ],
                    "common_phrases": [
                        "inform someone of",
                        "inform someone about",
                        "keep informed",
                        "please inform"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать информацию, факты или знания кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "передавать сведения"
                            },
                            {
                                "target_lang_word": "информировать",
                                "target_lang_sense_clarification": "предоставлять информацию"
                            },
                            {
                                "target_lang_word": "уведомлять",
                                "target_lang_sense_clarification": "официально извещать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90d8b465-e7a6-3be7-8d1f-30604e378524",
                    "sense_definition": "To influence something such as an opinion, attitude, or style; to be the essential quality of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her experience as a teacher <w>informs</w> her approach to parenting.",
                            "target_lang_translations": {
                                "ru": "Ее опыт учителя <w>определяет</w> ее подход к воспитанию детей."
                            }
                        },
                        {
                            "text": "A strong sense of social justice <w>informs</w> all of his writing.",
                            "target_lang_translations": {
                                "ru": "Сильное чувство социальной справедливости <w>пронизывает</w> все его произведения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inform the decision",
                        "inform the process",
                        "inform the approach"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Влиять на что-либо (мнение, отношение или стиль); определять характер чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влиять",
                                "target_lang_sense_clarification": "оказывать воздействие"
                            },
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "обуславливать, формировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16009c8c-336e-3bd5-974a-c18db39b7e1e",
                    "sense_definition": "To officially report someone's illegal or wrong activities to an authority.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "denounce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was afraid that his neighbors might <w>inform</w> on him to the police.",
                            "target_lang_translations": {
                                "ru": "Он боялся, что соседи могут <w>донести</w> на него в полицию."
                            }
                        },
                        {
                            "text": "The gang member refused to <w>inform</w> against his accomplices.",
                            "target_lang_translations": {
                                "ru": "Член банды отказался <w>доносить</w> на своих сообщников."
                            }
                        }
                    ],
                    "synonyms": [
                        "snitch",
                        "dob"
                    ],
                    "common_phrases": [
                        "inform on",
                        "inform against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально сообщать властям о чьих-либо незаконных действиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доносить",
                                "target_lang_sense_clarification": "сообщать о нарушениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
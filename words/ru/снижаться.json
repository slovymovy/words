    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ebeaf2e0-d1a2-3e4a-87d3-d994f8d7c063",
                    "sense_definition": "Перемещаться вниз, на меньшую высоту (обычно о летящих объектах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Самолёт начал плавно <снижаться> перед посадкой в аэропорту.",
                            "target_lang_translations": {
                                "en": "The plane began to smoothly <descend> before landing at the airport."
                            }
                        },
                        {
                            "text": "Орёл сложил крылья и стал стремительно <снижаться> к земле.",
                            "target_lang_translations": {
                                "en": "The eagle folded its wings and began to rapidly <descend> towards the ground."
                            }
                        }
                    ],
                    "synonyms": [
                        "опускаться",
                        "спускаться"
                    ],
                    "antonyms": [
                        "подниматься"
                    ],
                    "common_phrases": [
                        "плавно снижаться",
                        "начать снижаться",
                        "самолёт снижается",
                        "стремительно снижаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To move downwards, to a lower altitude (usually about flying objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "to move from a higher to a lower place"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "to move to a lower level or position"
                            },
                            {
                                "target_lang_word": "come down",
                                "target_lang_sense_clarification": "to move downwards towards the ground"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74a84638-de30-3f69-b0a2-500cfc6ac3f6",
                    "sense_definition": "Становиться меньше по количеству, уровню, силе или степени.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "decrease_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К вечеру температура воздуха обычно <снижается> на несколько градусов.",
                            "target_lang_translations": {
                                "en": "By evening, the air temperature usually <goes down> by a few degrees."
                            }
                        },
                        {
                            "text": "Врач отметил, что давление у пациента постепенно <снижается>.",
                            "target_lang_translations": {
                                "en": "The doctor noted that the patient's blood pressure was gradually <decreasing>."
                            }
                        }
                    ],
                    "synonyms": [
                        "уменьшаться",
                        "понижаться",
                        "сокращаться",
                        "опускаться"
                    ],
                    "antonyms": [
                        "повышаться",
                        "увеличиваться",
                        "расти",
                        "нарастать"
                    ],
                    "common_phrases": [
                        "температура снижается",
                        "цены снижаются",
                        "риск снижается",
                        "скорость снижается",
                        "постепенно снижаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To become less in quantity, level, strength, or degree."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decrease",
                                "target_lang_sense_clarification": "to become smaller or fewer in size, amount, or degree"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "to reduce in value or amount"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "to fall in amount, value, or strength"
                            },
                            {
                                "target_lang_word": "decline",
                                "target_lang_sense_clarification": "to become smaller, fewer, or less"
                            }
                        ]
                    }
                }
            ]
        }
    ]

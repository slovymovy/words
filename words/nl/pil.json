    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f0590016-fb95-39de-b9c0-181c48e96937",
                    "sense_definition": "Een medicijn in de vorm van een klein bolletje of schijfje dat je met water moet doorslikken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medicine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter heeft mij een <w>pil</w> voorgeschreven tegen de hoofdpijn.",
                            "target_lang_translations": {
                                "en": "The doctor prescribed me a <w>pill</w> for the headache.",
                                "ru": "Доктор прописал мне <w>таблетку</w> от головной боли."
                            }
                        },
                        {
                            "text": "Mijn opa moet elke ochtend een <w>pil</w> nemen voor zijn hart.",
                            "target_lang_translations": {
                                "en": "My grandpa has to take a <w>pill</w> for his heart every morning.",
                                "ru": "Мой дедушка должен каждое утро принимать <w>таблетку</w> для сердца."
                            }
                        }
                    ],
                    "synonyms": [
                        "tablet"
                    ],
                    "common_phrases": [
                        "een pil slikken",
                        "een pilletje nemen"
                    ],
                    "target_lang_definitions": {
                        "en": "A medicine in the form of a small ball or disc that you must swallow with water.",
                        "ru": "Лекарство в виде маленького шарика или диска, которое нужно проглотить, запивая водой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pill",
                                "target_lang_sense_clarification": "general medication"
                            },
                            {
                                "target_lang_word": "tablet",
                                "target_lang_sense_clarification": "solid medication"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "таблетка",
                                "target_lang_sense_clarification": "самый распространенный перевод"
                            },
                            {
                                "target_lang_word": "пилюля",
                                "target_lang_sense_clarification": "лекарственная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cd25796-57bd-3aa2-8862-4a8460f77512",
                    "sense_definition": "Het voorbehoedsmiddel in tabletvorm dat vrouwen dagelijks slikken om zwangerschap te voorkomen (meestal aangeduid als 'de pil').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medicine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is vorige maand gestopt met de <w>pil</w> omdat ze graag zwanger wil worden.",
                            "target_lang_translations": {
                                "en": "She stopped taking the <w>pill</w> last month because she wants to get pregnant.",
                                "ru": "В прошлом месяце она перестала принимать <w>противозачаточные</w>, потому что хочет забеременеть."
                            }
                        },
                        {
                            "text": "Veel jonge vrouwen gebruiken de <w>pil</w> als betrouwbaar anticonceptiemiddel.",
                            "target_lang_translations": {
                                "en": "Many young women use the <w>pill</w> as a reliable contraceptive.",
                                "ru": "Многие молодые женщины используют <w>противозачаточные таблетки</w> как надежное средство контрацепции."
                            }
                        }
                    ],
                    "synonyms": [
                        "anticonceptiepil"
                    ],
                    "common_phrases": [
                        "aan de pil zijn",
                        "met de pil stoppen"
                    ],
                    "target_lang_definitions": {
                        "en": "The contraceptive in tablet form that women take daily to prevent pregnancy (usually referred to as 'the pill').",
                        "ru": "Противозачаточное средство в форме таблеток, которое женщины принимают ежедневно (обычно называемое «пилюлей»)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the pill",
                                "target_lang_sense_clarification": "contraceptive"
                            },
                            {
                                "target_lang_word": "birth control pill",
                                "target_lang_sense_clarification": "contraceptive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "противозачаточные таблетки",
                                "target_lang_sense_clarification": "средство контрацепции"
                            },
                            {
                                "target_lang_word": "оральные контрацептивы",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eae83302-6c19-3a83-a0dc-3abcc96eff6a",
                    "sense_definition": "Een grote teleurstelling of vervelende ervaring die men moet verwerken.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het verlies van de belangrijke wedstrijd was een bittere <w>pil</w> voor het hele team.",
                            "target_lang_translations": {
                                "en": "The loss of the important match was a bitter <w>pill</w> for the whole team.",
                                "ru": "Проигрыш в важном матче стал горькой <w>пилюлей</w> для всей команды."
                            }
                        },
                        {
                            "text": "Dat hij niet werd aangenomen voor de baan was een zware <w>pil</w> om te slikken.",
                            "target_lang_translations": {
                                "en": "That he wasn't hired for the job was a hard <w>pill</w> to swallow.",
                                "ru": "То, что его не взяли на работу, было тяжелой <w>пилюлей</w>, которую пришлось проглотить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een bittere pil",
                        "de pil vergulden"
                    ],
                    "target_lang_definitions": {
                        "en": "A major disappointment or unpleasant experience that one has to process.",
                        "ru": "Большое разочарование или неприятный опыт, который нужно пережить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bitter pill",
                                "target_lang_sense_clarification": "idiom for disappointment"
                            },
                            {
                                "target_lang_word": "hard pill to swallow",
                                "target_lang_sense_clarification": "idiom for difficult truth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пилюля",
                                "target_lang_sense_clarification": "в выражении «горькая пилюля»"

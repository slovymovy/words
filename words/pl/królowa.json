    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea5fc36b-4f05-34c1-8565-13a82d581c73",
                    "sense_definition": "Kobieta sprawująca najwyższą władzę w królestwie jako monarchini.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "monarch_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecnie w Wielkiej Brytanii panuje <królowa>.",
                            "target_lang_translations": {
                                "ru": "В настоящее время в Великобритании правит <w>королева</w>."
                            }
                        },
                        {
                            "text": "Młoda <królowa> szybko zyskała szacunek swoich poddanych.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>королева</w> быстро завоевала уважение своих подданных."
                            }
                        }
                    ],
                    "antonyms": [
                        "król"
                    ],
                    "common_phrases": [
                        "królowa angielska",
                        "królowa Polski",
                        "panująca królowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, обладающая высшей властью в королевстве как монарх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королева",
                                "target_lang_sense_clarification": "правящий монарх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f09adb4-4d93-333f-ac1e-79bb526eae8c",
                    "sense_definition": "Żona panującego króla, nosząca tytuł królewski, ale niekoniecznie sprawująca władzę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "monarch_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Królowa>, chociaż nie jest głową państwa, ma mnóstwo obowiązków reprezentacyjnych.",
                            "target_lang_translations": {
                                "ru": "<w>Королева</w>, хотя и не является главой государства, имеет множество представительских обязанностей."
                            }
                        },
                        {
                            "text": "Podczas uroczystości król i <królowa> siedzieli na złotych tronach.",
                            "target_lang_translations": {
                                "ru": "Во время церемонии король и <w>королева</w> сидели на золотых тронах."
                            }
                        }
                    ],
                    "antonyms": [
                        "król"
                    ],
                    "common_phrases": [
                        "królowa małżonka",
                        "królowa matka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жена правящего короля, носящая королевский титул, но не обязательно обладающая властью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королева",
                                "target_lang_sense_clarification": "супруга короля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b64b14f9-4a9e-3c54-b0fd-188110ceb22f",
                    "sense_definition": "Najsilniejsza figura w szachach, mogąca poruszać się o dowolną liczbę pól w pionie, poziomie i na ukos.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "chess_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biała <królowa> zbiła czarnego konia w piątym ruchu.",
                            "target_lang_translations": {
                                "ru": "Белый <w>ферзь</w> побил черного коня на пятом ходу."
                            }
                        },
                        {
                            "text": "Musisz uważać, aby nie stracić swojej <królowej> na początku partii.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, чтобы не потерять своего <w>ферзя</w> в начале партии."
                            }
                        }
                    ],
                    "synonyms": [
                        "hetman",
                        "dama"
                    ],
                    "common_phrases": [
                        "biała królowa",
                        "czarna królowa",
                        "ruch królową"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая сильная фигура в шахматах, способная ходить на любое число полей по вертикали, горизонтали и диагонали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ферзь",
                                "target_lang_sense_clarification": "шахматная фигура"
                            },
                            {
                                "target_lang_word": "королева",
                                "target_lang_sense_clarification": "разговорное название фигуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae5fc3ab-ff72-3b50-9aa2-6c02de1def6e",
                    "sense_definition": "Karta do gry z wizerunkiem kobiety, starsza od waleta i młodsza od króla.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "playing_card",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ostatnim rozdaniu dostałem <królową> kier i asa.",
                            "target_lang_translations": {
                                "ru": "В последней раздаче мне пришли <w>дама</w> червей и туз."
                            }
                        },
                        {
                            "text": "Ta <królowa> nie pasuje do mojego strita.",
                            "target_lang_translations": {
                                "ru": "Эта <w>дама</w> не подходит к моему стриту."
                            }
                        }
                    ],
                    "synonyms": [
                        "dama"
                    ],
                    "common_phrases": [
                        "królowa kier",
                        "królowa pik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игральная карта с изображением женщины, старше валета и младше короля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дама",
                                "target_lang_sense_clarification": "игральная карта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f8f6059-ff74-3b78-a56f-995623e30727",
                    "sense_definition": "Płodna samica u owadów żyjących w zorganizowanych społecznościach, np. u pszczół lub mrówek.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "insect_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jednym kopcu mrówek mieszka czasem kilka <królowych>.",
                            "target_lang_translations": {
                                "ru": "В одном муравейнике иногда живет несколько <w>маток</w>."
                            }
                        },
                        {
                            "text": "Pszczela <królowa> jest znacznie większa od robotnic.",
                            "target_lang_translations": {
                                "ru": "Пчелиная <w>матка</w> значительно больше рабочих пчел."
                            }
                        }
                    ],
                    "synonyms": [
                        "matka"
                    ],
                    "antonyms": [
                        "robotnica"
                    ],
                    "common_phrases": [
                        "królowa pszczół",
                        "królowa mrówek",
                        "królowa termitów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плодовитая самка у насекомых, живущих организованными сообществами, например, у пчел или муравьев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матка",
                                "target_lang_sense_clarification": "насекомое"
                            },
                            {
                                "target_lang_word": "королева",
                                "target_lang_sense_clarification": "калька, реже используется"
                            }
                        ]
                    }
                }
            ]
        },
        {

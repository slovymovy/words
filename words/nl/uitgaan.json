                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go out to",
                                "target_lang_sense_clarification": "sympathy/thoughts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть направленным",
                                "target_lang_sense_clarification": "на кого-то или что-то"

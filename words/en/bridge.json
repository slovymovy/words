{
    "word_family": [
        "bridgeable",
        "unbridgeable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3bea20b-0ed8-3016-bc49-f0c22a99f711",
                    "sense_definition": "A structure built over a river, road, or railway to allow people or vehicles to cross from one side to the other.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train slowly crossed the iron <w>bridge</w>.",
                            "target_lang_translations": {
                                "ru": "Поезд медленно переезжал через железный <w>мост</w>."
                            }
                        },
                        {
                            "text": "Heavy rains washed away the old wooden <w>bridge</w> near the village.",
                            "target_lang_translations": {
                                "ru": "Сильные ливни смыли старый деревянный <w>мост</w> возле деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "span",
                        "viaduct",
                        "overpass"
                    ],
                    "common_phrases": [
                        "cross the bridge",
                        "suspension bridge",
                        "build a bridge",
                        "under the bridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сооружение, возведенное над рекой, дорогой или железнодорожными путями, позволяющее людям или транспортным средствам переправляться с одной стороны на другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мост",
                                "target_lang_sense_clarification": "сооружение для перехода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76de5d3e-1f86-3dfe-b8bc-94a42ffde0de",
                    "sense_definition": "Something that provides a connection or link between different things, groups, or periods of time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Education serves as a <w>bridge</w> to a better future.",
                            "target_lang_translations": {
                                "ru": "Образование служит <w>мостом</w> в лучшее будущее."
                            }
                        },
                        {
                            "text": "This agreement acts as a <w>bridge</w> between the two rival nations.",
                            "target_lang_translations": {
                                "ru": "Это соглашение выступает <w>мостом</w> между двумя враждующими нациями."
                            }
                        }
                    ],
                    "synonyms": [
                        "link",
                        "connection",
                        "bond",
                        "tie"
                    ],
                    "antonyms": [
                        "barrier",
                        "division",
                        "separation",
                        "wall"
                    ],
                    "common_phrases": [
                        "bridge between"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечто, обеспечивающее связь или соединение между различными вещами, группами или периодами времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мост",
                                "target_lang_sense_clarification": "связующее звено (перен.)"
                            },
                            {
                                "target_lang_word": "связующее звено",
                                "target_lang_sense_clarification": "соединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb896910-c602-384c-b6a4-b426adc14ae3",
                    "sense_definition": "A trick-taking card game played by four people in two partnerships.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandparents play <w>bridge</w> with their neighbors every Tuesday.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка играют в <w>бридж</w> с соседями каждый вторник."
                            }
                        },
                        {
                            "text": "She is learning the complex bidding rules of contract <w>bridge</w>.",
                            "target_lang_translations": {
                                "ru": "Она изучает сложные правила торговли в контрактном <w>бридже</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play bridge",
                        "game of bridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Карточная игра со взятками, в которую играют четыре человека, разбившись на две пары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бридж",
                                "target_lang_sense_clarification": "карточная игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41bce738-bd14-327a-ab4d-52218220ef45",
                    "sense_definition": "The elevated platform on a ship from which the captain and officers direct operations.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain stood on the <w>bridge</w> scanning the horizon for land.",
                            "target_lang_translations": {
                                "ru": "Капитан стоял на <w>мостике</w>, вглядываясь в горизонт в поисках земли."
                            }
                        },
                        {
                            "text": "Orders were sent down from the <w>bridge</w> to the engine room.",
                            "target_lang_translations": {
                                "ru": "Приказы были переданы с <w>мостика</w> в машинное отделение."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышенная платформа на корабле, с которой капитан и офицеры управляют судном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мостик",
                                "target_lang_sense_clarification": "площадка на судне"
                            },
                            {
                                "target_lang_word": "капитанский мостик",
                                "target_lang_sense_clarification": "пункт управления судном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdfad243-5073-3e06-9018-1623224b464d",
                    "sense_definition": "The upper, bony part of the human nose located between the eyes.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She pushed her glasses back up the <w>bridge</w> of her nose.",
                            "target_lang_translations": {
                                "ru": "Она поправила очки на <w>переносице</w>."
                            }
                        },
                        {
                            "text": "The boxer suffered a deep cut on the <w>bridge</w> of his nose.",
                            "target_lang_translations": {
                                "ru": "Боксер получил глубокий порез на <w>переносице</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bridge of the nose"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя костная часть носа человека, расположенная между глазами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переносица",
                                "target_lang_sense_clarification": "часть носа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c7e9409-91bd-3702-8c79-a347afa69002",
                    "sense_definition": "A section of a song that differs in melody and lyrics from the verse and chorus, usually appearing before the final chorus.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_bridge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The song features a melodic <w>bridge</w> that builds tension before the end.",
                            "target_lang_translations": {
                                "ru": "В песне есть мелодичный <w>бридж</w>, нагнетающий напряжение перед финалом."
                            }
                        },
                        {
                            "text": "The singer wrote a powerful <w>bridge</w> to lift the energy of the track.",
                            "target_lang_translations": {
                                "ru": "Певец написал мощный <w>бридж</w>, чтобы поднять энергетику трека."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть песни, отличающаяся мелодией и текстом от куплета и припева, обычно звучащая перед финальным припевом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бридж",
                                "target_lang_sense_clarification": "музыкальный термин"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "часть музыкальной композиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30f05957-687a-3223-a255-24c34b462ccd",
                    "sense_definition": "A device or software that connects two or more separate computer network segments.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The technician installed a <w>bridge</w> to link the wireless and wired networks.",
                            "target_lang_translations": {
                                "ru": "Техник установил <w>мост</w> для соединения беспроводной и проводной сетей."
                            }
                        },
                        {
                            "text": "Data packets are forwarded across the <w>bridge</w> to the correct destination.",
                            "target_lang_translations": {
                                "ru": "Пакеты данных пересылаются через <w>мост</w> к нужному адресату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "network bridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или программное обеспечение, соединяющее два или более отдельных сегмента компьютерной сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мост",
                                "target_lang_sense_clarification": "сетевое устройство"
                            },
                            {
                                "target_lang_word": "сетевой мост",
                                "target_lang_sense_clarification": "термин IT"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74debfba-5a0c-3216-99a4-8ab80a203463",
                    "sense_definition": "A fixed dental restoration used to replace one or more missing teeth by joining an artificial tooth to adjacent teeth.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dentist recommended a <w>bridge</w> to fill the gap left by the extraction.",
                            "target_lang_translations": {
                                "ru": "Стоматолог порекомендовал <w>мост</w>, чтобы заполнить промежуток после удаления зуба."
                            }
                        },
                        {
                            "text": "He has to be careful when flossing around his dental <w>bridge</w>.",
                            "target_lang_translations": {
                                "ru": "Ему нужно быть осторожным при чистке зубной нитью вокруг зубного <w>моста</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dental bridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несъемный зубной протез, используемый для замещения одного или нескольких отсутствующих зубов путем прикрепления искусственного зуба к соседним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мост",
                                "target_lang_sense_clarification": "зубной протез"
                            },
                            {
                                "target_lang_word": "мостовидный протез",
                                "target_lang_sense_clarification": "стоматология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d915867-cdf4-34a8-bdd2-5a313177956b",
                    "sense_definition": "A small piece of wood on a string instrument that supports the strings and transmits vibrations to the body.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_bridge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>bridge</w> on his violin snapped during the concert.",
                            "target_lang_translations": {
                                "ru": "<w>Подставка</w> на его скрипке сломалась во время концерта."
                            }
                        },
                        {
                            "text": "Adjusting the position of the <w>bridge</w> can change the instrument's tone.",
                            "target_lang_translations": {
                                "ru": "Регулировка положения <w>подставки</w> может изменить тембр инструмента."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая деревянная деталь на струнном инструменте, которая поддерживает струны и передает вибрации корпусу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подставка",
                                "target_lang_sense_clarification": "деталь инструмента"
                            },
                            {
                                "target_lang_word": "кобылка",
                                "target_lang_sense_clarification": "разг. подставка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8653b851-e87f-3d6e-a6b3-0a43311db113",
                    "sense_definition": "To reduce or eliminate differences, gaps, or divisions between things, such as ideas, groups, or time periods.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must find a way to <w>bridge</w> the gap between the rich and the poor.",
                            "target_lang_translations": {
                                "ru": "Мы должны найти способ <w>преодолеть</w> разрыв между богатыми и бедными."
                            }
                        },
                        {
                            "text": "Her speech helped <w>bridge</w> the differences between the two political parties.",
                            "target_lang_translations": {
                                "ru": "Ее речь помогла <w>сгладить</w> разногласия между двумя политическими партиями."
                            }
                        }
                    ],
                    "synonyms": [
                        "reconcile",
                        "unite",
                        "connect",
                        "overcome"
                    ],
                    "antonyms": [
                        "widen",
                        "divide",
                        "separate"
                    ],
                    "common_phrases": [
                        "bridge the gap",
                        "bridge the divide",
                        "bridge differences"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшать или устранять различия, пробелы или разделения между вещами, такими как идеи, группы или периоды времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преодолевать",
                                "target_lang_sense_clarification": "устранять разрыв"
                            },
                            {
                                "target_lang_word": "сглаживать",
                                "target_lang_sense_clarification": "уменьшать различия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cca1d938-b22f-31e1-ae61-f84434bfe6a5",
                    "sense_definition": "To build a structure over something; to span a physical obstacle.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Engineers plan to <w>bridge</w> the river with a new steel structure.",
                            "target_lang_translations": {
                                "ru": "Инженеры планируют <w>перекрыть</w> реку новой стальной конструкцией."
                            }
                        },
                        {
                            "text": "A fallen tree <w>bridged</w> the narrow stream, allowing us to cross.",
                            "target_lang_translations": {
                                "ru": "Упавшее дерево <w>перекрыло</w> узкий ручей, позволив нам переправиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "span",
                        "cross",
                        "traverse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строить сооружение над чем-либо; перекрывать физическое препятствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекрывать",
                                "target_lang_sense_clarification": "соединять берега"
                            },
                            {
                                "target_lang_word": "возводить мост",
                                "target_lang_sense_clarification": "строить мост"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2d81bf00-694d-3ce5-83bf-eda13430e2f4",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I have an appointment with Dr. <w>Bridge</w> at three o'clock.",
                            "target_lang_translations": {
                                "ru": "У меня прием у доктора <w>Бриджа</w> в три часа."
                            }
                        },
                        {
                            "text": "The famous author, Mr. <w>Bridge</w>, will be signing books today.",
                            "target_lang_translations": {
                                "ru": "Известный писатель, мистер <w>Бридж</w>, будет подписывать книги сегодня."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "derived from occupation or location"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бридж",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
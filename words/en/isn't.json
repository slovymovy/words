            "senses": [
                {
                    "sense_id": "ab13278c-df08-35de-b916-0c90d10d2425",
                    "sense_definition": "Something or someone that is defined by the lack of the characteristic being discussed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The report was full of <w>isn'ts</w> rather than concrete plans for the future.",
                            "target_lang_translations": {
                                "ru": "Отчет был полон <w>отрицаний</w>, а не конкретных планов на будущее."
                            }
                        },
                        {
                            "text": "He is defined more by his <w>isn'ts</w> than by what he actually does.",
                            "target_lang_translations": {
                                "ru": "Он определяется скорее своими <w>отрицаниями</w>, чем тем, что он делает на самом деле."
                            }
                        }
                    ],
                    "target_lang_definitions": {

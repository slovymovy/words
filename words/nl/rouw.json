                    "synonyms": [
                        "verdriet",
                        "droefenis",
                        "smart"
                    ],
                    "antonyms": [
                        "vreugde",
                        "blijdschap"
                    ],
                    "common_phrases": [
                        "in de rouw",
                        "diepe rouw"
                    ],
                    "target_lang_definitions": {
                        "en": "Great sorrow or sadness after a loss, especially after the death of a loved one.",
                        "ru": "Глубокая скорбь или печаль после утраты, особенно после смерти близкого человека."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mourning",
                                "target_lang_sense_clarification": "state of sorrow"
                            },
                            {

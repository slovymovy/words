{
    "word_family": [
        "competitief",
        "competeren"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bdf625b-4b56-36b0-a53e-0dffa3d81044",
                    "sense_definition": "Een reeks wedstrijden tussen verschillende clubs, teams of spelers om te bepalen wie de beste is, vaak over een heel seizoen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "league",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De winnaar van de <w>competitie</w> mag volgend jaar op Europees niveau spelen.",
                            "target_lang_translations": {
                                "ru": "Победитель <w>чемпионата</w> сможет играть на европейском уровне в следующем году."
                            }
                        },
                        {
                            "text": "Al op de eerste dag van de <w>competitie</w> was er veel publiek aanwezig bij de wedstrijden.",
                            "target_lang_translations": {
                                "ru": "Уже в первый день <w>соревнований</w> на матчах присутствовало много зрителей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "start van de competitie",
                        "in de competitie",
                        "reguliere competitie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия матчей между клубами или игроками для определения лучшего, часто в течение сезона"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемпионат",
                                "target_lang_sense_clarification": "первенство, лига"
                            },
                            {
                                "target_lang_word": "соревнование",
                                "target_lang_sense_clarification": "состязание, турнир"
                            },
                            {
                                "target_lang_word": "лига",
                                "target_lang_sense_clarification": "спортивное объединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13bf6c3a-62fc-3ede-ae43-9883f69d237a",
                    "sense_definition": "De strijd om beter te presteren dan anderen en een bepaald doel te bereiken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rivalry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De broers voerden iedere dag een <w>competitie</w> op leven en dood met elkaar.",
                            "target_lang_translations": {
                                "ru": "Братья каждый день устраивали друг с другом смертельное <w>состязание</w>."
                            }
                        },
                        {
                            "text": "Er heerst een gezonde <w>competitie</w> tussen de collega's op de verkoopafdeling.",
                            "target_lang_translations": {
                                "ru": "Между коллегами в отделе продаж царит здоровая <w>конкуренция</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rivaliteit",
                        "wedijver",
                        "concurrentie"
                    ],
                    "common_phrases": [
                        "onderlinge competitie",
                        "competitie aangaan",
                        "gezonde competitie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Борьба за то, чтобы превзойти других и достичь определенной цели"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкуренция",
                                "target_lang_sense_clarification": "соперничество (в бизнесе, работе)"
                            },
                            {
                                "target_lang_word": "соперничество",
                                "target_lang_sense_clarification": "борьба за первенство"
                            },
                            {
                                "target_lang_word": "состязание",
                                "target_lang_sense_clarification": "спор, соревнование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2af3bfb1-dcd1-3182-9eb8-685dd090dbb5",
                    "sense_definition": "De gezamenlijke groep van tegenstanders of mededingers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opponents",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf moet blijven innoveren om de <w>competitie</w> voor te blijven.",
                            "target_lang_translations": {
                                "ru": "Компания должна продолжать внедрять инновации, чтобы опережать <w>конкурентов</w>."
                            }
                        },
                        {
                            "text": "Zij was bang dat de <w>competitie</w> te sterk zou zijn tijdens het toernooi.",
                            "target_lang_translations": {
                                "ru": "Она боялась, что <w>соперники</w> будут слишком сильны во время турнира."
                            }
                        }
                    ],
                    "synonyms": [
                        "concurrentie"
                    ],
                    "common_phrases": [
                        "de competitie verslaan",
                        "moordende competitie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа противников или конкурентов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкуренты",
                                "target_lang_sense_clarification": "соперники (люди или компании)"
                            },
                            {
                                "target_lang_word": "соперники",
                                "target_lang_sense_clarification": "противники"
                            }
                        ]
                    }
                }
            ]
        }
    ]

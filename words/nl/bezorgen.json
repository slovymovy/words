            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e4ea6ea4-08f9-3a81-84df-1156d39ba5df",
                    "sense_definition": "Goederen, zoals post, pakketten of maaltijden, naar een bepaald adres brengen en daar afgeven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De postbode zal het pakketje morgen bij uw huis <w>bezorgen</w>.",
                            "target_lang_translations": {
                                "en": "The mail carrier will <w>deliver</w> the package to your home tomorrow.",
                                "ru": "Почтальон <w>доставит</w> посылку к вашему дому завтра."
                            }
                        },
                        {
                            "text": "Wij <w>bezorgen</w> onze pizza's altijd warm en binnen dertig minuten.",
                            "target_lang_translations": {
                                "en": "We always <w>deliver</w> our pizzas hot and within thirty minutes.",
                                "ru": "Мы всегда <w>доставляем</w> наши пиццы горячими и в течение тридцати минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "afleveren",
                        "brengen"
                    ],
                    "antonyms": [
                        "ophalen",
                        "afhalen"
                    ],
                    "common_phrases": [
                        "post bezorgen",
                        "pakketje bezorgen",
                        "aan huis bezorgen",
                        "maaltijden bezorgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring goods, such as mail, packages, or meals, to a specific address and hand them over.",
                        "ru": "Доставлять товары, такие как почта, посылки или еда, по определенному адресу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "to transport goods to a destination"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "доставлять",
                                "target_lang_sense_clarification": "привозить товары или заказы"
                            },
                            {
                                "target_lang_word": "разносить",
                                "target_lang_sense_clarification": "о почте или газетах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e785c74-f678-313a-96b5-5a5d5bf30c3b",
                    "sense_definition": "Ervoor zorgen dat iemand iets krijgt of in zijn bezit heeft; verschaffen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan je alles <w>bezorgen</w> wat je voor het feest nodig hebt.",
                            "target_lang_translations": {
                                "en": "I can <w>get</w> you everything you need for the party.",
                                "ru": "Я могу <w>достать</w> тебе всё, что нужно для вечеринки."
                            }
                        },
                        {
                            "text": "Zijn oom heeft hem een goede baan bij de bank <w>bezorgd</w>.",
                            "target_lang_translations": {
                                "en": "His uncle <w>got</w> him a good job at the bank.",
                                "ru": "Его дядя <w>устроил</w> его на хорошую работу в банке."
                            }
                        }
                    ],
                    "synonyms": [
                        "verschaffen",
                        "regelen",
                        "leveren"
                    ],
                    "common_phrases": [
                        "een baan bezorgen",
                        "geld bezorgen",
                        "iemand iets bezorgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To ensure someone receives or possesses something; to provide or procure.",
                        "ru": "Обеспечивать получение чего-либо кем-либо; доставать или устраивать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to obtain something for someone"
                            },
                            {
                                "target_lang_word": "provide",
                                "target_lang_sense_clarification": "to supply or furnish"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "доставать",
                                "target_lang_sense_clarification": "находить или добывать что-то нужное"
                            },
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "помогать получить (например, работу)"
                            },
                            {
                                "target_lang_word": "обеспечивать",
                                "target_lang_sense_clarification": "снабжать чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65822ceb-6c65-3547-b01b-03fe7ba57cce",
                    "sense_definition": "Iets bij iemand teweegbrengen of veroorzaken, vaak met betrekking tot een gevoel, probleem of toestand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <w>bezorgt</w> me echt hartkloppingen met dat plotselinge lawaai.",
                            "target_lang_translations": {
                                "en": "You really <w>give</w> me palpitations with that sudden noise.",
                                "ru": "Ты действительно <w>вызываешь</w> у меня сердцебиение этим внезапным шумом."
                            }
                        },
                        {
                            "text": "Die moeilijke beslissing heeft hem veel slapeloze nachten <w>bezorgd</w>.",
                            "target_lang_translations": {
                                "en": "That difficult decision <w>caused</w> him many sleepless nights.",
                                "ru": "Это трудное решение <w>принесло</w> ему много бессонных ночей."
                            }
                        }
                    ],
                    "synonyms": [
                        "veroorzaken",
                        "aandoen",
                        "teweegbrengen"
                    ],
                    "common_phrases": [
                        "problemen bezorgen",
                        "overlast bezorgen",
                        "plezier bezorgen",
                        "kopzorgen bezorgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring about or cause something in someone, often regarding a feeling, problem, or state.",
                        "ru": "Вызывать или причинять что-либо, часто в отношении чувств, проблем или состояний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cause",
                                "target_lang_sense_clarification": "to make something happen or exist"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "to cause someone to experience a feeling or state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "причинять",
                                "target_lang_sense_clarification": "вызывать неприятности или боль"
                            },
                            {
                                "target_lang_word": "доставлять",
                                "target_lang_sense_clarification": "вызывать (беспокойство, хлопоты или удовольствие)"
                            },
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "провоцировать чувства или реакции"
                            }
                        ]
                    }
                }
            ]
        }

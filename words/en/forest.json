                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Forest</w> Whitaker is a famous actor.",
                            "target_lang_translations": {
                                "ru": "<w>Форест</w> Уитакер — известный актёр."
                            }
                        },
                        {
                            "text": "My friend <w>Forest</w> loves nature.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>Форест</w> любит природу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя; вариант написания имени Форрест."

{
    "word_family": [
        "paca",
        "pacnąć",
        "pacnięcie"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e637a19e-932f-3dd9-be2e-7b846d0c4f0f",
                    "sense_definition": "Wyraz dźwiękonaśladowczy imitujący odgłos uderzenia płaskim przedmiotem lub upadku czegoś miękkiego na twardą powierzchnię.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_imitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Książka zsunęła się z biurka i zrobiła głośne <w>pac</w> o podłogę.",
                            "target_lang_translations": {
                                "ru": "Книга соскользнула со стола и с громким <w>шлепком</w> ударилась об пол."
                            }
                        },
                        {
                            "text": "Mucha latała koło nosa, aż nagle <w>pac</w> ją gazetą.",
                            "target_lang_translations": {
                                "ru": "Муха летала у носа, пока вдруг — <w>хлоп</w> её газетой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bęc"
                    ],
                    "common_phrases": [
                        "zrobić pac",
                        "i pac"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звукоподражание, имитирующее звук удара плоским предметом или падения чего-то мягкого на твердую поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлёп",
                                "target_lang_sense_clarification": "звук падения чего-то мягкого или удара плашмя"
                            },
                            {
                                "target_lang_word": "бац",
                                "target_lang_sense_clarification": "резкий звук удара или падения"
                            },
                            {
                                "target_lang_word": "хлоп",
                                "target_lang_sense_clarification": "звук удара ладонью или плоским предметом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9c8ff1fc-dbf3-3387-ae09-ed2b9bc62458",
                    "sense_definition": "Polskie nazwisko (męskie lub żeńskie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Pac</w> był znanym fundatorem wielu barokowych budowli.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Пац</w> был известным основателем многих барочных зданий."
                            }
                        },
                        {
                            "text": "W tym regionie mieszka wielu ludzi o nazwisku <w>Pac</w>.",
                            "target_lang_translations": {
                                "ru": "В этом регионе живет много людей по фамилии <w>Пац</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Wart Pac pałaca"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "male or female"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия (мужская или женская)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пац",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c883ab3-d836-386a-a866-7c7892855624",
                    "sense_definition": "Forma słowa <w>paca</w> (dopełniacz liczby mnogiej).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Murarz potrzebował kilku nowych <w>pac</w> do wykończenia tynków.",
                            "target_lang_translations": {
                                "ru": "Каменщику нужно было несколько новых <w>тёрок</w> для отделки штукатурки."
                            }
                        },
                        {
                            "text": "W magazynie nie było już ani kielni, ani <w>pac</w>.",
                            "target_lang_translations": {
                                "ru": "На складе уже не было ни кельм, ни <w>тёрок</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа слова «paca» (строительная тёрка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёрок",
                                "target_lang_sense_clarification": "родительный падеж мн. ч. от «тёрка»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db7a07c4-d14e-36d9-80a9-cac0e612fcb0",
                    "sense_definition": "Pułk artylerii ciężkiej (jednostka wojskowa).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze z tego <w>pacu</w> wsławili się odwagą podczas bitwy.",
                            "target_lang_translations": {
                                "ru": "Солдаты из этого <w>полка тяжёлой артиллерии</w> прославились отвагой во время битвы."
                            }
                        },
                        {
                            "text": "Dowództwo sformowało nowy <w>pac</w> na potrzeby frontu.",
                            "target_lang_translations": {
                                "ru": "Командование сформировало новый <w>тяжёлый артполк</w> для нужд фронта."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полк тяжёлой артиллерии (воинская часть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полк тяжёлой артиллерии",
                                "target_lang_sense_clarification": "военное подразделение"
                            },
                            {
                                "target_lang_word": "артполк",
                                "target_lang_sense_clarification": "сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d76dfbd6-366f-3281-9daf-9bad0c8dd853",
                    "sense_definition": "Ogromny szczur.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ciemnej piwnicy przemknął wielki <w>pac</w>, budząc przerażenie.",
                            "target_lang_translations": {
                                "ru": "В темном подвале промелькнул огромный <w>пасюк</w>, вызвав ужас."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie widziałem tak dużego <w>paca</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не видел такого большого <w>пасюка</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Огромная крыса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасюк",
                                "target_lang_sense_clarification": "крупная серая крыса"
                            },
                            {
                                "target_lang_word": "крыса",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
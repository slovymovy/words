                            }
                        },
                        {
                            "text": "The software was written in a complex computer <w>language</w>.",
                            "target_lang_translations": {
                                "ru": "Программное обеспечение было написано на сложном компьютерном <w>языке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "programming language",
                        "computer language",
                        "machine language"
                    ],
                    "common_phrases": [
                        "programming language",
                        "coding language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система инструкций, используемая для программирования компьютеров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык программирования",

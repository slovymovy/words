            "pos": "noun",
            "senses": [
                {
                    "sense_id": "adcd402e-4240-3633-bf4b-c438f1310e41",
                    "sense_definition": "Wyposażenie łóżka służące do spania, obejmujące poduszki, kołdrę i prześcieradło.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "bedding_items",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od babci Ani dostałam komplet ciepłej, puchowej <pościeli>.",
                            "target_lang_translations": {
                                "ru": "От бабушки Ани я получила комплект тёплых пуховых <постельных принадлежностей>."
                            }
                        },
                        {
                            "text": "Zimą najlepiej śpi się pod grubą <pościelą>.",
                            "target_lang_translations": {
                                "ru": "Зимой лучше всего спится под толстым <одеялом>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bety"
                    ],
                    "common_phrases": [
                        "ciepła pościel",
                        "komplet pościeli",
                        "puchowa pościel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежности для сна, включающие подушки, одеяло и простыню."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постель",
                                "target_lang_sense_clarification": "общее название для места сна и принадлежностей"
                            },
                            {
                                "target_lang_word": "постельные принадлежности",
                                "target_lang_sense_clarification": "официальное название предметов для сна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d4d5ae6-e842-36f5-b9a9-dccf13924a9b",
                    "sense_definition": "Zdejmowane powłoczki z materiału zakładane na kołdrę i poduszki, które można prać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bedding_items",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W hotelu <pościel> wymienia się dla nowych gości.",
                            "target_lang_translations": {
                                "ru": "В отеле <постельное бельё> меняют для новых гостей."
                            }
                        },
                        {
                            "text": "Muszę wrzucić brudną <pościel> do pralki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно закинуть грязное <постельное бельё> в стиральную машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zmieniać pościel",
                        "świeża pościel",
                        "czysta pościel",
                        "prać pościel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сменные тканевые чехлы (наволочки, пододеяльник), надеваемые на подушки и одеяло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постельное бельё",
                                "target_lang_sense_clarification": "тканевые изделия"
                            },
                            {
                                "target_lang_word": "постель",
                                "target_lang_sense_clarification": "разговорное обозначение белья"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "antonyms": [
                        "hogedrukgebied"
                    ],
                    "common_phrases": [
                        "een depressie trekt over",
                        "Atlantische depressie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Область в атмосфере с относительно низким давлением, часто приносящая облачность и осадки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "циклон",
                                "target_lang_sense_clarification": "метеорология"
                            },
                            {
                                "target_lang_word": "депрессия",
                                "target_lang_sense_clarification": "область низкого давления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "304ce0d1-27f5-3fa5-9df7-ef829487b570",
                    "sense_definition": "Een langdurige periode van ernstige economische achteruitgang en lage conjunctuur.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens een economische <w>depressie</w> verliezen veel mensen hun baan en inkomen.",
                            "target_lang_translations": {
                                "ru": "Во время экономической <w>депрессии</w> многие люди теряют работу и доход."
                            }
                        },
                        {
                            "text": "De beurskrach van 1929 leidde tot een wereldwijde <w>depressie</w> die jaren duurde.",
                            "target_lang_translations": {
                                "ru": "Биржевой крах 1929 года привел к мировой <w>депрессии</w>, которая длилась годами."
                            }

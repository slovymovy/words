    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d08ac6e9-9b4c-37c0-89a2-b8d3ded873ab",
                    "sense_definition": "To, co się mówi lub pisze w reakcji na czyjeś pytanie, stwierdzenie lub prośbę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication_reply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prosiłeś o <w>odpowiedź</w> i ją otrzymałeś.",
                            "target_lang_translations": {
                                "ru": "Ты просил <w>ответ</w>, и ты его получил."
                            }
                        },
                        {
                            "text": "Nie otrzymałem jeszcze żadnej <w>odpowiedzi</w> na mój list.",
                            "target_lang_translations": {
                                "ru": "Я еще не получил никакого <w>ответа</w> на мое письмо."
                            }
                        }
                    ],
                    "synonyms": [
                        "odzew",
                        "replika",
                        "respons"
                    ],
                    "antonyms": [
                        "pytanie"
                    ],
                    "common_phrases": [
                        "krótka odpowiedź",
                        "dać odpowiedź",
                        "czekać na odpowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что говорится или пишется в ответ на чей-то вопрос, утверждение или просьбу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответ",
                                "target_lang_sense_clarification": "на вопрос, письмо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "910439ba-feb2-39ec-a588-fd86ea36ac34",
                    "sense_definition": "Wynik zadania, rozwiązanie problemu lub zagadki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "solution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sfinks dusiła każdego, kto nie potrafił udzielić prawidłowej <w>odpowiedzi</w>.",
                            "target_lang_translations": {
                                "ru": "Сфинкс душил каждого, кто не мог дать правильного <w>ответа</w>."
                            }
                        },
                        {
                            "text": "Zaznacz właściwą <w>odpowiedź</w> na karcie egzaminacyjnej.",
                            "target_lang_translations": {
                                "ru": "Отметь правильный <w>ответ</w> на экзаменационном листе."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozwiązanie",
                        "wynik"
                    ],
                    "common_phrases": [
                        "poprawna odpowiedź",
                        "błędna odpowiedź",
                        "klucz odpowiedzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат задания, решение проблемы или загадки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответ",
                                "target_lang_sense_clarification": "решение задачи"
                            },
                            {
                                "target_lang_word": "разгадка",
                                "target_lang_sense_clarification": "тайны, загадки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51c93938-c8af-3d5c-8134-56a347e213a5",
                    "sense_definition": "Ustne sprawdzenie wiedzy ucznia przez nauczyciela podczas lekcji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "school_exam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel wezwał ucznia do <w>odpowiedzi</w> przy tablicy.",
                            "target_lang_translations": {
                                "ru": "Учитель вызвал ученика для <w>ответа</w> у доски."
                            }
                        },
                        {
                            "text": "Dostałem dobrą ocenę z ustnej <w>odpowiedzi</w> z historii.",
                            "target_lang_translations": {
                                "ru": "Я получил хорошую оценку за устный <w>ответ</w> по истории."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wezwać do odpowiedzi",
                        "ocena z odpowiedzi",
                        "odpowiedź ustna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устная проверка знаний ученика учителем во время урока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответ",
                                "target_lang_sense_clarification": "устный, у доски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56dbf033-9167-3182-952c-cba72ffbb219",
                    "sense_definition": "Reakcja na czyjeś zachowanie, działanie lub słowa; często cięta lub stanowcza.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reaction_retort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego cięta <w>odpowiedź</w> zamknęła usta krytykom.",
                            "target_lang_translations": {
                                "ru": "Его резкий <w>ответ</w> заткнул рот критикам."
                            }
                        },
                        {
                            "text": "W <w>odpowiedzi</w> na zarzuty opublikował oficjalne sprostowanie.",
                            "target_lang_translations": {
                                "ru": "В <w>ответ</w> на обвинения он опубликовал официальное опровержение."
                            }
                        }
                    ],
                    "synonyms": [
                        "riposta"
                    ],
                    "common_phrases": [
                        "w odpowiedzi na",
                        "ostra odpowiedź",
                        "szybka odpowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реакция на чье-то поведение, действие или слова; часто резкая или решительная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответ",
                                "target_lang_sense_clarification": "реакция"
                            },
                            {
                                "target_lang_word": "отповедь",
                                "target_lang_sense_clarification": "резкое возражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85301d35-a022-36b1-8b8e-5c9ae455d66e",
                    "sense_definition": "Pismo procesowe składane przez pozwanego, zawierające jego stanowisko wobec pozwu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pozwany złożył <w>odpowiedź</w> na pozew w wyznaczonym terminie.",
                            "target_lang_translations": {
                                "ru": "Ответчик подал <w>отзыв</w> на исковое заявление в назначенный срок."
                            }
                        },
                        {
                            "text": "Sąd analizuje <w>odpowiedź</w> na pozew przygotowaną przez adwokata.",
                            "target_lang_translations": {
                                "ru": "Суд анализирует <w>отзыв</w> на иск, подготовленный адвокатом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odpowiedź na pozew",
                        "złożyć odpowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процессуальный документ, подаваемый ответчиком, содержащий его позицию по иску."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отзыв",
                                "target_lang_sense_clarification": "на исковое заявление"
                            },
                            {
                                "target_lang_word": "возражение",
                                "target_lang_sense_clarification": "на иск"
                            }

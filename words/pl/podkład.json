                },
                {
                    "sense_id": "2b50edfb-0672-32b2-a5fa-2a18ab168034",
                    "sense_definition": "Poprzeczna belka (drewniana lub betonowa), na której spoczywają szyny toru kolejowego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare drewniane <w>podkłady</w> zostały wymienione na betonowe.",
                            "target_lang_translations": {
                                "ru": "Старые деревянные <w>шпалы</w> были заменены на бетонные."
                            }
                        },
                        {
                            "text": "Spacerowanie po torach i skakanie przez <w>podkłady</w> jest niebezpieczne.",
                            "target_lang_translations": {
                                "ru": "Гулять по путям и прыгать через <w>шпалы</w> опасно."

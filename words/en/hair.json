    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b0f6b72d-6168-3773-89f5-094477feb6e0",
                    "sense_definition": "The collection of fine filaments growing from the skin of humans and animals, especially the mass covering the human head.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to cut her long <w>hair</w> short for the summer.",
                            "target_lang_translations": {
                                "ru": "Она решила коротко подстричь свои длинные <w>волосы</w> на лето."
                            }
                        },
                        {
                            "text": "He needs to wash his <w>hair</w> after playing football.",
                            "target_lang_translations": {
                                "ru": "Ему нужно помыть <w>волосы</w> после игры в футбол."
                            }
                        }
                    ],
                    "synonyms": [
                        "tresses",
                        "mane",
                        "locks",
                        "mop"
                    ],
                    "antonyms": [
                        "baldness"
                    ],
                    "common_phrases": [
                        "long hair",
                        "short hair",
                        "brush hair",
                        "wash hair",
                        "lose hair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность тонких нитей, растущих на коже людей и животных, особенно густой покров на голове человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волосы",
                                "target_lang_sense_clarification": "растительность на голове"
                            },
                            {
                                "target_lang_word": "шевелюра",
                                "target_lang_sense_clarification": "густые волосы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7434a26-e753-3262-98c8-0f9d7d648ddd",
                    "sense_definition": "A single pigmented filament growing from a follicle on the skin.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_part_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The customer complained when he found a <w>hair</w> in his soup.",
                            "target_lang_translations": {
                                "ru": "Клиент пожаловался, когда нашел <w>волос</w> в своем супе."
                            }
                        },
                        {
                            "text": "She plucked a stray gray <w>hair</w> from her coat.",
                            "target_lang_translations": {
                                "ru": "Она сняла со своего пальто случайно прилипший седой <w>волос</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "strand",
                        "bristle",
                        "filament"
                    ],
                    "common_phrases": [
                        "gray hair",
                        "stray hair",
                        "single hair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная пигментированная нить, растущая из фолликула на коже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волос",
                                "target_lang_sense_clarification": "одна нить"
                            },
                            {
                                "target_lang_word": "волосок",
                                "target_lang_sense_clarification": "уменьшительное"
                            },
                            {
                                "target_lang_word": "волосинка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79a54da1-da8f-3f29-87a1-aee36ca9bda9",
                    "sense_definition": "A very small margin, distance, or degree.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The speeding car missed the pedestrian by a <w>hair</w>.",
                            "target_lang_translations": {
                                "ru": "Мчащаяся машина проехала всего на <w>волосок</w> от пешехода."
                            }
                        },
                        {
                            "text": "Could you turn the volume down just a <w>hair</w>?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты убавить громкость совсем <w>чуть-чуть</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "hairbreadth",
                        "whisker",
                        "fraction"
                    ],
                    "common_phrases": [
                        "by a hair",
                        "split hairs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень малая величина, расстояние или степень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волосок",
                                "target_lang_sense_clarification": "в идиомах о расстоянии"
                            },
                            {
                                "target_lang_word": "чуть-чуть",
                                "target_lang_sense_clarification": "наречие степени"
                            },
                            {
                                "target_lang_word": "малость",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b7b11ef-86d8-3ae1-b470-fed4e2ffd650",
                    "sense_definition": "A slender outgrowth on the surface of a plant, insect, or other organism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stinging nettle is covered in tiny <w>hairs</w> that irritate the skin.",
                            "target_lang_translations": {
                                "ru": "Крапива покрыта крошечными <w>волосками</w>, которые раздражают кожу."
                            }
                        },
                        {
                            "text": "Under the microscope, you can see the fine <w>hairs</w> on the spider's legs.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом можно увидеть тонкие <w>волоски</w> на лапках паука."
                            }
                        }
                    ],
                    "synonyms": [
                        "bristle",
                        "cilium",
                        "trichome"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий отросток на поверхности растения, насекомого или другого организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волосок",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "ворсинка",
                                "target_lang_sense_clarification": "на поверхности ткани или растения"
                            }

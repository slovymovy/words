            "senses": [
                {
                    "sense_id": "29f1a57b-3230-359c-b35f-9d32c470d3f2",
                    "sense_definition": "Płaskie pudełeczko służące do przechowywania pudru kosmetycznego, najczęściej zdobione i mające lusterko wewnątrz.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cosmetic_containers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Puderniczka była zamykana na zapięcie tak małe, że Märtha nie dała rady jej otworzyć.",
                            "target_lang_translations": {
                                "ru": "<w>Пудреница</w> закрывалась на застежку такую маленькую, что Марта не смогла её открыть."
                            }
                        },
                        {
                            "text": "W jej torebce zawsze znajduje się mała <puderniczka> i ulubiona szminka.",
                            "target_lang_translations": {
                                "ru": "В её сумочке всегда лежит маленькая <w>пудреница</w> и любимая помада."
                            }
                        }
                    ],
                    "synonyms": [
                        "pudernica"
                    ],
                    "common_phrases": [
                        "srebrna puderniczka",
                        "otwarta puderniczka",
                        "lusterko w puderniczce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская коробочка для хранения косметической пудры, часто украшенная и имеющая зеркальце внутри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пудреница",
                                "target_lang_sense_clarification": "футляр для пудры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb53f8c0-d7cf-3e92-a005-a246aec8f1ab",
                    "sense_definition": "Zdrobnienie od słowa <w>pudernica</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na toaletce stała urocza, porcelanowa <puderniczka>, którą dostała w prezencie.",
                            "target_lang_translations": {
                                "ru": "На туалетном столике стояла прелестная фарфоровая <w>пудреничка</w>, которую она получила в подарок."
                            }
                        },
                        {
                            "text": "To nie była duża skrzynia, lecz zaledwie malutka <puderniczka>.",
                            "target_lang_translations": {
                                "ru": "Это был не большой ящик, а всего лишь крошечная <w>пудреничка</w>."
                            }
                        }
                    ],

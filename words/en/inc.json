    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "833e3a6e-80c0-326c-9c93-bca5ea25ad28",
                    "sense_definition": "Abbreviation of <w>incorporated</w>; used after a company name to indicate it is a legal corporation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "business_abbrev",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software was developed by TechSolutions <w>Inc.</w> in California.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение было разработано компанией TechSolutions <w>Inc.</w> в Калифорнии."
                            }
                        },
                        {
                            "text": "Many large businesses, such as Amazon <w>Inc.</w>, are traded publicly.",
                            "target_lang_translations": {
                                "ru": "Многие крупные компании, такие как Amazon <w>Inc.</w>, торгуются на бирже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Apple Inc",
                        "Google Inc",
                        "Monsters Inc"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used after nouns"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «incorporated»; используется после названия компании для указания на то, что она является зарегистрированной корпорацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корпорация",
                                "target_lang_sense_clarification": "юридический статус"
                            },
                            {
                                "target_lang_word": "Инк.",
                                "target_lang_sense_clarification": "транслитерация в названиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c6f73f3-fca5-31f0-b560-f587e97f4f36",

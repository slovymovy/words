        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "d38aa5f6-ff4c-3b2b-a8ed-e1005378664e",
                    "sense_definition": "Spójnik wprowadzający zdanie podrzędne, wskazujący, że czynność w nim opisana nastąpiła później niż czynność w zdaniu nadrzędnym; to samo co <w>zanim</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_conjunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy uciekać, <w>nim</w> nas zauważą.",
                            "target_lang_translations": {
                                "ru": "Мы должны бежать, <w>прежде чем</w> нас заметят."
                            }
                        },
                        {
                            "text": "Zastanów się dwa razy, <w>nim</w> coś powiesz.",
                            "target_lang_translations": {
                                "ru": "Подумай дважды, <w>перед тем как</w> что-то сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanim"
                    ],
                    "common_phrases": [
                        "nim się obejrzysz",
                        "nim będzie za późno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий придаточное предложение времени; указывает, что действие в нём произошло позже, чем в главном; то же, что <w>zanim</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прежде чем",
                                "target_lang_sense_clarification": "союз времени"
                            },
                            {
                                "target_lang_word": "перед тем как",
                                "target_lang_sense_clarification": "союз времени"
                            },
                            {
                                "target_lang_word": "пока не",
                                "target_lang_sense_clarification": "в значении 'до того как'"
                            }
                        ]
                    }
                }
            ]
        },

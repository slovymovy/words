{
    "word_family": [
        "okoliczny",
        "okoliczność"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "74f62252-9f99-3c10-a67c-58cf737d3764",
                    "sense_definition": "Określony teren, zwykle zamieszkany, stanowiący pewną całość; część miasta lub wsi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkam w niebezpiecznej <w>okolicy</w>, gdzie rzadko widuje się policję.",
                            "target_lang_translations": {
                                "ru": "Я живу в опасном <w>районе</w>, где редко увидишь полицию."
                            }
                        },
                        {
                            "text": "To bardzo spokojna <w>okolica</w>, idealna dla rodzin z dziećmi.",
                            "target_lang_translations": {
                                "ru": "Это очень спокойный <w>район</w>, идеальный для семей с детьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "rejon",
                        "region",
                        "strony"
                    ],
                    "common_phrases": [
                        "spokojna okolica",
                        "niebezpieczna okolica",
                        "znana okolica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная территория, обычно заселенная; часть города или села."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "район",
                                "target_lang_sense_clarification": "часть города, жилой массив"
                            },
                            {
                                "target_lang_word": "местность",
                                "target_lang_sense_clarification": "территория, ландшафт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f35302b3-6732-31dc-b08b-a67ef6750fd0",
                    "sense_definition": "Obszar przylegający bezpośrednio do jakiegoś miejsca, obiektu lub punktu; przestrzeń wokół czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Adam czuje silne kłucie w <w>okolicy</w> serca.",
                            "target_lang_translations": {
                                "ru": "Адам чувствует сильное покалывание в <w>области</w> сердца."
                            }
                        },
                        {
                            "text": "W najbliższej <w>okolicy</w> nie ma żadnego sklepu spożywczego.",
                            "target_lang_translations": {
                                "ru": "В ближайшей <w>округе</w> нет ни одного продуктового магазина."
                            }
                        }
                    ],
                    "synonyms": [
                        "pobliże",
                        "sąsiedztwo"
                    ],
                    "common_phrases": [
                        "w najbliższej okolicy",
                        "w okolicy żołądka",
                        "zwiedzać okolicę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространство, непосредственно прилегающее к какому-либо месту или объекту; зона вокруг чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окрестность",
                                "target_lang_sense_clarification": "прилегающая территория"
                            },
                            {
                                "target_lang_word": "область",
                                "target_lang_sense_clarification": "анатомическая зона, часть тела"
                            },
                            {
                                "target_lang_word": "округа",
                                "target_lang_sense_clarification": "близлежащая местность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21a46ecf-b953-3658-8a82-2fdaad501474",
                    "sense_definition": "Ogół mieszkańców zamieszkujących dany obszar.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała <w>okolica</w> protestowała przeciwko decyzji burmistrza o budowie fabryki.",
                            "target_lang_translations": {
                                "ru": "Вся <w>округа</w> протестовала против решения мэра о строительстве фабрики."
                            }
                        },
                        {
                            "text": "Plotka o ich ślubie szybko obiegła całą <w>okolicę</w>.",
                            "target_lang_translations": {
                                "ru": "Слух об их свадьбе быстро облетел всю <w>округу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cała okolica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Все жители, населяющие данную местность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "округа",
                                "target_lang_sense_clarification": "жители окрестных мест"
                            },
                            {
                                "target_lang_word": "местные жители",
                                "target_lang_sense_clarification": "население района"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "40acee5f-8673-3a2a-a300-62373734ac6a",
                    "sense_definition": "Powodować, że ciecz płynie z jednego miejsca w drugie (np. do naczynia)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "liquid_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner ostrożnie <w>lał</w> czerwone wino do kieliszków gości.",
                            "target_lang_translations": {
                                "ru": "Официант осторожно <w>наливал</w> красное вино в бокалы гостей."
                            }
                        },
                        {
                            "text": "Proszę, nie <w>lej</w> tyle wody do wanny, bo się przeleje.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>лей</w> столько воды в ванну, а то она перельется."
                            }
                        }
                    ],
                    "synonyms": [
                        "wlewać",
                        "nalewać",
                        "rozlewać"
                    ],
                    "common_phrases": [
                        "lać wodę",
                        "lać wino",
                        "lać paliwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять жидкость течь из одного места в другое (например, в сосуд)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лить",
                                "target_lang_sense_clarification": "заставлять течь"
                            },
                            {
                                "target_lang_word": "наливать",
                                "target_lang_sense_clarification": "наполнять сосуд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf611fc2-8f48-34e3-a1fe-155fc36b93de",
                    "sense_definition": "O deszczu: padać bardzo intensywnie i obficie",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wychodź teraz na spacer, bo strasznie <w>leje</w>.",
                            "target_lang_translations": {
                                "ru": "Не выходи сейчас гулять, потому что страшно <w>льет</w>."
                            }
                        },
                        {
                            "text": "Przez cały tydzień <w>lało</w>, więc wszędzie są ogromne kałuże.",
                            "target_lang_translations": {
                                "ru": "Всю неделю <w>лило</w>, поэтому везде огромные лужи."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacinać",
                        "lunąć"
                    ],
                    "antonyms": [
                        "kropić"
                    ],
                    "common_phrases": [
                        "leje jak z cebra",
                        "deszcz leje"
                    ],
                    "target_lang_definitions": {
                        "ru": "О дожде: идти очень сильно и обильно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лить",
                                "target_lang_sense_clarification": "о сильном дожде"
                            },
                            {
                                "target_lang_word": "лить как из ведра",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5b349d0-cac8-37c8-aee9-84dd4606550a",
                    "sense_definition": "Płynąć strumieniem w dużej ilości (często z zaimkiem 'się')",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "liquid_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda <w>lała</w> się z pękniętej rury prosto na podłogę.",
                            "target_lang_translations": {
                                "ru": "Вода <w>лилась</w> из лопнувшей трубы прямо на пол."

{
    "word_family": [
        "voorlichten",
        "voorlichter"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c07b30f-f7e7-3fea-ac0c-19c37527d688",
                    "sense_definition": "Het geven van informatie en advies over een bepaald onderwerp om mensen bewust te maken of te helpen bij het nemen van beslissingen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "information",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de middelbare school krijgen alle leerlingen seksuele <w>voorlichting</w>.",
                            "target_lang_translations": {
                                "ru": "В средней школе все ученики получают половое <w>просвещение</w>."
                            }
                        },
                        {
                            "text": "De overheid geeft via campagnes <w>voorlichting</w> over gezond eten en bewegen.",
                            "target_lang_translations": {
                                "ru": "Правительство через кампании предоставляет <w>информацию</w> о здоровом питании и физической активности."
                            }
                        }
                    ],
                    "synonyms": [
                        "informatieverstrekking",
                        "instructie"
                    ],
                    "common_phrases": [
                        "seksuele voorlichting",
                        "voorlichting geven",
                        "voorlichting over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставление информации и советов по определенной теме для повышения осведомленности или помощи в принятии решений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "информирование",
                                "target_lang_sense_clarification": "процесс предоставления сведений"
                            },
                            {
                                "target_lang_word": "просвещение",
                                "target_lang_sense_clarification": "распространение знаний, например, в вопросах здоровья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "575e8f9c-97eb-3e83-a6d5-61087a51c2a5",
                    "sense_definition": "Het geven van uitleg of opheldering om iets duidelijker te maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "information",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vroeg de commissie om nadere <w>voorlichting</w> over de nieuwe regels.",
                            "target_lang_translations": {
                                "ru": "Он попросил комиссию дать дополнительные <w>разъяснения</w> по поводу новых правил."
                            }
                        },
                        {
                            "text": "Ter <w>voorlichting</w> stuur ik u hierbij een overzicht van de feiten.",
                            "target_lang_translations": {
                                "ru": "В качестве <w>пояснения</w> я направляю вам обзор фактов."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitleg",
                        "toelichting",
                        "opheldering"
                    ],
                    "common_phrases": [
                        "nadere voorlichting",
                        "ter voorlichting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставление объяснений или уточнений, чтобы сделать что-то более понятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъяснение",
                                "target_lang_sense_clarification": "уточнение смысла или деталей"
                            },
                            {
                                "target_lang_word": "пояснение",
                                "target_lang_sense_clarification": "дополнительное объяснение для ясности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

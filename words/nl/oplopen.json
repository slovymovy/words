                {
                    "sense_id": "9f9c2a81-921d-3fd8-878e-5db7dc98457c",
                    "sense_definition": "Lopend naar boven gaan of een oppervlak betreden.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_upwards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij zag haar broer de trap <w>oplopen</w>.",
                            "target_lang_translations": {
                                "en": "She saw her brother <w>walk up</w> the stairs.",
                                "ru": "Она видела, как её брат <w>поднимается</w> по лестнице."
                            }
                        },
                        {
                            "text": "Toen we het pad <w>opliepen</w>, begon de hond te blaffen.",
                            "target_lang_translations": {
                                "en": "When we <w>walked up</w> the path, the dog started barking.",
                                "ru": "Когда мы <w>поднимались</w> по дорожке, собака начала лаять."
                            }
                        }
                    ],
                    "synonyms": [
                        "opgaan",
                        "bestijgen"
                    ],
                    "antonyms": [
                        "aflopen",
                        "afgaan"
                    ],
                    "common_phrases": [
                        "de trap oplopen",
                        "het pad oplopen",
                        "de straat oplopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go upwards while walking or to enter onto a surface.",
                        "ru": "Подниматься пешком или ступать на поверхность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk up",
                                "target_lang_sense_clarification": "stairs, hill, or path"
                            },
                            {
                                "target_lang_word": "walk onto",
                                "target_lang_sense_clarification": "a street, field, or stage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "по лестнице, на холм"
                            },
                            {
                                "target_lang_word": "всходить",
                                "target_lang_sense_clarification": "на возвышение, трибуну"
                            },
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "на улицу, во двор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3e4aeb8-af67-3f07-9c67-f1d7cc8a8154",
                    "sense_definition": "Toenemen in aantal, hoogte, bedrag of intensiteit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kosten voor de reparatie kunnen flink <w>oplopen</w>.",
                            "target_lang_translations": {
                                "en": "The costs for the repair can <w>run up</w> significantly.",
                                "ru": "Расходы на ремонт могут значительно <w>возрасти</w>."
                            }
                        },
                        {
                            "text": "In de middag zal de temperatuur snel <w>oplopen</w>.",
                            "target_lang_translations": {
                                "en": "In the afternoon the temperature will <w>rise</w> quickly.",
                                "ru": "Днем температура будет быстро <w>повышаться</w>."
                            }
                        }
                    ],
                    "synonyms": [

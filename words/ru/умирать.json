    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83c7a6d9-1f87-39a7-948c-860d1643ad40",
                    "sense_definition": "Переставать жить, прекращать физическое существование.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие солдаты <умирали> от ран в госпитале.",
                            "target_lang_translations": {
                                "en": "Many soldiers <died> of their wounds in the hospital."
                            }
                        },
                        {
                            "text": "Мой дедушка <умирал> долго и тяжело.",
                            "target_lang_translations": {
                                "en": "My grandfather <was dying> a long and painful death."
                            }
                        }
                    ],
                    "synonyms": [
                        "помирать",
                        "кончаться",
                        "отходить",
                        "испускать дух",
                        "переходить в мир иной"
                    ],
                    "antonyms": [
                        "рождаться",
                        "воскресать"
                    ],
                    "common_phrases": [
                        "умирать от старости",
                        "умирать от голода",
                        "умирать молодым",
                        "умирать за родину"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop living, to cease physical existence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die",
                                "target_lang_sense_clarification": "to stop living"
                            },
                            {
                                "target_lang_word": "pass away",
                                "target_lang_sense_clarification": "polite euphemism"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19048c86-aa63-3ccb-a92e-5d10da0467b0",
                    "sense_definition": "Исчезать, прекращаться, переставать существовать (о чувствах, традициях, явлениях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Надежда <умирает> последней.",
                            "target_lang_translations": {
                                "en": "Hope <dies> last."
                            }
                        },
                        {
                            "text": "Старые традиции постепенно <умирают> в современном обществе.",
                            "target_lang_translations": {
                                "en": "Old traditions are gradually <dying> in modern society."
                            }
                        }
                    ],
                    "synonyms": [
                        "кончаться"
                    ],
                    "antonyms": [
                        "появляться",
                        "возникать",
                        "рождаться"
                    ],
                    "common_phrases": [
                        "надежда умирает последней",
                        "любовь умирает",
                        "традиция умирает"
                    ],
                    "target_lang_definitions": {
                        "en": "To disappear, to cease, to stop existing (about feelings, traditions, phenomena)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die",
                                "target_lang_sense_clarification": "to cease to exist"
                            },
                            {
                                "target_lang_word": "die out",
                                "target_lang_sense_clarification": "to disappear gradually"
                            },
                            {
                                "target_lang_word": "fade away",
                                "target_lang_sense_clarification": "to diminish and disappear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e3a65af-b6ca-346c-a625-63c47327f41a",
                    "sense_definition": "Испытывать очень сильное чувство (смех, скука, страх и т.п.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я просто <умираю> от скуки на этих лекциях.",
                            "target_lang_translations": {
                                "en": "I am simply <dying> of boredom at these lectures."
                            }
                        },
                        {
                            "text": "Мы <умирали> от смеха, слушая его забавные истории.",
                            "target_lang_translations": {
                                "en": "We <were dying> of laughter listening to his funny stories."
                            }
                        }
                    ],
                    "synonyms": [
                        "помирать"
                    ],
                    "common_phrases": [
                        "умирать от смеха",
                        "умирать от скуки",
                        "умирать от любопытства",
                        "умирать от зависти"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to exaggerate emotions"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To experience a very strong feeling (laughter, boredom, fear, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die",
                                "target_lang_sense_clarification": "figurative: to be overcome by emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e432e32c-6364-32c1-bf4b-50020da9e13d",
                    "sense_definition": "Переставать работать, ломаться или разряжаться (о технике).",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой телефон <умирает>, я перезвоню тебе позже.",
                            "target_lang_translations": {
                                "en": "My phone <is dying>, I'll call you back later."
                            }
                        },
                        {
                            "text": "Старый компьютер медленно <умирает> и постоянно зависает.",
                            "target_lang_translations": {
                                "en": "The old computer is slowly <dying> and constantly freezing."
                            }
                        }
                    ],
                    "synonyms": [
                        "кончаться"
                    ],
                    "common_phrases": [
                        "телефон умирает",
                        "батарея умирает"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop working, to break down or run out of power (about technology)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die",
                                "target_lang_sense_clarification": "to stop working or lose power"
                            }
                        ]
                    }
                }
            ]
        }
    ]

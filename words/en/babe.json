                {
                    "sense_id": "71b036e8-6e5c-3a2a-9311-9cdd900425c3",
                    "sense_definition": "Darling (term of endearment).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "endearment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>babe</w>, are you ready to go to the cinema?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>малыш</w>, ты готова идти в кино?"
                            }
                        },
                        {
                            "text": "I bought you some flowers on my way home, <w>babe</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил тебе цветы по дороге домой, <w>детка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "darling",
                        "dear",
                        "love",
                        "sweetheart"
                    ],
                    "common_phrases": [
                        "hey babe",
                        "thanks babe",
                        "love you babe"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used between romantic partners"
                        }
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f6f7895-18cf-3666-88e4-309323b2060e",
                    "sense_definition": "Глава племени, рода или феодального государства (княжества) на Руси и в некоторых других странах; правитель.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ruler_noble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Великий <w>князь</w> собрал дружину для защиты города от кочевников.",
                            "target_lang_translations": {
                                "en": "The Grand <w>Prince</w> gathered his retinue to defend the city from nomads."
                            }
                        },
                        {
                            "text": "В древности каждый <w>князь</w> стремился расширить свои владения.",
                            "target_lang_translations": {
                                "en": "In ancient times, every <w>prince</w> sought to expand his domains."
                            }
                        }
                    ],
                    "synonyms": [
                        "правитель",
                        "государь",
                        "властелин"
                    ],
                    "common_phrases": [
                        "великий князь",
                        "киевский князь",
                        "удельный князь"
                    ],
                    "target_lang_definitions": {
                        "en": "The head of a tribe, clan, or feudal state (principality) in Rus' and some other countries; a ruler."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prince",
                                "target_lang_sense_clarification": "ruler of a principality"
                            },
                            {
                                "target_lang_word": "grand prince",
                                "target_lang_sense_clarification": "specific historical title (Veliky Knyaz)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c1004c-588b-3764-ad42-287f3469e351",
                    "sense_definition": "Человек, носящий высший дворянский титул, который передавался по наследству или жаловался монархом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ruler_noble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На бал был приглашён молодой <w>князь</w> из знатного рода.",
                            "target_lang_translations": {
                                "en": "A young <w>prince</w> from a noble family was invited to the ball."
                            }
                        },
                        {
                            "text": "Старый <w>князь</w> любил проводить вечера в своей библиотеке.",
                            "target_lang_translations": {
                                "en": "The old <w>prince</w> liked to spend his evenings in his library."
                            }
                        }
                    ],
                    "synonyms": [
                        "аристократ",
                        "дворянин",
                        "вельможа"
                    ],
                    "antonyms": [
                        "простолюдин"
                    ],
                    "common_phrases": [
                        "светлейший князь",
                        "титул князя",
                        "род князей"
                    ],
                    "target_lang_definitions": {
                        "en": "A person holding a high title of nobility, which was hereditary or granted by a monarch."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prince",
                                "target_lang_sense_clarification": "noble title"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2c78b1e-c7c0-3d3c-9e95-1084d3aef344",
                    "sense_definition": "В религиозных и литературных текстах: повелитель злых сил, дьявол.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В литературе дьявола часто называют <w>князем</w> тьмы.",
                            "target_lang_translations": {
                                "en": "In literature, the devil is often called the <w>Prince</w> of Darkness."
                            }
                        },
                        {
                            "text": "Гордыня заставила его возомнить себя <w>князем</w> мира сего.",
                            "target_lang_translations": {
                                "en": "Pride made him imagine himself the <w>prince</w> of this world."
                            }
                        }
                    ],
                    "synonyms": [
                        "дьявол",
                        "сатана"
                    ],
                    "common_phrases": [
                        "князь тьмы",
                        "князь мира сего",
                        "князь бесовский"
                    ],
                    "target_lang_definitions": {
                        "en": "In religious and literary texts: the ruler of evil forces, the devil."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prince",
                                "target_lang_sense_clarification": "in 'Prince of Darkness'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dd9b9d7-4e41-3ef5-b069-73d613c03168",
                    "sense_definition": "В народной поэзии и старинных свадебных обрядах: жених.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "folklore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости встречали молодых, величая жениха <w>князем</w>, а невесту княгиней.",
                            "target_lang_translations": {
                                "en": "The guests welcomed the newlyweds, hailing the groom as a <w>prince</w> and the bride as a princess."
                            }
                        },
                        {
                            "text": "Едет <w>князь</w> молодой за своей суженой на тройке лошадей.",
                            "target_lang_translations": {
                                "en": "The young <w>prince</w> is riding for his betrothed on a troika of horses."
                            }
                        }
                    ],
                    "synonyms": [
                        "жених",
                        "новобрачный"
                    ],
                    "common_phrases": [
                        "князь молодой"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "poetic folklore usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In folk poetry and ancient wedding rituals: the bridegroom."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prince",
                                "target_lang_sense_clarification": "poetic term for a groom"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
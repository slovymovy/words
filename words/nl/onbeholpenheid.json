            "senses": [
                {
                    "sense_id": "2c301c04-6ec1-3282-b12f-3241b79c63cc",
                    "sense_definition": "De eigenschap of toestand van onhandig en niet geoefend zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>onbeholpenheid</w> zorgde ervoor dat hij het glas omstootte.",
                            "target_lang_translations": {
                                "en": "His <w>clumsiness</w> caused him to knock over the glass.",
                                "ru": "Из-за своей <w>неуклюжести</w> он опрокинул стакан."
                            }
                        },
                        {
                            "text": "Ondanks haar <w>onbeholpenheid</w> probeerde ze het probleem op te lossen.",
                            "target_lang_translations": {
                                "en": "Despite her <w>ineptitude</w>, she tried to solve the problem.",
                                "ru": "Несмотря на свою <w>неловкость</w>, она пыталась решить проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "onhandigheid"
                    ],
                    "antonyms": [
                        "behendigheid",
                        "vaardigheid"
                    ],
                    "common_phrases": [
                        "met enige onbeholpenheid",
                        "sociale onbeholpenheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality or state of being clumsy and unpracticed.",
                        "ru": "Свойство или состояние неуклюжести и отсутствия опыта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clumsiness",

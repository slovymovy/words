{
    "word_family": [
        "parental",
        "parenthood",
        "grandparent",
        "stepparent",
        "godparent",
        "unparented"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "04a91bd6-8b71-3784-8861-024d9129b4aa",
                    "sense_definition": "A father or mother; a person who has a child or raises a child.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>parents</w> were very proud of her achievements at school.",
                            "target_lang_translations": {
                                "ru": "Её <w>родители</w> очень гордились её успехами в школе."
                            }
                        },
                        {
                            "text": "Being a single <w>parent</w> can be challenging but rewarding.",
                            "target_lang_translations": {
                                "ru": "Быть <w>родителем</w>-одиночкой может быть трудно, но это того стоит."
                            }
                        },
                        {
                            "text": "The teacher asked the students to have a <w>parent</w> sign the permission slip.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников, чтобы <w>родитель</w> подписал разрешение."
                            }
                        }
                    ],
                    "synonyms": [
                        "mother",
                        "progenitor"
                    ],
                    "antonyms": [
                        "child",
                        "offspring"
                    ],
                    "common_phrases": [
                        "single parent",
                        "foster parent",
                        "biological parent",
                        "adoptive parent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отец или мать; человек, у которого есть ребенок или который воспитывает ребенка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родитель",
                                "target_lang_sense_clarification": "отец или мать"
                            },
                            {
                                "target_lang_word": "родительница",
                                "target_lang_sense_clarification": "мать (часто ирон. или устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f27d99b4-9702-36af-a82f-281b48f8ff5e",
                    "sense_definition": "A company or organization that owns or controls a smaller subsidiary company.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>parent</w> company decided to merge two of its smaller branches.",
                            "target_lang_translations": {
                                "ru": "<w>Материнская</w> компания решила объединить два своих меньших филиала."
                            }
                        },
                        {
                            "text": "Employees were transferred to the headquarters of the <w>parent</w> firm.",
                            "target_lang_translations": {
                                "ru": "Сотрудников перевели в штаб-квартиру <w>головной</w> фирмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parent company",
                        "parent firm",
                        "parent organization"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания или организация, которая владеет или контролирует меньшую дочернюю компанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материнская компания",
                                "target_lang_sense_clarification": "компания-владелец"
                            },
                            {
                                "target_lang_word": "головная организация",
                                "target_lang_sense_clarification": "главная фирма в холдинге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a91fcd0-e044-35ab-8005-8c5be118cc1a",
                    "sense_definition": "An organism, plant, or animal from which another is biologically descended.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>parent</w> plant was resistant to the disease, passing this trait to the seedlings.",
                            "target_lang_translations": {
                                "ru": "<w>Родительское</w> растение было устойчиво к болезни, передав эту черту саженцам."
                            }
                        },
                        {
                            "text": "Genetic traits are inherited from each <w>parent</w> organism.",
                            "target_lang_translations": {
                                "ru": "Генетические черты наследуются от каждого <w>родительского</w> организма."
                            }
                        }
                    ],
                    "synonyms": [
                        "progenitor"
                    ],
                    "antonyms": [
                        "offspring"
                    ],
                    "common_phrases": [
                        "parent cell",
                        "parent plant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организм, растение или животное, от которого биологически происходит другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родитель",
                                "target_lang_sense_clarification": "биологический предок"
                            },
                            {
                                "target_lang_word": "родительский организм",
                                "target_lang_sense_clarification": "исходный организм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c2c0440-cef7-3d5c-bdcc-085898a0510a",
                    "sense_definition": "The source, origin, or cause of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is often said that necessity is the <w>parent</w> of invention.",
                            "target_lang_translations": {
                                "ru": "Часто говорят, что необходимость — <w>мать</w> изобретения."
                            }
                        },
                        {
                            "text": "Poverty can be the <w>parent</w> of many social issues.",
                            "target_lang_translations": {
                                "ru": "Бедность может быть <w>источником</w> многих социальных проблем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parent of invention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Источник, происхождение или причина чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "начало, исток"
                            },
                            {
                                "target_lang_word": "причина",
                                "target_lang_sense_clarification": "основание, повод"
                            },
                            {
                                "target_lang_word": "прародитель",
                                "target_lang_sense_clarification": "перен. создатель"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2a48564-18b0-3017-8f6a-3b2a36a856f2",
                    "sense_definition": "To act as a mother or father; to raise and care for a child.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to co-<w>parent</w> their children amicably after the separation.",
                            "target_lang_translations": {
                                "ru": "Они решили совместно <w>воспитывать</w> своих детей мирно после расставания."
                            }
                        },
                        {
                            "text": "It takes a lot of patience and energy to <w>parent</w> a toddler.",
                            "target_lang_translations": {
                                "ru": "Требуется много терпения и энергии, чтобы <w>растить</w> малыша."
                            }
                        }
                    ],
                    "synonyms": [
                        "raise",
                        "rear"
                    ],
                    "common_phrases": [
                        "co-parent",
                        "active parenting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять обязанности матери или отца; растить и заботиться о ребенке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспитывать",
                                "target_lang_sense_clarification": "растить ребенка"
                            },
                            {
                                "target_lang_word": "быть родителем",
                                "target_lang_sense_clarification": "исполнять родительскую роль"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9ff0df5f-997e-3838-8d4c-49cf6876f1a4",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Parent</w> signed the guestbook at the hotel reception.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Пэрент</w> расписался в гостевой книге на стойке регистрации отеля."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Parent</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Пэрент</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "Used as a family name."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пэрент",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
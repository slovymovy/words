    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc06ec62-eeed-3d90-8ad5-51a6b15b6e80",
                    "sense_definition": "Переместить кого-либо или что-либо волоком или с усилием на какое-то расстояние.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы с большим трудом <протащили> тяжёлый диван через весь коридор.",
                            "target_lang_translations": {
                                "en": "We <w>dragged</w> the heavy sofa through the entire hallway with great difficulty."
                            }
                        },
                        {
                            "text": "Раненый солдат <протащил> своего товарища на себе до безопасного места.",
                            "target_lang_translations": {
                                "en": "The wounded soldier <w>dragged</w> his comrade on his back to a safe place."
                            }
                        }
                    ],
                    "synonyms": [
                        "проволочь",
                        "пронести"
                    ],
                    "common_phrases": [
                        "протащить на себе",
                        "протащить по земле",
                        "еле протащить"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something by dragging or with effort over a certain distance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "move something along the ground"
                            },
                            {
                                "target_lang_word": "haul",
                                "target_lang_sense_clarification": "pull or drag with effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ed4f1c8-6574-318a-be97-d9d5f085b19a",
                    "sense_definition": "Продеть или просунуть что-либо сквозь узкое отверстие, щель или тесное пространство.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Электрик смог аккуратно <протащить> новый кабель через отверстие в стене.",
                            "target_lang_translations": {
                                "en": "The electrician was able to carefully <w>pull</w> the new cable <w>through</w> the hole in the wall."
                            }
                        },
                        {
                            "text": "Тебе нужно <протащить> нитку через игольное ушко, чтобы зашить рубашку.",
                            "target_lang_translations": {
                                "en": "You need to <w>thread</w> the thread through the needle's eye to sew the shirt."
                            }
                        }
                    ],
                    "synonyms": [
                        "продеть",
                        "просунуть"
                    ],
                    "common_phrases": [
                        "протащить кабель",
                        "протащить через отверстие",
                        "протащить нитку"
                    ],
                    "target_lang_definitions": {
                        "en": "To thread, push, or pull something through a narrow opening or tight space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thread",
                                "target_lang_sense_clarification": "pass through a narrow hole"
                            },
                            {
                                "target_lang_word": "pull through",
                                "target_lang_sense_clarification": "draw something through an opening"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04de0e07-379c-3033-b8a9-b194ec4f0659",
                    "sense_definition": "Добиться принятия решения, утверждения закона или назначения кандидата, преодолевая сопротивление или используя хитрость.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Депутатам удалось <протащить> спорный законопроект в первом чтении.",
                            "target_lang_translations": {
                                "en": "The deputies managed to <w>push through</w> the controversial bill in the first reading."
                            }
                        },
                        {
                            "text": "Директор пытался <протащить> своего племянника на высокую должность в компании.",
                            "target_lang_translations": {
                                "en": "The director tried to <w>push</w> his nephew <w>into</w> a high position in the company."
                            }
                        }
                    ],
                    "synonyms": [
                        "продвинуть",
                        "провести"
                    ],
                    "antonyms": [
                        "провалить"
                    ],
                    "common_phrases": [
                        "протащить закон",
                        "протащить решение",
                        "протащить кандидата"
                    ],
                    "target_lang_definitions": {
                        "en": "To succeed in getting a decision made, a law passed, or a person appointed, often despite opposition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "push through",
                                "target_lang_sense_clarification": "force the acceptance of a plan or law"
                            },
                            {
                                "target_lang_word": "railroad",
                                "target_lang_sense_clarification": "rush or force through quickly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
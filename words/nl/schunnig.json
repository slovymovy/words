    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6c186d8f-bc71-371f-a5d6-c78b6ff8dde1",
                    "sense_definition": "Grof, onzedelijk of seksueel getint, vaak op een manier die als onfatsoenlijk wordt beschouwd.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "obscene",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertelde de hele avond <w>schunnige</w> moppen waar iedereen hard om moest lachen.",
                            "target_lang_translations": {
                                "ru": "Он весь вечер рассказывал <w>сальные</w> анекдоты, над которыми все громко смеялись."
                            }
                        },
                        {
                            "text": "De tekst van dat liedje is eigenlijk best <w>schunnig</w> als je goed naar de woorden luistert.",
                            "target_lang_translations": {
                                "ru": "Текст этой песни на самом деле довольно <w>непристойный</w>, если вслушаться в слова."
                            }
                        }
                    ],
                    "synonyms": [
                        "obsceen",
                        "schuin"
                    ],
                    "common_phrases": [
                        "schunnige moppen",
                        "schunnige taal",
                        "schunnige opmerkingen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубый, непристойный или с сексуальным подтекстом, часто в манере, считающейся неподобающей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непристойный",
                                "target_lang_sense_clarification": "общий термин для неприличного поведения или слов"
                            },
                            {
                                "target_lang_word": "сальный",
                                "target_lang_sense_clarification": "часто используется для описания шуток или анекдотов"
                            },
                            {
                                "target_lang_word": "скабрезный",
                                "target_lang_sense_clarification": "рискованный, неприличный (о речи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df8b9422-0f7b-3726-995d-e42dea8cc6f6",
                    "sense_definition": "Moreel verwerpelijk, gemeen of oneerlijk in gedrag of karakter.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mean",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een <w>schunnige</w> streek om iemand met schulden zo uit te buiten.",
                            "target_lang_translations": {
                                "ru": "Это <w>подлая</w> выходка — так эксплуатировать человека с долгами."
                            }
                        },
                        {
                            "text": "Zijn gedrag tegenover zijn collega's was ronduit <w>schunnig</w> en onacceptabel.",
                            "target_lang_translations": {
                                "ru": "Его поведение по отношению к коллегам было просто <w>гнусным</w> и неприемлемым."
                            }
                        }
                    ],
                    "synonyms": [
                        "gemeen",
                        "infaam",
                        "laaghartig",
                        "vuig"
                    ],
                    "common_phrases": [
                        "een schunnige streek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морально предосудительный, подлый или бесчестный в поведении или характере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подлый",
                                "target_lang_sense_clarification": "низкий, бесчестный поступок"
                            },
                            {
                                "target_lang_word": "гнусный",
                                "target_lang_sense_clarification": "вызывающий отвращение, омерзительный"
                            },
                            {
                                "target_lang_word": "низкий",
                                "target_lang_sense_clarification": "недостойный, аморальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2385c8dd-c041-3d17-9fd9-a987daeab062",
                    "sense_definition": "Armoedig, haveloos of versleten, vooral met betrekking tot kleding of uiterlijk.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shabby",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zwerver liep in een <w>schunnige</w> jas door de koude regen.",
                            "target_lang_translations": {
                                "ru": "Бродяга шёл под холодным дождём в <w>потрёпанном</w> пальто."
                            }
                        },
                        {
                            "text": "In de hoek van de kamer stond een <w>schunnig</w> kastje dat bijna uit elkaar viel.",
                            "target_lang_translations": {
                                "ru": "В углу комнаты стоял <w>убогий</w> шкафчик, который почти разваливался."
                            }
                        }
                    ],
                    "synonyms": [
                        "belabberd",
                        "ellendig",
                        "miserabel",
                        "schamel"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "vooral gebruikt in oudere teksten"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убогий, ветхий или поношенный, особенно в отношении одежды или внешнего вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потрёпанный",

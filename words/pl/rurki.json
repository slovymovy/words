            "senses": [
                {
                    "sense_id": "246deab5-95ce-3294-97ce-75f82fbdcb62",
                    "sense_definition": "Forma fleksyjna słowa <w>rurka</w> (dopełniacz liczby pojedynczej lub mianownik, biernik, wołacz liczby mnogiej).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci uwielbiają słodkie <w>rurki</w> z bitą śmietaną kupowane w cukierni.",
                            "target_lang_translations": {
                                "ru": "Дети обожают сладкие <w>трубочки</w> со взбитыми сливками, купленные в кондитерской."
                            }
                        },
                        {
                            "text": "Kelner przyniósł nam kolorowe <w>rurki</w> do picia soku.",
                            "target_lang_translations": {
                                "ru": "Официант принес нам цветные <w>трубочки</w> для сока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rurki z kremem",
                        "plastikowe rurki",
                        "metalowe rurki"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Флективная форма слова rurka (трубка, трубочка); часто используется для обозначения кондитерских изделий или соломинок для питья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубочки",
                                "target_lang_sense_clarification": "кондитерские или для питья"
                            },
                            {
                                "target_lang_word": "трубки",
                                "target_lang_sense_clarification": "маленькие трубы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7839772e-0a4e-36e9-98c8-d9343ffc0ab6",
                    "sense_definition": "Obcisłe spodnie z wąskimi, przylegającymi do ciała nogawkami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważam, że tylko szczupłe dziewczyny wyglądają dobrze w dżinsowych <w>rurkach</w>.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что только стройные девушки хорошо выглядят в джинсовых <w>узких брюках</w>."
                            }
                        },
                        {
                            "text": "Na koncert rockowy chętnie zakładam czarne <w>rurki</w> i skórzaną kurtkę.",
                            "target_lang_translations": {
                                "ru": "На рок-концерт я охотно надеваю черные <w>брюки-дудочки</w> и кожаную куртку."
                            }
                        }
                    ],
                    "synonyms": [
                        "slimki",
                        "rury"
                    ],
                    "common_phrases": [
                        "dżinsowe rurki",
                        "czarne rurki",
                        "nosić rurki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Облегающие брюки с очень узкими штанинами."
                    },

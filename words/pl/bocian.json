    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "428e9aa0-a966-3ec6-8d1e-a04f49c8d11b",
                    "sense_definition": "Duży, wędrowny ptak brodzący o białym upierzeniu z czarnymi lotkami, czerwonym dziobie i długich nogach, często gniazdujący w pobliżu siedzib ludzkich.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wysokim słupie energetycznym <w>bocian</w> zbudował swoje gniazdo.",
                            "target_lang_translations": {
                                "ru": "На высоком электрическом столбе <w>аист</w> свил своё гнездо."
                            }
                        },
                        {
                            "text": "Każdej wiosny z niecierpliwością wypatrujemy, kiedy przyleci pierwszy <w>bocian</w>.",
                            "target_lang_translations": {
                                "ru": "Каждой весной мы с нетерпением высматриваем, когда прилетит первый <w>аист</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bociek"
                    ],
                    "common_phrases": [
                        "bocian biały",
                        "gniazdo bociana",
                        "klekot bociana",
                        "czerwone jak nogi bociana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная перелётная птица с белым оперением, чёрными маховыми перьями, красным клювом и длинными ногами, часто гнездящаяся вблизи человеческого жилья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аист",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5805cb14-6791-3136-ad15-44df87f6e029",
                    "sense_definition": "W kulturze ludowej i opowieściach dla dzieci: postać ptaka, który rzekomo przynosi rodzicom nowo narodzone dzieci.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "folklore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy byłem mały, rodzice powiedzieli mi, że <w>bocian</w> przyniósł mnie w dziobie.",
                            "target_lang_translations": {
                                "ru": "Когда я был маленьким, родители сказали мне, что <w>аист</w> принёс меня в клюве."
                            }
                        },
                        {
                            "text": "To tylko stara legenda, że <w>bocian</w> przynosi dzieci.",
                            "target_lang_translations": {
                                "ru": "Это всего лишь старая легенда, что <w>аист</w> приносит детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "bociek"
                    ],
                    "common_phrases": [
                        "bocian przyniósł dziecko",
                        "wierzyć w bociana"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in humorous or childish context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В народной культуре и детских сказках: птица, которая якобы приносит родителям новорожденных детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аист",
                                "target_lang_sense_clarification": "в фольклоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9125d31-6a6c-3098-8878-26e32577ea8d",
                    "sense_definition": "Dawne narzędzie tortur, które krępowało szyję, ręce i stopy ofiary, wymuszając nienaturalną, skuloną pozycję ciała.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum średniowiecznego prawa karnego zobaczyliśmy żelaznego <w>bociana</w>.",
                            "target_lang_translations": {
                                "ru": "В музее средневекового уголовного права мы увидели железного <w>аиста</w>."
                            }
                        },
                        {
                            "text": "Więzień został zakuty w <w>bociana</w>, co uniemożliwiło mu jakikolwiek ruch.",
                            "target_lang_translations": {
                                "ru": "Заключённого заковали в <w>аиста</w>, что лишило его возможности двигаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "córka śmieciarza"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное орудие пыток, сковывавшее шею, руки и ноги жертвы, вынуждая тело принимать неестественную, скрюченную позу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аист",
                                "target_lang_sense_clarification": "орудие пыток"
                            }
                        ]
                    }
                }
            ]
        },
        {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1655d66f-0de4-3a3b-8ef6-0596b057691f",
                    "sense_definition": "Een geldbedrag dat men moet betalen als straf voor een overtreding",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_penalty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kreeg een <w>boete</w> omdat ik te hard reed met de auto.",
                            "target_lang_translations": {
                                "en": "I got a <w>fine</w> because I was driving too fast.",
                                "ru": "Я получил <w>штраф</w> за превышение скорости на автомобиле."
                            }
                        },
                        {
                            "text": "Je moet de openstaande <w>boete</w> binnen twee weken betalen.",
                            "target_lang_translations": {
                                "en": "You must pay the outstanding <w>fine</w> within two weeks.",
                                "ru": "Ты должен оплатить неоплаченный <w>штраф</w> в течение двух недель."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekeuring",
                        "geldboete"
                    ],
                    "common_phrases": [
                        "een boete krijgen",
                        "een boete betalen",
                        "een boete uitschrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "A sum of money that must be paid as punishment for breaking a law or rule",
                        "ru": "Денежная сумма, которую необходимо выплатить в качестве наказания за нарушение"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "financial punishment"
                            },
                            {
                                "target_lang_word": "penalty",
                                "target_lang_sense_clarification": "punishment for violation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "штраф",
                                "target_lang_sense_clarification": "денежное взыскание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "630b6656-4b08-3b40-b9ac-c4df959130a8",
                    "sense_definition": "Een straf of handeling waarmee men spijt betuigt voor een begane zonde of fout",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "penance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monnik wilde <w>boete</w> doen voor zijn zonden in het verleden.",
                            "target_lang_translations": {
                                "en": "The monk wanted to do <w>penance</w> for his past sins.",
                                "ru": "Монах хотел принести <w>покаяние</w> за свои прошлые грехи."
                            }
                        },
                        {
                            "text": "Zij hebben publiekelijk <w>boete</w> gedaan voor hun daden tijdens de oorlog.",
                            "target_lang_translations": {
                                "en": "They publicly did <w>penance</w> for their actions during the war.",
                                "ru": "Они публично принесли <w>покаяние</w> за свои действия во время войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "boetedoening"
                    ],
                    "common_phrases": [
                        "boete doen"
                    ],
                    "target_lang_definitions": {
                        "en": "A punishment or act performed to express regret for a sin or wrongdoing",
                        "ru": "Наказание или действие, выражающее раскаяние за совершенный грех или ошибку"
                    },
                    "translations": {

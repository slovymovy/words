    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a2c196ac-b6ff-3794-a216-bb57e898a3ff",
                    "sense_definition": "Laten zien hoe iets werkt of hoe je een bepaalde handeling moet uitvoeren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_show_prove",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verkoper <w>demonstreerde</w> hoe de nieuwe stofzuiger precies werkt.",
                            "target_lang_translations": {
                                "en": "The salesperson <w>demonstrated</w> exactly how the new vacuum cleaner works.",
                                "ru": "Продавец <w>продемонстрировал</w>, как именно работает новый пылесос."
                            }
                        },
                        {
                            "text": "Kun jij even <w>demonstreren</w> hoe ik deze app op mijn telefoon moet installeren?",
                            "target_lang_translations": {
                                "en": "Can you just <w>demonstrate</w> how I should install this app on my phone?",
                                "ru": "Можешь <w>показать</w>, как мне установить это приложение на телефон?"
                            }
                        }
                    ],
                    "synonyms": [
                        "voordoen",
                        "tonen",
                        "laten zien"
                    ],
                    "common_phrases": [
                        "een werking demonstreren",
                        "een techniek demonstreren",
                        "een product demonstreren"
                    ],
                    "target_lang_definitions": {
                        "en": "To show how something works or how to perform a specific action.",
                        "ru": "Показать, как что-то работает или как выполнить определенное действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "show how something works"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "perform an action for others to see"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "показывать работу устройства или техники"
                            },
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "наглядно объяснять действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca88755c-0d7d-3ea1-975d-3e426e8ca1a8",
                    "sense_definition": "Meedoen aan een openbare bijeenkomst of optocht om een mening te uiten of ergens tegen te protesteren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_protest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Duizenden mensen <w>demonstreerden</w> gisteren in de hoofdstad tegen de oorlog.",
                            "target_lang_translations": {
                                "en": "Thousands of people <w>demonstrated</w> in the capital yesterday against the war.",
                                "ru": "Тысячи людей <w>вышли на демонстрацию</w> вчера в столице против войны."
                            }
                        },
                        {
                            "text": "Wij gaan volgende week vreedzaam <w>demonstreren</w> voor een beter klimaatbeleid.",
                            "target_lang_translations": {
                                "en": "We are going to <w>demonstrate</w> peacefully next week for a better climate policy.",
                                "ru": "На следующей неделе мы собираемся мирно <w>протестовать</w> за лучшую климатическую политику."
                            }
                        }
                    ],
                    "synonyms": [
                        "betogen",
                        "protesteren",
                        "actievoeren"
                    ],
                    "common_phrases": [
                        "demonstreren tegen onrecht",
                        "op straat demonstreren",
                        "vreedzaam demonstreren"
                    ],
                    "target_lang_definitions": {
                        "en": "To participate in a public gathering or march to express an opinion or protest against something.",
                        "ru": "Участвовать в публичном собрании или шествии для выражения мнения или протеста."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "protest publicly"
                            },
                            {
                                "target_lang_word": "protest",
                                "target_lang_sense_clarification": "express objection publicly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "протестовать",
                                "target_lang_sense_clarification": "выражать несогласие публично"
                            },
                            {
                                "target_lang_word": "участвовать в демонстрации",
                                "target_lang_sense_clarification": "выходить на митинг или шествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5cd1ff7-ce37-309a-81f6-2044aef28592",
                    "sense_definition": "Met bewijzen of argumenten aantonen dat een stelling of theorie juist is.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_show_prove",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wetenschapper <w>demonstreerde</w> met dit experiment dat zijn hypothese klopte.",
                            "target_lang_translations": {
                                "en": "The scientist <w>demonstrated</w> with this experiment that his hypothesis was correct.",
                                "ru": "Этим экспериментом ученый <w>доказал</w>, что его гипотеза была верна."
                            }
                        },
                        {
                            "text": "Deze cijfers <w>demonstreren</w> duidelijk de noodzaak van een nieuwe aanpak.",
                            "target_lang_translations": {
                                "en": "These figures clearly <w>demonstrate</w> the need for a new approach.",
                                "ru": "Эти цифры ясно <w>демонстрируют</w> необходимость нового подхода."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewijzen",
                        "aantonen"
                    ],
                    "antonyms": [
                        "weerleggen"
                    ],
                    "common_phrases": [
                        "een stelling demonstreren",
                        "het ongelijk demonstreren"
                    ],
                    "target_lang_definitions": {
                        "en": "To show that a statement or theory is correct using evidence or arguments.",
                        "ru": "Доказать с помощью фактов или аргументов, что утверждение или теория верны."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "show validity with evidence"
                            },
                            {
                                "target_lang_word": "prove",
                                "target_lang_sense_clarification": "establish truth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "доказывать",
                                "target_lang_sense_clarification": "подтверждать истинность фактами"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "наглядно показывать наличие чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "589e691a-8dbb-3afb-91ae-1285ebaad6af",
                    "sense_definition": "Machtsvertoon laten zien, vaak op militair gebied, om de vijand af te schrikken of te intimideren.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het leger <w>demonstreerde</w> zijn slagkracht door tanks naar de grens te sturen.",
                            "target_lang_translations": {
                                "en": "The army <w>demonstrated</w> its striking power by sending tanks to the border.",
                                "ru": "Армия <w>продемонстрировала</w> свою ударную мощь, отправив танки к границе."
                            }
                        },
                        {
                            "text": "De generaal liet de troepen <w>demonstreren</w> om de vijand te waarschuwen.",
                            "target_lang_translations": {
                                "en": "The general had the troops <w>demonstrate</w> to warn the enemy.",
                                "ru": "Генерал приказал войскам <w>провести демонстрацию силы</w>, чтобы предупредить врага."
                            }
                        }
                    ],
                    "traits": [

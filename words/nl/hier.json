            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f25f2566-bb4f-3e79-af87-54d339283fc6",
                    "sense_definition": "Op deze plaats; de plek waar de spreker zich bevindt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit restaurant ziet er leuk uit, zullen we <w>hier</w> eten?",
                            "target_lang_translations": {
                                "en": "This restaurant looks nice, shall we eat <w>here</w>?",
                                "ru": "Этот ресторан выглядит неплохо, давай поедим <w>здесь</w>?"
                            }
                        },
                        {
                            "text": "<w>Hier</w> wordt goud gevonden in de rivier.",
                            "target_lang_translations": {
                                "en": "Gold is found <w>here</w> in the river.",
                                "ru": "<w>Здесь</w> в реке находят золото."
                            }
                        },
                        {
                            "text": "Kom <w>hier</w> en help me even met deze zware doos.",
                            "target_lang_translations": {
                                "en": "Come <w>here</w> and help me with this heavy box.",
                                "ru": "Иди <w>сюда</w> и помоги мне с этой тяжелой коробкой."
                            }
                        }
                    ],
                    "antonyms": [
                        "daar",
                        "elders"
                    ],
                    "common_phrases": [
                        "hier en daar",
                        "kom hier",
                        "tot hier"
                    ],
                    "target_lang_definitions": {
                        "en": "At this place; the location where the speaker is.",
                        "ru": "На этом месте; место, где находится говорящий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "here",
                                "target_lang_sense_clarification": "location"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "здесь",
                                "target_lang_sense_clarification": "на этом месте"
                            },
                            {
                                "target_lang_word": "тут",
                                "target_lang_sense_clarification": "разг. здесь"
                            },
                            {
                                "target_lang_word": "сюда",
                                "target_lang_sense_clarification": "в это место (при движении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07dda615-4b32-305d-a677-9a1ed1cc1513",
                    "sense_definition": "Deel van een voornaamwoordelijk bijwoord dat verwijst naar 'dit' of 'deze' (in combinatie met een voorzetsel).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronominal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Meneer, u moet <w>hier</w> nog voor betalen!",
                            "target_lang_translations": {
                                "en": "Sir, you still have to pay for <w>this</w>!",
                                "ru": "Господин, вы должны еще заплатить за <w>это</w>!"
                            }
                        },
                        {
                            "text": "<w>Hier</w> gaat niets boven, dit is echt het beste.",
                            "target_lang_translations": {
                                "en": "Nothing beats <w>this</w>, this is really the best.",
                                "ru": "Ничто не сравнится с <w>этим</w>, это действительно лучшее."
                            }
                        },
                        {
                            "text": "Ik ben het <w>hier</w> helemaal niet mee eens.",
                            "target_lang_translations": {
                                "en": "I don't agree with <w>this</w> at all.",
                                "ru": "Я совершенно с <w>этим</w> не согласен."
                            }
                        }
                    ],
                    "antonyms": [
                        "daar"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pronominal adverb part"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Part of a pronominal adverb referring to 'this' or 'these' (used with a preposition).",
                        "ru": "Часть местоименного наречия, которая ссылается на «это» (в сочетании с предлогом)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "this",
                                "target_lang_sense_clarification": "referring to a specific thing or situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "указание на предмет или ситуацию"
                            }
                        ]
                    }
                }
            ]
        }

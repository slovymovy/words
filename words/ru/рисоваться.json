    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "49c25a4d-edb9-35e8-8f2f-acd60556365f",
                    "sense_definition": "Страдательная форма от глагола <w>рисовать</w> (создаваться, изображаться кем-либо).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта картина <рисуется> маслом уже несколько месяцев.",
                            "target_lang_translations": {
                                "en": "This picture <is being painted> in oils for several months already."
                            }
                        },
                        {
                            "text": "Портрет <рисуется> известным художником по заказу музея.",
                            "target_lang_translations": {
                                "en": "The portrait <is being painted> by a famous artist commissioned by the museum."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be created or depicted by someone (passive voice of 'to draw' or 'to paint')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to be painted",
                                "target_lang_sense_clarification": "regarding paintings or art created with paint"
                            },
                            {
                                "target_lang_word": "to be drawn",
                                "target_lang_sense_clarification": "regarding sketches or drawings"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b64c5e25-d7f2-31af-a494-d1b7fc954f30",
                    "sense_definition": "Вести себя неестественно, жеманно, стараясь произвести впечатление и привлечь к себе внимание.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он слишком много <рисуется> перед новыми знакомыми.",
                            "target_lang_translations": {
                                "en": "He <shows off> too much in front of new acquaintances."
                            }
                        },
                        {
                            "text": "Перестань <рисоваться> и скажи правду.",
                            "target_lang_translations": {
                                "en": "Stop <showing off> and tell the truth."
                            }
                        }
                    ],
                    "common_phrases": [
                        "любить рисоваться",
                        "хватит рисоваться",
                        "рисоваться перед публикой"
                    ],
                    "target_lang_definitions": {
                        "en": "To behave in an affected, unnatural manner to impress others or attract attention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to show off",
                                "target_lang_sense_clarification": "to display oneself to attract attention"
                            },
                            {
                                "target_lang_word": "to pose",
                                "target_lang_sense_clarification": "to assume an attitude or posture"
                            },
                            {
                                "target_lang_word": "to put on airs",
                                "target_lang_sense_clarification": "idiomatic; to act superior or affected"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d0b22f4-494d-3fe6-a3bd-92f90b31c6ff",
                    "sense_definition": "Виднеться, выделяться своими очертаниями на каком-либо фоне.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вдали на холме смутно <рисовался> старый замок.",
                            "target_lang_translations": {
                                "en": "In the distance on the hill, an old castle vaguely <loomed>."
                            }
                        },
                        {
                            "text": "На фоне светлого неба чётко <рисуются> верхушки деревьев.",
                            "target_lang_translations": {
                                "en": "Against the light sky, the treetops <stand out> clearly."
                            }
                        }
                    ],
                    "synonyms": [
                        "виднеться",
                        "вырисовываться"
                    ],
                    "common_phrases": [
                        "рисоваться на фоне",
                        "четко рисоваться",
                        "смутно рисоваться"
                    ],
                    "target_lang_definitions": {
                        "en": "To be visible in outline against a background."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to stand out",
                                "target_lang_sense_clarification": "to be clearly visible against a background"
                            },
                            {
                                "target_lang_word": "to be silhouetted",
                                "target_lang_sense_clarification": "to appear as a dark shape against a light background"
                            },
                            {
                                "target_lang_word": "to loom",
                                "target_lang_sense_clarification": "to appear indistinctly, often as a large shape"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1e247b7-0e7b-3f39-a0bf-d68a24d0ca90",
                    "sense_definition": "Возникать, представляться в мыслях или воображении.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему <рисуется> счастливое и беззаботное будущее.",
                            "target_lang_translations": {
                                "en": "A happy and carefree future <appears> to him."
                            }
                        },
                        {
                            "text": "В моём воображении <рисовались> страшные картины последствий аварии.",
                            "target_lang_translations": {
                                "en": "Terrible pictures of the accident's consequences <arose> in my imagination."
                            }
                        }
                    ],
                    "synonyms": [
                        "представляться",
                        "видеться"
                    ],
                    "common_phrases": [
                        "рисоваться в воображении",
                        "будущее рисуется"
                    ],
                    "target_lang_definitions": {
                        "en": "To appear or present itself to the mind or imagination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to appear",
                                "target_lang_sense_clarification": "to come into one's mind"
                            },
                            {
                                "target_lang_word": "to be pictured",
                                "target_lang_sense_clarification": "to be visualized in the imagination"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "41128a2c-2b91-30c9-9044-5bc6a734383e",
                    "sense_definition": "Rock or other material that contains valuable or utilitarian materials; primarily a rock containing metals or gems for which it is typically mined and processed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mineral_resource",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company plans to mine iron <w>ore</w> from the new site next year.",
                            "target_lang_translations": {
                                "ru": "Компания планирует добывать железную <w>руду</w> на новом месторождении в следующем году."
                            }
                        },
                        {
                            "text": "Miners extract the raw <w>ore</w> and transport it to a factory for processing.",
                            "target_lang_translations": {
                                "ru": "Шахтёры добывают сырую <w>руду</w> и транспортируют её на завод для переработки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iron ore",
                        "copper ore",
                        "gold ore",
                        "ore deposit",
                        "raw ore"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горная порода или другой материал, содержащий ценные или полезные компоненты; преимущественно порода, содержащая металлы или драгоценные камни, которую добывают и перерабатывают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руда",
                                "target_lang_sense_clarification": "полезное ископаемое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac98037c-cf01-3262-9433-369544041bff",
                    "sense_definition": "A unit of currency used in England around the 10th to 12th centuries.",

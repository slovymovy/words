                },
                {
                    "sense_id": "88e5cfb1-7ac5-3cc2-8b7a-b5e8f7ad21dc",
                    "sense_definition": "Niedoskonałość, usterka lub słaba strona przedmiotu, osoby lub planu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek ma jakiś <w>brak</w>, nad którym powinien pracować.",

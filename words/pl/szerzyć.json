                {
                    "sense_id": "6e7b0a80-22aa-32ae-848e-139b76def1c3",
                    "sense_definition": "Popularyzować coś; sprawiać, że coś staje się coraz bardziej znane i powszechne (np. idee, informacje, nastroje).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rozprzestrzenianie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Janosik <w>szerzył</w> postrach wśród austriackiego żołdactwa.",
                            "target_lang_translations": {
                                "ru": "Яношик <w>сеял</w> страх среди австрийских солдат."
                            }
                        },
                        {
                            "text": "Misjonarze wyjeżdżali do odległych krajów, aby <w>szerzyć</w> swoją wiarę.",
                            "target_lang_translations": {
                                "ru": "Миссионеры уезжали в далекие страны, чтобы <w>распространять</w> свою веру."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpowszechniać",
                        "rozprzestrzeniać",
                        "propagować",
                        "głosić",
                        "popularyzować"
                    ],
                    "common_phrases": [
                        "szerzyć oświatę",
                        "szerzyć propagandę",
                        "szerzyć plotki",
                        "szerzyć kulturę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо известным, доступным или принятым среди многих людей; способствовать распространению идей или информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространять",
                                "target_lang_sense_clarification": "информацию, веру, слухи"
                            },
                            {
                                "target_lang_word": "сеять",
                                "target_lang_sense_clarification": "панику, страх, сомнения"
                            },
                            {
                                "target_lang_word": "пропагандировать",
                                "target_lang_sense_clarification": "идеи, культуру, знания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dab150a6-a61e-3c12-a0d3-c501714b65a5",
                    "sense_definition": "Stawać się coraz bardziej powszechnym, obejmować coraz szerszy zasięg (często o zjawiskach negatywnych lub chorobach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rozprzestrzenianie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wśród mieszkańców miasta <w>szerzyły</w> się nastroje buntu.",
                            "target_lang_translations": {
                                "ru": "Среди жителей города <w>ширились</w> бунтарские настроения."
                            }
                        },
                        {
                            "text": "W szkole bardzo szybko <w>szerzy</w> się grypa.",
                            "target_lang_translations": {
                                "ru": "В школе очень быстро <w>распространяется</w> грипп."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozprzestrzeniać",
                        "roznosić"
                    ],
                    "common_phrases": [
                        "szerzy się zaraza",
                        "szerzy się panika",
                        "szerzy się korupcja"
                    ],
                    "traits": [
                        {

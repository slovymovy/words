                        "ru": [
                            {
                                "target_lang_word": "пускать ветры",
                                "target_lang_sense_clarification": "идиома, соответствует устаревшему оттенку"
                            },
                            {
                                "target_lang_word": "пукать",
                                "target_lang_sense_clarification": "разговорное"

{
    "word_family": [
        "такой",
        "также",
        "итак",
        "этак",
        "таковой",
        "затак"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "65c85f96-00bf-3b5e-99dc-d246b0b56841",
                    "sense_definition": "Указывает на способ действия или обстоятельство; таким образом, подобным способом.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Почему ты <так> себя ведёшь?"
                        },
                        {
                            "text": "Мы всегда делаем это именно <так>."
                        },
                        {
                            "text": "Комнаты в доме расположены <так>, что всем удобно."
                        }
                    ],
                    "synonyms": [
                        "таким образом"
                    ],
                    "antonyms": [
                        "иначе",
                        "по-другому"
                    ],
                    "common_phrases": [
                        "именно так",
                        "не так",
                        "всё так",
                        "сделай так"
                    ]
                },
                {
                    "sense_id": "48352c25-9b8e-397f-9994-649bb66db1c8",
                    "sense_definition": "Указывает на высокую степень проявления признака или действия; очень, настолько.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <так> устал после работы."
                        },
                        {
                            "text": "Это не <так> сложно, как кажется на первый взгляд."
                        },
                        {
                            "text": "Он <так> быстро бежал, что никто не мог его догнать."
                        }
                    ],
                    "synonyms": [
                        "очень",
                        "настолько",
                        "столь",
                        "сильно"
                    ],
                    "common_phrases": [
                        "так много",
                        "так быстро",
                        "так сильно",
                        "так долго"
                    ]
                },
                {
                    "sense_id": "5975d21b-1e94-3879-a675-b3e3dab5c883",
                    "sense_definition": "Указывает на сходство степени или образа действия при сравнении (обычно в конструкции «так ..., как ...»).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Никто не знает этот город <так>, как я."
                        },
                        {
                            "text": "Она выглядит <так> же красиво, как на фотографиях."
                        }
                    ],
                    "synonyms": [
                        "столь же",
                        "одинаково"
                    ],
                    "common_phrases": [
                        "так же как",
                        "не так как"
                    ]
                },
                {
                    "sense_id": "166ae943-a4d2-338e-a20a-6516df68e55f",
                    "sense_definition": "Без особой цели, причины или последствий; просто, даром.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gratuitous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я зашёл к тебе не по делу, а просто <так>."
                        },
                        {
                            "text": "Это тебе <так> не сойдёт с рук."
                        }
                    ],
                    "synonyms": [
                        "просто",
                        "даром"
                    ],
                    "common_phrases": [
                        "просто так"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "fe834afd-bb69-3b26-a003-d65563b82a1f",
                    "sense_definition": "Выражает согласие, подтверждение или готовность слушать дальше.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "confirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Так>, я вас внимательно слушаю."
                        },
                        {
                            "text": "Всё <так>, вы абсолютно правы."
                        }
                    ],
                    "synonyms": [
                        "хорошо",
                        "ладно",
                        "да",
                        "верно"
                    ],
                    "antonyms": [
                        "нет"
                    ],
                    "common_phrases": [
                        "так точно",
                        "всё так"
                    ]
                },
                {
                    "sense_id": "21dd7feb-5716-3abf-a83e-c0c96b6c3176",
                    "sense_definition": "Вводное слово, употребляется в начале фразы для перехода к объяснению, перечислению или выводу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "introductory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Так>, с этим вопросом мы разобрались."
                        },
                        {
                            "text": "<Так>, например, многие птицы улетают на юг."
                        }
                    ],
                    "synonyms": [
                        "итак",
                        "например"
                    ],
                    "common_phrases": [
                        "так вот",
                        "так например"
                    ]
                },
                {
                    "sense_id": "07e72c42-aaaf-3dd0-8ca3-6d975ea2d79c",
                    "sense_definition": "Указывает на приблизительное количество, время или меру (обычно ставится между существительным и числительным).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От вокзала до дома идти километров <так> пять."
                        },
                        {
                            "text": "Это случилось лет <так> десять назад."
                        }
                    ],
                    "synonyms": [
                        "примерно",
                        "около",
                        "приблизительно"
                    ],
                    "antonyms": [
                        "ровно",
                        "точно"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "ca7c0cee-a7bd-3293-b72b-48de0c401173",
                    "sense_definition": "Употребляется как уклончивый ответ, означающий «ничего особенного» или «неважно».",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dismissive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Что ты ищешь? — Да <так>, ерунду какую-то."
                        },
                        {
                            "text": "— О чём вы говорили? — <Так>, о погоде."
                        }
                    ],
                    "synonyms": [
                        "ничего"
                    ],
                    "common_phrases": [
                        "да так"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "591c4c98-2144-35f9-82b2-ec5d0785f266",
                    "sense_definition": "В таком случае, тогда (выражает следствие или условие).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раз обещал, <так> сделай."
                        },
                        {
                            "text": "Хочешь помочь, <так> помоги делом, а не словами."
                        }
                    ],
                    "synonyms": [
                        "тогда",
                        "в таком случае"
                    ],
                    "common_phrases": [
                        "раз так",
                        "коли так"
                    ]
                },
                {
                    "sense_id": "70f969f8-4f83-37f1-8428-83694783cc82",
                    "sense_definition": "Употребляется как вторая часть двойного союза «как..., так и...» для соединения однородных членов.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "correlation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это полезно как молодым, <так> и старым."
                        },
                        {
                            "text": "В магазине продаются как овощи, <так> и фрукты."
                        }
                    ],
                    "common_phrases": [
                        "как ... так и"
                    ]
                },
                {
                    "sense_id": "a1846267-c947-312f-9022-7ff862d87441",
                    "sense_definition": "Но, однако (выражает противопоставление).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я бы поехал, <так> денег нет."
                        },
                        {
                            "text": "Хотел купить хлеба, <так> магазин был закрыт."
                        }
                    ],
                    "synonyms": [
                        "но",
                        "однако"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c563b9c1-ab36-3fb9-bcf4-ba649c376583",
                    "sense_definition": "Не очень хороший, посредственный, среднего качества.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mediocre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фильм оказался <так>, на один раз."
                        },
                        {
                            "text": "По вкусу этот суп... ну <так>, средненько."
                        }
                    ],
                    "synonyms": [
                        "так себе",
                        "посредственный",
                        "средний"
                    ],
                    "antonyms": [
                        "отличный",
                        "превосходный"
                    ],
                    "common_phrases": [
                        "так себе"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                }
            ]
        }
    ]

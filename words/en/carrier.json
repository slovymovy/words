{
    "word_family": [
        "carry",
        "carriage"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3fce345-ec94-3a30-9c48-6f724618b042",
                    "sense_definition": "A person or object that transports, holds, or conveys someone or something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_carrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mail <w>carrier</w> delivers letters to our house every day.",
                            "target_lang_translations": {
                                "ru": "<w>Почтальон</w> приносит письма в наш дом каждый день."
                            }
                        },
                        {
                            "text": "She placed her cat in the pet <w>carrier</w> before driving to the vet.",
                            "target_lang_translations": {
                                "ru": "Она посадила кошку в <w>переноску</w> перед поездкой к ветеринару."
                            }
                        }
                    ],
                    "synonyms": [
                        "bearer"
                    ],
                    "common_phrases": [
                        "mail carrier",
                        "pet carrier",
                        "baby carrier",
                        "luggage carrier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, который перевозит, держит или доставляет кого-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переноска",
                                "target_lang_sense_clarification": "контейнер для животных"
                            },
                            {
                                "target_lang_word": "разносчик",
                                "target_lang_sense_clarification": "человек, доставляющий что-либо"
                            },
                            {
                                "target_lang_word": "багажник",
                                "target_lang_sense_clarification": "устройство на велосипеде или автомобиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae550916-0c65-36c1-bdae-1b0f2eff7d51",
                    "sense_definition": "A company that transports goods or people for payment, such as an airline or a trucking company.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_carrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Southwest is a major domestic <w>carrier</w> in the United States.",
                            "target_lang_translations": {
                                "ru": "Southwest — крупный внутренний <w>перевозчик</w> в Соединенных Штатах."
                            }
                        },
                        {
                            "text": "We need to find a reliable freight <w>carrier</w> to ship these heavy boxes.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти надежного грузового <w>перевозчика</w>, чтобы отправить эти тяжелые коробки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flag carrier",
                        "low-cost carrier",
                        "national carrier",
                        "common carrier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания, занимающаяся перевозкой грузов или пассажиров за плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевозчик",
                                "target_lang_sense_clarification": "транспортная компания"
                            },
                            {
                                "target_lang_word": "транспортная компания",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53ba53d6-cf79-39a9-9500-758f08a2447f",
                    "sense_definition": "A company that provides wireless mobile phone or internet services.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_carrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I want to switch to a different <w>carrier</w> that offers better coverage in my area.",
                            "target_lang_translations": {
                                "ru": "Я хочу переключиться на другого <w>оператора</w>, который предлагает лучшее покрытие в моем районе."
                            }
                        },
                        {
                            "text": "My wireless <w>carrier</w> charges extra for international calls.",
                            "target_lang_translations": {
                                "ru": "Мой мобильный <w>оператор</w> взимает дополнительную плату за международные звонки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wireless carrier",
                        "mobile carrier"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания, предоставляющая услуги мобильной связи или интернета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "мобильной связи"
                            },
                            {
                                "target_lang_word": "провайдер",
                                "target_lang_sense_clarification": "поставщик услуг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4d8932a-e993-35c0-a06f-312c68a7fe88",
                    "sense_definition": "A large warship equipped with a long flat deck for aircraft to take off and land.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_carrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fighter jets landed safely on the deck of the aircraft <w>carrier</w>.",
                            "target_lang_translations": {
                                "ru": "Истребители благополучно приземлились на палубу <w>авианосца</w>."
                            }
                        },
                        {
                            "text": "The navy deployed a <w>carrier</w> to the region to support the operation.",
                            "target_lang_translations": {
                                "ru": "Флот направил <w>авианосец</w> в регион для поддержки операции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aircraft carrier",
                        "fleet carrier"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short for aircraft carrier"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой военный корабль с плоской палубой для взлёта и посадки самолётов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авианосец",
                                "target_lang_sense_clarification": "военный корабль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "644046f9-5f15-3f06-aa1b-2f6e94005940",
                    "sense_definition": "A person, animal, or organism that carries a disease or genetic trait and can transmit it to others but does not show symptoms.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_carrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mosquitoes are a common <w>carrier</w> of malaria in tropical regions.",
                            "target_lang_translations": {
                                "ru": "Комары являются распространенным <w>переносчиком</w> малярии в тропических регионах."
                            }
                        },
                        {
                            "text": "He is a healthy <w>carrier</w> of the virus, so he feels fine but can still infect others.",
                            "target_lang_translations": {
                                "ru": "Он здоровый <w>носитель</w> вируса, поэтому чувствует себя хорошо, но все же может заразить других."
                            }
                        }
                    ],
                    "common_phrases": [
                        "disease carrier",
                        "healthy carrier",
                        "asymptomatic carrier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или организм, который носит в себе болезнь или генетический признак и может передавать его другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переносчик",
                                "target_lang_sense_clarification": "инфекции или болезни"
                            },
                            {
                                "target_lang_word": "носитель",
                                "target_lang_sense_clarification": "гена или вируса (бессимптомный)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de53755c-a901-3fc1-b12e-03f9acdd49f1",
                    "sense_definition": "A plastic or paper bag with handles used for carrying shopping.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "bag_carrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you need a <w>carrier</w> for your groceries, or did you bring your own bag?",
                            "target_lang_translations": {
                                "ru": "Вам нужен <w>пакет</w> для продуктов или вы принесли свою сумку?"
                            }
                        },
                        {
                            "text": "The shop charges a small fee for every plastic <w>carrier</w>.",
                            "target_lang_translations": {
                                "ru": "Магазин взимает небольшую плату за каждый пластиковый <w>пакет</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "carrier bag"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        },
                        {
                            "trait_type": "form",
                            "comment": "short for carrier bag"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пластиковый или бумажный пакет с ручками для покупок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакет",
                                "target_lang_sense_clarification": "для покупок"
                            },
                            {
                                "target_lang_word": "сумка",
                                "target_lang_sense_clarification": "хозяйственная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2102a908-a8b1-3bea-9ec1-cbbf25ed50aa",
                    "sense_definition": "A substance, wave, or signal used to transmit, support, or convey something else in science or technology.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_carrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Radio stations use a <w>carrier</w> wave to transmit sound information over long distances.",
                            "target_lang_translations": {
                                "ru": "Радиостанции используют <w>несущую</w> волну для передачи звуковой информации на большие расстояния."
                            }
                        },
                        {
                            "text": "You should mix the essential oil with a neutral <w>carrier</w> oil before applying it to your skin.",
                            "target_lang_translations": {
                                "ru": "Вам следует смешать эфирное масло с нейтральным маслом-<w>основой</w> перед нанесением на кожу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "carrier wave",
                        "carrier oil",
                        "carrier gas",
                        "charge carrier"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical context (Physics/Chemistry)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, волна или сигнал, используемые для передачи или поддержки чего-либо в науке или технике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несущая",
                                "target_lang_sense_clarification": "волна или частота"
                            },
                            {
                                "target_lang_word": "основа",
                                "target_lang_sense_clarification": "базовое вещество"
                            },
                            {
                                "target_lang_word": "носитель",
                                "target_lang_sense_clarification": "физический или химический"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "826f941d-2688-35d7-bd43-2769ce457bb5",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_carrier",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Carrier</w> has been working at the firm for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Кэрриер</w> работает в фирме уже двадцать лет."
                            }
                        },
                        {
                            "text": "We invited the <w>Carrier</w> family to the neighborhood party.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Кэрриер</w> на вечеринку соседей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

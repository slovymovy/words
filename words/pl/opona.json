            "senses": [
                {
                    "sense_id": "6958b06e-a58b-3896-ae3d-096cd1fd94a0",
                    "sense_definition": "Zewnętrzna część koła nakładana na felgę lub obręcz, zazwyczaj wykonana z gumy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy zmieniłaś <w>opony</w> na zimowe?",
                            "target_lang_translations": {
                                "ru": "Ты поменяла <w>шины</w> на зимние?"
                            }
                        },
                        {
                            "text": "Wczoraj w drodze do pracy pękła mi <w>opona</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера по дороге на работу у меня лопнула <w>шина</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opony zimowe",
                        "opony letnie",
                        "przebita opona",
                        "wymiana opon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешняя часть колеса, надеваемая на обод, обычно изготовленная из резины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шина",
                                "target_lang_sense_clarification": "автомобильная"
                            },
                            {
                                "target_lang_word": "покрышка",
                                "target_lang_sense_clarification": "резиновая оболочка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1ed8df9-491a-3d12-a96f-ffe680f5ab21",
                    "sense_definition": "Fałdy tkanki tłuszczowej na brzuchu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę zacząć ćwiczyć, bo urosła mi spora <w>opona</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно начать заниматься, потому что у меня появился приличный <w>спасательный круг</w>."
                            }
                        },
                        {
                            "text": "Po świętach trudno jest zrzucić taką <w>oponę</w>.",
                            "target_lang_translations": {
                                "ru": "После праздников трудно сбросить такой <w>спасательный круг</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zrzucić oponę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Складки жировой ткани на животе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спасательный круг",
                                "target_lang_sense_clarification": "жировые складки на талии"
                            },
                            {
                                "target_lang_word": "жирок",
                                "target_lang_sense_clarification": "разг. лишний вес на животе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c449102-78ff-3405-b036-3e7e26233b6f",
                    "sense_definition": "Błona tkanki łącznej otaczająca mózg i rdzeń kręgowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz podejrzewał u pacjenta wirusowe zapalenie <w>opon</w> mózgowych.",
                            "target_lang_translations": {
                                "ru": "Врач подозревал у пациента вирусное воспаление <w>мозговых оболочек</w>."
                            }
                        },
                        {
                            "text": "<w>Opona</w> twarda stanowi zewnętrzną osłonę ośrodkowego układu nerwowego.",
                            "target_lang_translations": {
                                "ru": "Твердая <w>мозговая оболочка</w> представляет собой внешнюю защиту центральной нервной системы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapalenie opon",
                        "opona twarda",
                        "opona miękka",
                        "opona pajęcza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединительнотканная оболочка, окружающая головной и спинной мозг."

{
    "word_family": [
        "hart",
        "hartelijkheid",
        "onhartelijk",
        "openhartig",
        "barmhartig"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fcf93014-49c5-3345-b0ac-add3ac49d83e",
                    "sense_definition": "Aardig, gastvrij en spontaan in de omgang met anderen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_friendly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze nieuwe buren gaven ons een zeer <w>hartelijk</w> welkom in de straat.",
                            "target_lang_translations": {
                                "ru": "Наши новые соседи оказали нам очень <w>радушный</w> прием на нашей улице."
                            }
                        },
                        {
                            "text": "Zij is een <w>hartelijke</w> vrouw die altijd klaarstaat voor haar vrienden.",
                            "target_lang_translations": {
                                "ru": "Она <w>сердечная</w> женщина, которая всегда готова помочь своим друзьям."
                            }
                        }
                    ],
                    "synonyms": [
                        "gastvrij",
                        "warm",
                        "openhartig",
                        "vlot"
                    ],
                    "antonyms": [
                        "onhartelijk",
                        "ongastvrij"
                    ],
                    "common_phrases": [
                        "een hartelijk welkom",
                        "een hartelijke ontvangst",
                        "een hartelijke sfeer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятный, гостеприимный и открытый в общении с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радушный",
                                "target_lang_sense_clarification": "гостеприимный, приветливый"
                            },
                            {
                                "target_lang_word": "сердечный",
                                "target_lang_sense_clarification": "добрый, душевный"
                            },
                            {
                                "target_lang_word": "тёплый",
                                "target_lang_sense_clarification": "о приеме или атмосфере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7543bd2c-5ae4-39cf-bf50-b90a80140d07",
                    "sense_definition": "Eerlijk en oprecht gemeend, vaak gebruikt bij wensen of bedankjes.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_sincere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil u mijn <w>hartelijke</w> dank betuigen voor al uw hulp.",
                            "target_lang_translations": {
                                "ru": "Я хочу выразить вам свою <w>сердечную</w> благодарность за всю вашу помощь."
                            }
                        },
                        {
                            "text": "Ontvang de <w>hartelijke</w> groeten van mijn ouders uit Rotterdam.",
                            "target_lang_translations": {
                                "ru": "Примите <w>сердечный</w> привет от моих родителей из Роттердама."
                            }
                        }
                    ],
                    "synonyms": [
                        "innig",
                        "hartgrondig"
                    ],
                    "common_phrases": [
                        "hartelijke groeten",
                        "hartelijke dank",
                        "hartelijke gelukwensen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честный и искренний, часто используется в пожеланиях или выражении благодарности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердечный",
                                "target_lang_sense_clarification": "о привете, поздравлении или благодарности"
                            },
                            {
                                "target_lang_word": "искренний",
                                "target_lang_sense_clarification": "неподдельный, идущий от сердца"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ac4b566d-a620-31a2-8580-3a12b4769e95",
                    "sense_definition": "Op een manier die veel vriendelijkheid, warmte of enthousiasme toont.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_friendly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur bedankte het personeel <w>hartelijk</w> voor hun inzet dit jaar.",
                            "target_lang_translations": {
                                "ru": "Директор <w>сердечно</w> поблагодарил сотрудников за их работу в этом году."
                            }
                        },
                        {
                            "text": "We hebben gisteravond <w>hartelijk</w> gelachen om zijn grappige verhalen.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы <w>от души</w> посмеялись над его забавными историями."
                            }
                        }
                    ],
                    "synonyms": [
                        "warm",
                        "hartgrondig"
                    ],
                    "antonyms": [
                        "onhartelijk"
                    ],
                    "common_phrases": [
                        "hartelijk bedanken",
                        "hartelijk lachen",
                        "hartelijk gefeliciteerd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружелюбно, с теплотой или энтузиазмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердечно",
                                "target_lang_sense_clarification": "с теплотой"
                            },
                            {
                                "target_lang_word": "от души",
                                "target_lang_sense_clarification": "очень сильно, искренне (например, смеяться)"
                            },
                            {
                                "target_lang_word": "тепло",
                                "target_lang_sense_clarification": "дружелюбно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
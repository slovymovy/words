{
    "word_family": [
        "identificatie",
        "identificeerbaar",
        "identiteit",
        "identiek"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d05104fa-a15d-392b-9965-d23de765ad25",
                    "sense_definition": "De identiteit van iemand of iets vaststellen; herkennen wie of wat iemand of iets is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "determine_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie probeert de dader te <w>identificeren</w> met behulp van camerabeelden.",
                            "target_lang_translations": {
                                "en": "The police are trying to <w>identify</w> the perpetrator using camera footage."
                            }
                        },
                        {
                            "text": "Het is belangrijk om het probleem te <w>identificeren</w> voordat we een oplossing zoeken.",
                            "target_lang_translations": {
                                "en": "It is important to <w>identify</w> the problem before we look for a solution."
                            }
                        }
                    ],
                    "synonyms": [
                        "vereenzelvigen",
                        "zich vereenzelvigen",
                        "zich vereenzelvigen"
                    ],
                    "common_phrases": [
                        "een probleem identificeren",
                        "een verdachte identificeren",
                        "het slachtoffer identificeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish the identity of someone or something; to recognize who or what someone or something is."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "identify",
                                "target_lang_sense_clarification": "determine identity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8229ac8d-338e-3453-b5f2-16af3b15a2da",
                    "sense_definition": "Bewijzen wie je bent door een officieel document te laten zien.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "prove_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij de ingang van het gebouw moet iedereen <w>zich identificeren</w>.",
                            "target_lang_translations": {
                                "en": "At the entrance of the building, everyone must <w>identify themselves</w>."
                            }
                        },
                        {
                            "text": "U moet <w>zich</w> kunnen <w>identificeren</w> als de politie daarom vraagt.",
                            "target_lang_translations": {
                                "en": "You must be able to <w>identify yourself</w> if the police ask."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich vereenzelvigen",
                        "zich vereenzelvigen",
                        "zich vereenzelvigen"
                    ],
                    "common_phrases": [
                        "zich moeten identificeren",
                        "zich kunnen identificeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To prove who you are by showing an official document."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "identify oneself",
                                "target_lang_sense_clarification": "prove one's identity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64f94d77-0d44-31de-b45a-cba611cb38ad",
                    "sense_definition": "Zich sterk verbonden voelen met iemand, een groep of een idee; zichzelf in een ander herkennen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel jonge lezers kunnen <w>zich identificeren</w> met de hoofdpersoon uit het boek.",
                            "target_lang_translations": {
                                "en": "Many young readers can <w>identify</w> with the main character from the book."
                            }
                        },
                        {
                            "text": "Ik kan <w>me</w> totaal niet <w>identificeren</w> met de standpunten van die politieke partij.",
                            "target_lang_translations": {
                                "en": "I cannot <w>identify</w> at all with the views of that political party."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich vereenzelvigen"
                    ],
                    "common_phrases": [
                        "zich identificeren met",
                        "zich niet identificeren met"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel a strong connection with someone, a group, or an idea; to recognize oneself in another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "identify with",
                                "target_lang_sense_clarification": "feel connected to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9276df6-03c3-332c-989c-797feb9163f4",
                    "sense_definition": "Zichzelf beschouwen of benoemen als behorend tot een bepaalde groep, gender of categorie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "self_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Steeds meer mensen durven <w>zich</w> openlijk te <w>identificeren</w> als non-binair.",
                            "target_lang_translations": {
                                "en": "More and more people dare to openly <w>identify</w> as non-binary."
                            }
                        },
                        {
                            "text": "Zij <w>identificeert zich</w> als een trotse inwoner van deze stad.",
                            "target_lang_translations": {
                                "en": "She <w>identifies</w> as a proud resident of this city."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich identificeren als"
                    ],
                    "target_lang_definitions": {
                        "en": "To consider or label oneself as belonging to a specific group, gender, or category."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "identify as",
                                "target_lang_sense_clarification": "categorize oneself"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
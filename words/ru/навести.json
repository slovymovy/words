                    "common_phrases": [
                        "навести страх",
                        "навести ужас",
                        "навести тоску",
                        "навести панику"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause or instill a strong state or feeling (usually fear, horror, sadness)."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b0b86a9-245f-3e1a-acae-897c1bd25f7d",
                    "sense_definition": "Potoczne określenie lokomotywy napędzanej silnikiem spalinowym, używanej w transporcie kolejowym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niezelektryfikowanych liniach kolejowych wciąż króluje niezawodna <w>spalinówka</w>.",
                            "target_lang_translations": {
                                "ru": "На неэлектрифицированных железнодорожных линиях всё ещё царит надёжный <w>тепловоз</w>."
                            }
                        },
                        {
                            "text": "Ciężki pociąg towarowy do tartaku ciągnęła wysłużona, zielona <w>spalinówka</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжёлый товарный поезд к лесопилке тянул старый зелёный <w>тепловоз</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lokomotywa spalinowa",
                        "spalinowóz"
                    ],
                    "common_phrases": [
                        "stara spalinówka",
                        "prowadzić spalinówkę",
                        "ciężka spalinówka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo powstałe przez dodanie przyrostka -ówka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название локомотива, приводимого в движение двигателем внутреннего сгорания, используемого в железнодорожном транспорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тепловоз",
                                "target_lang_sense_clarification": "локомотив с двигателем внутреннего сгорания"
                            },
                            {
                                "target_lang_word": "дизель",
                                "target_lang_sense_clarification": "разговорное название дизельного поезда или локомотива"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7fd0d7b-fe4c-387d-8772-e49cc71c23aa",
                    "sense_definition": "Potoczne określenie małego urządzenia mechanicznego z napędem spalinowym, najczęściej kosiarki lub piły.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamiast kosiarki elektrycznej kupiłem mocną <w>spalinówkę</w>, żeby nie ciągnąć za sobą kabla.",
                            "target_lang_translations": {
                                "ru": "Вместо электрической газонокосилки я купил мощную <w>бензокосилку</w>, чтобы не таскать за собой кабель."
                            }
                        },
                        {
                            "text": "Mój sąsiad od samego rana hałasuje w ogrodzie swoją nową <w>spalinówką</w>.",
                            "target_lang_translations": {
                                "ru": "Мой сосед с самого утра шумит в саду своей новой <w>бензокосилкой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głośna spalinówka",
                        "mocna spalinówka",
                        "naprawić spalinówkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo potoczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название небольшого механического устройства с двигателем внутреннего сгорания, чаще всего газонокосилки или пилы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бензокосилка",
                                "target_lang_sense_clarification": "газонокосилка с бензиновым двигателем"
                            },
                            {
                                "target_lang_word": "бензопила",
                                "target_lang_sense_clarification": "пила с бензиновым двигателем"
                            },
                            {
                                "target_lang_word": "бензоинструмент",
                                "target_lang_sense_clarification": "общее название инструментов с бензиновым двигателем"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
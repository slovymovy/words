{
    "word_family": [
        "вывод",
        "ввод",
        "перевод",
        "развод",
        "водить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fddbf569-0d29-33cf-9507-81bab415f951",
                    "sense_definition": "Вести кого-либо или что-либо изнутри наружу, перемещать за пределы чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я каждое утро <вывожу> свою собаку на прогулку.",
                            "target_lang_translations": {
                                "en": "I <take> my dog <out> for a walk every morning."
                            }
                        },
                        {
                            "text": "Отец медленно <выводит> машину из узкого гаража.",
                            "target_lang_translations": {
                                "en": "Father slowly <takes> the car <out> of the narrow garage."
                            }
                        }
                    ],
                    "synonyms": [
                        "уводить"
                    ],
                    "antonyms": [
                        "вводить"
                    ],
                    "common_phrases": [
                        "выводить собаку",
                        "выводить из гаража",
                        "выводить войска",
                        "выводить людей"
                    ],
                    "target_lang_definitions": {
                        "en": "To lead someone or something from inside to outside, to move beyond the limits of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "to remove or guide something/someone outside"
                            },
                            {
                                "target_lang_word": "lead out",
                                "target_lang_sense_clarification": "to guide someone out of a place"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "to move something away from a place (e.g., troops)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4937ac72-8659-3154-86e2-6afe215fc692",
                    "sense_definition": "Отображать информацию на экране или печатать её на принтере (в информатике и технике).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tech_display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта программа <выводит> результат вычислений на экран.",
                            "target_lang_translations": {
                                "en": "This program <displays> the calculation result on the screen."
                            }
                        },
                        {
                            "text": "Принтер <выводит> последние страницы документа.",
                            "target_lang_translations": {
                                "en": "The printer <is printing> the last pages of the document."
                            }
                        }
                    ],
                    "synonyms": [
                        "показывать",
                        "выдавать"
                    ],
                    "common_phrases": [
                        "выводить на экран",
                        "выводить данные",
                        "выводить на печать"
                    ],
                    "target_lang_definitions": {
                        "en": "To display information on a screen or print it on a printer (in computer science and technology)."
                    },
                    "translations": {
                        "en": []
                    }
                },
                {
                    "sense_id": "24f11d4f-daf5-311c-897d-e4ebcb00219c",
                    "sense_definition": "Уничтожать насекомых или удалять пятна с поверхности.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хозяйка пытается <выводить> пятна с белой скатерти.",
                            "target_lang_translations": {
                                "en": "The hostess is trying to <remove> stains from the white tablecloth."
                            }
                        },
                        {
                            "text": "Мы решили <выводить> муравьёв на кухне специальным средством.",
                            "target_lang_translations": {
                                "en": "We decided to <exterminate> the ants in the kitchen with a special agent."
                            }
                        }
                    ],
                    "synonyms": [
                        "истреблять",
                        "искоренять",
                        "устранять"
                    ],
                    "common_phrases": [
                        "выводить пятна",
                        "выводить тараканов",
                        "выводить грязь"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy insects or remove stains from a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "to eliminate spots or stains"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "to clean or remove a spot"
                            },
                            {
                                "target_lang_word": "exterminate",
                                "target_lang_sense_clarification": "to destroy pests or insects completely"
                            },
                            {
                                "target_lang_word": "destroy",
                                "target_lang_sense_clarification": "to kill or eliminate pests"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "005be90b-4a64-33ba-9fa6-b3a5a97e0856",
                    "sense_definition": "Исключать из состава группы или лишать душевного равновесия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exclusion_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Громкий шум постоянно <выводит> меня из себя.",
                            "target_lang_translations": {
                                "en": "Loud noise constantly <drives> me crazy."
                            }
                        },
                        {
                            "text": "Тренер решил <выводить> игрока из основного состава команды.",
                            "target_lang_translations": {
                                "en": "The coach decided to <remove> the player from the team's main lineup."
                            }
                        }
                    ],
                    "synonyms": [
                        "исключать"
                    ],
                    "antonyms": [
                        "вводить"
                    ],
                    "common_phrases": [
                        "выводить из себя",
                        "выводить из строя",
                        "выводить из состава"
                    ],
                    "target_lang_definitions": {
                        "en": "To exclude from a group or to upset someone's emotional balance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "to dismiss or take someone out of a group/team"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "536e9125-5db1-3666-b106-deea94f3c80b",
                    "sense_definition": "Делать логическое умозаключение или получать формулу на основе данных.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёный <выводит> сложную математическую формулу на доске.",
                            "target_lang_translations": {
                                "en": "The scientist <is deriving> a complex mathematical formula on the board."
                            }
                        },
                        {
                            "text": "Из этих фактов можно <выводить> интересные заключения.",
                            "target_lang_translations": {
                                "en": "One can <draw> interesting conclusions from these facts."
                            }
                        }
                    ],
                    "synonyms": [
                        "заключать"
                    ],
                    "common_phrases": [
                        "выводить формулу",
                        "выводить правило",
                        "выводить закон"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a logical inference or derive a formula based on data."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deduce",
                                "target_lang_sense_clarification": "to reach a conclusion by reasoning"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to formulate a conclusion from facts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6ac4f5e-b4c5-3627-bf84-6d61d0177f68",
                    "sense_definition": "Выращивать новые сорта растений или породы животных.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "breeding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Садовник мечтает <выводить> редкие сорта роз.",
                            "target_lang_translations": {
                                "en": "The gardener dreams of <breeding> rare varieties of roses."
                            }
                        },
                        {
                            "text": "Фермеры начали <выводить> новую породу овец.",
                            "target_lang_translations": {
                                "en": "The farmers started <breeding> a new breed of sheep."
                            }
                        }
                    ],
                    "synonyms": [
                        "выращивать",
                        "создавать"
                    ],
                    "common_phrases": [
                        "выводить сорт",
                        "выводить породу",
                        "выводить птенцов"
                    ],
                    "target_lang_definitions": {
                        "en": "To breed new varieties of plants or breeds of animals."
                    },
                    "translations": {
                        "en": []
                    }
                },
                {
                    "sense_id": "39d7391a-40bd-340c-b202-201dae8eb197",
                    "sense_definition": "Тщательно и аккуратно писать буквы или рисовать линии.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drawing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок старательно <выводит> каждую букву в тетради.",
                            "target_lang_translations": {
                                "en": "The child diligently <writes out> every letter in the notebook."
                            }
                        },
                        {
                            "text": "Художник начал <выводить> тонкие узоры на вазе.",
                            "target_lang_translations": {
                                "en": "The artist began to <draw> fine patterns on the vase."
                            }
                        }
                    ],
                    "synonyms": [
                        "выписывать",
                        "вырисовывать"
                    ],
                    "common_phrases": [
                        "выводить буквы",
                        "выводить узоры",
                        "старательно выводить"
                    ],
                    "target_lang_definitions": {
                        "en": "To carefully and neatly write letters or draw lines."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to produce lines or figures carefully"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
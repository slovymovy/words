{
    "word_family": [
        "gainer",
        "gainful",
        "regain"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "901d0b86-f429-3f62-a402-5baad218fee5",
                    "sense_definition": "To get or obtain something that is useful, necessary, or positive.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "acquire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She worked hard to <w>gain</w> the respect of her colleagues.",
                            "target_lang_translations": {
                                "ru": "Она усердно работала, чтобы <w>завоевать</w> уважение коллег."
                            }
                        },
                        {
                            "text": "You can <w>gain</w> a lot of experience by volunteering.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>получить</w> большой опыт, занимаясь волонтёрством."
                            }
                        }
                    ],
                    "synonyms": [
                        "acquire",
                        "obtain",
                        "get"
                    ],
                    "antonyms": [
                        "loss"
                    ],
                    "common_phrases": [
                        "gain access",
                        "gain control",
                        "gain experience",
                        "gain knowledge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать или приобретать что-то полезное, необходимое или положительное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приобретать",
                                "target_lang_sense_clarification": "становиться обладателем качества или навыка"
                            },
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "общий термин для получения чего-либо"
                            },
                            {
                                "target_lang_word": "завоевывать",
                                "target_lang_sense_clarification": "получать с усилием (уважение, доверие)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30c64aaa-8fc8-3d18-bbfe-31a48ae1492a",
                    "sense_definition": "To increase in body weight.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "increase_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>gained</w> ten pounds over the winter holidays.",
                            "target_lang_translations": {
                                "ru": "Он <w>набрал</w> десять фунтов за зимние праздники."
                            }
                        },
                        {
                            "text": "The doctor was pleased to see that the baby had <w>gained</w> weight.",
                            "target_lang_translations": {
                                "ru": "Врач был доволен, увидев, что ребёнок <w>набрал</w> вес."
                            }
                        }
                    ],
                    "antonyms": [
                        "loss"
                    ],
                    "common_phrases": [
                        "gain weight",
                        "gain a few pounds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличиваться в весе (о теле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набирать",
                                "target_lang_sense_clarification": "увеличивать массу тела"
                            },
                            {
                                "target_lang_word": "поправляться",
                                "target_lang_sense_clarification": "становиться полнее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "298771d7-3f51-3a8b-b86d-025a0560d981",
                    "sense_definition": "To increase in amount, magnitude, speed, or degree.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "increase_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car <w>gained</w> speed as it went down the hill.",
                            "target_lang_translations": {
                                "ru": "Машина <w>набирала</w> скорость, спускаясь с холма."
                            }
                        },
                        {
                            "text": "The movement has <w>gained</w> momentum in recent weeks.",
                            "target_lang_translations": {
                                "ru": "Движение <w>набрало</w> обороты за последние недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "increase"
                    ],
                    "common_phrases": [
                        "gain speed",
                        "gain momentum",
                        "gain time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличиваться в количестве, величине, скорости или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набирать",
                                "target_lang_sense_clarification": "о скорости, темпе или высоте"
                            },
                            {
                                "target_lang_word": "увеличивать",
                                "target_lang_sense_clarification": "делать больше по величине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ea06a99-1558-3785-82a6-0a878a050dec",
                    "sense_definition": "To receive an advantage, benefit, or profit from something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is nothing to <w>gain</w> from arguing about the past.",
                            "target_lang_translations": {
                                "ru": "Спорами о прошлом ничего не <w>выиграешь</w>."
                            }
                        },
                        {
                            "text": "Both companies will <w>gain</w> from this new partnership.",
                            "target_lang_translations": {
                                "ru": "Обе компании <w>выиграют</w> от этого нового партнёрства."
                            }
                        }
                    ],
                    "synonyms": [
                        "benefit",
                        "profit"
                    ],
                    "common_phrases": [
                        "gain from",
                        "gain by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать преимущество, выгоду или прибыль от чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "получать преимущество"
                            },
                            {
                                "target_lang_word": "извлекать пользу",
                                "target_lang_sense_clarification": "получать выгоду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa33910c-e188-37e1-bbd6-43ec5f91ce6c",
                    "sense_definition": "To win a competition, battle, or legal case.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "win",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army <w>gained</w> a decisive victory on the battlefield.",
                            "target_lang_translations": {
                                "ru": "Армия <w>одержала</w> решительную победу на поле битвы."
                            }
                        },
                        {
                            "text": "They hoped to <w>gain</w> the seat in the upcoming election.",
                            "target_lang_translations": {
                                "ru": "Они надеялись <w>получить</w> место на предстоящих выборах."
                            }
                        }
                    ],
                    "synonyms": [
                        "win"
                    ],
                    "antonyms": [
                        "loss"
                    ],
                    "common_phrases": [
                        "gain a victory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выигрывать соревнование, битву или судебное дело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одерживать",
                                "target_lang_sense_clarification": "используется со словом победа"
                            },
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "побеждать в состязании"
                            },
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "добиваться места или позиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5afea2fa-bde7-34ea-a3cd-5d8dea00f658",
                    "sense_definition": "To get closer to someone or something that one is chasing or competing against.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "catch_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police car was <w>gaining</w> on the speeding suspect.",
                            "target_lang_translations": {
                                "ru": "Полицейская машина <w>нагоняла</w> подозреваемого, превысившего скорость."
                            }
                        },
                        {
                            "text": "Although he started late, he is <w>gaining</w> ground on the leader.",
                            "target_lang_translations": {
                                "ru": "Хотя он стартовал поздно, он <w>нагоняет</w> лидера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gain on",
                        "gain ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приближаться к кому-то или чему-то, кого преследуешь или с кем соревнуешься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагонять",
                                "target_lang_sense_clarification": "сокращать дистанцию"
                            },
                            {
                                "target_lang_word": "догонять",
                                "target_lang_sense_clarification": "приближаться к лидеру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e995dcfd-9b90-3bcb-a9cd-5532a2c5e65a",
                    "sense_definition": "To reach a specific destination, often after significant effort.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The climbers finally <w>gained</w> the summit after hours of struggle.",
                            "target_lang_translations": {
                                "ru": "Альпинисты наконец <w>достигли</w> вершины после нескольких часов борьбы."
                            }
                        },
                        {
                            "text": "We managed to <w>gain</w> the shore before the storm hit.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <w>добраться до</w> берега до начала шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "reach",
                        "arrive at"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать определенного места назначения, часто после значительных усилий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "добираться до труднодоступного места"
                            },
                            {
                                "target_lang_word": "добираться до",
                                "target_lang_sense_clarification": "попадать в место назначения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0161f701-6b24-3f19-8580-d40ee864b6c0",
                    "sense_definition": "An advantage, profit, or increase that is acquired or achieved.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_profit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company reported a significant financial <w>gain</w> this quarter.",
                            "target_lang_translations": {
                                "ru": "Компания отчиталась о значительной финансовой <w>прибыли</w> в этом квартале."
                            }
                        },
                        {
                            "text": "Regular exercise leads to a <w>gain</w> in overall strength and health.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения ведут к <w>приросту</w> общей силы и здоровья."
                            }
                        }
                    ],
                    "synonyms": [
                        "profit",
                        "benefit",
                        "increase",
                        "acquisition"
                    ],
                    "antonyms": [
                        "loss"
                    ],
                    "common_phrases": [
                        "capital gain",
                        "personal gain",
                        "net gain",
                        "weight gain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преимущество, прибыль или увеличение, которое приобретено или достигнуто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прибыль",
                                "target_lang_sense_clarification": "финансовая выгода"
                            },
                            {
                                "target_lang_word": "выгода",
                                "target_lang_sense_clarification": "польза или преимущество"
                            },
                            {
                                "target_lang_word": "прирост",
                                "target_lang_sense_clarification": "увеличение количества или качества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2484044-c443-3384-9967-3bc6f027b781",
                    "sense_definition": "The ratio of increase of a signal's power or amplitude in an electronic device.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to adjust the <w>gain</w> on the microphone to avoid feedback.",
                            "target_lang_translations": {
                                "ru": "Вам нужно отрегулировать <w>усиление</w> на микрофоне, чтобы избежать обратной связи."
                            }
                        },
                        {
                            "text": "The amplifier provides a high <w>gain</w> for weak signals.",
                            "target_lang_translations": {
                                "ru": "Усилитель обеспечивает высокое <w>усиление</w> для слабых сигналов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Коэффициент увеличения мощности или амплитуды сигнала в электронном устройстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усиление",
                                "target_lang_sense_clarification": "рост мощности сигнала"
                            },
                            {
                                "target_lang_word": "коэффициент усиления",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "94807950-8231-3c23-bc27-dad90bb47e5b",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Gain</w> will see you now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Гейн</w> сейчас примет вас."
                            }
                        },
                        {
                            "text": "I met a family with the surname <w>Gain</w>.",
                            "target_lang_translations": {
                                "ru": "Я встретил семью с фамилией <w>Гейн</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гейн",
                                "target_lang_sense_clarification": "транслитерация имени собственного"
                            }
                        ]
                    }
                }
            ]
        }
    ]

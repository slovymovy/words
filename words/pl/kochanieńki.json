{
    "word_family": [
        "kochać",
        "kochanie",
        "kochany",
        "ukochany",
        "kochanek"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0d7930f4-2988-381e-9837-c0fe209e1549",
                    "sense_definition": "Forma zdrobniała od przymiotnika <w>kochany</w>, wyrażająca bardzo dużą sympatię, czułość i emocjonalne ciepło.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "affectionate_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>kochanieńki</w> szczeniak od razu podbił serca wszystkich domowników.",
                            "target_lang_translations": {
                                "ru": "Этот <w>миленький</w> щенок сразу покорил сердца всех домочадцев."
                            }
                        },
                        {
                            "text": "Jej <w>kochanieńki</w> wnuczek narysował dla niej piękną laurkę na urodziny.",
                            "target_lang_translations": {
                                "ru": "Её <w>любименький</w> внучек нарисовал для неё красивую открытку на день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "kochaniutki"
                    ],
                    "common_phrases": [
                        "mój kochanieńki"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от прилагательного «любимый», выражающая очень большую симпатию, нежность и эмоциональную теплоту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миленький",
                                "target_lang_sense_clarification": "ласковое описание внешности или характера"
                            },
                            {
                                "target_lang_word": "любименький",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное от «любимый»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20753efe-6e21-37cd-954c-018f794f4618",
                    "sense_definition": "Pieszczotliwy zwrot do bliskiego mężczyzny lub chłopca, wyrażający sympatię i przywiązanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "affectionate_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie martw się, <w>kochanieńki</w>, na pewno poradzisz sobie z tym egzaminem.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся, <w>миленький</w>, ты обязательно справишься с этим экзаменом."
                            }
                        },
                        {
                            "text": "Zrób mi proszę herbatę, <w>kochanieńki</w>, bo jestem bardzo zmęczona.",
                            "target_lang_translations": {
                                "ru": "Сделай мне, пожалуйста, чай, <w>дорогой</w>, а то я очень устала."
                            }
                        }
                    ],
                    "synonyms": [
                        "kochany",
                        "kochaniutki"
                    ],
                    "common_phrases": [
                        "mój kochanieńki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к близкому мужчине или мальчику, выражающее симпатию и привязанность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миленький",
                                "target_lang_sense_clarification": "ласковое обращение"
                            },
                            {
                                "target_lang_word": "родненький",
                                "target_lang_sense_clarification": "выражение душевной близости"
                            },
                            {
                                "target_lang_word": "дорогой",
                                "target_lang_sense_clarification": "общее ласковое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6ede6b1-662f-39ef-be77-37bea5949a29",
                    "sense_definition": "Ironiczny lub protekcjonalny zwrot do mężczyzny, używany często w sytuacjach konfliktu lub irytacji, aby okazać wyższość.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ironic_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słuchaj no, <w>kochanieńki</w>, nie będziesz mi tu mówił, co mam robić.",
                            "target_lang_translations": {
                                "ru": "Послушай-ка, <w>милейший</w>, ты не будешь мне тут указывать, что делать."
                            }
                        },
                        {
                            "text": "Mylisz się, <w>kochanieńki</w>, te zasady dotyczą wszystkich bez wyjątku.",
                            "target_lang_translations": {
                                "ru": "Ты ошибаешься, <w>голубчик</w>, эти правила касаются всех без исключения."
                            }
                        }
                    ],
                    "synonyms": [
                        "kochaniutki"
                    ],
                    "common_phrases": [
                        "słuchaj no kochanieńki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "ironic or patronizing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ироничное или снисходительное обращение к мужчине, часто используемое в ситуациях конфликта или раздражения, чтобы показать превосходство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "милейший",
                                "target_lang_sense_clarification": "ироничное или официальное обращение"
                            },
                            {
                                "target_lang_word": "голубчик",
                                "target_lang_sense_clarification": "фамильярное или снисходительное обращение"
                            },
                            {
                                "target_lang_word": "дорогой мой",
                                "target_lang_sense_clarification": "обращение с оттенком превосходства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac7872b0-d0e4-389f-a468-e6356846e03c",
                    "sense_definition": "Duży środek transportu wodnego służący do przewozu ludzi lub towarów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na horyzoncie widać było sylwetkę wielkiego <statku>.",
                            "target_lang_translations": {
                                "ru": "На горизонте был виден силуэт большого <w>корабля</w>."
                            }
                        },
                        {
                            "text": "W porcie zacumował <statek> handlowy z egzotycznymi towarami.",
                            "target_lang_translations": {
                                "ru": "В порту пришвартовалось торговое <w>судно</w> с экзотическими товарами."
                            }
                        }
                    ],
                    "synonyms": [
                        "jednostka",
                        "łajba",
                        "korab",
                        "nawa"
                    ],
                    "common_phrases": [
                        "statek pasażerski",
                        "statek handlowy",
                        "płynąć statkiem",
                        "kapitan statku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное водное транспортное средство для перевозки людей или грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корабль",
                                "target_lang_sense_clarification": "крупное морское судно"
                            },
                            {
                                "target_lang_word": "судно",
                                "target_lang_sense_clarification": "водный транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be8beb73-7eee-3cbd-9bc0-e3069480318b",
                    "sense_definition": "Pojazd skonstruowany do poruszania się poza atmosferą ziemską lub w powietrzu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Statek> kosmiczny wylądował bezpiecznie na powierzchni Księżyca.",
                            "target_lang_translations": {
                                "ru": "Космический <w>корабль</w> благополучно приземлился на поверхности Луны."
                            }
                        },
                        {
                            "text": "Astronauci przygotowują się do wejścia na pokład <statku>.",
                            "target_lang_translations": {
                                "ru": "Астронавты готовятся подняться на борт <w>корабля</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jednostka"
                    ],
                    "common_phrases": [
                        "statek kosmiczny",
                        "statek powietrzny",
                        "załoga statku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство, предназначенное для полетов в космосе или воздухе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корабль",
                                "target_lang_sense_clarification": "космический аппарат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8caefc51-aeb4-3b38-9014-b5bd7e03c762",
                    "sense_definition": "Naczynia kuchenne i stołowe, zwłaszcza te brudne, przeznaczone do mycia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "household",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po obiedzie muszę pozmywać wszystkie brudne <statki>.",
                            "target_lang_translations": {
                                "ru": "После обеда я должен перемыть всю грязную <w>посуду</w>."
                            }
                        },
                        {
                            "text": "W zlewie leży sterta <statków> czekających na umycie.",
                            "target_lang_translations": {
                                "ru": "В раковине лежит гора <w>посуды</w>, ожидающей мытья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zmywać statki",
                        "myć statki"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in some regions of Poland"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кухонная и столовая посуда, особенно грязная, предназначенная для мытья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посуда",
                                "target_lang_sense_clarification": "кухонная утварь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1ae1a0e-56e6-37e5-abb1-f3c0cad1cf24",
                    "sense_definition": "Cecha osoby opanowanej, poważnej i rozsądnej; równowaga ducha.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z wiekiem nabrał <statku> i życiowej mądrości.",
                            "target_lang_translations": {
                                "ru": "С возрастом он набрался <w>степенности</w> и житейской мудрости."
                            }
                        },
                        {
                            "text": "Jego zachowanie cechuje wielki <statek> i rozwaga.",
                            "target_lang_translations": {
                                "ru": "Его поведение отличает большая <w>степенность</w> и рассудительность."
                            }
                        }
                    ],
                    "synonyms": [
                        "stateczność",
                        "powaga",
                        "rozsądek",
                        "ustatkowanie"
                    ],
                    "common_phrases": [
                        "nabrać statku",
                        "człowiek pełen statku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Черта характера, выражающаяся в сдержанности, серьезности и рассудительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "степенность",
                                "target_lang_sense_clarification": "черта характера"
                            },
                            {
                                "target_lang_word": "рассудительность",
                                "target_lang_sense_clarification": "здравый смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db405a85-ec04-3cb2-8eca-25f6ee1fb99d",
                    "sense_definition": "Cały dobytek, majątek lub gospodarstwo.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogień strawił cały jego <statek> zgromadzony przez lata.",
                            "target_lang_translations": {
                                "ru": "Огонь уничтожил всё его <w>имущество</w>, накопленное годами."
                            }
                        },
                        {
                            "text": "Zostawił synowi w spadku znaczny <statek>.",
                            "target_lang_translations": {
                                "ru": "Он оставил сыну в наследство значительное <w>состояние</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Всё имущество, пожитки или хозяйство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "имущество",
                                "target_lang_sense_clarification": "собственность"
                            },
                            {
                                "target_lang_word": "состояние",
                                "target_lang_sense_clarification": "богатство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29e2adcc-c642-3201-8a04-4b28242fff9f",
                    "sense_definition": "Zwierzęta gospodarskie, inwentarz żywy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz musi nakarmić <statek> w oborze.",
                            "target_lang_translations": {
                                "ru": "Хозяин должен накормить <w>скотину</w> в хлеву."
                            }
                        },
                        {
                            "text": "Wypędzili <statek> na łąkę wczesnym rankiem.",
                            "target_lang_translations": {
                                "ru": "Они выгнали <w>скот</w> на луг ранним утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "bydło"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Śląsk Cieszyński"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Домашний скот, живой инвентарь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скот",
                                "target_lang_sense_clarification": "домашние животные"
                            },
                            {
                                "target_lang_word": "скотина",
                                "target_lang_sense_clarification": "животные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74002022-d71e-363f-bb49-d43a80441bb4",
                    "sense_definition": "Narzędzie lub przyrząd potrzebny do wykonywania pracy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem wszystkie potrzebne <statki> stolarskie.",
                            "target_lang_translations": {
                                "ru": "Я купил все необходимые столярные <w>инструменты</w>."
                            }
                        },
                        {
                            "text": "Zgubił gdzieś swoje <statki> do rzeźbienia w drewnie.",
                            "target_lang_translations": {
                                "ru": "Он где-то потерял свои <w>принадлежности</w> для резьбы по дереву."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzędzie"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент или приспособление, необходимое для работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инструмент",
                                "target_lang_sense_clarification": "орудие труда"
                            },
                            {
                                "target_lang_word": "принадлежности",
                                "target_lang_sense_clarification": "аксессуары"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "fraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought a <w>quarter</w> pound of cheese from the deli.",
                            "target_lang_translations": {
                                "ru": "Мы купили <w>четверть</w> фунта сыра в гастрономе."
                            }
                        },
                        {
                            "text": "The recipe calls for a <w>quarter</w> cup of sugar.",
                            "target_lang_translations": {
                                "ru": "Рецепт требует <w>четверть</w> чашки сахара."
                            }
                        }
                    ],
                    "antonyms": [
                        "quadruple"
                    ],
                    "common_phrases": [
                        "quarter pound",
                        "quarter century"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из четвертой части; равный 25 процентам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "че́тверть",
                                "target_lang_sense_clarification": "мера количества"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5e6e2910-0743-33d7-a3a9-4f87b42b8e80",
                    "sense_definition": "A village in South Lanarkshire, Scotland.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "My ancestors lived in the small village of <w>Quarter</w> in Scotland.",

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "61dc3b8c-74d2-31a5-9839-aef45ff6b664",
                    "sense_definition": "Drobne zwierzę o ośmiu nogach (pajęczak), które często tka sieci w celu chwytania owadów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W rogu pokoju siedzi duży, czarny <w>pająk</w>.",
                            "target_lang_translations": {
                                "ru": "В углу комнаты сидит большой черный <w>паук</w>."
                            }
                        },
                        {
                            "text": "Wiele osób boi się, że ugryzie je jadowity <w>pająk</w>.",
                            "target_lang_translations": {
                                "ru": "Многие боятся, что их укусит ядовитый <w>паук</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bać się pająków",
                        "sieć pająka",
                        "jadowity pająk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкое восьминогое животное (паукообразное), которое часто плетет паутину для ловли насекомых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паук",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "634cb0ec-c4f4-34b3-83cc-a286471b4f09",
                    "sense_definition": "Popularna odmiana gry karcianej dla jednej osoby (pasjansa), często dostępna na komputerach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy się nudzę w biurze, układam na komputerze <w>pająka</w>.",
                            "target_lang_translations": {
                                "ru": "Когда мне скучно в офисе, я раскладываю на компьютере <w>паука</w>."
                            }
                        },
                        {
                            "text": "Ta wersja <w>pająka</w> jest bardzo trudna do ułożenia.",
                            "target_lang_translations": {
                                "ru": "Эту версию <w>паука</w> очень сложно сложить."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasjans"
                    ],
                    "common_phrases": [
                        "układać pająka",
                        "grać w pająka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная разновидность карточной игры для одного человека (пасьянс), часто доступная на компьютерах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паук",
                                "target_lang_sense_clarification": "пасьянс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51191b00-32f0-3c84-bcc2-0a2b55932f6c",
                    "sense_definition": "Duży, ozdobny żyrandol o wielu ramionach, często wykonany z kryształu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad stołem w jadalni wisiał stary, kryształowy <w>pająk</w>.",
                            "target_lang_translations": {
                                "ru": "Над столом в столовой висела старая хрустальная <w>люстра</w>."
                            }
                        },
                        {
                            "text": "Światło odbijało się od szklanych elementów wielkiego <w>pająka</w>.",
                            "target_lang_translations": {
                                "ru": "Свет отражался от стеклянных элементов огромной <w>люстры</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "żyrandol"
                    ],
                    "common_phrases": [
                        "kryształowy pająk"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "word used mostly in literature or regarding antiques"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая декоративная люстра с множеством рожков, часто сделанная из хрусталя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "люстра",
                                "target_lang_sense_clarification": "светильник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ed9402f-a761-39f7-a7ae-d473c98da26d",
                    "sense_definition": "Tradycyjna ozdoba ludowa wykonywana ze słomy, papieru i piór, wieszana pod sufitem w wiejskich izbach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na warsztatach etnograficznych dzieci uczyły się robić ludowego <w>pająka</w>.",
                            "target_lang_translations": {
                                "ru": "На этнографических мастер-классах дети учились делать народного <w>паука</w>."
                            }
                        },
                        {
                            "text": "Kolorowy <w>pająk</w> ze słomy obracał się powoli nad stołem wigilijnym.",
                            "target_lang_translations": {
                                "ru": "Разноцветный соломенный <w>паук</w> медленно вращался над рождественским столом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pająk ludowy",
                        "pająk ze słomy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "folk culture context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционное народное украшение из соломы, бумаги и перьев, подвешиваемое под потолком в деревенских избах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паук",
                                "target_lang_sense_clarification": "украшение из соломы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d12c432d-09bd-3270-a0d4-b7749afb0d33",
                    "sense_definition": "Prowizoryczny sposób montażu układu elektronicznego bez płytki drukowanej, poprzez bezpośrednie lutowanie elementów.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zlutuj ten próbny układ na <w>pająka</w>, żeby szybko sprawdzić, czy działa.",
                            "target_lang_translations": {
                                "ru": "Спаяй эту пробную схему <w>навесным монтажом</w>, чтобы быстро проверить, работает ли она."
                            }
                        },
                        {
                            "text": "Wewnątrz obudowy panował bałagan, a kable były połączone na <w>pająka</w>.",
                            "target_lang_translations": {
                                "ru": "Внутри корпуса был беспорядок, а кабели были соединены <w>навесным монтажом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "montaż na pająka",
                        "lutować na pająka"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "technical slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кустарный способ монтажа электронной схемы без печатной платы, путем непосредственной пайки элементов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навесной монтаж",
                                "target_lang_sense_clarification": "электроника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b9a1981-ba11-30dc-96a9-cb4cffd304e8",
                    "sense_definition": "Przyrząd pomocniczy w bilardzie i snookerze, służący do podwyższenia kija nad bilami.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia podał zawodnikowi <w>pająka</w>, aby ten mógł wykonać trudne uderzenie.",
                            "target_lang_translations": {
                                "ru": "Судья подал игроку <w>паука</w>, чтобы тот мог выполнить сложный удар."
                            }
                        },
                        {
                            "text": "Użycie <w>pająka</w> wymaga dużej precyzji i stabilnej ręki.",
                            "target_lang_translations": {
                                "ru": "Использование <w>паука</w> требует большой точности и твердой руки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательное приспособление в бильярде и снукере, служащее для поднятия кия над шарами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паук",
                                "target_lang_sense_clarification": "бильярд"
                            },
                            {
                                "target_lang_word": "машинка",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        },

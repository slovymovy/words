                },
                {
                    "sense_id": "39f48a35-f713-3418-8454-32cc27fbd0cc",
                    "sense_definition": "A surname derived from the name Robert.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "name_rob",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Rob</w> has been working at the company for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Роб</w> работает в компании уже более двадцати лет."
                            }
                        },
                        {
                            "text": "We looked up the history of the <w>Rob</w> family in the local archives.",
                            "target_lang_translations": {
                                "ru": "Мы искали историю семьи <w>Роб</w> в местных архивах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, образованная от имени Роберт."
                    },
                    "translations": {

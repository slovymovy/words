    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a24337d1-3ff9-3f8c-989a-441681e9511e",
                    "sense_definition": "Z każdego miejsca, ze wszystkich stron; określa kierunek do środka z wielu punktów wokół.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Płaskowyż <zewsząd> otaczały łańcuchy górskie, które na północy tworzyły masyw spiętrzonych szczytów.",
                            "target_lang_translations": {
                                "ru": "Плато <со всех сторон> окружали горные цепи, которые на севере образовывали массив нагромождённых вершин."
                            }
                        },
                        {
                            "text": "Podczas spaceru po lesie śpiew ptaków dobiegał nas <zewsząd>.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу пение птиц доносилось до нас <отовсюду>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ze wszech stron",
                        "dokoła"
                    ],
                    "antonyms": [
                        "znikąd"
                    ],
                    "common_phrases": [
                        "atakować zewsząd",
                        "otoczony zewsząd",
                        "napływać zewsząd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Со всех сторон, из разных мест; обозначает направление к центру из множества точек вокруг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отовсюду",
                                "target_lang_sense_clarification": "из каждого места"
                            },
                            {
                                "target_lang_word": "со всех сторон",
                                "target_lang_sense_clarification": "кругом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1a03cff-69bd-3d52-aa3e-224f97185328",
                    "sense_definition": "Biorąc pod uwagę wszystkie aspekty; całkowicie, w pełni.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego postawa w tej trudnej sytuacji była <zewsząd> godna pochwały.",
                            "target_lang_translations": {
                                "ru": "Его поведение в этой трудной ситуации было <во всех отношениях> достойно похвалы."
                            }
                        },
                        {
                            "text": "To był <zewsząd> udany projekt, który przyniósł firmie wiele korzyści.",
                            "target_lang_translations": {
                                "ru": "Это был <во всех отношениях> удачный проект, который принёс фирме много выгоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "pod każdym względem"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "użycie książkowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимая во внимание все аспекты; полностью, во всех отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "во всех отношениях",
                                "target_lang_sense_clarification": "с любой точки зрения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

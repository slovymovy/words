{
    "word_family": [
        "adjustment",
        "adjustable",
        "adjuster",
        "readjust",
        "maladjusted",
        "readjustment"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4e415797-b1cd-3c71-99d9-203534d67b26",
                    "sense_definition": "To change something slightly to make it more correct, effective, or suitable.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "modification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>adjust</w> the height of the chair to fit your desk.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>отрегулировать</w> высоту стула под ваш стол."
                            }
                        },
                        {
                            "text": "Please <w>adjust</w> the volume if the music is too loud.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>настройте</w> громкость, если музыка играет слишком громко."
                            }
                        }
                    ],
                    "synonyms": [
                        "modify",
                        "change",
                        "edit",
                        "set"
                    ],
                    "common_phrases": [
                        "adjust the volume",
                        "adjust the settings",
                        "adjust the mirror"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить что-то незначительно, чтобы сделать это более правильным, эффективным или подходящим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регулировать",
                                "target_lang_sense_clarification": "механизмы, высоту, процессы"
                            },
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "параметры, громкость, оборудование"
                            },
                            {
                                "target_lang_word": "поправлять",
                                "target_lang_sense_clarification": "одежду, положение предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7666bdc4-ed09-3645-90ce-c1a6cbbb7e5f",
                    "sense_definition": "To change one's behavior or ideas to fit new circumstances; to adapt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adaptation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took him a few months to <w>adjust</w> to life in a new country.",
                            "target_lang_translations": {
                                "ru": "Ему потребовалось несколько месяцев, чтобы <w>приспособиться</w> к жизни в новой стране."
                            }
                        },
                        {
                            "text": "Her eyes slowly <w>adjusted</w> to the darkness of the room.",
                            "target_lang_translations": {
                                "ru": "Её глаза медленно <w>привыкли</w> к темноте в комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "change"
                    ],
                    "common_phrases": [
                        "adjust to life",
                        "adjust quickly",
                        "adjust to the situation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить свое поведение или взгляды, чтобы соответствовать новым обстоятельствам; адаптироваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приспосабливаться",
                                "target_lang_sense_clarification": "к новым условиям, к жизни"
                            },
                            {
                                "target_lang_word": "адаптироваться",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "привыкать",
                                "target_lang_sense_clarification": "к темноте, к новой обстановке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f914e9b3-b18f-3d28-b034-0a6c99007dbc",
                    "sense_definition": "To correct or rectify data or figures to reflect new information.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "modification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The economist <w>adjusted</w> the figures to account for inflation.",
                            "target_lang_translations": {
                                "ru": "Экономист <w>скорректировал</w> цифры с учетом инфляции."
                            }
                        },
                        {
                            "text": "He <w>adjusted</w> his initial conclusion after reviewing the new evidence.",
                            "target_lang_translations": {
                                "ru": "Он <w>скорректировал</w> свой первоначальный вывод после изучения новых доказательств."
                            }
                        }
                    ],
                    "synonyms": [
                        "modify",
                        "change"
                    ],
                    "common_phrases": [
                        "adjust for inflation",
                        "adjust the data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исправить или уточнить данные или цифры с учетом новой информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корректировать",
                                "target_lang_sense_clarification": "данные, планы, выводы"
                            },
                            {
                                "target_lang_word": "вносить поправки",
                                "target_lang_sense_clarification": "в документы, в расчеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cc02fff-4c57-3c90-b3b9-2d30385383e6",
                    "sense_definition": "To assess and settle an insurance claim.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The agent arrived to <w>adjust</w> the claim for the fire damage.",
                            "target_lang_translations": {
                                "ru": "Агент прибыл, чтобы <w>оценить</w> ущерб от пожара по страховой претензии."
                            }
                        },
                        {
                            "text": "They will <w>adjust</w> the loss based on the terms of the policy.",
                            "target_lang_translations": {
                                "ru": "Они <w>рассчитают</w> сумму убытка на основании условий полиса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "adjust a claim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценить ущерб и определить сумму выплаты по страховому случаю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "урегулировать",
                                "target_lang_sense_clarification": "страховую претензию"
                            },
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "размер ущерба"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
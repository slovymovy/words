            "senses": [
                {
                    "sense_id": "a45562bd-d903-352b-b2f4-666aae520dcc",
                    "sense_definition": "zdrobnienie od <w>krok</w>; mały, krótki krok, często stawiany niepewnie (np. przez dziecko) lub w tańcu",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko postawiło swój pierwszy niepewny <w>kroczek</w> w stronę mamy.",
                            "target_lang_translations": {
                                "ru": "Ребенок сделал свой первый неуверенный <w>шажок</w> в сторону мамы."
                            }
                        },
                        {
                            "text": "Musimy realizować ten skomplikowany plan powoli, <w>kroczek</w> po kroczku.",
                            "target_lang_translations": {
                                "ru": "Мы должны реализовывать этот сложный план медленно, <w>шажок</w> за шажком."
                            }
                        }
                    ],
                    "synonyms": [
                        "kroczuś"
                    ],
                    "antonyms": [
                        "sus"
                    ],
                    "common_phrases": [
                        "metoda małych kroczków",
                        "stawiać pierwsze kroczki",
                        "kroczek po kroczku"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьшительно-ласкательное от слова «шаг»; маленький, короткий шаг, часто неуверенный (например, у ребенка) или в танце"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шажок",
                                "target_lang_sense_clarification": "уменьшительное от шаг"
                            },
                            {
                                "target_lang_word": "маленький шаг",
                                "target_lang_sense_clarification": "небольшое движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f40c7048-de5b-330e-9e15-60a5300e4d74",
                    "sense_definition": "precyzyjny przyrząd pomiarowy w kształcie cyrkla o dwóch ostrzach, służący do odmierzania małych odcinków, zwłaszcza na mapach",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nawigator użył <w>kroczka</w>, aby dokładnie zmierzyć odległość na mapie morskiej.",
                            "target_lang_translations": {
                                "ru": "Штурман использовал <w>измерительный циркуль</w>, чтобы точно измерить расстояние на морской карте."
                            }
                        },
                        {
                            "text": "W pracowni kartograficznej każdy <w>kroczek</w> musi być idealnie skalibrowany.",
                            "target_lang_translations": {
                                "ru": "В картографической мастерской каждый <w>циркуль-измеритель</w> должен быть идеально откалиброван."
                            }
                        }
                    ],
                    "synonyms": [
                        "przenośnik",
                        "cyrkiel podziałowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "точный измерительный прибор в виде циркуля с двумя иглами, используемый для измерения небольших отрезков, особенно на картах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерительный циркуль",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "циркуль-измеритель",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05c3a9be-2ae4-3617-a7c0-f416a9d5c9cf",
                    "sense_definition": "ryba hodowlana (zazwyczaj karp) w drugim roku życia",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosną hodowca wpuścił <w>kroczki</w> do większego stawu, aby urosły do jesieni.",
                            "target_lang_translations": {
                                "ru": "Весной рыбовод выпустил <w>двухлеток</w> в более крупный пруд, чтобы они подросли к осени."
                            }
                        },
                        {
                            "text": "Każdy <w>kroczek</w> jest regularnie ważony, aby kontrolować przyrost masy ryb.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>двухлеток</w> регулярно взвешивается, чтобы контролировать прирост массы рыбы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "staw dla kroczków"
                    ],
                    "target_lang_definitions": {
                        "ru": "разводимая рыба (обычно карп) на втором году жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двухлеток",
                                "target_lang_sense_clarification": "рыба"
                            },
                            {
                                "target_lang_word": "рыба-двухлеток",
                                "target_lang_sense_clarification": "возраст рыбы"
                            }
                        ]
                    }
                }
            ]
        }

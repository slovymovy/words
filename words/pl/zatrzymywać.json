{
    "word_family": [
        "zatrzymać",
        "zatrzymanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6c046c20-3151-3acf-b5b0-15b2467e9c38",
                    "sense_definition": "Sprawiać, że ktoś lub coś przestaje się poruszać, działać lub wykonywać jakąś czynność.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant podniósł rękę, aby <w>zatrzymywać</w> nadjeżdżające samochody do kontroli.",
                            "target_lang_translations": {
                                "ru": "Полицейский поднял руку, чтобы <w>останавливать</w> приближающиеся автомобили для проверки."
                            }
                        },
                        {
                            "text": "Ten nowoczesny system hamulcowy pozwala skutecznie <w>zatrzymywać</w> pociąg w nagłych wypadkach.",
                            "target_lang_translations": {
                                "ru": "Эта современная тормозная система позволяет эффективно <w>останавливать</w> поезд в экстренных случаях."
                            }
                        }
                    ],
                    "synonyms": [
                        "unieruchamiać",
                        "stopować"
                    ],
                    "antonyms": [
                        "uruchamiać"
                    ],
                    "common_phrases": [
                        "zatrzymywać samochód",
                        "zatrzymywać ruch",
                        "zatrzymywać krwawienie",
                        "zatrzymywać czas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то или что-то прекратить движение или действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливать",
                                "target_lang_sense_clarification": "прекращать движение"
                            },
                            {
                                "target_lang_word": "задерживать",
                                "target_lang_sense_clarification": "препятствовать движению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83dfc2bd-d86d-34ff-8016-5886aa56917d",
                    "sense_definition": "Przestawać się poruszać, iść lub jechać; stawać w miejscu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autobus miejski zwykł <w>zatrzymywać</w> się na każdym przystanku na tej trasie.",
                            "target_lang_translations": {
                                "ru": "Городской автобус обычно <w>останавливается</w> на каждой остановке этого маршрута."
                            }
                        },
                        {
                            "text": "Musimy często <w>zatrzymywać</w> się podczas wspinaczki, aby wyrównać oddech.",
                            "target_lang_translations": {
                                "ru": "Нам приходится часто <w>останавливаться</w> во время восхождения, чтобы восстановить дыхание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zatrzymywać się nagle",
                        "zatrzymywać się na światłach",
                        "zatrzymywać się w połowie drogi"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (używany z 'się')"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать двигаться, идти или ехать; замирать на месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливаться",
                                "target_lang_sense_clarification": "прекращать движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65f5c745-9c10-3eaf-a26f-ba3cec55b8b7",
                    "sense_definition": "Przebywać w jakimś miejscu przez pewien czas, na przykład nocować podczas podróży.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "accommodation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji lubimy <w>zatrzymywać</w> się w małych, rodzinnych pensjonatach.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы любим <w>останавливаться</w> в небольших семейных пансионатах."
                            }
                        },
                        {
                            "text": "Zawsze, gdy jestem w Warszawie, staram się <w>zatrzymywać</w> się u moich przyjaciół.",
                            "target_lang_translations": {
                                "ru": "Всегда, когда я бываю в Варшаве, я стараюсь <w>останавливаться</w> у моих друзей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zatrzymywać się w hotelu",
                        "zatrzymywać się na noc",
                        "zatrzymywać się u rodziny"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (używany z 'się')"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно проживать где-либо, например, ночевать во время путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливаться",
                                "target_lang_sense_clarification": "временно селиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f7b0e97-6def-3108-84f1-3ebdcdb37cd4",
                    "sense_definition": "Nie oddawać czegoś, zachowywać coś w swoim posiadaniu lub nie ujawniać czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klient powiedział kelnerowi, że może <w>zatrzymywać</w> resztę jako napiwek.",
                            "target_lang_translations": {
                                "ru": "Клиент сказал официанту, что тот может <w>оставить</w> сдачу себе в качестве чаевых."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>zatrzymywać</w> dla siebie informacji, które mogą pomóc w śledztwie.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>оставлять</w> при себе информацию, которая может помочь следствию."
                            }
                        }
                    ],
                    "antonyms": [
                        "pozbywać się"
                    ],
                    "common_phrases": [
                        "zatrzymywać resztę",
                        "zatrzymywać dla siebie",
                        "zatrzymywać w pamięci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не отдавать что-то, сохранять что-то у себя или не разглашать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "сохранять у себя"
                            },
                            {
                                "target_lang_word": "задерживать",
                                "target_lang_sense_clarification": "не отдавать вовремя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dec423ea-b6d2-3744-8355-675f1a0cf228",
                    "sense_definition": "Pozbawiać kogoś wolności, aresztować lub zmuszać do pozostania w danym miejscu wbrew woli.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja ma prawo <w>zatrzymywać</w> osoby podejrzane o popełnienie przestępstwa na 48 godzin.",
                            "target_lang_translations": {
                                "ru": "Полиция имеет право <w>задерживать</w> лиц, подозреваемых в совершении преступления, на 48 часов."
                            }
                        },
                        {
                            "text": "Strażnicy graniczni musieli <w>zatrzymywać</w> każdego, kto próbował nielegalnie przekroczyć granicę.",
                            "target_lang_translations": {
                                "ru": "Пограничники должны были <w>задерживать</w> каждого, кто пытался незаконно пересечь границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojmać"
                    ],
                    "antonyms": [
                        "zwalniać",
                        "wypuszczać"
                    ],
                    "common_phrases": [
                        "zatrzymywać podejrzanego",
                        "zatrzymywać do kontroli",
                        "zatrzymywać zakładników"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать кого-то свободы, брать под стражу или принуждать остаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задерживать",
                                "target_lang_sense_clarification": "брать под стражу"
                            },
                            {
                                "target_lang_word": "арестовывать",
                                "target_lang_sense_clarification": "лишать свободы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
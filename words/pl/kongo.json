                {
                    "sense_id": "529d0495-10b1-31b0-a01c-95365dcd7b8b",
                    "sense_definition": "Państwo w Afryce Środkowej ze stolicą w Brazzaville, położone na zachód od rzeki Kongo.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "countries_congo",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Planujemy podróż do <w>Konga</w>, aby zobaczyć lasy równikowe.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Конго</w>, чтобы увидеть экваториальные леса."
                            }
                        },
                        {
                            "text": "Stolicą <w>Konga</w> jest miasto Brazzaville, leżące nad rzeką.",
                            "target_lang_translations": {
                                "ru": "Столицей <w>Конго</w> является город Браззавиль, расположенный на реке."
                            }
                        }
                    ],
                    "synonyms": [
                        "Republika Konga",
                        "Kongo-Brazzaville",
                        "Kongo Francuskie",
                        "Kongo Środkowe"
                    ],
                    "common_phrases": [
                        "Republika Konga",
                        "wyjazd do Konga",
                        "stolica Konga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство в Центральной Африке со столицей в Браззавиле, расположенное к западу от реки Конго."
                    },
                    "translations": {

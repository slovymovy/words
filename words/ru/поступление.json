    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "194cfaa5-e7d7-3797-aea6-eec6850e29f5",
                    "sense_definition": "Зачисление в учебное заведение или приём на работу, вступление в состав организации.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "admission_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её успешное <поступление> в университет стало большой радостью для родителей.",
                            "target_lang_translations": {
                                "en": "Her successful <admission> to the university was a great joy for her parents."
                            }
                        },
                        {
                            "text": "Для <поступления> на эту должность необходимо пройти сложное собеседование.",
                            "target_lang_translations": {
                                "en": "For <entry> into this position, it is necessary to pass a difficult interview."
                            }
                        }
                    ],
                    "synonyms": [
                        "зачисление",
                        "приём"
                    ],
                    "antonyms": [
                        "отчисление",
                        "выпуск",
                        "увольнение"
                    ],
                    "common_phrases": [
                        "поступление в вуз",
                        "поступление на работу",
                        "правила поступления",
                        "экзамены для поступления"
                    ],
                    "target_lang_definitions": {
                        "en": "Enrollment in an educational institution, acceptance for a job, or joining an organization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "admission",
                                "target_lang_sense_clarification": "permission to enter an institution (e.g., university)"
                            },
                            {
                                "target_lang_word": "enrollment",
                                "target_lang_sense_clarification": "the process of registering for a school or course"
                            },
                            {
                                "target_lang_word": "entry",
                                "target_lang_sense_clarification": "the act of joining a group or profession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "846d8d53-07ed-319e-8533-c7a92c4887ad",
                    "sense_definition": "Приход, появление или доставка чего-либо (товаров, денег, информации) в место назначения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrival_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В магазине ожидается новое <поступление> летней коллекции одежды.",
                            "target_lang_translations": {
                                "en": "A new <arrival> of the summer clothing collection is expected in the store."
                            }
                        },
                        {
                            "text": "Регулярное <поступление> свежих новостей важно для работы журналиста.",
                            "target_lang_translations": {
                                "en": "The regular <inflow> of fresh news is important for a journalist's work."
                            }
                        }
                    ],
                    "synonyms": [
                        "приход",
                        "прибытие",
                        "получение"
                    ],
                    "antonyms": [
                        "расход",
                        "убытие",
                        "отправка"
                    ],
                    "common_phrases": [
                        "новое поступление",
                        "поступление товаров",
                        "поступление заказа",
                        "поступление средств"
                    ],
                    "target_lang_definitions": {
                        "en": "The arrival, appearance, or delivery of something (goods, money, information) to a destination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arrival",
                                "target_lang_sense_clarification": "the act of arriving (e.g., new stock)"
                            },
                            {
                                "target_lang_word": "receipt",
                                "target_lang_sense_clarification": "the act of receiving something sent"
                            },
                            {
                                "target_lang_word": "inflow",
                                "target_lang_sense_clarification": "a continuous coming in (e.g., of information or funds)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0399faf-d027-36b0-bbd5-2d5f3284e4e5",
                    "sense_definition": "Полученные денежные средства, доходы или материальные ценности (часто во множественном числе).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial_income",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Налоговые <поступления> в государственный бюджет значительно выросли в этом году.",
                            "target_lang_translations": {
                                "en": "Tax <revenues> to the state budget grew significantly this year."
                            }
                        },
                        {
                            "text": "Бухгалтер внимательно проверил все денежные <поступления> за последний месяц.",
                            "target_lang_translations": {
                                "en": "The accountant carefully checked all cash <receipts> for the last month."
                            }
                        }
                    ],
                    "synonyms": [
                        "доход",
                        "выручка",
                        "прибыль"
                    ],
                    "antonyms": [
                        "расходы",
                        "траты",
                        "издержки"
                    ],
                    "common_phrases": [
                        "налоговые поступления",
                        "бюджетные поступления",
                        "денежные поступления"
                    ],
                    "target_lang_definitions": {
                        "en": "Received funds, income, or material assets (often used in plural)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "revenues",
                                "target_lang_sense_clarification": "income, especially of a company or state"
                            },
                            {
                                "target_lang_word": "receipts",
                                "target_lang_sense_clarification": "amount of money received"
                            },
                            {
                                "target_lang_word": "income",
                                "target_lang_sense_clarification": "money received for work or through investments"
                            }
                        ]
                    }
                }
            ]
        }
    ]

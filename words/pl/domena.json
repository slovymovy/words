                {
                    "sense_id": "628c5769-28bf-3dd1-8d2b-d5b45c0945a6",
                    "sense_definition": "Unikalna nazwa w internecie, która identyfikuje stronę internetową lub serwer i pozwala na ich znalezienie",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "internet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy odnowić naszą <w>domenę</w> przed końcem miesiąca.",
                            "target_lang_translations": {
                                "ru": "Мы должны продлить наш <w>домен</w> до конца месяца."
                            }
                        },
                        {
                            "text": "Rejestracja <w>domeny</w> jest pierwszym krokiem do założenia strony.",
                            "target_lang_translations": {
                                "ru": "Регистрация <w>домена</w> — это первый шаг к созданию сайта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "domena internetowa",
                        "rejestracja domeny",
                        "wygasła domena"
                    ],
                    "target_lang_definitions": {

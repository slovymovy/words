                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попадать",
                                "target_lang_sense_clarification": "упасть (о многих объектах)"
                            },
                            {
                                "target_lang_word": "разориться",
                                "target_lang_sense_clarification": "обанкротиться (о фирмах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2beba34e-ee66-3155-a488-e6ed8a717554",
                    "sense_definition": "O skórze: stać się szorstką i popękaną pod wpływem wiatru lub mrozu",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz posmarować dłonie kremem, bo strasznie ci <w>popadały</w> na tym mrozie.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно смазать ладони кремом, потому что они у тебя сильно <w>потрескались</w> на этом морозе."
                            }
                        },
                        {
                            "text": "Usta <w>popadały</w> mi od wiatru i teraz bardzo pieką.",
                            "target_lang_translations": {

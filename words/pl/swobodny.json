                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mój pradziadek przez pewien czas mieszkał w mieście <w>Swobodny</w> na Dalekim Wschodzie.",
                            "target_lang_translations": {
                                "ru": "Мой прадедушка некоторое время жил в городе <w>Свободный</w> на Дальнем Востоке."

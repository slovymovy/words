    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88e382c6-3cf6-3fc5-89f5-4ef4038e2d58",
                    "sense_definition": "Категорическое возражение против чего-либо, решительное заявление о несогласии с чьими-либо действиями или мнением.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_objection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Жители города вышли на центральную площадь, чтобы выразить свой <w>протест</w> против строительства завода."
                        },
                        {
                            "text": "В знак <w>протеста</w> сотрудники компании отказались выходить на работу до повышения зарплаты."
                        }
                    ],
                    "synonyms": [
                        "выступление"
                    ],
                    "antonyms": [
                        "одобрение"
                    ],
                    "common_phrases": [
                        "выразить протест",
                        "знак протеста",
                        "акция протеста",
                        "вызвать протест"
                    ]
                },
                {
                    "sense_id": "19f87261-d343-3f72-ad5a-139bfa993125",
                    "sense_definition": "Официальное нотариальное подтверждение факта неуплаты долга по векселю в установленный срок.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Банк направил документы нотариусу, чтобы своевременно оформить <w>протест</w> векселя."
                        },
                        {
                            "text": "Если должник не платит вовремя, кредитор должен совершить <w>протест</w> в неплатеже для защиты своих прав."
                        }
                    ],
                    "common_phrases": [
                        "протест векселя",
                        "совершить протест"
                    ]
                },
                {
                    "sense_id": "c3d60183-7cb6-3403-ac14-60a3ae498c43",
                    "sense_definition": "Официальное заявление капитана судна о происшествии во время плавания или о повреждении груза для обеспечения требований к страховщикам.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "maritime_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан обязан заявить морской <w>протест</w> в течение двадцати четырёх часов после прибытия в порт."
                        },
                        {
                            "text": "Без правильно оформленного <w>протеста</w> судовладелец не сможет получить страховую выплату за испорченный груз."
                        }
                    ],
                    "common_phrases": [
                        "морской протест",
                        "заявить протест"
                    ]
                }
            ]
        }
    ]
}
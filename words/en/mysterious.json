            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "768cdfae-5d43-3319-8c70-d8751b6a1140",
                    "sense_definition": "Difficult to understand, explain, or identify; strange and interesting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hard_to_understand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police are still investigating the <w>mysterious</w> disappearance of the painting.",
                            "target_lang_translations": {
                                "ru": "Полиция всё ещё расследует <w>таинственное</w> исчезновение картины."
                            }
                        },
                        {
                            "text": "She received a <w>mysterious</w> letter with no return address.",
                            "target_lang_translations": {
                                "ru": "Она получила <w>загадочное</w> письмо без обратного адреса."
                            }
                        }
                    ],
                    "synonyms": [
                        "inexplicable",
                        "unexplainable",
                        "incomprehensible",
                        "enigmatic",
                        "baffling"
                    ],
                    "antonyms": [
                        "known",
                        "well-defined",
                        "unambiguous"
                    ],
                    "common_phrases": [
                        "mysterious circumstances",
                        "mysterious ways",
                        "mysterious illness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудный для понимания, объяснения или опознания; странный и вызывающий интерес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таинственный",
                                "target_lang_sense_clarification": "необъяснимый, окруженный тайной"
                            },
                            {
                                "target_lang_word": "загадочный",
                                "target_lang_sense_clarification": "содержащий загадку, непонятный"
                            },
                            {
                                "target_lang_word": "непостижимый",
                                "target_lang_sense_clarification": "невозможный для понимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b66bcf6d-c3b0-3913-b3f8-5a9db1056890",
                    "sense_definition": "Deliberately secretive or enigmatic about one's actions or nature.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "secretive_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave a <w>mysterious</w> smile when I asked about his plans.",
                            "target_lang_translations": {
                                "ru": "На его лице появилась <w>загадочная</w> улыбка, когда я спросил о его планах."
                            }
                        },
                        {
                            "text": "Stop being so <w>mysterious</w> and just tell us what happened.",
                            "target_lang_translations": {
                                "ru": "Перестань быть таким <w>таинственным</w> и просто расскажи нам, что случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "secretive",
                        "enigmatic",
                        "cryptic",
                        "furtive"
                    ],
                    "antonyms": [
                        "unmysterious"
                    ],
                    "common_phrases": [
                        "mysterious stranger",
                        "mysterious smile",
                        "mysterious look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно скрытный или создающий интригу вокруг своих действий или характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загадочный",
                                "target_lang_sense_clarification": "выражающий скрытность или недосказанность"
                            },
                            {
                                "target_lang_word": "таинственный",
                                "target_lang_sense_clarification": "напускающий на себя таинственность"
                            }
                        ]
                    }
                }
            ]
        }

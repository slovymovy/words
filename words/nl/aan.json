                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "сленг: очень круто"
                            },
                            {
                                "target_lang_word": "качает",
                                "target_lang_sense_clarification": "об атмосфере или музыке"
                            }
                        ]
                    }
                }
            ]
        }

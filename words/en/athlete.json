            "pos": "noun",
            "senses": [
                {
                    "sense_id": "54b3547f-de55-330a-b1ec-f1af666ee2a2",
                    "sense_definition": "A person who actively participates in physical sports, especially with great skill.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sports_participant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a talented <w>athlete</w> who trains every day.",
                            "target_lang_translations": {
                                "ru": "Она талантливая <w>спортсменка</w>, которая тренируется каждый день."
                            }
                        },
                        {
                            "text": "Becoming a professional <w>athlete</w> requires years of dedication.",
                            "target_lang_translations": {
                                "ru": "Чтобы стать профессиональным <w>спортсменом</w>, требуются годы самоотверженного труда."
                            }
                        }
                    ],
                    "synonyms": [
                        "sportsperson",
                        "sportsman",
                        "sportswoman"
                    ],
                    "antonyms": [
                        "nonathlete"
                    ],
                    "common_phrases": [
                        "professional athlete",
                        "star athlete",
                        "elite athlete",
                        "trained athlete"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который активно занимается спортом, особенно обладающий большим мастерством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спортсмен",
                                "target_lang_sense_clarification": "общее название для занимающихся спортом"
                            },
                            {
                                "target_lang_word": "атлет",
                                "target_lang_sense_clarification": "обычно в силовых видах спорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ea7fb6a-e456-34c2-a9a1-78b4e0eed8bb",
                    "sense_definition": "A participant in any of a group of sporting activities including track and field, road running, cross country running, and racewalking.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_participant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>athlete</w> prepared for the 100-meter sprint.",
                            "target_lang_translations": {
                                "ru": "<w>Легкоатлет</w> готовился к забегу на 100 метров."
                            }
                        },
                        {
                            "text": "Every track <w>athlete</w> must register before the competition begins.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>легкоатлет</w> должен зарегистрироваться до начала соревнований."
                            }
                        }
                    ],
                    "synonyms": [
                        "competitor"
                    ],
                    "common_phrases": [
                        "track athlete",
                        "field athlete",
                        "endurance athlete"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участник спортивных соревнований, включающих бег, прыжки и метания (лёгкая атлетика)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легкоатлет",
                                "target_lang_sense_clarification": "спортсмен в лёгкой атлетике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8370503f-9040-3b23-8d04-a6debdc3f07e",
                    "sense_definition": "An exceptionally physically fit person.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a natural <w>athlete</w> and excels at every game he plays.",
                            "target_lang_translations": {
                                "ru": "Он прирожденный <w>атлет</w> и преуспевает в любой игре."
                            }
                        },
                        {
                            "text": "You do not have to be a pro to look like an <w>athlete</w>.",
                            "target_lang_translations": {
                                "ru": "Не обязательно быть профи, чтобы выглядеть как <w>атлет</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "natural athlete"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек с отличной физической формой и развитой мускулатурой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атлет",
                                "target_lang_sense_clarification": "человек крепкого телосложения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

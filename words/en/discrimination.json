    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ed46cb3-6b32-3852-92be-ae3bab0544cd",
                    "sense_definition": "Unfair treatment of a person or group based on prejudice rather than individual merit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company was sued for <w>discrimination</w> against female employees.",
                            "target_lang_translations": {
                                "ru": "На компанию подали в суд за <w>дискриминацию</w> сотрудниц."
                            }
                        },
                        {
                            "text": "We must fight against racial <w>discrimination</w> in our society.",
                            "target_lang_translations": {
                                "ru": "Мы должны бороться с расовой <w>дискриминацией</w> в нашем обществе."
                            }
                        }
                    ],
                    "antonyms": [
                        "equality",
                        "fairness"
                    ],
                    "common_phrases": [
                        "racial discrimination",
                        "sex discrimination",
                        "age discrimination",
                        "reverse discrimination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несправедливое отношение к человеку или группе людей, основанное на предубеждениях, а не на личных заслугах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дискриминация",
                                "target_lang_sense_clarification": "ущемление прав"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7557efc-66e9-31ec-843d-969bb3def72b",
                    "sense_definition": "The ability or act of recognizing the difference between one thing and another.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "discernment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The test requires the <w>discrimination</w> between similar colors.",
                            "target_lang_translations": {
                                "ru": "Тест требует <w>различения</w> похожих цветов."
                            }
                        },
                        {
                            "text": "Dogs have a keen sense of smell that allows for the <w>discrimination</w> of many scents.",
                            "target_lang_translations": {
                                "ru": "У собак острое обоняние, которое позволяет <w>распознавать</w> множество запахов."
                            }
                        }
                    ],
                    "synonyms": [
                        "differentiation"
                    ],
                    "common_phrases": [
                        "discrimination between",
                        "visual discrimination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность или действие по выявлению различий между объектами или явлениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "различение",
                                "target_lang_sense_clarification": "способность видеть отличия"
                            },
                            {
                                "target_lang_word": "распознавание",
                                "target_lang_sense_clarification": "идентификация"
                            },
                            {
                                "target_lang_word": "разграничение",
                                "target_lang_sense_clarification": "проведение границ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25abb1a0-6f9f-3185-8152-3a93d4837266",
                    "sense_definition": "The ability to judge what is of high quality; good judgment or taste.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "discernment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a reader of great <w>discrimination</w> who only enjoys the classics.",
                            "target_lang_translations": {
                                "ru": "Он читатель с большой <w>разборчивостью</w>, который наслаждается только классикой."
                            }
                        },
                        {
                            "text": "She decorated her house with the <w>discrimination</w> of a professional artist.",
                            "target_lang_translations": {
                                "ru": "Она украсила свой дом с <w>тонким вкусом</w> профессионального художника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "man of discrimination",
                        "with discrimination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умение судить о высоком качестве; хороший вкус или проницательность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разборчивость",
                                "target_lang_sense_clarification": "требовательность"
                            },
                            {
                                "target_lang_word": "тонкий вкус",
                                "target_lang_sense_clarification": "эстетическое чутьё"
                            }
                        ]
                    }
                }
            ]
        }
    ]

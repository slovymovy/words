    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e82452ef-06a9-3db0-b0d8-e0d05d2606e9",
                    "sense_definition": "Extremely offensive, shocking, or morally unacceptable.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "offensive_shocking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is absolutely <w>outrageous</w> that the company treats its loyal employees with such disrespect.",
                            "target_lang_translations": {
                                "ru": "Абсолютно <w>возмутительно</w>, что компания относится к своим преданным сотрудникам с таким неуважением."
                            }
                        },
                        {
                            "text": "She told an <w>outrageous</w> lie to her parents to cover up where she had been.",
                            "target_lang_translations": {
                                "ru": "Она сказала родителям <w>вопиющую</w> ложь, чтобы скрыть, где она была."
                            }
                        }
                    ],
                    "synonyms": [
                        "preposterous"
                    ],
                    "common_phrases": [
                        "outrageous behavior",
                        "outrageous lie",
                        "outrageous conduct",
                        "outrageous fortune"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайне оскорбительный, шокирующий или морально неприемлемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возмутительный",
                                "target_lang_sense_clarification": "вызывающий негодование"
                            },
                            {
                                "target_lang_word": "вопиющий",
                                "target_lang_sense_clarification": "о лжи или несправедливости"
                            },
                            {
                                "target_lang_word": "скандальный",
                                "target_lang_sense_clarification": "позорящий, непристойный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92db6cea-fb4d-3768-a8f6-167580f3ddb1",
                    "sense_definition": "Greatly exceeding reasonable limits, especially regarding cost or demands.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "excessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We refused to pay the <w>outrageous</w> price they were asking for a simple cup of coffee.",
                            "target_lang_translations": {
                                "ru": "Мы отказались платить <w>грабительскую</w> цену, которую они просили за простую чашку кофе."
                            }
                        },
                        {
                            "text": "The union made <w>outrageous</w> demands that the management could not possibly accept.",
                            "target_lang_translations": {
                                "ru": "Профсоюз выдвинул <w>непомерные</w> требования, которые руководство никак не могло принять."
                            }
                        }
                    ],
                    "synonyms": [
                        "preposterous"
                    ],
                    "common_phrases": [
                        "outrageous price",
                        "outrageous cost",
                        "outrageous demands",
                        "outrageous fees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительно превышающий разумные пределы, особенно в отношении стоимости или требований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непомерный",
                                "target_lang_sense_clarification": "чрезмерно высокий"
                            },
                            {
                                "target_lang_word": "грабительский",
                                "target_lang_sense_clarification": "о ценах"
                            },
                            {
                                "target_lang_word": "возмутительный",
                                "target_lang_sense_clarification": "неприемлемый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b10543b-9868-3555-96fe-35cd31c71d4d",
                    "sense_definition": "Very bold, unusual, or shocking, often in an amusing or entertaining way.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "offensive_shocking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The singer wore an <w>outrageous</w> outfit made entirely of recycled plastic bottles.",
                            "target_lang_translations": {
                                "ru": "Певица была в <w>эпатажном</w> наряде, сделанном полностью из переработанных пластиковых бутылок."
                            }
                        },
                        {
                            "text": "He is known for his <w>outrageous</w> sense of humor that always makes everyone laugh.",
                            "target_lang_translations": {
                                "ru": "Он известен своим <w>дерзким</w> чувством юмора, которое всегда всех смешит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "outrageous outfit",
                        "outrageous clothes",
                        "outrageous sense of humor",
                        "outrageous comedy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень смелый, необычный или шокирующий, часто в забавном или развлекательном ключе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпатажный",
                                "target_lang_sense_clarification": "вызывающий, привлекающий внимание"
                            },
                            {
                                "target_lang_word": "экстравагантный",
                                "target_lang_sense_clarification": "необычный, причудливый"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e0d3ad0-e4ac-31a8-92c8-9e0228ca9c45",
                    "sense_definition": "Вычистить большое количество предметов, удаляя с них грязь или пыль.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "distributive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед праздником мама заставила нас <перечистить> всё столовое серебро.",
                            "target_lang_translations": {
                                "en": "Before the holiday, Mom made us <polish> all the silverware."
                            }
                        },
                        {
                            "text": "Она решила <перечистить> всю обувь в доме, чтобы подготовить её к новому сезону.",
                            "target_lang_translations": {
                                "en": "She decided to <clean> all the shoes in the house to get them ready for the new season."
                            }
                        }
                    ],
                    "synonyms": [
                        "вычистить"
                    ],
                    "antonyms": [
                        "испачкать",
                        "загрязнить"
                    ],
                    "common_phrases": [
                        "перечистить всю обувь",
                        "перечистить всё серебро",
                        "перечистить посуду"
                    ],
                    "target_lang_definitions": {
                        "en": "To clean a large number of items, removing dirt or dust."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clean (all)",
                                "target_lang_sense_clarification": "referring to a large quantity of items"
                            },
                            {
                                "target_lang_word": "polish (all)",
                                "target_lang_sense_clarification": "specifically for items like silverware or shoes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2df20ee-45f1-3d32-880e-9a8de174044a",
                    "sense_definition": "Очистить от кожуры, чешуи или скорлупы большое количество продуктов.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "distributive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Повару пришлось <перечистить> мешок картошки для приготовления обеда на всю роту.",
                            "target_lang_translations": {
                                "en": "The cook had to <peel> a sack of potatoes to prepare lunch for the whole company."
                            }
                        },
                        {
                            "text": "Мы <перечистили> гору рыбы, которую отец принёс с утренней рыбалки.",
                            "target_lang_translations": {
                                "en": "We <cleaned> a mountain of fish that father brought from the morning fishing trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "почистить"
                    ],
                    "common_phrases": [
                        "перечистить картошку",
                        "перечистить рыбу",
                        "перечистить грибы"
                    ],
                    "target_lang_definitions": {
                        "en": "To peel, scale, or shell a large quantity of food products."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peel (all)",
                                "target_lang_sense_clarification": "removing skin from vegetables or fruits"
                            },
                            {
                                "target_lang_word": "clean (all)",
                                "target_lang_sense_clarification": "removing scales or shells from fish or nuts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc645bd9-9079-326b-b6cf-25813e561ac4",
                    "sense_definition": "Вычистить что-либо заново, повторно, чтобы исправить недостатки первой чистки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "iterative_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пятно на пиджаке осталось, поэтому мне пришлось <перечистить> его ещё раз.",
                            "target_lang_translations": {
                                "en": "The stain on the jacket remained, so I had to <re-clean> it one more time."
                            }
                        },
                        {
                            "text": "Сержант приказал солдату <перечистить> оружие, так как заметил на нём пыль.",
                            "target_lang_translations": {
                                "en": "The sergeant ordered the soldier to <re-clean> the weapon because he noticed dust on it."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перечистить заново",
                        "перечистить костюм",
                        "перечистить ружьё"
                    ],
                    "target_lang_definitions": {
                        "en": "To clean something again to correct defects from the first cleaning."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "re-clean",
                                "target_lang_sense_clarification": "to clean again"
                            },
                            {
                                "target_lang_word": "clean again",
                                "target_lang_sense_clarification": "repeating the cleaning process"
                            }
                        ]
                    }
                }
            ]
        }
    ]

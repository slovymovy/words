                {
                    "sense_id": "af912340-566f-31c6-9daa-7afab8b12631",
                    "sense_definition": "To move carefully and slowly through a difficult place, selecting the best footing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had to <w>pick</w> our way through the muddy field.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>пробираться</w> через грязное поле."
                            }
                        },
                        {
                            "text": "The hikers <w>picked</w> their way down the steep, rocky slope.",
                            "target_lang_translations": {
                                "ru": "Туристы <w>пробирались</w> вниз по крутому каменистому склону."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pick one's way"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осторожно и медленно пробираться через трудное место, выбирая, куда наступить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробираться",
                                "target_lang_sense_clarification": "через препятствия"
                            },
                            {
                                "target_lang_word": "осторожно ступать",
                                "target_lang_sense_clarification": "выбирать дорогу"
                            }
                        ]
                    }
                }
            ]
        },
        {

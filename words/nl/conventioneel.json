{
    "word_family": [
        "conventie",
        "conventionaliteit"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f9e438c6-6861-3518-b829-faa9e6ca5bf1",
                    "sense_definition": "Gebaseerd op wat algemeen gebruikelijk, geaccepteerd of volgens de gewoonte is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kozen voor een <w>conventionele</w> aanpak om onnodige risico's te vermijden.",
                            "target_lang_translations": {
                                "en": "We chose a <w>conventional</w> approach to avoid unnecessary risks.",
                                "ru": "Мы выбрали <w>традиционный</w> подход, чтобы избежать ненужных рисков."
                            }
                        },
                        {
                            "text": "Het huis heeft een vrij <w>conventionele</w> indeling met de woonkamer beneden.",
                            "target_lang_translations": {
                                "en": "The house has a quite <w>conventional</w> layout with the living room downstairs.",
                                "ru": "У дома довольно <w>стандартная</w> планировка с гостиной внизу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebruikelijk",
                        "gangbaar",
                        "traditioneel"
                    ],
                    "antonyms": [
                        "onconventioneel"
                    ],
                    "common_phrases": [
                        "op conventionele wijze",
                        "conventionele geneeskunde"
                    ],
                    "target_lang_definitions": {
                        "en": "Based on what is generally common, accepted, or customary.",
                        "ru": "Основанный на общепринятых нормах, обычаях или традициях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conventional",
                                "target_lang_sense_clarification": "standard or accepted"
                            },
                            {
                                "target_lang_word": "customary",
                                "target_lang_sense_clarification": "according to custom"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "традиционный",
                                "target_lang_sense_clarification": "соответствующий традиции"
                            },
                            {
                                "target_lang_word": "общепринятый",
                                "target_lang_sense_clarification": "принятый большинством"
                            },
                            {
                                "target_lang_word": "стандартный",
                                "target_lang_sense_clarification": "обычный, типовой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f19809fe-3007-38cf-a171-12aeee888a8c",
                    "sense_definition": "Stijf en formeel vasthoudend aan de etiquette of sociale regels; niet spontaan.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haar ouders zijn erg <w>conventioneel</w> en houden niet van veranderingen.",
                            "target_lang_translations": {
                                "en": "Her parents are very <w>conventional</w> and do not like change.",
                                "ru": "Её родители очень <w>консервативны</w> и не любят перемен."
                            }
                        },
                        {
                            "text": "Hij heeft een nogal <w>conventionele</w> kijk op hoe een gezin moet functioneren.",
                            "target_lang_translations": {
                                "en": "He has a rather <w>conventional</w> view on how a family should function.",
                                "ru": "У него довольно <w>традиционные</w> взгляды на то, как должна функционировать семья."
                            }
                        }
                    ],
                    "synonyms": [
                        "stijf",
                        "formeel",
                        "behoudend"
                    ],
                    "antonyms": [
                        "onconventioneel"
                    ],
                    "common_phrases": [
                        "conventioneel gedrag",
                        "conventionele opvattingen"
                    ],
                    "target_lang_definitions": {
                        "en": "Stiffly and formally adhering to etiquette or social rules; not spontaneous.",
                        "ru": "Строго придерживающийся традиций, этикета или социальных норм; консервативный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conventional",
                                "target_lang_sense_clarification": "socially conforming"
                            },
                            {
                                "target_lang_word": "conservative",
                                "target_lang_sense_clarification": "holding traditional values"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "консервативный",
                                "target_lang_sense_clarification": "приверженный старым порядкам"
                            },
                            {
                                "target_lang_word": "старомодный",
                                "target_lang_sense_clarification": "следующий устаревшим правилам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f228741-fef7-331c-9d42-af14724c0265",
                    "sense_definition": "Met betrekking tot wapens of oorlogvoering zonder gebruik van nucleaire, biologische of chemische middelen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het leger beschikt over moderne <w>conventionele</w> wapens voor de verdediging.",
                            "target_lang_translations": {
                                "en": "The army possesses modern <w>conventional</w> weapons for defense.",
                                "ru": "Армия располагает современным <w>обычным</w> оружием для обороны."
                            }
                        },
                        {
                            "text": "Een <w>conventionele</w> oorlog kan nog steeds enorme schade aanrichten aan de infrastructuur.",
                            "target_lang_translations": {
                                "en": "A <w>conventional</w> war can still cause enormous damage to the infrastructure.",
                                "ru": "<w>Обычная</w> война всё ещё может нанести огромный ущерб инфраструктуре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "conventionele wapens",
                        "conventionele strijdkrachten"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to weapons or warfare excluding nuclear, biological, or chemical means.",
                        "ru": "Относящийся к оружию или военным действиям без использования ядерных, биологических или химических средств."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conventional",
                                "target_lang_sense_clarification": "military context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обычный",
                                "target_lang_sense_clarification": "неядерный (о вооружении)"
                            },
                            {
                                "target_lang_word": "конвенциональный",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

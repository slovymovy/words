{
    "word_family": [
        "reuk"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24c58fa2-beff-32ab-b13a-8f2baf35c12e",
                    "sense_definition": "Een geur waarnemen met de neus.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "zintuigen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ruik</w> verse koffie in de keuken.",
                            "target_lang_translations": {
                                "en": "I <w>smell</w> fresh coffee in the kitchen."
                            }
                        },
                        {
                            "text": "Kan jij de bloemen in de tuin <w>ruiken</w>?",
                            "target_lang_translations": {
                                "en": "Can you <w>smell</w> the flowers in the garden?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "iets lekkers ruiken",
                        "niets ruiken",
                        "rook ruiken"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive a scent with the nose."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smell",
                                "target_lang_sense_clarification": "to perceive a scent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc4a3e25-e6ce-3e99-ae55-e8bb8f56da73",
                    "sense_definition": "Een bepaalde geur verspreiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geur_verspreiden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>ruikt</w> hier heel sterk naar gas.",
                            "target_lang_translations": {
                                "en": "It <w>smells</w> very strongly of gas here."
                            }
                        },
                        {
                            "text": "Na het douchen <w>rook</w> hij weer lekker fris.",
                            "target_lang_translations": {
                                "en": "After showering, he <w>smelled</w> nice and fresh again."
                            }
                        }
                    ],
                    "synonyms": [
                        "geuren",
                        "stinken",
                        "rieken"
                    ],
                    "common_phrases": [
                        "lekker ruiken",
                        "vies ruiken",
                        "ruiken naar"
                    ],
                    "target_lang_definitions": {
                        "en": "To give off or emit a certain scent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smell",
                                "target_lang_sense_clarification": "to have a particular scent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08970e7f-de76-3a37-ae6e-f81426b1196f",
                    "sense_definition": "De neus dicht bij iets houden om de geur te onderzoeken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "zintuigen_actie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>rook</w> even aan de melk om te zien of die nog goed was.",
                            "target_lang_translations": {
                                "en": "He briefly <w>smelled</w> the milk to see if it was still good."
                            }
                        },
                        {
                            "text": "Mag ik even aan jouw nieuwe parfum <w>ruiken</w>?",
                            "target_lang_translations": {
                                "en": "May I <w>smell</w> your new perfume?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan iets ruiken",
                        "eraan ruiken"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold the nose close to something to investigate the scent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smell",
                                "target_lang_sense_clarification": "to check the scent of something"
                            },
                            {
                                "target_lang_word": "sniff",
                                "target_lang_sense_clarification": "to inhale briefly to detect a smell"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "6f752e3a-821c-32af-a381-9f0273c18e2c",
                    "sense_definition": "At all times; forever or since the beginning.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Green has <w>always</w> been my favorite color.",
                            "target_lang_translations": {
                                "ru": "Зеленый <w>всегда</w> был моим любимым цветом."
                            }
                        },
                        {
                            "text": "He promised that he would love her <w>always</w>.",
                            "target_lang_translations": {
                                "ru": "Он пообещал, что будет любить её <w>вечно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "forever",
                        "eternally",
                        "perpetually",
                        "evermore",
                        "for good"
                    ],
                    "antonyms": [
                        "never",
                        "at no time"
                    ],
                    "common_phrases": [
                        "always and forever",
                        "always be there"
                    ],
                    "target_lang_definitions": {
                        "ru": "Во все времена; вечно или с самого начала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всегда",
                                "target_lang_sense_clarification": "во все времена"
                            },
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "бесконечно во времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2089baac-4590-30fe-a4da-368e0e94b16f",
                    "sense_definition": "Used to suggest a possible alternative or solution if no better option is available.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you miss the bus, you can <w>always</w> walk home.",
                            "target_lang_translations": {
                                "ru": "Если опоздаешь на автобус, ты <w>всегда</w> можешь пойти домой пешком."
                            }
                        },
                        {
                            "text": "We could <w>always</w> try again tomorrow if today is too busy.",
                            "target_lang_translations": {
                                "ru": "Мы <w>всегда</w> можем попробовать снова завтра, если сегодня слишком много дел."
                            }
                        }
                    ],
                    "synonyms": [
                        "at any rate",
                        "in any event",
                        "anyhow",
                        "anyway"
                    ],
                    "common_phrases": [
                        "can always",
                        "could always"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для предложения возможной альтернативы или решения, если нет лучшего варианта."

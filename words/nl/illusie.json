    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1d0a50c5-e253-3b4e-8834-c941d5d21363",
                    "sense_definition": "Een droombeeld of hoopvolle verwachting die niet op de waarheid berust.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "false_belief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is een <w>illusie</w> om te denken dat dit probleem vanzelf verdwijnt.",
                            "target_lang_translations": {
                                "ru": "Думать, что эта проблема исчезнет сама собой — это <w>иллюзия</w>."
                            }
                        },
                        {
                            "text": "Hij maakte zich geen <w>illusies</w> meer over de uitslag van de wedstrijd.",
                            "target_lang_translations": {
                                "ru": "Он больше не питал <w>иллюзий</w> по поводу исхода матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "droombeeld",
                        "waanidee"
                    ],
                    "antonyms": [
                        "desillusie"
                    ],
                    "common_phrases": [
                        "zich geen illusies maken",
                        "een illusie armer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мечта или надежда, не основанная на реальности; ложное представление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иллюзия",
                                "target_lang_sense_clarification": "ложное представление или несбыточная надежда"
                            },
                            {
                                "target_lang_word": "самообман",
                                "target_lang_sense_clarification": "внушение самому себе ложных мыслей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4baa9531-e167-3894-85b1-2602ffb0ff33",
                    "sense_definition": "Een zinsbegoocheling; een waarneming die niet overeenkomt met de werkelijkheid.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensory_trick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De plas water op het hete asfalt was slechts een optische <w>illusie</w>.",
                            "target_lang_translations": {
                                "ru": "Лужа воды на горячем асфальте была лишь оптической <w>иллюзией</w>."
                            }
                        },
                        {
                            "text": "Door het gebruik van spiegels ontstond de <w>illusie</w> van een veel grotere kamer.",
                            "target_lang_translations": {
                                "ru": "Благодаря использованию зеркал возникла <w>иллюзия</w> гораздо большей комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "zinsbegoocheling",
                        "gezichtsbedrog"
                    ],
                    "common_phrases": [
                        "een optische illusie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обман чувств; восприятие, не соответствующее действительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иллюзия",
                                "target_lang_sense_clarification": "искаженное восприятие реальности"
                            },
                            {
                                "target_lang_word": "обман зрения",
                                "target_lang_sense_clarification": "зрительная ошибка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44a7c73d-191f-30df-8aca-38c7a58b570f",
                    "sense_definition": "Een kunstmatige voorstelling of sfeer die wordt opgeroepen om iets echt te laten lijken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sensory_trick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regisseur wist de <w>illusie</w> te wekken dat de acteurs echt in de ruimte zweefden.",
                            "target_lang_translations": {
                                "ru": "Режиссеру удалось создать <w>иллюзию</w>, что актеры действительно парят в космосе."
                            }
                        },
                        {
                            "text": "We genoten van de romantische <w>illusie</w> die de oude stad ons bood.",
                            "target_lang_translations": {
                                "ru": "Мы наслаждались романтической <w>иллюзией</w>, которую дарил нам старый город."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de illusie wekken",
                        "de illusie in stand houden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственно созданное представление или атмосфера, призванные заставить что-то казаться реальным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иллюзия",
                                "target_lang_sense_clarification": "созданный эффект или видимость"
                            },
                            {
                                "target_lang_word": "видимость",
                                "target_lang_sense_clarification": "внешнее впечатление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1f1253d9-1e79-3659-b782-f02d51b3b367",
                    "sense_definition": "An informal way of saying yes to express agreement or affirmation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are you coming to the movies with us tonight? <w>Yup</w>, I'll be there.",
                            "target_lang_translations": {
                                "ru": "Ты пойдёшь с нами сегодня в кино? — <w>Ага</w>, я буду."
                            }
                        },
                        {
                            "text": "<w>Yup</w>, that is exactly what I was trying to tell you earlier.",
                            "target_lang_translations": {
                                "ru": "<w>Ага</w>, это именно то, что я пытался сказать тебе раньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "yep"
                    ],
                    "antonyms": [
                        "nope"
                    ],
                    "common_phrases": [
                        "yup sure",
                        "oh yup"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальный способ сказать «да» для выражения согласия или подтверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "разговорное выражение согласия"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "стандартное утверждение"
                            },
                            {
                                "target_lang_word": "угу",
                                "target_lang_sense_clarification": "звук, выражающий подтверждение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c4d494f-27cf-304e-a36b-6e2e6c40c620",
                    "sense_definition": "An instance of saying yes; an affirmative answer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "affirmation_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She asked him a difficult question that could not be answered by a simple <w>yup</w> or nope.",
                            "target_lang_translations": {
                                "ru": "Она задала ему сложный вопрос, на который нельзя было ответить простым <w>да</w> или нет."
                            }
                        },
                        {
                            "text": "I heard a resounding <w>yup</w> from the audience when the speaker asked if they were excited.",
                            "target_lang_translations": {
                                "ru": "Я услышал громкое <w>да</w> из зала, когда оратор спросил, рады ли они."
                            }
                        }
                    ],
                    "synonyms": [
                        "yep"
                    ],
                    "antonyms": [
                        "nope"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Случай произнесения слова «да»; утвердительный ответ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "как существительное"
                            },
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "утвердительный ответ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d3d68b9-586a-3e3f-ae4c-733b28e6b8f5",
                    "sense_definition": "A clipping of <w>yuppie</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He saw two <w>yups</w> in expensive matching sweaters walking down the busy street.",
                            "target_lang_translations": {
                                "ru": "Он увидел двух <w>яппи</w> в дорогих одинаковых свитерах, идущих по оживлённой улице."
                            }
                        },
                        {
                            "text": "The new gym is always full of <w>yups</w> running on treadmills before heading to their office jobs.",
                            "target_lang_translations": {
                                "ru": "Новый спортзал всегда полон <w>яппи</w>, бегающих на дорожках перед уходом на офисную работу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова «яппи» (молодой состоятельный человек, работающий в офисе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яппи",
                                "target_lang_sense_clarification": "молодой профессионал"
                            }
                        ]
                    }
                }
            ]
        },

                    "sense_id": "6a9e86dc-67c4-3716-b572-8cb6dbf311dc",
                    "sense_definition": "Pokój w mieszkaniu lub domu przeznaczony do spania i odpoczynku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzisiaj kupiliśmy nowe łóżko do <w>sypialni</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня мы купили новую кровать в <w>спальню</w>."
                            }
                        },
                        {
                            "text": "Moja <w>sypialnia</w> ma okna wychodzące na cichy ogród.",
                            "target_lang_translations": {
                                "ru": "Окна моей <w>спальни</w> выходят в тихий сад."
                            }
                        }
                    ],
                    "common_phrases": [

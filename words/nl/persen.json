    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "414fc8f4-12e9-3dce-9b05-3b1f1c16a015",
                    "sense_definition": "Vloeistof uit een vrucht of ander object halen door er krachtig in te knijpen of op te drukken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "extracting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>perst</w> elke ochtend vers sap uit de sinaasappels.",
                            "target_lang_translations": {
                                "en": "He <w>squeezes</w> fresh juice from the oranges every morning.",
                                "ru": "Каждое утро он <w>выжимает</w> свежий сок из апельсинов."
                            }
                        },
                        {
                            "text": "Voor dit recept moet je een halve citroen <w>persen</w>.",
                            "target_lang_translations": {
                                "en": "For this recipe you need to <w>squeeze</w> half a lemon.",
                                "ru": "Для этого рецепта нужно <w>выжать</w> половину лимона."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitknijpen"
                    ],
                    "common_phrases": [
                        "sinaasappels persen",
                        "sap persen"
                    ],
                    "target_lang_definitions": {
                        "en": "To extract liquid from a fruit or other object by squeezing or pressing it firmly.",
                        "ru": "Извлекать жидкость из фрукта или другого предмета путем сильного сжатия или надавливания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "squeeze",
                                "target_lang_sense_clarification": "to extract liquid"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "to extract juice"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выжимать",
                                "target_lang_sense_clarification": "сок"
                            },
                            {
                                "target_lang_word": "давить",
                                "target_lang_sense_clarification": "извлекать сок нажатием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5a7b5ec-2266-31a5-bdcd-4c61d9019191",
                    "sense_definition": "Materiaal krachtig samendrukken om het te vormen, plat te maken of samen te binden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "compressing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De machine <w>perst</w> een aantal lagen folie op elkaar tot stevige panelen.",
                            "target_lang_translations": {
                                "en": "The machine <w>presses</w> a number of layers of foil together into sturdy panels.",
                                "ru": "Машина <w>прессует</w> несколько слоев пленки в прочные панели."
                            }
                        },
                        {
                            "text": "In de fabriek <w>persen</w> ze het afval samen tot grote blokken.",
                            "target_lang_translations": {
                                "en": "In the factory they <w>compress</w> the waste into large blocks.",
                                "ru": "На заводе мусор <w>спрессовывают</w> в большие блоки."
                            }
                        }
                    ],
                    "synonyms": [
                        "samendrukken"
                    ],
                    "common_phrases": [
                        "samen persen"
                    ],
                    "target_lang_definitions": {
                        "en": "To compress material forcefully in order to shape, flatten, or bind it together.",
                        "ru": "Сильно сжимать материал для придания формы, сплющивания или соединения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "to compress or shape"
                            },
                            {
                                "target_lang_word": "compress",
                                "target_lang_sense_clarification": "to squeeze together"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прессовать",
                                "target_lang_sense_clarification": "обрабатывать давлением"
                            },
                            {
                                "target_lang_word": "спрессовывать",
                                "target_lang_sense_clarification": "сжимать в плотную массу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a5f0295-2e6a-364d-8d1e-043d2195a2e3",
                    "sense_definition": "Zichzelf of iets anders met moeite door een nauwe opening of in een volle ruimte duwen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "forcing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mijnwerkers <w>persten</w> zich de lift in om naar beneden te gaan.",
                            "target_lang_translations": {
                                "en": "The miners <w>squeezed</w> themselves into the elevator to go down.",
                                "ru": "Шахтеры <w>втиснулись</w> в лифт, чтобы спуститься вниз."
                            }
                        },
                        {
                            "text": "Je moet het water met kracht door het filter <w>persen</w>.",
                            "target_lang_translations": {
                                "en": "You have to <w>force</w> the water through the filter.",
                                "ru": "Нужно с силой <w>продавить</w> воду через фильтр."
                            }
                        }
                    ],
                    "synonyms": [
                        "wringen",
                        "proppen"
                    ],
                    "common_phrases": [
                        "zich ergens in persen"
                    ],
                    "target_lang_definitions": {
                        "en": "To push oneself or something else with difficulty through a narrow opening or into a crowded space.",
                        "ru": "С трудом проталкивать себя или что-то другое через узкое отверстие или в переполненное пространство."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "squeeze",
                                "target_lang_sense_clarification": "into a tight space"
                            },
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "through resistance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "втискиваться",
                                "target_lang_sense_clarification": "пролезать с трудом (о людях)"
                            },
                            {
                                "target_lang_word": "продавливать",
                                "target_lang_sense_clarification": "проталкивать силой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",

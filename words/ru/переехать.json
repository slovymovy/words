        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "acaba452-cd02-36cb-9998-7836e5a45443",
                    "sense_definition": "Сменить место жительства или пребывания, переместившись в другое помещение, город или страну.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <переехать> в новую квартиру ближе к центру города."
                        },
                        {
                            "text": "Мой друг планирует <переехать> в другую страну ради учёбы."
                        }
                    ],
                    "synonyms": [
                        "переселиться",
                        "перебраться"
                    ],
                    "antonyms": [
                        "остаться"
                    ],
                    "common_phrases": [
                        "переехать в новый дом",
                        "переехать в другой город",
                        "переехать к родителям",
                        "переехать на квартиру"
                    ]
                },
                {
                    "sense_id": "035b4f30-53fe-355b-b889-99330d819601",
                    "sense_definition": "Пересечь какое-либо пространство, линию или границу, передвигаясь на транспортном средстве.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы успешно <переехали> границу и продолжили своё путешествие."
                        },
                        {
                            "text": "Нам нужно аккуратно <переехать> через железнодорожные пути."
                        }
                    ],
                    "synonyms": [
                        "пересечь"
                    ],
                    "common_phrases": [
                        "переехать границу",
                        "переехать через мост",
                        "переехать на другую сторону",
                        "переехать реку"
                    ]
                },
                {
                    "sense_id": "5891067c-2022-345f-826d-573bff3b8cc1",
                    "sense_definition": "Наехать колёсами на кого-либо или что-либо, раздавить тяжестью транспортного средства.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель не заметил коробку на дороге и случайно <переехал> её."
                        },
                        {
                            "text": "Грузовик едва не <переехал> упавший дорожный знак."
                        }
                    ],
                    "synonyms": [
                        "задавить"
                    ],
                    "common_phrases": [
                        "случайно переехать",
                        "переехать колесом"
                    ]
                }
            ]
        }

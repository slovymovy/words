{
    "word_family": [
        "подход",
        "подходящий"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f9537394-5b61-3a58-8f71-e1e7b9821ff9",
                    "sense_definition": "Приблизиться к кому-либо или чему-либо, идя пешком или перемещаясь.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>подошёл</w> к окну, чтобы посмотреть на улицу."
                        },
                        {
                            "text": "Поезд медленно <w>подошёл</w> к станции."
                        }
                    ],
                    "synonyms": [
                        "приблизиться"
                    ],
                    "antonyms": [
                        "отойти",
                        "удалиться"
                    ],
                    "common_phrases": [
                        "подойти к окну",
                        "подойти ближе",
                        "подойти к двери"
                    ]
                },
                {
                    "sense_id": "484caa0b-2caa-3788-8593-7108838bc610",
                    "sense_definition": "Оказаться годным, приемлемым или соответствующим по размеру, качеству или назначению.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "suitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это красивое платье ей не <w>подошло</w>."
                        },
                        {
                            "text": "Твой ключ не <w>подойдёт</w> к этому замку."
                        }
                    ],
                    "synonyms": [
                        "пригодиться"
                    ],
                    "common_phrases": [
                        "подойти по размеру",
                        "идеально подойти",
                        "это не подойдёт"
                    ]
                },
                {
                    "sense_id": "343d0a43-e5d5-3a93-ae99-7c15498ac509",
                    "sense_definition": "Приблизиться во времени или наступить (о сроке, событии, времени года).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Незаметно <w>подошла</w> осень с холодными дождями."
                        },
                        {
                            "text": "Моя очередь к врачу уже <w>подошла</w>."
                        }
                    ],
                    "synonyms": [
                        "наступить"
                    ],
                    "common_phrases": [
                        "время подошло",
                        "срок подошёл",
                        "очередь подошла"
                    ]
                },
                {
                    "sense_id": "492b4602-b3ec-3642-bf37-175735f40fbf",
                    "sense_definition": "Найти определённый способ действия, решения или обращения с кем-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны серьёзно <w>подойти</w> к решению этой проблемы."
                        },
                        {
                            "text": "Учитель сумел правильно <w>подойти</w> к трудному ученику."
                        }
                    ],
                    "synonyms": [
                        "найти подход"
                    ],
                    "common_phrases": [
                        "подойти к вопросу",
                        "подойти творчески",
                        "подойти с умом"
                    ]
                },
                {
                    "sense_id": "70138108-aa11-3a64-b94e-bab752697dc7",
                    "sense_definition": "Подняться и увеличиться в объёме в результате брожения (о тесте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поставь опару в тёплое место, чтобы она быстрее <w>подошла</w>."
                        },
                        {
                            "text": "Когда тесто <w>подойдёт</w>, можно начинать печь пироги."
                        }
                    ],
                    "common_phrases": [
                        "тесто подошло"
                    ]
                }
            ]
        }
    ]
}
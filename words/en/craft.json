    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f35f14e-8c61-3292-acde-54229c8a8ed9",
                    "sense_definition": "A job, activity, or profession that requires special skill, especially manual or artistic skill.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "skill_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent years as an apprentice learning the <w>craft</w> of carpentry.",
                            "target_lang_translations": {
                                "ru": "Он потратил годы, будучи подмастерьем, изучая столярное <w>ремесло</w>."
                            }
                        },
                        {
                            "text": "Writing is a <w>craft</w> that requires patience and constant practice.",
                            "target_lang_translations": {
                                "ru": "Писательство — это <w>ремесло</w>, которое требует терпения и постоянной практики."
                            }
                        }
                    ],
                    "synonyms": [
                        "trade",
                        "profession",
                        "art",
                        "handicraft"
                    ],
                    "common_phrases": [
                        "learn a craft",
                        "hone one's craft"
                    ],
                    "target_lang_definitions": {
                        "ru": "Профессия или занятие, требующие специальных навыков, особенно ручных или художественных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремесло",
                                "target_lang_sense_clarification": "профессия, требующая специальных навыков"
                            },
                            {
                                "target_lang_word": "искусство",
                                "target_lang_sense_clarification": "творческое занятие, требующее мастерства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd8f570b-3685-344c-9461-671f2a7b7643",
                    "sense_definition": "Handmade objects, especially decorative ones; the activity of making such objects.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "skill_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local market is full of pottery, jewelry, and other handmade <w>crafts</w>.",
                            "target_lang_translations": {
                                "ru": "Местный рынок полон керамики, украшений и других <w>изделий ручной работы</w>."
                            }
                        },
                        {
                            "text": "The children enjoyed the arts and <w>crafts</w> lesson at school today.",
                            "target_lang_translations": {
                                "ru": "Детям понравился сегодняшний урок <w>рукоделия</w> в школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "handicraft"
                    ],
                    "common_phrases": [
                        "arts and crafts",
                        "craft fair",
                        "craft shop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предметы ручной работы, особенно декоративные; деятельность по их изготовлению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изделие ручной работы",
                                "target_lang_sense_clarification": "предмет, сделанный руками"
                            },
                            {
                                "target_lang_word": "рукоделие",
                                "target_lang_sense_clarification": "занятие по созданию вещей вручную"
                            },
                            {
                                "target_lang_word": "поделка",
                                "target_lang_sense_clarification": "мелкое изделие, часто любительское или детское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3486a227-d716-30a1-8ad0-a984d8f0c9ce",
                    "sense_definition": "A vehicle designed for travel on water, in the air, or in space.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Several small fishing <w>craft</w> were seen near the harbor entrance.",
                            "target_lang_translations": {
                                "ru": "Несколько небольших рыболовецких <w>судов</w> были замечены у входа в гавань."
                            }
                        },
                        {
                            "text": "The landing <w>craft</w> approached the beach slowly to unload the supplies.",
                            "target_lang_translations": {
                                "ru": "Десантное <w>судно</w> медленно приближалось к пляжу для выгрузки припасов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "landing craft",
                        "pleasure craft",
                        "small craft"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "The plural form is often 'craft' without 's' in this context."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство, предназначенное для передвижения по воде, воздуху или в космосе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судно",
                                "target_lang_sense_clarification": "водный транспорт"
                            },
                            {
                                "target_lang_word": "корабль",
                                "target_lang_sense_clarification": "крупное судно или космический аппарат"
                            },
                            {
                                "target_lang_word": "аппарат",
                                "target_lang_sense_clarification": "техническое устройство (летательный, космический)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9844313d-0ca2-38d6-b2e6-39afd1a908d7",
                    "sense_definition": "Skill and experience, especially in relation to making objects or artistic work.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "skill_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The musician was a true master of his <w>craft</w>, playing with incredible emotion.",
                            "target_lang_translations": {
                                "ru": "Музыкант был настоящим мастером своего <w>дела</w>, играя с невероятным чувством."
                            }
                        },
                        {
                            "text": "It takes years of dedication to master the <w>craft</w> of violin making.",
                            "target_lang_translations": {
                                "ru": "Требуются годы самоотверженности, чтобы овладеть <w>мастерством</w> изготовления скрипок."
                            }
                        }
                    ],
                    "synonyms": [
                        "craftsmanship",
                        "workmanship",
                        "art"
                    ],
                    "common_phrases": [
                        "master of one's craft"
                    ],
                    "target_lang_definitions": {
                        "ru": "Навыки и опыт, особенно в создании предметов или в искусстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастерство",
                                "target_lang_sense_clarification": "высокий уровень умения"
                            },
                            {
                                "target_lang_word": "умение",
                                "target_lang_sense_clarification": "способность делать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96eea67a-f8b3-3b44-abf7-f6affa858d43",
                    "sense_definition": "Skill used in a deceptive or sly way to achieve something; cunning.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cunning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fox is known for its <w>craft</w> and ability to escape traps.",
                            "target_lang_translations": {
                                "ru": "Лиса известна своей <w>хитростью</w> и способностью избегать ловушек."
                            }
                        },
                        {
                            "text": "He used <w>craft</w> and lies to get what he wanted from the negotiation.",
                            "target_lang_translations": {
                                "ru": "Он использовал <w>коварство</w> и ложь, чтобы получить желаемое от переговоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "cunning",
                        "guile",
                        "slyness",
                        "craftiness",
                        "wiliness"
                    ],
                    "common_phrases": [
                        "by craft"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умение использовать обман или хитрость для достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хитрость",
                                "target_lang_sense_clarification": "изобретательность в обмане"
                            },
                            {
                                "target_lang_word": "коварство",
                                "target_lang_sense_clarification": "злонамеренная хитрость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cf51bad3-92a5-3d42-952d-4556f48416b1",
                    "sense_definition": "To make or produce something with care, skill, and attention to detail.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skill_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>crafted</w> a beautiful necklace out of silver and gemstones.",
                            "target_lang_translations": {
                                "ru": "Она <w>смастерила</w> красивое ожерелье из серебра и драгоценных камней."
                            }
                        },
                        {
                            "text": "The speech was carefully <w>crafted</w> to appeal to all the voters.",
                            "target_lang_translations": {
                                "ru": "Речь была тщательно <w>составлена</w>, чтобы понравиться всем избирателям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "carefully crafted",
                        "hand-crafted",
                        "well-crafted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать или производить что-либо с тщательностью, мастерством и вниманием к деталям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастерить",
                                "target_lang_sense_clarification": "делать что-либо своими руками"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "творить, разрабатывать"
                            },
                            {
                                "target_lang_word": "изготавливать",
                                "target_lang_sense_clarification": "производить"
                            }
                        ]
                    }
                }
            ]
        },

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "анатомический"
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "дыхательный"
                            },
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "проток"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f669489-b4ba-3c40-9bd8-2223148eb927",
                    "sense_definition": "To pass a pathogen, cell culture, or similar entity through a host or medium to maintain or modify it.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "science_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The researchers <w>passaged</w> the virus through several hosts to study its evolution.",
                            "target_lang_translations": {
                                "ru": "Исследователи <w>пассировали</w> вирус через несколько носителей, чтобы изучить его эволюцию."
                            }
                        },
                        {
                            "text": "Cells must be <w>passaged</w> regularly to keep the culture healthy.",
                            "target_lang_translations": {
                                "ru": "Клетки необходимо регулярно <w>пассировать</w>, чтобы поддерживать культуру здоровой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить патоген, культуру клеток или подобный объект через носителя или среду для его поддержания или изменения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пассировать",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "97cc2866-9ca9-3ee4-9d08-abab873f143b",
                    "sense_definition": "Of a bird: less than a year old but living independently, having left the nest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "falconry_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>passage</w> hawk is often easier to train than an older bird.",
                            "target_lang_translations": {
                                "ru": "<w>Перелётный</w> ястреб часто легче поддается дрессировке, чем более старая птица."
                            }
                        },
                        {
                            "text": "Falconers prize <w>passage</w> birds for their adaptability.",
                            "target_lang_translations": {
                                "ru": "Сокольники ценят <w>перелётных</w> птиц за их приспособляемость."

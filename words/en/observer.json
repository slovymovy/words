    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f7df605-4694-32d4-bbcb-0fd6677fb52a",
                    "sense_definition": "One who makes observations, monitors, or takes notice of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "watching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most impartial <w>observers</w> agreed that the tennis player had not served well."
                        },
                        {
                            "text": "A keen <w>observer</w> would notice the subtle changes in the painting's colors."
                        }
                    ],
                    "synonyms": [
                        "beholder"
                    ],
                    "antonyms": [
                        "observee"
                    ],
                    "common_phrases": [
                        "impartial observer",
                        "casual observer",
                        "keen observer"
                    ]
                },
                {
                    "sense_id": "f14dcf91-edbe-345f-aa35-f91b1445d462",
                    "sense_definition": "A person sent as a representative to a meeting or event to monitor proceedings but not to participate or vote.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The United Nations sent many <w>observers</w> to monitor the country's first democratic elections."
                        },
                        {
                            "text": "She attended the conference as an <w>observer</w> rather than a voting member."
                        }
                    ],
                    "common_phrases": [
                        "election observer",
                        "UN observer",
                        "international observer"
                    ]
                },
                {
                    "sense_id": "1fa23ded-3207-39a2-828f-15f1899f8101",
                    "sense_definition": "One who adheres to or follows laws, guidelines, customs, or religious rules.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "adherence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I try to be a respectful <w>observer</w> of the local customs whenever I travel abroad."
                        },
                        {
                            "text": "He was a strict <w>observer</w> of the traditions passed down by his ancestors."
                        }
                    ],
                    "common_phrases": [
                        "strict observer"
                    ]
                },
                {
                    "sense_id": "e1147702-b0b6-3305-a9ed-26d427d7e4b7",
                    "sense_definition": "A country or entity that has limited participation rights within an organization, often without a vote.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "political_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Vatican and Palestine currently hold <w>observer</w> status at the United Nations."
                        },
                        {
                            "text": "Several non-governmental organizations act as <w>observers</w> during the climate negotiations."
                        }
                    ],
                    "common_phrases": [
                        "observer status",
                        "permanent observer"
                    ]
                },
                {
                    "sense_id": "a7fb6863-462d-3ea5-8ff7-34b75755a4f0",
                    "sense_definition": "A crew member on a military aircraft responsible for spotting enemy positions or other aircraft.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The only crew member to survive the crash was the Canadian <w>observer</w>."
                        },
                        {
                            "text": "The pilot relied on his <w>observer</w> to navigate and spot targets on the ground."
                        }
                    ]
                },
                {
                    "sense_id": "087957d7-3801-3f05-ae24-56486955d0f9",
                    "sense_definition": "A sentry or soldier manning an observation post.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We waited until dusk when the <w>observers'</w> vision was poorest to make our move."
                        },
                        {
                            "text": "The forward <w>observer</w> radioed the coordinates back to the command center."
                        }
                    ],
                    "common_phrases": [
                        "forward observer"
                    ]
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "87481f8f-7959-3646-bf0c-a2f797f9b46b",
                    "sense_definition": "Osoba dowodząca, zarządzająca pracą zespołu lub grupy ludzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_in_charge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Paweł jest <w>liderem</w> naszej grupy projektowej w szkole.",
                            "target_lang_translations": {
                                "ru": "Павел — <w>лидер</w> нашей проектной группы в школе."
                            }
                        },
                        {
                            "text": "Dobry <w>lider</w> potrafi zmotywować swój zespół do ciężkiej pracy.",
                            "target_lang_translations": {
                                "ru": "Хороший <w>лидер</w> умеет мотивировать свою команду на усердную работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywódca"
                    ],
                    "common_phrases": [
                        "lider grupy",
                        "lider zespołu",
                        "urodzony lider"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, руководящий работой команды или группы людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лидер",
                                "target_lang_sense_clarification": "группы, команды"
                            },
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "начальник"
                            }
                        ]

                    "sense_id": "7331aeb8-8177-3b91-bc7a-0754e7291abb",
                    "sense_definition": "To, co łączy jednostki ze sobą; silna relacja emocjonalna, psychiczna lub społeczna między ludźmi.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "connection_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Między matką a dzieckiem tworzy się niezwykła <w>więź</w> już od pierwszych chwil życia.",
                            "target_lang_translations": {
                                "ru": "Между матерью и ребенком уже с первых минут жизни возникает необыкновенная <w>связь</w>."
                            }
                        },
                        {
                            "text": "Mimo odległości, ich <w>więź</w> emocjonalna pozostała nienaruszona przez lata.",
                            "target_lang_translations": {
                                "ru": "Несмотря на расстояние, их эмоциональная <w>связь</w> с годами осталась нерушимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "związek",
                        "relacja",
                        "powiązanie",
                        "więzy",
                        "spójnia",
                        "łączność"
                    ],
                    "common_phrases": [
                        "silna więź",
                        "więź rodzinna",
                        "więź emocjonalna",
                        "zerwać więź",
                        "budować więź"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что связывает людей друг с другом; сильная эмоциональная, психическая или социальная связь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "отношения, общее соединение"
                            },
                            {
                                "target_lang_word": "узы",
                                "target_lang_sense_clarification": "тесная связь, оковы (перен.)"
                            },
                            {
                                "target_lang_word": "привязанность",
                                "target_lang_sense_clarification": "эмоциональная близость"
                            }

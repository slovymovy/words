                {
                    "sense_id": "01e769f2-c3b4-3029-989d-2e87fdea4d87",
                    "sense_definition": "Een afdeling of onderdeel van een grotere organisatie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit bedrijf is de commerciële <w>arm</w> van de stichting.",
                            "target_lang_translations": {
                                "en": "This company is the commercial <w>arm</w> of the foundation.",
                                "ru": "Эта компания является коммерческим <w>подразделением</w> фонда."
                            }
                        },
                        {
                            "text": "De politie wordt ook wel de sterke <w>arm</w> der wet genoemd.",
                            "target_lang_translations": {
                                "en": "The police are also called the strong <w>arm</w> of the law.",
                                "ru": "Полицию также называют сильной <w>рукой</w> закона."
                            }
                        }
                    ],
                    "common_phrases": [

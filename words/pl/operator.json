                    "target_lang_definitions": {
                        "ru": "Создатель визуальной части фильма, отвечающий за освещение и работу камеры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "кино"
                            },
                            {
                                "target_lang_word": "кинооператор",
                                "target_lang_sense_clarification": "профессия"

                },
                {
                    "sense_id": "4dd173b7-2c2e-3fe3-a6ed-eadabbd19d75",
                    "sense_definition": "Pomp, ceremony, or dignity, often associated with royalty or high office.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ceremony_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The body of the president will lie in <w>state</w> at the Capitol.",
                            "target_lang_translations": {
                                "ru": "Тело президента будет выставлено для <w>торжественного прощания</w> в Капитолии."
                            }
                        },
                        {
                            "text": "The Queen arrived in <w>state</w> for the opening of Parliament.",
                            "target_lang_translations": {
                                "ru": "Королева прибыла со всей <w>пышностью</w> на открытие Парламента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lie in state",
                        "in state"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "formal"

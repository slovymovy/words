            "pos": "verb",
            "senses": [
                {
                    "sense_id": "222d92ae-05e0-35c8-8a42-f652d5584947",
                    "sense_definition": "Совершить небольшую прогулку, пройтись пешком ради отдыха или удовольствия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "walk_leisure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такую прекрасную погоду нам обязательно нужно <w>прогуляться</w>."
                        },
                        {
                            "text": "Отец предложил детям <w>прогуляться</w> перед сном по парку."
                        }
                    ],
                    "synonyms": [
                        "пройтись",
                        "погулять"
                    ],
                    "common_phrases": [
                        "пойти прогуляться",
                        "выйти прогуляться"
                    ]
                },
                {
                    "sense_id": "ec59935b-ee21-3528-a837-66d2ddfa580d",
                    "sense_definition": "Сходить куда-либо пешком, часто сопровождая кого-либо или за компанию.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "walk_destination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>прогуляюсь</w> до магазина, тебе купить что-нибудь?"
                        },
                        {
                            "text": "Мы решили <w>прогуляться</w> до остановки автобуса пешком."
                        }
                    ],
                    "synonyms": [
                        "сходить",
                        "пройтись"
                    ],
                    "common_phrases": [
                        "прогуляться до магазина",
                        "прогуляться пешком"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies a casual walk to a specific place"
                        }
                    ]
                },
                {
                    "sense_id": "94777286-9727-3c66-8f1e-9780f6aba4ae",
                    "sense_definition": "Побывать где-либо вынужденно, не по своей воле (обычно с иронией).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "forced_visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за ошибки в документах мне пришлось лишний раз <w>прогуляться</w> в налоговую."
                        },
                        {
                            "text": "Хулиганам пришлось <w>прогуляться</w> в отделение полиции для выяснения личности."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used ironically"
                        }
                    ]
                },
                {
                    "sense_id": "e7317497-9070-3200-af7b-a764c3fd22a1",
                    "sense_definition": "Растратить все деньги на развлечения и кутежи.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "squander",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Купец знатно <w>прогулялся</w> на ярмарке и вернулся домой без гроша."
                        },
                        {
                            "text": "Он боялся <w>прогуляться</w> до последней копейки и остаться ни с чем."
                        }
                    ],
                    "synonyms": [
                        "прокутиться",
                        "потратиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to squandering money"
                        }
                    ]
                }
            ]
        }
    ]

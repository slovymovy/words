{
    "word_family": [
        "unbreached"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5aa4e87d-b363-346f-8307-2eb1c802f248",
                    "sense_definition": "A failure to follow a law, rule, or agreement; an act of breaking a promise or obligation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "violation_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company was sued for a serious <w>breach</w> of contract.",
                            "target_lang_translations": {
                                "ru": "На компанию подали в суд за серьезное <w>нарушение</w> контракта."
                            }
                        },
                        {
                            "text": "This data leak represents a major security <w>breach</w> for the bank.",
                            "target_lang_translations": {
                                "ru": "Эта утечка данных представляет собой серьезное <w>нарушение</w> системы безопасности банка."
                            }
                        }
                    ],
                    "synonyms": [
                        "fissure",
                        "rupture",
                        "gap",
                        "rift"
                    ],
                    "common_phrases": [
                        "breach of contract",
                        "security breach",
                        "breach of trust",
                        "breach of peace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несоблюдение закона, правила или соглашения; нарушение обещания или обязательства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарушение",
                                "target_lang_sense_clarification": "закона, правил, договора"
                            },
                            {
                                "target_lang_word": "невыполнение",
                                "target_lang_sense_clarification": "обязательств, условий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae0b05c8-f67a-32af-b050-e4c660d34a2d",
                    "sense_definition": "A hole or opening made in a wall, defense, or structure, usually caused by force.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_gap_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water poured rapidly through the <w>breach</w> in the dam.",
                            "target_lang_translations": {
                                "ru": "Вода стремительно лилась через <w>пролом</w> в плотине."
                            }
                        },
                        {
                            "text": "Soldiers rushed into the city through a <w>breach</w> in the castle walls.",
                            "target_lang_translations": {
                                "ru": "Солдаты ворвались в город через <w>брешь</w> в крепостных стенах."
                            }
                        }
                    ],
                    "synonyms": [
                        "gap",
                        "rupture",
                        "rift",
                        "break"
                    ],
                    "common_phrases": [
                        "breach in the wall",
                        "hull breach",
                        "step into the breach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие или дыра, пробитая в стене, укреплении или корпусе, обычно в результате применения силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брешь",
                                "target_lang_sense_clarification": "в стене, обороне, корпусе"
                            },
                            {
                                "target_lang_word": "пролом",
                                "target_lang_sense_clarification": "большое отверстие, пробитое силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f293e46f-b510-32d6-87c1-dac72e15fbf5",
                    "sense_definition": "A serious disagreement or separation that breaks a relationship between people or groups.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "relationship_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hope to heal the <w>breach</w> between the two families before the wedding.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся преодолеть <w>разрыв</w> между двумя семьями до свадьбы."
                            }
                        },
                        {
                            "text": "There is a widening <w>breach</w> between the government and the public on this issue.",
                            "target_lang_translations": {
                                "ru": "По этому вопросу между правительством и общественностью растет <w>пропасть</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rift",
                        "rupture",
                        "break"
                    ],
                    "common_phrases": [
                        "heal the breach",
                        "widen the breach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серьезное разногласие или прекращение отношений между людьми или группами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрыв",
                                "target_lang_sense_clarification": "отношений, связей"
                            },
                            {
                                "target_lang_word": "раскол",
                                "target_lang_sense_clarification": "разногласия во мнениях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df57a39a-2929-3fe2-9c68-c2ebe61b88cf",
                    "sense_definition": "To break or violate a law, agreement, or code of conduct.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "violation_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you <w>breach</w> the terms of the lease, the landlord may evict you.",
                            "target_lang_translations": {
                                "ru": "Если вы <w>нарушите</w> условия аренды, арендодатель может вас выселить."
                            }
                        },
                        {
                            "text": "The newspaper was accused of <w>breaching</w> the celebrity's privacy.",
                            "target_lang_translations": {
                                "ru": "Газету обвинили в том, что она <w>нарушила</w> право знаменитости на частную жизнь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "breach a contract",
                        "breach copyright",
                        "breach protocol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушать закон, соглашение или кодекс поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарушать",
                                "target_lang_sense_clarification": "закон, правила"
                            },
                            {
                                "target_lang_word": "не выполнять",
                                "target_lang_sense_clarification": "обязательства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aeb8fd00-9c42-3fa6-b22a-f4d1c1d2b74c",
                    "sense_definition": "To make a physical opening or hole in a structure or defense.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_gap_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The floodwaters eventually <w>breached</w> the levees protecting the town.",
                            "target_lang_translations": {
                                "ru": "Паводковые воды в конце концов <w>прорвали</w> дамбы, защищающие город."
                            }
                        },
                        {
                            "text": "Enemy forces managed to <w>breach</w> the fortress walls after days of fighting.",
                            "target_lang_translations": {
                                "ru": "Вражеским войскам удалось <w>пробить брешь</w> в стенах крепости после нескольких дней боев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "breach the wall",
                        "breach the hull",
                        "breach defenses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пробивать отверстие или создавать проход в сооружении или защите."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробивать брешь",
                                "target_lang_sense_clarification": "в стене, обороне"
                            },
                            {
                                "target_lang_word": "прорывать",
                                "target_lang_sense_clarification": "о воде, плотине, фронте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2fef5b3-c4a4-3d30-b491-26ab0225ad89",
                    "sense_definition": "To leap high out of the water and land with a splash, typically said of whales.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "whale_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were lucky enough to see a humpback whale <w>breach</w> near our boat.",
                            "target_lang_translations": {
                                "ru": "Нам посчастливилось увидеть, как горбатый кит <w>выпрыгнул</w> из воды рядом с нашей лодкой."
                            }
                        },
                        {
                            "text": "Tourists gathered on the deck to watch the whales <w>breaching</w> in the distance.",
                            "target_lang_translations": {
                                "ru": "Туристы собрались на палубе, чтобы понаблюдать за <w>выпрыгивающими</w> вдалеке китами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "zoology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпрыгивать высоко из воды и с шумом падать обратно (обычно о китах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпрыгивать",
                                "target_lang_sense_clarification": "из воды (о китах)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "29156407-d5e3-3975-a08a-a95c8bb30685",
                    "sense_definition": "A specific security exploit used against encrypted web traffic.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tech_name",
                    "name_type": "technology_or_software_name",
                    "examples": [
                        {
                            "text": "The system administrator patched the server to protect against the <w>BREACH</w> attack.",
                            "target_lang_translations": {
                                "ru": "Системный администратор установил патч на сервер для защиты от атаки <w>BREACH</w>."
                            }
                        },
                        {
                            "text": "Understanding <w>BREACH</w> is important for modern web security professionals.",
                            "target_lang_translations": {
                                "ru": "Понимание уязвимости <w>BREACH</w> важно для современных специалистов по веб-безопасности."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическая уязвимость безопасности, используемая против зашифрованного веб-трафика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "BREACH",
                                "target_lang_sense_clarification": "название уязвимости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
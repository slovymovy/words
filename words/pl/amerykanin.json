                    "sense_id": "1d583867-dfd2-33da-ab8a-ec6024478344",
                    "sense_definition": "Mieszkaniec lub obywatel Stanów Zjednoczonych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad jest <w>Amerykaninem</w> i pochodzi z Teksasu.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед — <w>американец</w>, он родом из Техаса."
                            }
                        },
                        {
                            "text": "Po kilku latach pobytu w Stanach Zjednoczonych przyzwyczaiłam się do sposobu bycia <w>Amerykanów</w>.",
                            "target_lang_translations": {
                                "ru": "После нескольких лет пребывания в Соединенных Штатах я привыкла к образу жизни <w>американцев</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Jankes",
                        "Amerykaniec"
                    ],
                    "common_phrases": [
                        "typowy Amerykanin",
                        "przeciętny Amerykanin",
                        "Amerykanin polskiego pochodzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Житель или гражданин Соединенных Штатов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "американец",
                                "target_lang_sense_clarification": "житель США"

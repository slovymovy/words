    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5dbef75c-c1ef-3bd8-ac25-955980aadd15",
                    "sense_definition": "Человек, который совершает зло, преступления или дурные поступки.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В конце фильма главный герой победил коварного <w>злодея</w>.",
                            "target_lang_translations": {
                                "en": "At the end of the movie, the main hero defeated the treacherous <w>villain</w>."
                            }
                        },
                        {
                            "text": "Полиция долго искала этого опасного <w>злодея</w>, который держал в страхе весь город.",
                            "target_lang_translations": {
                                "en": "The police searched for a long time for this dangerous <w>villain</w> who kept the whole city in fear."
                            }
                        }
                    ],
                    "synonyms": [
                        "преступник",
                        "негодя́й",
                        "подле́ц",
                        "лиходе́й"
                    ],
                    "antonyms": [
                        "герой"
                    ],
                    "common_phrases": [
                        "главный злодей",
                        "коварный злодей",
                        "настоящий злодей"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who commits evil acts, crimes, or bad deeds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "villain",
                                "target_lang_sense_clarification": "a wicked person or antagonist in a story"
                            },
                            {
                                "target_lang_word": "evildoer",
                                "target_lang_sense_clarification": "someone who performs evil acts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b15818a-bbe1-394c-a218-7e03d637d35e",
                    "sense_definition": "Употребляется в разговорной речи (часто в шутку или с укором) по отношению к человеку, который провинился, схитрил или сделал что-то не так.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот маленький <w>злодей</w> опять съел все конфеты из шкафа!",
                            "target_lang_translations": {
                                "en": "That little <w>rascal</w> ate all the candy from the cupboard again!"
                            }
                        },
                        {
                            "text": "Ну ты и <w>злодей</w>, так напугать меня своим внезапным появлением!",
                            "target_lang_translations": {
                                "en": "You're such a <w>rascal</w>, scaring me like that with your sudden appearance!"
                            }
                        }
                    ],
                    "synonyms": [
                        "негодник"
                    ],
                    "common_phrases": [
                        "ах ты злодей",
                        "маленький злодей"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "шутливо или бранно"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used colloquially (often jokingly or reproachfully) to refer to someone who has misbehaved or played a trick."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rascal",
                                "target_lang_sense_clarification": "a mischievous person or child"
                            },
                            {
                                "target_lang_word": "villain",
                                "target_lang_sense_clarification": "used playfully for a mischievous person"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "extinction",
        "extinguisher",
        "extinguishable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6edaadd5-0375-3d12-b34d-34197f679461",
                    "sense_definition": "To cause a fire or light to stop burning or shining.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firefighters managed to <w>extinguish</w> the blaze before it spread to nearby houses.",
                            "target_lang_translations": {
                                "ru": "Пожарным удалось <w>потушить</w> пламя, прежде чем оно перекинулось на соседние дома."
                            }
                        },
                        {
                            "text": "Please <w>extinguish</w> your cigarette immediately as this is a non-smoking area.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, немедленно <w>погасите</w> сигарету, так как это зона для некурящих."
                            }
                        }
                    ],
                    "synonyms": [
                        "put out",
                        "douse",
                        "quench",
                        "snuff out"
                    ],
                    "antonyms": [
                        "ignite",
                        "light",
                        "kindle",
                        "start"
                    ],
                    "common_phrases": [
                        "extinguish a fire",
                        "extinguish a cigarette",
                        "extinguish the flames",
                        "extinguish a light"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить огонь или свет перестать гореть или светить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тушить",
                                "target_lang_sense_clarification": "огонь, пожар"
                            },
                            {
                                "target_lang_word": "гасить",
                                "target_lang_sense_clarification": "свет, свечу, сигарету"
                            },
                            {
                                "target_lang_word": "потушить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d8062f3-e7b1-3c0b-8353-2dec6c7222c2",
                    "sense_definition": "To put an end to something abstract completely, such as a hope, feeling, or idea.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The disappointing news did not <w>extinguish</w> her hope of finding a cure.",
                            "target_lang_translations": {
                                "ru": "Разочаровывающие новости не <w>погасили</w> её надежду найти лекарство."
                            }
                        },
                        {
                            "text": "Years of hardship could not <w>extinguish</w> the team's enthusiasm for the project.",
                            "target_lang_translations": {
                                "ru": "Годы трудностей не смогли <w>погасить</w> энтузиазм команды по поводу проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "destroy",
                        "eliminate",
                        "suppress",
                        "dash",
                        "wipe out",
                        "annihilate"
                    ],
                    "antonyms": [
                        "inspire",
                        "rekindle",
                        "arouse",
                        "foment"
                    ],
                    "common_phrases": [
                        "extinguish hope",
                        "extinguish a dream",
                        "extinguish enthusiasm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью положить конец чему-то абстрактному, например надежде, чувству или идее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погасить",
                                "target_lang_sense_clarification": "чувства, энтузиазм"
                            },
                            {
                                "target_lang_word": "уничтожить",
                                "target_lang_sense_clarification": "надежды, планы"
                            },
                            {
                                "target_lang_word": "затмить",
                                "target_lang_sense_clarification": "превзойти, заслонить собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b999caa-0bb6-336c-a3e3-c1a3f6625381",
                    "sense_definition": "To officially make a legal right, claim, or debt invalid or void.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new contract will <w>extinguish</w> all previous obligations between the two parties.",
                            "target_lang_translations": {
                                "ru": "Новый контракт <w>аннулирует</w> все предыдущие обязательства между двумя сторонами."
                            }
                        },
                        {
                            "text": "Paying the settlement in full will <w>extinguish</w> the debt permanently.",
                            "target_lang_translations": {
                                "ru": "Полная выплата суммы урегулирования навсегда <w>погасит</w> долг."
                            }
                        }
                    ],
                    "synonyms": [
                        "annul",
                        "nullify",
                        "abolish",
                        "cancel",
                        "void"
                    ],
                    "antonyms": [
                        "establish",
                        "validate",
                        "enact"
                    ],
                    "common_phrases": [
                        "extinguish a debt",
                        "extinguish a right",
                        "extinguish a claim",
                        "extinguish a liability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально сделать юридическое право, требование или долг недействительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погасить",
                                "target_lang_sense_clarification": "долг, кредит"
                            },
                            {
                                "target_lang_word": "аннулировать",
                                "target_lang_sense_clarification": "права, обязательства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
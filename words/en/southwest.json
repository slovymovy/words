    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6750d258-e9ca-3bb7-9a8e-bc2505e2bb42",
                    "sense_definition": "The direction or point on a compass that is halfway between south and west.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "compass_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strong wind is blowing from the <w>southwest</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер дует с <w>юго-запада</w>."
                            }
                        },
                        {
                            "text": "Look to the <w>southwest</w> to see the setting sun.",
                            "target_lang_translations": {
                                "ru": "Посмотрите на <w>юго-запад</w>, чтобы увидеть заходящее солнце."
                            }
                        }
                    ],
                    "antonyms": [
                        "northeast"
                    ],
                    "common_phrases": [
                        "to the southwest",
                        "in the southwest",
                        "from the southwest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление или точка на компасе посередине между югом и западом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юго-запад",
                                "target_lang_sense_clarification": "направление"
                            },
                            {
                                "target_lang_word": "зюйд-вест",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

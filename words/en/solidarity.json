    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5dce214-a44c-31a8-84a8-4d5de148f671",
                    "sense_definition": "A bond of unity or agreement between individuals, united around a common goal or against a common enemy; mutual support within a group.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers went on strike in <w>solidarity</w> with their colleagues who were unfairly dismissed.",
                            "target_lang_translations": {
                                "ru": "Рабочие объявили забастовку в знак <w>солидарности</w> со своими коллегами, которые были несправедливо уволены."
                            }
                        },
                        {
                            "text": "We must show <w>solidarity</w> if we want to achieve our shared objectives.",
                            "target_lang_translations": {
                                "ru": "Мы должны проявить <w>солидарность</w>, если хотим достичь наших общих целей."
                            }
                        }
                    ],
                    "synonyms": [
                        "unity",
                        "cohesion",
                        "unanimity",
                        "harmony",
                        "accord"
                    ],
                    "antonyms": [
                        "division",
                        "discord",
                        "conflict"
                    ],
                    "common_phrases": [
                        "show solidarity",
                        "in solidarity with",
                        "express solidarity",
                        "sense of solidarity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единство убеждений и действий, взаимная помощь и поддержка членов группы, объединенных общей целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солидарность",
                                "target_lang_sense_clarification": "единство мнений и интересов"
                            },
                            {
                                "target_lang_word": "единство",
                                "target_lang_sense_clarification": "общность, цельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea3610a1-ad4f-3c90-82b5-c2912872511c",
                    "sense_definition": "Willingness to support another person who is in a difficult position or needs help.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt a deep sense of <w>solidarity</w> with her friend who was going through a similar tragedy.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала глубокую <w>солидарность</w> со своей подругой, переживавшей похожую трагедию."
                            }
                        },
                        {
                            "text": "Family <w>solidarity</w> helped them survive the difficult winter months.",
                            "target_lang_translations": {
                                "ru": "Семейная <w>сплочённость</w> помогла им пережить трудные зимние месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "support",
                        "loyalty",
                        "sympathy"
                    ],
                    "antonyms": [
                        "abandonment",
                        "indifference"
                    ],
                    "common_phrases": [
                        "family solidarity",
                        "social solidarity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовность поддержать другого человека, находящегося в трудном положении или нуждающегося в помощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солидарность",
                                "target_lang_sense_clarification": "сочувствие и поддержка"
                            },
                            {
                                "target_lang_word": "сплочённость",
                                "target_lang_sense_clarification": "тесная связь внутри группы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6af900b1-9c56-321e-a85d-f9aacacba13a",
                    "sense_definition": "A political movement and trade union founded in Poland that contributed significantly to the fall of Communism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "ideology_or_movement_name",
                    "examples": [
                        {
                            "text": "The formation of <w>Solidarity</w> was a crucial moment in the history of Eastern Europe.",
                            "target_lang_translations": {
                                "ru": "Образование «<w>Солидарности</w>» стало ключевым моментом в истории Восточной Европы."
                            }
                        },
                        {
                            "text": "He wrote a detailed book about the leaders of <w>Solidarity</w> and their struggle for freedom.",
                            "target_lang_translations": {

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2fac5598-8046-3517-8d6e-9ee9ff6cfc56",
                    "sense_definition": "Определять или узнавать кого-либо или что-либо по характерным признакам, приметам или данным.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "identification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Современные смартфоны умеют мгновенно <w>распознавать</w> лица своих владельцев.",
                            "target_lang_translations": {
                                "en": "Modern smartphones can instantly <w>recognize</w> their owners' faces."
                            }
                        },
                        {
                            "text": "Врач должен уметь вовремя <w>распознавать</w> симптомы опасного заболевания.",
                            "target_lang_translations": {
                                "en": "A doctor must be able to timely <w>recognize</w> the symptoms of a dangerous disease."
                            }
                        }
                    ],
                    "synonyms": [
                        "узнавать",
                        "определять",
                        "идентифицировать"
                    ],
                    "common_phrases": [
                        "распознавать лица",
                        "распознавать голос",
                        "распознавать текст",
                        "распознавать болезнь",
                        "распознавать образы"
                    ],
                    "target_lang_definitions": {
                        "en": "To determine or identify someone or something based on characteristic signs, marks, or data."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recognize",
                                "target_lang_sense_clarification": "to identify someone or something known before"
                            },
                            {
                                "target_lang_word": "identify",
                                "target_lang_sense_clarification": "to establish the identity of something"
                            },
                            {
                                "target_lang_word": "detect",
                                "target_lang_sense_clarification": "to discover or notice the presence of something (e.g., a disease)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23f16f68-b1dc-3ec9-b60a-b28c6cf26688",
                    "sense_definition": "Понимать или разгадывать скрытый смысл, сущность или намерения, наблюдая и анализируя.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ей удавалось безошибочно <w>распознавать</w> ложь в словах собеседника.",
                            "target_lang_translations": {
                                "en": "She managed to unerringly <w>detect</w> lies in her interlocutor's words."
                            }
                        },
                        {
                            "text": "Мы пытались <w>распознавать</w> знаки судьбы в череде случайных событий.",
                            "target_lang_translations": {
                                "en": "We tried to <w>discern</w> signs of fate in a series of random events."
                            }
                        }
                    ],
                    "synonyms": [
                        "разгадывать",
                        "понимать",
                        "постигать"
                    ],
                    "common_phrases": [
                        "распознавать ложь",
                        "распознавать намерения",
                        "распознавать фальшь"
                    ],
                    "target_lang_definitions": {
                        "en": "To understand or figure out hidden meaning, essence, or intentions through observation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "discern",
                                "target_lang_sense_clarification": "to perceive or recognize something obscure or hidden"
                            },
                            {
                                "target_lang_word": "detect",
                                "target_lang_sense_clarification": "to discover something hidden, like lies or deception"
                            },
                            {
                                "target_lang_word": "spot",
                                "target_lang_sense_clarification": "to notice something difficult to see"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80e83dab-195c-3b3c-8dbc-4e3ac708d70e",
                    "sense_definition": "Находить различия между объектами; отличать одно от другого.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "distinction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Только опытный эксперт может <w>распознавать</w> качественную подделку среди оригиналов.",
                            "target_lang_translations": {
                                "en": "Only an experienced expert can <w>identify</w> a high-quality fake among originals."
                            }
                        },
                        {
                            "text": "Учителя в школе сначала не могли <w>распознавать</w> братьев-близнецов.",
                            "target_lang_translations": {
                                "en": "At first, the teachers at school could not <w>tell apart</w> the twin brothers."
                            }
                        }
                    ],
                    "synonyms": [
                        "различать",
                        "отличать"
                    ],
                    "common_phrases": [
                        "распознавать близнецов",
                        "распознавать оттенки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To find differences between objects; to distinguish one from another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distinguish",
                                "target_lang_sense_clarification": "to perceive a difference between things"
                            },
                            {
                                "target_lang_word": "tell apart",
                                "target_lang_sense_clarification": "to distinguish between people or things that look similar"
                            },
                            {
                                "target_lang_word": "differentiate",
                                "target_lang_sense_clarification": "to recognize what makes things different"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
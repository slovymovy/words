        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9ba70ee-f777-33b5-9dc1-0238543b05ad",
                    "sense_definition": "Een hoge, natuurlijke verheffing van het aardoppervlak, hoger dan een heuvel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het platte Nederland noemen we iets al snel een <w>berg</w>.",
                            "target_lang_translations": {
                                "ru": "В равнинных Нидерландах мы любую возвышенность готовы назвать <w>горой</w>."
                            }
                        },
                        {
                            "text": "Tijdens onze vakantie in de Alpen hebben we een hoge <w>berg</w> beklommen.",
                            "target_lang_translations": {
                                "ru": "Во время нашего отпуска в Альпах мы поднялись на высокую <w>гору</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "dal"
                    ],
                    "common_phrases": [
                        "gouden bergen beloven",
                        "tegen een berg opzien",
                        "de berg op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокое естественное возвышение земной поверхности, выше холма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гора",
                                "target_lang_sense_clarification": "географический объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e44fd932-5a05-3f04-abcb-4505796acb79",
                    "sense_definition": "Een grote hoeveelheid of stapel van iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het feestje lag er een enorme <w>berg</w> afwas in de keuken.",
                            "target_lang_translations": {
                                "ru": "После вечеринки на кухне была огромная <w>гора</w> грязной посуды."
                            }
                        },
                        {
                            "text": "Hij moet nog een hele <w>berg</w> huiswerk maken voor morgen.",
                            "target_lang_translations": {
                                "ru": "Ему нужно сделать целую <w>гору</w> домашней работы на завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoop"
                    ],
                    "common_phrases": [
                        "een berg werk",
                        "een berg geld"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe large quantities informally"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество или куча чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гора",
                                "target_lang_sense_clarification": "перен. большое количество"
                            },
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "разг. беспорядочное скопление"
                            }
                        ]
                    }
                }

                            {
                                "target_lang_word": "приветливый",
                                "target_lang_sense_clarification": "радушный, вежливый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "700adfd1-0cc7-3f56-9d72-b8c2d0d4ddc8",

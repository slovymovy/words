    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3082c683-d051-3c7a-8521-8348e3d4ad8e",
                    "sense_definition": "Относящийся к началу какого-либо промежутка времени (суток, года, жизни, исторической эпохи).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <w>раннее</w> утро воздух в лесу был особенно свежим и прохладным.",
                            "target_lang_translations": {
                                "en": "In the <w>early</w> morning, the air in the forest was especially fresh and cool."
                            }
                        },
                        {
                            "text": "<w>Ранняя</w> весна в этом году порадовала нас тёплыми солнечными днями.",
                            "target_lang_translations": {
                                "en": "<w>Early</w> spring pleased us with warm sunny days this year."
                            }
                        }
                    ],
                    "synonyms": [
                        "начальный"
                    ],
                    "antonyms": [
                        "поздний"
                    ],
                    "common_phrases": [
                        "раннее утро",
                        "ранняя весна",
                        "раннее детство",
                        "ранняя осень"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the beginning of a period of time (day, year, life, historical era)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "early",
                                "target_lang_sense_clarification": "happening near the beginning of a period"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19f6cfb0-0a07-3edd-b6d9-03d181905277",
                    "sense_definition": "Наступающий или совершаемый раньше обычного срока; преждевременный.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "premature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его слишком <w>ранний</w> приход удивил хозяев, которые ещё не накрыли на стол.",
                            "target_lang_translations": {
                                "en": "His too <w>early</w> arrival surprised the hosts, who had not yet set the table."
                            }
                        },
                        {
                            "text": "<w>Ранний</w> снег выпал уже в октябре и покрыл ещё зелёную траву.",
                            "target_lang_translations": {
                                "en": "<w>Early</w> snow fell as early as October and covered the still green grass."
                            }
                        }
                    ],
                    "antonyms": [
                        "поздний"
                    ],
                    "common_phrases": [
                        "ранний визит",
                        "ранний брак",
                        "ранний звонок"
                    ],
                    "target_lang_definitions": {
                        "en": "Occurring or done before the usual time; premature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "early",
                                "target_lang_sense_clarification": "sooner than expected or usual"
                            },
                            {
                                "target_lang_word": "premature",
                                "target_lang_sense_clarification": "happening too soon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab4b5bbb-dacb-33cc-aa30-57b39a68976a",
                    "sense_definition": "Созревающий или появляющийся раньше других (о растениях, плодах, сортах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "agricultural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На рынке уже начали продавать <w>раннюю</w> клубнику и свежую зелень.",
                            "target_lang_translations": {
                                "en": "They have already started selling <w>early</w> strawberries and fresh herbs at the market."
                            }
                        },
                        {
                            "text": "Садоводы предпочитают сажать <w>ранние</w> сорта картофеля, чтобы быстрее собрать урожай.",
                            "target_lang_translations": {
                                "en": "Gardeners prefer planting <w>early</w> varieties of potatoes to harvest faster."
                            }
                        }
                    ],
                    "antonyms": [
                        "поздний"
                    ],
                    "common_phrases": [
                        "ранние овощи",
                        "ранний сорт",
                        "ранний урожай"
                    ],
                    "target_lang_definitions": {
                        "en": "Ripening or appearing earlier than others (of plants, fruits, varieties)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "early",
                                "target_lang_sense_clarification": "ripening or maturing quickly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "обращать в бегство",
                        "обращать в пепел",
                        "обращать в прах"
                    ],
                    "target_lang_definitions": {
                        "en": "To change or convert from one form or state into another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "to change into something else"
                            },
                            {
                                "target_lang_word": "convert",
                                "target_lang_sense_clarification": "to transform physical state or form"
                            },
                            {
                                "target_lang_word": "transform",
                                "target_lang_sense_clarification": "to change completely"
                            }
                        ]

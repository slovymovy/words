    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ba5d85e1-e433-3fad-b545-31bd1aaff880",
                    "sense_definition": "będący ostatnim w liczbie miliona; odpowiadający liczbie milion w kolejności",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To już <w>milionowy</w> pasażer obsłużony na naszym lotnisku w tym roku.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>миллионный</w> пассажир, обслуженный в нашем аэропорту в этом году."
                            }
                        },
                        {
                            "text": "Jako <w>milionowy</w> klient sklepu internetowego otrzymał cenną nagrodę.",
                            "target_lang_translations": {
                                "ru": "Как <w>миллионный</w> клиент интернет-магазина, он получил ценный приз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "milionowy klient",
                        "milionowy pasażer",
                        "milionowy egzemplarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "являющийся последним при счёте до миллиона; соответствующий числу миллион по порядку"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миллионный",
                                "target_lang_sense_clarification": "порядковое числительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ebb6008-709b-3da6-8aed-8225ddccc3cd",
                    "sense_definition": "zawierający milion lub kilka milionów elementów, często w odniesieniu do liczby mieszkańców",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "quantity_magnitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To miasto jest <w>milionową</w> metropolią, która nigdy nie zasypia.",
                            "target_lang_translations": {
                                "ru": "Этот город — <w>миллионный</w> мегаполис, который никогда не спит."
                            }
                        },
                        {
                            "text": "Na placu zebrała się <w>milionowa</w> rzesza ludzi protestujących przeciwko wojnie.",
                            "target_lang_translations": {
                                "ru": "На площади собралась <w>миллионная</w> толпа людей, протестующих против войны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "milionowa rzesza",
                        "milionowe miasto",
                        "milionowa publiczność"
                    ],
                    "target_lang_definitions": {
                        "ru": "насчитывающий миллион или несколько миллионов элементов, часто о количестве жителей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миллионный",
                                "target_lang_sense_clarification": "о численности"
                            },
                            {
                                "target_lang_word": "многомиллионный",
                                "target_lang_sense_clarification": "состоящий из многих миллионов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7ded4ec-fcb8-37aa-a400-7240228b837e",
                    "sense_definition": "o wartości jednego lub kilku milionów jednostek monetarnych",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "quantity_magnitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aby sfinalizować tę wielką inwestycję, firma musiała wziąć <w>milionowy</w> kredyt.",
                            "target_lang_translations": {
                                "ru": "Чтобы завершить эту крупную инвестицию, фирме пришлось взять <w>миллионный</w> кредит."
                            }
                        },
                        {
                            "text": "Jego <w>milionowy</w> majątek budził podziw i zazdrość wśród sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Его <w>миллионное</w> состояние вызывало восхищение и зависть у соседей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "milionowy kredyt",
                        "milionowy zysk",
                        "milionowy majątek"
                    ],
                    "target_lang_definitions": {
                        "ru": "стоимостью в один или несколько миллионов денежных единиц"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миллионный",
                                "target_lang_sense_clarification": "о стоимости или сумме"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
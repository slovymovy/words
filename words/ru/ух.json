    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "de9df4c1-1f51-342d-ba8a-1bca5056fb88",
                    "sense_definition": "Выражает сильное удивление, восхищение или неожиданность.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Ух>, какая красивая картина висит на стене!",
                            "target_lang_translations": {
                                "en": "<Wow>, what a beautiful painting is hanging on the wall!"
                            }
                        },
                        {
                            "text": "<Ух> ты, я не ожидал тебя здесь увидеть.",
                            "target_lang_translations": {
                                "en": "<Wow>, I didn't expect to see you here."
                            }
                        }
                    ],
                    "synonyms": [
                        "ого"
                    ],
                    "common_phrases": [
                        "ух ты",
                        "ух какой"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses strong surprise, admiration, or unexpectedness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wow",
                                "target_lang_sense_clarification": "expression of admiration or surprise"
                            },
                            {
                                "target_lang_word": "whoa",
                                "target_lang_sense_clarification": "expression of surprise or amazement"
                            },
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "expression of sudden emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b187aa2-8cef-3bb7-a8f3-ac4b8a37c709",
                    "sense_definition": "Выражает чувство облегчения, выдох после напряжения или завершения трудного дела.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Ух>, наконец-то мы закончили эту тяжёлую работу.",
                            "target_lang_translations": {
                                "en": "<Phew>, we finally finished this hard work."
                            }
                        },
                        {
                            "text": "<Ух>, пронесло, мы едва не опоздали на автобус.",
                            "target_lang_translations": {
                                "en": "<Phew>, that was close, we almost missed the bus."
                            }
                        }
                    ],
                    "synonyms": [
                        "фух"
                    ],
                    "common_phrases": [
                        "ух пронесло"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses a feeling of relief, an exhale after tension or completing a difficult task."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "phew",
                                "target_lang_sense_clarification": "sound of relief"
                            },
                            {
                                "target_lang_word": "whew",
                                "target_lang_sense_clarification": "sound of relief or tiredness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd185ad6-588d-3c22-a871-dc60ce0883fd",
                    "sense_definition": "Восклицание, сопровождающее тяжёлое физическое усилие, резкое движение или сильное ощущение (например, холода или жара).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Ух>, как тяжело поднимать этот огромный камень!",
                            "target_lang_translations": {
                                "en": "<Oof>, it's so heavy to lift this huge stone!"
                            }
                        },
                        {
                            "text": "<Ух>, ну и мороз сегодня на улице!",
                            "target_lang_translations": {
                                "en": "<Whoa>, it is freezing outside today!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ух как",
                        "ух и"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation accompanying heavy physical effort, sudden movement, or a strong sensation (e.g., cold or heat)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oof",
                                "target_lang_sense_clarification": "sound made during physical exertion"
                            },
                            {
                                "target_lang_word": "whoa",
                                "target_lang_sense_clarification": "reaction to a strong sensation or intensity"
                            },
                            {
                                "target_lang_word": "ugh",
                                "target_lang_sense_clarification": "sound of strain or effort"
                            }
                        ]
                    }
                }
            ]
        },

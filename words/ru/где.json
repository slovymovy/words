    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "58132e8b-7201-37a1-9a0a-af8dfa0e603b",
                    "sense_definition": "Вопросительное наречие, используемое для выяснения местонахождения кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_query",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скажите, пожалуйста, <w>где</w> находится ближайшая станция метро?"
                        },
                        {
                            "text": "<w>Где</w> ты был вчера вечером, когда я тебе звонил?"
                        }
                    ],
                    "synonyms": [
                        "в каком месте"
                    ],
                    "antonyms": [
                        "нигде"
                    ],
                    "common_phrases": [
                        "где это",
                        "где ты",
                        "где находится"
                    ]
                },
                {
                    "sense_id": "3d7a7640-fd4d-3d7b-b093-e8dfeb95d57e",
                    "sense_definition": "Употребляется в значении «где-нибудь» или «где-либо» для обозначения неопределённого места.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "indefinite_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Посмотри, не лежит ли <w>где</w> на столе моя синяя ручка."
                        },
                        {
                            "text": "Если встретишь его <w>где</w> в городе, передай ему привет."
                        }
                    ],
                    "synonyms": [
                        "где-нибудь",
                        "где-либо"
                    ],
                    "common_phrases": [
                        "видел ли где",
                        "слышал ли где"
                    ]
                },
                {
                    "sense_id": "77a2485d-c77a-30e3-9358-36b85f4d53e6",
                    "sense_definition": "Частица, выражающая сомнение, невозможность или усиленное отрицание (обычно в сочетаниях «где уж», «где там»).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Где</w> уж мне понять такие сложные научные теории!"
                        },
                        {
                            "text": "Он обещал прийти вовремя, но <w>где</w> там, опять опоздал на час."
                        }
                    ],
                    "common_phrases": [
                        "где уж там",
                        "где уж мне",
                        "где там"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "66120776-d462-3986-aded-28cc878e3e8d",
                    "sense_definition": "Используется в повторяющихся конструкциях для указания на то, что в разных местах происходит разное (в значении «в одном месте..., в другом...»).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "distributive_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поляна была пёстрой: <w>где</w> цвели ромашки, <w>где</w> виднелись колокольчики."
                        },
                        {
                            "text": "Дорога была плохой, <w>где</w> ямы, <w>где</w> грязь, а <w>где</w> и вовсе лужи."
                        }
                    ],
                    "common_phrases": [
                        "где густо где пусто"
                    ]
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "2e5f7148-26cd-3d6c-9eec-9da092e49968",
                    "sense_definition": "Союзное слово, которое связывает придаточное предложение места с главным, указывая на место совершения действия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relative_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили пообедать в том ресторане, <w>где</w> готовят вкусную пиццу."
                        },
                        {
                            "text": "Я совершенно не помню, <w>где</w> мы оставили машину на парковке."
                        }
                    ],
                    "synonyms": [
                        "в каком месте"
                    ],
                    "common_phrases": [
                        "там где",
                        "туда где",
                        "знаю где"
                    ]
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc4463bc-eead-345a-a87e-5c3283297832",
                    "sense_definition": "Een stuk speelgoed, meestal kegelvormig, dat men snel om zijn as laat draaien en dat op een punt balanceert",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "toy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen speelden buiten met een houten <w>tol</w>.",
                            "target_lang_translations": {
                                "en": "The children played outside with a wooden <w>spinning top</w>."
                            }
                        },
                        {
                            "text": "Hij liet de <w>tol</w> zo hard draaien dat hij over de tafel zoemde.",
                            "target_lang_translations": {
                                "en": "He spun the <w>top</w> so fast that it buzzed across the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "draaitol"
                    ],
                    "common_phrases": [
                        "een draaiende tol",
                        "met een tol spelen"
                    ],
                    "target_lang_definitions": {
                        "en": "A toy, usually cone-shaped, that is made to spin rapidly on its axis and balances on a point."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spinning top",
                                "target_lang_sense_clarification": "toy"
                            },
                            {
                                "target_lang_word": "top",
                                "target_lang_sense_clarification": "toy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f68486f-7d82-3194-8393-e2c4de9f1b60",
                    "sense_definition": "Geld dat betaald moet worden voor het gebruik van bepaalde wegen, bruggen of tunnels",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fee",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moesten vijf euro <w>tol</w> betalen om door de tunnel te mogen rijden.",
                            "target_lang_translations": {
                                "en": "We had to pay five euros <w>toll</w> to be allowed to drive through the tunnel."
                            }
                        },
                        {
                            "text": "In Frankrijk moet je op veel snelwegen <w>tol</w> betalen.",
                            "target_lang_translations": {
                                "en": "In France, you have to pay <w>toll</w> on many highways."
                            }
                        }
                    ],
                    "synonyms": [
                        "tolgeld"
                    ],
                    "common_phrases": [
                        "tol betalen",
                        "tol heffen"
                    ],
                    "target_lang_definitions": {
                        "en": "Money that must be paid for the use of certain roads, bridges, or tunnels."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "toll",
                                "target_lang_sense_clarification": "fee for road use"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb2e44de-a141-3307-832d-019127ad60ae",
                    "sense_definition": "Een zware prijs, verlies of nadelig gevolg dat door een bepaalde situatie wordt veroorzaakt",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "burden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jaren van zwaar werk eisen nu hun <w>tol</w> op zijn gezondheid.",
                            "target_lang_translations": {
                                "en": "The years of hard work are now taking their <w>toll</w> on his health."
                            }
                        },
                        {
                            "text": "De oorlog eiste een zware <w>tol</w> van de burgerbevolking.",
                            "target_lang_translations": {
                                "en": "The war took a heavy <w>toll</w> on the civilian population."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zijn tol eisen",
                        "een zware tol"
                    ],
                    "target_lang_definitions": {
                        "en": "A heavy price, loss, or negative consequence caused by a particular situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "toll",
                                "target_lang_sense_clarification": "adverse consequence or burden"
                            }
                        ]
                    }
                }
            ]
        },
        {

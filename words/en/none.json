    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "0e27d5be-1be7-39aa-b41b-7f2f497cd1e1",
                    "sense_definition": "Not any amount, part, or member of a given number or group.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I looked for some coffee, but there was <w>none</w> left in the pot.",
                            "target_lang_translations": {
                                "ru": "Я искал кофе, но в кофейнике <w>ничего</w> не осталось."
                            }
                        },
                        {
                            "text": "<w>None</w> of the students knew the answer to the difficult question.",
                            "target_lang_translations": {
                                "ru": "<w>Никто</w> из студентов не знал ответа на этот сложный вопрос."
                            }
                        }
                    ],
                    "antonyms": [
                        "some",
                        "all"
                    ],
                    "common_phrases": [
                        "none of",
                        "none at all"
                    ],
                    "target_lang_definitions": {
                        "ru": "Никакое количество, часть или представитель данной группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни один",
                                "target_lang_sense_clarification": "из группы"
                            },
                            {
                                "target_lang_word": "никто",
                                "target_lang_sense_clarification": "о людях"
                            },
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "о вещах"
                            },
                            {
                                "target_lang_word": "нисколько",
                                "target_lang_sense_clarification": "о количестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b708d97-84da-304e-ae21-b56214f6eb92",
                    "sense_definition": "No person; no one.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I knocked on the door, but <w>none</w> answered.",
                            "target_lang_translations": {
                                "ru": "Я постучал в дверь, но <w>никто</w> не ответил."
                            }
                        },
                        {
                            "text": "<w>None</w> but the brave deserve the fair.",
                            "target_lang_translations": {
                                "ru": "<w>Никто</w>, кроме храбрых, не заслуживает красавиц."
                            }
                        }
                    ],
                    "synonyms": [
                        "midafternoon"
                    ],
                    "common_phrases": [
                        "none other than"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни один человек; никто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никто",
                                "target_lang_sense_clarification": "местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "442a1e45-51e8-3f62-9fd4-2b562d988461",
                    "sense_definition": "To no extent; in no way. Used to emphasize that a situation remains the same despite something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adverb_extent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He fell off his bike but was <w>none</w> the worse for it.",
                            "target_lang_translations": {
                                "ru": "Он упал с велосипеда, но ему от этого стало <w>ничуть</w> не хуже."
                            }
                        },
                        {
                            "text": "She explained it twice, but I am <w>none</w> the wiser.",
                            "target_lang_translations": {
                                "ru": "Она объяснила это дважды, но мне не стало <w>ничуть</w> понятнее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "none the worse",
                        "none the wiser"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни в какой мере; никоим образом. Используется для подчеркивания того, что ситуация остается неизменной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничуть",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "нисколько",
                                "target_lang_sense_clarification": "наречие степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1489aae7-fcdf-3f1a-bb57-d7c7e2c81b9c",
                    "sense_definition": "Not at all; not very.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "adverb_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager was <w>none</w> too pleased with the delay.",
                            "target_lang_translations": {
                                "ru": "Менеджер был <w>совсем не</w> рад задержке."
                            }
                        },
                        {
                            "text": "We arrived <w>none</w> too soon, as the rain started immediately after.",
                            "target_lang_translations": {
                                "ru": "Мы прибыли <w>вовсе не</w> слишком рано, так как сразу после этого начался дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "none too"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совсем не; вовсе не."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совсем не",
                                "target_lang_sense_clarification": "отрицание качества"
                            },
                            {
                                "target_lang_word": "вовсе не",
                                "target_lang_sense_clarification": "усиленное отрицание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

            "senses": [
                {
                    "sense_id": "92dd8fed-f817-34bc-a874-5640b24c5c16",
                    "sense_definition": "Wat gedaan of gebruikt mag worden omdat er toestemming voor is gegeven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is niet <w>toegestaan</w> om in dit gebouw te roken.",
                            "target_lang_translations": {
                                "en": "It is not <w>allowed</w> to smoke in this building."
                            }
                        },
                        {
                            "text": "Een hond is een <w>toegestaan</w> huisdier, mits de andere bewoners er geen last van hebben.",
                            "target_lang_translations": {
                                "en": "A dog is a <w>permitted</w> pet, provided the other residents are not bothered by it."

            "senses": [
                {
                    "sense_id": "f276e351-03a3-3ee9-893a-faee218229c7",
                    "sense_definition": "Potrawa podawana na zimno, składająca się z pokrojonych warzyw, owoców, jaj, ryb lub mięsa, zazwyczaj wymieszanych z sosem, np. majonezem lub oliwą.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_dish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na przyjęciu urodzinowym podałam gościom pyszną <w>sałatkę</w> jarzynową.",
                            "target_lang_translations": {
                                "ru": "На дне рождения я подала гостям вкусный овощной <w>салат</w>."
                            }
                        },
                        {
                            "text": "Heniu, pokrój proszę wszystkie ugotowane warzywa na <w>sałatkę</w>.",
                            "target_lang_translations": {
                                "ru": "Геня, порежь, пожалуйста, все вареные овощи на <w>салат</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sałatka jarzynowa",
                        "sałatka owocowa",
                        "sałatka grecka",
                        "sałatka z kurczakiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Холодное блюдо из нарезанных овощей, фруктов, яиц, рыбы или мяса, обычно заправленное соусом, например, майонезом или маслом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "салат",
                                "target_lang_sense_clarification": "блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c2dadf2-a4c8-352f-9937-269d2aa9bd63",
                    "sense_definition": "Zdrobnienie od: <w>sałata</w>; mała główka sałaty lub pieszczotliwe określenie tej rośliny.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "plant_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym małym ogródku wzeszła już <w>sałatka</w> i marchewka.",
                            "target_lang_translations": {
                                "ru": "В нашем маленьком огороде уже взошел <w>салатик</w> и морковка."
                            }
                        },
                        {
                            "text": "Ta młoda <w>sałatka</w> ma bardzo delikatne i smaczne liście.",
                            "target_lang_translations": {
                                "ru": "У этого молодого <w>салата</w> очень нежные и вкусные листья."
                            }
                        }
                    ],

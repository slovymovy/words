{
    "word_family": [
        "data",
        "datownik",
        "antydatować"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c188a10-9bcb-3cd3-849a-520ebd3ac94e",
                    "sense_definition": "Umieszczać na czymś (np. na dokumencie, liście) informację o dacie sporządzenia lub powstania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "writing_date",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To urzędowe pismo jest <w>datowane</w> na rok 1974.",
                            "target_lang_translations": {
                                "ru": "Это официальное письмо <w>датировано</w> 1974 годом."
                            }
                        },
                        {
                            "text": "Musisz własnoręcznie podpisać i <w>datować</w> ten wniosek przed wysłaniem.",
                            "target_lang_translations": {
                                "ru": "Ты должен собственноручно подписать и <w>датировать</w> это заявление перед отправкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "datować list",
                        "datować pismo",
                        "błędnie datować",
                        "datować dokument"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить дату на документе или письме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "датировать",
                                "target_lang_sense_clarification": "ставить дату"
                            },
                            {
                                "target_lang_word": "ставить дату",
                                "target_lang_sense_clarification": "указывать число"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44027dc1-698f-3c1f-9b23-faf335706083",
                    "sense_definition": "Mieć swój początek w określonym czasie w przeszłości; wywodzić się z danego okresu (zazwyczaj używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "origin_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Początki tej lokalnej tradycji <w>datują</w> się na wiek siedemnasty.",
                            "target_lang_translations": {
                                "ru": "Истоки этой местной традиции <w>датируются</w> семнадцатым веком."
                            }
                        },
                        {
                            "text": "Ich wieloletnia przyjaźń <w>datuje</w> się jeszcze z czasów szkolnych.",
                            "target_lang_translations": {
                                "ru": "Их многолетняя дружба <w>берет начало</w> еще со школьных времен."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochodzić",
                        "wywodzić się"
                    ],
                    "common_phrases": [
                        "datować się od",
                        "datować się na",
                        "datować się z"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать начало в определенном времени в прошлом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "датироваться",
                                "target_lang_sense_clarification": "относиться к периоду"
                            },
                            {
                                "target_lang_word": "вести начало",
                                "target_lang_sense_clarification": "происходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43ed594d-072b-3138-8956-0338970a185e",
                    "sense_definition": "Ustalać czas powstania jakiegoś obiektu, zabytku lub wystąpienia zdarzenia za pomocą badań.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "determining_age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te cenne archeologiczne znaleziska <w>datuje</w> się na wczesną epokę brązu.",
                            "target_lang_translations": {
                                "ru": "Эти ценные археологические находки <w>датируются</w> ранним бронзовым веком."
                            }
                        },
                        {
                            "text": "Naukowcy próbują precyzyjnie <w>datować</w> odkryte ruiny średniowiecznego zamku.",
                            "target_lang_translations": {
                                "ru": "Ученые пытаются точно <w>датировать</w> обнаруженные руины средневекового замка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "datować znalezisko",
                        "datować metodą węglową",
                        "datować powstanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять время создания объекта или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "датировать",
                                "target_lang_sense_clarification": "определять возраст"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "lampa",
        "lampion",
        "lampowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4dbf946-a237-31f6-aab7-8d747c75b1ff",
                    "sense_definition": "zdrobnienie od: <w>lampa</w>; niewielka lampa, zazwyczaj stojąca na biurku lub szafce nocnej, dająca łagodne światło",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przy łóżku, na małym stoliku stała nocna <lampka>.",
                            "target_lang_translations": {
                                "ru": "У кровати на маленьком столике стояла ночная <лампа>."
                            }
                        },
                        {
                            "text": "Wieczorem zawsze włączam <lampkę> na biurku, żeby odrobić lekcje.",
                            "target_lang_translations": {
                                "ru": "Вечером я всегда включаю <лампу> на столе, чтобы сделать уроки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lampka nocna",
                        "lampka na biurko",
                        "włączyć lampkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьшительное от: <w>lampa</w>; небольшая лампа, обычно стоящая на столе или прикроватной тумбочке, дающая мягкий свет"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лампа",
                                "target_lang_sense_clarification": "настольная или небольшая"
                            },
                            {
                                "target_lang_word": "ночник",
                                "target_lang_sense_clarification": "прикроватная лампа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1020ac3f-0679-3dfa-892d-89e8341cf9ee",
                    "sense_definition": "małe źródło światła, np. element dekoracyjny na choince lub wskaźnik świetlny w urządzeniu",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "light_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strojąc choinkę na święta, najpierw zawieszam kolorowe <lampki>.",
                            "target_lang_translations": {
                                "ru": "Наряжая ёлку на праздники, я сначала вешаю разноцветные <лампочки>."
                            }
                        },
                        {
                            "text": "Czerwona <lampka> na desce rozdzielczej ostrzega o awarii silnika.",
                            "target_lang_translations": {
                                "ru": "Красная <лампочка> на приборной панели предупреждает о поломке двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "światełko",
                        "żaróweczka",
                        "dioda"
                    ],
                    "common_phrases": [
                        "lampki choinkowe",
                        "lampka kontrolna",
                        "lampka oliwna"
                    ],
                    "target_lang_definitions": {
                        "ru": "небольшой источник света, например, декоративный элемент на ёлке или световой индикатор в приборе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лампочка",
                                "target_lang_sense_clarification": "индикатор или элемент гирлянды"
                            },
                            {
                                "target_lang_word": "огонёк",
                                "target_lang_sense_clarification": "светящийся элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7f91d64-ca68-38a3-a4c2-ae88a1ca999b",
                    "sense_definition": "kieliszek wina lub innego alkoholu; porcja trunku",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "container_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do niedzielnego obiadu wypiłam jedną <lampkę> czerwonego wina.",
                            "target_lang_translations": {
                                "ru": "За воскресным обедом я выпила один <бокал> красного вина."
                            }
                        },
                        {
                            "text": "Gospodarz zaproponował gościom <lampkę> szampana na powitanie.",
                            "target_lang_translations": {
                                "ru": "Хозяин предложил гостям <бокал> шампанского в качестве приветствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "kieliszek"
                    ],
                    "common_phrases": [
                        "lampka wina",
                        "lampka szampana",
                        "wypić lampkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "бокал вина или другого алкоголя; порция напитка"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бокал",
                                "target_lang_sense_clarification": "вина или шампанского"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
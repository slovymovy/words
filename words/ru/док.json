    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "116bf7fe-2a31-3c22-a5b4-6abdde4bb016",
                    "sense_definition": "Сооружение в порту, предназначенное для постройки, осмотра и ремонта судов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Корабль зашёл в сухой <w>док</w> для капитального ремонта корпуса.",
                            "target_lang_translations": {
                                "en": "The ship entered the dry <w>dock</w> for a major hull overhaul."
                            }
                        },
                        {
                            "text": "Рабочие трудились в плавучем <w>доке</w>, устраняя повреждения после шторма.",
                            "target_lang_translations": {
                                "en": "The workers were working in the floating <w>dock</w>, repairing damage after the storm."
                            }
                        }
                    ],
                    "synonyms": [
                        "эллинг"
                    ],
                    "common_phrases": [
                        "сухой док",
                        "плавучий док",
                        "стоять в доке"
                    ],
                    "target_lang_definitions": {
                        "en": "A structure in a port designed for the construction, inspection, and repair of ships."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dock",
                                "target_lang_sense_clarification": "maritime structure"
                            },
                            {
                                "target_lang_word": "dry dock",
                                "target_lang_sense_clarification": "specific type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "911879c1-455e-3307-802f-3893f95dc098",
                    "sense_definition": "Врач, доктор (часто используется как дружеское или фамильярное обращение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эй, <w>док</w>, у меня сильно болит голова после вчерашнего.",
                            "target_lang_translations": {
                                "en": "Hey, <w>doc</w>, I have a splitting headache after yesterday."
                            }
                        },
                        {
                            "text": "Наш судовой <w>док</w> знает лучшие средства от морской болезни.",
                            "target_lang_translations": {
                                "en": "Our ship's <w>doc</w> knows the best remedies for seasickness."
                            }
                        }
                    ],
                    "synonyms": [
                        "врач",
                        "доктор",
                        "лекарь"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A doctor or physician (often used as a friendly or familiar form of address)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "doc",
                                "target_lang_sense_clarification": "informal address"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9eca5a3-e98f-335a-86b6-10010f3a11c2",
                    "sense_definition": "Сокращение от «деревообрабатывающий комбинат»; промышленное предприятие по переработке древесины.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой отец проработал на местном <w>ДОКе</w> более двадцати лет.",
                            "target_lang_translations": {
                                "en": "My father worked at the local <w>woodworking plant</w> for over twenty years."
                            }
                        },
                        {
                            "text": "Городской <w>ДОК</w> начал выпускать новую линейку мебели.",
                            "target_lang_translations": {
                                "en": "The city <w>wood processing plant</w> started producing a new line of furniture."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"woodworking combine\"; an industrial enterprise for processing wood."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "woodworking plant",
                                "target_lang_sense_clarification": "industrial facility"
                            },
                            {
                                "target_lang_word": "wood processing combine",
                                "target_lang_sense_clarification": "literal translation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca04b087-be7d-3d46-b4ea-5c69904a38ef",
                    "sense_definition": "Форма родительного или винительного падежа множественного числа существительного <w>дока</w> (знаток своего дела).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Среди нас нет таких <w>док</w>, чтобы решить эту сложную задачу сходу.",
                            "target_lang_translations": {
                                "en": "There are no such <w>experts</w> among us to solve this complex task right away."
                            }
                        },
                        {
                            "text": "Я уважаю этих мастеров, настоящих <w>док</w> в своём ремесле.",
                            "target_lang_translations": {
                                "en": "I respect these craftsmen, real <w>pros</w> at their trade."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive or accusative plural form of the noun \"дока\" (an expert or master of their craft)."
                    },
                    "translations": {

                },
                {
                    "sense_id": "9d8796fe-38f5-315f-bd1c-056f29edbd21",
                    "sense_definition": "Уменьшительная форма существительного <w>пола</w> (нижний край одежды).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "form_hem",
                    "name_type": "no",
                    "examples": [

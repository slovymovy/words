                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory processes oilseed <w>crushings</w> to create animal feed.",
                            "target_lang_translations": {
                                "ru": "Завод перерабатывает масличные <w>выжимки</w> для создания корма для животных."
                            }

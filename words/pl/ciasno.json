                {
                    "sense_id": "4f0d35c8-708d-371a-9ff4-555b2b8eede0",
                    "sense_definition": "W sposób ograniczony przestrzennie, z małą ilością wolnego miejsca; tłoczno.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W małym pokoju było nam bardzo <w>ciasno</w>, ale przytulnie.",
                            "target_lang_translations": {
                                "ru": "В маленькой комнате нам было очень <w>тесно</w>, но уютно."
                            }
                        },
                        {
                            "text": "Pasażerowie stali <w>ciasno</w> w zatłoczonym autobusie miejskim.",
                            "target_lang_translations": {
                                "ru": "Пассажиры стояли <w>тесно</w> в переполненном городском автобусе."
                            }
                        }
                    ],
                    "synonyms": [
                        "tłoczno",
                        "wąsko"
                    ],
                    "antonyms": [
                        "luźno",
                        "przestronnie",
                        "szeroko"
                    ],
                    "common_phrases": [
                        "jest tu ciasno",
                        "siedzieć ciasno",
                        "mieszkać ciasno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченно в пространстве, с малым количеством свободного места; многолюдно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тесно",
                                "target_lang_sense_clarification": "мало места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0110ae9-34ac-328f-bc1e-79b2e3c4df8c",
                    "sense_definition": "W sposób ścisły, mocno przylegający do czegoś lub opasujący coś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasek był zapięty zbyt <w>ciasno</w> i uwierał mnie w brzuch.",
                            "target_lang_translations": {
                                "ru": "Ремень был застегнут слишком <w>туго</w> и давил мне на живот."
                            }
                        },
                        {
                            "text": "Bandaż musi być owinięty <w>ciasno</w>, ale nie może tamować dopływu krwi.",
                            "target_lang_translations": {
                                "ru": "Бинт должен быть намотан <w>туго</w>, но не должен перекрывать кровоток."
                            }
                        }
                    ],
                    "synonyms": [
                        "ściśle",
                        "mocno"
                    ],
                    "antonyms": [
                        "luźno"
                    ],
                    "common_phrases": [
                        "ciasno związany",
                        "ciasno przylegać",
                        "ciasno owinąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотно прилегая к чему-либо, сильно стягивая или охватывая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туго",
                                "target_lang_sense_clarification": "сильно затянуто"
                            },
                            {
                                "target_lang_word": "плотно",
                                "target_lang_sense_clarification": "близко прилегая"
                            }
                        ]
                    }
                }
            ]

                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "The poet wrote his verses in broad <w>Scotch</w> to capture the local dialect.",
                            "target_lang_translations": {
                                "ru": "Поэт писал свои стихи на простом <w>шотландском</w>, чтобы передать местный диалект."
                            }
                        },
                        {
                            "text": "It can be difficult for outsiders to understand when the locals speak <w>Scotch</w>.",
                            "target_lang_translations": {
                                "ru": "Посторонним бывает трудно понять, когда местные говорят на <w>шотландском</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Scots"
                    ],
                    "common_phrases": [
                        "broad Scotch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шотландский язык (германской группы)."
                    },
                    "translations": {

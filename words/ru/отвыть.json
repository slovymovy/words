    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42645819-7a08-3ebb-b293-e6009ec35082",
                    "sense_definition": "Перестать, закончить выть (о животных, ветре или механизмах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вьюга за окном наконец <w>отвыла</w>, и в доме наступила долгожданная тишина.",
                            "target_lang_translations": {
                                "en": "The blizzard outside the window finally <w>stopped howling</w>, and a long-awaited silence fell over the house."
                            }
                        },
                        {
                            "text": "Одинокий волк <w>отвыл</w> свою печальную песню и бесшумно скрылся в чаще леса.",
                            "target_lang_translations": {
                                "en": "The lone wolf <w>finished howling</w> its sad song and silently disappeared into the thick of the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "замолкнуть",
                        "утихнуть"
                    ],
                    "antonyms": [
                        "завыть"
                    ],
                    "common_phrases": [
                        "ветер отвыл",
                        "волк отвыл"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop or finish howling (referring to animals, wind, or mechanisms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop howling",
                                "target_lang_sense_clarification": "cessation of action"
                            },
                            {
                                "target_lang_word": "finish howling",
                                "target_lang_sense_clarification": "completion of action"
                            },
                            {
                                "target_lang_word": "cease howling",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7780f37e-8cef-3445-b37e-b11c225a679b",
                    "sense_definition": "Издать вой в ответ на чей-либо зов, звук или другой вой.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "respond_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Соседская собака громко <w>отвыла</w> в ответ на пронзительный звук полицейской сирены.",
                            "target_lang_translations": {
                                "en": "The neighbor's dog loudly <w>howled back</w> in response to the piercing sound of the police siren."
                            }
                        },
                        {
                            "text": "Где-то вдалеке другой зверь <w>отвыл</w> на призыв вожака стаи.",
                            "target_lang_translations": {
                                "en": "Somewhere in the distance, another beast <w>howled in response</w> to the pack leader's call."
                            }
                        }
                    ],
                    "synonyms": [
                        "отозваться"
                    ],
                    "common_phrases": [
                        "отвыть в ответ"
                    ],
                    "target_lang_definitions": {
                        "en": "To emit a howl in response to a call, sound, or another howl."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "howl back",
                                "target_lang_sense_clarification": "reciprocal action"
                            },
                            {
                                "target_lang_word": "howl in response",
                                "target_lang_sense_clarification": "answering a sound"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5e2cc43d-9390-3628-a5c1-b9dd7dd8f95b",
                    "sense_definition": "Быть в действительности, иметься в наличии, происходить на самом деле.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "existence_reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В мире <существует> огромное количество разных языков.",
                            "target_lang_translations": {
                                "en": "A huge number of different languages <exist> in the world."
                            }
                        },
                        {
                            "text": "Такой проблемы просто не <существует>, вы её выдумали.",
                            "target_lang_translations": {
                                "en": "Such a problem simply does not <exist>; you made it up."
                            }
                        },
                        {
                            "text": "Учёные спорят, <существует> ли жизнь на других планетах.",
                            "target_lang_translations": {
                                "en": "Scientists argue whether life <exists> on other planets."
                            }
                        }
                    ],
                    "synonyms": [
                        "быть",
                        "иметься"
                    ],
                    "antonyms": [
                        "отсутствовать"
                    ],
                    "common_phrases": [
                        "перестать существовать",
                        "реально существовать",
                        "иметь право существовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To be real, to be present, to actually occur."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exist",
                                "target_lang_sense_clarification": "to have objective reality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b849cfe2-29a3-360c-847e-e1735a9e37cd",
                    "sense_definition": "Жить, быть живым, находиться в процессе жизни (о людях, животных, народах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "life_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Динозавры перестали <существовать> миллионы лет назад.",
                            "target_lang_translations": {
                                "en": "Dinosaurs ceased to <exist> millions of years ago."
                            }
                        },
                        {
                            "text": "Этот древний народ продолжает <существовать> и сохранять свои традиции.",
                            "target_lang_translations": {
                                "en": "This ancient people continues to <exist> and preserve its traditions."
                            }
                        }
                    ],
                    "synonyms": [
                        "жить"
                    ],
                    "antonyms": [
                        "умирать"
                    ],
                    "common_phrases": [
                        "продолжать существовать",
                        "мирно существовать"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "книжн."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To live, to be alive, to be in the process of living (about people, animals, nations)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exist",
                                "target_lang_sense_clarification": "to be alive"
                            },
                            {
                                "target_lang_word": "live",
                                "target_lang_sense_clarification": "to have life"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9b320bd-dd14-3623-a4ae-222a6a27cfea",
                    "sense_definition": "Поддерживать свою жизнь, имея необходимые материальные средства (деньги, еду).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "subsistence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студентам иногда приходится <существовать> на одну стипендию.",
                            "target_lang_translations": {
                                "en": "Students sometimes have to <live> on just a scholarship."
                            }
                        },
                        {
                            "text": "Без воды и пищи человек не может <существовать> долгое время.",
                            "target_lang_translations": {
                                "en": "Without water and food, a person cannot <exist> for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "жить",
                        "быть"
                    ],
                    "common_phrases": [
                        "существовать на зарплату",
                        "с трудом существовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To support one's life by having necessary material means (money, food)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live",
                                "target_lang_sense_clarification": "to subsist"
                            },
                            {
                                "target_lang_word": "exist",
                                "target_lang_sense_clarification": "to survive"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
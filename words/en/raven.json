                {
                    "sense_id": "514d0b02-00cf-3a57-ba95-2036ac1c4704",
                    "sense_definition": "A surname found in English-speaking countries.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_concept",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Raven</w> has been our neighbor for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Рэйвен</w> был нашим соседом более двадцати лет."
                            }

                    "sense_id": "eaab8788-5ff1-3660-8aa3-084b217ab7c9",
                    "sense_definition": "Napisać odpowiedź na otrzymaną wiadomość, list lub e-mail.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <w>odpisać</w> na tego maila od szefa.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <w>ответить</w> на это письмо от начальника."
                            }
                        },
                        {
                            "text": "Dlaczego nie <w>odpisałeś</w> na moją wczorajszą wiadomość?",
                            "target_lang_translations": {
                                "ru": "Почему ты не <w>ответил</w> на моё вчерашнее сообщение?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zerżnąć"
                    ],
                    "common_phrases": [
                        "odpisać na wiadomość",
                        "odpisać na list",
                        "odpisać na maila"
                    ],
                    "target_lang_definitions": {
                        "ru": "Написать ответ на полученное сообщение, письмо или электронную почту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответить",
                                "target_lang_sense_clarification": "на письмо, сообщение"

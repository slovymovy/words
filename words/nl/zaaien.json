            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af856e31-4a0d-3b03-a43c-ca02b51c96bf",
                    "sense_definition": "Zaadjes over de grond verspreiden of in de aarde stoppen zodat er planten uit kunnen groeien.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "landbouw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het vroege voorjaar gaan de boeren het graan <w>zaaien</w>.",
                            "target_lang_translations": {
                                "en": "In early spring, the farmers will <w>sow</w> the grain.",
                                "ru": "Ранней весной фермеры будут <w>сеять</w> зерно."
                            }
                        },
                        {
                            "text": "Als je nu bloemen gaat <w>zaaien</w>, heb je in de zomer een kleurrijke tuin.",
                            "target_lang_translations": {
                                "en": "If you <w>sow</w> flowers now, you will have a colorful garden in the summer.",
                                "ru": "Если ты сейчас <w>посеешь</w> цветы, летом у тебя будет красочный сад."
                            }
                        }
                    ],
                    "synonyms": [
                        "inzaaien",
                        "planten"
                    ],
                    "antonyms": [
                        "oogsten"
                    ],
                    "common_phrases": [
                        "gras zaaien",
                        "bloemen zaaien",
                        "in de lente zaaien",
                        "wie zaait zal oogsten"
                    ],
                    "target_lang_definitions": {
                        "en": "To scatter seeds over the ground or put them in the earth so that plants can grow.",
                        "ru": "Разбрасывать семена по земле или помещать их в почву, чтобы из них выросли растения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sow",
                                "target_lang_sense_clarification": "seeds"
                            },
                            {
                                "target_lang_word": "plant",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сеять",
                                "target_lang_sense_clarification": "семена, зерно"
                            },
                            {
                                "target_lang_word": "сажать",
                                "target_lang_sense_clarification": "растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd253765-f353-3f6a-bbe9-33cd15d2dd36",
                    "sense_definition": "Iets veroorzaken of verspreiden, meestal iets negatiefs zoals onrust, angst of twijfel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figuurlijk_veroorzaken",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tegenstrijdige berichten <w>zaaiden</w> verwarring onder de bevolking.",
                            "target_lang_translations": {
                                "en": "The conflicting reports <w>sowed</w> confusion among the population.",
                                "ru": "Противоречивые сообщения <w>посеяли</w> смуту среди населения."
                            }
                        },
                        {
                            "text": "Hij probeerde met zijn leugens haat te <w>zaaien</w> tussen de twee groepen.",
                            "target_lang_translations": {
                                "en": "He tried to <w>sow</w> hatred between the two groups with his lies.",
                                "ru": "Своей ложью он пытался <w>посеять</w> ненависть между двумя группами."
                            }
                        }
                    ],
                    "synonyms": [
                        "veroorzaken",
                        "teweegbrengen",
                        "verspreiden"
                    ],
                    "common_phrases": [
                        "onrust zaaien",
                        "haat zaaien",
                        "angst zaaien",
                        "twijfel zaaien",
                        "verwarring zaaien"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause or spread something, usually something negative like unrest, fear, or doubt.",
                        "ru": "Вызывать или распространять что-то, обычно негативное, например, беспокойство, страх или сомнения."
                    },
                    "translations": {
                        "en": [

                    ],
                    "common_phrases": [
                        "eerste wagon",
                        "laatste wagon",
                        "achterste wagon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство на рельсах, являющееся частью поезда, но не локомотивом.",
                        "en": "A vehicle on rails that is part of a train, but is not a locomotive."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab1b5c75-f879-3b1f-8144-82ff6620de5f",
                    "sense_definition": "Iść z kimś, aby towarzyszyć mu w drodze do określonego miejsca, często w celu zapewnienia bezpieczeństwa lub na pożegnanie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze staram się <w>odprowadzać</w> moich gości do drzwi wyjściowych.",
                            "target_lang_translations": {
                                "ru": "Я всегда стараюсь <w>провожать</w> моих гостей до входной двери."
                            }
                        },
                        {
                            "text": "Mój starszy brat musi codziennie <w>odprowadzać</w> mnie do szkoły.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат должен каждый день <w>отводить</w> меня в школу."
                            }
                        }
                    ],
                    "synonyms": [
                        "towarzyszyć",
                        "zaprowadzać"
                    ],
                    "antonyms": [
                        "przyprowadzać"
                    ],
                    "common_phrases": [
                        "odprowadzać do domu",
                        "odprowadzać do drzwi",
                        "odprowadzać na pociąg",
                        "odprowadzać wzrokiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти вместе с кем-то, чтобы проводить его до определенного места или попрощаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провожать",
                                "target_lang_sense_clarification": "сопровождать уходящего"
                            },
                            {
                                "target_lang_word": "отводить",
                                "target_lang_sense_clarification": "доставлять кого-то в определенное место (напр. ребенка в школу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7be8264-ffa4-34a2-9085-06230b3049d4",
                    "sense_definition": "Przekazywać należne pieniądze, takie jak podatki lub składki, do odpowiedniej instytucji lub urzędu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy pracodawca ma obowiązek <w>odprowadzać</w> składki na ubezpieczenie zdrowotne.",
                            "target_lang_translations": {
                                "ru": "Каждый работодатель обязан <w>отчислять</w> взносы на медицинское страхование."
                            }
                        },
                        {
                            "text": "Przedsiębiorcy muszą regularnie <w>odprowadzać</w> podatek dochodowy do urzędu skarbowego.",
                            "target_lang_translations": {
                                "ru": "Предприниматели должны регулярно <w>отчислять</w> подоходный налог в налоговую инспекцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "płacić",
                        "uiszczać",
                        "przekazywać"
                    ],
                    "common_phrases": [
                        "odprowadzać podatek",
                        "odprowadzać składki",
                        "odprowadzać zysk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перечислять причитающиеся деньги, такие как налоги или взносы, в соответствующее учреждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отчислять",
                                "target_lang_sense_clarification": "переводить часть средств (налоги, взносы)"
                            },
                            {
                                "target_lang_word": "уплачивать",
                                "target_lang_sense_clarification": "вносить плату за что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54154c08-a0db-3493-a8f0-899ad5af5cc7",
                    "sense_definition": "Kierować ciecz, gaz lub inne substancje z jednego miejsca do drugiego, zazwyczaj na zewnątrz, używając do tego rur lub kanałów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Specjalna rura w dachu służy do tego, aby <w>odprowadzać</w> wodę deszczową do ogrodu.",
                            "target_lang_translations": {
                                "ru": "Специальная труба на крыше служит для того, чтобы <w>отводить</w> дождевую воду в сад."
                            }
                        },
                        {
                            "text": "Ten nowoczesny system wentylacji skutecznie <w>odprowadza</w> dym i ciepło z kuchni.",
                            "target_lang_translations": {
                                "ru": "Эта современная система вентиляции эффективно <w>отводит</w> дым и тепло из кухни."
                            }
                        }
                    ],
                    "synonyms": [
                        "usuwać",
                        "wyprowadzać"
                    ],
                    "antonyms": [
                        "doprowadzać"
                    ],
                    "common_phrases": [
                        "odprowadzać wodę",
                        "odprowadzać ścieki",
                        "odprowadzać ciepło",
                        "odprowadzać wilgoć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять жидкость, газ или другие вещества из одного места в другое, обычно наружу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отводить",
                                "target_lang_sense_clarification": "удалять жидкость или тепло в сторону"
                            },
                            {
                                "target_lang_word": "выводить",
                                "target_lang_sense_clarification": "удалять наружу (дым, пятна)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "obchód",
        "obejście",
        "obchodowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "03e4fb3c-b36e-38dc-b55b-a6452f9bdc09",
                    "sense_definition": "Być dla kogoś ważnym, budzić czyjeś zainteresowanie lub troskę (często używane w przeczeniach).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To, co robisz w wolnym czasie, w ogóle mnie nie <w>obchodzi</w>.",
                            "target_lang_translations": {
                                "ru": "То, чем ты занимаешься в свободное время, меня совершенно не <w>волнует</w>."
                            }
                        },
                        {
                            "text": "Czy twoich rodziców <w>obchodzi</w>, gdzie spędzasz wieczory?",
                            "target_lang_translations": {
                                "ru": "Твоих родителей <w>волнует</w>, где ты проводишь вечера?"
                            }
                        }
                    ],
                    "synonyms": [
                        "martwić",
                        "przejmować się",
                        "pomijać"
                    ],
                    "common_phrases": [
                        "nic mnie to nie obchodzi",
                        "co cię to obchodzi",
                        "mało mnie to obchodzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть важным для кого-то, вызывать интерес или беспокойство (часто используется в отрицаниях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "беспокоить, быть важным"
                            },
                            {
                                "target_lang_word": "интересовать",
                                "target_lang_sense_clarification": "вызывать интерес"
                            },
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "иметь отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8c8b887-b7e9-3a6c-8cad-2d64033fb51d",
                    "sense_definition": "Uroczyście spędzać czas z okazji święta, rocznicy lub ważnego wydarzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "celebration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu będę <w>obchodzić</w> moje osiemnaste urodziny.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе я буду <w>отмечать</w> свое восемнадцатилетие."
                            }
                        },
                        {
                            "text": "Polacy bardzo uroczyście <w>obchodzą</w> święta Bożego Narodzenia.",
                            "target_lang_translations": {
                                "ru": "Поляки очень торжественно <w>празднуют</w> Рождество."
                            }
                        }
                    ],
                    "synonyms": [
                        "martwić",
                        "przejmować się",
                        "pomijać"
                    ],
                    "common_phrases": [
                        "obchodzić urodziny",
                        "obchodzić rocznicę ślubu",
                        "hucznie obchodzić",
                        "obchodzić jubileusz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественно отмечать праздник, годовщину или важное событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмечать",
                                "target_lang_sense_clarification": "праздновать событие"
                            },
                            {
                                "target_lang_word": "праздновать",
                                "target_lang_sense_clarification": "проводить праздник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ff422ff-f746-3d89-83c6-0aad20742280",
                    "sense_definition": "Przemieszczać się dookoła jakiegoś miejsca, obiektu lub osoby.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <w>obchodzić</w> wielki płot, żeby znaleźć wejście do parku.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>обходить</w> большой забор, чтобы найти вход в парк."
                            }
                        },
                        {
                            "text": "Lubię <w>obchodzić</w> całe miasto podczas długich, wieczornych spacerów.",
                            "target_lang_translations": {
                                "ru": "Я люблю <w>обходить</w> весь город во время долгих вечерних прогулок."
                            }
                        }
                    ],
                    "synonyms": [
                        "okrążać"
                    ],
                    "common_phrases": [
                        "obchodzić dom dookoła",
                        "obchodzić jezioro",
                        "obchodzić teren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться вокруг какого-либо места, объекта или человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "идти вокруг чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3dbaf357-fa25-3860-94a5-5aa5260058ad",
                    "sense_definition": "Zachowywać się wobec kogoś lub używać czegoś w określony sposób.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z tym kruchym szkłem trzeba <w>obchodzić</w> się bardzo ostrożnie.",
                            "target_lang_translations": {
                                "ru": "С этим хрупким стеклом нужно <w>обращаться</w> очень осторожно."
                            }
                        },
                        {
                            "text": "On niestety nie umie <w>obchodzić</w> się z małymi dziećmi.",
                            "target_lang_translations": {
                                "ru": "К сожалению, он не умеет <w>обращаться</w> с маленькими детьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "martwić",
                        "przejmować się",
                        "pomijać"
                    ],
                    "common_phrases": [
                        "obchodzić się delikatnie",
                        "obchodzić się ostrożnie",
                        "obchodzić się brutalnie",
                        "obchodzić się z czymś"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wymaga zaimka zwrotnego \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя определенным образом по отношению к кому-то или использовать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "вести себя с кем-то/чем-то"
                            },
                            {
                                "target_lang_word": "обходиться",
                                "target_lang_sense_clarification": "поступать с кем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a50728b-5df7-3987-888f-b014a7307e1e",
                    "sense_definition": "Unikać czegoś, omijać przeszkodę lub sprytnie unikać przestrzegania przepisów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bypass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieuczciwi pracownicy próbowali <w>obchodzić</w> nowe przepisy podatkowe.",
                            "target_lang_translations": {
                                "ru": "Нечестные сотрудники пытались <w>обходить</w> новые налоговые правила."
                            }
                        },
                        {
                            "text": "Udało nam się <w>obchodzić</w> poranny korek bocznymi drogami.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <w>обойти</w> утреннюю пробку по боковым дорогам."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomijać"
                    ],
                    "common_phrases": [
                        "obchodzić prawo",
                        "obchodzić przepisy",
                        "obchodzić zabezpieczenia",
                        "obchodzić system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избегать чего-либо, огибать препятствие или хитро уклоняться от соблюдения правил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "избегать препятствия или закона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f94eba9e-1539-313b-9e8f-9019790e8964",
                    "sense_definition": "Radzić sobie bez posiadania czegoś lub bez czyjejś pomocy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sufficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>obchodzić</w> się bez deseru, jeśli nie zjesz całego obiadu.",
                            "target_lang_translations": {
                                "ru": "Тебе придется <w>обойтись</w> без десерта, если ты не съешь весь обед."
                            }
                        },
                        {
                            "text": "Przez tydzień będziemy musieli <w>obchodzić</w> się bez ciepłej wody.",
                            "target_lang_translations": {
                                "ru": "В течение недели нам придется <w>обходиться</w> без горячей воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "martwić",
                        "przejmować się",
                        "pomijać"
                    ],
                    "common_phrases": [
                        "obchodzić się bez pomocy",
                        "obchodzić się smakiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wymaga zaimka zwrotnego \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Справляться без наличия чего-либо или без чьей-либо помощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходиться",
                                "target_lang_sense_clarification": "жить/справляться без чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e8bc31dd-9814-38a8-b0c4-16d312a7f556",
                    "sense_definition": "Вызвать интерес, любопытство или симпатию, заставить обратить на себя внимание.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Громкая музыка на улице сразу <привлекла> моё внимание.",
                            "target_lang_translations": {
                                "en": "Loud music on the street immediately <drew> my attention."
                            }
                        },
                        {
                            "text": "Эта необычная картина <привлекла> множество посетителей в музей.",
                            "target_lang_translations": {
                                "en": "This unusual painting <attracted> many visitors to the museum."
                            }
                        }
                    ],
                    "synonyms": [
                        "заинтересовать",
                        "приманить"
                    ],
                    "antonyms": [
                        "оттолкнуть"
                    ],
                    "common_phrases": [
                        "привлечь внимание",
                        "привлечь взгляд",
                        "привлечь интерес"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause interest, curiosity, or sympathy; to make someone pay attention to oneself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "interest or attention"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "attention or crowds"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "someone's eye or attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0eb53c8c-b338-39a0-8508-413fbbcdcc7c",
                    "sense_definition": "Побудить кого-либо принять участие в каком-либо деле, работе или мероприятии.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "involve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <привлечь> к этому сложному проекту опытных специалистов.",
                            "target_lang_translations": {
                                "en": "We decided to <involve> experienced specialists in this complex project."
                            }
                        },
                        {
                            "text": "Учитель старался <привлечь> всех учеников к обсуждению новой темы.",
                            "target_lang_translations": {
                                "en": "The teacher tried to <engage> all the students in the discussion of the new topic."
                            }
                        }
                    ],
                    "synonyms": [
                        "мобилизовать"
                    ],
                    "antonyms": [
                        "отстранить"
                    ],
                    "common_phrases": [
                        "привлечь к работе",
                        "привлечь к участию",
                        "привлечь экспертов"
                    ],
                    "target_lang_definitions": {
                        "en": "To induce someone to participate in a task, job, or event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "involve",
                                "target_lang_sense_clarification": "in a project or activity"
                            },
                            {
                                "target_lang_word": "engage",
                                "target_lang_sense_clarification": "in participation"
                            },
                            {
                                "target_lang_word": "enlist",
                                "target_lang_sense_clarification": "help or support"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9b794e6-49a0-381e-8faf-b4220e76352c",
                    "sense_definition": "Найти и получить материальные средства или ресурсы для использования в определённых целях.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компании удалось <привлечь> крупные инвестиции для развития бизнеса.",
                            "target_lang_translations": {
                                "en": "The company managed to <attract> large investments for business development."
                            }
                        },
                        {
                            "text": "Городские власти планируют <привлечь> дополнительные средства на ремонт дорог.",
                            "target_lang_translations": {
                                "en": "City authorities plan to <raise> additional funds for road repairs."
                            }
                        }
                    ],
                    "synonyms": [
                        "мобилизовать"
                    ],
                    "common_phrases": [
                        "привлечь инвестиции",
                        "привлечь капитал",
                        "привлечь средства",
                        "привлечь финансирование"
                    ],
                    "target_lang_definitions": {
                        "en": "To find and obtain financial means or resources for specific purposes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "investment or capital"
                            },
                            {
                                "target_lang_word": "raise",
                                "target_lang_sense_clarification": "funds or money"
                            },
                            {
                                "target_lang_word": "secure",
                                "target_lang_sense_clarification": "resources"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be3bcc48-ce42-346b-94e0-5352ade7d7f8",
                    "sense_definition": "Призвать к ответу за правонарушение, заставить нести ответственность (юридический термин).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За нарушение правил дорожного движения водителя могут <привлечь> к ответственности.",
                            "target_lang_translations": {
                                "en": "For violating traffic rules, the driver can be <held> liable."
                            }
                        },
                        {
                            "text": "Полиция нашла преступника, и его <привлекли> к суду за кражу.",
                            "target_lang_translations": {
                                "en": "The police found the criminal, and he was <brought> to trial for theft."
                            }
                        }
                    ],
                    "common_phrases": [
                        "привлечь к ответственности",
                        "привлечь к суду"
                    ],
                    "target_lang_definitions": {
                        "en": "To call to account for an offense; to force to bear responsibility (legal term)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold liable",
                                "target_lang_sense_clarification": "legal responsibility"
                            },
                            {
                                "target_lang_word": "prosecute",
                                "target_lang_sense_clarification": "bring to court"
                            },
                            {
                                "target_lang_word": "bring to justice",
                                "target_lang_sense_clarification": "legal context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20da8582-4987-381d-a7f7-142d055d1e5a",
                    "sense_definition": "Приблизить или притянуть к себе физически.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он нежно <привлёк> её к себе и обнял за плечи.",
                            "target_lang_translations": {
                                "en": "He gently <drew> her to him and hugged her shoulders."
                            }
                        },
                        {
                            "text": "Магнит <привлёк> рассыпанные на столе железные скрепки.",
                            "target_lang_translations": {
                                "en": "The magnet <attracted> the iron paperclips scattered on the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "притянуть"
                    ],
                    "antonyms": [
                        "оттолкнуть",
                        "отогнать"
                    ],
                    "common_phrases": [
                        "привлечь к себе"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring closer or pull towards oneself physically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "pull closer"
                            },
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "physically"
                            },
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "magnetic force"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
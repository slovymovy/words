    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "47945ce3-5d86-3799-a862-f557f6bbe62a",
                    "sense_definition": "Сделать попытку, постараться совершить какое-либо действие.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attempt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <попробовать> выучить новый иностранный язык.",
                            "target_lang_translations": {
                                "en": "I want to <w>try</w> to learn a new foreign language."
                            }
                        },
                        {
                            "text": "<Попробуй> открыть эту дверь другим ключом.",
                            "target_lang_translations": {
                                "en": "<w>Try</w> to open this door with another key."
                            }
                        }
                    ],
                    "synonyms": [
                        "попытаться"
                    ],
                    "common_phrases": [
                        "попробовать сделать",
                        "стоит попробовать",
                        "попробовать снова"
                    ],
                    "target_lang_definitions": {
                        "en": "To make an attempt or effort to perform an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "to attempt to do something"
                            },
                            {
                                "target_lang_word": "attempt",
                                "target_lang_sense_clarification": "to make an effort to achieve something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "412c24a2-6873-3a76-8ab5-6aa628eea51c",
                    "sense_definition": "Съесть или выпить небольшое количество чего-либо, чтобы оценить вкус.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Можно мне <попробовать> этот красивый торт?",
                            "target_lang_translations": {
                                "en": "May I <w>try</w> this beautiful cake?"
                            }
                        },
                        {
                            "text": "Повар дал нам <попробовать> соус перед подачей блюда.",
                            "target_lang_translations": {
                                "en": "The chef gave us the sauce to <w>taste</w> before serving the dish."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дать попробовать",
                        "попробовать на вкус",
                        "хочу попробовать"
                    ],
                    "target_lang_definitions": {
                        "en": "To eat or drink a small amount of something to evaluate its taste."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "taste",
                                "target_lang_sense_clarification": "to sample the flavor of something"
                            },
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "to sample food or drink"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09a26456-22d2-3b71-b03c-9a168e2b1a25",
                    "sense_definition": "Испытать или проверить свойства чего-либо на практике.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "test",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <попробовать> новую компьютерную программу в работе.",
                            "target_lang_translations": {
                                "en": "We decided to <w>try</w> the new computer program at work."
                            }
                        },
                        {
                            "text": "Тебе обязательно нужно <попробовать> свои силы в этом конкурсе.",
                            "target_lang_translations": {
                                "en": "You definitely need to <w>test</w> your strength in this competition."
                            }
                        }
                    ],
                    "synonyms": [
                        "испытать",
                        "проверить"
                    ],
                    "common_phrases": [
                        "попробовать себя",
                        "попробовать в деле",
                        "попробовать метод"
                    ],
                    "target_lang_definitions": {
                        "en": "To test or check the properties of something in practice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "test",
                                "target_lang_sense_clarification": "to verify quality or performance"
                            },
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "to use something to see if it works"
                            }
                        ]
                    }
                }
            ]
        }
    ]

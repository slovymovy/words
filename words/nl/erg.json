    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "695d33d1-1d4b-3caf-9126-08a98a386932",
                    "sense_definition": "In hoge mate; gebruikt om een eigenschap te versterken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is vandaag <w>erg</w> koud buiten.",
                            "target_lang_translations": {

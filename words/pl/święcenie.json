                {
                    "sense_id": "1d24df3a-fd3c-3ae9-824b-b4cbaad91470",
                    "sense_definition": "rytuał religijny polegający na nadaniu komuś lub czemuś charakteru sakralnego, często poprzez pokropienie wodą święconą lub odmówienie modlitwy",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_ritual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Wielką Sobotę odbywa się tradycyjne <w>święcenie</w> pokarmów w kościołach.",
                            "target_lang_translations": {
                                "ru": "В Великую субботу в церквях проходит традиционное <w>освящение</w> еды."
                            }
                        },
                        {
                            "text": "Ksiądz został zaproszony na uroczyste <w>święcenie</w> nowego budynku szkoły.",
                            "target_lang_translations": {
                                "ru": "Священника пригласили на торжественное <w>освящение</w> нового здания школы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "święcenie pokarmów",
                        "święcenie wody",
                        "święcenie pól"
                    ],
                    "target_lang_definitions": {
                        "ru": "Религиозный обряд, придающий кому-либо или чему-либо сакральный характер, часто через окропление святой водой или чтение молитвы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освящение",
                                "target_lang_sense_clarification": "обряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0558025-bd68-3857-b6b5-ab6507a1a1fa",
                    "sense_definition": "sakrament w kościołach chrześcijańskich nadający władzę sprawowania funkcji kapłańskich; uroczystość wyświęcania duchownego",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_sacrament",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody diakon przygotowuje się duchowo, aby przyjąć <w>święcenia</w> kapłańskie.",
                            "target_lang_translations": {
                                "ru": "Молодой дьякон духовно готовится принять священническое <w>рукоположение</w>."
                            }
                        },
                        {
                            "text": "Uroczyste <w>święcenia</w> biskupie zgromadziły w katedrze wielu wiernych.",
                            "target_lang_translations": {
                                "ru": "Торжественное епископское <w>рукоположение</w> собрало в соборе множество верующих."
                            }
                        }
                    ],
                    "synonyms": [
                        "sakrament święceń"
                    ],
                    "common_phrases": [
                        "święcenia kapłańskie",
                        "przyjąć święcenia",
                        "święcenia biskupie"
                    ],
                    "traits": [

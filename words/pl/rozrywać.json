                            {
                                "target_lang_word": "разрывать на части",
                                "target_lang_sense_clarification": "переносное: не давать прохода из-за востребованности"
                            },
                            {
                                "target_lang_word": "расхватывать",
                                "target_lang_sense_clarification": "быстро разбирать или покупать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92109da6-12b0-39b5-bf4f-3ed9e0fc1830",
                    "sense_definition": "Niszczyć coś gwałtownie, powodując rozpad na drobne elementy, często w wyniku wybuchu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociski artyleryjskie potrafią <w>rozrywać</w> grube mury obronne.",
                            "target_lang_translations": {
                                "ru": "Артиллерийские снаряды способны <w>разрывать</w> толстые оборонительные стены."
                            }
                        },
                        {
                            "text": "Siła eksplozji może <w>rozrywać</w> budynki na drobne kawałki.",
                            "target_lang_translations": {
                                "ru": "Сила взрыва может <w>разносить</w> здания на мелкие кусочки."
                            }
                        }
                    ],
                    "synonyms": [

                {
                    "sense_id": "2f5518b0-178e-31dd-956a-9b6498b5d6a5",
                    "sense_definition": "Loyal, reliable, and adhering firmly to a person, cause, or duty.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sem_loyalty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>faithful</w> dog waits for me by the door every single day.",
                            "target_lang_translations": {
                                "ru": "Мой <w>верный</w> пёс ждет меня у двери каждый божий день."
                            }
                        },
                        {
                            "text": "She has been a <w>faithful</w> friend to me for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Она была мне <w>верным</w> другом более двадцати лет."
                            }

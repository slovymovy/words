                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in agricultural contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конусообразная куча сена или снопов, сложенная на поле для сушки."

{
    "word_family": [
        "nędza",
        "nędznik",
        "nędznie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9cca91d8-0d81-3923-bedf-85fc56a2dbc6",
                    "sense_definition": "Będący w złym stanie, zniszczony, zaniedbany lub mający niską wartość.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przy skraju drogi stała <w>nędzna</w> chatka, w której nikt już nie mieszkał.",
                            "target_lang_translations": {
                                "ru": "У края дороги стояла <w>убогая</w> лачуга, в которой уже никто не жил."
                            }
                        },
                        {
                            "text": "Ten <w>nędzny</w> hotel odstraszał turystów swoim wyglądem i brakiem udogodnień.",
                            "target_lang_translations": {
                                "ru": "Этот <w>жалкий</w> отель отпугивал туристов своим видом и отсутствием удобств."
                            }
                        }
                    ],
                    "synonyms": [
                        "marny",
                        "kiepski",
                        "lichy"
                    ],
                    "antonyms": [
                        "wyborny",
                        "doskonały"
                    ],
                    "common_phrases": [
                        "nędzna chatka",
                        "nędzny wygląd",
                        "nędzne ubranie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в плохом состоянии, разрушенный, запущенный или имеющий низкую ценность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убогий",
                                "target_lang_sense_clarification": "о виде, жилище или месте; крайне бедный и неприглядный"
                            },
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "невзрачный, плохой, производящий гнетущее впечатление"
                            },
                            {
                                "target_lang_word": "ветхий",
                                "target_lang_sense_clarification": "разрушающийся от старости (о строениях, вещах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f7dcd99-05a4-360e-887e-00037fae4eca",
                    "sense_definition": "Bardzo mały pod względem ilości, niewystarczający; świadczący o biedzie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za swoją ciężką pracę otrzymywał wyjątkowo <w>nędzną</w> pensję.",
                            "target_lang_translations": {
                                "ru": "За свою тяжелую работу он получал исключительно <w>ничтожную</w> зарплату."
                            }
                        },
                        {
                            "text": "Podróżni zjedli <w>nędzny</w> posiłek, który nie zaspokoił ich głodu.",
                            "target_lang_translations": {
                                "ru": "Путники съели <w>скудный</w> обед, который не утолил их голода."
                            }
                        }
                    ],
                    "synonyms": [
                        "marny",
                        "lichy"
                    ],
                    "antonyms": [
                        "suty",
                        "dostatni"
                    ],
                    "common_phrases": [
                        "nędzna pensja",
                        "nędzny posiłek",
                        "nędzne grosze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленький по количеству, недостаточный; свидетельствующий о бедности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничтожный",
                                "target_lang_sense_clarification": "очень малый, незначительный (о сумме, количестве)"
                            },
                            {
                                "target_lang_word": "скудный",
                                "target_lang_sense_clarification": "недостаточный, бедный (о еде, запасах)"
                            },
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "ничтожно малый, смехотворный (о деньгах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "458c8ec9-0cb1-307d-af9f-cf6aff673849",
                    "sense_definition": "Wzbudzający politowanie swoim losem lub zasługujący na pogardę swoim zachowaniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_emotional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okazał się <w>nędznym</w> kłamcą, któremu nikt w rodzinie nie ufał.",
                            "target_lang_translations": {
                                "ru": "Он оказался <w>жалким</w> лжецом, которому никто в семье не доверял."
                            }
                        },
                        {
                            "text": "Jego <w>nędzny</w> los nie obchodził bogatych sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Его <w>жалкая</w> участь не волновала богатых соседей."
                            }
                        }
                    ],
                    "synonyms": [
                        "marny"
                    ],
                    "common_phrases": [
                        "nędzny tchórz",
                        "nędzny los",
                        "nędzny żywot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий жалость своей судьбой или заслуживающий презрения своим поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "вызывающий презрение (о человеке) или сострадание (о судьбе)"
                            },
                            {
                                "target_lang_word": "презренный",
                                "target_lang_sense_clarification": "заслуживающий презрения, низкий (о человеке)"
                            },
                            {
                                "target_lang_word": "несчастный",
                                "target_lang_sense_clarification": "обездоленный, вызывающий сочувствие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "dowcipny",
        "dowcipkować",
        "dowcipniś",
        "dowcipas"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4044a818-ec57-3428-a50b-362e08b9b58b",
                    "sense_definition": "Krótka historyjka o zabawnej treści, zakończona zaskakującą puentą, opowiadana w celu rozśmieszenia kogoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "humor_story",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój tata zna chyba każdy <w>dowcip</w>, jaki kiedykolwiek wymyślono.",
                            "target_lang_translations": {
                                "ru": "Мой папа знает, пожалуй, каждый <w>анекдот</w>, который когда-либо придумали."
                            }
                        },
                        {
                            "text": "Opowiedziałem w pracy nowy <w>dowcip</w>, ale nikt się nie zaśmiał.",
                            "target_lang_translations": {
                                "ru": "Я рассказал на работе новый <w>анекдот</w>, но никто не засмеялся."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawał",
                        "żart",
                        "anegdota"
                    ],
                    "common_phrases": [
                        "opowiadać dowcip",
                        "śmieszny dowcip",
                        "spalony dowcip",
                        "słaby dowcip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая забавная история с неожиданной концовкой, которую рассказывают, чтобы рассмешить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анекдот",
                                "target_lang_sense_clarification": "короткая смешная история"
                            },
                            {
                                "target_lang_word": "шутка",
                                "target_lang_sense_clarification": "общее понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b935f6b-2ccc-3d4e-b883-0a3316717989",
                    "sense_definition": "Umiejętność bystrego kojarzenia faktów i trafnego, humorystycznego komentowania rzeczywistości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "humor_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej ostry jak brzytwa <w>dowcip</w> budził podziw wśród znajomych.",
                            "target_lang_translations": {
                                "ru": "Её острый как бритва <w>юмор</w> вызывал восхищение у знакомых."
                            }
                        },
                        {
                            "text": "To nie była zwykła rozmowa, lecz popis inteligencji i <w>dowcipu</w>.",
                            "target_lang_translations": {
                                "ru": "Это был не обычный разговор, а демонстрация интеллекта и <w>остроумия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawał",
                        "żart",
                        "anegdota"
                    ],
                    "common_phrases": [
                        "mieć dowcip",
                        "cięty dowcip",
                        "poczucie humoru",
                        "sytuacyjny dowcip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность быстро соображать и метко, с юмором комментировать происходящее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остроумие",
                                "target_lang_sense_clarification": "изобретательность в шутках"
                            },
                            {
                                "target_lang_word": "юмор",
                                "target_lang_sense_clarification": "способность видеть смешное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4191a023-a453-3ff0-8b0a-f99cd245b35b",
                    "sense_definition": "Istota problemu, kluczowy element lub haczyk w jakiejś sprawie (zwykle w wyrażeniu \"w tym dowcip\").",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "crux_issue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały <w>dowcip</w> polega na tym, żeby zrobić to szybko i tanio.",
                            "target_lang_translations": {
                                "ru": "Весь <w>фокус</w> заключается в том, чтобы сделать это быстро и дёшево."
                            }
                        },
                        {
                            "text": "Rozwiązanie wydaje się proste, ale <w>dowcip</w> tkwi w szczegółach.",
                            "target_lang_translations": {
                                "ru": "Решение кажется простым, но вся <w>соль</w> кроется в деталях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w tym cały dowcip",
                        "na tym polega dowcip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суть проблемы, ключевой элемент или хитрость в каком-либо деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соль",
                                "target_lang_sense_clarification": "суть, смысл"
                            },
                            {
                                "target_lang_word": "фокус",
                                "target_lang_sense_clarification": "хитрость, уловка"
                            },
                            {
                                "target_lang_word": "изюминка",
                                "target_lang_sense_clarification": "скрытая особенность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
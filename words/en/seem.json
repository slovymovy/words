    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa3a4f9f-a7b0-31b8-8e0e-7384c0f68fc6",
                    "sense_definition": "To give the impression of being or doing something; to appear to be true or probable.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>seems</w> to be very happy with her new job.",
                            "target_lang_translations": {
                                "ru": "Она, <w>кажется</w>, очень довольна своей новой работой."
                            }
                        },
                        {
                            "text": "It <w>seems</w> like we will arrive on time after all.",
                            "target_lang_translations": {
                                "ru": "<w>Кажется</w>, мы всё-таки приедем вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "appear",
                        "look"
                    ],
                    "common_phrases": [
                        "seem to be",
                        "it seems that",
                        "seem like",
                        "seem unlikely",
                        "seem impossible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить впечатление, иметь вид; представляться верным или вероятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление"
                            },
                            {
                                "target_lang_word": "показаться",
                                "target_lang_sense_clarification": "почудиться, привидеться"
                            },
                            {
                                "target_lang_word": "сдаваться",
                                "target_lang_sense_clarification": "разг. казаться, представляться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "953e6327-75ac-30a4-8e9a-f370798acd88",
                    "sense_definition": "To be suitable, appropriate, or fitting for a particular situation or person.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>seemed</w> him right to decline the invitation gracefully.",
                            "target_lang_translations": {
                                "ru": "Ему <w>подобало</w> вежливо отклонить приглашение."
                            }
                        },
                        {
                            "text": "Such rude behavior hardly <w>seems</w> a gentleman of his standing.",
                            "target_lang_translations": {
                                "ru": "Столь грубое поведение едва ли <w>подобает</w> джентльмену его положения."
                            }
                        }
                    ],
                    "synonyms": [
                        "befit",
                        "suit"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть уместным, подходящим или подобающим для конкретной ситуации или человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобать",
                                "target_lang_sense_clarification": "соответствовать статусу или положению"
                            },
                            {
                                "target_lang_word": "приличествовать",
                                "target_lang_sense_clarification": "быть приличным, уместным"

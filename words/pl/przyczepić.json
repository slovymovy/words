    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "33afe73a-bf2c-34ff-948a-abe8148b876d",
                    "sense_definition": "Przymocować coś do czegoś, połączyć jedną rzecz z drugą, sprawić, by się trzymała.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <przyczepić> to zdjęcie do tablicy korkowej w moim pokoju.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <прикрепить> эту фотографию к пробковой доске в моей комнате."
                            }
                        },
                        {
                            "text": "Czy możesz mi pomóc <przyczepić> ten odpadnięty guzik?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне <приделать> эту оторвавшуюся пуговицу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "przymocować",
                        "przypiąć",
                        "zamocować"
                    ],
                    "antonyms": [
                        "odczepić",
                        "odpiąć",
                        "oderwać"
                    ],
                    "common_phrases": [
                        "przyczepić znaczek",
                        "przyczepić kartkę",
                        "przyczepić ogłoszenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикрепить что-то к чему-то, соединить одну вещь с другой, зафиксировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикрепить",
                                "target_lang_sense_clarification": "общее значение: зафиксировать что-то на чем-то"
                            },
                            {
                                "target_lang_word": "прицепить",
                                "target_lang_sense_clarification": "соединить с помощью крючка, зажима или сцепки"
                            },
                            {
                                "target_lang_word": "приделать",
                                "target_lang_sense_clarification": "разговорное: присоединить, приладить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35ad0dbf-4243-38b1-9054-b40672b3aa87",
                    "sense_definition": "Mocno uchwycić się czegoś lub przylgnąć do powierzchni (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do twojego wełnianego swetra <przyczepił> się suchy liść.",
                            "target_lang_translations": {
                                "ru": "К твоему шерстяному свитеру <прицепился> сухой лист."
                            }
                        },
                        {
                            "text": "Mokra koszula nieprzyjemnie <przyczepiła> się do moich pleców.",
                            "target_lang_translations": {
                                "ru": "Мокрая рубашка неприятно <прилипла> к моей спине."
                            }
                        }
                    ],
                    "synonyms": [
                        "przylgnąć",
                        "przykleić się",
                        "uczepić się"
                    ],
                    "antonyms": [
                        "odczepić się",
                        "puścić"
                    ],
                    "common_phrases": [
                        "przyczepić się do ubrania",
                        "rzep się przyczepił"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепко ухватиться за что-то или прилипнуть к поверхности (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прицепиться",
                                "target_lang_sense_clarification": "зацепиться за что-то (крючком, когтями и т.д.)"
                            },
                            {
                                "target_lang_word": "прилипнуть",
                                "target_lang_sense_clarification": "плотно прижаться (часто о мокром или клейком)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50f99ecb-27dd-3fa3-8b83-4bf76c73da37",
                    "sense_definition": "Zacząć kogoś krytykować, mieć pretensje lub znaleźć błąd, często bez ważnego powodu (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <przyczepił> się do mojego charakteru pisma, choć praca była bezbłędna.",
                            "target_lang_translations": {
                                "ru": "Учитель <придрался> к моему почерку, хотя работа была безошибочной."
                            }
                        },
                        {
                            "text": "Zrobiliśmy wszystko idealnie, więc nikt się do nas nie <przyczepi>.",
                            "target_lang_translations": {
                                "ru": "Мы сделали всё идеально, так что никто к нам не <придерётся>."
                            }
                        }
                    ],
                    "synonyms": [
                        "czepiać się",
                        "skrytykować"
                    ],
                    "antonyms": [
                        "odpuścić",
                        "dać spokój"
                    ],
                    "common_phrases": [
                        "przyczepić się o byle co",
                        "przyczepić się bez powodu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech to describe annoying criticism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать критиковать кого-то, предъявлять претензии или искать ошибки, часто без веской причины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придраться",
                                "target_lang_sense_clarification": "найти повод для критики или обвинения"
                            },
                            {
                                "target_lang_word": "прицепиться",
                                "target_lang_sense_clarification": "разговорное: начать донимать претензиями"
                            },
                            {
                                "target_lang_word": "докопаться",
                                "target_lang_sense_clarification": "слэнг: настойчиво искать недостатки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "359fafac-4878-39a6-b991-23d4d834c7df",
                    "sense_definition": "Zacząć komuś towarzyszyć lub iść za kimś, mimo że ta osoba tego nie chce (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakiś nieznajomy mężczyzna <przyczepił> się do nas w parku i ciągle zagadywał.",
                            "target_lang_translations": {
                                "ru": "Какой-то незнакомый мужчина <увязался> за нами в парке и постоянно заговаривал."
                            }
                        },
                        {
                            "text": "Mały kotek <przyczepił> się do mnie na ulicy i szedł za mną aż do domu.",
                            "target_lang_translations": {
                                "ru": "Маленький котёнок <увязался> за мной на улице и шёл за мной до самого дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzucać się",
                        "przykleić się"
                    ],
                    "antonyms": [
                        "odczepić się"
                    ],
                    "common_phrases": [
                        "przyczepić się jak rzep",
                        "przyczepić się do kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать сопровождать кого-то или идти за кем-то, навязывая свое общество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увязаться",
                                "target_lang_sense_clarification": "пойти следом за кем-то (часто без приглашения)"
                            },
                            {
                                "target_lang_word": "пристать",
                                "target_lang_sense_clarification": "навязчиво присоединиться"
                            },
                            {
                                "target_lang_word": "прицепиться",
                                "target_lang_sense_clarification": "разговорное: не отставать от кого-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "travel",
        "traveller",
        "travelogue"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "540b38c4-b4a6-311a-8022-de7c0a227e1c",
                    "sense_definition": "Present participle of <w>travel</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are <w>travelling</w> to Spain for our holidays.",
                            "target_lang_translations": {
                                "ru": "Мы <w>едем</w> в Испанию в отпуск."
                            }
                        },
                        {
                            "text": "She is currently <w>travelling</w> around the world.",
                            "target_lang_translations": {
                                "ru": "Она сейчас <w>путешествует</w> по миру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола travel (путешествовать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествовать",
                                "target_lang_sense_clarification": "совершать путешествие"
                            },
                            {
                                "target_lang_word": "ехать",
                                "target_lang_sense_clarification": "направляться куда-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee0e86e9-c020-3aeb-8d3d-090718c1ba77",
                    "sense_definition": "The activity of going from one place to another, especially over long distances.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Travelling</w> is a great way to learn about other cultures.",
                            "target_lang_translations": {
                                "ru": "<w>Путешествия</w> — отличный способ узнать о других культурах."
                            }
                        },
                        {
                            "text": "He spends a lot of his time and money on <w>travelling</w>.",
                            "target_lang_translations": {
                                "ru": "Он тратит много времени и денег на <w>путешествия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "travel",
                        "touring",
                        "journeying"
                    ],
                    "common_phrases": [
                        "love travelling",
                        "go travelling",
                        "safe travelling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность, связанная с перемещением из одного места в другое, особенно на большие расстояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествие",
                                "target_lang_sense_clarification": "процесс или мероприятие"
                            },
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "перемещение, обычно более короткое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "726fe0df-ae84-3b96-85d8-1f15bcd9f7c8",
                    "sense_definition": "Relating to, used for, or accompanying someone on a journey.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was a very pleasant <w>travelling</w> companion.",
                            "target_lang_translations": {
                                "ru": "Она была очень приятной спутницей в <w>путешествии</w>."
                            }
                        },
                        {
                            "text": "He packed all his essentials into a small <w>travelling</w> bag.",
                            "target_lang_translations": {
                                "ru": "Он упаковал все самое необходимое в маленькую <w>дорожную</w> сумку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "travelling companion",
                        "travelling bag",
                        "travelling expenses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к путешествию, используемый в дороге или сопровождающий кого-либо в пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорожный",
                                "target_lang_sense_clarification": "предназначенный для дороги (о вещах)"
                            },
                            {
                                "target_lang_word": "походный",
                                "target_lang_sense_clarification": "для использования в походах и поездках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3145ca4c-c987-38cb-acd9-96022b6ef3ef",
                    "sense_definition": "Moving from place to place, typically for work or entertainment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>travelling</w> circus arrives in town tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "<w>Бродячий</w> цирк прибывает в город завтра утром."
                            }
                        },
                        {
                            "text": "My uncle worked as a <w>travelling</w> salesman for thirty years.",
                            "target_lang_translations": {
                                "ru": "Мой дядя тридцать лет работал <w>разъездным</w> торговцем."
                            }
                        }
                    ],
                    "synonyms": [
                        "itinerant",
                        "touring",
                        "roving",
                        "mobile"
                    ],
                    "antonyms": [
                        "stationary",
                        "sedentary"
                    ],
                    "common_phrases": [
                        "travelling salesman",
                        "travelling circus",
                        "travelling exhibition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переезжающий с места на место, обычно по работе или для выступлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродячий",
                                "target_lang_sense_clarification": "о цирке, труппе"
                            },
                            {
                                "target_lang_word": "передвижной",
                                "target_lang_sense_clarification": "о выставках, библиотеках"
                            },
                            {
                                "target_lang_word": "разъездной",
                                "target_lang_sense_clarification": "о характере работы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3be3150-1884-3360-8f22-eb9020d4ea9f",
                    "sense_definition": "Een rommelige toestand of chaos.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat een <w>zooi</w> is het hier in de woonkamer.",
                            "target_lang_translations": {
                                "ru": "Какой здесь в гостиной <w>бардак</w>.",
                                "en": "What a <w>mess</w> it is here in the living room."
                            }
                        },
                        {
                            "text": "Ruim je die <w>zooi</w> even op voordat je weggaat?",
                            "target_lang_translations": {
                                "ru": "Ты не уберешь этот <w>бардак</w>, прежде чем уйти?",
                                "en": "Will you clean up that <w>mess</w> before you leave?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zootje"
                    ],
                    "common_phrases": [
                        "wat een zooi",
                        "er een zooi van maken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспорядок, хаос или очень грязное состояние помещения.",
                        "en": "A messy state, disorder, or chaos."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бардак",
                                "target_lang_sense_clarification": "разговорное, полный беспорядок"
                            },
                            {
                                "target_lang_word": "беспорядок",
                                "target_lang_sense_clarification": "отсутствие порядка"
                            },
                            {
                                "target_lang_word": "свинарник",
                                "target_lang_sense_clarification": "разговорное, очень грязное место"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "mess",
                                "target_lang_sense_clarification": "a dirty or untidy state"
                            },
                            {
                                "target_lang_word": "shambles",
                                "target_lang_sense_clarification": "a state of total disorder"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62a1aca6-cdb6-352d-87f9-bb3ff4058153",
                    "sense_definition": "Een grote hoeveelheid spullen, vaak rommel of dingen van weinig waarde.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stuff",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb veel te veel <w>zooi</w> in mijn auto liggen.",

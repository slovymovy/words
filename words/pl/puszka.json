{
    "word_family": [
        "puszkowy",
        "puszkarz",
        "puszkować",
        "puszeczka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b12307d-61c2-311f-a660-f5f2bc037019",
                    "sense_definition": "Metalowy, szczelnie zamykany pojemnik służący do przechowywania żywności, napojów, farb lub innych substancji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eugenia kupiła dwie <puszki> karmy dla kotów.",
                            "target_lang_translations": {
                                "ru": "Евгения купила две <банки> кошачьего корма."
                            }
                        },
                        {
                            "text": "Po wypiciu napoju zgnieć <puszkę> i wyrzuć ją do odpowiedniego pojemnika.",
                            "target_lang_translations": {
                                "ru": "Выпив напиток, сомните <банку> и выбросьте её в соответствующий контейнер."
                            }
                        }
                    ],
                    "synonyms": [
                        "blaszanka",
                        "konserwa (puszka z żywnością)",
                        "reg. śl. biksa"
                    ],
                    "common_phrases": [
                        "puszka coli",
                        "puszka farby",
                        "puszka tuńczyka",
                        "puszka Pandory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический, герметично закрываемый контейнер для хранения продуктов, напитков, краски или других веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банка",
                                "target_lang_sense_clarification": "контейнер"
                            },
                            {
                                "target_lang_word": "жестянка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25997fed-b057-3efa-9792-e90641033435",
                    "sense_definition": "Potoczne określenie piwa sprzedawanego w metalowym pojemniku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "beer_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamo, kup mi <puszkę> bezalkoholową w sklepie.",
                            "target_lang_translations": {
                                "ru": "Мама, купи мне безалкогольную <банку> в магазине."
                            }
                        },
                        {
                            "text": "Wypiliśmy po jednej zimnej <puszce> oglądając mecz w telewizji.",
                            "target_lang_translations": {
                                "ru": "Мы выпили по одной холодной <банке>, смотря матч по телевизору."
                            }
                        }
                    ],
                    "synonyms": [
                        "browar",
                        "piana"
                    ],
                    "common_phrases": [
                        "zimna puszka",
                        "otworzyć puszkę",
                        "wypić puszkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название пива, продаваемого в металлической таре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банка",
                                "target_lang_sense_clarification": "пивная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dfa72bc-4cbc-399c-a0de-76fc88676e83",
                    "sense_definition": "Forma gramatyczna (dopełniacz liczby pojedynczej) rzeczownika <w>puszek</w> oznaczającego miękkie pióra lub włosy.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "form_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kurtka jest wypełniona naturalnym <puszkiem> gęsim, więc jest bardzo ciepła.",
                            "target_lang_translations": {
                                "ru": "Куртка наполнена натуральным гусиным <пухом>, поэтому она очень теплая."
                            }
                        },
                        {
                            "text": "Dotknęła delikatnego <puszku> na głowie małego dziecka.",
                            "target_lang_translations": {
                                "ru": "Она коснулась нежного <пушка> на голове маленького ребенка."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby pojedynczej słowa 'puszek'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный падеж единственного числа) существительного «puszek», означающего мягкие перья или волосы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пушок",
                                "target_lang_sense_clarification": "пух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "469840b9-5784-38f9-ae07-55f00eff4795",
                    "sense_definition": "Potoczne określenie zakładu karnego lub aresztu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "prison_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej trafił do <puszki> na trzy lata za kradzież samochodu.",
                            "target_lang_translations": {
                                "ru": "Вор попал в <тюрягу> на три года за кражу автомобиля."
                            }
                        },
                        {
                            "text": "Nie chcę znowu wylądować w <puszce> za swoje błędy młodości.",
                            "target_lang_translations": {
                                "ru": "Я не хочу снова оказаться в <кутузке> за ошибки молодости."
                            }
                        }
                    ],
                    "synonyms": [
                        "więzienie",
                        "ciupa",
                        "kić"
                    ],
                    "common_phrases": [
                        "trafić do puszki",
                        "siedzieć w puszce"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название исправительного учреждения или следственного изолятора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюряга",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "кутузка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "обезьянник",
                                "target_lang_sense_clarification": "камера предварительного заключения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b2cc737-1ce4-36e4-b68a-7650a5f61029",
                    "sense_definition": "Korpus aparatu fotograficznego, do którego mocuje się wymienny obiektyw.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "camera_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nową <puszkę>, ale nadal używam moich starych obiektywów.",
                            "target_lang_translations": {
                                "ru": "Я купил новую <тушку>, но по-прежнему использую свои старые объективы."
                            }
                        },
                        {
                            "text": "Ta profesjonalna <puszka> jest uszczelniona i odporna na deszcz.",
                            "target_lang_translations": {
                                "ru": "Эта профессиональная <тушка> герметична и устойчива к дождю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pełnoklatkowa puszka",
                        "puszka i obiektyw"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "fotograficzny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Корпус фотоаппарата, к которому крепится сменный объектив."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тушка",
                                "target_lang_sense_clarification": "фотосленг"
                            },
                            {
                                "target_lang_word": "боди",
                                "target_lang_sense_clarification": "профессиональный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ca5ac4f-979b-3a4b-a137-4eb0f7fd4258",
                    "sense_definition": "Naczynie liturgiczne w kościele katolickim służące do przechowywania konsekrowanych hostii.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "liturgical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ksiądz wyjął z tabernakulum złotą <puszkę> z komunikantami.",
                            "target_lang_translations": {
                                "ru": "Священник достал из дарохранительницы золотой <циборий> с гостиями."
                            }
                        },
                        {
                            "text": "Po komunii świętej pusta <puszka> została schowana do zakrystii.",
                            "target_lang_translations": {
                                "ru": "После причастия пустая <чаша> была убрана в ризницу."
                            }
                        }
                    ],
                    "synonyms": [
                        "cyborium"
                    ],
                    "common_phrases": [
                        "złota puszka",
                        "puszka z hostiami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Литургический сосуд в католической церкви, служащий для хранения освященных гостий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "циборий",
                                "target_lang_sense_clarification": "церковное"
                            },
                            {
                                "target_lang_word": "дароносица",
                                "target_lang_sense_clarification": "переносной сосуд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "488e13e6-e587-3545-ae08-cd53131d62e8",
                    "sense_definition": "Część sporofitu mszaków (zarodnia), w której powstają zarodniki.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na szczycie cienkiej łodyżki mchu znajduje się <puszka> z zarodnikami.",
                            "target_lang_translations": {
                                "ru": "На верхушке тонкого стебелька мха находится <коробочка> со спорами."
                            }
                        },
                        {
                            "text": "Dojrzała <puszka> pęka, uwalniając zarodniki na wiatr.",
                            "target_lang_translations": {
                                "ru": "Зрелая <коробочка> лопается, высвобождая споры на ветер."
                            }
                        }
                    ],
                    "common_phrases": [
                        "puszka mchu",
                        "zarodniki w puszce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть спорофита мхов (спорангий), в которой образуются споры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коробочка",
                                "target_lang_sense_clarification": "ботаника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4ebc286-0092-317f-8423-c26a668a5da5",
                    "sense_definition": "Dawne określenie działa, armaty lub innej broni palnej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weapon_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Huk wystrzałów ze starej <puszki> przestraszył konie rycerzy.",
                            "target_lang_translations": {
                                "ru": "Грохот выстрелов из старой <пушки> напугал рыцарских коней."
                            }
                        },
                        {
                            "text": "Na murach obronnych zamku ustawiono ciężkie <puszki>.",
                            "target_lang_translations": {
                                "ru": "На крепостных стенах установили тяжелые <пушки>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название орудия, пушки или другого огнестрельного оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пушка",
                                "target_lang_sense_clarification": "орудие"
                            },
                            {
                                "target_lang_word": "пищаль",
                                "target_lang_sense_clarification": "старинное оружие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "379158d3-a43d-3a66-8027-4f7fb61ad4ba",
                    "sense_definition": "Een verharde of onverharde strook grond die is aangelegd voor verkeer.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "road_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag alleen op de <w>weg</w> rijden en niet op de stoep.",
                            "target_lang_translations": {
                                "en": "You are only allowed to drive on the <w>road</w> and not on the sidewalk.",
                                "ru": "Ездить можно только по <w>дороге</w>, а не по тротуару."
                            }
                        },
                        {
                            "text": "Door de sneeuw was de <w>weg</w> erg glad geworden.",
                            "target_lang_translations": {
                                "en": "The <w>road</w> had become very slippery due to the snow.",
                                "ru": "Из-за снега <w>дорога</w> стала очень скользкой."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2eaf3830-c74b-384f-8cfd-a6e8f2cc7b96",
                    "sense_definition": "Группа сельскохозяйственных животных (обычно коров, овец или коз), которые пасутся вместе",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_groups",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пастух рано утром погнал <стадо> на зеленый луг.",
                            "target_lang_translations": {
                                "en": "Early in the morning, the shepherd drove the <herd> to the green meadow."
                            }
                        },
                        {
                            "text": "В деревне каждый вечер встречают <стадо> коров, возвращающееся домой.",
                            "target_lang_translations": {
                                "en": "In the village, every evening they meet the <herd> of cows returning home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пасти стадо",
                        "стадо коров",
                        "выгонять стадо",
                        "большое стадо"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of farm animals (usually cows, sheep, or goats) that graze together"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "herd",
                                "target_lang_sense_clarification": "group of farm animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ebb863f-5fe8-3867-9d5b-9d04c5e48c14",
                    "sense_definition": "Группа диких млекопитающих, птиц или рыб одного вида, держащихся и живущих вместе",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_groups",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы с интересом наблюдали, как <стадо> слонов переходит реку.",
                            "target_lang_translations": {
                                "en": "The tourists watched with interest as a <herd> of elephants crossed the river."
                            }
                        },
                        {
                            "text": "В заповеднике обитает огромное <стадо> диких оленей.",
                            "target_lang_translations": {
                                "en": "A huge <herd> of wild deer lives in the nature reserve."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стадо слонов",
                        "стадо оленей",
                        "стадо китов"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of wild mammals, birds, or fish of the same species living together"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "herd",
                                "target_lang_sense_clarification": "group of wild animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "614b1b7e-87b4-3c53-991b-bfa934d08b51",
                    "sense_definition": "Большая неорганизованная группа людей, слепо подчиняющаяся чужой воле или инстинктам",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не нужно вести себя как <стадо>, имейте собственное мнение.",
                            "target_lang_translations": {
                                "en": "You shouldn't behave like a <herd>; have your own opinion."
                            }
                        },
                        {
                            "text": "Пропаганда пытается превратить народ в послушное <стадо>.",
                            "target_lang_translations": {
                                "en": "Propaganda tries to turn the people into an obedient <herd>."
                            }
                        }
                    ],
                    "synonyms": [
                        "толпа"
                    ],
                    "antonyms": [
                        "личность",
                        "индивидуальность"
                    ],
                    "common_phrases": [
                        "как стадо",
                        "послушное стадо",
                        "людское стадо"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "пренебрежительное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A large unorganized group of people who blindly obey someone else's will or instincts"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "herd",
                                "target_lang_sense_clarification": "people following blindly"
                            },
                            {
                                "target_lang_word": "mob",
                                "target_lang_sense_clarification": "disorderly group of people"
                            },
                            {
                                "target_lang_word": "crowd",
                                "target_lang_sense_clarification": "large group of people"
                            }
                        ]
                    }
                }
            ]

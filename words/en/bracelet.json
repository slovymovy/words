{
    "word_family": [
        "brace",
        "embrace"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cea156e-3ca4-3132-9d02-aa0d346f1933",
                    "sense_definition": "A band or chain worn around the wrist as jewelry or an ornament.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "jewelry_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a beautiful gold <w>bracelet</w> on her left wrist.",
                            "target_lang_translations": {
                                "ru": "Она носила красивый золотой <w>браслет</w> на левом запястье."
                            }
                        },
                        {
                            "text": "He gave her a silver <w>bracelet</w> for her birthday.",
                            "target_lang_translations": {
                                "ru": "Он подарил ей серебряный <w>браслет</w> на день рождения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gold bracelet",
                        "silver bracelet",
                        "charm bracelet",
                        "wear a bracelet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшение в виде обруча или цепочки, которое носят на запястье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браслет",
                                "target_lang_sense_clarification": "украшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a9c59fd-5ca8-3e43-b30d-9faaa94cf2d2",
                    "sense_definition": "The strap of a wristwatch, used to secure it around the wrist.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "jewelry_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The metal <w>bracelet</w> of his watch was too loose.",
                            "target_lang_translations": {
                                "ru": "Металлический <w>браслет</w> его часов был слишком свободным."
                            }
                        },
                        {
                            "text": "I need to replace the broken <w>bracelet</w> on my wristwatch.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заменить сломанный <w>браслет</w> на моих наручных часах."
                            }
                        }
                    ],
                    "synonyms": [
                        "watchband",
                        "watchstrap"
                    ],
                    "common_phrases": [
                        "watch bracelet",
                        "metal bracelet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремешок или металлическая лента для крепления наручных часов на руке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браслет",
                                "target_lang_sense_clarification": "металлический, для часов"
                            },
                            {
                                "target_lang_word": "ремешок",
                                "target_lang_sense_clarification": "кожаный или тканевый, для часов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc09a29b-af3e-3273-b071-3c0a86f1b3d4",
                    "sense_definition": "A piece of defensive armour for the arm.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight's armor included a steel <w>bracelet</w> to protect his forearm.",
                            "target_lang_translations": {
                                "ru": "Доспехи рыцаря включали стальной <w>наруч</w> для защиты предплечья."
                            }
                        },
                        {
                            "text": "Museum visitors admired the ancient bronze <w>bracelet</w> used in battle.",
                            "target_lang_translations": {
                                "ru": "Посетители музея восхищались древним бронзовым <w>наручем</w>, использовавшимся в битвах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть защитных доспехов, закрывающая руку (предплечье)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наруч",
                                "target_lang_sense_clarification": "часть доспехов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "14b99b04-e9b3-3d02-86d0-217fc96e8328",
                    "sense_definition": "To surround with, or as if with, a bracelet; to ring or encompass.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tall trees <w>braceleted</w> the hidden clearing.",
                            "target_lang_translations": {
                                "ru": "Высокие деревья <w>опоясывали</w> скрытую поляну."
                            }
                        },
                        {
                            "text": "A ring of bright flowers <w>braceleted</w> the vase.",
                            "target_lang_translations": {
                                "ru": "Кольцо ярких цветов <w>обвивало</w> вазу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружать кольцом, словно браслетом; охватывать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опоясывать",
                                "target_lang_sense_clarification": "окружать кольцом"
                            },
                            {
                                "target_lang_word": "обвивать",
                                "target_lang_sense_clarification": "облегать вокруг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7da6b13c-1f9b-398a-8c2b-a8d2adc33d1e",
                    "sense_definition": "Able to be moved or set in motion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist designed the sculpture with <w>motionable</w> parts that spin in the wind.",
                            "target_lang_translations": {
                                "ru": "Художник создал скульптуру с <w>подвижными</w> частями, которые вращаются на ветру."
                            }
                        },
                        {
                            "text": "In the frozen winter landscape, nothing seemed <w>motionable</w> or alive.",
                            "target_lang_translations": {
                                "ru": "В замерзшем зимнем пейзаже ничто не казалось <w>подвижным</w> или живым."
                            }
                        }
                    ],
                    "synonyms": [
                        "movable",
                        "mobile"
                    ],
                    "antonyms": [
                        "immovable",
                        "fixed",
                        "stationary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный двигаться или быть приведенным в движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвижный",
                                "target_lang_sense_clarification": "способный перемещаться или изменять положение"
                            },
                            {
                                "target_lang_word": "движущийся",
                                "target_lang_sense_clarification": "находящийся в движении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9552eb15-0e8e-3619-b378-88955e74e067",
                    "sense_definition": "Suitable or valid to be proposed as a formal motion in a meeting or legal context.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chairperson ruled that the topic was not <w>motionable</w> under the current agenda.",
                            "target_lang_translations": {
                                "ru": "Председатель постановил, что эта тема не является <w>допустимой к обсуждению</w> в рамках текущей повестки дня."
                            }
                        },
                        {
                            "text": "We need to determine if this amendment is <w>motionable</w> before we proceed to a vote.",
                            "target_lang_translations": {
                                "ru": "Нам нужно определить, является ли эта поправка <w>допустимой к внесению</w>, прежде чем переходить к голосованию."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий или допустимый для внесения в качестве официального предложения на собрании или в юридическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "допустимый к внесению",
                                "target_lang_sense_clarification": "о предложении или поправке"
                            },
                            {
                                "target_lang_word": "процессуально допустимый",
                                "target_lang_sense_clarification": "соответствующий правилам процедуры"
                            }
                        ]
                    }
                }
            ]
        }
    ]

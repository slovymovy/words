    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aa66e880-be21-3d5a-aba9-bdf40670e86c",
                    "sense_definition": "Metaliczny pierwiastek chemiczny o symbolu Fe, niezbędny do życia i szeroko wykorzystywany w przemyśle.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szpinak jest warzywem bogatym w <w>żelazo</w>.",
                            "target_lang_translations": {
                                "ru": "Шпинат — это овощ, богатый <w>железом</w>."
                            }
                        },
                        {
                            "text": "Niedobór <w>żelaza</w> może prowadzić do anemii i osłabienia organizmu.",
                            "target_lang_translations": {
                                "ru": "Дефицит <w>железа</w> может привести к анемии и ослаблению организма."
                            }
                        }
                    ],
                    "synonyms": [
                        "Fe"
                    ],
                    "common_phrases": [
                        "niedobór żelaza",
                        "bogaty w żelazo",
                        "ruda żelaza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический элемент с символом Fe, необходимый для жизни и широко используемый в промышленности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железо",
                                "target_lang_sense_clarification": "химический элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b04de500-b113-3852-8026-2761258ba9c8",
                    "sense_definition": "Pospolity, twardy metal o srebrzystoszarej barwie, używany do wyrobu narzędzi i konstrukcji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kowal rozgrzał <w>żelazo</w> do czerwoności, aby nadać mu kształt.",
                            "target_lang_translations": {
                                "ru": "Кузнец раскалил <w>железо</w> докрасна, чтобы придать ему форму."
                            }
                        },
                        {
                            "text": "Brama wjazdowa została wykonana z solidnego <w>żelaza</w>.",
                            "target_lang_translations": {
                                "ru": "Въездные ворота были сделаны из прочного <w>железа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "metal",
                        "stal",
                        "żeliwo",
                        "staliwo",
                        "gw. (Poznań) ajzol",
                        "gw. (Górny Śląsk) zelazło, zielazło, zielazo, żelazło"
                    ],
                    "common_phrases": [
                        "kuć żelazo",
                        "z żelaza",
                        "stare żelazo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенный твердый металл серебристо-серого цвета, используемый для изготовления инструментов и конструкций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железо",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "943923f7-96a5-3f3c-9237-fb09ae2697fe",
                    "sense_definition": "Przedmiot wykonany z tego metalu, często stary lub niepotrzebny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na podwórku leżało zardzewiałe <w>żelazo</w>, które trzeba wywieźć na złom.",
                            "target_lang_translations": {
                                "ru": "Во дворе лежало ржавое <w>железо</w>, которое нужно вывезти на металлолом."
                            }
                        },
                        {
                            "text": "Handlarz skupował stare <w>żelaza</w> od okolicznych mieszkańców.",
                            "target_lang_translations": {
                                "ru": "Торговец скупал старое <w>железо</w> у местных жителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "żelastwo"
                    ],
                    "common_phrases": [
                        "kupa żelaza",
                        "zardzewiałe żelazo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, изготовленный из этого металла, часто старый или ненужный; металлолом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железяка",
                                "target_lang_sense_clarification": "предмет"
                            },
                            {
                                "target_lang_word": "железо",
                                "target_lang_sense_clarification": "лом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57d56288-a494-348d-9fec-94f21257e76f",
                    "sense_definition": "Ciężary i sprzęt używany w kulturystyce oraz dyscyplinach siłowych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdego dnia spędzam godzinę na siłowni, przerzucając <w>żelazo</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый день я провожу час в спортзале, тягая <w>железо</w>."
                            }
                        },
                        {
                            "text": "Dźwiganie <w>żelaza</w> pomaga zbudować masę mięśniową.",
                            "target_lang_translations": {
                                "ru": "Поднятие <w>железа</w> помогает нарастить мышечную массу."
                            }
                        }

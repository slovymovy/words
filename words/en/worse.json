{
    "word_family": [
        "worsen",
        "worst"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "84c4c89a-9b17-3bff-8a00-af1730f753ec",
                    "sense_definition": "Comparative form of <w>bad</w>: of lower quality, less pleasant, or less desirable.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather is <w>worse</w> today than it was yesterday.",
                            "target_lang_translations": {
                                "ru": "Погода сегодня <w>хуже</w>, чем была вчера."
                            }
                        },
                        {
                            "text": "Your exam results are <w>worse</w> than before.",
                            "target_lang_translations": {
                                "ru": "Твои результаты экзамена <w>хуже</w>, чем раньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "poorer",
                        "inferior"
                    ],
                    "antonyms": [
                        "better",
                        "superior"
                    ],
                    "common_phrases": [
                        "worse than",
                        "get worse",
                        "make matters worse",
                        "from bad to worse",
                        "worse off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень прилагательного \"bad\" (плохой): более низкого качества, менее приятный или менее желательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хуже",
                                "target_lang_sense_clarification": "сравнительная степень"
                            },
                            {
                                "target_lang_word": "более плохой",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "24873fa9-91f3-357e-9345-70bd89e9df60",
                    "sense_definition": "Comparative form of <w>badly</w>: in a less skillful, effective, or successful way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The harder you try, the <w>worse</w> you do.",
                            "target_lang_translations": {
                                "ru": "Чем больше ты стараешься, тем <w>хуже</w> у тебя получается."
                            }
                        },
                        {
                            "text": "He drives <w>worse</w> than anyone else I know.",
                            "target_lang_translations": {
                                "ru": "Он водит машину <w>хуже</w> всех, кого я знаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "more poorly"
                    ],
                    "antonyms": [
                        "better"
                    ],
                    "common_phrases": [
                        "do worse",
                        "fare worse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень наречия \"badly\" (плохо): менее умело, эффективно или успешно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хуже",
                                "target_lang_sense_clarification": "сравнительная степень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94aa518f-d6a7-3a58-b878-d4192e1dcf77",
                    "sense_definition": "More severely or seriously.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bad news affected me <w>worse</w> than it did my brother.",
                            "target_lang_translations": {
                                "ru": "Плохие новости повлияли на меня <w>сильнее</w>, чем на моего брата."
                            }
                        },
                        {
                            "text": "My leg hurts <w>worse</w> when I try to walk on it.",
                            "target_lang_translations": {
                                "ru": "Моя нога болит <w>сильнее</w>, когда я пытаюсь наступать на нее."
                            }
                        }
                    ],
                    "synonyms": [
                        "more severely",
                        "more gravely"
                    ],
                    "antonyms": [
                        "less severely"
                    ],
                    "target_lang_definitions": {
                        "ru": "Более сурово, сильно или серьезно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сильнее",
                                "target_lang_sense_clarification": "о боли или воздействии"
                            },
                            {
                                "target_lang_word": "хуже",
                                "target_lang_sense_clarification": "об общем состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d54bb753-6dec-328e-ba1c-cb069fe183e9",
                    "sense_definition": "Used to introduce a statement describing something even more unpleasant or serious.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her leg is infected, and still <w>worse</w>, she is developing a fever.",
                            "target_lang_translations": {
                                "ru": "У нее инфекция ноги, и, что еще <w>хуже</w>, у нее начинается жар."
                            }
                        },
                        {
                            "text": "We missed the bus, and even <w>worse</w>, it started to rain heavily.",
                            "target_lang_translations": {
                                "ru": "Мы опоздали на автобус, и, что еще <w>хуже</w>, начался сильный дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "still worse",
                        "even worse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения утверждения, описывающего что-то еще более неприятное или серьезное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хуже",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "хуже того",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7f201565-d333-32fa-8d2a-5a4d5debecc8",
                    "sense_definition": "A thing, event, or state that is less good or more unpleasant.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient took a sudden turn for the <w>worse</w> overnight.",
                            "target_lang_translations": {
                                "ru": "Состояние пациента внезапно изменилось к <w>худшему</w> за ночь."
                            }
                        },
                        {
                            "text": "Do not think the <w>worse</w> of him for his ambitious enterprise.",
                            "target_lang_translations": {
                                "ru": "Не думайте о нем <w>хуже</w> из-за его амбициозного предприятия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for the worse",
                        "turn for the worse",
                        "think the worse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, событие или состояние, которое является менее хорошим или более неприятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "худшее",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8d48b7e-5ebf-3d3f-978e-eec25e18523b",
                    "sense_definition": "A state of defeat or disadvantage.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defending army was put to the <w>worse</w> during the fierce battle.",
                            "target_lang_translations": {
                                "ru": "Обороняющаяся армия потерпела <w>поражение</w> в ходе ожесточенной битвы."
                            }
                        },
                        {
                            "text": "Judah was put to the <w>worse</w> before Israel.",
                            "target_lang_translations": {
                                "ru": "Иуда потерпел <w>поражение</w> перед Израилем."
                            }
                        }
                    ],
                    "synonyms": [
                        "defeat",
                        "disadvantage"
                    ],
                    "common_phrases": [
                        "put to the worse"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние поражения или невыгодного положения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поражение",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "худшее положение",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8097348c-1a50-3554-89ec-ebc73428ebae",
                    "sense_definition": "To make something worse or put at a disadvantage.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weapons more violent may serve to better us and <w>worse</w> our foes.",
                            "target_lang_translations": {
                                "ru": "Более жестокое оружие может послужить тому, чтобы улучшить наше положение и <w>ухудшить</w> положение наших врагов."
                            }
                        },
                        {
                            "text": "The sudden storm served to <w>worse</w> our situation significantly.",
                            "target_lang_translations": {
                                "ru": "Внезапный шторм послужил тому, чтобы значительно <w>ухудшить</w> нашу ситуацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "worsen",
                        "aggravate"
                    ],
                    "antonyms": [
                        "better",
                        "improve"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically uses 'worsen'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то хуже или ставить в невыгодное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухудшать",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
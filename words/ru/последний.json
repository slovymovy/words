            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2837672b-8d66-3781-b6e1-084c5d03a159",
                    "sense_definition": "Расположенный в конце ряда, очереди или списка; такой, за которым больше никого или ничего нет.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sequence_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы едва успели запрыгнуть в <w>последний</w> вагон уходящего поезда.",
                            "target_lang_translations": {
                                "en": "We barely managed to jump into the <w>last</w> car of the departing train."
                            }
                        },
                        {
                            "text": "Это был <w>последний</w> дом на нашей улице, дальше начинался лес.",
                            "target_lang_translations": {
                                "en": "It was the <w>last</w> house on our street; the forest began beyond it."
                            }
                        }
                    ],
                    "synonyms": [
                        "конечный"
                    ],
                    "antonyms": [
                        "первый",
                        "начальный"
                    ],
                    "common_phrases": [
                        "последний этаж",
                        "последний день",
                        "последний в очереди",
                        "последний путь"
                    ],
                    "target_lang_definitions": {
                        "en": "Located at the end of a row, queue, or list; followed by no one or nothing else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "coming after all others"
                            },
                            {
                                "target_lang_word": "final",
                                "target_lang_sense_clarification": "occurring at the end"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65b6c7bc-9ca4-37e8-b372-ec0784f99e54",
                    "sense_definition": "Самый новый, только что появившийся или произошедший недавно.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_recency_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты уже слышал <w>последние</w> новости о событиях в мире?",

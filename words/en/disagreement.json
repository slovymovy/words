    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9432c4c-0873-3952-9117-89dfe6b37a9d",
                    "sense_definition": "An argument or debate between people with different views.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conflict_instance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They had a bit of a <w>disagreement</w> about what color to paint the bedroom.",
                            "target_lang_translations": {
                                "ru": "У них вышло небольшое <w>разногласие</w> насчет того, в какой цвет покрасить спальню."
                            }
                        },
                        {
                            "text": "We need to settle this <w>disagreement</w> calmly before we can move forward with the project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно спокойно уладить это <w>разногласие</w>, прежде чем мы сможем продолжить работу над проектом."
                            }
                        }
                    ],
                    "synonyms": [
                        "argument",
                        "dispute",
                        "quarrel",
                        "conflict"
                    ],
                    "antonyms": [
                        "agreement",
                        "accord"
                    ],
                    "common_phrases": [
                        "have a disagreement",
                        "settle a disagreement",
                        "minor disagreement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спор или дискуссия между людьми с разными взглядами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разногласие",
                                "target_lang_sense_clarification": "спор, расхождение во мнениях"
                            },
                            {
                                "target_lang_word": "спор",
                                "target_lang_sense_clarification": "словесное состязание, конфликт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12ba2aa5-80d9-3742-adf5-8fdc506665fb",
                    "sense_definition": "A condition of not agreeing, holding different opinions, or failing to correspond.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_differing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two sides were in <w>disagreement</w> regarding the final terms of the contract.",
                            "target_lang_translations": {
                                "ru": "Между двумя сторонами возникло <w>разногласие</w> относительно окончательных условий контракта."
                            }
                        },
                        {
                            "text": "There is considerable <w>disagreement</w> among experts on how to solve this problem.",
                            "target_lang_translations": {
                                "ru": "Среди экспертов существует значительное <w>несогласие</w> по поводу того, как решить эту проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "dissent",
                        "variance",
                        "difference"
                    ],
                    "antonyms": [
                        "agreement",
                        "consensus",
                        "harmony"
                    ],
                    "common_phrases": [
                        "in disagreement",
                        "express disagreement",
                        "strong disagreement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отсутствия согласия, наличие разных мнений или несоответствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несогласие",
                                "target_lang_sense_clarification": "отсутствие согласия, отказ"
                            },
                            {
                                "target_lang_word": "разногласие",
                                "target_lang_sense_clarification": "отсутствие единства во мнениях"
                            },
                            {
                                "target_lang_word": "несоответствие",
                                "target_lang_sense_clarification": "отсутствие совпадения, противоречие"

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопровождал",
                                "target_lang_sense_clarification": "прошедшее время: шел или ехал вместе с кем-то"
                            },
                            {
                                "target_lang_word": "сопровождался",
                                "target_lang_sense_clarification": "страдательный залог: происходил одновременно с чем-то"
                            }
                        ]
                    }
                }
            ]
        },
        {

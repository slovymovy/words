    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "31a6253d-9311-3a00-9185-de94370c8ff4",
                    "sense_definition": "Iets nieuws, zoals een product, idee of campagne, introduceren bij het publiek.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf gaat volgende maand een nieuwe smartphone <w>lanceren</w>.",
                            "target_lang_translations": {
                                "ru": "В следующем месяце компания собирается <w>выпустить</w> новый смартфон.",
                                "en": "The company is going to <w>launch</w> a new smartphone next month."
                            }
                        },
                        {
                            "text": "De politieke partij heeft gisteren haar verkiezingscampagne <w>gelanceerd</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера политическая партия <w>запустила</w> свою предвыборную кампанию.",
                                "en": "The political party <w>launched</w> its election campaign yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "introduceren",
                        "uitbrengen",
                        "presenteren"
                    ],
                    "antonyms": [
                        "terugtrekken"
                    ],
                    "common_phrases": [
                        "een product lanceren",
                        "een campagne lanceren",
                        "een website lanceren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представить публике что-то новое, например продукт, идею или кампанию.",
                        "en": "To introduce something new, such as a product, idea, or campaign, to the public."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "начинать реализацию проекта, кампании"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "представлять товар на рынке"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "to start or introduce a new product or campaign"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "to make a product available to the public"
                            },
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "to bring something into use for the first time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ec492f6-d9f4-39e7-849c-655d5f0e9cd5",
                    "sense_definition": "Met kracht de lucht in schieten, meestal een raket of projectiel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ruimtevaartorganisatie heeft gisteren succesvol een raket <w>gelanceerd</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера космическое агентство успешно <w>запустило</w> ракету.",
                                "en": "The space agency successfully <w>launched</w> a rocket yesterday."
                            }
                        },
                        {
                            "text": "Vanaf het schip werd een torpedo <w>gelanceerd</w>.",
                            "target_lang_translations": {
                                "ru": "С корабля была <w>выпущена</w> торпеда.",
                                "en": "A torpedo was <w>launched</w> from the ship."
                            }
                        }
                    ],
                    "synonyms": [
                        "afschieten",
                        "afvuren"
                    ],
                    "common_phrases": [
                        "een raket lanceren",
                        "een satelliet lanceren"
                    ],
                    "target_lang_definitions": {
                        "ru": "С силой отправить в воздух или пространство, обычно ракету или снаряд.",
                        "en": "To shoot into the air with force, usually a rocket or projectile."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "отправлять в полет (ракету, спутник)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "to send a rocket or missile into the air"
                            },
                            {
                                "target_lang_word": "fire",
                                "target_lang_sense_clarification": "to shoot a projectile or weapon"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
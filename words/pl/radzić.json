    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "70a323c2-3557-3341-88de-bfdf2254bcfa",
                    "sense_definition": "Udzielać komuś wskazówek, sugestii lub pouczeń dotyczących tego, co należy zrobić w danej sytuacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz <radził> mi, abym więcej odpoczywał i dbał o zdrowie.",
                            "target_lang_translations": {
                                "ru": "Врач <советовал> мне больше отдыхать и следить за здоровьем."
                            }
                        },
                        {
                            "text": "Dobrze ci <radzę>, nie spóźnij się na to spotkanie.",
                            "target_lang_translations": {
                                "ru": "Я тебе искренне <советую>, не опаздывай на эту встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "doradzać",
                        "sugerować"
                    ],
                    "antonyms": [
                        "odradzać"
                    ],
                    "common_phrases": [
                        "dobrze radzić",
                        "szczerze radzić",
                        "radzić komuś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-либо советы, указания или рекомендации относительно того, как поступить в данной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "советовать",
                                "target_lang_sense_clarification": "давать рекомендацию или наставление"
                            },
                            {
                                "target_lang_word": "давать совет",
                                "target_lang_sense_clarification": "высказывать мнение о том, как поступить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb18cd58-c33a-3f72-8a95-bfa79f9c965a",
                    "sense_definition": "Występując z zaimkiem „sobie”: pokonywać trudności, umieć załatwić sprawy lub wykonywać zadania z powodzeniem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "coping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra świetnie sobie <radzi> w nowej pracy.",
                            "target_lang_translations": {
                                "ru": "Моя сестра отлично <справляется> на новой работе."
                            }
                        },
                        {
                            "text": "Musisz nauczyć się <radzić> sobie ze stresem w trudnych momentach.",
                            "target_lang_translations": {
                                "ru": "Ты должен научиться <справляться> со стрессом в трудные моменты."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawać sobie radę"
                    ],
                    "common_phrases": [
                        "radzić sobie z czymś",
                        "jakoś sobie radzić",
                        "świetnie sobie radzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "В сочетании с местоимением «sobie»: преодолевать трудности, успешно справляться с делами или задачами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справляться",
                                "target_lang_sense_clarification": "успешно выполнять задачу или преодолевать трудности"
                            },
                            {
                                "target_lang_word": "ладить",
                                "target_lang_sense_clarification": "разговорное; уметь обращаться с чем-либо или кем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "359e7df7-d8c8-3e37-a954-c55a3e96fe42",
                    "sense_definition": "Występując z zaimkiem „się”: zwracać się do kogoś z prośbą o opinię, wskazówkę lub pomoc w podjęciu decyzji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "consultation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <radzę> się ojca przed podjęciem ważnej decyzji finansowej.",
                            "target_lang_translations": {
                                "ru": "Я всегда <советуюсь> с отцом перед принятием важного финансового решения."
                            }
                        },
                        {
                            "text": "W sprawach zdrowotnych najlepiej <radzić> się doświadczonego lekarza.",
                            "target_lang_translations": {
                                "ru": "В вопросах здоровья лучше всего <советоваться> с опытным врачом."
                            }
                        }
                    ],
                    "synonyms": [
                        "konsultować się"
                    ],
                    "common_phrases": [
                        "radzić się kogoś",
                        "radzić się w sprawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В сочетании с местоимением «się»: обращаться к кому-либо за мнением, советом или помощью в принятии решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "советоваться",
                                "target_lang_sense_clarification": "спрашивать чьего-либо мнения или совета"
                            },
                            {
                                "target_lang_word": "консультироваться",
                                "target_lang_sense_clarification": "обращаться к специалисту за советом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ceeacd31-a15c-3190-a648-cdb9ac8aef9c",
                    "sense_definition": "Wspólnie zastanawiać się nad czymś, prowadzić rozmowy w celu podjęcia decyzji lub rozwiązania problemu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deliberation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komisja będzie <radzić> nad nowym projektem ustawy przez cały tydzień.",
                            "target_lang_translations": {
                                "ru": "Комиссия будет <совещаться> по поводу нового законопроекта всю неделю."
                            }
                        },
                        {
                            "text": "Generałowie długo <radzili> nad strategią obrony granic.",
                            "target_lang_translations": {
                                "ru": "Генералы долго <совещались> по поводу стратегии обороны границ."
                            }
                        }
                    ],
                    "synonyms": [
                        "obradować",
                        "naradzać się",
                        "deliberować",
                        "konferować"
                    ],
                    "common_phrases": [
                        "radzić nad czymś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совместно обсуждать что-либо, вести беседы для принятия решения или решения проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совещаться",
                                "target_lang_sense_clarification": "обсуждать что-либо сообща, обмениваться мнениями"
                            },
                            {
                                "target_lang_word": "обсуждать",
                                "target_lang_sense_clarification": "рассматривать вопрос, высказывая мнения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

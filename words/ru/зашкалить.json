                    ],
                    "common_phrases": [
                        "стрелка зашкалила",
                        "прибор зашкалил",
                        "датчик зашкалил"
                    ],
                    "target_lang_definitions": {
                        "en": "To go beyond the limits of the scale of a measuring instrument"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go off the scale",
                                "target_lang_sense_clarification": "literal, regarding measuring instruments"

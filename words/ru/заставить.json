    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53fb6f4d-555f-3c8d-b858-52b6fb16b7ae",
                    "sense_definition": "Принудить кого-либо совершить какое-либо действие против воли или в силу обстоятельств.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Плохая погода <заставила> нас остаться дома на весь день.",
                            "target_lang_translations": {
                                "en": "Bad weather <forced> us to stay home all day."
                            }
                        },
                        {
                            "text": "Учитель <заставил> ученика переписать домашнее задание без ошибок.",
                            "target_lang_translations": {
                                "en": "The teacher <made> the student rewrite the homework without errors."
                            }
                        },
                        {
                            "text": "Нужно <заставить> себя работать, даже если совсем не хочется.",
                            "target_lang_translations": {
                                "en": "You need to <force> yourself to work, even if you really don't want to."
                            }
                        }
                    ],
                    "synonyms": [
                        "принудить",
                        "вынудить",
                        "понудить"
                    ],
                    "common_phrases": [
                        "заставить себя",
                        "заставить ждать",
                        "заставить задуматься",
                        "заставить врасплох"
                    ],
                    "target_lang_definitions": {
                        "en": "To force or compel someone to perform an action against their will or due to circumstances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "to make someone do something against their will"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "to cause someone to perform an action"
                            },
                            {
                                "target_lang_word": "compel",
                                "target_lang_sense_clarification": "formal, often implying external circumstances"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b34b184-6a3a-39d7-98f8-b16af36f149b",
                    "sense_definition": "Занять всё свободное пространство, расставив множество предметов.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <заставила> весь подоконник горшками с красивыми цветами.",
                            "target_lang_translations": {
                                "en": "She <filled> the entire windowsill with pots of beautiful flowers."
                            }
                        },
                        {
                            "text": "Мы <заставили> маленькую комнату мебелью так, что негде пройти.",
                            "target_lang_translations": {
                                "en": "We <cluttered> the small room with furniture so much that there was nowhere to walk."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заставить стол посудой",
                        "заставить комнату мебелью",
                        "заставить вещами"
                    ],
                    "target_lang_definitions": {
                        "en": "To occupy all available space by placing many objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fill",
                                "target_lang_sense_clarification": "to occupy space with objects"
                            },
                            {
                                "target_lang_word": "clutter",
                                "target_lang_sense_clarification": "to fill a space untidily with too many things"
                            },
                            {
                                "target_lang_word": "cram",
                                "target_lang_sense_clarification": "to fill a space tightly or to overflowing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae87b967-e496-3f5d-8fd2-bb1d29ed46b7",
                    "sense_definition": "Преградить доступ к чему-либо или закрыть проход, поставив какой-то предмет.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы никто не вошёл, он <заставил> дверь тяжёлым шкафом.",
                            "target_lang_translations": {
                                "en": "To ensure no one entered, he <blocked> the door with a heavy wardrobe."
                            }
                        },
                        {
                            "text": "Рабочие <заставили> проход коробками, и нам пришлось их обходить.",
                            "target_lang_translations": {
                                "en": "The workers <blocked> the aisle with boxes, and we had to walk around them."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заставить дверь шкафом",
                        "заставить проход",
                        "заставить выход"
                    ],
                    "target_lang_definitions": {
                        "en": "To obstruct access or close a passage by placing an object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "block",
                                "target_lang_sense_clarification": "to obstruct a passage or opening"
                            },
                            {
                                "target_lang_word": "barricade",
                                "target_lang_sense_clarification": "to block a door or entrance with heavy objects"
                            }
                        ]
                    }
                }
            ]
        }
    ]

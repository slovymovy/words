    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da5548db-efb4-3602-b6d9-a0122d00c1a3",
                    "sense_definition": "Een verbinding vormen over een fysieke tussenruimte of obstakel, zoals een rivier of kloof",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_bridging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ingenieurs <w>overbruggen</w> de brede rivier met een nieuwe hangbrug.",
                            "target_lang_translations": {
                                "en": "The engineers are <w>bridging</w> the wide river with a new suspension bridge."
                            }
                        },
                        {
                            "text": "Langs een smal paadje moesten we voetje voor voetje proberen de twintig meter te <w>overbruggen</w>.",
                            "target_lang_translations": {
                                "en": "Along a narrow path, we had to try to <w>cover</w> the twenty meters step by step."
                            }
                        }
                    ],
                    "synonyms": [
                        "overspannen"
                    ],
                    "common_phrases": [
                        "een rivier overbruggen",
                        "een afstand overbruggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To form a connection across a physical space or obstacle, such as a river or gorge"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bridge",
                                "target_lang_sense_clarification": "to connect two sides physically"
                            },
                            {
                                "target_lang_word": "span",
                                "target_lang_sense_clarification": "to extend across a space"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cfd9367-14bd-3c51-863a-e099da883827",
                    "sense_definition": "Een tegenstelling, verschil of moeilijke periode wegnemen of doorkomen",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "figurative_bridging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De diplomaten doen hun best om de politieke meningsverschillen te <w>overbruggen</w>.",
                            "target_lang_translations": {
                                "en": "The diplomats are doing their best to <w>bridge</w> the political differences."
                            }
                        },
                        {
                            "text": "Hij nam een tijdelijke baan om de periode tot zijn studie te <w>overbruggen</w>.",
                            "target_lang_translations": {
                                "en": "He took a temporary job to <w>bridge</w> the period until his studies."
                            }
                        }
                    ],
                    "synonyms": [
                        "dichten",
                        "overkomen"
                    ],
                    "common_phrases": [
                        "een kloof overbruggen",
                        "verschillen overbruggen",
                        "een periode overbruggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To resolve a contrast or difference, or to get through a specific period"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bridge",
                                "target_lang_sense_clarification": "to connect figurative gaps or differences"
                            },
                            {
                                "target_lang_word": "reconcile",
                                "target_lang_sense_clarification": "to settle differences or disagreements"
                            },
                            {
                                "target_lang_word": "tide over",
                                "target_lang_sense_clarification": "to help get through a period of time"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bbf685fd-b3d0-343a-90b4-4e6998ea7786",
                    "sense_definition": "Zielonkawy lub niebieskawy nalot powstający na powierzchni przedmiotów metalowych, zwłaszcza miedzianych lub mosiężnych, wskutek utleniania się ich powierzchni pod wpływem wilgoci i powietrza.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "oxidation_coating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary miedziany dach kościoła pokryła z biegiem lat zielona <w>śniedź</w>.",
                            "target_lang_translations": {
                                "ru": "Старую медную крышу церкви с годами покрыла зеленая <w>патина</w>."
                            }
                        },
                        {
                            "text": "Musisz dokładnie wyczyścić ten świecznik, ponieważ osadziła się na nim <w>śniedź</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно тщательно почистить этот подсвечник, так как на нем образовалась <w>окись</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "patyna",
                        "grynszpan"
                    ],
                    "common_phrases": [
                        "pokryć się śniedzią",
                        "warstwa śniedzi",
                        "zielona śniedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зеленоватый или голубоватый налет, образующийся на поверхности металлических предметов (особенно медных или латунных) в результате окисления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патина",
                                "target_lang_sense_clarification": "благородный налет"
                            },
                            {
                                "target_lang_word": "ярь-медянка",
                                "target_lang_sense_clarification": "зеленый налет на меди"
                            },
                            {
                                "target_lang_word": "окись",
                                "target_lang_sense_clarification": "продукт окисления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b3eef66-05f7-341b-9bb8-ac720a0a7184",
                    "sense_definition": "Rzadziej używany wariant słowa <w>śnieć</w>, oznaczający chorobę roślin zbożowych wywoływaną przez pasożytnicze grzyby, objawiającą się czarnym pyłem na kłosach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plant_disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik zauważył, że tegoroczne zbiory owsa zostały zniszczone przez <w>śniedź</w>.",
                            "target_lang_translations": {
                                "ru": "Фермер заметил, что урожай овса в этом году был уничтожен <w>головнёй</w>."
                            }
                        },
                        {
                            "text": "Kłosy jęczmienia sczerniały, jakby zaatakowała je <w>śniedź</w>.",
                            "target_lang_translations": {
                                "ru": "Колосья ячменя почернели, словно их поразила <w>головня</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "grynszpan"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wariant słowa śnieć"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Реже используемый вариант слова «śnieć», обозначающий грибковое заболевание злаковых растений (головня)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "головня",
                                "target_lang_sense_clarification": "болезнь растений"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "488ba075-45ef-3b5f-8ef9-12241793bf03",
                    "sense_definition": "Jedna setna część pewnej wielkości lub liczby.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "math_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda stanowi duży <procent> masy ciała człowieka.",
                            "target_lang_translations": {
                                "ru": "Вода составляет большой <w>процент</w> массы тела человека."
                            }
                        },
                        {
                            "text": "Jestem pewien na sto <procent>, że zdam ten egzamin.",
                            "target_lang_translations": {
                                "ru": "Я на сто <w>процентов</w> уверен, что сдам этот экзамен."
                            }
                        },
                        {
                            "text": "Ceny w sklepach wzrosły o kilka <procent> w tym roku.",
                            "target_lang_translations": {
                                "ru": "Цены в магазинах выросли на несколько <w>процентов</w> в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "%",
                        "proc."
                    ],
                    "common_phrases": [
                        "sto procent",
                        "jeden procent",
                        "pięćdziesiąt procent",
                        "kilka procent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна сотая часть какой-либо величины или числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "математическая единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d2cb004-2442-31ac-a541-1852af264394",
                    "sense_definition": "Dochód uzyskiwany z kapitału złożonego w banku lub pożyczonego komuś; zysk finansowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bank oferuje bardzo korzystny <procent> na nowej lokacie.",
                            "target_lang_translations": {
                                "ru": "Банк предлагает очень выгодный <w>процент</w> по новому вкладу."
                            }
                        },
                        {
                            "text": "Pożyczyłem mu pieniądze na wysoki <procent>, żeby zarobić.",
                            "target_lang_translations": {
                                "ru": "Я одолжил ему деньги под высокий <w>процент</w>, чтобы заработать."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprocentowanie",
                        "odsetka"
                    ],
                    "common_phrases": [
                        "na procent",
                        "wysoki procent",
                        "żyć z procentów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доход, получаемый с капитала, вложенного в банк или одолженного кому-либо; финансовая прибыль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "доход с капитала, ставка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0488c09-1dff-3375-95d3-11ba5b0d5dc6",
                    "sense_definition": "Pewna część lub ułamek jakiejś zbiorowości, często nieokreślona precyzyjnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "proportion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znaczny <procent> uczniów nie odrobił pracy domowej.",
                            "target_lang_translations": {
                                "ru": "Значительный <w>процент</w> учеников не сделал домашнее задание."
                            }
                        },
                        {
                            "text": "Tylko niewielki <procent> ankietowanych zgodził się z tym zdaniem.",
                            "target_lang_translations": {
                                "ru": "Лишь небольшой <w>процент</w> опрошенных согласился с этим мнением."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsetek"
                    ],
                    "common_phrases": [
                        "duży procent",
                        "znikomy procent",
                        "mały procent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная часть или доля чего-либо, часто не определенная точно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "доля, часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d58e20d2-7a17-3cd4-82cd-3749ed57d573",
                    "sense_definition": "Znak graficzny (%) używany do zapisu wartości procentowej.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedyś pisano <procenty> z kreską poziomą, a nie ukośną.",
                            "target_lang_translations": {
                                "ru": "Раньше <w>проценты</w> писали с горизонтальной чертой, а не с косой."
                            }
                        },
                        {
                            "text": "Na klawiaturze nie działa mi klawisz z <procentem>.",
                            "target_lang_translations": {
                                "ru": "На клавиатуре у меня не работает клавиша с <w>процентом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "%"
                    ],
                    "common_phrases": [
                        "symbol procenta",
                        "znak procenta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графический знак (%), используемый для обозначения процентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак процента",
                                "target_lang_sense_clarification": "символ"
                            },
                            {
                                "target_lang_word": "процент",
                                "target_lang_sense_clarification": "название символа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e09b6215-c3f9-3861-8866-b4eee0838c9c",
                    "sense_definition": "Стать больше ростом или размером в процессе развития (о людях, животных, растениях).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За это лето мой сын очень сильно <вырос>.",
                            "target_lang_translations": {
                                "en": "My son has <grown> a lot this summer."
                            }
                        },
                        {
                            "text": "В нашем саду <выросло> красивое и высокое дерево.",
                            "target_lang_translations": {
                                "en": "A beautiful and tall tree has <grown> in our garden."
                            }
                        }
                    ],
                    "synonyms": [
                        "подрасти"
                    ],
                    "antonyms": [
                        "уменьшиться"
                    ],
                    "common_phrases": [
                        "быстро вырасти",
                        "вырасти большим"
                    ],
                    "target_lang_definitions": {
                        "en": "To become taller or larger in size during the process of development (about people, animals, plants)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "to increase in physical size or height"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "577845ab-9afc-3b16-8822-91378edd8c69",
                    "sense_definition": "Стать взрослым, выйти из детского возраста; провести детство и юность в определённой среде.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "biological_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <вырос> в маленьком городе на юге страны.",
                            "target_lang_translations": {
                                "en": "I <grew up> in a small town in the south of the country."
                            }
                        },
                        {
                            "text": "Когда ты <вырастешь>, ты поймёшь, почему я так поступил.",
                            "target_lang_translations": {
                                "en": "When you <grow up>, you will understand why I did that."
                            }
                        }
                    ],
                    "synonyms": [
                        "повыситься"
                    ],
                    "common_phrases": [
                        "вырасти в семье",
                        "вырасти без отца"
                    ],
                    "target_lang_definitions": {
                        "en": "To become an adult, to leave childhood behind; to spend one's childhood and youth in a specific environment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grow up",
                                "target_lang_sense_clarification": "to mature and become an adult"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5efa277a-7cda-3377-af32-72cc26e309e5",
                    "sense_definition": "Увеличиться в количестве, силе, степени или стоимости.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Цены на продукты питания заметно <выросли> в этом году.",
                            "target_lang_translations": {
                                "en": "Food prices have noticeably <risen> this year."
                            }
                        },
                        {
                            "text": "Интерес к изучению иностранных языков сильно <вырос>.",
                            "target_lang_translations": {
                                "en": "Interest in learning foreign languages has <grown> significantly."
                            }
                        }
                    ],
                    "synonyms": [
                        "увеличиться",
                        "повыситься",
                        "возрасти"
                    ],
                    "antonyms": [
                        "уменьшиться",
                        "уменьшиться"
                    ],
                    "common_phrases": [
                        "вырасти в цене",
                        "вырасти в разы"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase in quantity, strength, degree, or cost."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "of prices, numbers, or levels"
                            },
                            {
                                "target_lang_word": "increase",
                                "target_lang_sense_clarification": "to become greater in amount or degree"
                            },
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "to expand or increase in magnitude"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4eac04fa-9cca-3200-85d5-0b3dae46fb69",
                    "sense_definition": "Стать слишком большим для какой-либо одежды или перерасти прежние привычки.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок уже <вырос> из этих ботинок, нужно покупать новые.",
                            "target_lang_translations": {
                                "en": "The child has already <grown out of> these shoes; we need to buy new ones."
                            }
                        },
                        {
                            "text": "Мы давно <выросли> из детских сказок и верим в реальность.",
                            "target_lang_translations": {
                                "en": "We <grew out of> fairy tales a long time ago and believe in reality."
                            }
                        }
                    ],
                    "synonyms": [
                        "перерасти"
                    ],
                    "common_phrases": [
                        "вырасти из одежды",
                        "вырасти из пелёнок"
                    ],
                    "target_lang_definitions": {
                        "en": "To become too big for certain clothing or to move beyond former habits."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grow out of",
                                "target_lang_sense_clarification": "to become too big for something"
                            },
                            {
                                "target_lang_word": "outgrow",
                                "target_lang_sense_clarification": "to grow too large for"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fbfa437-692c-3f0a-915b-5e9ffc303678",
                    "sense_definition": "Форма повелительного наклонения глагола <w>вырастить</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <вырасти> этот цветок с любовью и заботой.",
                            "target_lang_translations": {
                                "en": "Please <grow> this flower with love and care."
                            }
                        },
                        {
                            "text": "<Вырасти> в себе силу воли, чтобы преодолеть трудности.",
                            "target_lang_translations": {
                                "en": "<Cultivate> willpower within yourself to overcome difficulties."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative of transitive verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of the verb 'to raise' or 'to cultivate'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "to cause plants to grow"
                            },
                            {
                                "target_lang_word": "cultivate",
                                "target_lang_sense_clarification": "to foster the growth of qualities or plants"
                            },
                            {
                                "target_lang_word": "raise",
                                "target_lang_sense_clarification": "to nurture or bring up"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "143308e8-970c-3566-a89d-884984e442cf",
                    "sense_definition": "Усовершенствоваться, достичь более высокого уровня в развитии или мастерстве.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За последние два года он сильно <вырос> как профессионал.",
                            "target_lang_translations": {
                                "en": "Over the last two years, he has <grown> significantly as a professional."
                            }
                        },
                        {
                            "text": "Она <выросла> в моих глазах после этого смелого поступка.",
                            "target_lang_translations": {
                                "en": "She <grew> in my eyes after this bold act."
                            }
                        }
                    ],
                    "synonyms": [
                        "усовершенствоваться",
                        "развиться"
                    ],
                    "common_phrases": [
                        "вырасти профессионально",
                        "вырасти над собой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "figurative usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To improve oneself, to reach a higher level of development or mastery."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "to develop professionally or personally"
                            },
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "to evolve or mature"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f445102-a3a0-3a04-bb66-3b78723f554e",
                    "sense_definition": "Внезапно появиться, возникнуть перед глазами (обычно о чём-то большом).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Неожиданно перед нами <выросла> высокая каменная стена.",
                            "target_lang_translations": {
                                "en": "Unexpectedly, a high stone wall <rose up> before us."
                            }
                        },
                        {
                            "text": "Гора <выросла> на горизонте, словно из-под земли.",
                            "target_lang_translations": {
                                "en": "A mountain <rose up> on the horizon, as if from under the ground."
                            }
                        }
                    ],
                    "synonyms": [
                        "возрасти"
                    ],
                    "common_phrases": [
                        "вырасти из-под земли",
                        "вырасти перед глазами"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "figurative usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To appear suddenly or arise before one's eyes (usually about something large)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise up",
                                "target_lang_sense_clarification": "to appear tall or high"
                            },
                            {
                                "target_lang_word": "loom",
                                "target_lang_sense_clarification": "to appear as a large, often threatening shape"
                            },
                            {
                                "target_lang_word": "spring up",
                                "target_lang_sense_clarification": "to appear suddenly"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "948ef1a2-cd4f-3eaa-bcc5-46bb22c8f3f0",
                    "sense_definition": "Przybyć z powrotem do miejsca, w którym się wcześniej było lub z którego się wyruszyło.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <wrócić> do domu, ponieważ zapomniałem wyłączyć żelazko.",
                            "target_lang_translations": {
                                "ru": "Я должен быстро <вернуться> домой, так как забыл выключить утюг."
                            }
                        },
                        {
                            "text": "Kiedy wreszcie <wrócisz> z tej dalekiej podróży dookoła świata?",
                            "target_lang_translations": {
                                "ru": "Когда ты наконец <вернёшься> из этого далёкого кругосветного путешествия?"
                            }
                        }
                    ],
                    "synonyms": [
                        "powrócić",
                        "przywrócić"
                    ],
                    "antonyms": [
                        "wyjechać",
                        "wyjść",
                        "wyruszyć"
                    ],
                    "common_phrases": [
                        "wrócić do domu",
                        "wrócić z pracy",
                        "wrócić na miejsce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибыть обратно в место, где вы были раньше или откуда вышли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуться",
                                "target_lang_sense_clarification": "прийти или приехать назад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dc4d8cd-4057-3e4e-b2f5-cca7d47220d2",
                    "sense_definition": "Zacząć ponownie zajmować się czymś, co zostało przerwane, lub znaleźć się znowu w poprzednim stanie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej chorobie dziadek nareszcie <wrócił> do pełni zdrowia.",
                            "target_lang_translations": {
                                "ru": "После долгой болезни дедушка наконец <вернулся> к полному здоровью."
                            }
                        },
                        {
                            "text": "Proponuję, abyśmy teraz <wrócili> do naszej przerwanej rozmowy o projekcie.",
                            "target_lang_translations": {
                                "ru": "Предлагаю нам сейчас <вернуться> к нашему прерванному разговору о проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "powrócić",
                        "zwrócić się"
                    ],
                    "antonyms": [
                        "porzucić",
                        "przerwać"
                    ],
                    "common_phrases": [
                        "wrócić do zdrowia",
                        "wrócić do tematu",
                        "wrócić do formy",
                        "wrócić do gry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снова начать заниматься чем-то прерванным или вновь оказаться в прежнем состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуться",
                                "target_lang_sense_clarification": "к теме, к состоянию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acc3542f-3423-377d-b25f-8fe3ea3001f0",
                    "sense_definition": "Zawrócić z drogi, idąc w przeciwnym kierunku, zazwyczaj po to, by coś zabrać lub sprawdzić (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem <wrócić> się do mieszkania, bo zostawiłem portfel na stole.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <вернуться> в квартиру, так как я оставил кошелёк на столе."
                            }
                        },
                        {
                            "text": "Może <wrócimy> się do samochodu po parasol, bo zaczyna padać deszcz?",
                            "target_lang_translations": {
                                "ru": "Может, <вернёмся> к машине за зонтом, так как начинается дождь?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zawrócić"
                    ],
                    "common_phrases": [
                        "wrócić się po coś",
                        "wrócić się do domu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуться с полдороги, идя в обратном направлении, обычно чтобы что-то забрать или проверить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуться",
                                "target_lang_sense_clarification": "с полдороги (за чем-то)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2205dc60-54ac-3a31-85b3-8c4acce84589",
                    "sense_definition": "Pojawić się znowu po pewnym czasie nieobecności (o uczuciach, zjawiskach, chorobach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy zobaczyłem stare zdjęcia, natychmiast <wróciły> wszystkie dawne wspomnienia.",
                            "target_lang_translations": {
                                "ru": "Когда я увидел старые фотографии, сразу же <вернулись> все старые воспоминания."
                            }
                        },
                        {
                            "text": "Niestety ból głowy <wrócił> wieczorem ze zdwojoną siłą.",
                            "target_lang_translations": {
                                "ru": "К сожалению, головная боль <вернулась> вечером с удвоенной силой."
                            }
                        }
                    ],
                    "synonyms": [
                        "powrócić"
                    ],
                    "antonyms": [
                        "ustąpić",
                        "minąć"
                    ],
                    "common_phrases": [
                        "wróciły wspomnienia",
                        "wrócił ból",
                        "wróciła moda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появиться снова спустя некоторое время (о чувствах, явлениях, болезнях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуться",
                                "target_lang_sense_clarification": "возобновиться (о боли, памяти)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a283a12d-4b9c-3a1a-b712-77c3802e3d21",
                    "sense_definition": "Oddać coś, co się wcześniej wzięło lub pożyczyło, prawowitemu właścicielowi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "give_back",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiecuję, że <wrócę> ci te pieniądze najpóźniej w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Обещаю, что <верну> тебе эти деньги не позднее следующей недели."
                            }
                        },
                        {
                            "text": "Czy możesz mi <wrócić> książkę, którą pożyczyłeś ode mnie miesiąc temu?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <вернуть> мне книгу, которую одолжил у меня месяц назад?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zwrócić",
                        "powrócić"
                    ],
                    "antonyms": [
                        "pożyczyć",
                        "zabrać"
                    ],
                    "common_phrases": [
                        "wrócić pieniądze",
                        "wrócić dług"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard Polish prefers 'zwrócić' or 'oddać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдать то, что было взято или одолжено, законному владельцу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "отдать долг, вещь"
                            },
                            {
                                "target_lang_word": "отдать",
                                "target_lang_sense_clarification": "возвратить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
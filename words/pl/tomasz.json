            "senses": [
                {
                    "sense_id": "ec7c5e86-9192-3251-9f88-53b9ef5389d6",
                    "sense_definition": "Męskie imię osobowe pochodzenia aramejskiego, odpowiednik angielskiego Thomas.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "male_names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mam na imię <w>Tomasz</w> i interesuję się motoryzacją.",
                            "target_lang_translations": {
                                "ru": "Меня зовут <w>Томаш</w>, и я интересуюсь автомобилями."
                            }
                        },
                        {
                            "text": "Judyta wyszła za mąż za <w>Tomasza</w> w zeszłym roku.",
                            "target_lang_translations": {
                                "ru": "Юдита вышла замуж за <w>Томаша</w> в прошлом году."
                            }
                        },
                        {
                            "text": "Nikt nie lubi, gdy ktoś zachowuje się jak niewierny <w>Tomasz</w>.",
                            "target_lang_translations": {
                                "ru": "Никто не любит, когда кто-то ведёт себя как <w>Фома</w> неверующий."
                            }
                        }
                    ],
                    "synonyms": [
                        "Tomek",
                        "Tomuś",
                        "Tomcio",
                        "Tomaszek",
                        "Tomeczek",
                        "Tomko"
                    ],
                    "common_phrases": [
                        "niewierny Tomasz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское личное имя арамейского происхождения, эквивалент английского Томас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Томаш",
                                "target_lang_sense_clarification": "польское имя"
                            },
                            {
                                "target_lang_word": "Фома",
                                "target_lang_sense_clarification": "русский эквивалент / в идиомах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f79b5ea8-f3cc-31ad-95ee-5c818b54e51b",
                    "sense_definition": "Postać biblijna, jeden z dwunastu apostołów Jezusa Chrystusa, znany z wątpliwości co do zmartwychwstania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_figures",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Święty <w>Tomasz</w> jest często przedstawiany z włócznią.",
                            "target_lang_translations": {
                                "ru": "Святой <w>Фома</w> часто изображается с копьём."
                            }
                        },
                        {
                            "text": "W Ewangelii czytamy o tym, jak <w>Tomasz</w> dotknął ran Chrystusa.",
                            "target_lang_translations": {
                                "ru": "В Евангелии мы читаем о том, как <w>Фома</w> коснулся ран Христа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ewangelia Tomasza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский персонаж, один из двенадцати апостолов Иисуса Христа, известный своими сомнениями в воскресении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фома",
                                "target_lang_sense_clarification": "библейское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83aeb6fc-df04-31cc-8d5c-40fa6434e6c0",
                    "sense_definition": "Nazwisko męskie używane w Polsce.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "surnames",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan Adam <w>Tomasz</w> został wybrany na prezesa spółki.",
                            "target_lang_translations": {
                                "ru": "Господин Адам <w>Томаш</w> был избран президентом компании."
                            }
                        },
                        {
                            "text": "W spisie lokatorów widnieje nazwisko <w>Tomasz</w>.",
                            "target_lang_translations": {

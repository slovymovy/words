            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c1eeb3bd-1fbf-36b3-a0f9-54d81413f691",
                    "sense_definition": "Geld betalen voor goederen of diensten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>geven</w> elke maand veel geld <w>uit</w> aan boodschappen.",
                            "target_lang_translations": {
                                "en": "We <w>spend</w> a lot of money on groceries every month.",
                                "ru": "Мы каждый месяц <w>тратим</w> много денег на продукты."
                            }
                        },
                        {
                            "text": "Je moet proberen niet meer geld <w>uit</w> te <w>geven</w> dan je verdient.",
                            "target_lang_translations": {
                                "en": "You should try not to <w>spend</w> more money than you earn.",
                                "ru": "Ты должен стараться не <w>тратить</w> больше денег, чем зарабатываешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "besteden",
                        "spenderen",
                        "opmaken"
                    ],
                    "antonyms": [
                        "sparen",
                        "oppotten",
                        "verdienen"
                    ],
                    "common_phrases": [
                        "geld uitgeven",
                        "veel uitgeven",
                        "niets uitgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To pay money for goods or services.",
                        "ru": "Тратить деньги на товары или услуги."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spend",
                                "target_lang_sense_clarification": "money"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тратить",
                                "target_lang_sense_clarification": "расходовать деньги"
                            },
                            {
                                "target_lang_word": "расходовать",
                                "target_lang_sense_clarification": "потреблять ресурсы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fe43382-f24d-3ec5-8b5f-7590eacb288d",
                    "sense_definition": "Een geschrift, zoals een boek of tijdschrift, openbaar maken en verspreiden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bekende schrijver gaat zijn nieuwe roman volgende maand <w>uitgeven</w>.",
                            "target_lang_translations": {
                                "en": "The famous writer is going to <w>publish</w> his new novel next month.",
                                "ru": "Известный писатель собирается <w>издать</w> свой новый роман в следующем месяце."
                            }
                        },
                        {
                            "text": "Dit leerboek wordt door een grote educatieve uitgeverij <w>uitgegeven</w>.",
                            "target_lang_translations": {
                                "en": "This textbook is <w>published</w> by a large educational publisher.",
                                "ru": "Этот учебник <w>издается</w> крупным образовательным издательством."
                            }
                        }
                    ],
                    "synonyms": [
                        "publiceren",
                        "uitbrengen",
                        "lanceren"
                    ],
                    "common_phrases": [
                        "een boek uitgeven",
                        "werk uitgeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a written work, such as a book or magazine, public and distribute it.",
                        "ru": "Публиковать и распространять печатные издания, такие как книги или журналы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "publish",
                                "target_lang_sense_clarification": "books, magazines, etc."
                            },
                            {

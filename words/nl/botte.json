            "senses": [
                {
                    "sense_id": "7b65d16a-0573-3f0e-90bc-90ea757191d3",
                    "sense_definition": "Enkelvoud verleden tijd van het werkwoord <w>botten</w> (knoppen krijgen of uitlopen van planten).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude eik <w>botte</w> pas laat in het voorjaar.",
                            "target_lang_translations": {
                                "ru": "Старый дуб <w>пустил почки</w> только поздно весной."
                            }
                        },
                        {
                            "text": "Toen de zon doorbrak, <w>botte</w> het struikgewas snel uit.",
                            "target_lang_translations": {
                                "ru": "Когда выглянуло солнце, кустарник быстро <w>распустился</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "liep uit",
                        "kreeg knoppen"
                    ],
                    "target_lang_definitions": {

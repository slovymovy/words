    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "865d2b5f-6f44-31b4-bb29-83367c3ad1b9",
                    "sense_definition": "A female parent.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mother_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>mom</w> drives me to school every morning.",
                            "target_lang_translations": {
                                "ru": "Моя <w>мама</w> отвозит меня в школу каждое утро."
                            }
                        },
                        {
                            "text": "She is a hardworking single <w>mom</w> with two children.",
                            "target_lang_translations": {
                                "ru": "Она трудолюбивая одинокая <w>мама</w> с двумя детьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "Mother",
                        "Mommy",
                        "Mum",
                        "Mam"
                    ],
                    "antonyms": [
                        "Dad"
                    ],
                    "common_phrases": [
                        "single mom",
                        "soccer mom",
                        "stay-at-home mom",
                        "tiger mom"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина-родитель; мать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мама",
                                "target_lang_sense_clarification": "общеупотребительное, нейтральное или ласковое"
                            },
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "более формальное или строгое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49be5914-d432-3d0b-884f-a41855368176",
                    "sense_definition": "A term of address for one's wife, typically used by a husband who has children with her.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mother_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>mom</w>, have you seen the car keys anywhere?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>мать</w>, ты нигде не видела ключи от машины?"
                            }
                        },
                        {
                            "text": "I think <w>mom</w> deserves a nice dinner out tonight.",
                            "target_lang_translations": {
                                "ru": "Я думаю, <w>мама</w> заслужила хороший ужин в ресторане сегодня вечером."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращение к жене, обычно используемое мужем, у которого с ней есть общие дети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "разговорное обращение мужа к жене"
                            },
                            {
                                "target_lang_word": "мама",
                                "target_lang_sense_clarification": "в присутствии детей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92becee3-de74-3850-92f7-fb7432dd604c",
                    "sense_definition": "A female owner of a pet, often referring to herself in relation to the animal.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mother_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The puppy ran straight to his <w>mom</w> when he got scared.",
                            "target_lang_translations": {
                                "ru": "Щенок побежал прямо к своей <w>маме</w>, когда испугался."
                            }
                        },
                        {
                            "text": "As a dedicated dog <w>mom</w>, she takes her pet everywhere.",
                            "target_lang_translations": {
                                "ru": "Будучи преданной «собачьей <w>мамой</w>», она берет питомца везде с собой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dog mom",
                        "cat mom"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Владелица домашнего животного, часто называющая себя так по отношению к питомцу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мама",
                                "target_lang_sense_clarification": "в контексте отношений с питомцем"
                            }
                        ]
                    }
                }
            ]
        },
        {

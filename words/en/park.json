    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "37486f85-f6cd-35ab-a619-c0d117920abc",
                    "sense_definition": "An area of land, often containing trees and grass, set aside for recreation and public use in a town or city, or for environmental preservation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "land_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went for a long walk in the <w>park</w> on Sunday afternoon.",
                            "target_lang_translations": {
                                "ru": "В воскресенье днем мы отправились на долгую прогулку в <w>парк</w>."
                            }
                        },
                        {
                            "text": "Children love playing on the swings and slides at the local <w>park</w>.",
                            "target_lang_translations": {
                                "ru": "Дети любят играть на качелях и горках в местном <w>парке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "national park",
                        "amusement park",
                        "walk in the park",
                        "central park"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, часто с деревьями и травой, предназначенный для отдыха и общественного пользования в городе или для охраны окружающей среды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парк",
                                "target_lang_sense_clarification": "общественное место отдыха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "781f4a44-4ec6-356e-a572-2bb3deddfd91",
                    "sense_definition": "An area of land designed and zoned for a specific purpose, such as business, industry, or science.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "land_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new office is located in a large business <w>park</w> outside the city.",
                            "target_lang_translations": {
                                "ru": "Новый офис расположен в большом бизнес-<w>парке</w> за городом."
                            }
                        },
                        {
                            "text": "Many technology companies have their headquarters in the science <w>park</w>.",
                            "target_lang_translations": {
                                "ru": "Штаб-квартиры многих технологических компаний находятся в научном <w>парке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "business park",
                        "industrial park",
                        "science park"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, спроектированная и зонированная для конкретной цели, например, для бизнеса, промышленности или науки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парк",
                                "target_lang_sense_clarification": "индустриальный, научный или бизнес-парк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48e5d2dc-da5c-3425-b538-7884de43479b",
                    "sense_definition": "The gear setting in an automatic vehicle transmission that locks the transmission to prevent the vehicle from moving.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure the car is in <w>park</w> before you turn off the engine.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что машина стоит на <w>паркинге</w>, прежде чем выключить двигатель."
                            }
                        },
                        {
                            "text": "She shifted the gear stick into <w>park</w> and unbuckled her seatbelt.",
                            "target_lang_translations": {
                                "ru": "Она перевела рычаг переключения передач в режим <w>парковки</w> и отстегнула ремень безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "put it in park",
                        "shift to park"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение шестерни в автоматической коробке передач, которое блокирует трансмиссию, предотвращая движение автомобиля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паркинг",
                                "target_lang_sense_clarification": "режим АКПП"
                            },
                            {
                                "target_lang_word": "режим парковки",
                                "target_lang_sense_clarification": "положение селектора передач"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96ef6043-39fb-38a1-bed9-7c9e2b60e6cd",
                    "sense_definition": "A designated space in which to leave a car.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took us twenty minutes to find a <w>park</w> near the stadium.",
                            "target_lang_translations": {
                                "ru": "Нам потребовалось двадцать минут, чтобы найти <w>парковку</w> возле стадиона."
                            }
                        },
                        {
                            "text": "That <w>park</w> is too small for my truck to fit into.",
                            "target_lang_translations": {
                                "ru": "Это <w>парковочное место</w> слишком маленькое, чтобы мой грузовик мог туда поместиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "find a park",
                        "car park"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia, New Zealand"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально отведенное место, где можно оставить автомобиль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парковка",
                                "target_lang_sense_clarification": "место для стоянки"
                            },
                            {
                                "target_lang_word": "парковочное место",
                                "target_lang_sense_clarification": "конкретное место для одной машины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9c09d52-93b6-3f64-818e-16b350dce2b2",
                    "sense_definition": "An area on which a sporting match is played, especially a soccer pitch.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "land_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The players ran onto the <w>park</w> to begin the second half of the match.",
                            "target_lang_translations": {
                                "ru": "Игроки выбежали на <w>поле</w>, чтобы начать второй тайм."
                            }
                        },
                        {
                            "text": "He is known as one of the best midfielders in the middle of the <w>park</w>.",
                            "target_lang_translations": {
                                "ru": "Он известен как один из лучших полузащитников в центре <w>поля</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "middle of the park"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Площадка, на которой проводится спортивный матч, особенно футбольное поле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "футбольное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10f36111-6b56-3d54-bc6f-3ec8d7d4630b",
                    "sense_definition": "A wide, flat-bottomed valley in a mountainous region.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "land_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The herd of elk grazed peacefully in the high mountain <w>park</w>.",
                            "target_lang_translations": {
                                "ru": "Стадо лосей мирно паслось в высокогорной <w>долине</w>."
                            }
                        },
                        {
                            "text": "We camped in a beautiful <w>park</w> surrounded by snowy peaks.",
                            "target_lang_translations": {
                                "ru": "Мы разбили лагерь в красивой <w>долине</w>, окруженной снежными вершинами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Western US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкая долина с плоским дном в горной местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долина",
                                "target_lang_sense_clarification": "горная"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "527a963e-f4b1-34e8-bb89-47ff92996f54",
                    "sense_definition": "To bring a vehicle to a halt and leave it in a specific place for a period of time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>park</w> your car in the driveway overnight.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>припарковать</w> машину на подъездной дорожке на ночь."
                            }
                        },
                        {
                            "text": "It is difficult to <w>park</w> in the city center during rush hour.",
                            "target_lang_translations": {
                                "ru": "В центре города трудно <w>припарковаться</w> в час пик."
                            }
                        }
                    ],
                    "antonyms": [
                        "unpark"
                    ],
                    "common_phrases": [
                        "park the car",
                        "double park",
                        "park illegally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить транспортное средство и оставить его в определенном месте на некоторое время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парковаться",
                                "target_lang_sense_clarification": "совершать парковку"
                            },
                            {
                                "target_lang_word": "парковать",
                                "target_lang_sense_clarification": "ставить машину на стоянку"
                            },
                            {
                                "target_lang_word": "ставить на стоянку",
                                "target_lang_sense_clarification": "оставлять машину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d4f9967-8590-3ce8-90db-be7470e3742b",
                    "sense_definition": "To sit down or put something in a specific place, often casually.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Just <w>park</w> yourself on the sofa and I will get you a drink.",
                            "target_lang_translations": {
                                "ru": "Просто <w>усаживайся</w> на диван, а я принесу тебе выпить."
                            }
                        },
                        {
                            "text": "He <w>parked</w> his heavy suitcase by the door and sighed.",
                            "target_lang_translations": {
                                "ru": "Он <w>поставил</w> свой тяжелый чемодан у двери и вздохнул."
                            }
                        }
                    ],
                    "common_phrases": [
                        "park yourself"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сесть или положить что-то в определенное место, часто небрежно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приземлиться",
                                "target_lang_sense_clarification": "сесть, устроиться"
                            },
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "поместить предмет"
                            },
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "небрежно положить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57673a7d-f08d-343b-abac-20ed713c9b8c",
                    "sense_definition": "To delay or postpone a discussion or decision until a later time.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deferral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We should <w>park</w> this topic for now and discuss it at the next meeting.",
                            "target_lang_translations": {
                                "ru": "Нам следует <w>отложить</w> эту тему на данный момент и обсудить её на следующей встрече."
                            }
                        },
                        {
                            "text": "Let's <w>park</w> the budget discussion until we have the final figures.",
                            "target_lang_translations": {
                                "ru": "Давайте <w>отложим</w> обсуждение бюджета, пока у нас не будет окончательных цифр."
                            }
                        }
                    ],
                    "common_phrases": [
                        "park that idea"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Business context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложить или перенести обсуждение или решение на более позднее время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отложить",
                                "target_lang_sense_clarification": "перенести обсуждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9686bb3b-85d5-3942-b3be-b4ad4412078b",
                    "sense_definition": "To invest money temporarily in a safe investment instrument.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investors are looking for a safe place to <w>park</w> their cash during the recession.",
                            "target_lang_translations": {
                                "ru": "Инвесторы ищут безопасное место, чтобы <w>разместить</w> свои наличные во время рецессии."
                            }
                        },
                        {
                            "text": "He decided to <w>park</w> his savings in a high-interest account.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>вложить</w> свои сбережения на счет с высокой процентной ставкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "park money"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно вложить деньги в надежный инвестиционный инструмент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разместить",
                                "target_lang_sense_clarification": "капитал или средства"
                            },
                            {
                                "target_lang_word": "вложить",
                                "target_lang_sense_clarification": "инвестировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d08ecfee-c709-3dae-a568-e73831c19395",
                    "sense_definition": "To stop a car in a secluded spot to engage in romantic activity.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "vehicle_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teenagers used to go <w>parking</w> up at the old lookout point.",
                            "target_lang_translations": {
                                "ru": "Подростки раньше ездили <w>уединяться в машинах</w> на старую смотровую площадку."
                            }
                        },
                        {
                            "text": "They were caught <w>parking</w> by a patrolling police officer.",
                            "target_lang_translations": {
                                "ru": "Патрульный полицейский застал их, когда они <w>уединились в машине</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go parking"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить машину в уединенном месте для романтических отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уединяться в машине",
                                "target_lang_sense_clarification": "для романтики"
                            }
                        ]
                    }
                }
            ]
        },

                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "совсем недавно"
                            },
                            {
                                "target_lang_word": "чуть",
                                "target_lang_sense_clarification": "немного больше"
                            }
                        ]
                    }
                }

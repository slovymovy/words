            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4374634a-f11f-3269-bc1b-75ec313f0f61",
                    "sense_definition": "Zwiększać temperaturę czegoś, dostarczać czemuś ciepła, czynić coś cieplejszym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "heat_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą musimy intensywnie <w>ogrzewać</w> nasze mieszkanie, aby utrzymać w nim komfortową temperaturę.",
                            "target_lang_translations": {
                                "ru": "Зимой нам приходится интенсивно <w>отапливать</w> нашу квартиру, чтобы поддерживать в ней комфортную температуру."
                            }
                        },
                        {
                            "text": "Promienie wiosennego słońca zaczęły powoli <w>ogrzewać</w> zmarzniętą ziemię po długiej zimie.",
                            "target_lang_translations": {
                                "ru": "Лучи весеннего солнца начали медленно <w>согревать</w> замёрзшую землю после долгой зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "grzać",
                        "ocieplać"
                    ],
                    "antonyms": [
                        "chłodzić",
                        "studzić",
                        "oziębiać",
                        "wychładzać"
                    ],
                    "common_phrases": [
                        "ogrzewać dom",
                        "ogrzewać budynek",
                        "ogrzewać wodę",
                        "ogrzewać ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повышать температуру чего-либо, снабжать теплом, делать что-либо более тёплым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согревать",
                                "target_lang_sense_clarification": "делать тёплым, передавать тепло"
                            },
                            {
                                "target_lang_word": "отапливать",
                                "target_lang_sense_clarification": "обогревать помещение с помощью системы отопления"
                            },
                            {
                                "target_lang_word": "обогревать",
                                "target_lang_sense_clarification": "снабжать теплом пространство или поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c99af49f-fa69-33cc-ab4c-837bb245f5f5",
                    "sense_definition": "Stawać się cieplejszym, dostarczać sobie ciepła, czerpać ciepło z jakiegoś źródła (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmarznięci turyści usiedli blisko kominka, aby <w>ogrzewać się</w> po całym dniu jazdy na nartach.",
                            "target_lang_translations": {
                                "ru": "Замёрзшие туристы сели поближе к камину, чтобы <w>согреться</w> после целого дня катания на лыжах."
                            }
                        },
                        {
                            "text": "Koty uwielbiają leżeć na parapecie i <w>ogrzewać się</w> w ciepłych promieniach słońca.",
                            "target_lang_translations": {
                                "ru": "Кошки обожают лежать на подоконнике и <w>греться</w> в тёплых лучах солнца."
                            }
                        }
                    ],
                    "synonyms": [
                        "grzać się"
                    ],
                    "antonyms": [
                        "chłodzić się",
                        "marznąć",
                        "ziębnąć"
                    ],
                    "common_phrases": [
                        "ogrzewać się przy ognisku",
                        "ogrzewać się w słońcu",
                        "ogrzewać się herbatą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться теплее, согревать себя, получать тепло от какого-либо источника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согреваться",
                                "target_lang_sense_clarification": "становиться тёплым, переставать мёрзнуть"
                            },
                            {
                                "target_lang_word": "греться",
                                "target_lang_sense_clarification": "находиться возле источника тепла"
                            }
                        ]
                    }
                }
            ]
        }
    ]

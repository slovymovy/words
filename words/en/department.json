                            }
                        ]
                    }
                },
                {
                    "sense_id": "33f76709-ba29-3b28-97ec-90cc75cf0981",
                    "sense_definition": "The act of leaving or departing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_of_leaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His sudden <w>department</w> from the court surprised the king.",
                            "target_lang_translations": {
                                "ru": "Его внезапный <w>уход</w> со двора удивил короля."
                            }
                        },
                        {
                            "text": "We mourned the <w>department</w> of our dear friend from this life.",
                            "target_lang_translations": {
                                "ru": "Мы оплакивали <w>уход</w> нашего дорогого друга из этой жизни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт ухода или отъезда (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уход",
                                "target_lang_sense_clarification": "покидание места или должности"
                            },
                            {
                                "target_lang_word": "отъезд",
                                "target_lang_sense_clarification": "отправление в путь"
                            }
                        ]
                    }
                }
            ]
        }

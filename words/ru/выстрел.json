            "senses": [
                {
                    "sense_id": "a15573b2-8bb1-3e58-87d9-3ac20875121f",
                    "sense_definition": "Действие по глаголу «стрелять»; выбрасывание пули или снаряда из огнестрельного оружия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотник прицелился и сделал один меткий <w>выстрел</w>."
                        },
                        {
                            "text": "Каждый <w>выстрел</w> в тире приближал его к победе в соревновании."
                        }
                    ],
                    "common_phrases": [
                        "произвести выстрел",
                        "меткий выстрел",
                        "случайный выстрел",
                        "предупредительный выстрел"
                    ]
                },
                {
                    "sense_id": "db0778b4-1164-338f-931f-8c79513859fa",
                    "sense_definition": "Резкий и громкий звук, возникающий при стрельбе из оружия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ночной тишине неожиданно раздался громкий <w>выстрел</w>."
                        },
                        {
                            "text": "Мы услышали <w>выстрел</w> и сразу побежали в укрытие."
                        }
                    ],
                    "synonyms": [
                        "хлопок"
                    ],
                    "common_phrases": [
                        "звук выстрела",
                        "услышать выстрел",
                        "одиночный выстрел"
                    ]
                },
                {
                    "sense_id": "304dafbe-2e57-348e-871c-2b1f8295b281",
                    "sense_definition": "Расстояние, которое может пролететь пуля или снаряд; дальность стрельбы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осторожный зверь не подпускал охотников на верный <w>выстрел</w>."
                        },
                        {
                            "text": "К этому старому замку никто не подходил даже на пушечный <w>выстрел</w>."
                        }
                    ],
                    "synonyms": [
                        "дальность"
                    ],
                    "common_phrases": [
                        "на пушечный выстрел",
                        "подойти на выстрел"
                    ]
                },
                {
                    "sense_id": "2e995f10-b792-351d-a5c5-8793424c604a",
                    "sense_definition": "Воен. Комплект боеприпасов (снаряд, заряд, гильза) для стрельбы из артиллерийских орудий.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты аккуратно укладывали артиллерийские <w>выстрелы</w> в ящики."
                        },
                        {
                            "text": "Для этого танка требуются специальные <w>выстрелы</w> повышенной мощности."
                        }
                    ],
                    "synonyms": [
                        "боеприпас",
                        "снаряд"
                    ],
                    "common_phrases": [
                        "артиллерийский выстрел",
                        "комплект выстрелов"
                    ]
                },
                {
                    "sense_id": "b7555c2d-378e-3296-83a4-75859aea7f69",
                    "sense_definition": "Морск. Длинная балка на судне, выступающая за борт, для крепления шлюпок или парусов.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Матросы получили приказ закрепить шлюпку под <w>выстрел</w>."
                        },
                        {
                            "text": "Во время шторма <w>выстрел</w> был поврежден ударом волны."
                        }
                    ]
                }
            ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b1462f5-6f1f-3120-b426-b8ddfa61a0ba",
                    "sense_definition": "To, co przynosi ulgę w smutku, uspokojenie, wsparcie duchowe lub psychiczne; stan ukojenia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "comfort_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach muzyka była dla niej jedyną <w>pociechą</w>.",
                            "target_lang_translations": {
                                "ru": "В трудные минуты музыка была для неё единственным <w>утешением</w>."
                            }
                        },
                        {
                            "text": "Rozmowa z przyjacielem przyniosła mu wielką <w>pociechę</w> po stracie pracy.",
                            "target_lang_translations": {
                                "ru": "Разговор с другом принес ему большое <w>утешение</w> после потери работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "otucha",
                        "ukojenie",
                        "wsparcie",
                        "pocieszenie"
                    ],
                    "antonyms": [
                        "zmartwienie",
                        "rozpacz",
                        "smutek",
                        "zgryzota"
                    ],
                    "common_phrases": [
                        "na pociechę",
                        "szukać pociechy",
                        "marna pociecha",
                        "słowa pociechy"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что приносит облегчение в печали, успокоение, духовную или психологическую поддержку; состояние покоя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утешение",
                                "target_lang_sense_clarification": "облегчение в горе или печали"
                            },
                            {
                                "target_lang_word": "отрада",
                                "target_lang_sense_clarification": "источник успокоения и радости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a6c9438-d5fd-31bc-a26b-0bec06aafc51",
                    "sense_definition": "Pieszczotliwe lub żartobliwe określenie dziecka.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "child_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W weekendy rodzice często zabierają swoje <w>pociechy</w> do parku rozrywki.",
                            "target_lang_translations": {
                                "ru": "По выходным родители часто берут своих <w>чад</w> в парк развлечений."
                            }
                        },
                        {
                            "text": "Muszę odebrać moją małą <w>pociechę</w> z przedszkola przed godziną siedemnastą.",
                            "target_lang_translations": {
                                "ru": "Мне нужно забрать мое маленькое <w>чадо</w> из детского сада до пяти часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziecko",
                        "maluch",
                        "latorośl",
                        "szkrab",
                        "milusiński"
                    ],
                    "common_phrases": [
                        "nasze pociechy",
                        "zabrać pociechy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое или шутливое обозначение ребенка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чадо",
                                "target_lang_sense_clarification": "шутливо или ласково о ребенке"
                            },
                            {
                                "target_lang_word": "ребёнок",
                                "target_lang_sense_clarification": "нейтральное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ad5f12f-e0f6-3cd7-a524-127930deff78",
                    "sense_definition": "Osoba, rzecz lub sytuacja będąca źródłem radości, dumy i zadowolenia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "comfort_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zdolny uczeń to prawdziwa <w>pociecha</w> dla każdego nauczyciela.",
                            "target_lang_translations": {
                                "ru": "Этот способный ученик — настоящая <w>отрада</w> для каждого учителя."
                            }
                        },
                        {
                            "text": "Babcia ma wiele <w>pociechy</w> ze swoich dorastających wnuków.",
                            "target_lang_translations": {
                                "ru": "Бабушке много <w>радости</w> от её подрастающих внуков."
                            }
                        }
                    ],
                    "synonyms": [
                        "radość",
                        "duma",
                        "uciecha",
                        "zadowolenie"
                    ],
                    "antonyms": [
                        "utrapienie",
                        "kłopot",
                        "ciężar"
                    ],
                    "common_phrases": [
                        "mieć pociechę",
                        "ku pociesze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, предмет или ситуация, являющиеся источником радости, гордости и удовлетворения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрада",
                                "target_lang_sense_clarification": "то, что доставляет удовольствие и радость"
                            },
                            {
                                "target_lang_word": "радость",
                                "target_lang_sense_clarification": "чувство удовольствия или его источник"
                            }
                        ]
                    }
                }
            ]
        },
        {

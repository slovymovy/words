    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "607ec285-051c-3414-afc6-aa26b18aedea",
                    "sense_definition": "Gebruikt om een ontkenning tegen te spreken of extra nadruk te leggen op een bevestiging (het tegenovergestelde van 'niet').",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis_affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik dacht dat je niet wilde gaan, maar ik wil <wel> gaan.",
                            "target_lang_translations": {
                                "en": "I thought you didn't want to go, but I <do> want to go.",
                                "ru": "Я думал, ты не хочешь идти, но я <всё-таки> хочу."
                            }
                        },
                        {
                            "text": "Hij heeft het <wel> gedaan, ook al zegt hij van niet.",
                            "target_lang_translations": {
                                "en": "He <did> do it, even though he says he didn't.",
                                "ru": "Он <всё-таки> сделал это, хотя и говорит, что нет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wel of niet",
                        "wel degelijk",
                        "dank je wel"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to contradict a negative statement or to add emphasis to an affirmation.",
                        "ru": "Используется для возражения на отрицание или для усиления утверждения (противоположность 'не')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "emphatic auxiliary used to contradict a negative"
                            },
                            {
                                "target_lang_word": "actually",
                                "target_lang_sense_clarification": "indicating something is true despite doubt"
                            },
                            {
                                "target_lang_word": "indeed",
                                "target_lang_sense_clarification": "formal emphasis of affirmation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "всё-таки",
                                "target_lang_sense_clarification": "усиление утверждения вопреки сомнениям"
                            },
                            {
                                "target_lang_word": "действительно",
                                "target_lang_sense_clarification": "подтверждение факта"
                            },
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "частица для эмоционального выделения"
                            }
                        ]
                    }
                },
                {

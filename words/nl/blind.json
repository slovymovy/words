            "senses": [
                {
                    "sense_id": "7d826d54-5dca-3d8f-a46f-9ae2e9c09ccb",
                    "sense_definition": "Niet in staat om te zien; zonder gezichtsvermogen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_blindness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>blinde</w> man liep voorzichtig met zijn stok over straat.",
                            "target_lang_translations": {
                                "en": "The <w>blind</w> man walked carefully down the street with his cane.",
                                "ru": "<w>Слепой</w> мужчина осторожно шел с тростью по улице."
                            }
                        },
                        {
                            "text": "Zij is al sinds haar geboorte volledig <w>blind</w> aan beide ogen.",
                            "target_lang_translations": {
                                "en": "She has been completely <w>blind</w> in both eyes since birth.",
                                "ru": "Она с самого рождения полностью <w>слепа</w> на оба глаза."
                            }
                        }
                    ],
                    "antonyms": [
                        "ziend"
                    ],
                    "common_phrases": [
                        "blind zijn",
                        "blind geboren worden"
                    ],
                    "target_lang_definitions": {
                        "en": "Unable to see; without eyesight.",
                        "ru": "Неспособный видеть; лишенный зрения."
                    },
                    "translations": {

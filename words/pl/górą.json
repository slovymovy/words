    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7c9f03e6-2d45-37df-a557-a92fc23035d6",
                    "sense_definition": "w górnej części, na pewnej wysokości, ponad czymś",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te kable puścimy teraz <w>górą</w>, tuż pod sufitem.",
                            "target_lang_translations": {
                                "ru": "Эти кабели мы пустим <w>поверху</w>, прямо под потолком."
                            }
                        },
                        {
                            "text": "Ptaki przelatywały <w>górą</w> nad naszymi głowami.",
                            "target_lang_translations": {
                                "ru": "Птицы пролетали <w>верхом</w> над нашими головами."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyżej",
                        "wysoko"
                    ],
                    "antonyms": [
                        "dołem"
                    ],
                    "common_phrases": [
                        "lecieć górą",
                        "iść górą"
                    ],
                    "target_lang_definitions": {
                        "ru": "в верхней части, на высоте, поверху"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поверху",
                                "target_lang_sense_clarification": "в верхней части пространства"
                            },
                            {
                                "target_lang_word": "верхом",
                                "target_lang_sense_clarification": "по верхней части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2c19635-bf84-360d-b79d-bdd08aebefd4",
                    "sense_definition": "drogą prowadzącą przez wzniesienie lub szczyt",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_path",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ty pójdziesz <w>górą</w>, a ja pójdę doliną.",
                            "target_lang_translations": {
                                "ru": "Ты пойдешь <w>горой</w>, а я пойду долиной."
                            }
                        },
                        {
                            "text": "Szlak turystyczny prowadzi <w>górą</w>, omijając dno wąwozu.",
                            "target_lang_translations": {
                                "ru": "Туристическая тропа ведет <w>верхом</w>, минуя дно ущелья."
                            }
                        }
                    ],
                    "synonyms": [
                        "górami"
                    ],
                    "antonyms": [
                        "doliną",
                        "równiną"
                    ],
                    "common_phrases": [
                        "pójść górą"
                    ],
                    "target_lang_definitions": {
                        "ru": "дорогой, ведущей через возвышенность или вершину"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горой",
                                "target_lang_sense_clarification": "путь через гору"
                            },
                            {
                                "target_lang_word": "верхом",
                                "target_lang_sense_clarification": "путь по возвышенности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "962e3021-9abf-3742-9f91-3b80189d45cb",
                    "sense_definition": "w sytuacji przewagi, zwycięstwa nad kimś lub czymś",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wygraliśmy ten trudny mecz, znowu jesteśmy <w>górą</w>!",
                            "target_lang_translations": {
                                "ru": "Мы выиграли этот трудный матч, мы снова <w>взяли верх</w>!"
                            }
                        },
                        {
                            "text": "W tym sporze rozsądek ostatecznie był <w>górą</w>.",
                            "target_lang_translations": {
                                "ru": "В этом споре здравый смысл в конечном счете <w>взял верх</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być górą"
                    ],
                    "target_lang_definitions": {
                        "ru": "в ситуации превосходства, победы над кем-то или чем-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взять верх",
                                "target_lang_sense_clarification": "одержать победу (фразеологизм)"
                            },
                            {
                                "target_lang_word": "победить",
                                "target_lang_sense_clarification": "выиграть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7dbbf13f-c740-3ad8-8d58-5cd95088ead0",
                    "sense_definition": "forma fleksyjna (narzędnik liczby pojedynczej) rzeczownika <w>góra</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce powoli zachodziło za wysoką <w>górą</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце медленно садилось за высокой <w>горой</w>."
                            }
                        },
                        {
                            "text": "Miasto leży tuż za tą <w>górą</w>, którą widzisz w oddali.",
                            "target_lang_translations": {
                                "ru": "Город лежит прямо за той <w>горой</w>, которую ты видишь вдалеке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "narzędnik liczby pojedynczej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "творительный падеж существительного \"góra\" (гора)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горой",
                                "target_lang_sense_clarification": "творительный падеж слова \"гора\""
                            }
                        ]
                    }
                }
            ]
        }

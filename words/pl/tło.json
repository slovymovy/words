                        "przód"
                    ],
                    "common_phrases": [
                        "w tle",
                        "na tle nieba",
                        "pozostać w tle",
                        "góry w tle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задний план вида, картины или сцены, расположенный за главным объектом."
                    },
                    "translations": {
                        "ru": [
                            {

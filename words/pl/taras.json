            "senses": [
                {
                    "sense_id": "21cfcb67-0150-3d70-93dd-b8f1ef68ffb5",
                    "sense_definition": "Odkryta, płaska część budynku, znajdująca się na parterze, piętrze lub dachu, przeznaczona do wypoczynku",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem cała rodzina lubi jeść śniadania na słonecznym <w>tarasie</w>.",
                            "target_lang_translations": {
                                "ru": "Летом вся семья любит завтракать на солнечной <w>террасе</w>."
                            }
                        },
                        {
                            "text": "Z naszego pokoju hotelowego jest bezpośrednie wyjście na <w>taras</w> z widokiem na morze.",
                            "target_lang_translations": {
                                "ru": "Из нашего гостиничного номера есть прямой выход на <w>террасу</w> с видом на море."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjść na taras",
                        "siedzieć na tarasie",
                        "taras widokowy",
                        "duży taras"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытая, плоская часть здания, расположенная на первом этаже, этаже или крыше, предназначенная для отдыха"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терраса",
                                "target_lang_sense_clarification": "архитектурное сооружение"
                            },
                            {
                                "target_lang_word": "веранда",
                                "target_lang_sense_clarification": "пристройка к дому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1bbd830-a460-36ca-9383-534739ed0334",
                    "sense_definition": "Płaski stopień terenu uformowany naturalnie lub sztucznie na zboczu góry, w dolinie rzecznej lub na wzniesieniu",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy w Azji często uprawiają ryż na stromych <w>tarasach</w> górskich.",
                            "target_lang_translations": {
                                "ru": "Фермеры в Азии часто выращивают рис на крутых горных <w>террасах</w>."
                            }
                        },
                        {
                            "text": "Rzeka płynąca w dole wyżłobiła szerokie <w>tarasy</w> rzeczne.",
                            "target_lang_translations": {
                                "ru": "Река, текущая внизу, вымыла широкие речные <w>террасы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "półka"
                    ],
                    "common_phrases": [
                        "taras rzeczny",
                        "tarasy ryżowe",
                        "uprawa tarasowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский уступ рельефа, сформированный естественно или искусственно на склоне горы или в долине"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терраса",
                                "target_lang_sense_clarification": "геологический или сельскохозяйственный уступ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7a955e4-d407-3ba0-87ed-e8026e938755",
                    "sense_definition": "Dawne umocnienie ziemne lub drewniane pełniące funkcję obronną lub zabezpieczającą",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>taras</w> chronił zamek przed bezpośrednim atakiem wroga.",
                            "target_lang_translations": {
                                "ru": "Старый <w>вал</w> защищал замок от прямой атаки врага."
                            }
                        },
                        {
                            "text": "Woda wezbrała tak szybko, że przerwała <w>taras</w> i zalała okoliczne pola.",
                            "target_lang_translations": {
                                "ru": "Вода поднялась так быстро, что прорвала <w>дамбу</w> и затопила окрестные поля."
                            }
                        }
                    ],
                    "synonyms": [

            "senses": [
                {
                    "sense_id": "f4c53ae3-9db5-33a8-8156-1251154045b9",
                    "sense_definition": "Describing a microphone pattern that picks up sound primarily from the front and sides while rejecting sound from the rear.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "audio_technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>cardioid</w> microphone is ideal for recording vocals in a noisy room.",
                            "target_lang_translations": {
                                "ru": "<w>Кардиоидный</w> микрофон идеально подходит для записи вокала в шумном помещении."
                            }
                        },
                        {
                            "text": "The engineer selected a <w>cardioid</w> pickup pattern to isolate the singer's voice.",
                            "target_lang_translations": {
                                "ru": "Звукорежиссер выбрал <w>кардиоидную</w> диаграмму направленности, чтобы изолировать голос певца."

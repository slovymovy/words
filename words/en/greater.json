            "senses": [
                {
                    "sense_id": "4d276228-a20d-3090-8e54-535a9eb48c5d",
                    "sense_definition": "Comparative form of <w>great</w>: more great.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The demand for new housing is <w>greater</w> than the supply.",
                            "target_lang_translations": {
                                "ru": "Спрос на новое жилье <w>бо́льше</w>, чем предложение."
                            }
                        },
                        {
                            "text": "You need to pay <w>greater</w> attention to the small details.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно уделять <w>бо́льшее</w> внимание мелким деталям."
                            }
                        }
                    ],
                    "antonyms": [
                        "lesser"
                    ],
                    "common_phrases": [
                        "greater than",
                        "greater number",
                        "greater risk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень прилагательного \"great\": более великий, значительный или крупный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бо́льший",
                                "target_lang_sense_clarification": "превосходящий по размеру или количеству"
                            },
                            {
                                "target_lang_word": "бо́лее значи́тельный",
                                "target_lang_sense_clarification": "более важный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63f42b6a-34ef-3382-9a47-f29ea3184ef5",
                    "sense_definition": "Larger in size, extent, or importance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spent the <w>greater</w> part of the day reading in the garden.",
                            "target_lang_translations": {
                                "ru": "Она провела <w>бо́льшую</w> часть дня, читая в саду."
                            }
                        },
                        {
                            "text": "We must work together for the <w>greater</w> good of the community.",
                            "target_lang_translations": {
                                "ru": "Мы должны работать вместе ради <w>общего</w> блага общества."
                            }
                        }
                    ],
                    "antonyms": [
                        "lesser"
                    ],
                    "common_phrases": [
                        "the greater part",
                        "greater good",
                        "greater share"
                    ],
                    "target_lang_definitions": {
                        "ru": "Более крупный по размеру, объему или важности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бо́льший",
                                "target_lang_sense_clarification": "основной, преобладающий"
                            },
                            {
                                "target_lang_word": "значи́тельный",
                                "target_lang_sense_clarification": "существенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3544aca5-379d-32da-8065-896c02d97ec9",
                    "sense_definition": "Used in the names of cities to refer to the city and its surrounding urban areas.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He lives in <w>Greater</w> Manchester but works in the city center.",
                            "target_lang_translations": {
                                "ru": "Он живет в <w>Большо́м</w> Манчестере, но работает в центре города."
                            }
                        },
                        {
                            "text": "The population of <w>Greater</w> Tokyo is one of the highest in the world.",
                            "target_lang_translations": {
                                "ru": "Население <w>Большо́го</w> Токио — одно из самых высоких в мире."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Greater London",
                        "Greater New York"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в названиях городов для обозначения города вместе с пригородами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Большо́й",
                                "target_lang_sense_clarification": "в географических названиях"

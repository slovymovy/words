    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3952d826-c227-3573-bfdb-c113f6d39e67",
                    "sense_definition": "Umowny znak, dźwięk lub gest służący do przekazania informacji, ostrzeżenia lub rozpoczęcia działania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca dał <w>sygnał</w> ręką, że zamierza skręcić w prawo.",
                            "target_lang_translations": {
                                "ru": "Водитель подал <w>сигнал</w> рукой, что собирается повернуть направо."
                            }
                        },
                        {
                            "text": "Czerwone światło to <w>sygnał</w>, żeby się zatrzymać przed pasami.",
                            "target_lang_translations": {
                                "ru": "Красный свет — это <w>сигнал</w> остановиться перед пешеходным переходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "cynk"
                    ],
                    "common_phrases": [
                        "sygnał dźwiękowy",
                        "sygnał świetlny",
                        "dać sygnał",
                        "sygnał do startu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Условный знак, звук или жест, служащий для передачи информации, предупреждения или начала действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "условный знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20801b6c-8784-3a4d-821b-533ca3f3e550",
                    "sense_definition": "Fale radiowe lub impulsy elektryczne przenoszące dane; jakość połączenia w urządzeniach elektronicznych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W piwnicy mój telefon często gubi <w>sygnał</w>.",
                            "target_lang_translations": {
                                "ru": "В подвале мой телефон часто теряет <w>сигнал</w>."
                            }
                        },
                        {
                            "text": "Telewizor nie odbiera żadnego <w>sygnału</w> z anteny satelitarnej.",
                            "target_lang_translations": {
                                "ru": "Телевизор не принимает никакого <w>сигнала</w> со спутниковой антенны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brak sygnału",
                        "sygnał Wi-Fi",
                        "sygnał GPS",
                        "szukać sygnału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Радиоволны или электрические импульсы, передающие данные; качество соединения в электронных устройствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "связь, радиоволны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03df7292-c492-3e61-a8c0-37db79bfd50e",
                    "sense_definition": "Charakterystyczny dźwięk w słuchawce telefonicznej informujący o stanie połączenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po usłyszeniu <w>sygnału</w> proszę zostawić wiadomość na poczcie głosowej.",
                            "target_lang_translations": {
                                "ru": "После звукового <w>сигнала</w> оставьте сообщение на голосовой почте."
                            }
                        },
                        {
                            "text": "W słuchawce nie ma <w>sygnału</w>, chyba linia jest uszkodzona.",
                            "target_lang_translations": {
                                "ru": "В трубке нет <w>гудка</w>, наверное, линия повреждена."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sygnał zajętości",
                        "sygnał w słuchawce",
                        "czekać na sygnał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерный звук в телефонной трубке, сообщающий о статусе соединения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гудок",
                                "target_lang_sense_clarification": "в телефоне"
                            },
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "звуковое оповещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67d4521d-600b-37b9-a351-3cb0f3862a9d",
                    "sense_definition": "Zdarzenie lub zjawisko, które zapowiada nadejście czegoś lub świadczy o istnieniu jakiegoś problemu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pierwsze bociany to wyraźny <w>sygnał</w>, że nadchodzi wiosna.",
                            "target_lang_translations": {
                                "ru": "Первые аисты — это явный <w>сигнал</w>, что приближается весна."
                            }
                        },
                        {
                            "text": "Ból głowy może być <w>sygnałem</w>, że organizm potrzebuje odpoczynku.",
                            "target_lang_translations": {
                                "ru": "Головная боль может быть <w>сигналом</w>, что организму нужен отдых."
                            }
                        }
                    ],
                    "synonyms": [
                        "cynk"
                    ],
                    "common_phrases": [
                        "sygnał ostrzegawczy",
                        "wyraźny sygnał",
                        "niepokojący sygnał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Событие или явление, предвещающее что-либо или указывающее на проблему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "предупреждение"
                            },
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "предзнаменование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f6d31ba-4650-3eac-9572-cee9da3c5ef1",
                    "sense_definition": "Poufna lub nieoficjalna informacja przekazana komuś w tajemnicy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja otrzymała <w>sygnał</w> o planowanym napadzie na bank.",
                            "target_lang_translations": {
                                "ru": "Полиция получила <w>сигнал</w> о планируемом ограблении банка."
                            }
                        },
                        {
                            "text": "Dostałem <w>sygnał</w> od kolegi, że szef planuje zwolnienia.",
                            "target_lang_translations": {
                                "ru": "Я получил <w>сигнал</w> от коллеги, что начальник планирует увольнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "cynk"
                    ],
                    "common_phrases": [
                        "dostać sygnał",
                        "sygnał od informatora"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секретная или неофициальная информация, переданная кому-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "сообщение"
                            },
                            {
                                "target_lang_word": "наводка",
                                "target_lang_sense_clarification": "неофициальные сведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2dc57f3-39cd-3efb-aa17-8108729039fa",
                    "sense_definition": "Bodziec fizyczny odbierany przez zmysły i przesyłany przez układ nerwowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mózg przetwarza <w>sygnały</w> płynące z oczu i uszu.",
                            "target_lang_translations": {
                                "ru": "Мозг обрабатывает <w>сигналы</w>, поступающие от глаз и ушей."
                            }
                        },
                        {
                            "text": "Uszkodzone nerwy mogą wysyłać błędne <w>sygnały</w> bólowe.",
                            "target_lang_translations": {
                                "ru": "Поврежденные нервы могут посылать ложные болевые <w>сигналы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sygnał nerwowy",
                        "przesyłanie sygnałów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический стимул, воспринимаемый чувствами и передаваемый нервной системой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "нервный импульс"
                            }
                        ]
                    }
                }
            ]
        }
    ]

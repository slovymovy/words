    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8d099b79-74ee-3fd6-89c9-0029cdb37049",
                    "sense_definition": "The anniversary of the day on which someone is born.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anniversary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I baked a chocolate cake for her <w>birthday</w>.",
                            "target_lang_translations": {
                                "ru": "Я испек шоколадный торт на её <w>день рождения</w>."
                            }
                        },
                        {
                            "text": "We always sing happy <w>birthday</w> before eating the cake.",
                            "target_lang_translations": {
                                "ru": "Мы всегда поем «С днём <w>рождения</w>» перед тем, как есть торт."
                            }
                        }
                    ],
                    "synonyms": [
                        "natal day"
                    ],
                    "common_phrases": [
                        "happy birthday",
                        "birthday party",
                        "birthday cake",
                        "birthday present",
                        "birthday card"
                    ],
                    "target_lang_definitions": {
                        "ru": "Годовщина дня, в который родился человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день рожде́ния",
                                "target_lang_sense_clarification": "основной термин"
                            },
                            {
                                "target_lang_word": "дню́ха",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "797b0d22-80d9-30d8-bf85-261a7403cae4",
                    "sense_definition": "A birthday party.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "celebration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are you coming to my <w>birthday</w> this Saturday?",
                            "target_lang_translations": {
                                "ru": "Ты придешь на мой <w>день рождения</w> в эту субботу?"
                            }
                        },
                        {
                            "text": "We had a lot of fun at his <w>birthday</w> last year.",
                            "target_lang_translations": {
                                "ru": "Мы отлично повеселились на его <w>дне рождения</w> в прошлом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "invite to my birthday",
                        "at my birthday"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечеринка или празднование в честь дня рождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день рожде́ния",
                                "target_lang_sense_clarification": "праздник, вечеринка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0d60261-70c9-37dd-997f-37c647b1e9f0",
                    "sense_definition": "The date on which someone is born or something is created, more commonly called birthdate or date of birth.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "date",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please write your full name and <w>birthday</w> on the form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите свое полное имя и <w>дату рождения</w> в бланке."
                            }
                        },
                        {
                            "text": "Do you know the exact <w>birthday</w> of the famous poet?",
                            "target_lang_translations": {
                                "ru": "Вы знаете точную <w>дату рождения</w> этого известного поэта?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "date of birth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Календарная дата, когда кто-то родился (используется в документах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да́та рожде́ния",
                                "target_lang_sense_clarification": "официально"

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "62085310-fdd1-3a17-9749-1b7c9f304ee7",
                    "sense_definition": "Powstający przy okazji innego, głównego działania lub procesu; towarzyszący czemuś, często niezamierzony.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy silny lek może wywołać niepożądany skutek <w>uboczny</w>.",
                            "target_lang_translations": {
                                "ru": "Любое сильное лекарство может вызвать нежелательный <w>побочный</w> эффект."
                            }
                        },
                        {
                            "text": "Dwutlenek węgla jest wydzielany jako produkt <w>uboczny</w> tego procesu.",
                            "target_lang_translations": {
                                "ru": "Углекислый газ выделяется как <w>побочный</w> продукт этого процесса."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieplanowany",
                        "niezamierzony",
                        "wtórny"
                    ],
                    "common_phrases": [
                        "skutek uboczny",
                        "efekt uboczny",
                        "produkt uboczny",
                        "działanie uboczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возникающий попутно при другом, основном действии или процессе; сопутствующий чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побочный",
                                "target_lang_sense_clarification": "сопутствующий (об эффекте, продукте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70814e09-8974-38d8-b843-c7305f46c591",
                    "sense_definition": "Mający mniejsze znaczenie, niebędący głównym przedmiotem zainteresowania; drugorzędny.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej powieści wątek <w>uboczny</w> jest ciekawszy niż główna historia.",
                            "target_lang_translations": {
                                "ru": "В этом романе <w>побочная</w> сюжетная линия интереснее, чем основная история."
                            }
                        },
                        {
                            "text": "Nie powinniśmy tracić czasu na sprawy <w>uboczne</w> podczas tego spotkania.",
                            "target_lang_translations": {
                                "ru": "Мы не должны тратить время на <w>второстепенные</w> вопросы во время этой встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "poboczny",
                        "drugorzędny",
                        "mało ważny",
                        "nieistotny",
                        "incydentalny"
                    ],
                    "common_phrases": [
                        "wątek uboczny",
                        "cel uboczny",
                        "kwestia uboczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий меньшее значение, не являющийся главным предметом интереса; второстепенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "не главный по значимости"
                            },
                            {
                                "target_lang_word": "побочный",
                                "target_lang_sense_clarification": "не основной"
                            }
                        ]
                    }
                }
            ]
        }
    ]

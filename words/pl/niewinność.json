    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1e46946-9e05-3227-af3e-373a2f35a186",
                    "sense_definition": "Stan nieponoszenia winy za przestępstwo, wykroczenie lub czyn moralnie naganny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_moral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd ostatecznie potwierdził jego <w>niewinność</w> w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Суд окончательно подтвердил его <w>невиновность</w> в этом деле."
                            }
                        },
                        {
                            "text": "W naszym systemie prawnym obowiązuje zasada domniemania <w>niewinności</w>.",
                            "target_lang_translations": {
                                "ru": "В нашей правовой системе действует принцип презумпции <w>невиновности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezgrzeszność",
                        "nieskazitelność"
                    ],
                    "antonyms": [
                        "wina"
                    ],
                    "common_phrases": [
                        "domniemanie niewinności",
                        "udowodnić niewinność",
                        "wierzyć w niewinność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отсутствия вины за преступление, проступок или морально предосудительное деяние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невиновность",
                                "target_lang_sense_clarification": "отсутствие вины в юридическом смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5128e6d4-6b30-3f54-97d4-7fef74758945",
                    "sense_definition": "Brak doświadczenia życiowego, prostoduszność i ufne podejście do świata, typowe dla dzieci.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej dziecięca <w>niewinność</w> sprawiała, że wszyscy jej ufali.",
                            "target_lang_translations": {
                                "ru": "Её детская <w>невинность</w> заставляла всех доверять ей."
                            }
                        },
                        {
                            "text": "Patrzył na świat z rozbrajającą <w>niewinnością</w>, nie dostrzegając zagrożeń.",
                            "target_lang_translations": {
                                "ru": "Он смотрел на мир с обезоруживающей <w>невинностью</w>, не замечая угроз."
                            }
                        }
                    ],
                    "synonyms": [
                        "naiwność"
                    ],
                    "antonyms": [
                        "doświadczenie"
                    ],
                    "common_phrases": [
                        "dziecięca niewinność",
                        "stracić niewinność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие жизненного опыта, простодушие и доверчивое отношение к миру, характерное для детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невинность",
                                "target_lang_sense_clarification": "чистота души, отсутствие хитрости"
                            },
                            {
                                "target_lang_word": "наивность",
                                "target_lang_sense_clarification": "детская доверчивость"
                            },
                            {
                                "target_lang_word": "простодушие",
                                "target_lang_sense_clarification": "открытость и бесхитростность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e66b6bc-9653-38e3-9848-ac5cb959a2ec",
                    "sense_definition": "Stan zachowania czystości seksualnej; bycie dziewicą lub prawiczkiem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych kulturach <w>niewinność</w> panny młodej była bardzo ważna dla rodziny męża.",
                            "target_lang_translations": {
                                "ru": "В древних культурах <w>невинность</w> невесты была очень важна для семьи мужа."
                            }
                        },
                        {
                            "text": "Biały kolor sukni ślubnej tradycyjnie symbolizuje <w>niewinność</w>.",
                            "target_lang_translations": {
                                "ru": "Белый цвет свадебного платья традиционно символизирует <w>невинность</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "czystość",
                        "dziewictwo",
                        "cnotliwość"
                    ],
                    "common_phrases": [
                        "stracić niewinność",
                        "symbol niewinności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сохранения сексуальной чистоты; девственность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невинность",
                                "target_lang_sense_clarification": "целомудрие"
                            },
                            {
                                "target_lang_word": "девственность",
                                "target_lang_sense_clarification": "физическая чистота"
                            }

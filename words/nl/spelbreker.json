{
    "word_family": [
        "spel",
        "breken"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8bdf9db-bcc7-38fc-934b-b7e96908d240",
                    "sense_definition": "Iemand die het plezier van anderen bederft door niet mee te doen of dwars te liggen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "person_spoilsport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wees geen <w>spelbreker</w> en ga gewoon met ons mee naar het feest.",
                            "target_lang_translations": {
                                "en": "Don't be a <w>spoilsport</w> and just come along to the party with us.",
                                "ru": "Не будь <w>занудой</w> и просто пойдем с нами на вечеринку."
                            }
                        },
                        {
                            "text": "Hij gedroeg zich als een echte <w>spelbreker</w> door constant te klagen over de muziek.",
                            "target_lang_translations": {
                                "en": "He acted like a real <w>killjoy</w> by constantly complaining about the music.",
                                "ru": "Он вел себя как настоящий <w>зануда</w>, постоянно жалуясь на музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "spelbederver"
                    ],
                    "common_phrases": [
                        "een echte spelbreker",
                        "geen spelbreker zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who spoils the fun of others by not participating or being obstructive.",
                        "ru": "Тот, кто портит удовольствие другим, отказываясь участвовать или мешая."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spoilsport",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "killjoy",
                                "target_lang_sense_clarification": "someone who ruins enjoyment"
                            },
                            {
                                "target_lang_word": "party pooper",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зануда",
                                "target_lang_sense_clarification": "человек, который портит веселье своим поведением"
                            },
                            {
                                "target_lang_word": "кайфолом",
                                "target_lang_sense_clarification": "разговорное; тот, кто портит удовольствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db17a3e9-c60a-34ac-baa4-6b9a9f6b5239",
                    "sense_definition": "Een onvoorziene factor of omstandigheid die een plan of gebeurtenis verstoort.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "obstacle_factor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De plotselinge ziekte was een flinke <w>spelbreker</w> in zijn carrièreplannen.",
                            "target_lang_translations": {
                                "en": "The sudden illness was a major <w>spoiler</w> for his career plans.",
                                "ru": "Внезапная болезнь стала серьезной <w>помехой</w> в его карьерных планах."
                            }
                        },
                        {
                            "text": "Het slechte weer bleek de grote <w>spelbreker</w> tijdens onze vakantie.",
                            "target_lang_translations": {
                                "en": "The bad weather turned out to be the big <w>dampener</w> on our vacation.",
                                "ru": "Плохая погода оказалась главной <w>помехой</w> во время нашего отпуска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de grote spelbreker"
                    ],
                    "target_lang_definitions": {
                        "en": "An unforeseen factor or circumstance that disrupts a plan or event.",
                        "ru": "Непредвиденный фактор или обстоятельство, нарушающее планы или ход события."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spoiler",
                                "target_lang_sense_clarification": "factor ruining a plan"
                            },
                            {
                                "target_lang_word": "dampener",
                                "target_lang_sense_clarification": "something that reduces excitement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "помеха",
                                "target_lang_sense_clarification": "обстоятельство, мешающее осуществлению чего-либо"
                            },
                            {
                                "target_lang_word": "препятствие",
                                "target_lang_sense_clarification": "преграда на пути к цели"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
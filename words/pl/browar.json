    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e159049a-7fe1-3591-94d5-711186cd75cc",
                    "sense_definition": "Potoczne określenie piwa, często oznaczające jedną butelkę lub puszkę tego napoju.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "beer_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypiłem trzy <w>browary</w> i zrobiło mi się niedobrze.",
                            "target_lang_translations": {
                                "ru": "Я выпил три <w>пива</w>, и мне стало нехорошо."
                            }
                        },
                        {
                            "text": "Masz ochotę skoczyć wieczorem na jednego <w>browara</w>?",
                            "target_lang_translations": {
                                "ru": "Хочешь сходить вечером на одно <w>пиво</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "piwo",
                        "browiec"
                    ],
                    "common_phrases": [
                        "iść na browara",
                        "zimny browar",
                        "otworzyć browara"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo powszechnie używane w mowie potocznej"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "używane w języku młodzieżowym i nieformalnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название пива, часто означающее одну бутылку или банку этого напитка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пиво",
                                "target_lang_sense_clarification": "разговорное употребление"
                            },
                            {
                                "target_lang_word": "пивас",
                                "target_lang_sense_clarification": "слэнг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4e0144a-ed70-3fe2-b29a-280534457438",
                    "sense_definition": "Zakład przemysłowy zajmujący się produkcją (warzeniem) piwa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "production_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważam, że najlepsze piwo powstaje w <w>browarach</w> na Śląsku.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что лучшее пиво делают в <w>пивоварнях</w> Силезии."
                            }
                        },
                        {
                            "text": "W naszym mieście otworzono nowy <w>browar</w> rzemieślniczy, który przyciąga wielu turystów.",
                            "target_lang_translations": {
                                "ru": "В нашем городе открыли новую крафтовую <w>пивоварню</w>, которая привлекает много туристов."
                            }
                        }
                    ],
                    "synonyms": [
                        "browarnia"
                    ],
                    "common_phrases": [
                        "lokalny browar",
                        "zwiedzać browar",
                        "stary browar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Промышленное предприятие, занимающееся производством (варкой) пива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пивоварня",
                                "target_lang_sense_clarification": "предприятие"
                            },
                            {
                                "target_lang_word": "пивзавод",
                                "target_lang_sense_clarification": "крупное производство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bac8876-7ce6-34fb-ad05-67296be30b25",
                    "sense_definition": "Lokal gastronomiczny lub karczma, w której podawano piwo, często warzone na miejscu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczeni podróżni zatrzymali się w przydrożnym <w>browarze</w>, aby odpocząć i się posilić.",
                            "target_lang_translations": {
                                "ru": "Уставшие путники остановились в придорожной <w>пивной</w>, чтобы отдохнуть и поесть."
                            }
                        },
                        {
                            "text": "W tamtych czasach miejski <w>browar</w> był centrum życia towarzyskiego.",
                            "target_lang_translations": {
                                "ru": "В те времена городская <w>пивная</w> была центром светской жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "piwiarnia"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "znaczenie historyczne, rzadziej używane współcześnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гастрономическое заведение или корчма, где подавали пиво, часто сваренное на месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пивная",
                                "target_lang_sense_clarification": "заведение"
                            },
                            {
                                "target_lang_word": "трактир",
                                "target_lang_sense_clarification": "историческое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c83625d-4a59-3e20-8097-abaec7e8c149",
                    "sense_definition": "Dawne określenie zakładu produkującego wódkę lub inne alkohole destylowane.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "production_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych dokumentach ten budynek opisano jako <w>browar</w> produkujący okowitę.",
                            "target_lang_translations": {
                                "ru": "В старых документах это здание описано как <w>винокурня</w>, производящая аквавиту."
                            }
                        },
                        {
                            "text": "Dawny <w>browar</w> dworski został przekształcony w muzeum techniki.",
                            "target_lang_translations": {
                                "ru": "Бывшая дворцовая <w>винокурня</w> была превращена в технический музей."
                            }
                        }
                    ],
                    "synonyms": [
                        "gorzelnia"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie wyszło z użycia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название предприятия по производству водки или других дистиллятов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винокурня",
                                "target_lang_sense_clarification": "производство спирта"
                            }
                        ]
                    }
                }
            ]
        }
    ]

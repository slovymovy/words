{
    "word_family": [
        "szczerość",
        "szczerze"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b97f984f-bb7b-33b5-b592-8ee667a2836f",
                    "sense_definition": "Taki, który nie ukrywa swoich prawdziwych myśli i uczuć, mówi prawdę i nie udaje.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój przyjaciel jest bardzo <w>szczerym</w> człowiekiem i zawsze mówi to, co myśli.",
                            "target_lang_translations": {
                                "ru": "Мой друг — очень <w>искренний</w> человек, и он всегда говорит то, что думает."
                            }
                        },
                        {
                            "text": "To była długa i <w>szczera</w> rozmowa o naszych problemach.",
                            "target_lang_translations": {
                                "ru": "Это был долгий и <w>откровенный</w> разговор о наших проблемах."
                            }
                        }
                    ],
                    "synonyms": [
                        "autentyczny",
                        "otwarty",
                        "naturalny",
                        "bezpośredni",
                        "niekłamany"
                    ],
                    "antonyms": [
                        "nieszczery",
                        "fałszywy",
                        "obłudny",
                        "kłamliwy"
                    ],
                    "common_phrases": [
                        "szczera prawda",
                        "szczere chęci",
                        "szczera rozmowa",
                        "szczery uśmiech"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не скрывает своих истинных мыслей и чувств, говорит правду и не притворяется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искренний",
                                "target_lang_sense_clarification": "выражающий подлинные чувства"
                            },
                            {
                                "target_lang_word": "откровенный",
                                "target_lang_sense_clarification": "прямой, не скрывающий мыслей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05dada0f-8ef5-37c7-be27-5b243bd528ee",
                    "sense_definition": "O terenie: otwarty, pusty, pozbawiony zabudowań i drzew.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatrzymaliśmy się na środku <w>szczerego</w> pola, żeby odpocząć.",
                            "target_lang_translations": {
                                "ru": "Мы остановились посреди <w>чистого</w> поля, чтобы отдохнуть."
                            }
                        },
                        {
                            "text": "Wokół nas było tylko <w>szczere</w> pole i cisza.",
                            "target_lang_translations": {
                                "ru": "Вокруг нас было только <w>чистое</w> поле и тишина."
                            }
                        }
                    ],
                    "synonyms": [
                        "prosty",
                        "prosty"
                    ],
                    "common_phrases": [
                        "szczere pole"
                    ],
                    "target_lang_definitions": {
                        "ru": "О местности: открытое, пустое пространство, лишенное строений и деревьев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "в выражении \"чистое поле\""
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "о пространстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9841ab5a-893d-3786-8c87-4ee22a40dc0b",
                    "sense_definition": "Jednolity, czysty, nie zawierający żadnych domieszek (zwykle o złocie lub lesie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "purity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten pierścionek jest wykonany ze <w>szczerego</w> złota.",
                            "target_lang_translations": {
                                "ru": "Это кольцо сделано из <w>чистого</w> золота."
                            }
                        },
                        {
                            "text": "Zabłądzili w <w>szczerym</w> lesie podczas wycieczki.",
                            "target_lang_translations": {
                                "ru": "Они заблудились в <w>дремучем</w> лесу во время прогулки."
                            }
                        }
                    ],
                    "synonyms": [
                        "autentyczny"
                    ],
                    "common_phrases": [
                        "szczere złoto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Однородный, без примесей (о металлах) или сплошной, густой (о лесе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "без примесей, напр. о золоте"
                            },
                            {
                                "target_lang_word": "сплошной",
                                "target_lang_sense_clarification": "без перерывов, напр. о лесе или песке"
                            },
                            {
                                "target_lang_word": "дремучий",
                                "target_lang_sense_clarification": "густой, непроходимый (о лесе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "dec9ff9d-3398-3aa1-bdf7-b5917bb5b708",
                    "sense_definition": "Pertaining to an economy, trade, industry, or the study of money.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finance_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government has announced a new plan to stimulate <w>economic</w> growth.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новом плане по стимулированию <w>экономического</w> роста."
                            }
                        },
                        {
                            "text": "Many families are struggling to pay bills due to the current <w>economic</w> crisis.",
                            "target_lang_translations": {
                                "ru": "Многие семьи с трудом оплачивают счета из-за текущего <w>экономического</w> кризиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "financial",
                        "fiscal",
                        "monetary",
                        "commercial"
                    ],
                    "common_phrases": [
                        "economic growth",
                        "economic crisis",
                        "economic development",
                        "economic policy",
                        "economic system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к экономике, торговле, промышленности или управлению деньгами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономи́ческий",
                                "target_lang_sense_clarification": "относящийся к экономике страны или региона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56f4aef1-91f2-3e2a-af58-0a95d4c5625f",
                    "sense_definition": "Justified in terms of profitability; representing good value or being cost-effective.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "value_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is not <w>economic</w> to repair the old car given the high cost of parts.",
                            "target_lang_translations": {
                                "ru": "Ремонтировать старую машину не <w>выгодно</w>, учитывая высокую стоимость запчастей."
                            }
                        },
                        {
                            "text": "The mine was closed down because it was no longer <w>economic</w> to operate.",
                            "target_lang_translations": {
                                "ru": "Шахту закрыли, потому что её эксплуатация перестала быть <w>рентабельной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "profitable",
                        "cost-effective",
                        "viable",
                        "remunerative"
                    ],
                    "antonyms": [
                        "uneconomic",
                        "unprofitable"
                    ],
                    "common_phrases": [
                        "economic viability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оправданный с точки зрения прибыльности; представляющий ценность или являющийся экономически эффективным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рентабельный",
                                "target_lang_sense_clarification": "оправдывающий расходы, приносящий доход"
                            },
                            {
                                "target_lang_word": "выгодный",
                                "target_lang_sense_clarification": "приносящий пользу или прибыль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad0fe14a-4900-37ca-ad28-4f28fcdcd91d",
                    "sense_definition": "Pertaining to the management of a household.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "household_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historian described the <w>economic</w> duties of the steward in the large estate.",
                            "target_lang_translations": {
                                "ru": "Историк описал <w>хозяйственные</w> обязанности управляющего в большом поместье."
                            }
                        },
                        {
                            "text": "She was responsible for the <w>economic</w> organization of the daily household tasks.",
                            "target_lang_translations": {
                                "ru": "Она отвечала за <w>хозяйственную</w> организацию повседневных домашних дел."
                            }
                        }
                    ],
                    "synonyms": [
                        "domestic",
                        "household"
                    ],

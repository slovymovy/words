                {
                    "sense_id": "4ee4dc6a-ae0a-3692-8e99-a0bedd1f536a",
                    "sense_definition": "Een gekoelde ruimte of apparaat waarin producten, zoals eten en drinken, koud worden bewaard.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de melk na gebruik direct terug in de <w>koeling</w>.",
                            "target_lang_translations": {
                                "en": "Put the milk directly back in the <w>fridge</w> after use.",
                                "ru": "Поставь молоко сразу обратно в <w>холодильник</w> после использования."
                            }
                        },
                        {
                            "text": "Hebben jullie nog koud bier in de <w>koeling</w> staan?",
                            "target_lang_translations": {
                                "en": "Do you still have cold beer in the <w>cooler</w>?",
                                "ru": "У вас есть ещё холодное пиво в <w>холодильнике</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "koelcel"
                    ],
                    "common_phrases": [
                        "in de koeling",
                        "uit de koeling"
                    ],
                    "target_lang_definitions": {
                        "en": "A refrigerated space or appliance where products, such as food and drinks, are kept cold.",
                        "ru": "Охлаждаемое пространство или устройство, в котором хранятся продукты, такие как еда и напитки."
                    },

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23dee159-b43b-3cb3-9ad1-335a32723e2f",
                    "sense_definition": "Het verband of de samenhang tussen bepaalde zaken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "connection_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze nieuwe regels hebben <w>betrekking</w> op de veiligheid van alle werknemers.",
                            "target_lang_translations": {
                                "en": "These new rules <w>relate</w> to the safety of all employees.",
                                "ru": "Эти новые правила имеют <w>отношение</w> к безопасности всех сотрудников."
                            }
                        },
                        {
                            "text": "Heeft u nog vragen met <w>betrekking</w> tot de procedure die we net hebben besproken?",
                            "target_lang_translations": {
                                "en": "Do you have any questions with <w>regard</w> to the procedure we just discussed?",
                                "ru": "У вас есть еще вопросы в <w>отношении</w> процедуры, которую мы только что обсудили?"
                            }
                        }
                    ],
                    "synonyms": [
                        "verband",
                        "relatie"
                    ],
                    "common_phrases": [
                        "met betrekking tot",
                        "betrekking hebben op"
                    ],
                    "target_lang_definitions": {
                        "en": "The connection or link between specific matters.",
                        "ru": "Связь или взаимосвязь между определенными вещами."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b372ac11-2377-365e-b67b-f5f62490801c",
                    "sense_definition": "Een computerprogramma of softwaretoepassing, meestal ontworpen voor mobiele apparaten zoals smartphones of tablets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een nieuwe <w>app</w> gedownload om Nederlands te leren.",
                            "target_lang_translations": {
                                "ru": "Я скачал новое <w>приложение</w>, чтобы учить нидерландский."
                            }
                        },
                        {
                            "text": "Deze <w>app</w> werkt niet goed op mijn oude telefoon.",
                            "target_lang_translations": {
                                "ru": "Это <w>приложение</w> плохо работает на моем старом телефоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "applicatie",
                        "toepassing"
                    ],
                    "common_phrases": [
                        "app downloaden",
                        "app installeren",
                        "app openen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа или программное обеспечение, обычно разработанное для мобильных устройств, таких как смартфоны или планшеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приложение",
                                "target_lang_sense_clarification": "программа для смартфона"
                            },
                            {
                                "target_lang_word": "апп",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b68671e2-55eb-3154-8731-0afdcb9fd46c",
                    "sense_definition": "Een digitaal berichtje dat verstuurd is via een berichtendienst zoals WhatsApp.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "message",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stuur me even een <w>app</w> als je veilig thuis bent.",
                            "target_lang_translations": {
                                "ru": "Отправь мне <w>сообщение</w>, когда благополучно доберешься домой."
                            }
                        },
                        {
                            "text": "Ik heb je <w>app</w> gisteren helemaal niet gezien.",
                            "target_lang_translations": {
                                "ru": "Я вчера вообще не видел твоего <w>сообщения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "appje",
                        "berichtje"
                    ],
                    "common_phrases": [
                        "een app sturen",
                        "een app krijgen",
                        "een app lezen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровое сообщение, отправленное через службу обмена сообщениями, такую как WhatsApp."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщение",
                                "target_lang_sense_clarification": "в мессенджере"
                            },
                            {
                                "target_lang_word": "смс",
                                "target_lang_sense_clarification": "разговорное обобщение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9103853-455a-3103-b9d9-e60576cd55f9",
                    "sense_definition": "Een digitaal gesprek of chatgroep binnen een berichtenapplicatie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "message",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de foto's van het feestje maar even in de <w>app</w>.",
                            "target_lang_translations": {
                                "ru": "Скинь фотки с вечеринки в <w>чат</w>."
                            }
                        },
                        {
                            "text": "Er werd druk overlegd in de <w>app</w> van het studentenhuis.",
                            "target_lang_translations": {
                                "ru": "В <w>чате</w> студенческого общежития шло бурное обсуждение."
                            }
                        }
                    ],
                    "synonyms": [
                        "groepsapp",
                        "groepschat"
                    ],
                    "common_phrases": [
                        "in de app",
                        "uit de app"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "refers to the group chat context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровая беседа или чат-группа в приложении для обмена сообщениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чат",
                                "target_lang_sense_clarification": "групповая переписка"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "беседа в мессенджере"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "13042ea7-ef45-348d-93f4-02e0fcd62cc9",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>appen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>app</w> je zodra ik meer weet over de plannen.",
                            "target_lang_translations": {
                                "ru": "Я <w>напишу</w> тебе, как только узнаю больше о планах."
                            }
                        },
                        {
                            "text": "Ik <w>app</w> meestal niet als ik aan het fietsen ben.",
                            "target_lang_translations": {
                                "ru": "Я обычно не <w>пишу сообщения</w>, когда еду на велосипеде."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'appen' (писать сообщение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пишу",
                                "target_lang_sense_clarification": "отправляю сообщение"
                            },
                            {
                                "target_lang_word": "напишу",
                                "target_lang_sense_clarification": "будущее время (часто используется в контексте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe974563-5abb-366b-b217-a8932279997c",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>appen</w> (bij inversie).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>App</w> je mij even het adres door?",
                            "target_lang_translations": {
                                "ru": "<w>Скинешь</w> мне адрес?"
                            }
                        },
                        {
                            "text": "Waarom <w>app</w> je zo laat nog naar je collega's?",
                            "target_lang_translations": {
                                "ru": "Почему ты так поздно <w>пишешь</w> коллегам?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени от глагола 'appen' (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пишешь",
                                "target_lang_sense_clarification": "отправляешь сообщение"
                            },
                            {
                                "target_lang_word": "скинешь",
                                "target_lang_sense_clarification": "разговорное: отправишь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f3d0bec-cc4d-3412-b4fb-e83491fabce8",
                    "sense_definition": "Gebiedende wijs van <w>appen</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>App</w> me als je bij het station bent aangekomen.",
                            "target_lang_translations": {
                                "ru": "<w>Напиши</w> мне, когда приедешь на вокзал."
                            }
                        },
                        {
                            "text": "<w>App</w> even als je hulp nodig hebt met de boodschappen.",
                            "target_lang_translations": {
                                "ru": "<w>Напиши</w>, если тебе нужна помощь с покупками."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'appen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напиши",
                                "target_lang_sense_clarification": "отправь сообщение"
                            },
                            {
                                "target_lang_word": "скинь",
                                "target_lang_sense_clarification": "разговорное: отправь"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящать",
                                "target_lang_sense_clarification": "в детали, в тайны"
                            },
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "в курс дела"
                            },
                            {
                                "target_lang_word": "знакомить",
                                "target_lang_sense_clarification": "с обязанностями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "230fa73e-ed0d-3a53-8f29-c0bfc82deab3",
                    "sense_definition": "Wykonywać działanie matematyczne polegające na mnożeniu liczb.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych podręcznikach uczono, jak <wprowadzać> liczby przez siebie w pamięci.",
                            "target_lang_translations": {
                                "ru": "В старых учебниках учили, как <умножать> числа друг на друга в уме."
                            }
                        },
                        {
                            "text": "Nauczyciel kazał uczniom <wprowadzać> podane wartości, aby uzyskać wynik iloczynu.",
                            "target_lang_translations": {
                                "ru": "Учитель велел ученикам <умножать> заданные значения, чтобы получить результат произведения."
                            }

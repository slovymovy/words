{
    "word_family": [
        "zemsta",
        "pomsta",
        "mściciel",
        "mściwy",
        "pomścić",
        "zemścić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "204009cf-7468-3eed-a695-c9928220f087",
                    "sense_definition": "Odpłacać komuś złem za wyrządzone krzywdy lub przykrości (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "revenge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie warto <mścić> się za drobne złośliwości, lepiej po prostu zapomnieć.",
                            "target_lang_translations": {
                                "ru": "Не стоит <мстить> за мелкие пакости, лучше просто забыть."
                            }
                        },
                        {
                            "text": "Bohater filmu postanowił okrutnie <mścić> się na ludziach, którzy go oszukali.",
                            "target_lang_translations": {
                                "ru": "Герой фильма решил жестоко <мстить> людям, которые его обманули."
                            }
                        }
                    ],
                    "synonyms": [
                        "odgrywać się",
                        "brać odwet"
                    ],
                    "antonyms": [
                        "przebaczać",
                        "darować"
                    ],
                    "common_phrases": [
                        "mścić się na wrogu",
                        "mścić się za zdradę",
                        "okrutnie się mścić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Платить кому-то злом за причиненное зло или неприятности (используется с возвратным местоимением)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мстить",
                                "target_lang_sense_clarification": "совершать месть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a930f11c-4ae6-39d2-b3b7-f7b462b2a527",
                    "sense_definition": "Dokonywać zemsty za kogoś lub za coś; wymierzać sprawiedliwość na własną rękę za doznane zło",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "revenge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz przysiągł <mścić> śmierć swojego króla aż do ostatniego tchu.",
                            "target_lang_translations": {
                                "ru": "Рыцарь поклялся <мстить> за смерть своего короля до последнего вздоха."
                            }
                        },
                        {
                            "text": "Zamiast <mścić> dawne krzywdy, powinniśmy skupić się na budowaniu przyszłości.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы <мстить> за старые обиды, нам следует сосредоточиться на построении будущего."
                            }
                        }
                    ],
                    "synonyms": [
                        "brać odwet"
                    ],
                    "antonyms": [
                        "wybaczać"
                    ],
                    "common_phrases": [
                        "mścić krzywdy",
                        "mścić zniewagę",
                        "mścić krew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать месть за кого-то или что-то; вершить правосудие своими силами за причиненное зло"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мстить",
                                "target_lang_sense_clarification": "за кого-то или что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e79bd785-38f9-3eab-9a42-af4ccb528355",
                    "sense_definition": "Przynosić negatywne konsekwencje lub skutki po pewnym czasie, będące rezultatem wcześniejszych zaniedbań (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consequences",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieloletnie palenie papierosów zaczęło <mścić> się na jego zdrowiu.",
                            "target_lang_translations": {
                                "ru": "Многолетнее курение начало <сказываться> на его здоровье."
                            }
                        },
                        {
                            "text": "Brak regularnego serwisu samochodu lubi <mścić> się w trakcie długiej podróży.",
                            "target_lang_translations": {
                                "ru": "Отсутствие регулярного обслуживания автомобиля любит <аукаться> во время долгой поездки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mścić się na zdrowiu",
                        "mścić się po latach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить к негативным последствиям спустя время как результат прошлых упущений"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказываться",
                                "target_lang_sense_clarification": "о последствиях"
                            },
                            {
                                "target_lang_word": "аукаться",
                                "target_lang_sense_clarification": "разговорное: давать о себе знать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
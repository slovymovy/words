            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6666c78a-4c28-3421-ba78-cbee3e69e0bb",
                    "sense_definition": "Исполнить музыкальное произведение голосом (совершенный вид глагола «петь»).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг обещал <спеть> нам свою новую песню под гитару.",
                            "target_lang_translations": {
                                "en": "My friend promised to <sing> us his new song accompanied by the guitar."
                            }
                        },
                        {
                            "text": "Дети решили <спеть> хором на школьном празднике.",
                            "target_lang_translations": {
                                "en": "The children decided to <sing> in a choir at the school festival."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропеть"
                    ],
                    "common_phrases": [
                        "спеть песню",
                        "спеть дуэтом",
                        "спеть на бис",
                        "спеть громко"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a musical piece using one's voice (perfective aspect of \"to sing\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing",
                                "target_lang_sense_clarification": "to produce musical sounds with the voice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e404e56e-e34e-3c47-859b-9583c6f4ca4a",
                    "sense_definition": "Становиться зрелым, пригодным для еды (о фруктах, овощах, ягодах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем саду уже <спеют> сладкие красные вишни.",
                            "target_lang_translations": {
                                "en": "Sweet red cherries are already <ripening> in our garden."
                            }
                        },
                        {
                            "text": "Помидоры на южном солнце <спеют> гораздо быстрее.",
                            "target_lang_translations": {
                                "en": "Tomatoes <ripen> much faster in the southern sun."
                            }
                        }
                    ],
                    "synonyms": [
                        "зреть",
                        "созревать",
                        "поспевать"
                    ],
                    "common_phrases": [
                        "ягоды спеют",
                        "быстро спеть",
                        "урожай спеет"
                    ],
                    "target_lang_definitions": {
                        "en": "To become mature and ready to be eaten (referring to fruits, vegetables, berries)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ripen",
                                "target_lang_sense_clarification": "to become ready for eating"
                            },
                            {
                                "target_lang_word": "mature",
                                "target_lang_sense_clarification": "to reach full development"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c4184d0-1cdd-3ff0-bc81-837371fff4ab",
                    "sense_definition": "Стараться сделать что-либо быстро; то же, что спешить.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путник <спел> домой, чтобы успеть до начала сильной грозы.",
                            "target_lang_translations": {
                                "en": "The traveler <hurried> home to make it before the heavy thunderstorm started."
                            }
                        },
                        {
                            "text": "Не надо так <спеть>, у нас ещё достаточно времени.",
                            "target_lang_translations": {
                                "en": "There is no need to <rush> so much; we still have enough time."
                            }
                        }
                    ],
                    "synonyms": [
                        "поспевать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used in older texts"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "regional usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To try to do something quickly; to move with haste (archaic or dialectal)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hurry",
                                "target_lang_sense_clarification": "to move or act with speed"
                            },
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "to move with urgent haste"
                            },
                            {
                                "target_lang_word": "hasten",
                                "target_lang_sense_clarification": "to move or travel quickly"
                            }
                        ]
                    }
                }
            ]
        }
    ]

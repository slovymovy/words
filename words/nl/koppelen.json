                        "samenvoegen",
                        "associëren"
                    ],
                    "antonyms": [
                        "ontkoppelen",
                        "ontkoppelen",
                        "ontkoppelen"
                    ],
                    "common_phrases": [
                        "aan elkaar koppelen",
                        "naam koppelen aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To fasten two or more things together or relate them to each other.",
                        "ru": "Соединять две или более вещи вместе или связывать их друг с другом."

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b12753fd-8815-36db-a440-8ac6e2d462cc",
                    "sense_definition": "Повернуть голову или туловище назад или в сторону, чтобы посмотреть на кого-либо или что-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Услышав своё имя, он резко <w>обернулся</w>."
                        },
                        {
                            "text": "Девушка <w>обернулась</w> и помахала нам рукой на прощание."
                        }
                    ],
                    "synonyms": [
                        "оглянуться",
                        "повернуться"
                    ],
                    "antonyms": [
                        "отвернуться"
                    ],
                    "common_phrases": [
                        "резко обернуться",
                        "обернуться назад",
                        "обернуться на звук"
                    ]
                },
                {
                    "sense_id": "8d7299fe-4c9a-3422-beb5-b9b731d2ac18",
                    "sense_definition": "Привести к какому-либо результату, закончиться чем-либо (обычно неожиданным или неприятным) для кого-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта маленькая ошибка может <w>обернуться</w> большой проблемой для всей команды."
                        },
                        {
                            "text": "Поездка в горы <w>обернулась</w> настоящим приключением."
                        }
                    ],
                    "synonyms": [
                        "оказаться",
                        "стать",
                        "вылиться"
                    ],
                    "common_phrases": [
                        "обернуться бедой",
                        "обернуться трагедией",
                        "обернуться удачей"
                    ]
                },
                {
                    "sense_id": "2d84367d-2f9a-38ad-aea8-8f9e5dfd2795",
                    "sense_definition": "Быстро сходить или съездить куда-либо и вернуться обратно.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_trip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я мигом <w>обернусь</w> до магазина, жди меня здесь."
                        },
                        {
                            "text": "Отец обещал <w>обернуться</w> за один день и привезти подарки."
                        }
                    ],
                    "synonyms": [
                        "сбегать",
                        "съездить"
                    ],
                    "common_phrases": [
                        "быстро обернуться",
                        "обернуться за час",
                        "мигом обернуться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "cd2c7dc9-0646-3c5b-8db2-ea9c3a8d27f4",
                    "sense_definition": "Превратиться в кого-либо или что-либо (часто в сказках, мифах или переносном смысле).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "transformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В русской сказке Иван-царевич <w>обернулся</w> серым волком."
                        },
                        {
                            "text": "Злая колдунья ударилась о землю и <w>обернулась</w> чёрной птицей."
                        }
                    ],
                    "synonyms": [
                        "превратиться"
                    ],
                    "common_phrases": [
                        "обернуться зверем",
                        "обернуться птицей"
                    ]
                },
                {
                    "sense_id": "10fe4645-b8f6-37f0-8c58-40e62b669116",
                    "sense_definition": "Замотать себя во что-либо (в ткань, одеяло), плотно укрыться.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От холода она решила <w>обернуться</w> в тёплый шерстяной плед."
                        },
                        {
                            "text": "После душа ребёнок <w>обернулся</w> большим полотенцем."
                        }
                    ],
                    "synonyms": [
                        "закутаться",
                        "завернуться"
                    ],
                    "common_phrases": [
                        "обернуться одеялом",
                        "обернуться в плащ"
                    ]
                }
            ]
        }
    ]

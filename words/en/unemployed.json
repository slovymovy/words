    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9a7f5ba0-d311-37bc-a764-8bfa1c381b91",
                    "sense_definition": "Having no job despite being able and willing to work.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "jobless_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has been <w>unemployed</w> for six months and is looking for a new position.",
                            "target_lang_translations": {
                                "ru": "Он был <w>безработным</w> в течение шести месяцев и ищет новую должность."
                            }
                        },
                        {
                            "text": "Many <w>unemployed</w> workers are retraining to find jobs in different industries.",
                            "target_lang_translations": {
                                "ru": "Многие <w>безработные</w> рабочие проходят переподготовку, чтобы найти работу в других отраслях."
                            }
                        }
                    ],
                    "synonyms": [
                        "jobless",
                        "out of work",
                        "between jobs"
                    ],
                    "antonyms": [
                        "employed",
                        "working"
                    ],
                    "common_phrases": [
                        "currently unemployed",
                        "unemployed workers",
                        "remain unemployed",
                        "registered unemployed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий работы, но способный и желающий работать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безрабо́тный",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "нетрудоустро́енный",
                                "target_lang_sense_clarification": "официальный статус"
                            },
                            {
                                "target_lang_word": "нерабо́тающий",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8746edd8-149a-3156-be59-9b0e62655b7f",
                    "sense_definition": "Not being used or not currently performing a task; idle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The goalkeeper was largely <w>unemployed</w> during the match because his team dominated possession.",
                            "target_lang_translations": {
                                "ru": "Вратарь оставался по большей части <w>без работы</w> во время матча, так как его команда владела мячом."
                            }
                        },
                        {
                            "text": "Vast amounts of capital remained <w>unemployed</w> in the bank accounts instead of being invested.",
                            "target_lang_translations": {
                                "ru": "Огромные суммы капитала оставались <w>неиспользованными</w> на банковских счетах вместо того, чтобы быть инвестированными."
                            }
                        }
                    ],
                    "synonyms": [
                        "idle",
                        "unused",
                        "inactive"
                    ],
                    "antonyms": [
                        "in use",
                        "employed"
                    ],
                    "common_phrases": [
                        "largely unemployed",
                        "virtually unemployed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неиспользуемый или не выполняющий задачу в данный момент; простаивающий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неза́нятый",
                                "target_lang_sense_clarification": "свободный, простаивающий"
                            },
                            {
                                "target_lang_word": "неиспо́льзуемый",
                                "target_lang_sense_clarification": "о ресурсах или капитале"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80603858-47b5-3cf0-9e16-25358999cd61",
                    "sense_definition": "People who do not have a job, considered as a group.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "jobless_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government has announced new benefits to support the <w>unemployed</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новых пособиях для поддержки <w>безработных</w>."
                            }
                        },
                        {
                            "text": "Statistics show that the number of <w>unemployed</w> has decreased slightly this quarter.",
                            "target_lang_translations": {
                                "ru": "Статистика показывает, что число <w>безработных</w> немного снизилось в этом квартале."
                            }
                        }
                    ],
                    "antonyms": [
                        "employed"
                    ],
                    "common_phrases": [
                        "the long-term unemployed",
                        "help the unemployed",
                        "young unemployed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, не имеющие работы, как социальная группа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безрабо́тные",
                                "target_lang_sense_clarification": "группа людей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

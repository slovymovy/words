    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "299eefee-7d31-33be-862f-a16c8483f023",
                    "sense_definition": "Werktuig dat kan worden gebruikt om te slaan, meestal bestaande uit een steel en een zware metalen kop.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sloeg met de <w>hamer</w> de spijker diep in het hout.",
                            "target_lang_translations": {
                                "ru": "Он глубоко забил гвоздь в дерево <w>молотком</w>."
                            }
                        },
                        {
                            "text": "Toen het huis onder de <w>hamer</w> kwam, was er veel belangstelling.",
                            "target_lang_translations": {
                                "ru": "Когда дом пошел с <w>молотка</w>, интерес был огромным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "onder de hamer",
                        "man met de hamer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент, используемый для нанесения ударов, обычно состоящий из рукоятки и тяжелой металлической головки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молоток",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52a02f3f-497b-3c94-b0bd-a60a536137a7",
                    "sense_definition": "Een van de drie gehoorbeentjes in het middenoor, gelegen tussen het trommelvlies en het aambeeld.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>hamer</w> geeft de trillingen van het geluid door aan het aambeeld.",
                            "target_lang_translations": {
                                "ru": "<w>Молоточек</w> передает звуковые вибрации на наковальню."
                            }
                        },
                        {
                            "text": "Bij de operatie werd gekeken of de <w>hamer</w> nog intact was.",
                            "target_lang_translations": {
                                "ru": "Во время операции проверяли, цел ли еще <w>молоточек</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "malleus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из трех слуховых косточек в среднем ухе, расположенная между барабанной перепонкой и наковальней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молоточек",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "59d82877-4f86-3b35-a58d-2848a5a0f345",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>hameren</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hamer</w> er steeds op dat we de regels moeten volgen.",
                            "target_lang_translations": {
                                "ru": "Я постоянно <w>настаиваю</w> на том, что мы должны следовать правилам."
                            }
                        },
                        {
                            "text": "Voorzichtig <w>hamer</w> ik het laatste stukje van de plank vast.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>прибиваю</w> последнюю часть доски."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола hameren (бить молотком, настаивать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настаиваю",
                                "target_lang_sense_clarification": "переносное значение: постоянно повторять"
                            },
                            {
                                "target_lang_word": "забиваю",
                                "target_lang_sense_clarification": "прямое значение: работать молотком"
                            },
                            {
                                "target_lang_word": "стучу",
                                "target_lang_sense_clarification": "издавать звук ударов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c29374a7-f535-3b76-96f8-8538ea07c70e",
                    "sense_definition": "Gebiedende wijs van <w>hameren</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hamer</w> die spijkers niet te diep in het zachte hout.",
                            "target_lang_translations": {
                                "ru": "Не <w>забивай</w> эти гвозди слишком глубоко в мягкое дерево."
                            }
                        },
                        {
                            "text": "<w>Hamer</w> er niet zo op, ik heb het de eerste keer al begrepen.",
                            "target_lang_translations": {
                                "ru": "Не <w>тверди</w> так об этом, я понял с первого раза."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола hameren."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забивай",
                                "target_lang_sense_clarification": "прямое значение"
                            },
                            {
                                "target_lang_word": "настаивай",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "тверди",
                                "target_lang_sense_clarification": "разговорное: постоянно повторять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67b239d5-1928-36c2-b501-d030daf3cfdc",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>hameren</w> (bij inversie).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>hamer</w> je zo op dat ene onbelangrijke detail?",
                            "target_lang_translations": {
                                "ru": "Почему ты так <w>настаиваешь</w> на этой несущественной детали?"
                            }
                        },
                        {
                            "text": "<w>Hamer</w> je de planken even vast voordat we pauze nemen?",
                            "target_lang_translations": {
                                "ru": "Ты <w>прибьешь</w> доски перед тем, как мы сделаем перерыв?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма второго лица единственного числа настоящего времени глагола hameren (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настаиваешь",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "забиваешь",
                                "target_lang_sense_clarification": "прямое значение"
                            },
                            {
                                "target_lang_word": "прибиваешь",
                                "target_lang_sense_clarification": "скреплять молотком"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "93c83238-00bf-39e5-b362-01fa43581a82",
                    "sense_definition": "Een jongensnaam.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn achterneef heet <w>Hamer</w>, wat ik een heel stoere naam vind.",
                            "target_lang_translations": {
                                "ru": "Моего троюродного брата зовут <w>Хамер</w>, и я считаю это очень крутым именем."
                            }
                        },
                        {
                            "text": "Op de lijst met deelnemers zag ik dat <w>Hamer</w> ook meedoet.",
                            "target_lang_translations": {
                                "ru": "В списке участников я увидел, что <w>Хамер</w> тоже участвует."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хамер",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

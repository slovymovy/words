            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "53e50e50-603b-3ea9-a9dd-7ed7abcc0443",
                    "sense_definition": "Net zoals; op dezelfde manier als.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze man is <w>evenals</w> zijn vader een directeur.",
                            "target_lang_translations": {
                                "ru": "Этот человек, <w>как и</w> его отец, — директор.",
                                "en": "This man, <w>just like</w> his father, is a director."
                            }
                        },
                        {
                            "text": "Zij houdt van klassieke muziek, <w>evenals</w> haar zus.",
                            "target_lang_translations": {
                                "ru": "Она любит классическую музыку, <w>так же, как и</w> её сестра.",
                                "en": "She loves classical music, <w>just like</w> her sister."
                            }
                        }
                    ],
                    "antonyms": [
                        "evenmin"
                    ],
                    "common_phrases": [
                        "evenals zijn vader",
                        "evenals de rest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точно так же, как; таким же образом, как.",
                        "en": "Just like; in the same manner as."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "1493d312-24a3-3a3b-a8f3-412e8b7fcd5f",
                    "sense_definition": "Wieś w Polsce położona w województwie pomorskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają w małej miejscowości <w>Kuchnia</w>.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка живут в маленьком населенном пункте <w>Кухня</w>."
                            }
                        },
                        {
                            "text": "Jadąc na północ, przejeżdżaliśmy przez wieś <w>Kuchnia</w>.",
                            "target_lang_translations": {
                                "ru": "Ехав на север, мы проезжали через деревню <w>Кухня</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в Польше, расположенная в Поморском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кухня",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5c7435d-4a08-3de2-8e08-54818b30f3f6",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Kuchnia</w> jest naszym nowym sąsiadem.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Кухня</w> — наш новый сосед."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Kuchnia</w>.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Кухня</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, встречающаяся в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кухня",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

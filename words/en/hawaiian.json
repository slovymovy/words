                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гава́ец",
                                "target_lang_sense_clarification": "коренной житель"
                            },
                            {
                                "target_lang_word": "гава́йка",
                                "target_lang_sense_clarification": "коренная жительница"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f9666a6-2234-3864-b384-d1568993012d",
                    "sense_definition": "Попросить повторить сказанное, так как оно не было услышано или понято.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ask_again",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не расслышал адрес и решил вежливо <w>переспросить</w> у прохожего.",
                            "target_lang_translations": {
                                "en": "I didn't catch the address and decided to politely <w>ask</w> the passerby <w>to repeat it</w>."
                            }
                        },
                        {
                            "text": "Если вы не поняли задание учителя, не бойтесь <w>переспросить</w>.",
                            "target_lang_translations": {
                                "en": "If you didn't understand the teacher's assignment, don't be afraid to <w>ask again</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "уточнить"
                    ],
                    "common_phrases": [
                        "не стесняйся переспросить",
                        "можно переспросить",
                        "вынужден переспросить"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask someone to repeat what was said because it was not heard or understood."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask again",
                                "target_lang_sense_clarification": "request repetition"
                            },
                            {
                                "target_lang_word": "ask to repeat",
                                "target_lang_sense_clarification": "request repetition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fad997fc-e116-362e-a8fe-4265dff3c54e",
                    "sense_definition": "Задать тот же самый вопрос ещё раз.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ask_again",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не ответил сразу, и мне пришлось <w>переспросить</w> громче.",
                            "target_lang_translations": {
                                "en": "He didn't answer immediately, and I had to <w>ask again</w> louder."
                            }
                        },
                        {
                            "text": "Мальчик решил <w>переспросить</w>, правильно ли он понял правила новой игры.",
                            "target_lang_translations": {
                                "en": "The boy decided to <w>ask again</w> if he had understood the rules of the new game correctly."
                            }
                        }
                    ],
                    "common_phrases": [
                        "переспросить дважды"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask the same question one more time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask again",
                                "target_lang_sense_clarification": "repeat a question"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "411172a5-e1f7-3f91-a961-6e676a8e0d07",
                    "sense_definition": "Обратиться с вопросом ко многим людям по очереди; опросить всех.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы успели <w>переспросить</w> всех соседей, но нашу кошку никто не видел.",
                            "target_lang_translations": {
                                "en": "We managed to <w>ask</w> all the neighbors, but no one had seen our cat."
                            }
                        },
                        {
                            "text": "За этот вечер он умудрился <w>переспросить</w> каждого гостя о его работе.",
                            "target_lang_translations": {
                                "en": "During the evening, he managed to <w>ask</w> every guest about their job."
                            }
                        }
                    ],
                    "synonyms": [
                        "опросить"
                    ],
                    "common_phrases": [
                        "переспросить всех"
                    ],
                    "target_lang_definitions": {
                        "en": "To address a question to many people in turn; to poll everyone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "ask multiple people"
                            },
                            {
                                "target_lang_word": "question",
                                "target_lang_sense_clarification": "interview or poll"
                            }
                        ]
                    }
                }
            ]
        }
    ]

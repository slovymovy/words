                    "sense_id": "d9cd66af-3036-34f3-a01d-2ddf7c8c4145",
                    "sense_definition": "Kobieta lub dziewczyna budząca litość i współczucie z powodu swojego losu, wyglądu lub biedy; wariant słowa <w>biedula</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta mała <bidula> zgubiła się w lesie i płakała z przerażenia.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая <бедняжка> заблудилась в лесу и плакала от ужаса."
                            }
                        },
                        {
                            "text": "Sąsiadka to straszna <bidula>, która ledwo wiąże koniec z końcem.",
                            "target_lang_translations": {
                                "ru": "Соседка — страшная <бедолага>, которая едва сводит концы с концами."
                            }
                        }
                    ],
                    "synonyms": [
                        "biedaczka",
                        "nieboraczka",
                        "biedula"
                    ],
                    "antonyms": [
                        "szczęściara"
                    ],
                    "common_phrases": [
                        "taka bidula",
                        "straszna bidula"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девушка, вызывающая жалость и сочувствие из-за своей судьбы, внешнего вида или бедности; разговорный вариант слова biedula."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедняжка",
                                "target_lang_sense_clarification": "ласкательно, о ком-то, вызывающем жалость"
                            },
                            {
                                "target_lang_word": "бедняга",
                                "target_lang_sense_clarification": "общий род, о несчастном человеке"
                            },
                            {
                                "target_lang_word": "горемыка",
                                "target_lang_sense_clarification": "разг., о человеке с тяжелой судьбой"
                            }
                        ]
                    }
                },
                {

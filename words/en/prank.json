{
    "word_family": [
        "prankster",
        "prankish"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0d4af3b-896b-3d20-9594-b4032b2eb491",
                    "sense_definition": "A practical joke or mischievous trick.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_trick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students decided to play a harmless <w>prank</w> on their teacher.",
                            "target_lang_translations": {
                                "ru": "Студенты решили устроить безобидный <w>розыгрыш</w> над своим учителем."
                            }
                        },
                        {
                            "text": "He pulled a funny <w>prank</w> on his sister yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера он устроил забавную <w>шалость</w> со своей сестрой."
                            }
                        }
                    ],
                    "synonyms": [
                        "practical joke",
                        "trick",
                        "gag",
                        "lark",
                        "shenanigan"
                    ],
                    "common_phrases": [
                        "play a prank",
                        "pull a prank",
                        "harmless prank"
                    ],
                    "target_lang_definitions": {
                        "ru": "Розыгрыш или озорная выходка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розыгрыш",
                                "target_lang_sense_clarification": "шутка над кем-то"
                            },
                            {
                                "target_lang_word": "шалость",
                                "target_lang_sense_clarification": "озорство"
                            },
                            {
                                "target_lang_word": "выходка",
                                "target_lang_sense_clarification": "поступок"
                            },
                            {
                                "target_lang_word": "пранк",
                                "target_lang_sense_clarification": "современный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e956e417-c80f-34b8-9b58-36e20005841f",
                    "sense_definition": "An evil deed; a malicious trick or an act of cruel deception.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_trick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cruel <w>prank</w> left the victim in tears and distress.",
                            "target_lang_translations": {
                                "ru": "Эта жестокая <w>выходка</w> довела жертву до слёз и отчаяния."
                            }
                        },
                        {
                            "text": "He was known for many a wicked <w>prank</w> in his youth.",
                            "target_lang_translations": {
                                "ru": "В молодости он был известен своими злыми <w>проделками</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trick"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically implies harmlessness."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Злое деяние; злонамеренная уловка или акт жестокого обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злая выходка",
                                "target_lang_sense_clarification": "жестокий поступок"
                            },
                            {
                                "target_lang_word": "проделка",
                                "target_lang_sense_clarification": "хитрость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17a6e49f-4aca-341b-a040-f1e0ebdb58aa",
                    "sense_definition": "To perform a practical joke on someone; to trick and make a fool of them.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_trick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They planned to <w>prank</w> their friend by hiding his phone.",
                            "target_lang_translations": {
                                "ru": "Они планировали <w>разыграть</w> друга, спрятав его телефон."
                            }
                        },
                        {
                            "text": "I can't believe you <w>pranked</w> me like that!",
                            "target_lang_translations": {
                                "ru": "Не могу поверить, что ты так меня <w>разыграл</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "trick",
                        "lark",
                        "gag",
                        "windup",
                        "rag"
                    ],
                    "common_phrases": [
                        "get pranked",
                        "prank someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разыграть кого-то, устроив практическую шутку; выставить кого-то в глупом свете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разыграть",
                                "target_lang_sense_clarification": "подшутить"
                            },
                            {
                                "target_lang_word": "подшутить",
                                "target_lang_sense_clarification": "сделать объектом шутки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb3f8248-1dab-3095-86e8-2d264f8fdcea",
                    "sense_definition": "To call someone's phone and hang up before they answer, usually to signal them without paying for the call.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_call",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Just <w>prank</w> me when you are outside so I can come out.",
                            "target_lang_translations": {
                                "ru": "Просто <w>маякни</w> мне, когда будешь на улице, чтобы я вышел."
                            }
                        },
                        {
                            "text": "I do not have credit, so I will <w>prank</w> you when I arrive.",
                            "target_lang_translations": {
                                "ru": "У меня нет денег на счету, поэтому я <w>сделаю дозвон</w>, когда приеду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Позвонить кому-то и сбросить вызов до ответа, обычно чтобы подать сигнал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маякнуть",
                                "target_lang_sense_clarification": "разг. дать сигнал звонком"
                            },
                            {
                                "target_lang_word": "сделать дозвон",
                                "target_lang_sense_clarification": "сбросить вызов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "017710ba-67d4-3d72-92aa-f78d624ebb84",
                    "sense_definition": "To adorn in a showy manner; to dress or equip ostentatiously.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_adorn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was <w>pranked</w> in her finest jewels for the ball.",
                            "target_lang_translations": {
                                "ru": "Она <w>вырядилась</w> в свои лучшие драгоценности для бала."
                            }
                        },
                        {
                            "text": "The hall was <w>pranked</w> with garlands and ribbons.",
                            "target_lang_translations": {
                                "ru": "Зал был <w>украшен</w> гирляндами и лентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "caper",
                        "curvet"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наряжать в броской манере; одевать или украшать напоказ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырядиться",
                                "target_lang_sense_clarification": "одеться напоказ"
                            },
                            {
                                "target_lang_word": "украсить",
                                "target_lang_sense_clarification": "декорировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
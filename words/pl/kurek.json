{
    "word_family": [
        "kur",
        "kura",
        "kurka",
        "kurkowy",
        "kurkowaty"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d798a876-fb2b-3929-8180-d668b540248e",
                    "sense_definition": "Element armatury (np. w instalacji wodnej lub gazowej), często w postaci pokrętła lub dźwigni, służący do otwierania, zamykania lub regulacji przepływu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mechanics_valve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjazdem na wakacje zakręć główny <kurek> z wodą.",
                            "target_lang_translations": {
                                "ru": "Перед отъездом в отпуск перекрой главный <вентиль> подачи воды."
                            }
                        },
                        {
                            "text": "Hydraulik wymienił uszczelkę, ponieważ <kurek> przy zlewie przeciekał.",
                            "target_lang_translations": {
                                "ru": "Сантехник заменил прокладку, так как <кран> у раковины протекал."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawór",
                        "kran"
                    ],
                    "common_phrases": [
                        "zakręcić kurek",
                        "odkręcić kurek",
                        "kurek z gazem",
                        "główny kurek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент арматуры (например, в водопроводной или газовой системе), часто в виде ручки или рычага, служащий для открытия, закрытия или регулирования потока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кран",
                                "target_lang_sense_clarification": "водопроводный или газовый"
                            },
                            {
                                "target_lang_word": "вентиль",
                                "target_lang_sense_clarification": "запорный клапан"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff2bb0ff-187b-3aa8-8a42-69553c731870",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>kurka</w> (oznaczającego popularny grzyb jadalny lub małą kurę).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku w lesie jest bardzo dużo <kurek>.",
                            "target_lang_translations": {
                                "ru": "В этом году в лесу очень много <лисичек>."
                            }
                        },
                        {
                            "text": "Na obiad przygotowaliśmy pyszny sos z <kurek> i śmietany.",
                            "target_lang_translations": {
                                "ru": "На обед мы приготовили вкусный соус из <лисичек> и сметаны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sos z kurek",
                        "zbieranie kurek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of kurka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного <w>kurka</w> (обозначающего популярный съедобный гриб или маленькую курицу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лисичек",
                                "target_lang_sense_clarification": "грибы (родительный падеж)"
                            },
                            {
                                "target_lang_word": "курочек",
                                "target_lang_sense_clarification": "птицы (родительный падеж)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e4c7385-d8ec-396a-9893-73a37f703823",
                    "sense_definition": "Część mechanizmu uderzeniowego w broni palnej, która po zwolnieniu spustu uderza w iglicę, powodując wystrzał.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "weaponry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rewolwerowiec powoli odwiódł <kurek> broni, celując w tarczę.",
                            "target_lang_translations": {
                                "ru": "Стрелок медленно взвел <курок> оружия, целясь в мишень."
                            }
                        },
                        {
                            "text": "Usłyszałem cichy trzask, gdy <kurek> opadł na pustą komorę nabojową.",
                            "target_lang_translations": {
                                "ru": "Я услышал тихий щелчок, когда <курок> ударил по пустой патронник."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odwieść kurek",
                        "spuścić kurek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть ударного механизма в огнестрельном оружии, которая после нажатия на спуск ударяет по бойку, вызывая выстрел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курок",
                                "target_lang_sense_clarification": "часть оружия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d00c19eb-06fe-3166-b420-cc8f7dc232f7",
                    "sense_definition": "Zdrobnienie od słowa <w>kur</w> (kogut); mały kogut lub przedmiot go przypominający.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na płocie siedział mały, kolorowy <kurek> i głośno piał.",
                            "target_lang_translations": {
                                "ru": "На заборе сидел маленький разноцветный <петушок> и громко кукарекал."
                            }
                        },
                        {
                            "text": "Dziecko bawiło się drewnianym <kurkiem> na kółkach.",
                            "target_lang_translations": {
                                "ru": "Ребенок играл с деревянным <петушком> на колесиках."
                            }
                        }
                    ],
                    "synonyms": [
                        "kogucik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова <w>kur</w> (петух); маленький петух или предмет, его напоминающий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петушок",
                                "target_lang_sense_clarification": "уменьшительное от петух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dd90dd6-7110-3832-a04d-53926ffcd18c",
                    "sense_definition": "Ruchoma figurka w kształcie koguta umieszczana na szczycie dachu, wskazująca kierunek wiatru.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "architecture_decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Metalowy <kurek> na wieży kościoła obracał się niespokojnie na wietrze.",
                            "target_lang_translations": {
                                "ru": "Металлический <флюгер> на башне церкви беспокойно вращался на ветру."
                            }
                        },
                        {
                            "text": "Stary dom był ozdobiony zardzewiałym <kurkiem>, który wskazywał północ.",
                            "target_lang_translations": {
                                "ru": "Старый дом был украшен ржавым <флюгером>, который указывал на север."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kurek na dachu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная фигурка в форме петуха, размещаемая на крыше и указывающая направление ветра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флюгер",
                                "target_lang_sense_clarification": "устройство для определения ветра"
                            },
                            {
                                "target_lang_word": "петушок",
                                "target_lang_sense_clarification": "фигурка на крыше"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5972a2b5-fabf-38ad-a27a-3cef01ee0434",
                    "sense_definition": "Drapieżna ryba morska o dużej głowie i jaskrawym ubarwieniu, potrafiąca wydawać charakterystyczne dźwięki.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędkarz był zaskoczony, gdy wyciągnął z wody dziwnego <kurka>.",
                            "target_lang_translations": {
                                "ru": "Рыбак удивился, когда вытащил из воды странного <морского петуха>."
                            }
                        },
                        {
                            "text": "<Kurek> szary jest gatunkiem ryby występującym w Bałtyku.",
                            "target_lang_translations": {
                                "ru": "Серый <морской петух> — это вид рыбы, обитающий в Балтийском море."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "biological/fishing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хищная морская рыба с большой головой и яркой окраской, способная издавать характерные звуки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морской петух",
                                "target_lang_sense_clarification": "рыба"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "98ce8521-bd36-3b8f-9778-a0ff3a5391bd",
                    "sense_definition": "Popularne nazwisko polskie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Kurek> mieszka w naszym bloku od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Господин <Курек> живет в нашем доме много лет."
                            }
                        },
                        {
                            "text": "Na liście obecności brakuje dzisiaj Anny <Kurek>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих сегодня нет Анны <Курек>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "common Polish surname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Курек",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
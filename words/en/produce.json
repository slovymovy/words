    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dddbfa02-5b24-35c3-9ed4-4a3a86b4cca5",
                    "sense_definition": "To make, manufacture, or create something, often as a result of a process.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory was built to <w>produce</w> cars for the international market.",
                            "target_lang_translations": {
                                "ru": "Завод был построен, чтобы <w>производить</w> автомобили для международного рынка."
                            }
                        },
                        {
                            "text": "Bees <w>produce</w> honey from the nectar they collect.",
                            "target_lang_translations": {
                                "ru": "Пчелы <w>производят</w> мед из нектара, который они собирают."
                            }
                        }
                    ],
                    "synonyms": [
                        "make",
                        "create",
                        "generate",
                        "yield",
                        "form",
                        "bring forth"
                    ],
                    "antonyms": [
                        "destroy",
                        "ruin"
                    ],
                    "common_phrases": [
                        "mass-produce",
                        "produce results",
                        "produce energy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать, изготавливать или выпускать что-либо, часто в результате определенного процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производить",
                                "target_lang_sense_clarification": "делать, создавать"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "производить продукцию"
                            },
                            {
                                "target_lang_word": "изготавливать",
                                "target_lang_sense_clarification": "делать, мастерить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ce6e7ae-d89a-30a9-ac34-32f812453608",
                    "sense_definition": "To show something or make it available for inspection.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "presentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>produce</w> your passport when you arrive at the border.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>предъявить</w> паспорт по прибытии на границу."
                            }
                        },
                        {
                            "text": "The lawyer asked the witness to <w>produce</w> evidence to support the claim.",
                            "target_lang_translations": {
                                "ru": "Адвокат попросил свидетеля <w>представить</w> доказательства в поддержку заявления."
                            }
                        }
                    ],
                    "synonyms": [
                        "present",
                        "offer"
                    ],
                    "common_phrases": [
                        "produce evidence",
                        "produce identification",
                        "produce a ticket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показать что-либо или предоставить для проверки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предъявлять",
                                "target_lang_sense_clarification": "показывать документ"
                            },
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "предоставлять доказательства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d9b547f-e18e-3da6-97d9-f95b5ac804a0",
                    "sense_definition": "To organize and supervise the making of a film, play, or musical recording.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "media_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>produce</w> the film herself to maintain creative control.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>продюсировать</w> фильм сама, чтобы сохранить творческий контроль."
                            }
                        },
                        {
                            "text": "It costs a lot of money to <w>produce</w> a high-quality television show.",
                            "target_lang_translations": {
                                "ru": "<w>Продюсировать</w> качественное телешоу стоит больших денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "create",
                        "make"
                    ],
                    "common_phrases": [
                        "produce a film",
                        "produce an album",
                        "produce a show"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовывать и контролировать создание фильма, спектакля или музыкальной записи."
                    },
                    "translations": {
                        "ru": [
                            {

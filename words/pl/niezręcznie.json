                        "trzymać niezręcznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неудобным образом, затрудняющим выполнение действий или доступ к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудобно",
                                "target_lang_sense_clarification": "физическое неудобство"
                            },
                            {
                                "target_lang_word": "несподручно",
                                "target_lang_sense_clarification": "затруднительно для выполнения"

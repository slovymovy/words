                },
                {
                    "sense_id": "36086755-3bda-3fa3-a7c4-ad54c5993fc5",
                    "sense_definition": "A set of drums and cymbals arranged to be played by one person.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The drummer sat behind his <w>kit</w> and started to play a beat.",
                            "target_lang_translations": {
                                "ru": "Барабанщик сел за свою <w>установку</w> и начал играть ритм."
                            }
                        },
                        {
                            "text": "He bought a new electronic drum <w>kit</w> to practice quietly.",
                            "target_lang_translations": {
                                "ru": "Он купил новую электронную ударную <w>установку</w>, чтобы репетировать тихо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drum kit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор барабанов и тарелок, расставленных для игры одним человеком."

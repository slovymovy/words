                },
                {
                    "sense_id": "e05af64a-7fed-3118-81c2-01d9c1749b0c",
                    "sense_definition": "De geestelijke of lichamelijke energievoorraad van een persoon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "energy_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het weekend neem ik rust om mijn <w>accu</w> weer op te laden.",
                            "target_lang_translations": {
                                "ru": "В выходные я отдыхаю, чтобы подзарядить свои <w>батарейки</w>."
                            }
                        },
                        {
                            "text": "Na die drukke werkweek was zijn <w>accu</w> volledig leeg.",
                            "target_lang_translations": {
                                "ru": "После этой напряженной рабочей недели его <w>силы</w> полностью иссякли."
                            }
                        }
                    ],
                    "synonyms": [
                        "accumulator"
                    ],
                    "common_phrases": [
                        "de accu opladen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запас душевных или физических сил человека."

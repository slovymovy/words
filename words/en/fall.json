    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c1a8bfd-9c4b-3d2d-ba1d-491645bcd4af",
                    "sense_definition": "To move downwards to a lower position, usually by accident or under the effect of gravity.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apple <w>fell</w> from the tree and hit the ground.",
                            "target_lang_translations": {
                                "ru": "Яблоко <w>упало</w> с дерева и ударилось о землю."
                            }
                        },
                        {
                            "text": "Please be careful not to <w>fall</w> on the icy stairs.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будь осторожен, чтобы не <w>упасть</w> на обледенелых ступеньках."
                            }
                        },
                        {
                            "text": "He <w>fell</w> down and hurt his knee while running.",
                            "target_lang_translations": {
                                "ru": "Он <w>упал</w> и ушиб колено во время бега."
                            }
                        }
                    ],
                    "synonyms": [
                        "drop",
                        "descend",
                        "tumble",
                        "plummet",
                        "plunge"
                    ],
                    "antonyms": [
                        "rise",
                        "ascend",
                        "go up"
                    ],
                    "common_phrases": [
                        "fall down",
                        "fall off",
                        "fall over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться вниз в более низкое положение, обычно случайно или под действием гравитации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "двигаться вниз"
                            },
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0e486ff-a675-3f93-8f7b-b9383f946eaf",
                    "sense_definition": "To pass into a particular state or condition, often suddenly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was so tired that she <w>fell</w> asleep immediately.",
                            "target_lang_translations": {
                                "ru": "Она так устала, что сразу же <w>уснула</w>."
                            }
                        },
                        {
                            "text": "They <w>fell</w> in love during their summer vacation.",
                            "target_lang_translations": {
                                "ru": "Они <w>влюбились</w> друг в друга во время летнего отпуска."
                            }
                        },
                        {
                            "text": "The audience <w>fell</w> silent as the show began.",
                            "target_lang_translations": {
                                "ru": "Зрители <w>замолчали</w>, когда началось представление."
                            }
                        }
                    ],
                    "synonyms": [
                        "become",
                        "get"
                    ],
                    "common_phrases": [
                        "fall asleep",
                        "fall in love",
                        "fall ill",
                        "fall silent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходить в определенное состояние или условие, часто внезапно."

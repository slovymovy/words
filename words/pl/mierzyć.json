    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7be9c25c-c0d0-3d55-9421-4ac38887ca1a",
                    "sense_definition": "Sprawdzać wielkość, ilość, długość lub stopień czegoś za pomocą odpowiedniego przyrządu lub wzorca.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "measurement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka musi regularnie <mierzyć> pacjentowi temperaturę.",
                            "target_lang_translations": {
                                "ru": "Медсестра должна регулярно <измерять> пациенту температуру."
                            }
                        },
                        {
                            "text": "Zanim kupisz nowe meble, powinieneś dokładnie <mierzyć> pokój.",
                            "target_lang_translations": {
                                "ru": "Прежде чем покупать новую мебель, тебе следует точно <измерить> комнату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mierzyć temperaturę",
                        "mierzyć czas",
                        "mierzyć ciśnienie",
                        "mierzyć odległość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять величину, количество, длину или степень чего-либо с помощью соответствующего прибора или эталона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерять",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "мерить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e44a95c-0050-326e-a404-93f45820c5ea",
                    "sense_definition": "Zakładać na siebie ubranie lub obuwie, aby sprawdzić, czy pasuje i dobrze leży.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię <mierzyć> różne sukienki przed wielkim wyjściem.",
                            "target_lang_translations": {
                                "ru": "Я люблю <примерять> разные платья перед важным выходом."
                            }
                        },
                        {
                            "text": "Czy mogę <mierzyć> te buty w rozmiarze trzydzieści osiem?",
                            "target_lang_translations": {
                                "ru": "Могу я <примерить> эти туфли тридцать восьмого размера?"
                            }
                        }
                    ],
                    "synonyms": [
                        "przymierzać"
                    ],
                    "common_phrases": [
                        "mierzyć ubrania",
                        "mierzyć buty",
                        "mierzyć spodnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать одежду или обувь, чтобы проверить, подходит ли она и хорошо ли сидит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примерять",
                                "target_lang_sense_clarification": "одежду или обувь"
                            },
                            {
                                "target_lang_word": "мерить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8907dd1c-04ab-3293-b381-09a254f2cffc",
                    "sense_definition": "Kierować broń, wzrok lub inny przedmiot w stronę określonego celu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "aiming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wolno <mierzyć> do ludzi z naładowanej broni.",
                            "target_lang_translations": {
                                "ru": "Нельзя <целиться> в людей из заряженного оружия."
                            }
                        },
                        {
                            "text": "Zaczął <mierzyć> wzrokiem nieznajomego, który wszedł do sali.",
                            "target_lang_translations": {
                                "ru": "Он начал <мерить> взглядом незнакомца, вошедшего в зал."
                            }
                        }
                    ],
                    "synonyms": [
                        "celować"
                    ],
                    "common_phrases": [
                        "mierzyć z pistoletu",
                        "mierzyć wzrokiem",
                        "mierzyć do celu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять оружие, взгляд или другой предмет в сторону определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целиться",
                                "target_lang_sense_clarification": "из оружия"
                            },
                            {
                                "target_lang_word": "мерить",
                                "target_lang_sense_clarification": "взглядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bda6b5f9-f88a-3829-a743-243e54e72279",
                    "sense_definition": "Mieć określoną wielkość, długość, szerokość lub wysokość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dimensions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary dąb może <mierzyć> ponad dwadzieścia metrów wysokości.",
                            "target_lang_translations": {
                                "ru": "Этот старый дуб может <достигать> более двадцати метров в высоту."
                            }
                        },
                        {
                            "text": "Mój brat szybko rośnie i wkrótce będzie <mierzyć> dwa metry.",
                            "target_lang_translations": {
                                "ru": "Мой брат быстро растет и скоро <достигнет> двух метров роста."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mierzyć dwa metry",
                        "mierzyć dużo",
                        "mierzyć mało"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенную величину, длину, ширину или высоту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "размера или высоты"
                            },
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "о величине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bc91abd-887f-33d0-9279-db6ce0113aaa",
                    "sense_definition": "Stawiać czoła trudnościom lub rywalizować z kimś (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "confrontation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz odważnie <mierzyć> się ze swoimi lękami i problemami.",
                            "target_lang_translations": {
                                "ru": "Ты должен смело <бороться> со своими страхами и проблемами."
                            }
                        },
                        {
                            "text": "W finale zawodnik będzie <mierzyć> się z aktualnym mistrzem świata.",
                            "target_lang_translations": {
                                "ru": "В финале спортсмен будет <сражаться> с действующим чемпионом мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamierzać się",
                        "próbować się"
                    ],
                    "common_phrases": [
                        "mierzyć się z problemem",
                        "mierzyć się z wyzwaniem",
                        "mierzyć się z przeciwnikiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Противостоять трудностям или соперничать с кем-либо (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сражаться",
                                "target_lang_sense_clarification": "с противником"
                            },
                            {
                                "target_lang_word": "бороться",
                                "target_lang_sense_clarification": "с проблемами"
                            },
                            {
                                "target_lang_word": "состязаться",
                                "target_lang_sense_clarification": "в спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1dd1cea-2e53-32b5-95a1-2a1bdb5e348e",
                    "sense_definition": "Dążyć do osiągnięcia ambitnego celu (często w wyrażeniu „mierzyć wysoko”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze warto <mierzyć> wysoko i spełniać swoje marzenia.",
                            "target_lang_translations": {
                                "ru": "Всегда стоит <метить> высоко и исполнять свои мечты."
                            }
                        },
                        {
                            "text": "Młody polityk zdaje się <mierzyć> w najwyższe stanowiska w państwie.",
                            "target_lang_translations": {
                                "ru": "Молодой политик, кажется, <метит> на самые высокие посты в государстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdążać",
                        "kierować się"
                    ],
                    "common_phrases": [
                        "mierzyć wysoko",
                        "mierzyć w sukces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремиться к достижению амбициозной цели (часто в выражении «mierzyć wysoko»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метить",
                                "target_lang_sense_clarification": "на должность или высокий пост"
                            },
                            {
                                "target_lang_word": "стремиться",
                                "target_lang_sense_clarification": "к цели"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52205e46-2008-3f93-a383-b90a5bf1a2a5",
                    "sense_definition": "One of two usually roughly equal parts into which anything may be divided.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please cut the apple in <w>half</w> so we can share it.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, разрежь яблоко <w>пополам</w>, чтобы мы могли поделиться."
                            }
                        },
                        {
                            "text": "He ate <w>half</w> of the pizza before anyone else arrived.",
                            "target_lang_translations": {
                                "ru": "Он съел <w>половину</w> пиццы до того, как кто-либо пришел."
                            }
                        }
                    ],
                    "synonyms": [
                        "½"
                    ],
                    "common_phrases": [
                        "in half",
                        "cut in half",
                        "break in half",
                        "bigger half"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух обычно примерно равных частей, на которые что-либо может быть разделено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "общая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91c0cde8-3617-3dd7-9c3d-d588b61bb34b",
                    "sense_definition": "The fraction obtained by dividing one by two (1/2).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "One <w>half</w> plus one quarter equals three quarters.",
                            "target_lang_translations": {
                                "ru": "Одна <w>вторая</w> плюс одна четверть равно три четверти."
                            }
                        },
                        {
                            "text": "The recipe calls for a <w>half</w> of a cup of sugar.",
                            "target_lang_translations": {
                                "ru": "В рецепте указана <w>половина</w> чашки сахара."
                            }
                        }
                    ],
                    "synonyms": [
                        "½"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дробь, получаемая при делении единицы на два (1/2)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одна вторая",
                                "target_lang_sense_clarification": "математическая дробь"
                            },
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "678856e2-a27b-3d44-8093-a028fad96576",
                    "sense_definition": "One of two equal periods into which a game (such as football or rugby) is divided.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our team played much better in the second <w>half</w>.",
                            "target_lang_translations": {
                                "ru": "Наша команда играла намного лучше во втором <w>тайме</w>."
                            }
                        },
                        {
                            "text": "The referee added three minutes of stoppage time to the first <w>half</w>.",
                            "target_lang_translations": {
                                "ru": "Судья добавил три минуты к первому <w>тайму</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first half",
                        "second half",
                        "bottom half"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двух равных периодов, на которые делится игра (например, в футболе или регби)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тайм",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3609e9c-fb67-3d09-82d5-2071a1620d4b",
                    "sense_definition": "One of the two opposite parts of a playing field where each team starts the game.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The players kept the ball in their own <w>half</w> to waste time.",
                            "target_lang_translations": {
                                "ru": "Игроки держали мяч на своей <w>половине</w> поля, чтобы тянуть время."
                            }
                        },
                        {
                            "text": "He ran from his own <w>half</w> to score a spectacular goal.",
                            "target_lang_translations": {
                                "ru": "Он пробежал со своей <w>половины</w> поля, чтобы забить эффектный гол."
                            }
                        }
                    ],
                    "common_phrases": [
                        "own half",
                        "opposing half"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух противоположных частей игрового поля, где каждая команда начинает игру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "часть поля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54907559-f808-37f0-b4fb-7edb05a62fae",
                    "sense_definition": "A standard measure of drink equal to half a pint, usually of beer or cider.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will just have a <w>half</w> of lager, please.",
                            "target_lang_translations": {
                                "ru": "Я буду только <w>полпинты</w> лагера, пожалуйста."
                            }
                        },
                        {
                            "text": "He ordered a pint for himself and a <w>half</w> for his friend.",
                            "target_lang_translations": {
                                "ru": "Он заказал пинту для себя и <w>полпинты</w> для друга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "half of lager",
                        "half of bitter"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартная мера напитка, равная половине пинты, обычно пива или сидра."

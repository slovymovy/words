    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "341cdf1f-e50e-3dbb-84c6-4e3bafdd35f8",
                    "sense_definition": "The act of obtaining goods, services, or supplies, especially for a company or organization.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was responsible for the <w>procurement</w> of materials and supplies.",
                            "target_lang_translations": {
                                "ru": "Он отвечал за <w>закупку</w> материалов и припасов."
                            }
                        },
                        {
                            "text": "The government has strict rules regarding the <w>procurement</w> of military equipment.",
                            "target_lang_translations": {
                                "ru": "У правительства есть строгие правила касательно <w>закупок</w> военного оборудования."
                            }
                        }
                    ],
                    "synonyms": [
                        "acquisition",
                        "obtainment"
                    ],
                    "common_phrases": [
                        "procurement process",
                        "public procurement",
                        "procurement strategy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс приобретения товаров, услуг или ресурсов, особенно для компании или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закупка",
                                "target_lang_sense_clarification": "приобретение товаров, обычно оптовое"
                            },
                            {
                                "target_lang_word": "снабжение",
                                "target_lang_sense_clarification": "обеспечение необходимыми ресурсами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf8a1c24-435a-3edd-bd08-51bc38e16d37",
                    "sense_definition": "The department within a company that is responsible for purchasing goods and services.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to send that invoice to <w>procurement</w> for final approval.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно отправить этот счет в <w>отдел закупок</w> для окончательного утверждения."
                            }
                        },
                        {
                            "text": "She has worked in <w>procurement</w> for over ten years.",
                            "target_lang_translations": {
                                "ru": "Она проработала в <w>отделе снабжения</w> более десяти лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "head of procurement",
                        "work in procurement",
                        "procurement team"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдел в компании, отвечающий за закупку товаров и услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдел закупок",
                                "target_lang_sense_clarification": "подразделение компании"
                            },
                            {
                                "target_lang_word": "отдел снабжения",
                                "target_lang_sense_clarification": "подразделение по обеспечению ресурсами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0828c5f4-cbbd-3767-896a-382a15a2509a",
                    "sense_definition": "The act of bringing something about through management, agency, or secret contrivance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They think the event was done by her <w>procurement</w>.",
                            "target_lang_translations": {
                                "ru": "Они полагают, что это событие произошло при её <w>содействии</w>."
                            }
                        },
                        {
                            "text": "The meeting happened through the <w>procurement</w> of a mutual friend.",
                            "target_lang_translations": {
                                "ru": "Встреча состоялась при <w>посредничестве</w> общего друга."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Refers to the archaic sense of management or agency."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, направленное на достижение чего-либо посредством управления, посредничества или тайных ухищрений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содействие",
                                "target_lang_sense_clarification": "помощь или участие в достижении цели"
                            },
                            {
                                "target_lang_word": "посредничество",
                                "target_lang_sense_clarification": "участие третьей стороны"
                            }
                        ]
                    }
                }
            ]
        }
    ]

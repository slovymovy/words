    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2aacdd74-6ba7-323e-aebc-3012998fde83",
                    "sense_definition": "The ability to learn, understand, and think in a logical way about things; mental capacity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist specializes in artificial <w>intelligence</w> and robotics research.",
                            "target_lang_translations": {
                                "ru": "Ученый специализируется на исследованиях в области искусственного <w>интеллекта</w> и робототехники."
                            }
                        },
                        {
                            "text": "Dogs are often praised for their high <w>intelligence</w> and ability to learn tricks.",
                            "target_lang_translations": {
                                "ru": "Собак часто хвалят за высокий <w>интеллект</w> и способность обучаться трюкам."
                            }
                        }
                    ],
                    "synonyms": [
                        "intellect",
                        "brains",
                        "cleverness",
                        "mind",
                        "reason",
                        "smartness",
                        "wit",
                        "judgment",
                        "brightness"
                    ],
                    "antonyms": [
                        "stupidity",
                        "unintelligence",
                        "idiocy",
                        "imbecility"
                    ],
                    "common_phrases": [
                        "artificial intelligence",
                        "emotional intelligence",
                        "high intelligence",
                        "lack of intelligence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность учиться, понимать и логически мыслить; умственные способности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интеллект",
                                "target_lang_sense_clarification": "mental capacity, often scientific or formal"
                            },
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "general mental ability, wit"
                            },
                            {
                                "target_lang_word": "разум",
                                "target_lang_sense_clarification": "reason, higher cognitive faculty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c72553c-abe8-3969-8ae8-634e2524dcd7",
                    "sense_definition": "Secret information that is collected, for example about a foreign country or an enemy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government received secret <w>intelligence</w> regarding a planned attack on the city.",
                            "target_lang_translations": {
                                "ru": "Правительство получило секретные <w>разведданные</w> о планируемом нападении на город."
                            }
                        },
                        {
                            "text": "Soldiers were sent to the border to gather <w>intelligence</w> on enemy movements.",
                            "target_lang_translations": {
                                "ru": "Солдаты были отправлены на границу для сбора <w>разведданных</w> о передвижениях противника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gather intelligence",
                        "military intelligence",
                        "intelligence report",
                        "actionable intelligence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секретная информация, собранная о противнике или иностранном государстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведданные",
                                "target_lang_sense_clarification": "intelligence data, military context"
                            },
                            {
                                "target_lang_word": "сведения",
                                "target_lang_sense_clarification": "information, news"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "531395de-9fc8-3118-be47-2fe2ed72609e",
                    "sense_definition": "A government organization or department that collects secret information about enemies or criminals.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He worked for British <w>intelligence</w> during the war as a code breaker.",
                            "target_lang_translations": {
                                "ru": "Во время войны он работал в британской <w>разведке</w> дешифровщиком."
                            }
                        },
                        {
                            "text": "The <w>intelligence</w> officer analyzed the documents carefully to find the source of the leak.",
                            "target_lang_translations": {
                                "ru": "Офицер <w>разведки</w> тщательно проанализировал документы, чтобы найти источник утечки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "intelligence agency",
                        "intelligence officer",
                        "intelligence service",
                        "naval intelligence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правительственная организация или служба, занимающаяся сбором секретной информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведка",
                                "target_lang_sense_clarification": "intelligence service or activity"
                            },
                            {
                                "target_lang_word": "разведслужба",
                                "target_lang_sense_clarification": "intelligence agency"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7ade009-1d54-38db-9e9c-34a3725e0c28",
                    "sense_definition": "A being or entity that has the ability to think and understand, such as a spirit or an advanced computer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists wonder if there is an alien <w>intelligence</w> trying to contact Earth from deep space.",
                            "target_lang_translations": {
                                "ru": "Ученые гадают, пытается ли инопланетный <w>разум</w> связаться с Землей из глубокого космоса."
                            }
                        },
                        {
                            "text": "The machine became a vast <w>intelligence</w> capable of controlling the entire network.",
                            "target_lang_translations": {
                                "ru": "Машина превратилась в мощный <w>интеллект</w>, способный управлять всей сетью."
                            }
                        }
                    ],
                    "synonyms": [
                        "sophont"
                    ],
                    "common_phrases": [
                        "alien intelligence",
                        "vast intelligence",
                        "higher intelligence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существо или сущность, обладающая способностью мыслить и понимать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разум",
                                "target_lang_sense_clarification": "sentient entity (e.g., alien intelligence)"
                            },
                            {
                                "target_lang_word": "интеллект",
                                "target_lang_sense_clarification": "cognitive entity (e.g., AI)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bda6c580-b674-3fba-9499-79a45af87423",
                    "sense_definition": "Communication or news communicated between people; mutual understanding.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two spies maintained a secret <w>intelligence</w> through coded letters sent every month.",
                            "target_lang_translations": {
                                "ru": "Два шпиона поддерживали тайную <w>связь</w> с помощью зашифрованных писем, отправляемых каждый месяц."
                            }
                        },
                        {
                            "text": "He had no <w>intelligence</w> of his family's whereabouts for many years after the war.",
                            "target_lang_translations": {
                                "ru": "У него не было никаких <w>сведений</w> о местонахождении его семьи в течение многих лет после войны."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Общение или новости, передаваемые между людьми; взаимопонимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сведения",
                                "target_lang_sense_clarification": "news, information"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "communication, connection"
                            }
                        ]
                    }
                }
            ]
        }
    ]

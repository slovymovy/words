                        "verminderen",
                        "slinken"
                    ],
                    "antonyms": [
                        "toenemen",
                        "groeien"
                    ],
                    "common_phrases": [
                        "langzaam verdwijnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To decrease in quantity or strength.",
                        "ru": "Уменьшаться в количестве или силе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dwindle",
                                "target_lang_sense_clarification": "to diminish gradually in size or amount"
                            },
                            {
                                "target_lang_word": "fade",
                                "target_lang_sense_clarification": "to lose strength or intensity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "улетучиваться",
                                "target_lang_sense_clarification": "постепенно исчезать (о чувствах или надежде)"
                            },
                            {
                                "target_lang_word": "таять",
                                "target_lang_sense_clarification": "постепенно сходить на нет"
                            },
                            {
                                "target_lang_word": "рассеиваться",

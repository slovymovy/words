                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болт",
                                "target_lang_sense_clarification": "снаряд для арбалета"
                            },
                            {
                                "target_lang_word": "стрела",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "651b1f9f-2ed8-3efe-834e-955f551013d9",
                    "sense_definition": "A sieve, especially one used for sifting flour or meal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sifting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The miller used a fine <w>bolt</w> to separate the flour from the bran.",
                            "target_lang_translations": {
                                "ru": "Мельник использовал мелкое <w>сито</w>, чтобы отделить муку от отрубей."
                            }
                        },
                        {
                            "text": "Old machinery often included a wooden <w>bolt</w> for processing grain.",
                            "target_lang_translations": {
                                "ru": "Старое оборудование часто включало деревянное <w>сито</w> для обработки зерна."
                            }
                        }
                    ],
                    "synonyms": [
                        "sieve",
                        "sifter"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical milling context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сито, особенно используемое для просеивания муки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сито",
                                "target_lang_sense_clarification": "инструмент для просеивания"
                            },
                            {
                                "target_lang_word": "решето",
                                "target_lang_sense_clarification": "крупное сито"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c930f802-0377-3e6a-8b5d-fde677679e30",

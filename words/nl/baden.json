                    "sense_id": "c1df7051-9bd8-3b60-b02d-245a2a66d9ab",
                    "sense_definition": "Zich in water begeven om zich te wassen of te ontspannen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bathing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer <w>baden</w> veel toeristen in de zee.",
                            "target_lang_translations": {
                                "en": "In the summer, many tourists <w>bathe</w> in the sea.",
                                "ru": "Летом многие туристы <w>купаются</w> в море."
                            }
                        },
                        {
                            "text": "Hij <w>baadt</w> elke ochtend koud om wakker te worden.",
                            "target_lang_translations": {
                                "en": "He <w>bathes</w> in cold water every morning to wake up.",
                                "ru": "Каждое утро он <w>принимает холодную ванну</w>, чтобы проснуться."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwemmen",
                        "poedelen"
                    ],
                    "common_phrases": [
                        "in zee baden",
                        "gaan baden"
                    ],
                    "target_lang_definitions": {
                        "en": "To go into water to wash oneself or to relax",
                        "ru": "Погружаться в воду, чтобы помыться или расслабиться"
                    },
                    "translations": {

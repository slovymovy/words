                {
                    "sense_id": "b61cc6dd-717b-387d-9943-741061f24078",
                    "sense_definition": "Het lichaamsdeel aan de achterkant van de romp, tussen de nek en de billen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft veel pijn in zijn <w>rug</w> na het zware werk.",
                            "target_lang_translations": {
                                "en": "He has a lot of pain in his <w>back</w> after the heavy work.",
                                "ru": "У него сильно болит <w>спина</w> после тяжелой работы."
                            }
                        },
                        {
                            "text": "De kat ligt graag op haar <w>rug</w> in de warme zon.",
                            "target_lang_translations": {
                                "en": "The cat likes to lie on its <w>back</w> in the warm sun.",
                                "ru": "Кошка любит лежать на <w>спине</w> на теплом солнце."
                            }
                        }
                    ],
                    "antonyms": [
                        "buik"
                    ],
                    "common_phrases": [
                        "achter de rug",
                        "op je rug",
                        "een rechte rug",
                        "de rug toekeren"
                    ],
                    "target_lang_definitions": {
                        "en": "The body part at the back of the torso, between the neck and the buttocks.",
                        "ru": "Часть тела с задней стороны туловища, между шеей и ягодицами."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "574e3008-ffd0-3a8f-ad8b-ff907ab82849",
                    "sense_definition": "Having had people removed from a place, usually for safety reasons.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>evacuated</w> residents were taken to a nearby shelter for the night.",
                            "target_lang_translations": {
                                "ru": "<w>Эвакуированных</w> жителей отвезли в ближайшее убежище на ночь."
                            }
                        },
                        {
                            "text": "Police officers guarded the <w>evacuated</w> area to prevent anyone from entering.",
                            "target_lang_translations": {
                                "ru": "Полицейские охраняли <w>эвакуированную</w> территорию, чтобы никто не мог туда проникнуть."
                            }

{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "0c82dc28-dffe-340b-a17a-384d13864cd1",
                    "sense_definition": "Обозначает движение из места, находящегося позади или с обратной стороны чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Внезапно <w>из-за</w> угла выехала большая машина.",
                            "target_lang_translations": {
                                "en": "Suddenly a big car drove out <w>from around</w> the corner."
                            }
                        },
                        {
                            "text": "Яркое солнце наконец выглянуло <w>из-за</w> тёмных туч.",
                            "target_lang_translations": {
                                "en": "The bright sun finally peeked out <w>from behind</w> the dark clouds."
                            }
                        }
                    ],
                    "common_phrases": [
                        "из-за угла",
                        "из-за туч",
                        "из-за границы",
                        "из-за двери",
                        "из-за спины"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates movement from a place located behind or on the other side of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from behind",
                                "target_lang_sense_clarification": "movement from the back of something"
                            },
                            {
                                "target_lang_word": "from around",
                                "target_lang_sense_clarification": "movement from the side of an obstacle, e.g., a corner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44ffc8a8-03cf-3631-a304-4c1a7bcac157",
                    "sense_definition": "Указывает на причину, основание или виновника действия или состояния (часто с оттенком негативного результата).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы опоздали на поезд <w>из-за</w> сильного дождя.",
                            "target_lang_translations": {
                                "en": "We missed the train <w>because of</w> the heavy rain."
                            }
                        },
                        {
                            "text": "Друзья поссорились <w>из-за</w> какой-то ерунды.",
                            "target_lang_translations": {
                                "en": "The friends quarreled <w>over</w> some nonsense."
                            }
                        }
                    ],
                    "synonyms": [
                        "по причине",
                        "вследствие",
                        "ввиду"
                    ],
                    "common_phrases": [
                        "из-за этого",
                        "из-за тебя",
                        "из-за денег",
                        "из-за ерунды",
                        "из-за проблем"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the cause, reason, or source of an action or state, often with a negative result."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "because of",
                                "target_lang_sense_clarification": "general cause or reason"
                            },
                            {
                                "target_lang_word": "due to",
                                "target_lang_sense_clarification": "attributable to; caused by"
                            },
                            {
                                "target_lang_word": "on account of",
                                "target_lang_sense_clarification": "formal; regarding a reason"
                            },
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "regarding the cause of a disagreement or conflict"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79e7193f-c3a7-3254-94c6-b38a803a8316",
                    "sense_definition": "Обозначает движение от предмета, за которым человек сидел или занимался каким-либо делом.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец поблагодарил за обед и встал <w>из-за</w> стола.",
                            "target_lang_translations": {
                                "en": "Father thanked us for lunch and got up <w>from</w> the table."
                            }
                        },
                        {
                            "text": "Ученик вышел <w>из-за</w> парты и направился к доске.",
                            "target_lang_translations": {
                                "en": "The student came out <w>from behind</w> the desk and headed to the board."
                            }
                        }
                    ],
                    "common_phrases": [
                        "встать из-за стола",
                        "выйти из-за парты",
                        "встать из-за рояля"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates movement away from an object (like a table or desk) where one was sitting or working."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "leaving a position at a table or desk"
                            },
                            {
                                "target_lang_word": "from behind",
                                "target_lang_sense_clarification": "literal movement from the space behind an object"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
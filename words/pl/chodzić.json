    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d59b9392-adb2-3883-bdc3-462846a668bf",
                    "sense_definition": "Poruszać się, stawiając kroki; przemieszczać się pieszo z miejsca na miejsce (czasownik niedokonany, oznaczający czynność powtarzalną lub trwającą).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek codziennie rano <chodzi> na długi spacer do parku.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка каждое утро <ходит> на долгую прогулку в парк."
                            }
                        },
                        {
                            "text": "Małe dziecko dopiero uczy się <chodzić> i często się przewraca.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок только учится <ходить> и часто падает."
                            }
                        }
                    ],
                    "synonyms": [
                        "spacerować",
                        "stawiać kroki"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "chodzić pieszo",
                        "chodzić na palcach",
                        "chodzić spać",
                        "chodzić boso"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться пешком, делая шаги; двигаться с места на место (о повторяющемся или длительном действии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "перемещаться пешком"
                            },
                            {
                                "target_lang_word": "гулять",
                                "target_lang_sense_clarification": "прогуливаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae6a1926-c908-3e77-9a35-3cb140b197c1",
                    "sense_definition": "Regularnie udawać się w jakieś miejsce, zwykle w określonym celu, np. do pracy, szkoły lub na zajęcia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attendance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie moje dzieci <chodzą> do tej samej szkoły podstawowej.",
                            "target_lang_translations": {
                                "ru": "Все мои дети <ходят> в одну и ту же начальную школу."
                            }
                        },
                        {
                            "text": "Kiedyś regularnie <chodziłem> na basen, ale teraz nie mam czasu.",
                            "target_lang_translations": {
                                "ru": "Раньше я регулярно <ходил> в бассейн, но сейчас у меня нет времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "uczęszczać",
                        "chadzać"
                    ],
                    "common_phrases": [
                        "chodzić do szkoły",
                        "chodzić do pracy",
                        "chodzić na zajęcia",
                        "chodzić do kościoła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярно посещать какое-либо место, обычно с определенной целью (например, школу, работу или занятия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "посещать регулярно"
                            },
                            {
                                "target_lang_word": "посещать",
                                "target_lang_sense_clarification": "бывать где-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "850c713e-dc73-360c-b3bc-dea0de97bb03",
                    "sense_definition": "Dotyczyć czegoś, stanowić istotę problemu lub sytuacji; mieć coś na myśli (używane głównie w konstrukcjach typu „o co chodzi”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zupełnie nie rozumiem, o co ci teraz <chodzi>.",
                            "target_lang_translations": {
                                "ru": "Я совершенно не понимаю, о чем <идет> речь."
                            }
                        },
                        {
                            "text": "W tej grze <chodzi> o to, żeby zebrać jak najwięcej punktów.",
                            "target_lang_translations": {
                                "ru": "Смысл этой игры <заключается> в том, чтобы набрать как можно больше очков."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozchodzić się"
                    ],
                    "common_phrases": [
                        "o co chodzi",
                        "o to chodzi",
                        "nie chodzi o to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Касаться чего-либо, составлять суть проблемы или ситуации (используется в конструкциях типа «в чем дело», «речь идет о»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "в выражении «речь идет»"
                            },
                            {
                                "target_lang_word": "заключаться",
                                "target_lang_sense_clarification": "состоять в чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f6466d5-2faa-321e-a60d-cdcc2c6dceb4",
                    "sense_definition": "O urządzeniach i mechanizmach: być sprawnym, wykonywać pracę; o środkach transportu: kursować według rozkładu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "functioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary zegar na ścianie wciąż <chodzi> bardzo dokładnie.",
                            "target_lang_translations": {
                                "ru": "Эти старые часы на стене всё ещё <ходят> очень точно."
                            }
                        },
                        {
                            "text": "Autobusy do centrum miasta <chodzą> co piętnaście minut.",
                            "target_lang_translations": {
                                "ru": "Автобусы в центр города <ходят> каждые пятнадцать минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "działać",
                        "kursować"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "zegarek chodzi",
                        "silnik chodzi",
                        "winda nie chodzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "О механизмах: работать, функционировать; о транспорте: курсировать по маршруту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "о часах или транспорте"
                            },
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "функционировать"
                            },
                            {
                                "target_lang_word": "курсировать",
                                "target_lang_sense_clarification": "о транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4bb32db-e4a5-3b11-8855-9de263d5e725",
                    "sense_definition": "Nosić na sobie określone ubranie; ubierać się w pewien sposób.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "wearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą zawsze <chodzę> w ciepłej czapce i wełnianym szaliku.",
                            "target_lang_translations": {
                                "ru": "Зимой я всегда <хожу> в теплой шапке и шерстяном шарфе."
                            }
                        },
                        {
                            "text": "Ona bardzo lubi <chodzić> w eleganckich, czarnych sukienkach.",
                            "target_lang_translations": {
                                "ru": "Она очень любит <ходить> в элегантных черных платьях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chodzić w spodniach",
                        "chodzić na czarno",
                        "chodzić w czymś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Носить определенную одежду; одеваться определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "быть одетым во что-то"
                            },
                            {
                                "target_lang_word": "носить",
                                "target_lang_sense_clarification": "одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77f0fd8c-293d-3faa-be4c-5aa6038d68f8",
                    "sense_definition": "Być z kimś w romantycznej relacji, spotykać się z kimś regularnie jako para.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tomek <chodzi> z Kasią już od trzech lat i planują ślub.",
                            "target_lang_translations": {
                                "ru": "Томек <встречается> с Касей уже три года, и они планируют свадьбу."
                            }
                        },
                        {
                            "text": "Wszyscy w klasie wiedzą, że oni ze sobą <chodzą>.",
                            "target_lang_translations": {
                                "ru": "Все в классе знают, что они <встречаются>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spotykać się"
                    ],
                    "common_phrases": [
                        "chodzić z kimś",
                        "chodzić ze sobą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоять в романтических отношениях, регулярно встречаться с кем-либо как пара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "быть парой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8d9acdf-3416-3f21-8483-25d60e8b4436",
                    "sense_definition": "Starać się coś załatwić, zabiegać o coś lub opiekować się kimś.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "care_solicit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem długo <chodzić> za tym pozwoleniem w urzędzie.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось долго <ходить> за этим разрешением в учреждении."
                            }
                        },
                        {
                            "text": "Pielęgniarka <chodziła> koło chorego pacjenta przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Медсестра всю ночь <ходила> вокруг больного пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabiegać",
                        "troszczyć się",
                        "dbać"
                    ],
                    "common_phrases": [
                        "chodzić za pracą",
                        "chodzić koło kogoś",
                        "chodzić za czymś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться что-то получить, хлопотать о чем-то или заботиться о ком-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлопотать",
                                "target_lang_sense_clarification": "добиваться чего-либо"
                            },
                            {
                                "target_lang_word": "ухаживать",
                                "target_lang_sense_clarification": "заботиться"
                            },
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "разг. добиваться или ухаживать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
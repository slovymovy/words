    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e23fe94d-6e52-3f3b-9744-61b5a98366bc",
                    "sense_definition": "A formal written agreement between two or more countries, governments, or international organizations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "international_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two nations signed a peace <w>treaty</w> to officially end the war.",
                            "target_lang_translations": {
                                "ru": "Две страны подписали мирный <w>договор</w>, чтобы официально положить конец войне."
                            }
                        },
                        {
                            "text": "Under the new <w>treaty</w>, both countries agreed to reduce their carbon emissions.",
                            "target_lang_translations": {
                                "ru": "В соответствии с новым <w>договором</w> обе страны согласились сократить выбросы углекислого газа."
                            }
                        }
                    ],
                    "synonyms": [
                        "pact",
                        "accord",
                        "agreement",
                        "convention",
                        "protocol",
                        "concordat"
                    ],
                    "common_phrases": [
                        "peace treaty",
                        "sign a treaty",
                        "ratify a treaty",
                        "treaty obligation",
                        "extradition treaty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное письменное соглашение между двумя или более странами, правительствами или международными организациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "договор",
                                "target_lang_sense_clarification": "международный"
                            },
                            {
                                "target_lang_word": "соглашение",
                                "target_lang_sense_clarification": "официальная договоренность"
                            },
                            {
                                "target_lang_word": "пакт",
                                "target_lang_sense_clarification": "политический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1203fa2f-cd73-374e-b06b-683e45e642f7",
                    "sense_definition": "A method of selling property or goods through direct negotiation between the buyer and seller, rather than by public auction.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commercial_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historic estate was sold by private <w>treaty</w> to an anonymous buyer.",
                            "target_lang_translations": {
                                "ru": "Историческое поместье было продано по частному <w>соглашению</w> анонимному покупателю."
                            }
                        },
                        {
                            "text": "If the house does not sell at auction, it may be offered for sale by private <w>treaty</w>.",
                            "target_lang_translations": {
                                "ru": "Если дом не будет продан на аукционе, он может быть выставлен на продажу по частному <w>договору</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "contract",
                        "deal",
                        "compact"
                    ],
                    "antonyms": [
                        "auction"
                    ],
                    "common_phrases": [
                        "by private treaty",
                        "sale by private treaty"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "chiefly in the phrase 'private treaty'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод продажи имущества или товаров путем прямых переговоров между покупателем и продавцом, а не через публичный аукцион."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соглашение",
                                "target_lang_sense_clarification": "частная договоренность"
                            },
                            {
                                "target_lang_word": "договор",
                                "target_lang_sense_clarification": "сделка по взаимному согласию"
                            }
                        ]
                    }
                }
            ]
        }
    ]

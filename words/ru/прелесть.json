    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e49e9612-d938-3783-9964-6674c744282e",
                    "sense_definition": "Что-то очень красивое, приятное или милое; слово, выражающее высшую степень восхищения кем-либо или чем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "charm_delight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твоя новая шляпка — просто <w>прелесть</w>!",
                            "target_lang_translations": {
                                "en": "Your new hat is simply <w>lovely</w>!"
                            }
                        },
                        {
                            "text": "Какая <w>прелесть</w>, что мы наконец встретились!",
                            "target_lang_translations": {
                                "en": "What a <w>delight</w> that we have finally met!"
                            }
                        }
                    ],
                    "synonyms": [
                        "чудо",
                        "красота"
                    ],
                    "antonyms": [
                        "гадость",
                        "ужас"
                    ],
                    "common_phrases": [
                        "какая прелесть",
                        "просто прелесть"
                    ],
                    "target_lang_definitions": {
                        "en": "Something very beautiful, pleasant, or cute; a word expressing the highest degree of admiration for someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "delight",
                                "target_lang_sense_clarification": "Something or someone that gives great pleasure"
                            },
                            {
                                "target_lang_word": "lovely",
                                "target_lang_sense_clarification": "Used as a predicative adjective to describe something very beautiful or pleasant"
                            },
                            {
                                "target_lang_word": "beauty",
                                "target_lang_sense_clarification": "A beautiful person or thing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "895e8263-2a7a-3e9a-b2b6-9cd56bc0407c",
                    "sense_definition": "Обаяние, очарование, притягательная сила, исходящая от кого-либо или чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "charm_delight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом старом парке есть своя особая, тихая <w>прелесть</w>.",
                            "target_lang_translations": {
                                "en": "This old park has its own special, quiet <w>charm</w>."
                            }
                        },
                        {
                            "text": "Она ещё не понимала всей <w>прелести</w> деревенской жизни.",
                            "target_lang_translations": {
                                "en": "She did not yet understand all the <w>charms</w> of country life."
                            }
                        }
                    ],
                    "synonyms": [
                        "очарование",
                        "обаяние",
                        "притягательность"
                    ],
                    "antonyms": [
                        "безобразие",
                        "отвращение"
                    ],
                    "common_phrases": [
                        "прелесть новизны",
                        "таить в себе прелесть"
                    ],
                    "target_lang_definitions": {
                        "en": "Charm, fascination, or attractive force emanating from someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "charm",
                                "target_lang_sense_clarification": "The quality of being pleasant or attractive"
                            },
                            {
                                "target_lang_word": "fascination",
                                "target_lang_sense_clarification": "The power to fascinate or attract"
                            },
                            {
                                "target_lang_word": "allure",
                                "target_lang_sense_clarification": "The quality of being powerfully and mysteriously attractive"
                            }
                        ]

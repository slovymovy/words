{
    "word_family": [
        "animatie",
        "animeren",
        "animatiefilm"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d1c55d0-98f6-3b5c-89f1-87411c82c562",
                    "sense_definition": "Iemand die animatiefilms maakt door beelden na elkaar te plaatsen zodat er beweging ontstaat",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "film_maker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>animator</w> tekende elk frame met de hand voor de nieuwe film.",
                            "target_lang_translations": {
                                "ru": "<w>Мультипликатор</w> рисовал каждый кадр вручную для нового фильма."
                            }
                        },
                        {
                            "text": "Hij wil later graag <w>animator</w> worden bij een grote studio.",
                            "target_lang_translations": {
                                "ru": "В будущем он хочет стать <w>аниматором</w> в крупной студии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "professionele animator",
                        "3D-animator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, создающий анимационные фильмы путем последовательного соединения изображений для создания иллюзии движения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мультипликатор",
                                "target_lang_sense_clarification": "создатель мультфильмов"
                            },
                            {
                                "target_lang_word": "аниматор",
                                "target_lang_sense_clarification": "специалист по анимации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adad3547-4993-31ff-90c1-d4fff1fa51b5",
                    "sense_definition": "Iemand die als beroep groepen mensen vermaakt en activiteiten organiseert, bijvoorbeeld op een vakantiepark",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "entertainer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>animator</w> zorgde voor een leuke sfeer tijdens het kinderfeestje.",
                            "target_lang_translations": {
                                "ru": "<w>Аниматор</w> создал веселую атмосферу на детском празднике."
                            }
                        },
                        {
                            "text": "In de zomer werkt zij als <w>animator</w> op een camping in Frankrijk.",
                            "target_lang_translations": {
                                "ru": "Летом она работает <w>аниматором</w> в кемпинге во Франции."
                            }
                        }
                    ],
                    "synonyms": [
                        "animateur"
                    ],
                    "common_phrases": [
                        "enthousiaste animator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, чья профессия — развлекать группы людей и организовывать мероприятия, например, в доме отдыха"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аниматор",
                                "target_lang_sense_clarification": "организатор досуга и развлечений"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
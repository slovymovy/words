    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "17961446-ff10-354e-b0ba-740e4b54dce4",
                    "sense_definition": "An outer garment with sleeves, worn outdoors to protect the upper body from cold or rain.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She put on her warm <w>coat</w> before going out into the snow.",
                            "target_lang_translations": {
                                "ru": "Она надела свое теплое <w>пальто</w>, прежде чем выйти на снег."
                            }
                        },
                        {
                            "text": "Please hang your <w>coat</w> on the rack near the door.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, повесьте ваше <w>пальто</w> на вешалку у двери."
                            }
                        }
                    ],
                    "synonyms": [
                        "jacket",
                        "overcoat"
                    ],
                    "common_phrases": [
                        "winter coat",
                        "put on a coat",
                        "wear a coat",
                        "heavy coat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя одежда с рукавами, которую носят на улице для защиты верхней части тела от холода или дождя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пальто",
                                "target_lang_sense_clarification": "верхняя одежда"
                            },
                            {
                                "target_lang_word": "куртка",
                                "target_lang_sense_clarification": "короткая верхняя одежда"
                            },
                            {
                                "target_lang_word": "плащ",
                                "target_lang_sense_clarification": "от дождя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5912ee90-6d5d-3aae-bfe9-71a9070c0c47",
                    "sense_definition": "A layer of a substance, such as paint or dust, that covers a surface.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The walls look much better after a fresh <w>coat</w> of paint.",
                            "target_lang_translations": {
                                "ru": "Стены выглядят намного лучше после свежего <w>слоя</w> краски."
                            }
                        },
                        {
                            "text": "A thin <w>coat</w> of ice covered the windshield this morning.",
                            "target_lang_translations": {
                                "ru": "Тонкий <w>слой</w> льда покрыл лобовое стекло сегодня утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "layer",
                        "covering",
                        "film"
                    ],
                    "common_phrases": [
                        "coat of paint",
                        "fresh coat",
                        "second coat",
                        "thick coat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой вещества, например краски или пыли, покрывающий поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слой",
                                "target_lang_sense_clarification": "краски, пыли и т.д."
                            },
                            {
                                "target_lang_word": "покрытие",
                                "target_lang_sense_clarification": "защитный слой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eeda5f68-be16-38f9-aa87-cb73109104ac",
                    "sense_definition": "The natural covering of fur, hair, or wool on an animal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog has a shiny black <w>coat</w> that is very soft to touch.",
                            "target_lang_translations": {
                                "ru": "У собаки блестящая черная <w>шерсть</w>, очень мягкая на ощупь."
                            }
                        },
                        {
                            "text": "Many animals grow a thicker <w>coat</w> in the winter to stay warm.",
                            "target_lang_translations": {
                                "ru": "Многие животные отращивают более густую <w>шерсть</w> зимой, чтобы не замерзнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "fur",
                        "pelt",
                        "fleece"
                    ],
                    "common_phrases": [
                        "shiny coat",
                        "thick coat",
                        "winter coat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественный покров из меха, шерсти или волос у животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шерсть",
                                "target_lang_sense_clarification": "волосяной покров животного"
                            },
                            {
                                "target_lang_word": "шкура",
                                "target_lang_sense_clarification": "кожа с шерстью"
                            },
                            {
                                "target_lang_word": "мех",
                                "target_lang_sense_clarification": "густая шерсть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "194d6e21-59c5-3e6a-9870-7e0f90019d78",
                    "sense_definition": "A heraldic design or shield used as a symbol of a family, city, or organization; a coat of arms.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family's <w>coat</w> of arms features a golden lion.",
                            "target_lang_translations": {
                                "ru": "Фамильный <w>герб</w> изображает золотого льва."
                            }
                        },
                        {
                            "text": "He studied the ancient <w>coat</w> painted on the shield.",
                            "target_lang_translations": {
                                "ru": "Он изучал старинный <w>герб</w>, нарисованный на щите."
                            }
                        }
                    ],
                    "synonyms": [
                        "crest",
                        "emblem",
                        "shield"
                    ],
                    "common_phrases": [
                        "coat of arms"
                    ],
                    "target_lang_definitions": {
                        "ru": "Геральдический рисунок или щит, используемый как символ семьи, города или организации; герб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герб",
                                "target_lang_sense_clarification": "геральдический символ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e796b602-4210-3c41-8b93-2f93ebb52320",
                    "sense_definition": "A piece of canvas or other material used to cover a part of a ship, such as a mast, to prevent water from entering.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailors secured the <w>coat</w> around the mast before the storm hit.",
                            "target_lang_translations": {
                                "ru": "Матросы закрепили <w>брюканец</w> вокруг мачты перед штормом."
                            }
                        },
                        {
                            "text": "Water leaked into the hold because the <w>coat</w> was damaged.",
                            "target_lang_translations": {
                                "ru": "Вода просочилась в трюм, потому что <w>брюканец</w> был поврежден."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок парусины или другого материала, используемый для покрытия части судна, например мачты, для предотвращения попадания воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брюканец",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "чехол",
                                "target_lang_sense_clarification": "защитное покрытие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eac45dab-ef77-35fb-817d-e76c343a95f4",
                    "sense_definition": "A petticoat or skirt worn by women or children in the past.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The painting depicted a young child in <w>coats</w> playing in the garden.",
                            "target_lang_translations": {
                                "ru": "На картине был изображен маленький ребенок в <w>платьице</w>, играющий в саду."
                            }
                        },
                        {
                            "text": "She lifted her heavy <w>coats</w> to step over the puddle.",
                            "target_lang_translations": {
                                "ru": "Она приподняла свои тяжелые <w>юбки</w>, чтобы перешагнуть через лужу."
                            }
                        }
                    ],
                    "synonyms": [
                        "petticoat"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя юбка или платье, которое носили женщины или дети в прошлом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юбка",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "платье",
                                "target_lang_sense_clarification": "детское или женское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "517b56d9-e2f2-3408-b728-882c866b749e",
                    "sense_definition": "The distinctive clothing or habit worn by members of a specific order or profession.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Men of his <w>coat</w> were expected to behave with great dignity.",
                            "target_lang_translations": {
                                "ru": "От людей его <w>звания</w> ожидали поведения с большим достоинством."
                            }
                        },
                        {
                            "text": "He recognized the stranger by his <w>coat</w> as a member of the clergy.",
                            "target_lang_translations": {
                                "ru": "Он узнал незнакомца по его <w>одеянию</w> как члена духовенства."
                            }
                        }
                    ],
                    "synonyms": [
                        "habit",
                        "vesture"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличительная одежда или облачение, которое носят члены определенного ордена или профессии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеяние",
                                "target_lang_sense_clarification": "профессиональное или религиозное"
                            },
                            {
                                "target_lang_word": "мундир",
                                "target_lang_sense_clarification": "форменная одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5cf416b-c658-3db2-89c8-d97bcc452aa7",
                    "sense_definition": "A playing card that features a picture of a person, such as a King, Queen, or Jack.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this old game, the <w>coats</w> are worth ten points each.",
                            "target_lang_translations": {
                                "ru": "В этой старой игре <w>фигуры</w> стоят по десять очков каждая."
                            }
                        },
                        {
                            "text": "He held three <w>coats</w> in his hand, ensuring a strong position.",
                            "target_lang_translations": {
                                "ru": "Он держал в руке три <w>фигуры</w>, обеспечивая сильную позицию."
                            }
                        }
                    ],
                    "synonyms": [
                        "face card",
                        "court card"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Игральная карта с изображением человека, например короля, дамы или валета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фигура",
                                "target_lang_sense_clarification": "карточный термин"
                            },
                            {
                                "target_lang_word": "картинка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d736614c-4a1e-34fb-aa5d-4942bc0510d3",
                    "sense_definition": "To cover an object with a thin layer of a substance.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>coat</w> the chicken in flour before you fry it.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>обвалять</w> курицу в муке перед жаркой."
                            }
                        },
                        {
                            "text": "Snow began to <w>coat</w> the trees and streets late last night.",
                            "target_lang_translations": {
                                "ru": "Снег начал <w>покрывать</w> деревья и улицы поздно ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "cover",
                        "layer",
                        "glaze"
                    ],
                    "antonyms": [
                        "strip"
                    ],
                    "common_phrases": [
                        "coat with",
                        "coat in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать предмет тонким слоем вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "наносить слой"
                            },
                            {
                                "target_lang_word": "обваливать",
                                "target_lang_sense_clarification": "в кулинарии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "086a56b2-beb3-36aa-b332-8a8478666c3c",
                    "sense_definition": "To provide with clothes or dress someone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clothe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy merchant promised to <w>coat</w> the beggar in warm garments.",
                            "target_lang_translations": {
                                "ru": "Богатый купец пообещал <w>одеть</w> нищего в теплую одежду."
                            }
                        },
                        {
                            "text": "She was <w>coated</w> in the finest silks for the ceremony.",
                            "target_lang_translations": {
                                "ru": "Она была <w>облачена</w> в лучшие шелка для церемонии."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothe",
                        "dress"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать одеждой или одевать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одевать",
                                "target_lang_sense_clarification": "снабжать одеждой"
                            },
                            {
                                "target_lang_word": "облачать",
                                "target_lang_sense_clarification": "торжественно или официально"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "28acaf31-2619-3bf9-8732-956bae1c7af5",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Coat</w> arrived at the meeting early.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Коут</w> прибыл на встречу пораньше."
                            }
                        },
                        {
                            "text": "We looked up the <w>Coat</w> family in the local registry.",
                            "target_lang_translations": {
                                "ru": "Мы искали семью <w>Коут</w> в местном реестре."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Коут",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

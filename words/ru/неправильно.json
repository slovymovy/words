                {
                    "sense_id": "e1ee046d-36c0-3115-815d-632c7a14169c",
                    "sense_definition": "Ошибочно, неверно; не так, как следует или как принято.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "incorrectly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вы <неправильно> поняли мой вопрос, поэтому ответили невпопад.",
                            "target_lang_translations": {
                                "en": "You understood my question <w>wrong</w>, so you answered inappropriately."
                            }
                        },
                        {
                            "text": "Ученик <неправильно> написал сложное слово в диктанте.",
                            "target_lang_translations": {
                                "en": "The student wrote the difficult word <w>incorrectly</w> in the dictation."
                            }
                        }
                    ],
                    "synonyms": [
                        "неверно",
                        "ошибочно",
                        "ложно",
                        "некорректно"
                    ],
                    "antonyms": [
                        "правильно"
                    ],
                    "common_phrases": [
                        "понять неправильно",
                        "поступить неправильно",
                        "сделать неправильно",
                        "написать неправильно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a mistaken or incorrect way; not in accordance with rules, facts, or standards."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "incorrectly",
                                "target_lang_sense_clarification": "general term for mistakes or errors"
                            },
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "common informal adverb"
                            },
                            {
                                "target_lang_word": "wrongly",
                                "target_lang_sense_clarification": "often used before a verb or in formal contexts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a87b49a0-2d28-3598-8651-52111fed0257",
                    "sense_definition": "Неравномерно, несимметрично; с нарушением ритма или строя.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "irregularly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сердце больного билось <неправильно>, то замирая, то ускоряясь.",
                            "target_lang_translations": {
                                "en": "The patient's heart was beating <w>irregularly</w>, now stopping, now speeding up."
                            }
                        },
                        {
                            "text": "Деревья в старом саду были посажены <неправильно> и хаотично.",
                            "target_lang_translations": {
                                "en": "The trees in the old garden were planted <w>irregularly</w> and chaotically."
                            }
                        }
                    ],
                    "common_phrases": [
                        "срастаться неправильно",
                        "биться неправильно"
                    ],
                    "target_lang_definitions": {
                        "en": "In an uneven or asymmetrical manner; lacking a regular rhythm, pattern, or order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "irregularly",
                                "target_lang_sense_clarification": "lacking a regular rhythm or pattern"
                            },
                            {
                                "target_lang_word": "unevenly",
                                "target_lang_sense_clarification": "lacking symmetry or balance"
                            }
                        ]
                    }
                }
            ]
        },

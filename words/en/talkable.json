                },
                {
                    "sense_id": "71dc3f17-7416-38cc-89a9-f66a89336163",
                    "sense_definition": "Capable of being talked about or discussed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "discussable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Politics is not always a <w>talkable</w> subject at a polite dinner table.",
                            "target_lang_translations": {

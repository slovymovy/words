                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To deliver a strong blow or hit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "land",
                                "target_lang_sense_clarification": "a punch or blow"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "a slap"
                            }
                        ]
                    }
                }
            ]

            "senses": [
                {
                    "sense_id": "16d23bcb-72f1-364a-ac25-ff19c5045e84",
                    "sense_definition": "Resembling marble stone in appearance, hardness, or coldness.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The statue has a smooth, <w>marblelike</w> surface that shines in the sun.",
                            "target_lang_translations": {
                                "ru": "У статуи гладкая, <w>мраморовидная</w> поверхность, которая блестит на солнце."
                            }
                        },
                        {
                            "text": "Her pale, <w>marblelike</w> skin seemed cold and unmoving.",
                            "target_lang_translations": {
                                "ru": "Её бледная, <w>мраморная</w> кожа казалась холодной и неподвижной."
                            }
                        }
                    ],
                    "synonyms": [
                        "marmoreal"
                    ],
                    "common_phrases": [
                        "marblelike surface",
                        "marblelike skin",
                        "marblelike texture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий мрамор по внешнему виду, твердости или холоду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мраморовидный",
                                "target_lang_sense_clarification": "имеющий вид или структуру мрамора"
                            },
                            {
                                "target_lang_word": "мраморный",
                                "target_lang_sense_clarification": "перен. белый, гладкий и холодный, как мрамор"
                            },
                            {
                                "target_lang_word": "похожий на мрамор",
                                "target_lang_sense_clarification": "общее описание сходства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94ce0876-d310-3b13-9a99-ea85385410ee",
                    "sense_definition": "Resembling a small spherical toy marble in shape or size.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The insect had large, <w>marblelike</w> eyes that reflected the light.",
                            "target_lang_translations": {

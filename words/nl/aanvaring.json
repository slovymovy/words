                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rivier is geblokkeerd als gevolg van een <w>aanvaring</w> tussen twee boten.",
                            "target_lang_translations": {
                                "ru": "Река заблокирована в результате <w>столкновения</w> двух лодок."
                            }

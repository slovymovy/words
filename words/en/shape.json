    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7491c1c0-cabd-36ae-b3d7-53ba35eb712d",
                    "sense_definition": "The external form, outline, or appearance of someone or something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cookies were cut into the <w>shape</w> of stars.",
                            "target_lang_translations": {
                                "ru": "Печенье вырезали в <w>форме</w> звёзд."
                            }
                        },
                        {
                            "text": "Can you draw a square or another simple <w>shape</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь нарисовать квадрат или другую простую <w>фигуру</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "form",
                        "form",
                        "mold"
                    ],
                    "common_phrases": [
                        "in the shape of",
                        "all shapes and sizes",
                        "take shape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешняя форма, очертание или вид кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "внешний вид, конфигурация"
                            },
                            {
                                "target_lang_word": "очерта́ние",
                                "target_lang_sense_clarification": "контур предмета"
                            },
                            {
                                "target_lang_word": "фигу́ра",
                                "target_lang_sense_clarification": "геометрическая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2008ed4d-dcc2-3a2e-9225-c110b5242d0c",
                    "sense_definition": "The physical condition of a person's body, specifically regarding health and fitness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He exercises every morning to stay in good <w>shape</w>.",
                            "target_lang_translations": {
                                "ru": "Он тренируется каждое утро, чтобы оставаться в хорошей <w>форме</w>."
                            }
                        },
                        {
                            "text": "I need to get back in <w>shape</w> before the summer holidays.",
                            "target_lang_translations": {
                                "ru": "Мне нужно прийти в <w>форму</w> перед летними каникулами."
                            }
                        }
                    ],
                    "synonyms": [
                        "form",
                        "form",
                        "mold"
                    ],
                    "common_phrases": [
                        "get in shape",
                        "stay in shape",
                        "out of shape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое состояние тела человека, особенно в отношении здоровья и тренированности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "физическая кондиция, тонус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3ae8bc5-6955-3389-818c-471dce1e50e4",
                    "sense_definition": "The particular condition or state of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old car is still in excellent <w>shape</w>.",
                            "target_lang_translations": {
                                "ru": "Старая машина всё ещё в отличном <w>состоянии</w>."
                            }
                        },
                        {
                            "text": "The economy is in bad <w>shape</w> right now.",
                            "target_lang_translations": {
                                "ru": "Экономика сейчас в плохом <w>состоянии</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "form",
                        "form",
                        "mold"
                    ],
                    "common_phrases": [
                        "in good shape",
                        "in bad shape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное состояние или положение чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состоя́ние",
                                "target_lang_sense_clarification": "положение дел, исправность"
                            },
                            {
                                "target_lang_word": "положе́ние",
                                "target_lang_sense_clarification": "ситуация, обстоятельства"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4ca907b4-599e-3da0-800a-e8faece2f4f8",
                    "sense_definition": "To give a particular form or outline to something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "create_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Use your hands to <w>shape</w> the clay into a bowl.",
                            "target_lang_translations": {
                                "ru": "Используйте руки, чтобы <w>вылепить</w> из глины чашу."
                            }
                        },
                        {
                            "text": "The artist will <w>shape</w> the stone into a beautiful statue.",
                            "target_lang_translations": {
                                "ru": "Художник <w>придаст</w> камню <w>форму</w> прекрасной статуи."
                            }
                        }
                    ],
                    "synonyms": [
                        "form",
                        "mold"
                    ],
                    "common_phrases": [
                        "shape into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать чему-либо определенную форму или очертание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придава́ть фо́рму",
                                "target_lang_sense_clarification": "делать определенной формы"
                            },
                            {
                                "target_lang_word": "формирова́ть",
                                "target_lang_sense_clarification": "создавать структуру"
                            },
                            {
                                "target_lang_word": "лепи́ть",
                                "target_lang_sense_clarification": "создавать из мягкого материала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ea28a13-5e68-3d09-b9e3-1b5e64e5f756",
                    "sense_definition": "To have a significant influence on the development or character of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His childhood experiences helped to <w>shape</w> his personality.",
                            "target_lang_translations": {
                                "ru": "Его детские переживания помогли <w>сформировать</w> его личность."
                            }
                        },
                        {
                            "text": "The media can <w>shape</w> public opinion on important issues.",
                            "target_lang_translations": {
                                "ru": "СМИ могут <w>формировать</w> общественное мнение по важным вопросам."
                            }
                        }
                    ],
                    "synonyms": [
                        "form",
                        "mold"
                    ],
                    "common_phrases": [
                        "shape the future",
                        "shape public opinion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать значительное влияние на развитие или характер чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формирова́ть",
                                "target_lang_sense_clarification": "влиять на развитие, создавать"
                            },
                            {
                                "target_lang_word": "определя́ть",
                                "target_lang_sense_clarification": "обуславливать характер"
                            }
                        ]
                    }
                }
            ]
        }

                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень хороший; отличный, превосходный (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отличный",
                                "target_lang_sense_clarification": "очень хороший"
                            },
                            {
                                "target_lang_word": "первоклассный",
                                "target_lang_sense_clarification": "высшего качества"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [

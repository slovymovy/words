    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3f3ccaff-5fe4-3b66-9e15-9b5d9160fcda",
                    "sense_definition": "Aan de hand; niet in orde; mis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vraag me af wat er met hem <w>loos</w> is vandaag.",
                            "target_lang_translations": {
                                "ru": "Интересно, что с ним сегодня <w>не так</w>.",
                                "en": "I wonder what is <w>wrong</w> with him today."
                            }
                        },
                        {
                            "text": "Er is iets <w>loos</w> in de motor van de auto.",
                            "target_lang_translations": {
                                "ru": "С двигателем машины что-то <w>не так</w>.",
                                "en": "There is something <w>wrong</w> with the car's engine."
                            }
                        }
                    ],
                    "synonyms": [
                        "mis",
                        "aan de hand"
                    ],
                    "common_phrases": [
                        "er is iets loos",
                        "wat is er loos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то не в порядке; что-то случилось; не так.",
                        "en": "Something is not right; something is happening; wrong."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не так",
                                "target_lang_sense_clarification": "используется в вопросах о состоянии (что-то не так)"
                            },
                            {
                                "target_lang_word": "случилось",
                                "target_lang_sense_clarification": "произошло что-то негативное"
                            },
                            {
                                "target_lang_word": "стряслось",
                                "target_lang_sense_clarification": "разговорный вариант 'случилось'"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "not in order, incorrect state"
                            },
                            {
                                "target_lang_word": "the matter",
                                "target_lang_sense_clarification": "used in questions about a problem"
                            },
                            {
                                "target_lang_word": "going on",
                                "target_lang_sense_clarification": "happening (usually negative)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fba3b1f-26e7-38ba-bd9e-384ba48a0ee2",
                    "sense_definition": "Zonder inhoud of betekenis; leeg.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "empty_false",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij deed een <w>loze</w> belofte die hij nooit van plan was na te komen.",
                            "target_lang_translations": {
                                "ru": "Он дал <w>пустое</w> обещание, которое никогда не собирался выполнять.",
                                "en": "He made an <w>empty</w> promise that he never intended to keep."
                            }
                        },
                        {
                            "text": "Tussen de twee muren zit een <w>loze</w> ruimte voor de leidingen.",
                            "target_lang_translations": {
                                "ru": "Между двумя стенами есть <w>пустое</w> пространство для труб.",
                                "en": "Between the two walls, there is a <w>void</w> space for the pipes."
                            }
                        }
                    ],
                    "synonyms": [
                        "leeg",
                        "inhoudsloos",
                        "hol"
                    ],
                    "antonyms": [
                        "gevuld",
                        "betekenisvol"
                    ],
                    "common_phrases": [
                        "loze belofte",
                        "loze ruimte",
                        "loze kreet"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7dc23a82-0851-33c6-92a8-24cc3e9b10d7",
                    "sense_definition": "Посредник, третейский судья, избранный для разрешения спора между сторонами.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компании пригласили независимого <w>арбитра</w> для урегулирования конфликта.",
                            "target_lang_translations": {
                                "en": "The companies invited an independent <w>arbitrator</w> to settle the conflict."
                            }
                        },
                        {
                            "text": "В этом сложном споре только время выступит лучшим <w>арбитром</w>.",
                            "target_lang_translations": {
                                "en": "In this complex dispute, only time will act as the best <w>arbiter</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "посредник",
                        "третейский судья"
                    ],
                    "common_phrases": [
                        "независимый арбитр",
                        "выступить арбитром"
                    ],
                    "target_lang_definitions": {
                        "en": "A mediator or third-party judge chosen to resolve a dispute between parties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arbitrator",
                                "target_lang_sense_clarification": "legal or formal dispute resolution"
                            },
                            {
                                "target_lang_word": "arbiter",
                                "target_lang_sense_clarification": "ultimate authority or judge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "760266c3-1009-3cb9-91af-b845109e1d87",
                    "sense_definition": "Судья в спортивных состязаниях, следящий за соблюдением правил игры.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главный <w>арбитр</w> остановил матч из-за сильного дождя.",
                            "target_lang_translations": {
                                "en": "The main <w>referee</w> stopped the match due to heavy rain."
                            }
                        },
                        {
                            "text": "Игроки не стали спорить с решением <w>арбитра</w>.",
                            "target_lang_translations": {
                                "en": "The players did not argue with the <w>referee</w>'s decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "судья",
                        "рефери"
                    ],
                    "common_phrases": [
                        "главный арбитр",
                        "свисток арбитра",
                        "футбольный арбитр"
                    ],
                    "target_lang_definitions": {
                        "en": "A judge in sports competitions who ensures compliance with the rules of the game."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "referee",
                                "target_lang_sense_clarification": "sports official (e.g. football, boxing)"
                            },
                            {
                                "target_lang_word": "umpire",
                                "target_lang_sense_clarification": "sports official (e.g. tennis, baseball)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

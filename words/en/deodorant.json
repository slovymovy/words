                    ],
                    "common_phrases": [
                        "room deodorant",
                        "carpet deodorant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любое вещество, используемое для устранения или маскировки неприятных запахов в помещении или от предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дезодорант",
                                "target_lang_sense_clarification": "средство для устранения запахов"
                            },
                            {
                                "target_lang_word": "поглотитель запаха",
                                "target_lang_sense_clarification": "вещество, впитывающее запахи"
                            }
                        ]
                    }
                }
            ]
        },
        {

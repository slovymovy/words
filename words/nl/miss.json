                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "фифа",
                                "target_lang_sense_clarification": "высокомерная девушка (разг.)"
                            },
                            {
                                "target_lang_word": "цаца",
                                "target_lang_sense_clarification": "важная особа (разг., ирон.)"
                            }
                        ]
                    }
                }

            "senses": [
                {
                    "sense_id": "2114cc99-6fbc-3bf5-b204-772dbeb56b05",
                    "sense_definition": "bardzo zły, nędzny, trudny do zniesienia (często w odniesieniu do życia, losu lub pogody)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez ostatnie lata wiódł naprawdę <pieskie> życie na emigracji.",
                            "target_lang_translations": {
                                "ru": "Последние годы он вёл поистине <собачью> жизнь в эмиграции."
                            }
                        },
                        {
                            "text": "Zostańmy w domu, bo na zewnątrz panuje <pieska> pogoda.",
                            "target_lang_translations": {
                                "ru": "Давай останемся дома, потому что на улице <собачья> погода."
                            }
                        }
                    ],
                    "synonyms": [
                        "nędzny",
                        "kiepski",
                        "marny",
                        "podły",
                        "lichy"
                    ],
                    "common_phrases": [
                        "pieskie życie",
                        "pieska pogoda",
                        "pieski los"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень плохой, жалкий, невыносимый (часто о жизни, судьбе или погоде)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собачий",
                                "target_lang_sense_clarification": "о жизни, холоде, настроении"
                            },
                            {
                                "target_lang_word": "скверный",
                                "target_lang_sense_clarification": "плохой, негодный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "792eb1dd-907d-34c5-bced-f8050bc08020",
                    "sense_definition": "związany z psem, dotyczący psa; rzadziej używane niż słowo „psi”, występuje głównie w utartych zwrotach",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest twój <pieski> obowiązek, aby zadbać o bezpieczeństwo rodziny.",
                            "target_lang_translations": {
                                "ru": "Это твой <святой> долг — позаботиться о безопасности семьи."
                            }
                        },
                        {
                            "text": "Jego wierność była wręcz <pieska>, nigdy nie opuścił przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Его верность была прямо-таки <собачьей>, он никогда не оставлял друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "psi",
                        "sobaczy"
                    ],
                    "common_phrases": [
                        "pieski obowiązek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с собакой, относящийся к собаке; встречается в основном в устойчивых выражениях"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собачий",
                                "target_lang_sense_clarification": "свойственный собаке"
                            },
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "в идиоме 'pieski obowiązek' (святой долг)"

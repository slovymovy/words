    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1fd41d5a-ed8f-3f3c-ae8e-2d62f20b7780",
                    "sense_definition": "To lean, tilt, or incline to one side, often used in reference to a ship or vessel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tilt_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old ship began to <w>hield</w> heavily to the port side during the storm.",
                            "target_lang_translations": {
                                "ru": "Старый корабль начал сильно <w>крениться</w> на левый борт во время шторма."
                            }
                        },
                        {
                            "text": "You need to <w>hield</w> the barrel slightly to get the liquid out.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно слегка <w>наклонить</w> бочку, чтобы вылить жидкость."
                            }
                        }
                    ],
                    "synonyms": [
                        "tilt",
                        "lean",
                        "heel",
                        "incline"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This term is largely obsolete in modern standard English."
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "Historically used in Scottish and Northern English dialects."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклоняться или крениться набок (часто используется по отношению к кораблю или судну)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крениться",
                                "target_lang_sense_clarification": "о судне"
                            },
                            {
                                "target_lang_word": "наклоняться",
                                "target_lang_sense_clarification": "принимать наклонное положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "950ea253-126f-365e-ae7f-231fb0bfd9b4",
                    "sense_definition": "To pour out contents by tilting a container.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tilt_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The servant <w>hielded</w> the wine into the goblet with a steady hand.",
                            "target_lang_translations": {
                                "ru": "Слуга уверенной рукой <w>налил</w> вино в кубок."
                            }
                        },
                        {
                            "text": "She <w>hielded</w> the water from the bucket onto the dry earth.",
                            "target_lang_translations": {
                                "ru": "Она <w>вылила</w> воду из ведра на сухую землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "pour",
                        "decant"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This usage is obsolete."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выливать содержимое, наклоняя сосуд"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выливать",
                                "target_lang_sense_clarification": "жидкость из емкости"
                            },
                            {
                                "target_lang_word": "наливать",
                                "target_lang_sense_clarification": "наполнять, наклоняя сосуд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8e671f9-0227-351f-a2ae-5f41f248b57d",
                    "sense_definition": "To throw, cast, or fling something forcefully.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "throw_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>hielded</w> the stone far across the river.",
                            "target_lang_translations": {
                                "ru": "Он <w>швырнул</w> камень далеко через реку."
                            }
                        },
                        {
                            "text": "The angry crowd <w>hielded</w> debris at the castle walls.",
                            "target_lang_translations": {
                                "ru": "Разъяренная толпа <w>бросала</w> мусор в стены замка."
                            }
                        }
                    ],
                    "synonyms": [
                        "fling",
                        "hurl",
                        "cast",
                        "pitch",
                        "toss",
                        "heave",
                        "launch"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Rare and obsolete usage."
                        }
                    ],
                    "target_lang_definitions": {

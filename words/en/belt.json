                    ],
                    "synonyms": [
                        "seat belt",
                        "safety belt",
                        "restraint",
                        "strap"
                    ],
                    "common_phrases": [
                        "fasten your belt",
                        "seat belt",
                        "safety belt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремень, используемый для фиксации тела в целях безопасности, например, в автомобиле или самолете."

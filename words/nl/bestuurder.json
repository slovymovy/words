                {
                    "sense_id": "dfa270b6-7a8b-3b34-84b4-fac43dfe5aac",
                    "sense_definition": "Een persoon die een voertuig bestuurt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_operator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bestuurder</w> verloor de macht over het stuur en raakte van de weg.",
                            "target_lang_translations": {
                                "ru": "<w>Водитель</w> потерял управление и съехал с дороги."
                            }
                        },
                        {
                            "text": "De politie hield de <w>bestuurder</w> van de auto aan voor een controle.",
                            "target_lang_translations": {
                                "ru": "Полиция остановила <w>водителя</w> автомобиля для проверки."
                            }
                        }
                    ],
                    "synonyms": [
                        "chauffeur",
                        "coureur"
                    ],
                    "common_phrases": [
                        "dronken bestuurder",
                        "bestuurder van de auto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который управляет транспортным средством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водитель",
                                "target_lang_sense_clarification": "транспортного средства"
                            },
                            {
                                "target_lang_word": "шофёр",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22b60015-fccb-3511-a3e9-763ebc9ad26c",
                    "sense_definition": "Een persoon die leiding geeft aan een bedrijf of organisatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "organization_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bestuurder</w> is verantwoordelijk voor het financiële beleid van de stichting.",
                            "target_lang_translations": {
                                "ru": "<w>Руководитель</w> несет ответственность за финансовую политику фонда."
                            }
                        },
                        {
                            "text": "Als nieuwe <w>bestuurder</w> wil zij de organisatie moderner maken.",
                            "target_lang_translations": {
                                "ru": "Как новый <w>управляющий</w>, она хочет модернизировать организацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "directeur",
                        "manager",
                        "leider",
                        "topman",
                        "topvrouw"
                    ],
                    "common_phrases": [
                        "dagelijks bestuurder",
                        "bestuurder van een stichting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который руководит компанией или организацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "глава организации"
                            },
                            {
                                "target_lang_word": "управляющий",
                                "target_lang_sense_clarification": "администратор"
                            },
                            {
                                "target_lang_word": "член правления",
                                "target_lang_sense_clarification": "входит в состав руководства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb723038-a1e4-3014-84c5-3ff3f1262141",
                    "sense_definition": "Een persoon die een grote machine bedient.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "machine_operator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft de <w>bestuurder</w> van de hijskraan verhoord na het ongeluk.",
                            "target_lang_translations": {
                                "ru": "Полиция допросила <w>машиниста</w> подъемного крана после аварии."
                            }
                        },
                        {
                            "text": "Elke <w>bestuurder</w> van deze zware machine moet een speciaal certificaat hebben.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>оператор</w> этой тяжелой машины должен иметь специальный сертификат."
                            }
                        }
                    ],
                    "common_phrases": [

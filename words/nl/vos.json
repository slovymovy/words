    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "58d6498b-1bfb-347f-9676-c2e57523320c",
                    "sense_definition": "Een hondachtig roofdier met een roodbruine vacht, puntige oren en een dikke staart.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de duinen kun je soms een wilde <w>vos</w> zien lopen.",
                            "target_lang_translations": {
                                "en": "In the dunes you can sometimes see a wild <w>fox</w> walking.",
                                "ru": "В дюнах иногда можно увидеть бегущую дикую <w>лису</w>."
                            }
                        },
                        {
                            "text": "De boer beschermt zijn kippen tegen de hongerige <w>vos</w>.",
                            "target_lang_translations": {
                                "en": "The farmer protects his chickens against the hungry <w>fox</w>.",
                                "ru": "Фермер защищает своих кур от голодной <w>лисы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rode vos",
                        "gewone vos"
                    ],
                    "common_phrases": [
                        "een sluwe vos",
                        "zo slim als een vos"
                    ],
                    "target_lang_definitions": {
                        "en": "A dog-like predator with a reddish-brown coat, pointed ears, and a thick tail.",
                        "ru": "Собаковидный хищник с рыжевато-коричневой шерстью, острыми ушами и пушистым хвостом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fox",
                                "target_lang_sense_clarification": "animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лиса",
                                "target_lang_sense_clarification": "животное"
                            },
                            {
                                "target_lang_word": "лисица",
                                "target_lang_sense_clarification": "животное (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3d43af4-b0e8-31a6-880c-0f78ead7e51b",
                    "sense_definition": "Iemand die erg slim, listig en doortrapt is.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een sluwe <w>vos</w> die precies weet hoe hij zijn zin moet krijgen.",
                            "target_lang_translations": {
                                "en": "He is a sly <w>fox</w> who knows exactly how to get his way.",
                                "ru": "Он хитрый <w>лис</w>, который точно знает, как добиться своего."
                            }
                        },
                        {
                            "text": "Laat je niet misleiden door die oude <w>vos</w>.",
                            "target_lang_translations": {
                                "en": "Don't be misled by that old <w>fox</w>.",
                                "ru": "Не дай этому старому <w>лису</w> ввести тебя в заблуждение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een oude vos",
                        "een lepe vos"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used metaphorically"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is very smart, cunning, and crafty.",
                        "ru": "Человек, который очень умен, хитер и коварен."
                    },
                    "translations": {

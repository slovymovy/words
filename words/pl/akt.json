                },
                {
                    "sense_id": "719fa4ba-6b15-3848-9664-ab85024a2bfa",
                    "sense_definition": "Dzieło sztuki (obraz, rzeźba, fotografia) przedstawiające nagie ciało ludzkie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "art_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W galerii sztuki podziwialiśmy piękny <w>akt</w> namalowany farbami olejnymi.",
                            "target_lang_translations": {
                                "ru": "В художественной галерее мы любовались прекрасным <w>ню</w>, написанным масляными красками."
                            }
                        },
                        {
                            "text": "Studenci akademii sztuk pięknych uczą się rysować <w>akt</w> z natury.",
                            "target_lang_translations": {
                                "ru": "Студенты академии изящных искусств учатся рисовать <w>обнажённую натуру</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "akt kobiecy",
                        "akt męski",
                        "malować akt",
                        "studium aktu"
                    ],
                    "target_lang_definitions": {

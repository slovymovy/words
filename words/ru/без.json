    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "4b4e1e00-a455-3479-b595-75a8d95b8737",
                    "sense_definition": "Указывает на отсутствие кого-либо или чего-либо, неимение чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обычно пью чёрный кофе <без> сахара.",
                            "target_lang_translations": {
                                "en": "I usually drink black coffee <w>without</w> sugar."
                            }
                        },
                        {
                            "text": "Мы не сможем закончить этот проект <без> твоей помощи.",
                            "target_lang_translations": {
                                "en": "We won't be able to finish this project <w>without</w> your help."
                            }
                        },
                        {
                            "text": "Он ответил на сложный вопрос <без> сомнения.",
                            "target_lang_translations": {
                                "en": "He answered the difficult question <w>without</w> doubt."
                            }
                        }
                    ],
                    "synonyms": [
                        "в отсутствие",
                        "при отсутствии"
                    ],
                    "antonyms": [
                        "с",
                        "при"
                    ],
                    "common_phrases": [
                        "без сомнения",
                        "без проблем",
                        "без ума",
                        "без понятия",
                        "без конца"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the absence of someone or something, or the lack of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "without",
                                "target_lang_sense_clarification": "indicating absence or lack"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9ae16e8-036c-3037-952a-bcc1c89868fe",
                    "sense_definition": "Указывает на количество, которого не хватает до целого (обычно при обозначении времени).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сейчас уже <без> пятнадцати минут пять.",
                            "target_lang_translations": {
                                "en": "It is already fifteen minutes <w>to</w> five."
                            }
                        },
                        {
                            "text": "Моему дедушке исполнилось восемьдесят лет <без> малого.",
                            "target_lang_translations": {
                                "en": "My grandfather turned eighty <w>minus</w> a little."
                            }
                        }
                    ],
                    "synonyms": [
                        "за вычетом"
                    ],
                    "common_phrases": [
                        "без четверти",
                        "без пяти",
                        "без малого"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an amount missing from a whole, commonly used when telling time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "used when telling time (e.g., ten to five)"
                            },
                            {
                                "target_lang_word": "minus",
                                "target_lang_sense_clarification": "indicating subtraction or a missing quantity"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
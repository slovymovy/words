            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef15f289-2f50-3ffb-9bd5-e7754331d9fc",
                    "sense_definition": "Перемещаться в определённом направлении с помощью транспортного средства (автомобиля, автобуса, поезда, велосипеда и т. п.).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро я <ехать>еду</ехать> на работу на автобусе.",
                            "target_lang_translations": {
                                "en": "Every morning I <w>go</w> to work by bus."
                            }
                        },
                        {
                            "text": "Мы решили <ехать>ехать</ехать> домой на такси, так как было уже поздно.",
                            "target_lang_translations": {
                                "en": "We decided to <w>go</w> home by taxi since it was already late."
                            }
                        }
                    ],
                    "synonyms": [
                        "двигаться",
                        "направляться"
                    ],
                    "antonyms": [
                        "идти",
                        "стоять"
                    ],
                    "common_phrases": [
                        "ехать на машине",
                        "ехать на автобусе",
                        "ехать домой",
                        "ехать на работу",
                        "ехать быстро",
                        "ехать медленно"
                    ],
                    "target_lang_definitions": {
                        "en": "To move in a specific direction using a vehicle (car, bus, train, bicycle, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "general movement by vehicle"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "operating a vehicle"
                            },
                            {
                                "target_lang_word": "ride",
                                "target_lang_sense_clarification": "as a passenger or on a bike"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "066bf60f-a0ff-32f0-955f-9dbbe5cfe9fe",
                    "sense_definition": "Отправляться в путь, совершать поездку в другой город, страну или отдалённое место.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом наша семья планирует <ехать>ехать</ехать> в отпуск на море.",
                            "target_lang_translations": {
                                "en": "In the summer, our family plans to <w>go</w> on vacation to the sea."
                            }
                        },
                        {
                            "text": "Студентам нужно <ехать>ехать</ехать> в Москву на конференцию.",
                            "target_lang_translations": {
                                "en": "The students need to <w>go</w> to Moscow for a conference."
                            }
                        }
                    ],
                    "synonyms": [
                        "путешествовать",
                        "отправляться"
                    ],
                    "antonyms": [
                        "оставаться"
                    ],
                    "common_phrases": [
                        "ехать в отпуск",
                        "ехать за границу",
                        "ехать в гости",
                        "ехать в командировку"
                    ],
                    "target_lang_definitions": {
                        "en": "To set off on a journey, to travel to another city, country, or distant place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "to a destination"
                            },
                            {
                                "target_lang_word": "travel",
                                "target_lang_sense_clarification": "long distance journey"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c28c789-2dc9-3c38-90ea-dca7f7e38636",
                    "sense_definition": "Скользить, плавно перемещаться по поверхности (о лыжах, санях, коньках) или сдвигаться с места.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sliding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такую тёплую погоду лыжи совсем не <ехать>едут</ехать> по снегу.",
                            "target_lang_translations": {
                                "en": "In such warm weather, the skis don't <w>slide</w> on the snow at all."
                            }
                        },
                        {
                            "text": "От постоянного стресса у него, кажется, крыша <ехать>едет</ехать>.",
                            "target_lang_translations": {
                                "en": "From constant stress, it seems he is <w>going</w> crazy."
                            }
                        }
                    ],
                    "synonyms": [
                        "скользить"
                    ],
                    "common_phrases": [
                        "крыша едет",
                        "лыжи не едут"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в переносном значении (о психике)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To slide or move smoothly over a surface (of skis, sleds, skates) or to shift from a place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "movement on a surface"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "idiomatic (e.g., go crazy)"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9b05f555-acd4-313f-ab25-b200958db252",
                    "sense_definition": "A test done in order to learn something or to discover if something works or is true.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scientific_test",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientists conducted an <w>experiment</w> to test the effectiveness of the new drug.",
                            "target_lang_translations": {
                                "ru": "Ученые провели <w>эксперимент</w>, чтобы проверить эффективность нового препарата."
                            }
                        },
                        {
                            "text": "We performed a simple scientific <w>experiment</w> in chemistry class today.",
                            "target_lang_translations": {
                                "ru": "Сегодня на уроке химии мы провели простой научный <w>эксперимент</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "test",
                        "trial",
                        "investigation"
                    ],
                    "common_phrases": [
                        "conduct an experiment",
                        "scientific experiment",
                        "carry out an experiment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Научная процедура, выполняемая с целью изучения чего-либо, проверки гипотезы или демонстрации известного факта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эксперимент",
                                "target_lang_sense_clarification": "научно поставленный опыт"
                            },
                            {
                                "target_lang_word": "опыт",
                                "target_lang_sense_clarification": "научное испытание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b9fb420-a5bb-39d0-85ad-bab98986eb83",
                    "sense_definition": "Practical experience or familiarity with something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "experience_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Through long <w>experiment</w>, the captain knew every current in the bay.",
                            "target_lang_translations": {
                                "ru": "Благодаря многолетнему <w>опыту</w> капитан знал каждое течение в бухте."
                            }
                        },
                        {
                            "text": "He spoke with the wisdom gained from years of <w>experiment</w> in the field.",
                            "target_lang_translations": {
                                "ru": "Он говорил с мудростью, обретенной за годы <w>опыта</w> в этой области."
                            }
                        }
                    ],
                    "synonyms": [
                        "experience"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "usage is obsolete"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Практический опыт или знакомство с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опыт",
                                "target_lang_sense_clarification": "совокупность знаний и навыков"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b93b344-bc89-31e8-b4fb-42d675b09a93",
                    "sense_definition": "To perform a scientific test or to try new methods or ideas to see what happens.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scientific_test",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chefs often <w>experiment</w> with different spices to create unique flavors.",
                            "target_lang_translations": {
                                "ru": "Повара часто <w>экспериментируют</w> с различными специями, чтобы создать уникальные вкусы."
                            }
                        },
                        {
                            "text": "Researchers continue to <w>experiment</w> on the virus to find a cure.",
                            "target_lang_translations": {
                                "ru": "Исследователи продолжают <w>экспериментировать</w> над вирусом, чтобы найти лекарство."
                            }
                        }
                    ],
                    "synonyms": [
                        "test",
                        "trial"
                    ],
                    "common_phrases": [
                        "experiment with",
                        "experiment on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить научные опыты или пробовать новые методы и идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспериментировать",
                                "target_lang_sense_clarification": "проводить эксперименты"
                            },
                            {
                                "target_lang_word": "ставить опыт",
                                "target_lang_sense_clarification": "проводить научное испытание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2db44256-a0e6-3f46-9b50-821074da0b51",
                    "sense_definition": "To feel, perceive, or experience something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "experience_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I did <w>experiment</w> a sudden chill when I entered the old house.",
                            "target_lang_translations": {
                                "ru": "Я действительно <w>ощутил</w> внезапный озноб, когда вошел в старый дом."
                            }
                        },
                        {
                            "text": "They <w>experiment</w> the joy of freedom after years of confinement.",
                            "target_lang_translations": {
                                "ru": "Они <w>испытывают</w> радость свободы после долгих лет заточения."
                            }
                        }
                    ],
                    "synonyms": [
                        "experience",
                        "perceive"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "usage is obsolete"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать, ощущать или испытывать что-либо."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]

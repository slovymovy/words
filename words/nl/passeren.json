            "pos": "verb",
            "senses": [
                {
                    "sense_id": "db15fb21-e58b-377d-b25c-a55b68ff7e63",
                    "sense_definition": "Langs iemand of iets gaan zonder te stoppen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>passeren</w> elke ochtend dit grote gebouw op weg naar school.",
                            "target_lang_translations": {
                                "en": "We <w>pass</w> this large building every morning on our way to school.",
                                "ru": "Мы каждое утро <w>проходим</w> мимо этого большого здания по дороге в школу."
                            }
                        },
                        {
                            "text": "Er <w>passeerden</w> veel wandelaars toen we in het bos aan het rusten waren.",
                            "target_lang_translations": {
                                "en": "Many hikers <w>passed</w> while we were resting in the forest.",
                                "ru": "Мимо <w>проходило</w> много пешеходов, когда мы отдыхали в лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebeuren",
                        "gebeuren"
                    ],
                    "antonyms": [
                        "stoppen",
                        "blijven"
                    ],
                    "common_phrases": [
                        "de grens passeren",
                        "een station passeren",
                        "iemand passeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To go past someone or something without stopping.",
                        "ru": "Проходить или проезжать мимо кого-то или чего-то, не останавливаясь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "go past"
                            },
                            {
                                "target_lang_word": "go past",
                                "target_lang_sense_clarification": "move by"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "пешком"
                            },
                            {
                                "target_lang_word": "проезжать",
                                "target_lang_sense_clarification": "на транспорте"
                            },
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "оставить позади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3082d2fa-9d9f-35c4-80ad-129c6e87bf7b",

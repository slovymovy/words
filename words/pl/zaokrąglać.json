    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24f25c11-a5cd-3293-bcbe-9666dd8d00c0",
                    "sense_definition": "Nadawać przedmiotowi kształt koła, kuli lub łuku; wygładzać ostre krawędzie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_modification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stolarz zaczął powoli <w>zaokrąglać</w> ostre rogi drewnianego stołu.",
                            "target_lang_translations": {
                                "ru": "Столяр начал медленно <w>закруглять</w> острые углы деревянного стола."
                            }
                        },
                        {
                            "text": "Musisz delikatnie <w>zaokrąglać</w> krawędzie papieru nożyczkami, aby się nie skaleczyć.",
                            "target_lang_translations": {
                                "ru": "Ты должен аккуратно <w>закруглять</w> края бумаги ножницами, чтобы не порезаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyokrąglać"
                    ],
                    "antonyms": [
                        "zaostrzać"
                    ],
                    "common_phrases": [
                        "zaokrąglać rogi",
                        "zaokrąglać krawędzie",
                        "zaokrąglać kształty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать предмету форму круга, шара или дуги; сглаживать острые края."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закруглять",
                                "target_lang_sense_clarification": "сглаживать углы или края"
                            },
                            {
                                "target_lang_word": "округлять",
                                "target_lang_sense_clarification": "придавать круглую форму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34bfa9c1-bb35-37b6-a767-d3432bed209e",
                    "sense_definition": "Wyrażać liczbę w postaci przybliżonej, redukując liczbę cyfr po przecinku lub zastępując końcówkę zerami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel matematyki kazał nam <w>zaokrąglać</w> wyniki do dwóch miejsc po przecinku.",
                            "target_lang_translations": {
                                "ru": "Учитель математики велел нам <w>округлять</w> результаты до двух знаков после запятой."
                            }
                        },
                        {
                            "text": "Sprzedawcy często próbują <w>zaokrąglać</w> ceny w górę, aby zarobić więcej.",
                            "target_lang_translations": {
                                "ru": "Продавцы часто пытаются <w>округлять</w> цены в большую сторону, чтобы заработать больше."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyokrąglać"
                    ],
                    "common_phrases": [
                        "zaokrąglać w górę",
                        "zaokrąglać w dół",
                        "zaokrąglać do całości",
                        "zaokrąglać wynik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать число в приближенном виде, уменьшая количество цифр после запятой или заменяя последние цифры нулями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "округлять",
                                "target_lang_sense_clarification": "числа, математика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27a45e0d-1f5f-3216-a724-61083ada3f8f",
                    "sense_definition": "Stawać się pełniejszym, przybierać na wadze lub nabierać bardziej krągłych kształtów (używane z zaimkiem zwrotnym 'się').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "body_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z każdym miesiącem ciąży jej brzuch zaczyna się wyraźnie <w>zaokrąglać</w>.",
                            "target_lang_translations": {
                                "ru": "С каждым месяцем беременности её живот начинает заметно <w>округляться</w>."
                            }
                        },
                        {
                            "text": "Zauważyłem, że po zimie moja twarz lubi się nieco <w>zaokrąglać</w>.",
                            "target_lang_translations": {
                                "ru": "Я заметил, что после зимы моё лицо любит немного <w>округляться</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zaokrąglać się na twarzy",
                        "brzuch się zaokrągla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться полнее, прибавлять в весе или приобретать более округлые формы (о теле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "округляться",
                                "target_lang_sense_clarification": "полнеть, становиться круглым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1814ac05-f573-32c3-9df3-16066514b622",
                    "sense_definition": "Układać wargi w kształt zbliżony do małego koła, zazwyczaj podczas wymawiania niektórych głosek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas ćwiczeń logopedycznych trzeba mocno <w>zaokrąglać</w> usta.",
                            "target_lang_translations": {
                                "ru": "Во время логопедических упражнений нужно сильно <w>округлять</w> губы."
                            }
                        },
                        {
                            "text": "Aktor uczył się wyraźnie <w>zaokrąglać</w> wargi, aby poprawić swoją dykcję.",
                            "target_lang_translations": {
                                "ru": "Актёр учился чётко <w>округлять</w> губы, чтобы улучшить свою дикцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zaokrąglać usta",
                        "zaokrąglać wargi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Складывать губы в форму, близкую к маленькому кругу, обычно при произнесении звуков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "округлять",
                                "target_lang_sense_clarification": "губы, фонетика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
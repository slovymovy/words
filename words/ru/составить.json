                    "common_phrases": [
                        "составить основу",
                        "составить часть",
                        "составить конкуренцию",
                        "составить компанию",
                        "составить проблему"
                    ],
                    "target_lang_definitions": {
                        "en": "To be a part of something; to form a whole, collection, or content of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "constitute",

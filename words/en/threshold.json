{
    "word_family": [
        "subthreshold",
        "thresholding"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6710230f-6d77-3340-b54a-cf35eead32b8",
                    "sense_definition": "The point just before a new situation, period, or event begins.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_boundary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are currently standing on the <w>threshold</w> of a new era in space exploration.",
                            "target_lang_translations": {
                                "ru": "В настоящее время мы стоим на <w>пороге</w> новой эры в освоении космоса."
                            }
                        },
                        {
                            "text": "The young scientist felt she was at the <w>threshold</w> of a major discovery.",
                            "target_lang_translations": {
                                "ru": "Молодая ученая чувствовала, что находится на <w>пороге</w> крупного открытия."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorstep",
                        "doorsill"
                    ],
                    "common_phrases": [
                        "on the threshold of",
                        "at the threshold"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент, непосредственно предшествующий началу новой ситуации, периода или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порог",
                                "target_lang_sense_clarification": "начало, канун события"
                            },
                            {
                                "target_lang_word": "преддверие",
                                "target_lang_sense_clarification": "время перед событием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31494c62-1c36-321f-b54f-9d31e76e88af",
                    "sense_definition": "The level or point at which something starts to happen, change, or become valid.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "quantitative_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The noise in the factory exceeded the safety <w>threshold</w> for workers.",
                            "target_lang_translations": {
                                "ru": "Уровень шума на заводе превысил <w>порог</w> безопасности для рабочих."
                            }
                        },
                        {
                            "text": "She has a very high pain <w>threshold</w> and rarely complains about injuries.",
                            "target_lang_translations": {
                                "ru": "У неё очень высокий болевой <w>порог</w>, и она редко жалуется на травмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pain threshold",
                        "tax threshold",
                        "high threshold",
                        "low threshold"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень или точка, на которой что-то начинает происходить, меняться или вступать в силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порог",
                                "target_lang_sense_clarification": "предельная величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "727127a2-feaf-31b7-bd88-b58bdc4756f1",
                    "sense_definition": "The piece of wood or stone at the bottom of a doorway that one steps over when entering.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_boundary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He paused to wipe his boots before he would cross the <w>threshold</w>.",
                            "target_lang_translations": {
                                "ru": "Он остановился, чтобы вытереть ботинки, прежде чем переступить <w>порог</w>."
                            }
                        },
                        {
                            "text": "The bride was carried over the <w>threshold</w> into their new home.",
                            "target_lang_translations": {
                                "ru": "Невесту перенесли через <w>порог</w> в их новый дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorsill",
                        "doorstep"
                    ],
                    "antonyms": [
                        "lintel"
                    ],
                    "common_phrases": [
                        "cross the threshold"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянная или каменная планка в нижней части дверного проема, через которую переступают при входе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порог",
                                "target_lang_sense_clarification": "часть дверного проема"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
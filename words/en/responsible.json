    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "36fa7bbc-bfee-3d11-89e0-258203239200",
                    "sense_definition": "Having the duty of taking care of something or someone; answerable for an act or its consequences.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "duty_accountability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parents are <w>responsible</w> for their child's behaviour.",
                            "target_lang_translations": {
                                "ru": "Родители <w>ответственны</w> за поведение своего ребенка."
                            }
                        },
                        {
                            "text": "The manager is <w>responsible</w> for the daily operations of the store.",
                            "target_lang_translations": {
                                "ru": "Менеджер <w>ответственный</w> за ежедневную работу магазина."
                            }
                        }
                    ],
                    "synonyms": [
                        "accountable",
                        "answerable"
                    ],
                    "antonyms": [
                        "irresponsible"
                    ],
                    "common_phrases": [
                        "responsible for",
                        "hold responsible",
                        "fully responsible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязанный заботиться о ком-либо или чем-либо; отвечающий за поступок или его последствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "несущий обязательства"
                            },
                            {
                                "target_lang_word": "в ответе",
                                "target_lang_sense_clarification": "предикатив, разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "036dbf8b-94b7-3e87-a4e8-d84b326d0b55",
                    "sense_definition": "Able to be trusted to do the right thing; reliable, sensible, and capable of rational conduct.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He looks like a <w>responsible</w> guy who can handle the job.",
                            "target_lang_translations": {
                                "ru": "Он выглядит как <w>ответственный</w> парень, который справится с работой."
                            }
                        },
                        {
                            "text": "Please be <w>responsible</w> and clean up after yourself.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будь <w>сознательным</w> и убери за собой."
                            }
                        }
                    ],
                    "synonyms": [
                        "reliable",
                        "trustworthy",
                        "sensible"
                    ],
                    "antonyms": [
                        "irresponsible"
                    ],
                    "common_phrases": [
                        "responsible person",
                        "act responsible",
                        "socially responsible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заслуживающий доверия; надежный, благоразумный и способный к рациональному поведению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "надежный, сознательный"
                            },
                            {
                                "target_lang_word": "сознательный",
                                "target_lang_sense_clarification": "понимающий важность своих действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c90b91b4-7e6f-3c66-af7b-1a35572fb733",
                    "sense_definition": "Being the primary cause of a situation or action.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Who is <w>responsible</w> for this mess in the kitchen?",
                            "target_lang_translations": {
                                "ru": "Кто <w>ответственный</w> за этот беспорядок на кухне?"
                            }
                        },
                        {
                            "text": "The heavy storm was <w>responsible</w> for the delay of our flight.",
                            "target_lang_translations": {
                                "ru": "Сильный шторм стал <w>причиной</w> задержки нашего рейса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "responsible for",
                        "partly responsible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся основной причиной ситуации или действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виновный",
                                "target_lang_sense_clarification": "являющийся причиной (обычно негативной)"
                            },
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "несущий вину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2b64d2c-2252-3870-9cbb-cbbca595a77d",
                    "sense_definition": "Involving important duties and independent decision-making.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "job_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a very <w>responsible</w> position in the law firm.",
                            "target_lang_translations": {
                                "ru": "У нее очень <w>ответственная</w> должность в юридической фирме."
                            }
                        },
                        {
                            "text": "It is a <w>responsible</w> job that requires a lot of focus and dedication.",
                            "target_lang_translations": {
                                "ru": "Это <w>ответственная</w> работа, требующая большой сосредоточенности и самоотдачи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "responsible position",
                        "responsible job"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с важными обязанностями и принятием самостоятельных решений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "важный, серьезный (о работе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a825e9fe-6db1-3988-a59f-ca78b8ec62cd",
                    "sense_definition": "Required to report to a superior or authority; answerable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "duty_accountability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prime minister is <w>responsible</w> to the parliament for his actions.",
                            "target_lang_translations": {
                                "ru": "Премьер-министр <w>подотчетен</w> парламенту за свои действия."
                            }
                        },
                        {
                            "text": "Every employee is <w>responsible</w> to their direct supervisor.",
                            "target_lang_translations": {
                                "ru": "Каждый сотрудник <w>подотчетен</w> своему непосредственному руководителю."
                            }
                        }
                    ],
                    "synonyms": [
                        "answerable"
                    ],
                    "common_phrases": [
                        "responsible to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязанный отчитываться перед начальником или органом власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подотчетный",
                                "target_lang_sense_clarification": "обязанный отчитываться"
                            },
                            {
                                "target_lang_word": "ответственный",
                                "target_lang_sense_clarification": "перед кем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {

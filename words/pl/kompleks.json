                        "kompleks aktywny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химическое соединение, состоящее из центрального атома, окруженного другими атомами или группами атомов (лигандами)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплекс",
                                "target_lang_sense_clarification": "химия"
                            },
                            {
                                "target_lang_word": "комплексное соединение",
                                "target_lang_sense_clarification": "химический термин"
                            }
                        ]
                    }
                }
            ]

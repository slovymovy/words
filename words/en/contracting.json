    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cc33229c-2dc3-3988-8bde-2bac89f4b8bb",
                    "sense_definition": "Present participle and gerund of <w>contract</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The metal is <w>contracting</w> as it cools down.",
                            "target_lang_translations": {
                                "ru": "Металл <w>сжимается</w> по мере остывания."
                            }
                        },
                        {
                            "text": "We are currently <w>contracting</w> with a new supplier for raw materials.",
                            "target_lang_translations": {
                                "ru": "В данный момент мы <w>заключаем договор</w> с новым поставщиком сырья."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола contract."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сжиматься",
                                "target_lang_sense_clarification": "уменьшаться в объеме"
                            },
                            {
                                "target_lang_word": "сокращаться",
                                "target_lang_sense_clarification": "об экономике, мышцах"
                            },
                            {
                                "target_lang_word": "заключать контракт",
                                "target_lang_sense_clarification": "подписывать договор"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "235f257c-031a-39da-83ef-58dd02ba30a3",
                    "sense_definition": "Work done on a contract basis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to leave her permanent job and switch to <w>contracting</w>.",
                            "target_lang_translations": {
                                "ru": "Она решила уйти с постоянной работы и перейти на <w>работу по контракту</w>."
                            }
                        },
                        {
                            "text": "Government <w>contracting</w> can be a very lucrative field for small businesses.",
                            "target_lang_translations": {
                                "ru": "Государственные <w>подряды</w> могут быть очень прибыльной сферой для малого бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "freelancing",
                        "outsourcing"
                    ],
                    "antonyms": [
                        "employment"
                    ],
                    "common_phrases": [
                        "independent contracting",
                        "government contracting",
                        "general contracting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работа, выполняемая на контрактной основе, а не в штате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работа по контракту",
                                "target_lang_sense_clarification": "вид занятости"
                            },
                            {
                                "target_lang_word": "подряд",
                                "target_lang_sense_clarification": "вид деятельности, бизнес"
                            },
                            {
                                "target_lang_word": "заключение договоров",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d3532e8-087e-3704-9410-60fb38566e71",
                    "sense_definition": "The act of something that contracts or shrinks; contraction.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rhythmic <w>contracting</w> of the heart muscle pumps blood through the body.",
                            "target_lang_translations": {
                                "ru": "Ритмичное <w>сокращение</w> сердечной мышцы качает кровь по всему телу."
                            }
                        },
                        {
                            "text": "Sudden <w>contracting</w> of the cooling metal caused it to crack.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>сжатие</w> остывающего металла привело к тому, что он треснул."
                            }
                        }
                    ],
                    "synonyms": [
                        "shrinking",
                        "contraction"
                    ],
                    "antonyms": [
                        "expansion",
                        "stretching"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс уменьшения в объеме или размере; сжатие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сжатие",
                                "target_lang_sense_clarification": "физическое уменьшение объема"
                            },
                            {
                                "target_lang_word": "сокращение",
                                "target_lang_sense_clarification": "мышц или абстрактных понятий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6a59df66-690e-39ce-8d87-98608bab86bb",
                    "sense_definition": "Getting smaller; drawing itself together into a smaller area.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investors are worried about the <w>contracting</w> economy and rising unemployment.",
                            "target_lang_translations": {
                                "ru": "Инвесторы обеспокоены <w>сокращающейся</w> экономикой и растущей безработицей."
                            }
                        },
                        {
                            "text": "The <w>contracting</w> cloud of gas eventually collapsed to form a star.",
                            "target_lang_translations": {
                                "ru": "<w>Сжимающееся</w> газовое облако в конечном итоге схлопнулось, образовав звезду."
                            }
                        }
                    ],
                    "synonyms": [
                        "shrinking",
                        "narrowing"
                    ],
                    "antonyms": [
                        "expanding",
                        "growing"
                    ],
                    "common_phrases": [
                        "contracting market",
                        "contracting economy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшающийся в размере; стягивающийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сокращающийся",
                                "target_lang_sense_clarification": "об экономике, количестве"
                            },
                            {
                                "target_lang_word": "сжимающийся",
                                "target_lang_sense_clarification": "физически"
                            }
                        ]
                    }
                }
            ]
        }
    ]

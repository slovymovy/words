                {
                    "sense_id": "f3372291-2267-32ba-8737-bfa1db1e4195",
                    "sense_definition": "Een verkorte vorm van 'eens', vaak gebruikt om een verzoek vriendelijker te maken of een onbepaald moment aan te duiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kom <w>es</w> kijken wat ik hier heb gevonden.",
                            "target_lang_translations": {
                                "en": "<w>Just</w> come and have a look at what I found here.",
                                "ru": "Иди-<w>ка</w> посмотри, что я здесь нашел."
                            }
                        },
                        {
                            "text": "Ik zal het <w>es</w> aan mijn vader vragen.",
                            "target_lang_translations": {
                                "en": "I will ask my father about it <w>sometime</w>.",
                                "ru": "Я <w>как-нибудь</w> спрошу об этом у отца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kom es",
                        "luister es",
                        "kijk es"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "spreektaal"
                        },
                        {
                            "trait_type": "form",
                            "comment": "van 'eens'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A shortened form of 'eens' (once), often used to make a request friendlier or to indicate an indefinite moment.",
                        "ru": "Сокращенная форма от 'eens', часто используется для смягчения просьбы или указания на неопределенный момент."
                    },
                    "translations": {

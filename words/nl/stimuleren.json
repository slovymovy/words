{
    "word_family": [
        "stimulans",
        "stimulatie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2a9c3249-5495-358e-9e5a-fe51aa1f2e50",
                    "sense_definition": "Maatregelen nemen om de ontwikkeling of voortgang van iets te bevorderen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bevorderen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering wil de economie <w>stimuleren</w> door de belastingen te verlagen.",
                            "target_lang_translations": {
                                "en": "The government wants to <w>stimulate</w> the economy by lowering taxes."
                            }
                        },
                        {
                            "text": "Het nieuwe beleid moet de groei van duurzame energie <w>stimuleren</w>.",
                            "target_lang_translations": {
                                "en": "The new policy aims to <w>boost</w> the growth of sustainable energy."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevorderen"
                    ],
                    "common_phrases": [
                        "de economie stimuleren",
                        "de groei stimuleren",
                        "innovatie stimuleren",
                        "de verkoop stimuleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To take measures to promote the development or progress of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stimulate",
                                "target_lang_sense_clarification": "general / economy"
                            },
                            {
                                "target_lang_word": "boost",
                                "target_lang_sense_clarification": "increase activity or growth"
                            },
                            {
                                "target_lang_word": "promote",
                                "target_lang_sense_clarification": "support progress"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dff355b-212a-3c33-b6a1-013b44bb1bfc",
                    "sense_definition": "Iemand of iets aansporen om iets te doen of actiever te worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "aansporen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De docent probeert de studenten te <w>stimuleren</w> om kritisch na te denken.",
                            "target_lang_translations": {
                                "en": "The teacher tries to <w>encourage</w> the students to think critically."
                            }
                        },
                        {
                            "text": "We moeten elkaar <w>stimuleren</w> om gezond te blijven eten.",
                            "target_lang_translations": {
                                "en": "We must <w>motivate</w> each other to keep eating healthily."
                            }
                        }
                    ],
                    "synonyms": [
                        "aansporen",
                        "prikkelen",
                        "aanzetten",
                        "aanporren"
                    ],
                    "common_phrases": [
                        "elkaar stimuleren",
                        "kinderen stimuleren",
                        "creativiteit stimuleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To urge someone or something to do something or become more active."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "encourage",
                                "target_lang_sense_clarification": "give support or confidence"
                            },
                            {
                                "target_lang_word": "motivate",
                                "target_lang_sense_clarification": "give a reason or desire to act"
                            },
                            {
                                "target_lang_word": "stimulate",
                                "target_lang_sense_clarification": "arouse interest or activity"
                            }
                        ]
                    }
                }
            ]
        }
    ]

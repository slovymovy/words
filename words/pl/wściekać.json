                {
                    "sense_id": "02987f38-9713-3f5c-97d5-0b3e77578925",
                    "sense_definition": "Odczuwać bardzo silną złość, wpadać w gniew lub szał; tracić panowanie nad sobą (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anger_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie ma sensu <w>wściekać</w> się o rzeczy, na które nie mamy wpływu.",
                            "target_lang_translations": {
                                "ru": "Нет смысла <w>беситься</w> из-за вещей, на которые мы не можем повлиять."
                            }
                        },
                        {
                            "text": "Mój brat zawsze <w>wścieka</w> się, gdy przegrywa w gry komputerowe.",
                            "target_lang_translations": {
                                "ru": "Мой брат всегда <w>бесится</w>, когда проигрывает в компьютерные игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "denerwować się",
                        "złościć się",
                        "irytować się",
                        "pieklić się",
                        "wkurzać się"
                    ],
                    "antonyms": [
                        "uspokajać się",
                        "wyciszać się"
                    ],
                    "common_phrases": [
                        "wściekać się o byle co",
                        "wściekać się na kogoś",
                        "wściekać się bez powodu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo nacechowane emocjonalnie, częste w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать сильную злость, приходить в ярость или бешенство; терять самообладание (используется с возвратным местоимением)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беситься",
                                "target_lang_sense_clarification": "разговорное; приходить в крайнее раздражение, неистовствовать"
                            },
                            {
                                "target_lang_word": "злиться",
                                "target_lang_sense_clarification": "испытывать чувство злости, гнева"
                            },
                            {
                                "target_lang_word": "выходить из себя",
                                "target_lang_sense_clarification": "терять самообладание от гнева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1be66b11-6d18-3079-a514-4029f1024bd9",
                    "sense_definition": "Doprowadzać kogoś do silnego gniewu lub szału; bardzo mocno kogoś denerwować",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anger_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego aroganckie zachowanie zaczyna mnie powoli <w>wściekać</w>.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4672ece1-8205-3caa-87d5-c0de1b6babbc",
                    "sense_definition": "A large wild animal of the family Cervidae that eats grass and leaves and has hooves; the male usually has antlers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We saw a wild <w>deer</w> standing quietly in the forest.",
                            "target_lang_translations": {
                                "ru": "Мы увидели дикого <w>оленя</w>, спокойно стоящего в лесу."
                            }
                        },
                        {
                            "text": "A large herd of <w>deer</w> was grazing in the open field.",
                            "target_lang_translations": {
                                "ru": "Большое стадо <w>оленей</w> паслось на открытом поле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "herd of deer",
                        "wild deer",
                        "deer hunting",
                        "red deer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное дикое животное семейства оленевых, питающееся травой и листьями и имеющее копыта; у самцов обычно есть рога"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "олень",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "643a591c-cc53-3f80-843e-f73fd36c9747",
                    "sense_definition": "The meat of a deer used as food.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "meat_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He prefers the taste of <w>deer</w> over beef.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает вкус <w>оленины</w> говядине."
                            }
                        },
                        {
                            "text": "We tried some dried <w>deer</w> meat during our camping trip.",
                            "target_lang_translations": {
                                "ru": "Мы попробовали сушёное мясо <w>оленя</w> во время нашего похода."
                            }
                        }
                    ],
                    "synonyms": [
                        "venison"
                    ],
                    "common_phrases": [
                        "deer meat",
                        "deer sausage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо оленя, употребляемое в пищу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оленина",
                                "target_lang_sense_clarification": "мясо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ffd7ea1-86e3-3321-a9d7-c0603c9cf407",
                    "sense_definition": "Any animal, especially a four-legged mammal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mice, rats, and such small <w>deer</w> were often found in the old barn.",
                            "target_lang_translations": {
                                "ru": "Мыши, крысы и прочие мелкие <w>звери</w> часто водились в старом сарае."
                            }
                        },
                        {
                            "text": "The ancient forest was home to many kinds of wild <w>deer</w>.",
                            "target_lang_translations": {
                                "ru": "Древний лес был домом для многих видов диких <w>зверей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "beast",
                        "creature"
                    ],
                    "common_phrases": [
                        "small deer"
                    ],
                    "traits": [

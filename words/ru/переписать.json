                        {
                            "text": "Нам нужно <переписать> всё оборудование на складе до конца недели.",
                            "target_lang_translations": {
                                "en": "We need to <inventory> all the equipment in the warehouse by the end of the week."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарегистрировать",
                        "учесть",
                        "инвентаризировать"
                    ],

                            },
                            {
                                "target_lang_word": "supremacy",
                                "target_lang_sense_clarification": "position of highest power"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "превосходство",
                                "target_lang_sense_clarification": "более высокое качество, сила или статус"
                            },
                            {

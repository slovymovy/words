{
    "word_family": [
        "tangent",
        "tangency",
        "tangentially"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1268ca3b-3b3d-3c2f-9d8a-d3f0ea34f828",
                    "sense_definition": "Only slightly or indirectly related to the main subject; diverging from the primary topic.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That subject is <w>tangential</w> to our discussion, and we cannot let it distract us.",
                            "target_lang_translations": {
                                "ru": "Эта тема является <w>второстепенной</w> для нашей дискуссии, и мы не можем позволить ей отвлечь нас."
                            }
                        },
                        {
                            "text": "The cost is <w>tangential</w> to the main goal of achieving high quality.",
                            "target_lang_translations": {
                                "ru": "Стоимость <w>второстепенна</w> по отношению к главной цели — достижению высокого качества."
                            }
                        }
                    ],
                    "synonyms": [
                        "peripheral",
                        "incidental",
                        "digressive",
                        "unrelated"
                    ],
                    "antonyms": [
                        "central",
                        "relevant",
                        "essential",
                        "core"
                    ],
                    "common_phrases": [
                        "tangential to",
                        "tangential issue",
                        "tangential point",
                        "tangential remark"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишь отдалённо или косвенно связанный с главной темой; второстепенный или уходящий в сторону от сути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косвенный",
                                "target_lang_sense_clarification": "непрямой, имеющий отдалённое отношение"
                            },
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "не основной, менее важный"
                            },
                            {
                                "target_lang_word": "отдалённый",
                                "target_lang_sense_clarification": "слабо связанный с предметом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dcf301b-2c7c-34c3-9c5d-a877ceb7e6a5",
                    "sense_definition": "Relating to or moving along a straight line that touches a curve at a single point without crossing it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geometric_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The meteor came in on a <w>tangential</w> orbit and exploded above the earth's surface.",
                            "target_lang_translations": {
                                "ru": "Метеор вошел по <w>касательной</w> орбите и взорвался над поверхностью Земли."
                            }
                        },
                        {
                            "text": "Applying a <w>tangential</w> force to the wheel causes it to spin faster.",
                            "target_lang_translations": {
                                "ru": "Приложение <w>тангенциальной</w> силы к колесу заставляет его вращаться быстрее."
                            }
                        }
                    ],
                    "synonyms": [
                        "touching"
                    ],
                    "antonyms": [
                        "radial"
                    ],
                    "common_phrases": [
                        "tangential force",
                        "tangential velocity",
                        "tangential acceleration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направленный по касательной; относящийся к прямой линии, которая касается кривой в одной точке, не пересекая её."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тангенциальный",
                                "target_lang_sense_clarification": "спец. термин в физике и механике"
                            },
                            {
                                "target_lang_word": "касательный",
                                "target_lang_sense_clarification": "геометрический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
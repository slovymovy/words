    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ff50951-aa50-3a54-8bff-980f2676e758",
                    "sense_definition": "Специально оборудованное место в магазине или другом учреждении, где производят расчёт за покупки или услуги.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "place_of_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, пройдите на <кассу> для оплаты выбранных товаров.",
                            "target_lang_translations": {
                                "en": "Please proceed to the <w>checkout</w> to pay for the selected items."
                            }
                        },
                        {
                            "text": "В супермаркете работала только одна <касса>, поэтому собралась большая очередь.",
                            "target_lang_translations": {
                                "en": "Only one <w>checkout</w> was open in the supermarket, so a long line formed."
                            }
                        }
                    ],
                    "common_phrases": [
                        "свободная касса",
                        "стоять на кассе",
                        "пройти на кассу"
                    ],
                    "target_lang_definitions": {
                        "en": "A specially equipped place in a shop or other establishment where payment for purchases or services is made."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "checkout",
                                "target_lang_sense_clarification": "place to pay in a store"
                            },
                            {
                                "target_lang_word": "cash desk",
                                "target_lang_sense_clarification": "counter for payment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac7fcb63-e51a-3a8a-9c87-9edd37b493a2",
                    "sense_definition": "Окошко, киоск или специальное помещение, где продаются билеты в театр, кино, на вокзал или на стадион.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_of_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы купили билеты в театр через городскую <кассу>.",
                            "target_lang_translations": {
                                "en": "We bought theater tickets through the city <w>ticket office</w>."
                            }
                        },
                        {
                            "text": "Железнодорожная <касса> закрывается на перерыв через десять минут.",
                            "target_lang_translations": {
                                "en": "The railway <w>ticket office</w> closes for a break in ten minutes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "театральная касса",
                        "билетная касса",
                        "билеты в кассе"
                    ],
                    "target_lang_definitions": {
                        "en": "A window, kiosk, or special room where tickets for the theater, cinema, train station, or stadium are sold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ticket office",
                                "target_lang_sense_clarification": "general place to buy tickets"
                            },
                            {
                                "target_lang_word": "box office",
                                "target_lang_sense_clarification": "for theater or cinema tickets"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6281ca3e-19bb-3f7a-92a0-e216ce95232c",
                    "sense_definition": "Аппарат для фиксации денежных операций и печати чеков.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Продавец не смог пробить чек, так как <касса> внезапно зависла.",
                            "target_lang_translations": {
                                "en": "The salesperson couldn't print the receipt because the <w>cash register</w> suddenly froze."
                            }
                        },
                        {
                            "text": "Новая <касса> позволяет принимать оплату как картой, так и телефоном.",
                            "target_lang_translations": {
                                "en": "The new <w>cash register</w> allows accepting payment by both card and phone."
                            }
                        }
                    ],
                    "synonyms": [
                        "кассовый аппарат"
                    ],
                    "common_phrases": [
                        "касса сломалась",
                        "зарегистрировать кассу"
                    ],
                    "target_lang_definitions": {

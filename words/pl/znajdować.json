            "senses": [
                {
                    "sense_id": "79bd965e-a363-372e-8656-505ea20a27e5",
                    "sense_definition": "Zdobywać to, czego się szukało; ustalać położenie zagubionej osoby lub rzeczy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_finding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <znajduję> moje klucze w zupełnie innym miejscu.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <нахожу> свои ключи в совершенно другом месте."
                            }
                        },
                        {
                            "text": "Dzięki mapie w telefonie szybko <znajdujemy> drogę do hotelu.",
                            "target_lang_translations": {
                                "ru": "Благодаря карте в телефоне мы быстро <находим> дорогу к отелю."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnajdować",
                        "wyszukiwać"
                    ],
                    "antonyms": [
                        "gubić"
                    ],
                    "common_phrases": [
                        "znajdować klucze",
                        "znajdować drogę",
                        "znajdować zgubę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находить то, что искали; определять местонахождение потерянного человека или вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "отыскивать потерянное"
                            },
                            {
                                "target_lang_word": "отыскивать",
                                "target_lang_sense_clarification": "искать и находить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b7a81bf-54bb-3289-b424-1ab6f3a15cd9",
                    "sense_definition": "Być umiejscowionym w konkretnym miejscu lub przestrzeni (używane z zaimkiem „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muzeum Narodowe <znajduje> się w samym centrum Warszawy.",
                            "target_lang_translations": {
                                "ru": "Национальный музей <находится> в самом центре Варшавы."
                            }
                        },
                        {
                            "text": "Na stole w kuchni <znajduje> się duży talerz ze świeżymi owocami.",
                            "target_lang_translations": {
                                "ru": "На кухонном столе <находится> большая тарелка со свежими фруктами."
                            }
                        }
                    ],
                    "synonyms": [
                        "być",
                        "przebywać ( uwagi)"
                    ],
                    "common_phrases": [
                        "znajdować się w",
                        "znajdować się na",
                        "znajdować się blisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагаться в конкретном месте или пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "располагаться"
                            },
                            {
                                "target_lang_word": "располагаться",
                                "target_lang_sense_clarification": "занимать место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b71ce998-fb6c-31cd-a38e-cd027d52d3c2",
                    "sense_definition": "Odkrywać coś, natrafiać na coś lub zauważać coś przypadkiem.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_finding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Często <znajduję> drobne monety na chodniku w drodze do pracy.",
                            "target_lang_translations": {
                                "ru": "Я часто <нахожу> мелкие монеты на тротуаре по дороге на работу."
                            }
                        },
                        {
                            "text": "Archeolodzy <znajdują> w tej jaskini wiele starych narzędzi.",
                            "target_lang_translations": {
                                "ru": "Археологи <находят> в этой пещере много старинных инструментов."
                            }
                        }
                    ],
                    "synonyms": [
                        "odkrywać",
                        "zauważać",
                        "spotykać"
                    ],
                    "common_phrases": [
                        "znajdować pieniądze",
                        "znajdować ślady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаруживать что-то, натыкаться на что-то или замечать что-то случайно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "обнаруживать случайно"
                            },
                            {
                                "target_lang_word": "обнаруживать",
                                "target_lang_sense_clarification": "замечать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "122bace2-ad17-3667-97c4-d8fbdcc905dd",
                    "sense_definition": "Wygospodarować lub wykrzesać z siebie coś potrzebnego, na przykład czas, siłę lub odwagę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_finding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo wielu obowiązków zawsze <znajduję> czas na czytanie książek.",
                            "target_lang_translations": {
                                "ru": "Несмотря на множество обязанностей, я всегда <нахожу> время для чтения книг."
                            }
                        },
                        {
                            "text": "On rzadko <znajduje> w sobie dość odwagi, by głośno wyrazić sprzeciw.",
                            "target_lang_translations": {
                                "ru": "Он редко <находит> в себе достаточно смелости, чтобы громко выразить протест."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znajdować czas",
                        "znajdować rozwiązanie",
                        "znajdować siłę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделять или изыскивать что-то необходимое, например время, силы или смелость."

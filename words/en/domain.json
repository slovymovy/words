                },
                {
                    "sense_id": "b67a4965-40ae-3ed4-84dd-3d5e4578dd33",
                    "sense_definition": "The set of all possible input values for which a mathematical function is defined.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>domain</w> of this function includes all positive real numbers.",
                            "target_lang_translations": {

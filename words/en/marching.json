            "senses": [
                {
                    "sense_id": "9183e3de-c9ac-3408-b310-a8efa3ed5a53",
                    "sense_definition": "The action of walking with a steady, rhythmic step, typically in a group.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constant <w>marching</w> left the troops exhausted by sunset.",
                            "target_lang_translations": {
                                "ru": "Постоянная <w>маршировка</w> вымотала войска к закату."
                            }
                        },
                        {
                            "text": "They practiced their <w>marching</w> on the parade ground for hours.",
                            "target_lang_translations": {
                                "ru": "Они часами отрабатывали <w>строевой шаг</w> на плацу."
                            }
                        }
                    ],
                    "synonyms": [
                        "parading"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходьба ровным, ритмичным шагом, обычно в группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршировка",
                                "target_lang_sense_clarification": "процесс ходьбы"
                            },
                            {
                                "target_lang_word": "строевой шаг",
                                "target_lang_sense_clarification": "техника ходьбы"
                            },
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "организованное движение"
                            }
                        ]
                    }
                }
            ]

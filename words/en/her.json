    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "2ac37d40-b7da-3692-a03c-417a54f0b327",
                    "sense_definition": "Belonging to or associated with a female person or animal previously mentioned.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wrote <w>her</w> name on the paper.",
                            "target_lang_translations": {
                                "ru": "Она написала <w>своё</w> имя на бумаге."
                            }
                        },
                        {
                            "text": "This is <w>her</w> book, not mine.",
                            "target_lang_translations": {
                                "ru": "Это <w>её</w> книга, а не моя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "her name",
                        "her family",
                        "her eyes",
                        "her job",
                        "her life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий или относящийся к лицу женского пола, упомянутому ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "её",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "возвратное притяжательное местоимение (когда принадлежит подлежащему)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc2a9d86-63e7-3687-84ed-32a7151724b6",
                    "sense_definition": "Belonging to a person of unspecified gender, often used to avoid the assumption that a generic person is male.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every student should bring <w>her</w> own lunch to the field trip.",
                            "target_lang_translations": {
                                "ru": "Каждый студент должен принести <w>свой</w> обед на экскурсию."
                            }
                        },
                        {
                            "text": "A doctor must always listen to <w>her</w> patients carefully.",
                            "target_lang_translations": {
                                "ru": "Врач должен всегда внимательно выслушивать <w>своих</w> пациентов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий лицу неопределенного пола (часто используется, чтобы избежать предположения, что это лицо мужского пола)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "возвратное притяжательное местоимение"
                            },
                            {
                                "target_lang_word": "её",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "ed1eb09a-f1dc-390c-af41-87bd48bc2280",
                    "sense_definition": "The object form of the pronoun 'she', used to refer to a female person or animal as the object of a verb or preposition.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "object_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saw <w>her</w> at the park yesterday.",
                            "target_lang_translations": {
                                "ru": "Я видел <w>её</w> вчера в парке."
                            }
                        },
                        {
                            "text": "Please give the letter to <w>her</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай письмо <w>ей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tell her",
                        "ask her",
                        "with her",
                        "for her",
                        "call her"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объектная форма местоимения 'она', используется для обозначения лица женского пола как объекта действия или после предлогов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "её",
                                "target_lang_sense_clarification": "винительный или родительный падеж"
                            },
                            {
                                "target_lang_word": "ей",
                                "target_lang_sense_clarification": "дательный или творительный падеж"
                            },
                            {
                                "target_lang_word": "ней",
                                "target_lang_sense_clarification": "после предлогов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "47737d06-0849-3e78-9b72-7653a9ac2d0f",
                    "sense_definition": "A female person or animal.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "gender_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I think this bird is a <w>her</w>, not a him.",
                            "target_lang_translations": {
                                "ru": "Я думаю, что эта птица — <w>самка</w>, а не самец."
                            }
                        },
                        {
                            "text": "Is the new baby a him or a <w>her</w>?",
                            "target_lang_translations": {
                                "ru": "Новый ребенок — мальчик или <w>девочка</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "she"
                    ],
                    "antonyms": [
                        "him"
                    ],
                    "common_phrases": [
                        "a him or a her"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to gender as a noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или животное женского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самка",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "девочка",
                                "target_lang_sense_clarification": "разговорное, о людях"
                            },
                            {
                                "target_lang_word": "она",
                                "target_lang_sense_clarification": "в значении существительного"
                            }
                        ]
                    }
                }
            ]
        }
    ]

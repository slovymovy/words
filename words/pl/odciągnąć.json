    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b28ef498-4a18-3ff6-811e-8130a0216294",
                    "sense_definition": "Zmienić położenie kogoś lub czegoś, ciągnąc w bok lub do tyłu; oddzielić kogoś od kogoś innego siłą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem siłą <odciągnąć> psa od bramy, bo szczekał na listonosza.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось силой <оттащить> собаку от ворот, так как она лаяла на почтальона."
                            }
                        },
                        {
                            "text": "Ochroniarze zdołali szybko <odciągnąć> agresywnego klienta od lady.",
                            "target_lang_translations": {
                                "ru": "Охранникам удалось быстро <оттащить> агрессивного клиента от прилавка."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsunąć",
                        "odholować",
                        "odwlec"
                    ],
                    "antonyms": [
                        "przyciągnąć",
                        "przysunąć"
                    ],
                    "common_phrases": [
                        "odciągnąć na bok",
                        "odciągnąć siłą",
                        "odciągnąć rękę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить кого-то или что-то, потянув в сторону или назад; силой отделить кого-то от кого-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оттащить",
                                "target_lang_sense_clarification": "переместить силой или волоком"
                            },
                            {
                                "target_lang_word": "оттянуть",
                                "target_lang_sense_clarification": "потянуть на себя или в сторону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fde1eaf-2c68-3d01-82b2-ebe0e6f5d453",
                    "sense_definition": "Sprawić, że ktoś przestanie zwracać na coś uwagę lub o czymś myśleć; odwrócić uwagę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hałas na ulicy skutecznie <odciągnął> moją uwagę od nauki.",
                            "target_lang_translations": {
                                "ru": "Шум на улице эффективно <отвлек> мое внимание от учебы."
                            }
                        },
                        {
                            "text": "Próbowałem <odciągnąć> jego myśli od problemów w pracy, proponując spacer.",
                            "target_lang_translations": {
                                "ru": "Я пытался <отвлечь> его мысли от проблем на работе, предложив прогуляться."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwrócić",
                        "zająć"
                    ],
                    "antonyms": [
                        "skupić",
                        "przyciągnąć"
                    ],
                    "common_phrases": [
                        "odciągnąć uwagę",
                        "odciągnąć myśli",
                        "odciągnąć od pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то перестать обращать внимание на что-то или думать о чем-то; отвлечь внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвлечь",
                                "target_lang_sense_clarification": "переключить внимание или мысли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4738cd2c-f635-37fd-b458-54fe4d177069",
                    "sense_definition": "Wydobyć płyn z jakiegoś zbiornika lub naczynia, zazwyczaj poprzez ssanie lub spuszczanie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik musiał <odciągnąć> trochę paliwa ze zbiornika, aby naprawić usterkę.",
                            "target_lang_translations": {
                                "ru": "Механику пришлось <слить> немного топлива из бака, чтобы устранить неисправность."
                            }
                        },
                        {
                            "text": "Młoda matka nauczyła się, jak bezpiecznie <odciągnąć> pokarm dla swojego dziecka.",
                            "target_lang_translations": {
                                "ru": "Молодая мама научилась безопасно <сцедить> молоко для своего ребенка."
                            }
                        }
                    ],
                    "synonyms": [
                        "odlać",
                        "wypompować",
                        "spuścić"
                    ],
                    "antonyms": [
                        "dolać",
                        "wlać"
                    ],
                    "common_phrases": [
                        "odciągnąć paliwo",
                        "odciągnąć mleko",
                        "odciągnąć wodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлечь жидкость из емкости, обычно путем отсасывания или сливания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слить",
                                "target_lang_sense_clarification": "жидкость, топливо"
                            },
                            {
                                "target_lang_word": "сцедить",
                                "target_lang_sense_clarification": "грудное молоко"
                            },
                            {
                                "target_lang_word": "откачать",
                                "target_lang_sense_clarification": "жидкость с помощью насоса или шланга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1edd8b5-b56d-3f20-9939-f70d04a4ab6e",
                    "sense_definition": "Odjąć określoną sumę pieniędzy od wypłaty lub rachunku na poczet jakiegoś zobowiązania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracodawca musiał <odciągnąć> podatek od mojej pierwszej wypłaty.",
                            "target_lang_translations": {
                                "ru": "Работодатель должен был <вычесть> налог из моей первой зарплаты."
                            }
                        },
                        {
                            "text": "Bank automatycznie <odciągnął> ratę kredytu z mojego konta osobistego.",
                            "target_lang_translations": {
                                "ru": "Банк автоматически <списал> взнос по кредиту с моего личного счета."
                            }
                        }
                    ],
                    "synonyms": [
                        "potrącić",
                        "odliczyć"
                    ],
                    "antonyms": [
                        "doliczyć",
                        "dodać"
                    ],
                    "common_phrases": [
                        "odciągnąć podatek",
                        "odciągnąć z pensji",
                        "odciągnąć z wypłaty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used instead of 'potrącić'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вычесть определенную сумму денег из выплаты или счета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вычесть",
                                "target_lang_sense_clarification": "отнять часть суммы"
                            },
                            {
                                "target_lang_word": "удержать",
                                "target_lang_sense_clarification": "не выплатить часть суммы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d68d387c-0589-3fa0-bec7-ba10ed8bc6fd",
                    "sense_definition": "Pociągnąć ruchomą część mechanizmu do tyłu, aby go napiąć lub przygotować do działania.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz <odciągnął> zamek karabinu, aby sprawdzić komorę nabojową.",
                            "target_lang_translations": {
                                "ru": "Солдат <оттянул> затвор винтовки, чтобы проверить патронник."
                            }
                        },
                        {
                            "text": "Musisz mocno <odciągnąć> kurek rewolweru przed oddaniem strzału.",
                            "target_lang_translations": {
                                "ru": "Ты должен сильно <отвести> курок револьвера перед выстрелом."
                            }
                        }
                    ],
                    "synonyms": [
                        "naciągnąć",
                        "odwieść"
                    ],
                    "antonyms": [
                        "zwolnić",
                        "spuścić"
                    ],
                    "common_phrases": [
                        "odciągnąć kurek",
                        "odciągnąć zamek",
                        "odciągnąć cięciwę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потянуть подвижную часть механизма назад для подготовки к действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оттянуть",
                                "target_lang_sense_clarification": "затвор или подвижную часть"
                            },
                            {
                                "target_lang_word": "отвести",
                                "target_lang_sense_clarification": "курок или механизм назад"
                            }
                        ]
                    }
                }
            ]
        }
    ]

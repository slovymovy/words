{
    "word_family": [
        "pogorszenie",
        "pogorszyć",
        "gorszy",
        "najgorszy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "67a3735d-54a4-3524-b19d-d792a69cfc48",
                    "sense_definition": "Sprawiać, że coś staje się gorsze, trudniejsze, mniej korzystne lub bardziej intensywne w negatywnym sensie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "zmiana_stanu_negatywna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Palenie papierosów może znacznie <w>pogarszać</w> stan twojego zdrowia.",
                            "target_lang_translations": {
                                "ru": "Курение сигарет может значительно <w>ухудшать</w> состояние твоего здоровья."
                            }
                        },
                        {
                            "text": "Nie chcę <w>pogarszać</w> sprawy, więc wolałbym nie komentować tego incydentu.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>усугублять</w> ситуацию, поэтому предпочел бы не комментировать этот инцидент."
                            }
                        },
                        {
                            "text": "Kryzys gospodarczy zaczął <w>pogarszać</w> sytuację finansową wielu rodzin.",
                            "target_lang_translations": {
                                "ru": "Экономический кризис начал <w>ухудшать</w> финансовое положение многих семей."
                            }
                        }
                    ],
                    "synonyms": [
                        "psuć",
                        "zaostrzać",
                        "komplikować"
                    ],
                    "antonyms": [
                        "polepszać",
                        "poprawiać",
                        "naprawiać"
                    ],
                    "common_phrases": [
                        "pogarszać sytuację",
                        "pogarszać sprawę",
                        "pogarszać stan",
                        "pogarszać samopoczucie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то хуже, сложнее, менее благоприятным или более интенсивным в негативном смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухудшать",
                                "target_lang_sense_clarification": "делать хуже, чем было"
                            },
                            {
                                "target_lang_word": "усугублять",
                                "target_lang_sense_clarification": "делать более тяжелым, серьезным (о ситуации, проблеме)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "773b4bbe-b440-36f1-b454-72d138898096",
                    "sense_definition": "Stawać się gorszym, tracić na jakości lub wartości (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "zmiana_stanu_negatywna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety, pogoda zaczęła się gwałtownie <w>pogarszać</w> tuż po południu.",
                            "target_lang_translations": {
                                "ru": "К сожалению, погода начала резко <w>портиться</w> сразу после полудня."
                            }
                        },
                        {
                            "text": "Jego stan zdrowia <w>pogarsza</w> się z każdym kolejnym dniem.",
                            "target_lang_translations": {
                                "ru": "Его состояние здоровья <w>ухудшается</w> с каждым днем."
                            }
                        },
                        {
                            "text": "Sytuacja na rynku pracy może się <w>pogarszać</w> w nadchodzących miesiącach.",
                            "target_lang_translations": {
                                "ru": "Ситуация на рынке труда может <w>ухудшаться</w> в ближайшие месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "psuć się",
                        "podupadać"
                    ],
                    "antonyms": [
                        "polepszać się",
                        "poprawiać się"
                    ],
                    "common_phrases": [
                        "pogarszać się z czasem",
                        "stan się pogarsza",
                        "pogoda się pogarsza",
                        "zdrowie się pogarsza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться хуже, терять в качестве или ценности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухудшаться",
                                "target_lang_sense_clarification": "становиться хуже"
                            },
                            {
                                "target_lang_word": "портиться",
                                "target_lang_sense_clarification": "терять свои свойства, становиться плохим (о погоде, настроении)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
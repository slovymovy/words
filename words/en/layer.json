    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b1e69f3f-41c7-3a8c-873d-d1af8479c682",
                    "sense_definition": "A single thickness of some material covering a surface.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A thick <w>layer</w> of dust covered the old books.",
                            "target_lang_translations": {
                                "ru": "Толстый <w>слой</w> пыли покрывал старые книги."
                            }
                        },
                        {
                            "text": "Apply a thin <w>layer</w> of glue to the back of the paper.",
                            "target_lang_translations": {
                                "ru": "Нанесите тонкий <w>слой</w> клея на обратную сторону бумаги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thin layer",
                        "thick layer",
                        "outer layer",
                        "top layer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один уровень толщины какого-либо материала, покрывающего поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слой",
                                "target_lang_sense_clarification": "материала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94eaf3fb-7ca3-36ea-a230-5852bc2fb889",
                    "sense_definition": "A level or rank in a hierarchy or complex system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company has too many <w>layers</w> of management.",

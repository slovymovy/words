    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "03e0a26e-2765-3212-b484-4c297f9d708e",
                    "sense_definition": "Выполнить действие ещё раз, повторно, начиная с самого начала (часто подразумевает переделывание сделанного ранее).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если результат тебя не устраивает, лучше начать всё <w>заново</w>.",
                            "target_lang_translations": {
                                "en": "If the result does not satisfy you, it is better to start everything <w>all over again</w>."
                            }
                        },
                        {
                            "text": "После пожара дом пришлось отстраивать <w>заново</w>.",
                            "target_lang_translations": {
                                "en": "After the fire, the house had to be rebuilt <w>from scratch</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "снова",
                        "вновь",
                        "ещё раз",
                        "сызнова"
                    ],
                    "common_phrases": [
                        "начать заново",
                        "родиться заново",
                        "строить заново",
                        "учиться заново"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform an action again, starting from the very beginning (often implies redoing what was done before)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "all over again",
                                "target_lang_sense_clarification": "Emphasizes repeating the entire process"
                            },
                            {
                                "target_lang_word": "from scratch",
                                "target_lang_sense_clarification": "Idiomatic, implies starting from the very beginning with nothing"
                            },
                            {
                                "target_lang_word": "anew",
                                "target_lang_sense_clarification": "Formal or literary, meaning once more"
                            },
                            {
                                "target_lang_word": "from the start",
                                "target_lang_sense_clarification": "Literal translation indicating the beginning point"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "467fa869-8292-3499-be91-b45a56d26580",
                    "sense_definition": "Сделать что-то иначе, по-новому, не так, как было раньше; пересмотреть подход.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Историю этого города нужно написать <w>заново</w>, опираясь на найденные документы.",
                            "target_lang_translations": {
                                "en": "The history of this city needs to be written <w>anew</w>, based on the found documents."
                            }
                        },
                        {
                            "text": "Он смог <w>заново</w> взглянуть на привычные вещи и найти решение проблемы.",
                            "target_lang_translations": {
                                "en": "He was able to look at familiar things <w>afresh</w> and find a solution to the problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "вновь",
                        "снова",
                        "сызнова"
                    ],
                    "common_phrases": [
                        "переосмыслить заново",
                        "открыть заново",
                        "взглянуть заново"
                    ],
                    "target_lang_definitions": {
                        "en": "To do something differently, in a new way, or with a fresh perspective compared to how it was done before."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anew",
                                "target_lang_sense_clarification": "Often used with verbs like write, build, or think to imply a new manner"
                            },
                            {
                                "target_lang_word": "afresh",
                                "target_lang_sense_clarification": "Implies a fresh start or new perspective"
                            },
                            {
                                "target_lang_word": "in a new way",
                                "target_lang_sense_clarification": "General description of doing something differently"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
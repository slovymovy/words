{
    "word_family": [
        "kadłubowy",
        "kadłubek"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "03e85d37-beba-37b6-83a5-31731c991c9a",
                    "sense_definition": "Główna część konstrukcji statku wodnego, powietrznego lub innej maszyny, do której przymocowane są pozostałe elementy i wyposażenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas silnego sztormu stalowy <w>kadłub</w> statku wytrzymał uderzenia fal.",
                            "target_lang_translations": {
                                "ru": "Во время сильного шторма стальной <w>корпус</w> судна выдержал удары волн."
                            }
                        },
                        {
                            "text": "Inżynierowie zaprojektowali aerodynamiczny <w>kadłub</w> samolotu, aby zmniejszyć zużycie paliwa.",
                            "target_lang_translations": {
                                "ru": "Инженеры спроектировали аэродинамический <w>фюзеляж</w> самолета, чтобы снизить расход топлива."
                            }
                        }
                    ],
                    "synonyms": [
                        "korpus"
                    ],
                    "common_phrases": [
                        "kadłub statku",
                        "kadłub samolotu",
                        "sztywny kadłub",
                        "uszkodzony kadłub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная часть конструкции водного или воздушного судна, либо другой машины, к которой крепятся остальные элементы и оборудование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корпус",
                                "target_lang_sense_clarification": "судна, машины"
                            },
                            {
                                "target_lang_word": "фюзеляж",
                                "target_lang_sense_clarification": "самолета"
                            },
                            {
                                "target_lang_word": "остов",
                                "target_lang_sense_clarification": "каркас, скелет конструкции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0fcd497-4bea-3dfb-9c2c-ef53fb25c726",
                    "sense_definition": "Tułów człowieka lub zwierzęcia, stanowiący główną część ciała, zazwyczaj w kontekście braku głowy lub kończyn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum podziwialiśmy antyczny <w>kadłub</w> rzeźby greckiego herosa.",
                            "target_lang_translations": {
                                "ru": "В музее мы любовались античным <w>торсом</w> статуи греческого героя."
                            }
                        },
                        {
                            "text": "Jego potężny <w>kadłub</w> z trudem mieścił się w wąskim fotelu pasażera.",
                            "target_lang_translations": {
                                "ru": "Его мощное <w>туловище</w> с трудом помещалось в узком пассажирском кресле."
                            }
                        }
                    ],
                    "synonyms": [
                        "tułów",
                        "korpus"
                    ],
                    "common_phrases": [
                        "potężny kadłub",
                        "ludzki kadłub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Туловище человека или животного, составляющее основную часть тела, обычно рассматриваемое без головы и конечностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туловище",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "торс",
                                "target_lang_sense_clarification": "скульптура, верхняя часть тела"
                            },
                            {
                                "target_lang_word": "корпус",
                                "target_lang_sense_clarification": "телосложение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c8ba90a-b76f-3a1a-8067-a1adf05d7c78",
                    "sense_definition": "Wydrążony pień drzewa, wykorzystywany dawniej jako naczynie, element konstrukcyjny lub prosta łódź.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "historical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wystawie etnograficznej pokazano stary <w>kadłub</w> służący kiedyś do przechowywania zboża.",
                            "target_lang_translations": {
                                "ru": "На этнографической выставке показали старую <w>колоду</w>, служившую когда-то для хранения зерна."
                            }
                        },
                        {
                            "text": "Rybacy wypłynęli na jezioro, używając łodzi wykonanej jako jeden drewniany <w>kadłub</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбаки выплыли на озеро, используя лодку, сделанную из цельной деревянной <w>колоды</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drewniany kadłub",
                        "wydrążony kadłub"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "odnosi się do dawnych zastosowań wydrążonego pnia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдолбленный ствол дерева, использовавшийся в прошлом как сосуд, элемент конструкции или простая лодка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колода",
                                "target_lang_sense_clarification": "выдолбленное бревно"
                            },
                            {
                                "target_lang_word": "долблёнка",
                                "target_lang_sense_clarification": "вид лодки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8e8ed8b9-0460-31e5-a081-777444c12ffd",
                    "sense_definition": "Polskie nazwisko używane zarówno przez mężczyzn, jak i kobiety.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Kadłub</w> został wybrany na przewodniczącego lokalnego stowarzyszenia.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Кадлуб</w> был избран председателем местной ассоциации."
                            }
                        },
                        {
                            "text": "W książce telefonicznej znalazłem tylko jedną osobę o nazwisku <w>Kadłub</w>.",
                            "target_lang_translations": {
                                "ru": "В телефонной книге я нашел только одного человека с фамилией <w>Кадлуб</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия, используемая как мужчинами, так и женщинами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кадлуб",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

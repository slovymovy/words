{
    "word_family": [
        "wspierać",
        "wsparcie",
        "wspornik"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7026289-18c9-3b84-b5ea-32a5ad4f75e4",
                    "sense_definition": "Udzielić komuś pomocy, ratunku lub wsparcia (finansowego, duchowego, militarnego) w trudnej sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "help_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd obiecał <wesprzeć> finansowo rodziny poszkodowane w powodzi.",
                            "target_lang_translations": {
                                "ru": "Правительство обещало финансово <поддержать> семьи, пострадавшие от наводнения."
                            }
                        },
                        {
                            "text": "Przyjaciele powinni <wesprzeć> cię w tych trudnych chwilach.",
                            "target_lang_translations": {
                                "ru": "Друзья должны <поддержать> тебя в эти трудные минуты."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspomóc",
                        "pomóc",
                        "poratować"
                    ],
                    "antonyms": [
                        "zaszkodzić",
                        "opuścić"
                    ],
                    "common_phrases": [
                        "wesprzeć finansowo",
                        "wesprzeć duchowo",
                        "wesprzeć inicjatywę",
                        "wesprzeć budżet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказать кому-либо помощь, спасение или поддержку (финансовую, духовную, военную) в трудной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддержать",
                                "target_lang_sense_clarification": "оказать помощь"
                            },
                            {
                                "target_lang_word": "оказать поддержку",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9975ac87-1885-3def-8729-d994cacba270",
                    "sense_definition": "Oprzeć ciężar swojego ciała na czymś lub na kimś dla zachowania równowagi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starsza pani musiała <wesprzeć się> na lasce, aby wejść po schodach.",
                            "target_lang_translations": {
                                "ru": "Пожилой даме пришлось <опереться> на трость, чтобы подняться по лестнице."
                            }
                        },
                        {
                            "text": "Zmęczony turysta <wsparł się> o pień drzewa i głęboko odetchnął.",
                            "target_lang_translations": {
                                "ru": "Уставший турист <оперся> о ствол дерева и глубоко вздохнул."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprzeć się",
                        "podeprzeć się"
                    ],
                    "common_phrases": [
                        "wesprzeć się na łokciach",
                        "wesprzeć się na ramieniu",
                        "wesprzeć się o ścianę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенести вес своего тела на что-то или на кого-то для сохранения равновесия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опереться",
                                "target_lang_sense_clarification": "перенести вес тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5acf96d-a567-3b2e-b711-414455881c04",
                    "sense_definition": "Podeprzeć coś, aby się nie przewróciło; oprzeć jedną rzecz o drugą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna <wsparła> brodę na dłoniach i zamyśliła się głęboko.",
                            "target_lang_translations": {
                                "ru": "Девушка <подперла> подбородок ладонями и глубоко задумалась."
                            }
                        },
                        {
                            "text": "Architekt postanowił <wesprzeć> strop dodatkowymi kolumnami.",
                            "target_lang_translations": {
                                "ru": "Архитектор решил <подпереть> перекрытие дополнительными колоннами."
                            }
                        }
                    ],
                    "synonyms": [
                        "podeprzeć",
                        "podtrzymać"
                    ],
                    "common_phrases": [
                        "wesprzeć głowę na rękach",
                        "wesprzeć konstrukcję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подставить опору, чтобы что-то не упало; положить одну вещь на другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпереть",
                                "target_lang_sense_clarification": "подставить опору"
                            },
                            {
                                "target_lang_word": "опереть",
                                "target_lang_sense_clarification": "прислонить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b788216-c6a1-35be-9a8f-6f12733fdd22",
                    "sense_definition": "Udzielić sobie nawzajem pomocy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "help_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W obliczu kryzysu sąsiedzi potrafili <wesprzeć się> i działać razem.",
                            "target_lang_translations": {
                                "ru": "Перед лицом кризиса соседи смогли <поддержать друг друга> и действовать сообща."
                            }
                        },
                        {
                            "text": "Rodzeństwo powinno <wesprzeć się> w opiece nad chorymi rodzicami.",
                            "target_lang_translations": {
                                "ru": "Брат и сестра должны <поддержать друг друга> в уходе за больными родителями."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspomóc się"
                    ],
                    "common_phrases": [
                        "wesprzeć się nawzajem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказать друг другу помощь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддержать друг друга",
                                "target_lang_sense_clarification": "взаимно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
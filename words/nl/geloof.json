    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "03796910-463d-35c5-a138-6e54aa978b9a",
                    "sense_definition": "Een godsdienstige overtuiging of het stelsel van religieuze opvattingen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Welk <w>geloof</w> hang jij eigenlijk aan?",
                            "target_lang_translations": {
                                "ru": "Какую <w>религию</w> ты исповедуешь?"
                            }
                        },
                        {
                            "text": "Zij vindt veel steun en troost in haar <w>geloof</w>.",
                            "target_lang_translations": {
                                "ru": "Она находит большую поддержку и утешение в своей <w>вере</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "godsdienst",
                        "religie",
                        "geloofsovertuiging"
                    ],
                    "antonyms": [
                        "ongeloof"
                    ],
                    "common_phrases": [
                        "het christelijk geloof",
                        "van zijn geloof vallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Религиозное убеждение или система религиозных взглядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вера",
                                "target_lang_sense_clarification": "религиозное чувство"
                            },
                            {
                                "target_lang_word": "религия",
                                "target_lang_sense_clarification": "система взглядов"
                            },
                            {
                                "target_lang_word": "вероисповедание",
                                "target_lang_sense_clarification": "принадлежность к конфессии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "540e04d5-ded8-302f-9399-28e5002a7553",
                    "sense_definition": "De sterke overtuiging dat iets waar is of dat men op iemand kan vertrouwen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conviction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb er alle <w>geloof</w> in dat het project zal slagen.",
                            "target_lang_translations": {
                                "ru": "Я твердо <w>верю</w> в то, что проект будет успешным."
                            }
                        },
                        {
                            "text": "Je moet niet te veel <w>geloof</w> hechten aan die geruchten.",
                            "target_lang_translations": {
                                "ru": "Не стоит принимать эти слухи на <w>веру</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "overtuiging",
                        "fiducie"
                    ],
                    "antonyms": [
                        "ongeloof"
                    ],
                    "common_phrases": [
                        "geloof hechten aan",
                        "in goed geloof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердая уверенность в истинности чего-либо или доверие к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вера",
                                "target_lang_sense_clarification": "убежденность"
                            },
                            {
                                "target_lang_word": "доверие",
                                "target_lang_sense_clarification": "надежность"
                            },
                            {
                                "target_lang_word": "уверенность",
                                "target_lang_sense_clarification": "отсутствие сомнений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48434dc2-8003-37af-92b1-054157caff35",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>geloven</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>geloof</w> dat het morgen gaat regenen.",
                            "target_lang_translations": {
                                "ru": "Я <w>думаю</w>, что завтра пойдет дождь."
                            }
                        },
                        {
                            "text": "Ik <w>geloof</w> je op je woord.",
                            "target_lang_translations": {
                                "ru": "Я <w>верю</w> тебе на слово."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik geloof van wel"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "conjugation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 1-го лица единственного числа настоящего времени глагола <w>geloven</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верю",
                                "target_lang_sense_clarification": "я верю"
                            },
                            {
                                "target_lang_word": "думаю",
                                "target_lang_sense_clarification": "полагаю / мне кажется"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8c35e32-641f-33c4-b71e-cc386559266c",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>geloven</w> (bij inversie).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Geloof</w> jij in het bestaan van buitenaards leven?",
                            "target_lang_translations": {
                                "ru": "Ты <w>веришь</w> в существование инопланетной жизни?"
                            }
                        },
                        {
                            "text": "Waarom <w>geloof</w> je niet wat ik zeg?",
                            "target_lang_translations": {
                                "ru": "Почему ты не <w>веришь</w> тому, что я говорю?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inversion"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 2-го лица единственного числа настоящего времени глагола <w>geloven</w> (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веришь",
                                "target_lang_sense_clarification": "ты веришь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d446779-a8e0-30d2-b51d-ce6d1f9e6d3a",
                    "sense_definition": "Gebiedende wijs van <w>geloven</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Geloof</w> me, ik heb de waarheid gesproken.",
                            "target_lang_translations": {
                                "ru": "<w>Поверь</w> мне, я сказал правду."
                            }
                        },
                        {
                            "text": "<w>Geloof</w> in jezelf en geef niet op.",
                            "target_lang_translations": {
                                "ru": "<w>Верь</w> в себя и не сдавайся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "geloof me maar"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0dd3295d-7dc1-3840-9162-212a6262f399",
                    "sense_definition": "A member of a senate, which is typically the upper house of a legislature in countries like the United States or Canada.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legislator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>senator</w> voted in favor of the new education bill.",
                            "target_lang_translations": {
                                "ru": "<w>Сенатор</w> проголосовал за новый законопроект об образовании."
                            }
                        },
                        {
                            "text": "She wrote a letter to her state <w>senator</w> regarding the local park.",
                            "target_lang_translations": {
                                "ru": "Она написала письмо <w>сенатору</w> своего штата по поводу местного парка."
                            }
                        }
                    ],
                    "synonyms": [
                        "legislator",
                        "lawmaker"
                    ],
                    "common_phrases": [
                        "state senator",
                        "senior senator",
                        "junior senator",
                        "US senator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член сената, который обычно является верхней палатой законодательного органа в таких странах, как США или Канада."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенатор",
                                "target_lang_sense_clarification": "член парламента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5636a8e-5dbe-3243-acdb-bf53d0dc9261",
                    "sense_definition": "A title used before the name of a person who is a member of a senate.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legislator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are waiting for <w>Senator</w> Smith to arrive at the conference.",
                            "target_lang_translations": {
                                "ru": "Мы ждем, когда <w>сенатор</w> Смит прибудет на конференцию."
                            }
                        },
                        {
                            "text": "The reporter asked <w>Senator</w> Jones a difficult question about the economy.",
                            "target_lang_translations": {
                                "ru": "Репортер задал <w>сенатору</w> Джонсу сложный вопрос об экономике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "title"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, используемый перед именем человека, являющегося членом сената."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенатор",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b72e594b-8cf9-3e24-ba04-ee9ce276e3a5",
                    "sense_definition": "A member of the governing council in ancient Rome.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every Roman <w>senator</w> wore a toga with a distinctive purple stripe.",
                            "target_lang_translations": {
                                "ru": "Каждый римский <w>сенатор</w> носил тогу с отличительной пурпурной полосой."
                            }
                        },
                        {
                            "text": "The <w>senator</w> delivered a powerful speech in the forum.",
                            "target_lang_translations": {
                                "ru": "<w>Сенатор</w> произнес мощную речь на форуме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Roman senator"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член правящего совета в Древнем Риме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенатор",
                                "target_lang_sense_clarification": "в Древнем Риме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a818395-b7df-354b-9f2a-7012a89df0b8",
                    "sense_definition": "A government minister in certain German city-states such as Berlin, Bremen, or Hamburg.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "german_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>senator</w> for finance presented the city's annual budget.",
                            "target_lang_translations": {
                                "ru": "<w>Сенатор</w> по финансам представил годовой бюджет города."
                            }
                        },
                        {
                            "text": "He was appointed as a <w>senator</w> in the government of Hamburg.",
                            "target_lang_translations": {
                                "ru": "Он был назначен <w>сенатором</w> в правительстве Гамбурга."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Germany"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член правительства (министр) в некоторых немецких городах-государствах, таких как Берлин, Бремен или Гамбург."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенатор",
                                "target_lang_sense_clarification": "в Германии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

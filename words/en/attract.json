{
    "word_family": [
        "attraction",
        "attractive",
        "attractiveness",
        "unattractive",
        "attractor"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f3a3b86-a4f3-384e-91a3-04825d071464",
                    "sense_definition": "To cause someone to have a liking for or interest in something; to draw attention or allure someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Advertising is designed to <w>attract</w> customers to the new store.",
                            "target_lang_translations": {
                                "ru": "Реклама создана, чтобы <w>привлекать</w> покупателей в новый магазин."
                            }
                        },
                        {
                            "text": "She waved her hand to <w>attract</w> the waiter's attention.",
                            "target_lang_translations": {
                                "ru": "Она помахала рукой, чтобы <w>привлечь</w> внимание официанта."
                            }
                        }
                    ],
                    "synonyms": [
                        "allure",
                        "draw"
                    ],
                    "antonyms": [
                        "repel"
                    ],
                    "common_phrases": [
                        "attract attention",
                        "attract customers",
                        "attract interest",
                        "attract tourists"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать интерес, симпатию или обращать на себя внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "вызывать интерес или внимание"
                            },
                            {
                                "target_lang_word": "притягивать",
                                "target_lang_sense_clarification": "манить, располагать к себе"
                            },
                            {
                                "target_lang_word": "заинтересовывать",
                                "target_lang_sense_clarification": "пробуждать любопытство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4098d053-b61e-3487-a19e-8d096d71df5f",
                    "sense_definition": "To pull something towards itself by a physical force like gravity or magnetism without touching it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A strong magnet <w>attracts</w> iron filings instantly.",
                            "target_lang_translations": {
                                "ru": "Сильный магнит мгновенно <w>притягивает</w> железные опилки."
                            }
                        },
                        {
                            "text": "The gravity of the earth <w>attracts</w> the moon and keeps it in orbit.",
                            "target_lang_translations": {
                                "ru": "Гравитация Земли <w>притягивает</w> Луну и удерживает её на орбите."
                            }
                        }
                    ],
                    "synonyms": [
                        "draw"
                    ],
                    "antonyms": [
                        "repel"
                    ],
                    "common_phrases": [
                        "attract iron",
                        "attract particles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тянуть к себе физической силой (например, гравитацией или магнетизмом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притягивать",
                                "target_lang_sense_clarification": "физически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b97954c-eb15-393a-baa0-9db1b7c7cbab",
                    "sense_definition": "To cause a reaction or consequence, often a negative one such as criticism or a fee.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Using the minibar in a hotel room usually <w>attracts</w> additional charges.",
                            "target_lang_translations": {
                                "ru": "Использование мини-бара в номере отеля обычно <w>влечет</w> за собой дополнительные расходы."
                            }
                        },
                        {
                            "text": "The controversial decision <w>attracted</w> severe criticism from the public.",
                            "target_lang_translations": {
                                "ru": "Это спорное решение <w>вызвало</w> суровую критику со стороны общественности."
                            }
                        }
                    ],
                    "synonyms": [
                        "draw"
                    ],
                    "common_phrases": [
                        "attract criticism",
                        "attract charges",
                        "attract publicity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать реакцию или приводить к последствиям (часто негативным)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "провоцировать реакцию"
                            },
                            {
                                "target_lang_word": "навлекать",
                                "target_lang_sense_clarification": "неприятности или критику"
                            },
                            {
                                "target_lang_word": "влечь",
                                "target_lang_sense_clarification": "иметь следствием"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
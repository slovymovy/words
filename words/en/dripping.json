    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7cdc57dc-a001-3158-999a-ab4cd8caf963",
                    "sense_definition": "Present participle and gerund of <w>drip</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water was <w>dripping</w> from the ceiling onto the floor.",
                            "target_lang_translations": {
                                "ru": "С потолка на пол <w>капала</w> вода."
                            }
                        },
                        {
                            "text": "He came inside with rain <w>dripping</w> from his coat.",
                            "target_lang_translations": {
                                "ru": "Он вошел внутрь, и с его пальто <w>капал</w> дождь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола \"drip\" (капать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капающий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "капая",
                                "target_lang_sense_clarification": "деепричастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "edea48c5-0dd5-3ba7-971f-98df88be6111",
                    "sense_definition": "Extremely wet to the point that liquid is falling in drops.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should dry off because you are <w>dripping</w> wet.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит вытереться, потому что с тебя <w>капает</w>."
                            }
                        },
                        {
                            "text": "The <w>dripping</w> trees made the forest feel very cold and damp.",
                            "target_lang_translations": {
                                "ru": "<w>Капающие</w> деревья делали лес очень холодным и сырым."
                            }
                        }
                    ],
                    "synonyms": [
                        "soaking",
                        "drenched",
                        "saturated"
                    ],
                    "antonyms": [
                        "dry"
                    ],
                    "common_phrases": [
                        "dripping wet",
                        "dripping with sweat",
                        "dripping with blood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень мокрый, настолько, что с него стекает жидкость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промокший насквозь",
                                "target_lang_sense_clarification": "очень мокрый"
                            },
                            {
                                "target_lang_word": "стекающий",
                                "target_lang_sense_clarification": "о жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6becc1fd-6c02-3019-9485-fd5ebbedbc4a",
                    "sense_definition": "The sound or action of liquid falling in drops.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constant <w>dripping</w> of the faucet kept me awake all night.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>капанье</w> крана не давало мне спать всю ночь."
                            }
                        },
                        {
                            "text": "We need to fix the <w>dripping</w> coming from the roof.",
                            "target_lang_translations": {
                                "ru": "Нам нужно устранить <w>течь</w> с крыши."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук или действие падения жидкости каплями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капанье",
                                "target_lang_sense_clarification": "процесс или звук"
                            },
                            {
                                "target_lang_word": "капель",
                                "target_lang_sense_clarification": "звук падающих капель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8db7173-be0c-3c80-a999-1b2592144222",
                    "sense_definition": "Fat that has melted from meat during roasting, which becomes solid when cold and is used for cooking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother used to roast potatoes in beef <w>dripping</w> for extra flavor.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка жарила картошку на говяжьем <w>жире</w> для аромата."
                            }
                        },
                        {
                            "text": "He ate a slice of bread with <w>dripping</w> when there was no butter.",
                            "target_lang_translations": {
                                "ru": "Он съел кусок хлеба с <w>жиром</w>, когда не было масла."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bread and dripping",
                        "beef dripping"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жир, вытопившийся из мяса при жарке, который застывает при охлаждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жир",
                                "target_lang_sense_clarification": "кулинарный"
                            },
                            {
                                "target_lang_word": "топлёный жир",
                                "target_lang_sense_clarification": "вытопленный из мяса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d35197d-2ba8-351e-a433-ee570a10feef",
                    "sense_definition": "The method of applying e-liquid directly onto the heating element of an electronic cigarette.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some users prefer <w>dripping</w> because it produces a stronger flavor.",
                            "target_lang_translations": {
                                "ru": "Некоторые пользователи предпочитают <w>дриппинг</w>, потому что он дает более сильный вкус."
                            }
                        },
                        {
                            "text": "You have to be careful not to flood the device when <w>dripping</w>.",
                            "target_lang_translations": {
                                "ru": "Нужно быть осторожным, чтобы не залить устройство при <w>закапывании</w>."
                            }

                    "common_phrases": [
                        "Fox News",
                        "Fox Sports"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная телевизионная сеть и медиакомпания."
                    },
                    "translations": {
                        "ru": [
                            {

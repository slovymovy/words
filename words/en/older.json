                            },
                            {
                                "target_lang_word": "старший",
                                "target_lang_sense_clarification": "прилагательное (о родственниках)"
                            },
                            {
                                "target_lang_word": "старее",
                                "target_lang_sense_clarification": "сравнительная степень (обычно о предметах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57d9a6f8-97e4-3e05-83c9-f3013fb4e992",
                    "sense_definition": "Advanced in years; used to describe people who are aging, often as a polite alternative to old.",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4314d4c9-b19c-3847-b42f-98eecca9a28e",
                    "sense_definition": "Simple past tense of the verb <w>ride</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_ride_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>rode</w> her bicycle to school yesterday morning.",
                            "target_lang_translations": {
                                "ru": "Вчера утром она <w>ехала</w> в школу на велосипеде."
                            }
                        },
                        {
                            "text": "We <w>rode</w> the bus because it was raining outside.",
                            "target_lang_translations": {
                                "ru": "Мы <w>поехали</w> на автобусе, потому что на улице шел дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rode a bike",
                        "rode a horse",
                        "rode the bus",
                        "rode away"
                    ],
                    "target_lang_definitions": {

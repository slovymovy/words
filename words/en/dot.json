        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2893b28f-2b16-3c42-9c06-621fde7031b6",
                    "sense_definition": "To cover or mark a surface with small spots.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mark_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The painter used a brush to <w>dot</w> the canvas with red paint.",
                            "target_lang_translations": {
                                "ru": "Художник использовал кисть, чтобы <w>покрыть точками</w> красной краски холст."
                            }
                        },
                        {
                            "text": "Small islands <w>dot</w> the blue ocean.",
                            "target_lang_translations": {
                                "ru": "Маленькие острова <w>усеивают</w> синий океан."

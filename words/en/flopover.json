    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c41e616-f95e-3039-a389-de222f8629dc",
                    "sense_definition": "The action of bending or falling over loosely, or a part of something that hangs down.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain caused a <w>flopover</w> of the tall flowers in the garden.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь вызвал <w>полегание</w> высоких цветов в саду."
                            }
                        },
                        {
                            "text": "He tried to style the <w>flopover</w> of hair that kept falling into his eyes.",
                            "target_lang_translations": {
                                "ru": "Он пытался уложить <w>свисающую прядь</w> волос, которая постоянно лезла ему в глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "droop",
                        "slump"
                    ],
                    "common_phrases": [
                        "hair flopover",
                        "resist flopover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие свободного сгибания или падения, либо свисающая часть чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свисающая прядь",
                                "target_lang_sense_clarification": "о волосах"
                            },
                            {
                                "target_lang_word": "полегание",
                                "target_lang_sense_clarification": "о растениях"
                            },
                            {
                                "target_lang_word": "свисание",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "016dad31-6734-355e-b643-5c3720744191",
                    "sense_definition": "A sudden change from one position, opinion, or allegiance to another.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a sudden <w>flopover</w> in the committee's decision regarding the new policy.",
                            "target_lang_translations": {
                                "ru": "В решении комитета относительно новой политики произошла внезапная <w>перемена</w>."
                            }
                        },
                        {
                            "text": "Analysts predicted a <w>flopover</w> in voter sentiment just weeks before the election.",
                            "target_lang_translations": {
                                "ru": "Аналитики предсказывали <w>разворот</w> в настроениях избирателей всего за несколько недель до выборов."
                            }
                        }
                    ],
                    "synonyms": [
                        "reversal",
                        "flip-flop",
                        "shift"
                    ],
                    "common_phrases": [
                        "sudden flopover",
                        "policy flopover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапная смена позиции, мнения или стороны на противоположную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резкая перемена",
                                "target_lang_sense_clarification": "мнения или решения"
                            },
                            {
                                "target_lang_word": "разворот",
                                "target_lang_sense_clarification": "изменение курса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9639367f-7502-3ad4-afdd-588c603d3951",
                    "sense_definition": "A form of television signal distortion where the displayed image appears to scroll vertically rather than remain still.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to adjust the vertical hold knob to stop the picture <w>flopover</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно отрегулировать ручку кадровой синхронизации, чтобы остановить <w>срыв кадра</w>."
                            }
                        },
                        {
                            "text": "The old screen suffers from constant <w>flopover</w>, making it impossible to watch the show.",
                            "target_lang_translations": {
                                "ru": "Старый экран страдает от постоянной <w>вертикальной прокрутки</w>, из-за чего невозможно смотреть передачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertical roll"
                    ],
                    "common_phrases": [
                        "picture flopover",
                        "vertical flopover"

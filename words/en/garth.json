            "senses": [
                {
                    "sense_id": "b1699eb2-e963-3c2b-a36c-fa18aeca5c27",
                    "sense_definition": "A male given name, historically a shortened form of Gareth.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Garth</w> plays the guitar in a country band.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Гарт</w> играет на гитаре в кантри-группе."
                            }
                        },
                        {
                            "text": "We decided to name the baby <w>Garth</w> after his grandfather.",
                            "target_lang_translations": {
                                "ru": "Мы решили назвать ребенка <w>Гарт</w> в честь его дедушки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, исторически являющееся сокращенной формой имени Гарет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гарт",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]

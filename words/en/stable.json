                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ae4db583-f340-39fb-9f0a-4322590410df",
                    "sense_definition": "A building where horses are kept, fed, and cared for.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horses were led back to the <w>stable</w> for the night.",
                            "target_lang_translations": {
                                "ru": "Лошадей отвели обратно в <w>конюшню</w> на ночь."
                            }
                        },
                        {
                            "text": "She spends every weekend cleaning the <w>stable</w> and grooming the ponies.",
                            "target_lang_translations": {
                                "ru": "Она проводит каждые выходные, убирая <w>конюшню</w> и ухаживая за пони."
                            }
                        }
                    ],
                    "synonyms": [
                        "horsebarn"
                    ],
                    "common_phrases": [
                        "riding stable",
                        "livery stable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, где содержат, кормят и ухаживают за лошадьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конюшня",
                                "target_lang_sense_clarification": "помещение для лошадей"

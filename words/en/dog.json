    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "75e100ef-3b45-381e-9847-1842d8428b0a",
                    "sense_definition": "A domesticated carnivorous mammal that typically has a long snout, an acute sense of smell, and a barking voice.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "canine_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small <w>dog</w> barked happily when its owner returned home.",
                            "target_lang_translations": {
                                "ru": "Маленькая <w>собака</w> радостно залаяла, когда её хозяин вернулся домой."
                            }
                        },
                        {
                            "text": "She wants to adopt a <w>dog</w> from the local animal shelter.",
                            "target_lang_translations": {
                                "ru": "Она хочет взять <w>собаку</w> из местного приюта для животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "hound",
                        "pooch",
                        "canine"
                    ],
                    "common_phrases": [
                        "walk the dog",
                        "guide dog",
                        "stray dog",
                        "guard dog"
                    ],
                    "target_lang_definitions": {
                        "ru": "Домашнее плотоядное млекопитающее семейства псовых, обычно имеющее длинную морду, острый нюх и умеющее лаять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собака",
                                "target_lang_sense_clarification": "общее название животного"
                            },
                            {
                                "target_lang_word": "пёс",
                                "target_lang_sense_clarification": "часто о самце или разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55cf96c6-6f3e-3910-a0ac-4c04c0ae6733",
                    "sense_definition": "A sausage served in a sliced bun; a hot dog.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He bought a <w>dog</w> and a soda at the baseball game.",
                            "target_lang_translations": {
                                "ru": "Он купил <w>хот-дог</w> и газировку на бейсбольном матче."
                            }
                        },
                        {
                            "text": "I like to put plenty of mustard and onions on my <w>dog</w>.",
                            "target_lang_translations": {
                                "ru": "Я люблю класть побольше горчицы и лука в свой <w>хот-дог</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hot dog",
                        "chili dog"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for hot dog"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Горячая сосиска в разрезанной булочке; хот-дог."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хот-дог",
                                "target_lang_sense_clarification": "блюдо фастфуда"

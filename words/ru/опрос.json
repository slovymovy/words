    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e877e44-2b86-3465-b331-47c4316c9c44",
                    "sense_definition": "Проверка знаний учащихся преподавателем, обычно в устной форме.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель начал <опрос> по домашнему заданию в начале урока.",
                            "target_lang_translations": {
                                "en": "The teacher started the <quiz> on the homework at the beginning of the lesson."
                            }
                        },
                        {
                            "text": "Сегодня в классе был устный <опрос>, и я получил хорошую оценку.",
                            "target_lang_translations": {
                                "en": "Today there was an oral <quiz> in class, and I got a good grade."
                            }
                        }
                    ],
                    "common_phrases": [
                        "устный опрос",
                        "опрос по домашнему заданию",
                        "провести опрос"
                    ],
                    "target_lang_definitions": {
                        "en": "A check of students' knowledge by a teacher, usually in oral form."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quiz",
                                "target_lang_sense_clarification": "short assessment"
                            },
                            {
                                "target_lang_word": "oral questioning",
                                "target_lang_sense_clarification": "checking knowledge verbally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2af8051e-eef0-377a-a3ac-6e5ee950095a",
                    "sense_definition": "Метод сбора информации путём выяснения мнений множества людей по определённым вопросам.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы провели <опрос> среди студентов, чтобы узнать их интересы.",
                            "target_lang_translations": {
                                "en": "We conducted a <survey> among students to find out their interests."
                            }
                        },
                        {
                            "text": "Результаты <опроса> показали, что большинство жителей поддерживают этот проект.",
                            "target_lang_translations": {
                                "en": "The results of the <poll> showed that the majority of residents support this project."
                            }
                        }
                    ],
                    "synonyms": [
                        "анкетирование"
                    ],
                    "common_phrases": [
                        "опрос общественного мнения",
                        "социологический опрос",
                        "результаты опроса",
                        "пройти опрос"
                    ],
                    "target_lang_definitions": {
                        "en": "A method of collecting information by asking for the opinions of many people on specific issues."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "poll",
                                "target_lang_sense_clarification": "public opinion"
                            },
                            {
                                "target_lang_word": "survey",
                                "target_lang_sense_clarification": "data collection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f9b4892-76b6-36ec-aa42-f0cf30db7be5",
                    "sense_definition": "Официальная беседа с целью получения сведений от свидетелей или участников события.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция провела <опрос> свидетелей, чтобы выяснить детали происшествия.",
                            "target_lang_translations": {
                                "en": "The police conducted <questioning> of the witnesses to find out the details of the incident."
                            }
                        },
                        {
                            "text": "Тщательный <опрос> соседей помог найти владельца потерянной вещи.",
                            "target_lang_translations": {
                                "en": "Careful <questioning> of the neighbors helped find the owner of the lost item."
                            }
                        }
                    ],
                    "common_phrases": [
                        "опрос свидетелей",
                        "опрос потерпевших",
                        "протокол опроса"
                    ],
                    "target_lang_definitions": {
                        "en": "An official conversation to obtain information from witnesses or participants of an event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "questioning",
                                "target_lang_sense_clarification": "by police or officials"
                            },
                            {
                                "target_lang_word": "interview",
                                "target_lang_sense_clarification": "gathering witness statements"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "91b94caa-a318-3844-b323-296a08ac6fce",
                    "sense_definition": "Duże naturalne wzniesienie terenu, wyższe od pagórka, zazwyczaj o stromych zboczach i wyraźnym szczycie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W te wakacje planujemy pojechać w <w>góry</w>, żeby odpocząć od miasta.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем поехать в <w>горы</w>, чтобы отдохнуть от города."
                            }
                        },
                        {
                            "text": "Wspinaczka na tę wysoką <w>górę</w> zajęła nam cały dzień.",
                            "target_lang_translations": {
                                "ru": "Восхождение на эту высокую <w>гору</w> заняло у нас весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczyt",
                        "wierzch",
                        "wyniosłość"
                    ],
                    "antonyms": [
                        "dolina",
                        "nizina",
                        "dół"
                    ],
                    "common_phrases": [
                        "iść w góry",
                        "szczyt góry",
                        "łańcuch gór",
                        "wysokie góry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое естественное возвышение рельефа, выше холма, обычно с крутыми склонами и выраженной вершиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гора",
                                "target_lang_sense_clarification": "географический объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a725321-3ee2-38e9-bae7-d33b6ace0951",
                    "sense_definition": "Najwyżej położona część jakiegoś przedmiotu, obiektu lub strony.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę podpisać się u <w>góry</w> strony, a nie na dole.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подпишитесь <w>вверху</w> страницы, а не внизу."
                            }
                        },
                        {
                            "text": "<w>Góra</w> tego pudełka jest lekko wgnieciona.",
                            "target_lang_translations": {
                                "ru": "<w>Верх</w> этой коробки слегка помят."
                            }
                        }
                    ],
                    "synonyms": [
                        "wierzch",
                        "szczyt",
                        "początek"
                    ],
                    "antonyms": [
                        "dół",
                        "dolina",
                        "nizina"
                    ],
                    "common_phrases": [
                        "u góry",
                        "do góry nogami",
                        "z góry na dół",
                        "góra strony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая высокая часть какого-либо предмета, объекта или страницы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верх",
                                "target_lang_sense_clarification": "верхняя часть"
                            },
                            {
                                "target_lang_word": "верхушка",
                                "target_lang_sense_clarification": "самый верх"
                            }

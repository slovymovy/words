                {
                    "sense_id": "b58537d7-404d-3aa1-aaa8-05b47a795046",
                    "sense_definition": "pierwsza osoba liczby pojedynczej czasu teraźniejszego czasownika <w>mieć</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam na imię Anna i pochodzę z Krakowa.",
                            "target_lang_translations": {
                                "ru": "Меня <w>зовут</w> Анна, и я из Кракова."
                            }
                        },
                        {
                            "text": "Mam nadzieję, że jutro nie będzie padać deszcz.",
                            "target_lang_translations": {
                                "ru": "<w>Надеюсь</w>, что завтра не будет дождя."
                            }
                        },
                        {
                            "text": "Niestety nie mam teraz czasu na rozmowę.",
                            "target_lang_translations": {
                                "ru": "К сожалению, у меня сейчас <w>нет</w> времени на разговор."
                            }
                        }
                    ],
                    "synonyms": [
                        "posiadam"
                    ],
                    "common_phrases": [
                        "mam na imię",
                        "mam nadzieję",
                        "mam rację",
                        "mam dość",
                        "mam czas"
                    ],
                    "target_lang_definitions": {
                        "ru": "1-е лицо единственного числа настоящего времени глагола <w>иметь</w>"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "имею",
                                "target_lang_sense_clarification": "форма глагола 'иметь'"
                            },
                            {
                                "target_lang_word": "у меня есть",
                                "target_lang_sense_clarification": "конструкция обладания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9e22383-161a-3d50-8cff-ed5b983e1d67",
                    "sense_definition": "druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>mamić</w>",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam mnie wizją łatwego zarobku, bo w to nie wierzę.",
                            "target_lang_translations": {
                                "ru": "Не <w>мани</w> меня видением легкого заработка, потому что я в это не верю."
                            }
                        },
                        {
                            "text": "Nie mam ludzi obietnicami, których nie możesz spełnić.",
                            "target_lang_translations": {
                                "ru": "Не <w>обманывай</w> людей обещаниями, которые не можешь выполнить."
                            }
                        }
                    ],
                    "synonyms": [
                        "łudź",
                        "zwódź",
                        "kusi"
                    ],
                    "target_lang_definitions": {
                        "ru": "2-е лицо единственного числа повелительного наклонения глагола <w>манить</w> / <w>обманывать</w>"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мани",
                                "target_lang_sense_clarification": "привлекай, соблазняй"
                            },
                            {
                                "target_lang_word": "обманывай",
                                "target_lang_sense_clarification": "вводи в заблуждение"
                            }
                        ]
                    }
                }
            ]
        },
        {

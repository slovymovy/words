    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8cca9297-da14-3932-94bf-5265b86c3d00",
                    "sense_definition": "Ostre lub najwyżej położone zakończenie jakiegoś przedmiotu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tip_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poczuł chłód na samym <w>czubku</w> nosa, gdy wyszedł na mróz.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал холод на самом <w>кончике</w> носа, когда вышел на мороз."
                            }
                        },
                        {
                            "text": "Mam to słowo na <w>czubku</w> języka, ale nie mogę sobie przypomnieć.",
                            "target_lang_translations": {
                                "ru": "Это слово вертится у меня на <w>кончике</w> языка, но я не могу вспомнить."
                            }
                        },
                        {
                            "text": "Dziecko stanęło na <w>czubkach</w> palców, aby zobaczyć, co jest na stole.",
                            "target_lang_translations": {
                                "ru": "Ребёнок встал на <w>цыпочки</w>, чтобы увидеть, что на столе."
                            }
                        }
                    ],
                    "antonyms": [
                        "spód",
                        "dół",
                        "podstawa"
                    ],
                    "common_phrases": [
                        "czubek nosa",
                        "czubek języka",
                        "czubek góry",
                        "na czubkach palców"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый или самый верхний конец какого-либо предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кончик",
                                "target_lang_sense_clarification": "острая или крайняя часть (носа, языка, пальца)"
                            },
                            {
                                "target_lang_word": "верхушка",
                                "target_lang_sense_clarification": "самая верхняя часть (дерева, горы)"
                            },
                            {
                                "target_lang_word": "макушка",
                                "target_lang_sense_clarification": "верхняя часть головы или дерева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fd2949b-217a-3071-97ea-4e190f699087",
                    "sense_definition": "Potoczne, często lekceważące określenie osoby zachowującej się nieracjonalnie, dziwnie lub chorej psychicznie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "crazy_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten kierowca jeździ jak jakiś <w>czubek</w>, stwarzając zagrożenie na drodze.",
                            "target_lang_translations": {
                                "ru": "Этот водитель ездит как какой-то <w>псих</w>, создавая опасность на дороге."
                            }
                        },
                        {
                            "text": "Sąsiedzi uważają go za <w>czubka</w>, ponieważ krzyczy w nocy bez powodu.",
                            "target_lang_translations": {
                                "ru": "Соседи считают его <w>чокнутым</w>, так как он кричит по ночам без причины."
                            }
                        }
                    ],
                    "synonyms": [
                        "wariat",
                        "świr",
                        "psychol",
                        "czub",
                        "szajbus",
                        "oszołom",
                        "pomyleniec",
                        "pomylony",
                        "świrus",
                        "nienormalny"
                    ],
                    "antonyms": [
                        "normalny"
                    ],
                    "common_phrases": [
                        "kompletny czubek",
                        "zachowywać się jak czubek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej, może być obraźliwe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто пренебрежительное название человека, ведущего себя нерационально, странно или психически больного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "псих",
                                "target_lang_sense_clarification": "сумасшедший, неуравновешенный человек"
                            },
                            {
                                "target_lang_word": "чокнутый",
                                "target_lang_sense_clarification": "странный, ненормальный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "498e46f3-2b99-3421-aea2-43d9703cc265",
                    "sense_definition": "Potoczne określenie zakonnika ze zgromadzenia bonifratrów, zajmującego się opieką nad chorymi.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "monk_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnym szpitalu pracowali <w>czubkowie</w>, którzy słynęli z ziołolecznictwa.",
                            "target_lang_translations": {
                                "ru": "В старой больнице работали <w>бонифратры</w>, которые славились траволечением."
                            }
                        },
                        {
                            "text": "Spotkałem zakonnika, którego miejscowi nazywali <w>czubkiem</w> ze względu na kaptur.",
                            "target_lang_translations": {
                                "ru": "Я встретил монаха, которого местные называли <w>бонифратром</w> из-за капюшона."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "nazwa historyczna lub środowiskowa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название монаха из ордена бонифратров, занимающегося уходом за больными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бонифратр",
                                "target_lang_sense_clarification": "монах ордена бонифратров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f7c31f5-05d2-3f83-ae4b-6c7c1879bf68",
                    "sense_definition": "Rodzaj mchu należący do gromady wątrobowców.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas badań terenowych studenci biologii znaleźli rzadki okaz <w>czubka</w>.",
                            "target_lang_translations": {
                                "ru": "Во время полевых исследований студенты-биологи нашли редкий экземпляр <w>лофозии</w>."
                            }
                        },
                        {
                            "text": "<w>Czubek</w> jest rośliną preferującą wilgotne i zacienione stanowiska.",
                            "target_lang_translations": {
                                "ru": "<w>Лофозия</w> — это растение, предпочитающее влажные и тенистые места."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Род мха, относящийся к отделу печёночников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лофозия",
                                "target_lang_sense_clarification": "ботаническое название"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8fddb8a0-7eda-3b46-b4c5-61e57ca9c3b1",
                    "sense_definition": "Om bang van te worden; angstaanjagend of zeer zorgwekkend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fear_alarm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het rapport schetst een <w>schrikbarend</w> beeld van de huidige situatie.",
                            "target_lang_translations": {
                                "en": "The report paints an <w>alarming</w> picture of the current situation.",
                                "ru": "Отчет рисует <w>ужасающую</w> картину текущей ситуации."
                            }
                        },
                        {
                            "text": "De toename van het aantal inbraken in de wijk is <w>schrikbarend</w>.",
                            "target_lang_translations": {
                                "en": "The increase in the number of burglaries in the neighborhood is <w>alarming</w>.",
                                "ru": "Рост числа краж со взломом в районе <w>ужасающий</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "angstaanjagend",
                        "alarmerend",
                        "onrustbarend",
                        "vreselijk"
                    ],
                    "antonyms": [
                        "geruststellend"
                    ],
                    "common_phrases": [
                        "een schrikbarend tekort",
                        "schrikbarende cijfers",
                        "schrikbarende gevolgen"
                    ],
                    "target_lang_definitions": {
                        "en": "Causing fear; frightening or very worrying.",
                        "ru": "Вызывающий страх, ужас или серьезное беспокойство."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "alarming",
                                "target_lang_sense_clarification": "causing worry or fear"
                            },
                            {
                                "target_lang_word": "frightening",
                                "target_lang_sense_clarification": "causing fear"
                            },
                            {
                                "target_lang_word": "shocking",
                                "target_lang_sense_clarification": "causing intense surprise or dismay"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ужасающий",
                                "target_lang_sense_clarification": "вызывающий ужас или сильную тревогу"
                            },
                            {
                                "target_lang_word": "пугающий",
                                "target_lang_sense_clarification": "вызывающий страх"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c922284a-6d4c-306a-b254-11eaa9ace1ee",
                    "sense_definition": "In zeer hoge of verontrustende mate; gebruikt om een negatieve eigenschap te versterken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De prijzen van woningen zijn de laatste jaren <w>schrikbarend</w> gestegen.",
                            "target_lang_translations": {
                                "en": "Housing prices have risen <w>alarmingly</w> in recent years.",
                                "ru": "Цены на жилье за последние годы <w>пугающе</w> выросли."
                            }
                        },
                        {
                            "text": "Het is <w>schrikbarend</w> duur om in het centrum te parkeren.",
                            "target_lang_translations": {
                                "en": "It is <w>shockingly</w> expensive to park in the center.",
                                "ru": "Парковаться в центре <w>ужасающе</w> дорого."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontzettend",
                        "enorm",
                        "buitengewoon",
                        "vreselijk"
                    ],
                    "antonyms": [
                        "nauwelijks"
                    ],
                    "common_phrases": [
                        "schrikbarend duur",
                        "schrikbarend hoog",
                        "schrikbarend laag",
                        "schrikbarend slecht"
                    ],
                    "target_lang_definitions": {
                        "en": "To a very high or disturbing degree; used to intensify a negative quality.",
                        "ru": "В чрезвычайно высокой или тревожной степени; используется для усиления негативного качества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "alarmingly",
                                "target_lang_sense_clarification": "in a way that causes worry"
                            },
                            {
                                "target_lang_word": "shockingly",
                                "target_lang_sense_clarification": "to a surprising or disturbing extent"
                            },
                            {
                                "target_lang_word": "frighteningly",
                                "target_lang_sense_clarification": "to a degree that causes fear"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ужасающе",

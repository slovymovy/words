                {
                    "sense_id": "7bda944e-8434-3db6-8376-b8a463dc0fe6",
                    "sense_definition": "Financiële kosten vergoeden of bescherming bieden tegen financiële risico's.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verzekering zal de schade aan de auto volledig <w>dekken</w>.",
                            "target_lang_translations": {

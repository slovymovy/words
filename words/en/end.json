                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зака́нчивать",
                                "target_lang_sense_clarification": "доводить до конца"
                            },
                            {
                                "target_lang_word": "прекраща́ть",
                                "target_lang_sense_clarification": "останавливать, переставать делать"
                            },
                            {
                                "target_lang_word": "заверша́ть",
                                "target_lang_sense_clarification": "оканчивать"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5984982-be10-3e91-9ba9-3267085f645f",
                    "sense_definition": "Получить деньги или иное вознаграждение за выполненную работу или труд.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <заработать> денег, чтобы купить новый компьютер.",
                            "target_lang_translations": {
                                "en": "I want to <earn> money to buy a new computer."
                            }
                        },
                        {
                            "text": "Студенты пытаются <заработать> на жизнь во время летних каникул.",
                            "target_lang_translations": {
                                "en": "Students try to <earn> a living during summer break."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заработать денег",
                        "заработать на жизнь",
                        "заработать миллион",
                        "честно заработать"
                    ],
                    "target_lang_definitions": {
                        "en": "To receive money or other remuneration for work performed or labor."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earn",
                                "target_lang_sense_clarification": "money"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3d14799-3789-3133-9c7d-470f091b1aff",
                    "sense_definition": "Начать действовать, функционировать или выполнять свои функции (о механизмах, устройствах, органах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "inception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгого ремонта двигатель машины наконец-то <заработал>.",
                            "target_lang_translations": {
                                "en": "After a long repair, the car engine finally <started working>."
                            }
                        },
                        {
                            "text": "Внезапно <заработала> громкая сигнализация, и все испугались.",
                            "target_lang_translations": {
                                "en": "Suddenly a loud alarm <went off>, and everyone got scared."
                            }
                        }
                    ],
                    "antonyms": [
                        "остановиться"
                    ],
                    "common_phrases": [
                        "мотор заработал",
                        "сердце заработало",
                        "система заработала"
                    ],
                    "target_lang_definitions": {
                        "en": "To begin to act, function, or perform its functions (about mechanisms, devices, organs)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start working",
                                "target_lang_sense_clarification": "mechanism"
                            },
                            {
                                "target_lang_word": "begin to work",
                                "target_lang_sense_clarification": "mechanism"
                            },
                            {
                                "target_lang_word": "start up",
                                "target_lang_sense_clarification": "engine/motor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3049c0e-8157-37ce-be6f-73f9c7ab02ad",
                    "sense_definition": "Добиться чего-либо или получить что-либо (обычно нематериальное) в результате своих усилий или действий.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему удалось <заработать> уважение коллег своим упорным трудом.",
                            "target_lang_translations": {
                                "en": "He managed to <earn> the respect of his colleagues through his hard work."
                            }
                        },
                        {
                            "text": "Эта команда смогла <заработать> право на участие в финале.",
                            "target_lang_translations": {
                                "en": "This team was able to <earn> the right to participate in the final."
                            }
                        }
                    ],
                    "synonyms": [
                        "приобрести"
                    ],
                    "common_phrases": [
                        "заработать уважение",
                        "заработать репутацию",
                        "заработать авторитет",
                        "заработать право"
                    ],
                    "target_lang_definitions": {
                        "en": "To achieve or obtain something (usually intangible) as a result of one's efforts or actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earn",
                                "target_lang_sense_clarification": "respect/reputation"
                            },
                            {
                                "target_lang_word": "gain",
                                "target_lang_sense_clarification": "trust/experience"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "523e113d-7d55-34c9-9c7b-173e39abde91",
                    "sense_definition": "Получить что-либо неприятное (болезнь, наказание) как следствие своих поступков или неосторожности.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если ты будешь гулять без шапки зимой, можешь <заработать> простуду.",
                            "target_lang_translations": {
                                "en": "If you walk without a hat in winter, you can <catch> a cold."
                            }
                        },
                        {
                            "text": "За опоздание он умудрился <заработать> строгий выговор от начальника.",
                            "target_lang_translations": {
                                "en": "For being late, he managed to <earn> a severe reprimand from the boss."
                            }
                        }
                    ],
                    "synonyms": [
                        "схлопотать"
                    ],
                    "common_phrases": [
                        "заработать простуду",
                        "заработать выговор",
                        "заработать синяк"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To receive something unpleasant (illness, punishment) as a consequence of one's actions or carelessness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "illness"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "something unpleasant"
                            },
                            {
                                "target_lang_word": "earn",
                                "target_lang_sense_clarification": "punishment (ironic)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

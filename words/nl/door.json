                            {
                                "target_lang_word": "throughout",
                                "target_lang_sense_clarification": "in every part of a period"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5ab3469-fbaf-34ee-9bcc-598b9eb5d706",

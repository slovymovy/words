    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de057e4b-1599-3a29-a7df-e30f1c38db13",
                    "sense_definition": "Zdobyć lub otrzymać z powrotem coś, co się wcześniej miało i utraciło (np. przedmiot, zdrowie, wolność).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "regaining_possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wypadku musiał długo ćwiczyć, aby <odzyskać> pełną sprawność fizyczną.",
                            "target_lang_translations": {
                                "ru": "После аварии ему пришлось долго тренироваться, чтобы <восстановить> полную физическую форму."
                            }
                        },
                        {
                            "text": "Udało nam się <odzyskać> skradziony samochód dzięki szybkiej reakcji policji.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <вернуть> украденную машину благодаря быстрой реакции полиции."
                            }
                        },
                        {
                            "text": "Pacjent leżał w śpiączce przez tydzień, zanim w końcu <odzyskał> przytomność.",
                            "target_lang_translations": {
                                "ru": "Пациент лежал в коме неделю, прежде чем наконец <пришел в> сознание."
                            }
                        }
                    ],
                    "synonyms": [
                        "odebrać",
                        "odnaleźć"
                    ],
                    "antonyms": [
                        "stracić",
                        "utracić",
                        "zgubić"
                    ],
                    "common_phrases": [
                        "odzyskać przytomność",
                        "odzyskać siły",
                        "odzyskać pieniądze",
                        "odzyskać wolność",
                        "odzyskać równowagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить обратно то, что было утрачено или потеряно (например, предмет, здоровье, свободу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "получить обратно (предмет, деньги, свободу)"
                            },
                            {
                                "target_lang_word": "восстановить",
                                "target_lang_sense_clarification": "привести в прежнее состояние (здоровье, силы, равновесие)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "434948ab-6324-310c-9bb2-57e9c37d287e",
                    "sense_definition": "Sprawić, że dana osoba wróci do nas lub ponownie nawiąże z nami bliską relację.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "regaining_relationships",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek kupił kwiaty i przeprosił, ponieważ chciał <odzyskać> serce swojej dziewczyny.",
                            "target_lang_translations": {
                                "ru": "Марек купил цветы и извинился, так как хотел <вернуть> сердце своей девушки."
                            }
                        },
                        {
                            "text": "Bardzo trudno jest <odzyskać> zaufanie przyjaciela, gdy raz się go okłamało.",
                            "target_lang_translations": {
                                "ru": "Очень трудно <вернуть> доверие друга, если однажды солгал ему."
                            }
                        }
                    ],
                    "antonyms": [
                        "zrazić"
                    ],
                    "common_phrases": [
                        "odzyskać zaufanie",
                        "odzyskać miłość",
                        "odzyskać rodzinę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снова добиться чьего-то расположения, любви или восстановить отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "добиться возвращения человека или чувства"
                            },
                            {
                                "target_lang_word": "восстановить",
                                "target_lang_sense_clarification": "возобновить (доверие, отношения)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

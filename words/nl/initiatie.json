            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa4c1ca2-9795-3e4a-9b04-ad9e0ee19e32",
                    "sense_definition": "Een reeks lessen of een korte cursus bedoeld als eerste kennismaking met een vaardigheid, sport of kunstvorm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sportclub organiseert zaterdag een gratis <w>initiatie</w> tennis voor beginners.",
                            "target_lang_translations": {
                                "ru": "Спортивный клуб организует в субботу бесплатное <w>ознакомительное занятие</w> по теннису для начинающих."
                            }
                        },
                        {
                            "text": "Tijdens de <w>initiatie</w> leerden we de basisstappen van de salsa.",
                            "target_lang_translations": {
                                "ru": "Во время <w>вводного урока</w> мы выучили основные шаги сальсы."
                            }
                        }
                    ],
                    "synonyms": [
                        "introductie",
                        "initiatiecursus"
                    ],
                    "common_phrases": [
                        "gratis initiatie",
                        "initiatie volgen",
                        "korte initiatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия уроков или краткий курс, предназначенный для первого знакомства с навыком, видом спорта или искусства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ознакомительное занятие",
                                "target_lang_sense_clarification": "вводный урок"
                            },
                            {
                                "target_lang_word": "вводный курс",
                                "target_lang_sense_clarification": "серия уроков"
                            },
                            {
                                "target_lang_word": "мастер-класс",
                                "target_lang_sense_clarification": "практическое занятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67df401c-5e6f-3bdc-8bbd-3c8165ebb830",
                    "sense_definition": "De plechtige handeling of ceremonie waarbij iemand officieel wordt toegelaten tot een groep of gemeenschap.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "ritual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>initiatie</w> van de nieuwe leden ging gepaard met oude rituelen.",
                            "target_lang_translations": {
                                "ru": "<w>Посвящение</w> новых членов сопровождалось древними ритуалами."
                            }
                        },
                        {
                            "text": "In sommige culturen markeert de <w>initiatie</w> de overgang van kind naar volwassene.",
                            "target_lang_translations": {
                                "ru": "В некоторых культурах <w>инициация</w> знаменует переход от детства к взрослой жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "inwijding"
                    ],
                    "common_phrases": [
                        "rituele initiatie",
                        "initiatie tot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественный акт или церемония официального принятия кого-либо в группу или сообщество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящение",
                                "target_lang_sense_clarification": "обряд принятия"
                            },
                            {
                                "target_lang_word": "инициация",
                                "target_lang_sense_clarification": "антропологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee14b155-e1c1-3940-9409-8ff4755b91d1",
                    "sense_definition": "Het in gang zetten of starten van een proces, vaak gebruikt in een wetenschappelijke of technische context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor de <w>initiatie</w> van de chemische reactie is een katalysator nodig.",
                            "target_lang_translations": {
                                "ru": "Для <w>инициирования</w> химической реакции необходим катализатор."
                            }
                        },
                        {
                            "text": "De <w>initiatie</w> van het project liep vertraging op door een gebrek aan middelen.",
                            "target_lang_translations": {
                                "ru": "<w>Запуск</w> проекта задержался из-за нехватки средств."
                            }
                        }
                    ],

{
    "word_family": [
        "umbrellalike",
        "subumbrella"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "55e16c13-e9d9-3063-86f1-24b011042661",
                    "sense_definition": "A cloth-covered frame used for protection against rain or sun.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She opened her <w>umbrella</w> as soon as the rain started.",
                            "target_lang_translations": {
                                "ru": "Она раскрыла свой <w>зонт</w>, как только начался дождь."
                            }
                        },
                        {
                            "text": "We sat under a large beach <w>umbrella</w> to stay out of the sun.",
                            "target_lang_translations": {
                                "ru": "Мы сидели под большим пляжным <w>зонтом</w>, чтобы укрыться от солнца."
                            }
                        }
                    ],
                    "synonyms": [
                        "bumbershoot",
                        "gamp"
                    ],
                    "common_phrases": [
                        "beach umbrella",
                        "open an umbrella",
                        "close an umbrella",
                        "under an umbrella"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каркас, обтянутый тканью, используемый для защиты от дождя или солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зонт",
                                "target_lang_sense_clarification": "стандартное название"
                            },
                            {
                                "target_lang_word": "зонтик",
                                "target_lang_sense_clarification": "разговорное или уменьшительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d51ac241-858c-361a-af88-897d14cdb3a9",
                    "sense_definition": "Anything that provides similar overarching coverage of a range of concepts, purposes, or groups.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marketing is often used as an <w>umbrella</w> term for sales and advertising.",
                            "target_lang_translations": {
                                "ru": "Маркетинг часто используется как <w>обобщающий</w> термин для продаж и рекламы."
                            }
                        },
                        {
                            "text": "Several small charities operate under the <w>umbrella</w> of the main foundation.",
                            "target_lang_translations": {
                                "ru": "Несколько небольших благотворительных организаций работают под <w>эгидой</w> главного фонда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "umbrella term",
                        "umbrella organization",
                        "under the umbrella"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-либо, объединяющее или охватывающее ряд понятий, целей или групп."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эгида",
                                "target_lang_sense_clarification": "в значении защиты или покровительства"
                            },
                            {
                                "target_lang_word": "обобщающее понятие",
                                "target_lang_sense_clarification": "для терминов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fff016f-c91f-329a-ba92-b7e8d34a5f8d",
                    "sense_definition": "An umbrella-shaped reflector with a white or silvery inner surface, used to diffuse a nearby light.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The photographer set up a white <w>umbrella</w> to soften the flash.",
                            "target_lang_translations": {
                                "ru": "Фотограф установил белый <w>зонт</w>, чтобы смягчить свет вспышки."
                            }
                        },
                        {
                            "text": "Using a reflective <w>umbrella</w> helps to eliminate harsh shadows in portraits.",
                            "target_lang_translations": {
                                "ru": "Использование отражающего <w>зонта</w> помогает устранить резкие тени на портретах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flash umbrella",
                        "reflective umbrella"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отражатель в форме зонта с белой или серебристой внутренней поверхностью, используемый для рассеивания света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фотозонт",
                                "target_lang_sense_clarification": "профессиональный термин"
                            },
                            {
                                "target_lang_word": "зонт",
                                "target_lang_sense_clarification": "краткая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a09ed56-baf9-37a4-b116-4d11034ea402",
                    "sense_definition": "The main body of a jellyfish, excluding the tentacles.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>umbrella</w> of the jellyfish pulses rhythmically to propel it through the water.",
                            "target_lang_translations": {
                                "ru": "<w>Купол</w> медузы ритмично пульсирует, продвигая её сквозь толщу воды."
                            }
                        },
                        {
                            "text": "Scientists measured the diameter of the specimen's <w>umbrella</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые измерили диаметр <w>купола</w> образца."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная часть тела медузы, исключая щупальца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купол",
                                "target_lang_sense_clarification": "анатомия медузы"
                            },
                            {
                                "target_lang_word": "зонтик",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8006d482-162f-3c80-9ce2-f1e56fa86d58",
                    "sense_definition": "To cover or protect, as if by an umbrella.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Large oak trees <w>umbrellaed</w> the quiet street, providing cool shade.",
                            "target_lang_translations": {
                                "ru": "Большие дубы <w>осеняли</w> тихую улицу, создавая прохладную тень."
                            }
                        },
                        {
                            "text": "The mother bird <w>umbrellaed</w> her chicks with her wings to keep them dry.",
                            "target_lang_translations": {
                                "ru": "Птица-мать <w>укрыла</w> птенцов крыльями, чтобы они оставались сухими."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Накрывать или защищать сверху, подобно зонту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрывать",
                                "target_lang_sense_clarification": "защищать"
                            },
                            {
                                "target_lang_word": "осенять",
                                "target_lang_sense_clarification": "поэтическое, о деревьях или крыльях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
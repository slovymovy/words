            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5362893-7c68-30aa-895a-af631df21651",
                    "sense_definition": "De hulp of steun van iemand of een instantie vragen om een probleem op te lossen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij hadden de hulp van de NAVO <w>ingeroepen</w> om de vrede te bewaren.",
                            "target_lang_translations": {
                                "ru": "Они <w>призвали</w> на помощь НАТО, чтобы сохранить мир."
                            }
                        },
                        {
                            "text": "Bij ernstige medische problemen moet je direct een arts <w>inroepen</w>.",
                            "target_lang_translations": {
                                "ru": "При серьезных медицинских проблемах нужно немедленно <w>вызвать</w> врача."
                            }
                        }
                    ],
                    "synonyms": [
                        "inschakelen",
                        "erbij halen",
                        "ontbieden"
                    ],
                    "antonyms": [
                        "wegsturen"
                    ],
                    "common_phrases": [
                        "hulp inroepen",
                        "bijstand inroepen",
                        "de politie inroepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Просить помощи или поддержки у кого-либо или у организации для решения проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призывать",
                                "target_lang_sense_clarification": "звать на помощь"
                            },
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "приглашать специалиста (напр. врача)"
                            },
                            {
                                "target_lang_word": "запрашивать",
                                "target_lang_sense_clarification": "просить официальной поддержки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c01e5eb5-e658-3498-b111-5bcd130e7348",
                    "sense_definition": "Een recht, wet, excuus of autoriteit noemen om jezelf te verdedigen of een standpunt te onderbouwen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De advocaat besloot verzachtende omstandigheden <w>in</w> te <w>roepen</w> voor zijn cliënt.",
                            "target_lang_translations": {
                                "ru": "Адвокат решил <w>сослаться</w> на смягчающие обстоятельства в защиту своего клиента."
                            }
                        },
                        {
                            "text": "Je kunt niet zomaar overmacht <w>inroepen</w> als je je huiswerk bent vergeten.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто так <w>сослаться</w> на форс-мажор, если ты забыл сделать домашнее задание."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanvoeren",
                        "zich beroepen op",
                        "aanhalen"
                    ],
                    "common_phrases": [
                        "overmacht inroepen",
                        "een recht inroepen",
                        "verzachtende omstandigheden inroepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ссылаться на право, закон или обстоятельство для защиты или обоснования позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссылаться",
                                "target_lang_sense_clarification": "приводить в качестве довода или оправдания"
                            },
                            {
                                "target_lang_word": "апеллировать",
                                "target_lang_sense_clarification": "обращаться к закону или авторитету"
                            }
                        ]
                    }
                }
            ]
        }
    ]

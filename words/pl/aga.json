{
    "word_family": [
        "Agusia"
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "fe010897-546d-3f65-8eb0-4943bfa43e26",
                    "sense_definition": "Zdrobnienie od imion żeńskich <w>Agata</w> lub <w>Agnieszka</w>, używane w sytuacjach nieoficjalnych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "diminutive_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja siostra <w>Aga</w> mieszka w Warszawie.",
                            "target_lang_translations": {
                                "ru": "Моя сестра <w>Ага</w> живет в Варшаве."
                            }
                        },
                        {
                            "text": "Cześć, jestem <w>Aga</w>, miło mi cię poznać.",
                            "target_lang_translations": {
                                "ru": "Привет, я <w>Ага</w>, приятно познакомиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "Agusia"
                    ],
                    "common_phrases": [
                        "ciocia Aga",
                        "moja Aga"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common short form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женских имен Агата или Агнешка, используемая в неофициальной обстановке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ага",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e6e0c39-2f89-3627-a4f0-9ec6912dec2b",
                    "sense_definition": "Rzeka w azjatyckiej części Rosji, na Zabajkalu, będąca lewym dopływem Ononu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Rzeka <w>Aga</w> przepływa przez malownicze tereny Zabajkala.",
                            "target_lang_translations": {
                                "ru": "Река <w>Ага</w> протекает через живописные территории Забайкалья."
                            }
                        },
                        {
                            "text": "Nigdy nie słyszałem o rzece o nazwie <w>Aga</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда не слышал о реке под названием <w>Ага</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Река в азиатской части России, в Забайкалье, являющаяся левым притоком Онона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ага",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1b3d3c4-d548-3864-a1c1-2fbdabd9d4e9",
                    "sense_definition": "Historyczny tytuł oficerski lub urzędniczy w Turcji i krajach muzułmańskich; także tytuł grzecznościowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki <w>aga</w> wydał rozkaz swoim żołnierzom.",
                            "target_lang_translations": {
                                "ru": "Великий <w>ага</w> отдал приказ своим солдатам."
                            }
                        },
                        {
                            "text": "W dawnej Turcji <w>aga</w> cieszył się dużym szacunkiem społecznym.",
                            "target_lang_translations": {
                                "ru": "В древней Турции <w>ага</w> пользовался большим общественным уважением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aga janczarów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исторический офицерский или чиновничий титул в Турции и мусульманских странах; также форма вежливого обращения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cf04be9-7ab8-366e-95df-347a0674c0fe",
                    "sense_definition": "Gatunek dużego płaza z rodziny ropuchowatych (Rhinella marina), znany z silnego jadu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ropucha <w>aga</w> została sprowadzona do Australii, gdzie stała się szkodnikiem.",
                            "target_lang_translations": {
                                "ru": "Жаба <w>ага</w> была завезена в Австралию, где стала вредителем."
                            }
                        },
                        {
                            "text": "Jad, który wydziela <w>aga</w>, może być niebezpieczny dla psów.",
                            "target_lang_translations": {
                                "ru": "Яд, который выделяет <w>ага</w>, может быть опасен для собак."
                            }
                        }
                    ],
                    "synonyms": [
                        "kururu",
                        "ropucha olbrzymia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид крупного земноводного из семейства жаб (Rhinella marina), известный своим сильным ядом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "жаба"
                            },
                            {
                                "target_lang_word": "жаба-ага",
                                "target_lang_sense_clarification": "вид земноводного"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
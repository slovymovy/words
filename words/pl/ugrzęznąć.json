{
    "word_family": [
        "grzęznąć",
        "grząski",
        "grzęzawisko",
        "grząsko"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "05475dbb-639c-3839-b221-b5e0cee606ad",
                    "sense_definition": "zapaść się w czymś miękkim, sypkim lub grząskim (np. w błocie, piasku, śniegu) i nie móc się wydostać",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_stuck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas leśnej wycieczki nasz samochód terenowy <ugrzęznął> w głębokim błocie.",
                            "target_lang_translations": {
                                "ru": "Во время лесной поездки наш внедорожник <увяз> в глубокой грязи."
                            }
                        },
                        {
                            "text": "Duży statek handlowy zboczył z kursu i <ugrzęznął> na mieliźnie niedaleko portu.",
                            "target_lang_translations": {
                                "ru": "Большое торговое судно сбилось с курса и <застряло> на мели недалеко от порта."
                            }
                        }
                    ],
                    "synonyms": [
                        "ugrząźć"
                    ],
                    "common_phrases": [
                        "ugrzęznąć w błocie",
                        "ugrzęznąć na mieliźnie",
                        "ugrzęznąć w zaspie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провалиться во что-то мягкое, сыпучее или вязкое (например, в грязь, песок, снег) и не иметь возможности выбраться"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увязнуть",
                                "target_lang_sense_clarification": "в грязи, болоте или снегу"
                            },
                            {
                                "target_lang_word": "застрять",
                                "target_lang_sense_clarification": "общее значение (напр. машина в песке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0cbf793-958c-3260-a7ae-8278bd398d35",
                    "sense_definition": "zostać zablokowanym w jakimś miejscu, obiekcie lub sytuacji, nie mogąc się swobodnie przemieścić",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_stuck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj po południu <ugrzęzłem> w korku w centrum miasta na ponad godzinę.",
                            "target_lang_translations": {
                                "ru": "Вчера днём я <застрял> в пробке в центре города более чем на час."
                            }
                        },
                        {
                            "text": "Arkusz papieru <ugrzęznął> wewnątrz starej drukarki i zablokował całe urządzenie.",
                            "target_lang_translations": {
                                "ru": "Лист бумаги <застрял> внутри старого принтера и заблокировал всё устройство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ugrzęznąć w korku",
                        "ugrzęznąć w windzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказаться заблокированным в каком-либо месте, объекте или ситуации, не имея возможности свободно перемещаться"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застрять",
                                "target_lang_sense_clarification": "в пробке, лифте или механизме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c2e1859-c74c-36c8-a1ea-f5aa402bc604",
                    "sense_definition": "znaleźć się w trudnej sytuacji, problemach lub zawiłościach, które uniemożliwiają dalsze działanie lub postęp",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_stuck",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ważne negocjacje handlowe <ugrzęzły> w martwym punkcie z powodu braku kompromisu.",
                            "target_lang_translations": {
                                "ru": "Важные торговые переговоры <зашли> в тупик из-за отсутствия компромисса."
                            }
                        },
                        {
                            "text": "Młody przedsiębiorca szybko <ugrzęznął> w długach przez nieprzemyślane inwestycje.",
                            "target_lang_translations": {
                                "ru": "Молодой предприниматель быстро <погряз> в долгах из-за необдуманных инвестиций."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ugrzęznąć w martwym punkcie",
                        "ugrzęznąć w długach",
                        "ugrzęznąć w szczegółach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказаться в сложной ситуации, проблемах или запутанных обстоятельствах, препятствующих дальнейшим действиям"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погрязнуть",
                                "target_lang_sense_clarification": "в долгах или пороках"
                            },
                            {
                                "target_lang_word": "завязнуть",
                                "target_lang_sense_clarification": "в деталях, делах или переговорах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
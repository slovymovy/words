    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "23153a8d-17cf-38d0-8777-c5625c97092b",
                    "sense_definition": "Of the highest rank, position, or importance.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is widely recognized as the <w>foremost</w> expert in her field of study.",
                            "target_lang_translations": {
                                "ru": "Она широко признана как <w>ведущий</w> эксперт в своей области исследований."
                            }
                        },
                        {
                            "text": "Our <w>foremost</w> priority is ensuring the safety and well-being of all students.",
                            "target_lang_translations": {
                                "ru": "Наш <w>главный</w> приоритет — обеспечение безопасности и благополучия всех студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "leading",
                        "principal",
                        "primary",
                        "top",
                        "greatest",
                        "paramount"
                    ],
                    "common_phrases": [
                        "foremost authority",
                        "foremost expert",
                        "world's foremost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый важный по званию, положению или значимости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведущий",
                                "target_lang_sense_clarification": "находящийся впереди, возглавляющий что-либо"
                            },
                            {
                                "target_lang_word": "главный",
                                "target_lang_sense_clarification": "основной, наиболее существенный"
                            },
                            {
                                "target_lang_word": "выдающийся",
                                "target_lang_sense_clarification": "выделяющийся среди других своими качествами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41487582-c299-39da-b65e-879f077fc79c",
                    "sense_definition": "Positioned in front of all others or at the most forward point.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>foremost</w> runners in the marathon were already approaching the finish line.",
                            "target_lang_translations": {
                                "ru": "<w>Бегущие впереди</w> марафонцы уже приближались к финишной черте."
                            }
                        },
                        {
                            "text": "We sat in the <w>foremost</w> row of the theater to get the best view.",
                            "target_lang_translations": {
                                "ru": "Мы сели в <w>самый первый</w> ряд театра, чтобы получить лучший обзор."
                            }
                        }
                    ],
                    "synonyms": [
                        "front",
                        "frontmost",
                        "forwardmost"
                    ],
                    "antonyms": [
                        "back",
                        "backmost",
                        "hindmost",
                        "rear",
                        "rearmost",
                        "rearwardmost"
                    ],
                    "common_phrases": [
                        "foremost part",
                        "foremost rank"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный впереди всех остальных или в самой передней точке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передний",
                                "target_lang_sense_clarification": "находящийся впереди"
                            },
                            {
                                "target_lang_word": "самый первый",
                                "target_lang_sense_clarification": "расположенный раньше всех остальных"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2cf74528-e69f-397b-920e-1ccb9beffc21",
                    "sense_definition": "Most importantly or chiefly.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "First and <w>foremost</w>, we must address the urgent needs of the community.",
                            "target_lang_translations": {
                                "ru": "Прежде всего и <w>в первую очередь</w>, мы должны удовлетворить насущные потребности общества."
                            }
                        },
                        {
                            "text": "He considered himself <w>foremost</w> a writer, despite his other professional roles.",
                            "target_lang_translations": {
                                "ru": "Он считал себя <w>прежде всего</w> писателем, несмотря на другие свои профессиональные роли."
                            }
                        }
                    ],
                    "synonyms": [
                        "especially",
                        "particularly"
                    ],
                    "common_phrases": [
                        "first and foremost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важнее всего или главным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прежде всего",
                                "target_lang_sense_clarification": "в первую очередь, сначала"
                            },
                            {
                                "target_lang_word": "главным образом",
                                "target_lang_sense_clarification": "преимущественно"
                            }
                        ]
                    }
                }
            ]
        },

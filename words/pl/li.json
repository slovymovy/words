    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "58cf8d15-5d10-32ab-be9f-2f63d64a2a16",
                    "sense_definition": "Archaiczna partykuła pytajna, zwykle stawiana po czasowniku, nadająca zdaniu charakter pytania; pełni funkcję współczesnego słowa \"czy\".",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "interrogative_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znasz <w>li</w> ten kraj, o którym wszyscy mówią?",
                            "target_lang_translations": {
                                "ru": "Знаешь <w>ли</w> ты тот край, о котором все говорят?"
                            }
                        },
                        {
                            "text": "Widzisz <w>li</w> te chmury zbierające się na horyzoncie?",
                            "target_lang_translations": {
                                "ru": "Видишь <w>ли</w> эти тучи, собирающиеся на горизонте?"
                            }
                        }
                    ],
                    "synonyms": [
                        "czy"
                    ],
                    "common_phrases": [
                        "znasz-li",
                        "widzisz-li"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane w literaturze i stylu podniosłym"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Архаичная вопросительная частица, обычно стоящая после глагола и придающая предложению вопросительный характер; выполняет функцию современного слова «ли»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "вопросительная частица"
                            },
                            {
                                "target_lang_word": "ль",
                                "target_lang_sense_clarification": "сокращенная форма частицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84308727-72da-3ff4-9b42-2c79c2355c85",
                    "sense_definition": "Przestarzały spójnik wprowadzający zdanie warunkowe, oznaczający \"jeśli\" lub \"gdyby\".",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "conditional_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chcesz <w>li</w> być szczęśliwy, musisz żyć w zgodzie z samym sobą.",
                            "target_lang_translations": {
                                "ru": "Хочешь <w>ли</w> быть счастлив, ты должен жить в согласии с самим собой."
                            }
                        },
                        {
                            "text": "Będziesz <w>li</w> pilnie pracował, osiągniesz sukces.",
                            "target_lang_translations": {
                                "ru": "Будешь <w>ли</w> усердно работать, достигнешь успеха."
                            }
                        }
                    ],
                    "synonyms": [
                        "jeśli",
                        "skoro",
                        "kiedy"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревший союз, вводящий условное предложение, означающий «если» или «коли»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коли",
                                "target_lang_sense_clarification": "устаревший/просторечный условный союз"
                            },
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "современный эквивалент"
                            },
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "в значении условия (устар.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "208bb247-40cd-339b-b518-a1b52e38d7fb",
                    "sense_definition": "Tradycyjna chińska jednostka miary długości, wynosząca około 500 metrów.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chinese_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wioska znajduje się zaledwie kilka <w>li</w> od rzeki.",
                            "target_lang_translations": {
                                "ru": "Деревня находится всего в нескольких <w>ли</w> от реки."
                            }
                        },
                        {
                            "text": "Podróżnicy musieli przebyć tysiąc <w>li</w>, aby dotrzeć do stolicy.",
                            "target_lang_translations": {
                                "ru": "Путешественникам пришлось преодолеть тысячу <w>ли</w>, чтобы добраться до столицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "mila chińska"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Chiny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционная китайская единица измерения расстояния, равная примерно 500 метрам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "китайская мера длины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "833d33c7-58c3-3494-9f79-9eaa80b6550c",
                    "sense_definition": "W filozofii konfucjańskiej: pojęcie oznaczające rytuał, etykietę, właściwe postępowanie i porządek społeczny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "philosophy_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Konfucjusz uważał przestrzeganie <w>li</w> za fundament harmonijnego społeczeństwa.",
                            "target_lang_translations": {
                                "ru": "Конфуций считал соблюдение <w>ли</w> фундаментом гармоничного общества."
                            }
                        },
                        {
                            "text": "Bez zrozumienia <w>li</w> trudno jest pojąć chińską kulturę tradycyjną.",
                            "target_lang_translations": {
                                "ru": "Без понимания <w>ли</w> трудно постичь традиционную китайскую культуру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "filozofia chińska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В конфуцианстве: понятие, обозначающее ритуал, этикет, правильное поведение и общественный порядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "философское понятие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f3e86f2f-fa7f-3139-a5e4-7e1f80a4027a",
                    "sense_definition": "Przestarzałe lub gwarowe słowo oznaczające ograniczenie; odpowiada współczesnemu \"tylko\" lub \"jedynie\".",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "adverb_only",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>li</w> tylko sen, który zaraz przeminie.",
                            "target_lang_translations": {
                                "ru": "Это <w>лишь</w> сон, который скоро пройдет."
                            }
                        },
                        {
                            "text": "Jedno <w>li</w> pragnienie nosił w swoim sercu przez całe życie.",
                            "target_lang_translations": {
                                "ru": "Одно <w>лишь</w> желание носил он в своем сердце всю жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "tylko",
                        "jedynie",
                        "le"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или диалектное слово, обозначающее ограничение; соответствует современному «только» или «лишь»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "ограничительная частица"
                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

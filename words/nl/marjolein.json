            "pos": "noun",
            "senses": [
                {
                    "sense_id": "808ec176-cb11-3e75-b277-7623ee3bcef3",
                    "sense_definition": "De aromatische blaadjes van de echte marjolein, vers of gedroogd gebruikt om gerechten te kruiden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "herb_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voeg op het laatste moment wat verse <w>marjolein</w> toe aan de saus.",
                            "target_lang_translations": {
                                "ru": "В самый последний момент добавьте в соус немного свежего <w>майорана</w>."
                            }
                        },
                        {
                            "text": "Je kunt gedroogde <w>marjolein</w> gebruiken als je geen verse kruiden hebt.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать сушеный <w>майоран</w>, если у вас нет свежей зелени."
                            }
                        }
                    ],
                    "synonyms": [
                        "majoraan"
                    ],
                    "common_phrases": [
                        "verse marjolein",
                        "gedroogde marjolein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ароматные листья майорана, свежие или сушеные, используемые для приправы блюд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майоран",
                                "target_lang_sense_clarification": "пряность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7a8e0bf-97a2-30c4-a3a2-f2efd6e029eb",
                    "sense_definition": "Een plant uit de lipbloemenfamilie, specifiek de soort Origanum majorana.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plant_species",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In mijn kruidentuin staat de <w>marjolein</w> er prachtig bij.",
                            "target_lang_translations": {
                                "ru": "В моем саду трав <w>майоран</w> выглядит просто великолепно."
                            }
                        },
                        {
                            "text": "De <w>marjolein</w> is gevoelig voor vorst en moet in de winter beschermd worden.",
                            "target_lang_translations": {
                                "ru": "<w>Майоран</w> чувствителен к морозу, и зимой его нужно защищать."
                            }
                        }
                    ],
                    "synonyms": [
                        "echte marjolein",
                        "majoraan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение из семейства яснотковых, в частности вид Origanum majorana."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майоран",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cece3d14-7c68-366d-b5a8-cbdf17c2b977",
                    "sense_definition": "Een meisjesnaam.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn zus heet <w>Marjolein</w> en ze is jarig in mei.",
                            "target_lang_translations": {
                                "ru": "Мою сестру зовут <w>Марьолейн</w>, и у нее день рождения в мае."
                            }
                        },
                        {
                            "text": "Ik ga vanavond met <w>Marjolein</w> naar de bioscoop.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером я иду с <w>Марьолейн</w> в кино."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Марьолейн",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
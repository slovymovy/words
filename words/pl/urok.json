                {
                    "sense_id": "7b87ffb8-428e-3a25-aa44-dc95627b7cd0",
                    "sense_definition": "Cechy osoby, rzeczy lub miejsca, które sprawiają, że są one przyjemne, atrakcyjne i budzą zachwyt",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "aesthetic_charm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta jednoaktówka dodała jeszcze <w>uroku</w> całemu przedstawieniu.",
                            "target_lang_translations": {
                                "ru": "Эта одноактная пьеса добавила ещё больше <w>очарования</w> всему представлению."
                            }
                        },
                        {
                            "text": "Jej naturalny <w>urok</w> osobisty sprawia, że wszyscy czują się przy niej dobrze.",
                            "target_lang_translations": {
                                "ru": "Её природное личное <w>обаяние</w> заставляет всех чувствовать себя с ней комфортно."
                            }
                        }
                    ],
                    "synonyms": [
                        "wdzięk",
                        "powab",
                        "czar",
                        "gracja",
                        "urokliwość"
                    ],
                    "antonyms": [
                        "brzydota"
                    ],
                    "common_phrases": [
                        "urok osobisty",
                        "mieć urok",
                        "pełen uroku",
                        "dodać uroku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Черты человека, предмета или места, которые делают их приятными, привлекательными и вызывают восхищение"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очарование",
                                "target_lang_sense_clarification": "притягательная сила, прелесть чего-либо или кого-либо"
                            },
                            {
                                "target_lang_word": "обаяние",
                                "target_lang_sense_clarification": "личный шарм, способность нравиться окружающим (обычно о людях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ab0c0ba-64d5-39f0-94cd-2093f4a8fbaa",
                    "sense_definition": "Według wierzeń ludowych: tajemnicza siła magiczna lub zaklęcie rzucone na kogoś w celu zaszkodzenia mu",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "magic_spell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzuciwszy <w>urok</w>, czarownica rechocząc odleciała na miotle.",
                            "target_lang_translations": {
                                "ru": "Наложив <w>заклятие</w>, ведьма, хохоча, улетела на метле."
                            }
                        },
                        {
                            "text": "Stara kobieta twierdziła, że potrafi odczynić każdy zły <w>urok</w>.",
                            "target_lang_translations": {
                                "ru": "Старая женщина утверждала, что может снять любой злой <w>сглаз</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "czar",
                        "magia"
                    ],
                    "common_phrases": [
                        "rzucić urok",
                        "odczynić urok",
                        "zły urok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Согласно народным поверьям: таинственная магическая сила или заклинание, наложенное с целью навредить"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сглаз",
                                "target_lang_sense_clarification": "вред, причиняемый дурным глазом (часто непреднамеренно)"
                            },
                            {
                                "target_lang_word": "порча",
                                "target_lang_sense_clarification": "намеренное магическое воздействие с целью навредить"
                            },
                            {
                                "target_lang_word": "заклятие",
                                "target_lang_sense_clarification": "магическая формула или действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2caa3e87-398c-3ee5-b977-74aab2f9e9ee",
                    "sense_definition": "In elk geval; hoe dan ook; ongeacht de omstandigheden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inevitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het maakt niet uit of het regent, ik ga <w>sowieso</w> naar buiten.",
                            "target_lang_translations": {
                                "en": "It doesn't matter if it rains, I'm going outside <w>anyway</w>."
                            }
                        },
                        {
                            "text": "Ik wilde <w>sowieso</w> al niet naar dat feest gaan, dus ik vind het niet erg dat het is afgelast.",
                            "target_lang_translations": {
                                "en": "I didn't want to go to that party <w>anyway</w>, so I don't mind that it's cancelled."
                            }
                        }
                    ],
                    "synonyms": [
                        "in elk geval",
                        "toch al"
                    ],
                    "common_phrases": [
                        "sowieso niet",
                        "ik ga sowieso",
                        "dat is sowieso"
                    ],
                    "target_lang_definitions": {
                        "en": "In any case; no matter what happens; regardless of the circumstances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anyway",
                                "target_lang_sense_clarification": "regardless of other factors"
                            },
                            {
                                "target_lang_word": "in any case",
                                "target_lang_sense_clarification": "no matter what happens"
                            },
                            {
                                "target_lang_word": "regardless",
                                "target_lang_sense_clarification": "despite circumstances"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6b83376-9f93-331e-9b48-7777e356cf32",
                    "sense_definition": "Eigenlijk; over het algemeen genomen; überhaupt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inevitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben <w>sowieso</w> geen liefhebber van spruitjes, hoe je ze ook klaarmaakt.",
                            "target_lang_translations": {
                                "en": "I'm not a fan of Brussels sprouts <w>to begin with</w>, no matter how you prepare them."
                            }
                        },
                        {
                            "text": "Is er <w>sowieso</w> nog wel genoeg tijd om dit project af te ronden?",
                            "target_lang_translations": {
                                "en": "Is there enough time to finish this project <w>at all</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "überhaupt"
                    ],
                    "common_phrases": [
                        "sowieso al"
                    ],
                    "target_lang_definitions": {
                        "en": "Actually; generally speaking; at all (often used to reinforce a fundamental fact)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to begin with",
                                "target_lang_sense_clarification": "fundamentally; from the start"
                            },
                            {
                                "target_lang_word": "at all",
                                "target_lang_sense_clarification": "used in questions or negatives to emphasize scope"
                            },
                            {
                                "target_lang_word": "in general",
                                "target_lang_sense_clarification": "as a general rule"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d19fff56-fd5c-39f8-92ec-4ce9572816bb",
                    "sense_definition": "Noodzakelijkerwijs; per se; in alle gevallen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "necessity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een duurder product hoeft niet <w>sowieso</w> van betere kwaliteit te zijn.",
                            "target_lang_translations": {
                                "en": "A more expensive product is not <w>necessarily</w> of better quality."
                            }
                        },
                        {
                            "text": "Dat dit plan mislukt is, betekent niet <w>sowieso</w> dat we moeten stoppen.",
                            "target_lang_translations": {
                                "en": "The fact that this plan failed does not mean <w>automatically</w> that we have to stop."
                            }
                        }
                    ],
                    "synonyms": [
                        "noodzakelijk",
                        "noodzakelijkerwijs",
                        "per se"
                    ],
                    "common_phrases": [
                        "niet sowieso"
                    ],
                    "target_lang_definitions": {
                        "en": "Necessarily; automatically; by definition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "necessarily",
                                "target_lang_sense_clarification": "inevitably or logically required"
                            },
                            {
                                "target_lang_word": "automatically",
                                "target_lang_sense_clarification": "as a direct result"
                            },
                            {
                                "target_lang_word": "per se",
                                "target_lang_sense_clarification": "by itself"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
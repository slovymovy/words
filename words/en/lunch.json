                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обед",
                                "target_lang_sense_clarification": "прием пищи"
                            },
                            {
                                "target_lang_word": "ланч",
                                "target_lang_sense_clarification": "легкая закуска, фуршет"
                            }
                        ]

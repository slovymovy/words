                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leech",
                                "target_lang_sense_clarification": "vertical or aft edge of a sail"
                            },
                            {
                                "target_lang_word": "boltrope",
                                "target_lang_sense_clarification": "rope sewn to the edge of a sail"
                            },
                            {
                                "target_lang_word": "edge",
                                "target_lang_sense_clarification": "of a sail"
                            }
                        ]
                    }
                }
            ]

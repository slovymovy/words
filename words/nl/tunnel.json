                {
                    "sense_id": "2f6de98c-15b9-365d-893d-c05fe0402d8c",
                    "sense_definition": "Een kunstmatige ondergrondse doorgang voor verkeer, treinen of voetgangers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein rijdt met hoge snelheid de donkere <w>tunnel</w> in.",
                            "target_lang_translations": {
                                "ru": "Поезд на высокой скорости въезжает в темный <w>тоннель</w>.",
                                "en": "The train enters the dark <w>tunnel</w> at high speed."
                            }
                        },
                        {
                            "text": "We moesten lang wachten voordat we door de <w>tunnel</w> konden rijden.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось долго ждать, прежде чем мы смогли проехать через <w>тоннель</w>.",
                                "en": "We had to wait a long time before we could drive through the <w>tunnel</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderdoorgang"
                    ],
                    "common_phrases": [
                        "door de tunnel",
                        "lange tunnel",
                        "einde van de tunnel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственный подземный проход для транспорта, поездов или пешеходов.",
                        "en": "An artificial underground passage for traffic, trains, or pedestrians."

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2522b8ab-e246-399d-9be0-8e7297c5a564",
                    "sense_definition": "Утолить жажду; выпить воды или другого напитка вдоволь.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В жаркий летний день так приятно <напиться> холодной воды из родника.",
                            "target_lang_translations": {
                                "en": "On a hot summer day, it is so pleasant to <drink one's fill> of cold water from the spring."
                            }
                        },
                        {
                            "text": "Путник остановился у колодца, чтобы <напиться> и отдохнуть.",
                            "target_lang_translations": {
                                "en": "The traveler stopped at the well to <quench his thirst> and rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "утолить жажду"
                    ],
                    "common_phrases": [
                        "напиться воды",
                        "дать напиться",
                        "хочу напиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To satisfy thirst; to drink enough water or another beverage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quench one's thirst",
                                "target_lang_sense_clarification": "to satisfy the need to drink"
                            },
                            {
                                "target_lang_word": "drink one's fill",
                                "target_lang_sense_clarification": "to drink until fully satisfied"
                            },
                            {
                                "target_lang_word": "slake one's thirst",
                                "target_lang_sense_clarification": "literary/formal way to say satisfy thirst"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e77d2920-5934-33b1-a4be-d262863ec215",
                    "sense_definition": "Употребить много алкоголя и стать пьяным.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он пообещал жене больше никогда не <напиваться> на вечеринках.",
                            "target_lang_translations": {
                                "en": "He promised his wife never to <get drunk> at parties again."
                            }
                        },
                        {
                            "text": "С горя он решил <напиться>, чтобы забыть о своих проблемах.",
                            "target_lang_translations": {
                                "en": "Out of grief, he decided to <get drunk> to forget his problems."
                            }
                        }
                    ],
                    "synonyms": [
                        "опьянеть",
                        "набраться",
                        "набухаться"
                    ],
                    "antonyms": [
                        "протрезветь"
                    ],
                    "common_phrases": [
                        "напиться пьяным",
                        "напиться в стельку",
                        "напиться до беспамятства"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To consume a lot of alcohol and become drunk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get drunk",
                                "target_lang_sense_clarification": "general term for becoming intoxicated"
                            },
                            {
                                "target_lang_word": "get wasted",
                                "target_lang_sense_clarification": "slang/colloquial for becoming very drunk"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf32f8e4-695a-3431-96b3-a3ea1ecf2687",
                    "sense_definition": "Принять внутрь большое количество лекарств или химических веществ (обычно с негативным оттенком).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач строго предупредил, чтобы пациент случайно не <напился> этих опасных таблеток.",
                            "target_lang_translations": {
                                "en": "The doctor strictly warned the patient not to accidentally <take too many> of these dangerous pills."
                            }
                        },
                        {
                            "text": "В детективном романе герой пытался <напиться> снотворного, но его спасли.",
                            "target_lang_translations": {
                                "en": "In the detective novel, the hero tried to <overdose> on sleeping pills, but he was saved."
                            }
                        }
                    ],
                    "common_phrases": [
                        "напиться таблеток",
                        "напиться лекарств"
                    ],
                    "target_lang_definitions": {
                        "en": "To ingest a large amount of medicine or chemicals (usually with negative connotation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overdose on",
                                "target_lang_sense_clarification": "to take a dangerous amount of a substance"
                            },
                            {
                                "target_lang_word": "take a lot of",
                                "target_lang_sense_clarification": "to consume a large quantity of pills or liquid"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
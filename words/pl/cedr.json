            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33261132-138d-3f4c-88c1-572f314da5af",
                    "sense_definition": "Drzewo iglaste z rodziny sosnowatych o rozłożystej koronie i trwałym drewnie, rosnące dziko w górach regionu śródziemnomorskiego i w Himalajach.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zbocza góry gęsto porastały stare <w>cedry</w>.",
                            "target_lang_translations": {
                                "ru": "Склоны горы густо поросли старыми <w>кедрами</w>."
                            }
                        },
                        {
                            "text": "Na fladze Libanu widnieje charakterystyczny zielony <w>cedr</w>.",
                            "target_lang_translations": {
                                "ru": "На флаге Ливана изображён характерный зелёный <w>кедр</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cedr libański",
                        "potężny cedr",
                        "gałąź cedru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хвойное дерево семейства сосновых с раскидистой кроной и прочной древесиной, дикорастущее в горах Средиземноморья и в Гималаях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кедр",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "343c1475-df4d-3e98-815d-58b8da00766f",
                    "sense_definition": "Drewno pozyskiwane z drzewa cedrowego, cenione za przyjemny zapach i odporność na wilgoć.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zabytkowa skrzynia została wykonana z litego <w>cedru</w>.",
                            "target_lang_translations": {
                                "ru": "Этот старинный сундук был изготовлен из цельного <w>кедра</w>."
                            }
                        },
                        {
                            "text": "W całym warsztacie unosił się intensywny zapach <w>cedru</w>.",
                            "target_lang_translations": {
                                "ru": "Во всей мастерской стоял сильный запах <w>кедра</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cedrowina",
                        "cedrzyna"
                    ],
                    "common_phrases": [
                        "zapach cedru",
                        "stół z cedru",
                        "olejek z cedru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древесина, получаемая из кедра, ценимая за приятный запах и устойчивость к влаге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кедр",
                                "target_lang_sense_clarification": "древесина"
                            }
                        ]
                    }
                }
            ]
        }
    ]

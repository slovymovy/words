            "senses": [
                {
                    "sense_id": "5cc3740e-34ed-3391-ae91-f3dd55f3f4d7",
                    "sense_definition": "Describing something that happens again and again or stays the same.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wallpaper had a <w>repeating</w> pattern of small blue flowers.",
                            "target_lang_translations": {
                                "ru": "На обоях был <w>повторяющийся</w> узор из маленьких голубых цветов."
                            }
                        },
                        {
                            "text": "We encountered a <w>repeating</w> problem with the software installation.",
                            "target_lang_translations": {
                                "ru": "Мы столкнулись с <w>повторяющейся</w> проблемой при установке программного обеспечения."
                            }

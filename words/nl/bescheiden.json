        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "23241612-18b6-3a78-9064-ca726d85016c",
                    "sense_definition": "Iets aan iemand toewijzen of bepalen (vaak door het lot of een hogere macht).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "allot",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niemand weet wat het lot hem <w>bescheiden</w> heeft.",
                            "target_lang_translations": {
                                "en": "No one knows what fate has <w>allotted</w> him."
                            }
                        },
                        {
                            "text": "Het was hem niet <w>bescheiden</w> om oud te worden.",
                            "target_lang_translations": {
                                "en": "It was not <w>granted</w> to him to grow old."
                            }
                        }
                    ],
                    "synonyms": [
                        "toebedelen",
                        "bedelen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To assign or allot to someone (usually by fate)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allotted",
                                "target_lang_sense_clarification": "given as a share or task"
                            },
                            {
                                "target_lang_word": "granted",
                                "target_lang_sense_clarification": "bestowed upon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28fad096-5022-3283-b137-e56b7cf43796",
                    "sense_definition": "Iemand officieel vragen om ergens te komen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "invite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning <w>bescheidde</w> zijn raadslieden naar het paleis.",
                            "target_lang_translations": {
                                "en": "The king <w>summoned</w> his councilors to the palace."
                            }
                        },
                        {
                            "text": "Hij werd naar de rechtbank <w>bescheiden</w> om te getuigen.",
                            "target_lang_translations": {
                                "en": "He was <w>summoned</w> to the court to testify."
                            }
                        }
                    ],
                    "synonyms": [
                        "nodigen",
                        "uitnodigen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To summon or officially invite someone to appear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "summoned",
                                "target_lang_sense_clarification": "ordered to be present"
                            },
                            {
                                "target_lang_word": "invited",
                                "target_lang_sense_clarification": "formally requested"
                            }
                        ]
                    }
                }
            ]

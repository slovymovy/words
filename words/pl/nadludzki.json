    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8e618e8b-f584-38be-8374-66ca00f5f1a6",
                    "sense_definition": "Przekraczający przeciętne ludzkie możliwości, siły lub wytrzymałość; niezwykle wielki lub intensywny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uratowanie tych ludzi z pożaru wymagało od strażaków <w>nadludzkiego</w> wysiłku.",
                            "target_lang_translations": {
                                "ru": "Спасение этих людей из пожара потребовало от пожарных <w>сверхчеловеческого</w> усилия."
                            }
                        },
                        {
                            "text": "Wydawało się, że ten sportowiec ma w sobie jakąś <w>nadludzką</w> siłę.",
                            "target_lang_translations": {
                                "ru": "Казалось, что этот спортсмен обладает какой-то <w>сверхчеловеческой</w> силой."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogromny",
                        "ponadludzki",
                        "nieludzki",
                        "nadzwyczajny"
                    ],
                    "common_phrases": [
                        "nadludzki wysiłek",
                        "nadludzka siła",
                        "nadludzka moc",
                        "nadludzka cierpliwość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превышающий средние человеческие возможности, силы или выносливость; невероятно огромный или интенсивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сверхчеловеческий",
                                "target_lang_sense_clarification": "превышающий обычные силы"
                            },
                            {
                                "target_lang_word": "нечеловеческий",
                                "target_lang_sense_clarification": "чрезвычайно сильный, интенсивный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d53a82a9-b734-3849-be0f-435507bfdfd8",
                    "sense_definition": "Mający cechy istoty wyższej, niebędący z tego świata; wykraczający poza naturę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wierzeniach przypisywano herosom <w>nadludzkie</w> zdolności.",
                            "target_lang_translations": {
                                "ru": "В древних верованиях героям приписывали <w>сверхчеловеческие</w> способности."
                            }
                        },
                        {
                            "text": "Jej uroda wydawała się wręcz <w>nadludzka</w> i onieśmielała wszystkich wokół.",
                            "target_lang_translations": {
                                "ru": "Её красота казалась поистине <w>сверхчеловеческой</w> и смущала всех вокруг."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieziemski",
                        "nadprzyrodzony",
                        "cudowny"
                    ],
                    "common_phrases": [
                        "nadludzka istota",
                        "nadludzkie zdolności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий черты высшего существа, не от мира сего; выходящий за рамки природы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сверхчеловеческий",
                                "target_lang_sense_clarification": "свойственный высшим существам"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9330b96-1aa2-3678-8b8f-ff84d8c8f002",
                    "sense_definition": "Поездка, поход группы лиц с каким-либо специальным заданием (исследовательским, военным, спасательным и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "journey_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго готовились к этой полярной <w>экспедиции</w>.",
                            "target_lang_translations": {
                                "en": "We spent a long time preparing for this polar <w>expedition</w>."
                            }
                        },
                        {
                            "text": "Археологическая <w>экспедиция</w> обнаружила руины древнего города.",
                            "target_lang_translations": {
                                "en": "The archaeological <w>expedition</w> discovered the ruins of an ancient city."
                            }
                        }
                    ],
                    "synonyms": [
                        "поход",
                        "путешествие",
                        "поездка"
                    ],
                    "common_phrases": [
                        "научная экспедиция",
                        "отправиться в экспедицию",
                        "спасательная экспедиция",
                        "геологическая экспедиция"
                    ],
                    "target_lang_definitions": {
                        "en": "A journey undertaken by a group of people with a particular purpose, especially for exploration, scientific research, or military ends."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expedition",
                                "target_lang_sense_clarification": "journey"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bb4a6e5-1f8c-3d91-ac32-6a16b8989e98",
                    "sense_definition": "Группа людей, участвующих в специальном путешествии или походе.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "journey_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся <w>экспедиция</w> устала после долгого перехода через горы.",
                            "target_lang_translations": {
                                "en": "The entire <w>expedition</w> was tired after the long trek through the mountains."
                            }
                        },
                        {
                            "text": "Наконец наша <w>экспедиция</w> прибыла к месту назначения.",
                            "target_lang_translations": {
                                "en": "Finally, our <w>expedition</w> arrived at its destination."
                            }
                        }
                    ],
                    "common_phrases": [
                        "начальник экспедиции",
                        "члены экспедиции",
                        "состав экспедиции"
                    ],
                    "target_lang_definitions": {
                        "en": "The group of people participating in a specific journey or enterprise."
                    },
                    "translations": {

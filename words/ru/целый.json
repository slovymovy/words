            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b424cbee-1b6f-3b07-baf5-c63932f35385",
                    "sense_definition": "Полный, без изъятия; весь от начала до конца.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "whole_full",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы гуляли по городу <целый> день.",
                            "target_lang_translations": {
                                "en": "We walked around the city the <whole> day."
                            }
                        },
                        {
                            "text": "Я съел <целый> торт и теперь не хочу ужинать.",
                            "target_lang_translations": {
                                "en": "I ate a <whole> cake and now I don't want dinner."
                            }
                        }
                    ],
                    "synonyms": [
                        "весь",
                        "полный"
                    ],
                    "antonyms": [
                        "неполный",
                        "частичный"
                    ],
                    "common_phrases": [
                        "целый день",
                        "целый год",
                        "целый мир",
                        "целый час"
                    ],
                    "target_lang_definitions": {
                        "en": "Complete, with no parts missing; entire."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whole",
                                "target_lang_sense_clarification": "general term for something complete"
                            },
                            {
                                "target_lang_word": "entire",
                                "target_lang_sense_clarification": "formal, emphasizing completeness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee6c4531-1089-3382-8e80-246830768733",
                    "sense_definition": "Значительный, большой, настоящий (используется для усиления выразительности речи).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы не виделись с другом <целую> вечность.",
                            "target_lang_translations": {
                                "en": "My friend and I haven't seen each other for a <whole> eternity."
                            }
                        },
                        {
                            "text": "Это была <целая> история, как мы искали дорогу домой.",
                            "target_lang_translations": {
                                "en": "It was a <whole> story how we looked for the way home."
                            }
                        }
                    ],
                    "synonyms": [
                        "настоящий"
                    ],
                    "common_phrases": [
                        "целая вечность",
                        "целая куча",
                        "целая история",
                        "целый ряд"
                    ],
                    "target_lang_definitions": {
                        "en": "Significant or large; used to emphasize the extent or importance of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whole",
                                "target_lang_sense_clarification": "used as an intensifier (e.g., a whole lot)"
                            },
                            {
                                "target_lang_word": "entire",
                                "target_lang_sense_clarification": "intensifier for time or extent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eda8e9be-672d-3c10-80f4-6c9c54feefb4",
                    "sense_definition": "Невредимый, неповреждённый, сохранившийся в здоровом состоянии.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "intact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После аварии водитель, к счастью, остался <цел>.",
                            "target_lang_translations": {
                                "en": "After the accident, the driver, fortunately, remained <unharmed>."
                            }
                        },
                        {
                            "text": "Стаканы упали на пол, но остались <целыми>.",
                            "target_lang_translations": {
                                "en": "The glasses fell on the floor but remained <intact>."
                            }
                        }
                    ],
                    "synonyms": [
                        "невредимый",
                        "неповреждённый",
                        "сохранный"
                    ],
                    "common_phrases": [
                        "цел и невредим",
                        "остаться целым"
                    ],
                    "target_lang_definitions": {
                        "en": "Not damaged, injured, or broken; in good condition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "intact",

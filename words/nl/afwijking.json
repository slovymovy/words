{
    "word_family": [
        "afwijken",
        "afwijkend"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4d3cc4f-99b0-30dd-a72a-7a589fb0f4d8",
                    "sense_definition": "Een verschil ten opzichte van wat normaal, verwacht of gemiddeld is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De resultaten van het onderzoek vertoonden een significante <w>afwijking</w> van het gemiddelde.",
                            "target_lang_translations": {
                                "en": "The results of the study showed a significant <w>deviation</w> from the mean.",
                                "ru": "Результаты исследования показали значительное <w>отклонение</w> от среднего значения."
                            }
                        },
                        {
                            "text": "Er is een lichte <w>afwijking</w> in de berekening geslopen.",
                            "target_lang_translations": {
                                "en": "A slight <w>discrepancy</w> has crept into the calculation.",
                                "ru": "В расчеты закралось небольшое <w>расхождение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "standaard afwijking",
                        "lichte afwijking",
                        "grote afwijking"
                    ],
                    "target_lang_definitions": {
                        "en": "A difference relative to what is normal, expected, or average.",
                        "ru": "Различие по сравнению с тем, что считается нормальным, ожидаемым или средним."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deviation",
                                "target_lang_sense_clarification": "general or statistical difference"
                            },
                            {
                                "target_lang_word": "anomaly",
                                "target_lang_sense_clarification": "something that deviates from the norm"
                            },
                            {
                                "target_lang_word": "discrepancy",
                                "target_lang_sense_clarification": "difference in calculations or figures"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отклонение",
                                "target_lang_sense_clarification": "общее значение, несовпадение с нормой"
                            },
                            {
                                "target_lang_word": "расхождение",
                                "target_lang_sense_clarification": "разница в данных или результатах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3aba13ea-62c1-3d50-aaca-f11b50982028",
                    "sense_definition": "Een lichamelijke of geestelijke gesteldheid die niet als normaal wordt beschouwd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts ontdekte een aangeboren <w>afwijking</w> aan zijn hartklep.",
                            "target_lang_translations": {
                                "en": "The doctor discovered a congenital <w>defect</w> in his heart valve.",
                                "ru": "Врач обнаружил врожденный <w>порок</w> сердечного клапана."
                            }
                        },
                        {
                            "text": "Ondanks haar genetische <w>afwijking</w> leidt ze een heel gelukkig en actief leven.",
                            "target_lang_translations": {
                                "en": "Despite her genetic <w>abnormality</w>, she leads a very happy and active life.",
                                "ru": "Несмотря на свою генетическую <w>аномалию</w>, она ведет очень счастливую и активную жизнь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aangeboren afwijking",
                        "genetische afwijking",
                        "lichamelijke afwijking"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical or mental condition considered abnormal.",
                        "ru": "Физическое или психическое состояние, которое не считается нормальным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "abnormality",
                                "target_lang_sense_clarification": "medical or physical irregularity"
                            },
                            {
                                "target_lang_word": "defect",
                                "target_lang_sense_clarification": "imperfection or malformation"
                            },
                            {
                                "target_lang_word": "anomaly",
                                "target_lang_sense_clarification": "unusual medical condition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "аномалия",
                                "target_lang_sense_clarification": "генетическое или физическое нарушение"
                            },
                            {
                                "target_lang_word": "порок",
                                "target_lang_sense_clarification": "дефект развития органа (например, сердца)"
                            },
                            {
                                "target_lang_word": "дефект",
                                "target_lang_sense_clarification": "физический недостаток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9aa6f4f-bb66-3e83-95c7-796b32812710",
                    "sense_definition": "Een onnauwkeurigheid in de richting of afstelling van een instrument of voertuig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het stuur van de auto had een gevaarlijke <w>afwijking</w> naar links.",
                            "target_lang_translations": {
                                "en": "The car's steering had a dangerous <w>deviation</w> to the left.",
                                "ru": "Рулевое управление автомобиля имело опасное <w>отклонение</w> влево."
                            }
                        },
                        {
                            "text": "Door een magnetische storing vertoonde het kompas een tijdelijke <w>afwijking</w>.",
                            "target_lang_translations": {
                                "en": "Due to magnetic interference, the compass showed a temporary <w>deviation</w>.",
                                "ru": "Из-за магнитных помех компас показывал временную <w>девиацию</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "afwijking naar links",
                        "afwijking naar rechts"
                    ],
                    "target_lang_definitions": {
                        "en": "An inaccuracy in the direction or calibration of an instrument or vehicle.",
                        "ru": "Неточность в направлении или настройке прибора или транспортного средства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deviation",
                                "target_lang_sense_clarification": "divergence from a course or standard"
                            },
                            {
                                "target_lang_word": "bias",
                                "target_lang_sense_clarification": "systematic error in an instrument"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отклонение",
                                "target_lang_sense_clarification": "смещение от курса или направления"
                            },
                            {
                                "target_lang_word": "девиация",
                                "target_lang_sense_clarification": "технический термин, например, для компаса"
                            }
                        ]
                    }
                }
            ]
        }
    ]

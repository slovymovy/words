                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "limit_endpoint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Blijf hier wachten <tot> ik terugkom.",
                            "target_lang_translations": {
                                "en": "Wait here <w>until</w> I come back.",
                                "ru": "Жди здесь, <пока> я не вернусь."

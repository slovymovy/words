            "pos": "noun",
            "senses": [
                {
                    "sense_id": "238b8333-d926-3050-8a91-de6c6c15a49a",
                    "sense_definition": "Właściwość, atrybut lub element charakterystyczny wyróżniający osobę, przedmiot lub zjawisko od innych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "feature_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cierpliwość to bardzo ważna <w>cecha</w> dobrego nauczyciela.",
                            "target_lang_translations": {
                                "ru": "Терпение — это очень важная <w>черта</w> хорошего учителя."
                            }
                        },
                        {
                            "text": "Ten model samochodu posiada jedną unikalną <w>cechę</w>, której nie mają inne pojazdy.",
                            "target_lang_translations": {
                                "ru": "У этой модели автомобиля есть одна уникальная <w>особенность</w>, которой нет у других транспортных средств."
                            }
                        },
                        {
                            "text": "Każda <w>cecha</w> charakteru wpływa na to, jak postrzegają nas inni ludzie.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>черта</w> характера влияет на то, как нас воспринимают другие люди."
                            }
                        }
                    ],
                    "synonyms": [
                        "właściwość",
                        "atrybut",
                        "charakter",
                        "wyróżnik",
                        "własność"
                    ],
                    "common_phrases": [
                        "cecha charakteru",
                        "cecha szczególna",
                        "cecha wspólna",
                        "cecha wyróżniająca",
                        "cecha dziedziczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство, атрибут или характерный элемент, отличающий человека, предмет или явление от других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черта",
                                "target_lang_sense_clarification": "особенность характера или внешности"
                            },
                            {
                                "target_lang_word": "свойство",
                                "target_lang_sense_clarification": "качество или атрибут"
                            },
                            {
                                "target_lang_word": "признак",
                                "target_lang_sense_clarification": "отличительный знак"
                            },
                            {
                                "target_lang_word": "особенность",
                                "target_lang_sense_clarification": "характерное отличие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2dc6992-6f66-3563-a00c-a123be63c06c",
                    "sense_definition": "Znak wybity na wyrobie (np. z metalu szlachetnego) potwierdzający jego próbę, pochodzenie lub własność; także narzędzie służące do wybijania takiego znaku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mark_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na złotym pierścionku widniała <w>cecha</w> probiercza potwierdzająca wysoką jakość kruszcu.",
                            "target_lang_translations": {
                                "ru": "На золотом кольце стояло пробирное <w>клеймо</w>, подтверждающее высокое качество металла."
                            }
                        },
                        {
                            "text": "Leśniczy użył specjalnej <w>cechy</w>, aby trwale oznaczyć drzewa przeznaczone do wycinki.",
                            "target_lang_translations": {
                                "ru": "Лесничий использовал специальное <w>клеймо</w>, чтобы навсегда пометить деревья, предназначенные для вырубки."
                            }
                        }
                    ],
                    "synonyms": [
                        "znak",
                        "piętno",
                        "cechówka"
                    ],
                    "common_phrases": [
                        "cecha probiercza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак, выбитый на изделии (например, из драгоценного металла), подтверждающий его пробу, происхождение или право собственности; также инструмент для нанесения такого знака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клеймо",
                                "target_lang_sense_clarification": "знак на изделии"
                            },
                            {
                                "target_lang_word": "проба",
                                "target_lang_sense_clarification": "знак качества металла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ea0e39d-930a-30a2-bfe4-d0c5b1c7bdc0",
                    "sense_definition": "Część całkowita logarytmu lub liczby rzeczywistej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "math_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Cecha</w> logarytmu dziesiętnego z liczby sto wynosi dokładnie dwa.",
                            "target_lang_translations": {
                                "ru": "<w>Характеристика</w> десятичного логарифма числа сто равна ровно двум."
                            }
                        },
                        {
                            "text": "Na lekcji matematyki uczniowie uczyli się wyznaczać, ile wynosi <w>cecha</w> i mantysa danej liczby.",
                            "target_lang_translations": {
                                "ru": "На уроке математики ученики учились определять, чему равны <w>характеристика</w> и мантисса данного числа."
                            }
                        }
                    ],
                    "synonyms": [
                        "podłoga"
                    ],
                    "antonyms": [
                        "mantysa"
                    ],
                    "common_phrases": [
                        "cecha logarytmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Целая часть логарифма или действительного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "характеристика",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2165bc39-3ae2-33e0-a7a1-0e7c87c7aeeb",
                    "sense_definition": "Poszewka na poduszkę lub powłoka na pierzynę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia kazała mi zmienić brudną <w>cechę</w> na czystą przed przyjazdem gości.",
                            "target_lang_translations": {
                                "ru": "Бабушка велела мне сменить грязную <w>наволочку</w> на чистую перед приездом гостей."
                            }
                        },
                        {
                            "text": "W szafie leżały starannie wyprasowane, białe <w>cechy</w> na poduszki.",
                            "target_lang_translations": {
                                "ru": "В шкафу лежали аккуратно выглаженные белые <w>наволочки</w> для подушек."
                            }
                        }
                    ],

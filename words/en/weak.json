                },
                {
                    "sense_id": "e837e0e1-5433-397b-a96f-e0e7bce8ce0f",
                    "sense_definition": "Likely to break or collapse; not firmly constructed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "lack_of_strength",
                    "name_type": "no",
                    "examples": [
                        {

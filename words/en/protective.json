    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "541c1fd7-44b6-3669-90ff-c3c6603ab61c",
                    "sense_definition": "Designed or intended to keep someone or something safe from harm or damage.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must wear <w>protective</w> clothing when handling these chemicals.",
                            "target_lang_translations": {
                                "ru": "При работе с этими химикатами необходимо носить <w>защи́тную</w> одежду."
                            }
                        },
                        {
                            "text": "The varnish forms a <w>protective</w> layer that prevents the wood from rotting.",
                            "target_lang_translations": {
                                "ru": "Лак образует <w>защи́тный</w> слой, который предотвращает гниение древесины."
                            }
                        }
                    ],
                    "synonyms": [
                        "defensive",
                        "shielding",
                        "safety"
                    ],
                    "common_phrases": [
                        "protective clothing",
                        "protective gear",
                        "protective layer",
                        "protective measures"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для обеспечения безопасности, защиты кого-либо или чего-либо от вреда или повреждений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защи́тный",
                                "target_lang_sense_clarification": "об одежде, слое, покрытии"
                            },
                            {
                                "target_lang_word": "предохрани́тельный",
                                "target_lang_sense_clarification": "о мерах безопасности или устройствах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "951a0132-25c7-3c89-9513-84f7e4211e8f",
                    "sense_definition": "Having a strong desire to keep someone safe; showing care and defense for someone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is very <w>protective</w> of her younger brother and watches him constantly.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>оберега́ет</w> своего младшего брата и постоянно следит за ним."
                            }
                        },
                        {
                            "text": "The dog became <w>protective</w> when a stranger approached the house.",
                            "target_lang_translations": {
                                "ru": "Собака начала проявлять <w>охра́нный</w> инстинкт, когда к дому подошел незнакомец."
                            }
                        }
                    ],
                    "synonyms": [
                        "watchful",
                        "caring",
                        "defensive"
                    ],
                    "antonyms": [
                        "indifferent",
                        "neglectful"
                    ],
                    "common_phrases": [
                        "protective of",
                        "protective towards",
                        "feel protective",
                        "overly protective"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий сильное желание оградить кого-либо от опасности; проявляющий заботу и стремление защитить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оберега́ющий",
                                "target_lang_sense_clarification": "заботливый, стремящийся защитить близких"
                            },
                            {
                                "target_lang_word": "охра́нный",
                                "target_lang_sense_clarification": "об инстинктах или поведении животных"
                            },
                            {
                                "target_lang_word": "покрови́тельственный",
                                "target_lang_sense_clarification": "об отношении или тоне"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b2c49c2-210c-3663-ae05-6c7e2a65ff51",
                    "sense_definition": "To turn into a jelly-like substance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "transformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sauce will <w>gelatinize</w> as it cools in the refrigerator.",
                            "target_lang_translations": {
                                "ru": "Соус <w>зажелируется</w>, когда остынет в холодильнике."
                            }
                        },
                        {
                            "text": "We must allow the mixture to <w>gelatinize</w> before serving the dessert.",
                            "target_lang_translations": {
                                "ru": "Мы должны дать смеси <w>зажелироваться</w> перед подачей десерта."
                            }
                        }
                    ],
                    "synonyms": [
                        "gel",
                        "congeal",
                        "set",
                        "jell"
                    ],
                    "antonyms": [
                        "melt",
                        "liquefy"
                    ],
                    "common_phrases": [
                        "begin to gelatinize",
                        "allow to gelatinize"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превращаться в желеобразную массу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желатинироваться",
                                "target_lang_sense_clarification": "превращаться в желе"
                            },
                            {
                                "target_lang_word": "желироваться",
                                "target_lang_sense_clarification": "кулинарный термин"
                            },
                            {
                                "target_lang_word": "застудневать",
                                "target_lang_sense_clarification": "становиться студнем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b79707a-b965-3f4c-84d9-b370df479f01",
                    "sense_definition": "To cause a substance to become like jelly or gelatin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "transformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The high temperature helps to <w>gelatinize</w> the starches in the grain.",
                            "target_lang_translations": {
                                "ru": "Высокая температура помогает <w>клейстеризовать</w> крахмал в зерне."
                            }
                        },
                        {
                            "text": "You need to boil the liquid to <w>gelatinize</w> the ingredients properly.",
                            "target_lang_translations": {
                                "ru": "Вам нужно вскипятить жидкость, чтобы правильно <w>желатинировать</w> ингредиенты."
                            }
                        }
                    ],
                    "synonyms": [
                        "jellify",
                        "gel"
                    ],
                    "antonyms": [
                        "liquefy"
                    ],
                    "common_phrases": [
                        "gelatinize the starch",
                        "gelatinize the mixture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превращать вещество в желе или желатин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желатинировать",
                                "target_lang_sense_clarification": "превращать в желе"
                            },
                            {
                                "target_lang_word": "клейстеризовать",
                                "target_lang_sense_clarification": "о крахмале"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc6ad630-362f-3934-862f-c0907efa2640",
                    "sense_definition": "To cover or treat something with gelatin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "coating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manufacturer uses a machine to <w>gelatinize</w> the film base.",
                            "target_lang_translations": {
                                "ru": "Производитель использует машину, чтобы <w>покрыть желатином</w> основу пленки."
                            }
                        },
                        {
                            "text": "It is difficult to <w>gelatinize</w> the surface evenly by hand.",
                            "target_lang_translations": {
                                "ru": "Трудно равномерно <w>покрыть желатином</w> поверхность вручную."
                            }
                        }
                    ],
                    "synonyms": [
                        "coat"
                    ],
                    "common_phrases": [
                        "gelatinize the surface"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать или обрабатывать что-либо желатином."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать желатином",
                                "target_lang_sense_clarification": "наносить слой"
                            },
                            {
                                "target_lang_word": "желатинировать",
                                "target_lang_sense_clarification": "обрабатывать поверхность"
                            }
                        ]
                    }
                }
            ]
        }
    ]

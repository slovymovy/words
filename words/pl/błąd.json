            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2b2cebd6-defb-3ca9-8cd3-7b57dc11c634",
                    "sense_definition": "Niezgodność z obowiązującymi regułami, zasadami pisowni, gramatyki lub faktami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mistake_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W twoim wypracowaniu znalazłem tylko jeden <w>błąd</w> ortograficzny.",
                            "target_lang_translations": {
                                "ru": "В твоем сочинении я нашел только одну орфографическую <w>ошибку</w>."
                            }
                        },
                        {
                            "text": "To nie jest prawda, w obliczeniach musi być jakiś <w>błąd</w>.",
                            "target_lang_translations": {
                                "ru": "Это неправда, в расчетах должна быть какая-то <w>ошибка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomyłka",
                        "omyłka",
                        "byk",
                        "babol"
                    ],
                    "common_phrases": [
                        "błąd ortograficzny",
                        "błąd językowy",
                        "zrobić błąd",
                        "rażący błąd"
                    ],
                    "target_lang_definitions": {

                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'baal' (bale).",
                        "ru": "Множественное число от слова 'baal' (тюк, кипа)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bales",
                                "target_lang_sense_clarification": "bundles of material"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тюки",
                                "target_lang_sense_clarification": "упакованные связки сена, соломы и т.п."
                            },
                            {
                                "target_lang_word": "кипы",
                                "target_lang_sense_clarification": "большие связки хлопка, бумаги или ткани"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "31c55f39-bca4-3aa2-9a84-99f16a26285a",
                    "sense_definition": "Находиться в вертикальном положении, опираясь на ягодицы (о человеке) или задние конечности (о животном); занимать место для сидения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я люблю <w>сидеть</w> у окна и смотреть на улицу.",
                            "target_lang_translations": {
                                "en": "I like to <w>sit</w> by the window and look at the street."
                            }
                        },
                        {
                            "text": "Ученики должны тихо <w>сидеть</w> за партами во время урока.",
                            "target_lang_translations": {
                                "en": "Students must <w>sit</w> quietly at their desks during the lesson."
                            }
                        },
                        {
                            "text": "Маленькая птичка <w>сидела</w> на ветке дерева.",
                            "target_lang_translations": {
                                "en": "A little bird <w>sat</w> on a tree branch."
                            }
                        }
                    ],
                    "antonyms": [
                        "стоять",
                        "лежать"
                    ],
                    "common_phrases": [
                        "сидеть на стуле",
                        "сидеть за столом",
                        "сидеть прямо"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in an upright position resting on the buttocks; to occupy a seat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sit",
                                "target_lang_sense_clarification": "general physical action"
                            },
                            {
                                "target_lang_word": "be seated",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4eb28bc7-7b93-3496-b985-4906f89b60f3",
                    "sense_definition": "Находиться, пребывать где-либо (обычно в помещении), оставаться на месте; проводить время дома.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такую плохую погоду лучше <w>сидеть</w> дома.",
                            "target_lang_translations": {
                                "en": "In such bad weather, it is better to <w>stay</w> home."
                            }
                        },
                        {
                            "text": "Мы весь вечер <w>сидели</w> в гостях и пили чай.",
                            "target_lang_translations": {
                                "en": "We <w>stayed</w> at our friends' place all evening drinking tea."
                            }
                        }
                    ],
                    "synonyms": [
                        "находиться",
                        "пребывать",
                        "оставаться"
                    ],
                    "antonyms": [
                        "гулять",
                        "уходить"
                    ],
                    "common_phrases": [
                        "сидеть дома",
                        "сидеть в гостях",
                        "сидеть на месте"
                    ],
                    "target_lang_definitions": {
                        "en": "To stay or remain in a specific place, usually indoors."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stay",
                                "target_lang_sense_clarification": "remain in a place (e.g., stay home)"
                            },
                            {
                                "target_lang_word": "sit",
                                "target_lang_sense_clarification": "spend time in a place (often inactive)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4075151f-3872-3e8c-bbb9-d7982d527e7c",
                    "sense_definition": "Заниматься какой-либо работой или делом (часто требующим сидячего положения); проводить время за использованием чего-либо (компьютера, интернета).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "activity_occupation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат может часами <w>сидеть</w> за компьютером и играть.",
                            "target_lang_translations": {
                                "en": "My brother can <w>sit</w> at the computer and play for hours."
                            }
                        },
                        {
                            "text": "Студенты всю ночь <w>сидели</w> над учебниками перед экзаменом.",
                            "target_lang_translations": {
                                "en": "Students <w>sat</w> over their textbooks all night before the exam."
                            }
                        },
                        {
                            "text": "Бабушка часто <w>сидит</w> с внуками, когда родители на работе.",
                            "target_lang_translations": {
                                "en": "Grandma often <w>babysits</w> the grandchildren when the parents are at work."
                            }
                        }
                    ],
                    "synonyms": [
                        "заниматься",
                        "работать"
                    ],
                    "common_phrases": [
                        "сидеть в интернете",
                        "сидеть за работой",
                        "сидеть с ребёнком"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend time engaged in a task or looking after someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sit",
                                "target_lang_sense_clarification": "at a desk, computer, or over work"
                            },
                            {
                                "target_lang_word": "babysit",
                                "target_lang_sense_clarification": "look after children"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4453062-020f-3402-9d4a-339ba8624921",
                    "sense_definition": "Подходить по размеру, хорошо облегать фигуру (об одежде).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_fit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это новое платье отлично <w>сидит</w> на тебе.",
                            "target_lang_translations": {
                                "en": "This new dress <w>fits</w> you perfectly."
                            }
                        },
                        {
                            "text": "Костюм должен <w>сидеть</w> идеально, чтобы выглядеть элегантно.",
                            "target_lang_translations": {
                                "en": "The suit must <w>fit</w> perfectly to look elegant."
                            }
                        }
                    ],
                    "synonyms": [
                        "подходить",
                        "облегать",
                        "смотреться"
                    ],
                    "antonyms": [
                        "висеть"
                    ],
                    "common_phrases": [
                        "хорошо сидеть",
                        "плохо сидеть",
                        "сидеть как влитой"
                    ],
                    "target_lang_definitions": {
                        "en": "To fit or suit the body shape (of clothing)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "be the right size or shape"
                            },
                            {
                                "target_lang_word": "sit",
                                "target_lang_sense_clarification": "hang or drape on the body"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5af84000-1913-3c6c-a43d-84656c5a797a",
                    "sense_definition": "Соблюдать определённый режим (обычно питания) или постоянно употреблять что-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "regimen_dependence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед отпуском многие девушки начинают <w>сидеть</w> на строгой диете.",
                            "target_lang_translations": {
                                "en": "Before vacation, many girls start to <w>go on</w> a strict diet."
                            }
                        },
                        {
                            "text": "Пациент вынужден постоянно <w>сидеть</w> на лекарствах из-за болезни.",
                            "target_lang_translations": {
                                "en": "The patient is forced to constantly <w>be on</w> medication due to illness."
                            }
                        }
                    ],
                    "synonyms": [
                        "соблюдать"
                    ],
                    "common_phrases": [
                        "сидеть на диете",
                        "сидеть на таблетках"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To follow a strict regimen or be dependent on medication."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be on",
                                "target_lang_sense_clarification": "a diet or medication"
                            },
                            {
                                "target_lang_word": "stick to",
                                "target_lang_sense_clarification": "a diet or regimen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd6033de-42e8-3dbd-98d7-25c3172662f1",
                    "sense_definition": "Отбывать наказание в местах лишения свободы (в тюрьме).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "imprisonment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Преступник будет <w>сидеть</w> в тюрьме за свои злодеяния.",
                            "target_lang_translations": {
                                "en": "The criminal will <w>serve time</w> in prison for his misdeeds."
                            }
                        },
                        {
                            "text": "Ему пришлось <w>сидеть</w> пять лет, прежде чем он вышел на свободу.",
                            "target_lang_translations": {
                                "en": "He had to <w>serve</w> five years before he was released."
                            }
                        }
                    ],
                    "synonyms": [
                        "отбывать срок"
                    ],
                    "common_phrases": [
                        "сидеть в тюрьме",
                        "сидеть за решёткой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard term in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To serve a sentence in prison."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve time",
                                "target_lang_sense_clarification": "standard term"
                            },
                            {
                                "target_lang_word": "do time",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "be in prison",
                                "target_lang_sense_clarification": "state of imprisonment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc85fdba-84d6-3bf8-9ddb-d45bdaf0dd08",
                    "sense_definition": "Находиться внутри чего-либо, быть прочно укреплённым или застрявшим в чём-либо (о предметах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_fixation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый гвоздь так крепко <w>сидит</w> в стене, что его невозможно вытащить.",
                            "target_lang_translations": {
                                "en": "The old nail <w>is stuck</w> so firmly in the wall that it is impossible to pull out."
                            }
                        },
                        {
                            "text": "Заноза <w>сидит</w> глубоко под кожей и причиняет боль.",
                            "target_lang_translations": {
                                "en": "The splinter <w>is embedded</w> deep under the skin and causing pain."
                            }
                        }
                    ],
                    "synonyms": [
                        "держаться",
                        "застрять"
                    ],
                    "antonyms": [
                        "болтаться"
                    ],
                    "common_phrases": [
                        "сидеть крепко",
                        "сидеть глубоко"
                    ],
                    "target_lang_definitions": {
                        "en": "To be firmly fixed, stuck, or embedded inside something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be stuck",
                                "target_lang_sense_clarification": "unable to move"
                            },
                            {
                                "target_lang_word": "be embedded",
                                "target_lang_sense_clarification": "fixed deeply inside"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
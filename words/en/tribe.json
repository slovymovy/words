                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I have a meeting scheduled with Mr. <w>Tribe</w> this afternoon.",
                            "target_lang_translations": {

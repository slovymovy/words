    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f0c451e8-7fa1-3643-b4c6-4a0a0266b9db",
                    "sense_definition": "Een gedomesticeerd zoogdier dat afstamt van de wolf en vaak als huisdier wordt gehouden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze trouwe <w>hond</w> wacht elke dag bij de deur.",
                            "target_lang_translations": {
                                "ru": "Наша верная <w>собака</w> ждёт у двери каждый день."
                            }
                        },
                        {
                            "text": "Je moet de <w>hond</w> uitlaten voordat het donker wordt.",
                            "target_lang_translations": {
                                "ru": "Нужно выгулять <w>собаку</w>, пока не стемнело."
                            }
                        },
                        {
                            "text": "In de verte hoorde ik een <w>hond</w> luid blaffen.",
                            "target_lang_translations": {
                                "ru": "Вдали я услышал, как громко лает <w>собака</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "viervoeter"
                    ],
                    "antonyms": [
                        "kat"
                    ],
                    "common_phrases": [
                        "de hond uitlaten",
                        "blaffende honden bijten niet",
                        "bekend als de bonte hond",
                        "pas op voor de hond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одомашненное млекопитающее, произошедшее от волка, которое часто держат в качестве домашнего животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собака",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "пёс",
                                "target_lang_sense_clarification": "разговорное, часто о самце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b37f596-a3e1-3c0d-bd22-2555d502deb5",
                    "sense_definition": "Een scheldwoord voor een gemeen, onbetrouwbaar of minderwaardig persoon.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft me bedrogen, die vuile <w>hond</w>!",
                            "target_lang_translations": {
                                "ru": "Он обманул меня, этот грязный <w>пёс</w>!"
                            }
                        },
                        {
                            "text": "Gedraag je niet als een <w>hond</w> en wees gewoon eerlijk.",
                            "target_lang_translations": {
                                "ru": "Не веди себя как <w>собака</w> и будь просто честным."
                            }
                        }
                    ],
                    "synonyms": [
                        "schoft",
                        "hufter"
                    ],
                    "common_phrases": [
                        "vuile hond"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as an insult"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ругательство в адрес подлого, ненадежного или недостойного человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собака",
                                "target_lang_sense_clarification": "бранное слово"
                            },
                            {
                                "target_lang_word": "пёс",
                                "target_lang_sense_clarification": "бранное слово, негодяй"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae664f39-f075-39bf-802d-5955676a5f63",
                    "sense_definition": "Een oude oppervlaktemaat van 100 roeden, ongeveer 0,14 hectare.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer verkocht twee <w>hond</w> land aan zijn buurman.",
                            "target_lang_translations": {
                                "ru": "Фермер продал два <w>хонда</w> земли своему соседу."
                            }
                        },
                        {
                            "text": "In oude documenten wordt de grootte van het perceel in <w>hond</w> uitgedrukt.",
                            "target_lang_translations": {
                                "ru": "В старых документах размер участка выражается в <w>хондах</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical unit of measurement"
                        }
                    ],
                    "target_lang_definitions": {

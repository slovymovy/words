                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подошва",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "ступня",
                                "target_lang_sense_clarification": "часть ноги"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1a52a9d4-e252-378b-b2d8-a4d508361a35",
                    "sense_definition": "Vervoeging van <w>planten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd en gebiedende wijs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form_planten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>plant</w> een nieuwe boom in de voortuin.",
                            "target_lang_translations": {
                                "ru": "Он <w>сажает</w> новое дерево в палисаднике."
                            }
                        },
                        {
                            "text": "Ik <w>plant</w> de zaadjes voorzichtig in de aarde.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>сажаю</w> семена в землю."
                            }
                        }
                    ],
                    "common_phrases": [

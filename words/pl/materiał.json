                {
                    "sense_id": "e0a3d4e4-ee96-399e-9d90-c26e07381451",
                    "sense_definition": "Przedmioty i przybory niezbędne do wykonywania jakiejś pracy lub działalności (zwykle w liczbie mnogiej).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma zamówiła nowe <w>materiały</w> biurowe, takie jak papier i długopisy.",
                            "target_lang_translations": {
                                "ru": "Фирма заказала новые офисные <w>материалы</w> (канцелярию), такие как бумага и ручки."
                            }
                        },
                        {
                            "text": "Kosztowne <w>materiały</w> eksploatacyjne obciążają budżet naszego działu.",
                            "target_lang_translations": {
                                "ru": "Дорогостоящие расходные <w>материалы</w> обременяют бюджет нашего отдела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "materiały biurowe",
                        "materiały piśmienne",
                        "materiały eksploatacyjne",
                        "materiały promocyjne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предметы и принадлежности, необходимые для выполнения какой-либо работы или деятельности (обычно во мн. ч.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материалы",
                                "target_lang_sense_clarification": "расходные, офисные"
                            },
                            {
                                "target_lang_word": "принадлежности",
                                "target_lang_sense_clarification": "инструменты, средства"
                            }
                        ]

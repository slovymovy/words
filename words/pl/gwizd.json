    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7eff69c5-ca09-3d54-be22-46918f0d8033",
                    "sense_definition": "Wysoki, ostry dźwięk wywołany szybkim ruchem powietrza przelatującego przez wąski otwór (np. usta lub gwizdek).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia zakończył mecz głośnym <w>gwizdem</w>.",
                            "target_lang_translations": {
                                "ru": "Судья закончил матч громким <w>свистом</w>."
                            }
                        },
                        {
                            "text": "Z oddali słychać było przeciągły <w>gwizd</w> nadjeżdżającego pociągu.",
                            "target_lang_translations": {
                                "ru": "Издалека был слышен протяжный <w>свист</w> приближающегося поезда."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwizdor",
                        "gwizduch",
                        "gwizduch"
                    ],
                    "common_phrases": [
                        "gwizd sędziego",
                        "gwizd pociągu",
                        "przeciągły gwizd",
                        "gwizd wiatru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий, резкий звук, возникающий при быстром движении воздуха через узкое отверстие (например, губы или свисток)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свист",
                                "target_lang_sense_clarification": "звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7fdadc3-1658-3e44-a21a-13ed72a80d45",
                    "sense_definition": "Głos wydawany przez niektóre gatunki ptaków (np. słonki i jarząbka).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony myśliwy potrafi rozpoznać <w>gwizd</w> jarząbka w lesie.",
                            "target_lang_translations": {
                                "ru": "Опытный охотник может распознать <w>свист</w> рябчика в лесу."
                            }
                        },
                        {
                            "text": "Wiosną można usłyszeć charakterystyczny <w>gwizd</w> słonki podczas lotu godowego.",
                            "target_lang_translations": {
                                "ru": "Весной можно услышать характерный <w>свист</w> вальдшнепа во время брачного полета."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Голос, издаваемый некоторыми видами птиц (например, вальдшнепами и рябчиками)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свист",
                                "target_lang_sense_clarification": "пение птиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "441313fb-f6d6-388f-abc9-0135ba0c25ec",
                    "sense_definition": "Pysk dzika (terminologia łowiecka).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzik uniósł swój długi <w>gwizd</w>, wietrząc zagrożenie.",
                            "target_lang_translations": {
                                "ru": "Кабан поднял свое длинное <w>рыло</w>, чуя опасность."
                            }
                        },
                        {
                            "text": "Na śniegu było widać ślady, gdzie zwierzę ryło ziemię <w>gwizdem</w>.",
                            "target_lang_translations": {
                                "ru": "На снегу были видны следы, где животное рыло землю <w>рылом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwizduch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рыло дикого кабана (охотничья терминология)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыло",
                                "target_lang_sense_clarification": "часть тела кабана"
                            },
                            {
                                "target_lang_word": "пятак",
                                "target_lang_sense_clarification": "разг. нос свиньи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbc5c23a-442b-368c-8b36-0ad30b064d0f",
                    "sense_definition": "Postać przebierańca w stroju obrzędowym, odwiedzająca domy w okresie Bożego Narodzenia (zwyczaj ludowy).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wigilijny wieczór do izby wszedł <w>gwizd</w> i zaczął przepytywać dzieci.",
                            "target_lang_translations": {
                                "ru": "В рождественский вечер в избу вошел <w>ряженый</w> и начал расспрашивать детей."
                            }
                        },
                        {
                            "text": "Tradycyjny <w>gwizd</w> nosił maskę i kożuch wywrócony na drugą stronę.",
                            "target_lang_translations": {
                                "ru": "Традиционный <w>ряженый</w> носил маску и вывернутый наизнанку тулуп."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwizdor",
                        "gwizduch"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "etnografia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряженый персонаж в обрядовом костюме, посещающий дома в период Рождества (народный обычай)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ряженый",
                                "target_lang_sense_clarification": "персонаж фольклора"
                            },
                            {
                                "target_lang_word": "Гвизд",
                                "target_lang_sense_clarification": "имя собственное, этногр."
                            }
                        ]
                    }
                }
            ]

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "949e5e2d-9531-3806-a407-73ce80f6354a",
                    "sense_definition": "Podać czyjeś słowa, fragment tekstu, fakty lub przykłady w celu potwierdzenia czegoś lub zilustrowania tematu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_quote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W swojej pracy magisterskiej student <przytoczył> wyniki najnowszych badań naukowych.",
                            "target_lang_translations": {
                                "ru": "В своей магистерской диссертации студент <привел> результаты новейших научных исследований."
                            }
                        },
                        {
                            "text": "Chciałbym <przytoczyć> słowa znanego poety, które idealnie pasują do tej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <привести> слова известного поэта, которые идеально подходят к этой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacytować",
                        "wymienić",
                        "wyliczyć"
                    ],
                    "common_phrases": [
                        "przytoczyć argument",
                        "przytoczyć przykład",
                        "przytoczyć słowa",
                        "przytoczyć fakt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести чьи-то слова, текст, факты или примеры для подтверждения чего-либо или иллюстрации темы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привести",
                                "target_lang_sense_clarification": "пример, аргумент, факт"
                            },
                            {
                                "target_lang_word": "процитировать",
                                "target_lang_sense_clarification": "дословно повторить чьи-то слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d218fbf-55eb-30c0-9ff7-1659d8b7ba74",
                    "sense_definition": "Przemieścić coś ciężkiego lub okrągłego w określone miejsce, wprawiając to w ruch obrotowy (tocząc).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_roll",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy z wielkim wysiłkiem <przytoczyli> ciężki głaz pod bramę wjazdową.",
                            "target_lang_translations": {
                                "ru": "Рабочие с большим усилием <прикатили> тяжелый валун к въездным воротам."
                            }
                        },
                        {
                            "text": "Dzieci wspólnie <przytoczyły> ogromną kulę śniegu na środek podwórka.",
                            "target_lang_translations": {
                                "ru": "Дети вместе <прикатили> огромный снежный ком на середину двора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przytoczyć kamień",
                        "przytoczyć beczkę",
                        "przytoczyć kłodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить что-то тяжелое или круглое в определенное место, катя его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикатить",
                                "target_lang_sense_clarification": "переместить качением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1048923e-c324-3431-816f-e773d03c11f5",
                    "sense_definition": "W formie zwrotnej „przytoczyć się”: zbliżyć się do jakiegoś miejsca, tocząc się po powierzchni (zwykle o przedmiotach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_roll",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piłka plażowa niesiona wiatrem <przytoczyła> się pod nasze koce.",
                            "target_lang_translations": {
                                "ru": "Пляжный мяч, гонимый ветром, <прикатился> к нашим одеялам."
                            }
                        },
                        {
                            "text": "Oderwane koło od wozu <przytoczyło> się aż pod płot sąsiada.",
                            "target_lang_translations": {
                                "ru": "Оторвавшееся от телеги колесо <прикатилось> прямо к забору соседа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piłka się przytoczyła",
                        "przytoczyć się pod nogi"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (wymaga „się”)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизиться к какому-либо месту, катясь по поверхности (обычно о предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикатиться",
                                "target_lang_sense_clarification": "приблизиться катясь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76748b6a-0ea8-36e2-ab15-660864a3c8c7",
                    "sense_definition": "W formie zwrotnej „przytoczyć się”: dotrzeć gdzieś powoli, ociężale lub z trudem (o ludziach lub pojazdach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_roll",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po całodniowej wędrówce turyści ledwo <przytoczyli> się do schroniska na szczycie.",
                            "target_lang_translations": {
                                "ru": "После целого дня похода туристы едва <притащились> к приюту на вершине."
                            }
                        },
                        {
                            "text": "Stary autobus z trudem <przytoczył> się na przystanek w mroźny poranek.",
                            "target_lang_translations": {
                                "ru": "Старый автобус с трудом <прикатил> на остановку морозным утром."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ledwo się przytoczyć",
                        "przytoczyć się do domu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (wymaga „się”)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Добраться куда-либо медленно, тяжело или с трудом (о людях или транспорте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притащиться",
                                "target_lang_sense_clarification": "разговорное; прийти с трудом"
                            },
                            {
                                "target_lang_word": "прикатить",
                                "target_lang_sense_clarification": "разговорное; приехать (о транспорте)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "44fc64fe-959d-3738-986a-c2526aff3e5d",
                    "sense_definition": "Simple past and past participle of <w>curse</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witch <w>cursed</w> the prince to sleep for a hundred years.",
                            "target_lang_translations": {
                                "ru": "Ведьма <w>прокляла</w> принца, погрузив его в сон на сто лет."
                            }
                        },
                        {
                            "text": "He <w>cursed</w> under his breath when he dropped the heavy box.",
                            "target_lang_translations": {
                                "ru": "Он тихо <w>выругался</w>, когда уронил тяжелую коробку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cursed at",
                        "cursed the day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола <w>curse</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проклял",
                                "target_lang_sense_clarification": "наложил проклятие"
                            },
                            {
                                "target_lang_word": "выругался",
                                "target_lang_sense_clarification": "произнес ругательство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7ef0b40b-3c06-3ee4-8708-789436e3661f",
                    "sense_definition": "Suffering from a curse, bad luck, or divine harm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "afflicted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers believed the old castle was <w>cursed</w> by a powerful sorcerer.",
                            "target_lang_translations": {
                                "ru": "Жители деревни верили, что старый замок был <w>проклят</w> могущественным колдуном."
                            }
                        },
                        {
                            "text": "After losing his job and his car, he felt like a <w>cursed</w> man.",
                            "target_lang_translations": {
                                "ru": "Потеряв работу и машину, он чувствовал себя <w>проклятым</w> человеком."
                            }
                        }
                    ],
                    "synonyms": [
                        "accursed",
                        "doomed"
                    ],
                    "antonyms": [
                        "blessed"
                    ],
                    "common_phrases": [
                        "cursed earth",
                        "cursed treasure",
                        "cursed existence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся под действием проклятия, преследуемый неудачами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проклятый",
                                "target_lang_sense_clarification": "подвергшийся проклятию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81b3e56d-ffc2-3301-99a0-445207e9fe9f",
                    "sense_definition": "Strange, disturbing, or confusing in a funny or unsettling way.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sent me a <w>cursed</w> image of a cat with human teeth.",
                            "target_lang_translations": {
                                "ru": "Он прислал мне <w>проклятую</w> картинку кота с человеческими зубами."
                            }
                        },
                        {
                            "text": "The atmosphere in the empty playground at night felt completely <w>cursed</w>.",
                            "target_lang_translations": {
                                "ru": "Атмосфера на пустой детской площадке ночью казалась совершенно <w>жуткой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bizarre",
                        "macabre"
                    ],
                    "common_phrases": [
                        "cursed image",
                        "cursed comment"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Странный, тревожный или сбивающий с толку (интернет-сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проклятый",
                                "target_lang_sense_clarification": "в контексте мемов"
                            },
                            {
                                "target_lang_word": "жуткий",
                                "target_lang_sense_clarification": "пугающий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b22620fe-3137-3e78-999b-a2d89eda2f08",
                    "sense_definition": "Hateful or annoying; used to express anger.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "afflicted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot get this <w>cursed</w> engine to start no matter what I do!",
                            "target_lang_translations": {
                                "ru": "Я никак не могу завести этот <w>проклятый</w> двигатель, что бы я ни делал!"
                            }
                        },
                        {
                            "text": "That <w>cursed</w> bird keeps stealing the seeds from my garden.",
                            "target_lang_translations": {
                                "ru": "Эта <w>проклятая</w> птица продолжает таскать семена из моего сада."
                            }
                        }
                    ],
                    "synonyms": [
                        "accursed"
                    ],
                    "common_phrases": [
                        "cursed nuisance",
                        "this cursed weather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ненавистный или раздражающий; используется для выражения гнева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проклятый",
                                "target_lang_sense_clarification": "ненавистный"
                            },
                            {
                                "target_lang_word": "чёртов",
                                "target_lang_sense_clarification": "разговорное, ругательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b68153ca-202f-360a-816f-26a8e2d04ba5",
                    "sense_definition": "Ill-tempered or quarrelsome.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The play features a <w>cursed</w> woman who eventually learns to be kind.",
                            "target_lang_translations": {
                                "ru": "В пьесе рассказывается о <w>сварливой</w> женщине, которая в конце концов учится быть доброй."
                            }
                        },
                        {
                            "text": "He was known for his <w>cursed</w> tongue and inability to keep friends.",
                            "target_lang_translations": {
                                "ru": "Он был известен своим <w>злым</w> языком и неумением сохранять дружбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "shrewish"
                    ],
                    "traits": [
                        {

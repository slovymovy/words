            "senses": [
                {
                    "sense_id": "039d281b-b93e-33d4-9900-74a88d1b4595",
                    "sense_definition": "forma żeńska liczby pojedynczej przymiotnika <w>królowy</w>",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wola <królowa> była dla rycerzy świętym prawem.",
                            "target_lang_translations": {
                                "ru": "<w>Королева</w> воля была для рыцарей священным законом."
                            }
                        },
                        {
                            "text": "Ta ziemia to dawna własność <królowa>.",
                            "target_lang_translations": {
                                "ru": "Эта земля — давняя <w>королева</w> собственность."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "przymiotnik dzierżawczy oznaczający przynależność do króla"
                        },
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская форма единственного числа прилагательного \"królowy\" (принадлежащий королю)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королева",
                                "target_lang_sense_clarification": "притяжательное прилагательное (устар.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c37333c4-4dab-35d3-a66f-d91951ba8c55",
                    "sense_definition": "Lovable, charming, or attractive, especially in a youthful, sweet, or dainty way.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pleasing_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look at that <w>cute</w> puppy playing in the garden.",
                            "target_lang_translations": {
                                "ru": "Посмотри на этого <w>милого</w> щенка, играющего в саду."
                            }
                        },
                        {
                            "text": "She wore a <w>cute</w> dress with flowers on it to the party.",
                            "target_lang_translations": {
                                "ru": "Она надела на вечеринку <w>хорошенькое</w> платье в цветочек."
                            }
                        }
                    ],
                    "synonyms": [
                        "pretty",
                        "cuddly animals",
                        "having features mainly attributed to infants and small",
                        "shrewd",
                        "clever"
                    ],
                    "antonyms": [
                        "ugly"
                    ],
                    "common_phrases": [
                        "so cute",
                        "cute baby",
                        "cute little",
                        "very cute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очаровательный, привлекательный или вызывающий умиление, особенно если речь идет о чем-то маленьком, юном или нежном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "милый",
                                "target_lang_sense_clarification": "вызывающий умиление"
                            },
                            {
                                "target_lang_word": "хорошенький",
                                "target_lang_sense_clarification": "привлекательный (часто о детях или женщинах)"
                            },
                            {
                                "target_lang_word": "симпатичный",
                                "target_lang_sense_clarification": "приятный на вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d3f7822-acaf-373a-bd82-beda086ba983",
                    "sense_definition": "Sexually attractive or good-looking.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pleasing_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I met a really <w>cute</w> guy at the coffee shop yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера в кофейне я встретила очень <w>симпатичного</w> парня."
                            }
                        },
                        {
                            "text": "She thinks the lead singer of the band is very <w>cute</w>.",
                            "target_lang_translations": {
                                "ru": "Она считает солиста группы очень <w>симпатичным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pretty"
                    ],
                    "antonyms": [
                        "ugly"
                    ],
                    "common_phrases": [
                        "cute guy",
                        "cute girl"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Common in casual conversation."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сексуально привлекательный или красивый (обычно о людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "симпатичный",
                                "target_lang_sense_clarification": "привлекательный внешне"
                            },
                            {
                                "target_lang_word": "милый",
                                "target_lang_sense_clarification": "нравящийся, приятный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf5c6440-c3d3-3f42-8e39-ffc81001341d",
                    "sense_definition": "Trying to be clever or charming in a way that is annoying, disrespectful, or contrived.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cleverness_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't get <w>cute</w> with me when I ask you a serious question.",
                            "target_lang_translations": {
                                "ru": "Не <w>умничай</w>, когда я задаю тебе серьезный вопрос."
                            }
                        },
                        {
                            "text": "He thought he was being <w>cute</w> by hiding my keys, but I was not amused.",
                            "target_lang_translations": {
                                "ru": "Он думал, что это <w>остроумно</w> — спрятать мои ключи, но мне было не смешно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "don't get cute",
                        "trying to be cute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытающийся казаться остроумным или хитрым, но выглядящий при этом раздражающе, неуважительно или неестественно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умничающий",
                                "target_lang_sense_clarification": "пытающийся казаться умным (негативно)"
                            },
                            {
                                "target_lang_word": "дерзкий",
                                "target_lang_sense_clarification": "нахальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bec0a885-b76a-315a-8bb8-62041ef39f71",
                    "sense_definition": "Clever, elegant, or interesting, often referring to a solution, trick, or idea.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cleverness_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mathematician found a <w>cute</w> way to solve the complex equation.",
                            "target_lang_translations": {
                                "ru": "Математик нашел <w>изящный</w> способ решения сложного уравнения."
                            }
                        },
                        {
                            "text": "It was a <w>cute</w> trick, but it won't work in every situation.",
                            "target_lang_translations": {
                                "ru": "Это был <w>хитрый</w> трюк, но он сработает не в каждой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "clever"
                    ],
                    "common_phrases": [
                        "cute trick",
                        "cute solution"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изобретательный, элегантный или интересный (часто о решении, трюке или идее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изящный",
                                "target_lang_sense_clarification": "элегантный (о решении)"
                            },
                            {
                                "target_lang_word": "остроумный",
                                "target_lang_sense_clarification": "изобретательный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47bb8775-0d58-33bd-83aa-72d3a43569ac",
                    "sense_definition": "Mentally keen, shrewd, or discerning.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cleverness_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The merchant was <w>cute</w> enough to hide his money before the thieves arrived.",
                            "target_lang_translations": {
                                "ru": "Торговец был достаточно <w>сообразителен</w>, чтобы спрятать деньги до прихода воров."
                            }
                        },
                        {
                            "text": "He is a <w>cute</w> observer of human nature and notices everything.",
                            "target_lang_translations": {
                                "ru": "Он <w>проницательный</w> наблюдатель человеческой натуры и замечает всё."
                            }
                        }
                    ],
                    "synonyms": [
                        "shrewd",
                        "clever"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Short for 'acute'; rarely used in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проницательный, сообразительный или догадливый (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проницательный",
                                "target_lang_sense_clarification": "наблюдательный"
                            },
                            {
                                "target_lang_word": "сообразительный",
                                "target_lang_sense_clarification": "догадливый"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "11a2cc42-088d-3aa5-9a14-039d8fc7e713",
                    "sense_definition": "Het gevoel hebbend dat alles naar wens is en dat je niets meer nodig hebt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben erg <w>tevreden</w> met mijn nieuwe fiets.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>доволен</w> своим новым велосипедом.",
                                "en": "I am very <w>satisfied</w> with my new bike."
                            }
                        },
                        {
                            "text": "De leraar was <w>tevreden</w> over de resultaten van de toets.",
                            "target_lang_translations": {
                                "ru": "Учитель был <w>доволен</w> результатами теста.",
                                "en": "The teacher was <w>satisfied</w> with the results of the test."
                            }
                        }
                    ],
                    "synonyms": [
                        "voldaan"
                    ],
                    "antonyms": [
                        "ontevreden"
                    ],
                    "common_phrases": [
                        "tevreden zijn met",
                        "tevreden over",
                        "erg tevreden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство, когда всё идёт так, как хочется, и больше ничего не нужно.",
                        "en": "Feeling that everything is as desired and that nothing more is needed."
                    },
                    "translations": {

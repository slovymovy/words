            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec8b5899-a4bc-3f5f-b14c-6862518f9c2f",
                    "sense_definition": "Een persoon die ondeugend of grappig is, vaak op een slimme of charmante manier; iemand die streken uithaalt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rogue_joker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen moest lachen om de grappen van die kleine <w>schalk</w>.",
                            "target_lang_translations": {
                                "en": "Everyone had to laugh at the jokes of that little <w>rascal</w>.",
                                "ru": "Все смеялись над шутками этого маленького <w>проказника</w>."
                            }
                        },
                        {
                            "text": "Hij is een ware <w>schalk</w> die altijd probeert om mensen voor de gek te houden.",
                            "target_lang_translations": {
                                "en": "He is a true <w>rogue</w> who always tries to fool people.",
                                "ru": "Он настоящий <w>плут</w>, который всегда пытается одурачить людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "guit",
                        "schelm",
                        "schavuit",
                        "grapjas",
                        "grappenmaker",
                        "ondeugd",
                        "gladakker"
                    ],
                    "common_phrases": [

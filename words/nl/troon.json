    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24b2716a-808c-3e93-9946-351f62377c8c",
                    "sense_definition": "Een ceremoniële zetel waar een vorst of hoogwaardigheidsbekleder op zit tijdens officiële gelegenheden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "seat_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning zat tijdens de ceremonie op zijn gouden <w>troon</w>.",
                            "target_lang_translations": {
                                "en": "The king sat on his golden <w>throne</w> during the ceremony."
                            }
                        },
                        {
                            "text": "In de grote zaal stond een indrukwekkende <w>troon</w> bekleed met fluweel.",
                            "target_lang_translations": {
                                "en": "In the great hall stood an impressive <w>throne</w> covered in velvet."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de troon zitten",
                        "van de troon stoten"
                    ],
                    "target_lang_definitions": {
                        "en": "A ceremonial seat where a monarch or dignitary sits during official occasions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throne",
                                "target_lang_sense_clarification": "seat of a monarch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fee4137c-51de-3951-897c-f03dd6228660",
                    "sense_definition": "De koninklijke macht, waardigheid of het koningschap zelf.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "seat_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De prins is de eerste in de lijn van opvolging voor de <w>troon</w>.",
                            "target_lang_translations": {
                                "en": "The prince is first in the line of succession to the <w>throne</w>."
                            }
                        },
                        {
                            "text": "Hij deed onverwacht afstand van de <w>troon</w> ten gunste van zijn dochter.",
                            "target_lang_translations": {
                                "en": "He unexpectedly abdicated the <w>throne</w> in favor of his daughter."
                            }
                        }
                    ],
                    "common_phrases": [
                        "afstand doen van de troon",
                        "de troon bestijgen",
                        "aanspraak maken op de troon"
                    ],
                    "target_lang_definitions": {
                        "en": "Royal power, dignity, or the monarchy itself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throne",
                                "target_lang_sense_clarification": "royal power or monarchy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ce184a4-655d-3432-ae13-0477c8793ab6",
                    "sense_definition": "Een engel die behoort tot de derde orde van de engelenhiërarchie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de theologie wordt een <w>troon</w> beschreven als een drager van goddelijke gerechtigheid.",
                            "target_lang_translations": {
                                "en": "In theology, a <w>throne</w> is described as a bearer of divine justice."
                            }
                        },
                        {
                            "text": "De schilder beeldde de <w>troon</w> af met vurige wielen en vele ogen.",
                            "target_lang_translations": {
                                "en": "The painter depicted the <w>throne</w> with fiery wheels and many eyes."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An angel belonging to the third order of the angelic hierarchy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throne",
                                "target_lang_sense_clarification": "order of angels"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef4cdd55-15dd-3873-a3e1-b78713df5b7f",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>tronen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>troon</w> mijn bezoekers mee naar de bibliotheek om ze mijn collectie te tonen.",
                            "target_lang_translations": {
                                "en": "I <w>lead</w> my visitors along to the library to show them my collection."
                            }
                        },
                        {
                            "text": "Als een vorst <w>troon</w> ik boven de menigte uit op het balkon.",
                            "target_lang_translations": {
                                "en": "Like a monarch, I <w>sit enthroned</w> above the crowd on the balcony."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {

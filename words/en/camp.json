        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d457a3cb-b899-3f6f-b98f-5f6e3538dc16",
                    "sense_definition": "An outdoor area where people stay in tents or temporary shelters.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_camp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We pitched our tent at a <w>camp</w> near the river.",
                            "target_lang_translations": {
                                "ru": "Мы поставили палатку в <w>лагере</w> у реки."
                            }
                        },
                        {
                            "text": "The hikers returned to <w>camp</w> before it got dark.",
                            "target_lang_translations": {
                                "ru": "Туристы вернулись в <w>лагерь</w> до наступления темноты."
                            }
                        }
                    ],
                    "synonyms": [
                        "campy",
                        "campish"
                    ],
                    "common_phrases": [
                        "set up camp",
                        "break camp",
                        "base camp",
                        "make camp"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место на открытом воздухе, где люди живут в палатках или временных укрытиях."

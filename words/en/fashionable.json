    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a724e6e2-7d1b-36de-9f5f-6795662bff4f",
                    "sense_definition": "Characteristic of or influenced by a current popular trend or style.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "following_trend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She always wears <w>fashionable</w> clothes when she goes to work.",
                            "target_lang_translations": {
                                "ru": "Она всегда носит <w>модную</w> одежду, когда идет на работу."
                            }
                        },
                        {
                            "text": "It has become <w>fashionable</w> to drink herbal tea after dinner.",
                            "target_lang_translations": {
                                "ru": "Стало <w>модным</w> пить травяной чай после ужина."
                            }
                        }
                    ],
                    "synonyms": [
                        "stylish",
                        "trendy",
                        "chic",
                        "in fashion",
                        "smart",
                        "modish"
                    ],
                    "antonyms": [
                        "unfashionable"
                    ],
                    "common_phrases": [
                        "fashionable clothes",
                        "look fashionable",
                        "fashionable restaurant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерный для текущей популярной тенденции или стиля; следующий моде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модный",
                                "target_lang_sense_clarification": "об одежде, стиле"
                            },
                            {
                                "target_lang_word": "стильный",
                                "target_lang_sense_clarification": "обладающий стилем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d69e0922-28ac-32d3-a803-5cca07170737",
                    "sense_definition": "Established or favoured by custom or use; prevailing at a particular time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "following_trend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That economic theory is no longer <w>fashionable</w> among modern experts.",
                            "target_lang_translations": {
                                "ru": "Эта экономическая теория больше не считается <w>модной</w> среди современных экспертов."
                            }
                        },
                        {
                            "text": "He holds <w>fashionable</w> opinions that align with the current political climate.",
                            "target_lang_translations": {
                                "ru": "Он придерживается <w>модных</w> взглядов, которые соответствуют текущему политическому климату."
                            }
                        }
                    ],
                    "synonyms": [
                        "popular",
                        "current",
                        "prevailing"
                    ],
                    "antonyms": [
                        "unfashionable"
                    ],
                    "common_phrases": [
                        "fashionable idea",
                        "fashionable view",
                        "fashionable philosophy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общепринятый или популярный в определенное время (о взглядах, идеях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модный",
                                "target_lang_sense_clarification": "популярный в данный момент"
                            },
                            {
                                "target_lang_word": "популярный",
                                "target_lang_sense_clarification": "широко распространенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8e9e222-96d3-317e-86fb-5f409e55a797",
                    "sense_definition": "Associated with high society or wealthy people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They bought a house in a <w>fashionable</w> part of the city.",
                            "target_lang_translations": {
                                "ru": "Они купили дом в <w>фешенебельном</w> районе города."
                            }
                        },
                        {
                            "text": "She wanted to be accepted into <w>fashionable</w> society.",
                            "target_lang_translations": {
                                "ru": "Она хотела быть принятой в <w>светское</w> общество."
                            }
                        }
                    ],
                    "synonyms": [
                        "elite",
                        "high-class",
                        "genteel",
                        "exclusive"
                    ],
                    "antonyms": [
                        "unfashionable"
                    ],
                    "common_phrases": [
                        "fashionable society",
                        "fashionable resort",
                        "fashionable crowd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с высшим обществом или богатыми людьми; элитный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фешенебельный",
                                "target_lang_sense_clarification": "о местах, курортах, районах"
                            },
                            {
                                "target_lang_word": "светский",
                                "target_lang_sense_clarification": "об обществе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "816b57e8-9d21-3d41-88d0-bd073a943f48",
                    "sense_definition": "A person who dresses according to the latest trends, often excessively so.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young <w>fashionables</w> gathered at the club to show off their suits.",
                            "target_lang_translations": {
                                "ru": "Молодые <w>модники</w> собрались в клубе, чтобы похвастаться своими костюмами."
                            }
                        },
                        {
                            "text": "He tried to act like a <w>fashionable</w> despite having little money.",
                            "target_lang_translations": {
                                "ru": "Он пытался вести себя как <w>щеголь</w>, несмотря на нехватку денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "dandy",
                        "fop",
                        "fashionista"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "term used historically, similar to dandy or fop"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который одевается по последней моде (часто чрезмерно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модник",
                                "target_lang_sense_clarification": "человек, следящий за модой"
                            },
                            {
                                "target_lang_word": "щеголь",
                                "target_lang_sense_clarification": "франт, любитель наряжаться"
                            }

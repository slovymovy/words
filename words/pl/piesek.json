                {
                    "sense_id": "c07d622f-3461-3924-848f-a4d909ec9147",
                    "sense_definition": "zdrobnienie od <w>pies</w>; mały pies lub szczeniak",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_dog",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mały <piesek> uwielbia codzienne spacery po parku.",
                            "target_lang_translations": {
                                "ru": "Мой маленький <пёсик> обожает ежедневные прогулки в парке."
                            }
                        },
                        {
                            "text": "Na podwórku biegał wesoły <piesek> sąsiadów i szczekał na gołębie.",
                            "target_lang_translations": {
                                "ru": "По двору бегал весёлый соседский <пёсик> и лаял на голубей."
                            }
                        }
                    ],

                {
                    "sense_id": "b29d631a-5139-3fc8-9788-c8c2db7e5713",
                    "sense_definition": "De aandacht of concentratie volledig op iets of iemand richten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_focus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je goed <w>focussen</w> als je dit moeilijke boek leest.",
                            "target_lang_translations": {
                                "en": "You need to <w>focus</w> well when reading this difficult book.",
                                "ru": "Тебе нужно хорошо <w>сосредоточиться</w>, когда читаешь эту сложную книгу."
                            }
                        },
                        {
                            "text": "Het bedrijf gaat zich volgend jaar <w>focussen</w> op duurzame energie.",
                            "target_lang_translations": {
                                "en": "The company is going to <w>focus</w> on sustainable energy next year.",
                                "ru": "В следующем году компания собирается <w>сфокусироваться</w> на устойчивой энергетике."
                            }
                        }
                    ],
                    "synonyms": [
                        "concentreren",
                        "richten"
                    ],
                    "antonyms": [
                        "afleiden"
                    ],
                    "common_phrases": [
                        "focussen op",
                        "zich focussen"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct one's full attention or concentration towards something or someone.",
                        "ru": "Направлять полное внимание или концентрацию на что-то или кого-то."
                    },

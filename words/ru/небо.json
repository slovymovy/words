    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a9ce8ea6-581c-3da6-832a-2638c1db0b51",
                    "sense_definition": "Пространство над поверхностью Земли, где видны облака, солнце, луна и звёзды.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над нашей головой сияло чистое голубое <w>небо</w>.",
                            "target_lang_translations": {
                                "en": "A clear blue <w>sky</w> shone above our heads."
                            }
                        },
                        {
                            "text": "Летом мы часто ночевали в саду под открытым <w>небом</w>.",
                            "target_lang_translations": {
                                "en": "In summer, we often slept in the garden under the open <w>sky</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "небеса",
                        "небосвод",
                        "небосклон",
                        "поднебесье"
                    ],
                    "antonyms": [
                        "земля"
                    ],
                    "common_phrases": [
                        "голубое небо",
                        "звёздное небо",
                        "на небе",
                        "под открытым небом",
                        "чистое небо"
                    ],
                    "target_lang_definitions": {
                        "en": "The space above the Earth's surface where clouds, the sun, the moon, and stars are visible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sky",
                                "target_lang_sense_clarification": "the atmosphere seen from the earth"
                            },
                            {
                                "target_lang_word": "firmament",
                                "target_lang_sense_clarification": "literary or poetic term for the sky"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a76d21fc-ba6d-31a0-a76f-46ab3eb7114c",
                    "sense_definition": "Место обитания Бога, ангелов и душ умерших в религиозных представлениях; рай.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верующие люди надеются, что после смерти их души попадут на <w>небо</w>.",
                            "target_lang_translations": {
                                "en": "Believers hope that after death their souls will go to <w>heaven</w>."
                            }
                        },
                        {
                            "text": "Когда он узнал эту радостную новость, то был на седьмом <w>небе</w> от счастья.",
                            "target_lang_translations": {
                                "en": "When he found out this joyful news, he was in seventh <w>heaven</w> with happiness."
                            }
                        }
                    ],
                    "synonyms": [
                        "рай",
                        "небеса"
                    ],
                    "antonyms": [
                        "ад",
                        "преисподняя",
                        "земля"
                    ],
                    "common_phrases": [
                        "на седьмом небе",
                        "как небо и земля",
                        "возносить до небес",
                        "воля неба"
                    ],
                    "target_lang_definitions": {
                        "en": "The dwelling place of God, angels, and the souls of the dead in religious beliefs; paradise."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heaven",
                                "target_lang_sense_clarification": "the abode of God and angels"
                            },
                            {
                                "target_lang_word": "paradise",
                                "target_lang_sense_clarification": "a place of perfect happiness"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1a979f14-6344-3270-921f-e081933b6df3",
                    "sense_definition": "Przyjmować określoną pozę, trwać w bezruchu, aby umożliwić komuś zrobienie zdjęcia lub namalowanie obrazu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "posing_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Modelka musiała cierpliwie <w>pozować</w> malarzowi przez kilka godzin bez ruchu.",
                            "target_lang_translations": {
                                "ru": "Модель должна была терпеливо <w>позировать</w> художнику несколько часов без движения."
                            }
                        },
                        {
                            "text": "Wszyscy goście weselni chcieli wspólnie <w>pozować</w> do pamiątkowej fotografii.",
                            "target_lang_translations": {
                                "ru": "Все гости на свадьбе хотели вместе <w>позировать</w> для памятной фотографии."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustawiać się"
                    ],
                    "common_phrases": [
                        "pozować do zdjęcia",
                        "pozować malarzowi",
                        "pozować do portretu",
                        "pozować nago"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать определенную позу, оставаться неподвижным, чтобы позволить кому-то сделать фотографию или нарисовать картину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позировать",
                                "target_lang_sense_clarification": "для фото или художника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8419485d-f682-349a-bc9c-efd994378b26",
                    "sense_definition": "Zachowywać się w sposób nienaturalny, udając kogoś innego lub przypisując sobie cechy, których się nie posiada, aby wywrzeć wrażenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "posing_fake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kolega lubi <w>pozować</w> na wielkiego znawcę sztuki, choć niewiele o niej wie.",
                            "target_lang_translations": {
                                "ru": "Мой друг любит <w>строить из себя</w> великого знатока искусства, хотя мало о нем знает."
                            }
                        },
                        {
                            "text": "Nie próbuj <w>pozować</w> na kogoś, kim nie jesteś, bo ludzie szybko to zauważą.",
                            "target_lang_translations": {
                                "ru": "Не пытайся <w>изображать из себя</w> того, кем ты не являешься, так как люди быстро это заметят."
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać",
                        "grać",
                        "zgrywać się"
                    ],
                    "common_phrases": [
                        "pozować na ofiarę",
                        "pozować na intelektualistę",
                        "pozować na bohatera",
                        "pozować na twardziela"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя неестественно, притворяясь кем-то другим или приписывая себе качества, которыми не обладаешь, чтобы произвести впечатление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строить из себя",
                                "target_lang_sense_clarification": "притворяться кем-то важным"
                            },
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "играть роль, притворяться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

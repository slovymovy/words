        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "407780d5-7024-3ad0-adf4-f04b3bd1868f",
                    "sense_definition": "To assume or to place in a particular position or orientation; to pose.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The figures were <w>attituded</w> around the central statue.",
                            "target_lang_translations": {
                                "ru": "Фигуры были <w>расставлены</w> в позах вокруг центральной статуи."
                            }
                        },
                        {
                            "text": "She <w>attituded</w> herself like a model for the painter.",
                            "target_lang_translations": {
                                "ru": "Она <w>позировала</w>, как модель для художника."
                            }

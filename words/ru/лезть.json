    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "88a76840-aa17-3612-9bc8-9cc6b21feeff",
                    "sense_definition": "Перемещаться вверх или вперед, карабкаясь, цепляясь руками и ногами, или с трудом пробираться через препятствия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький мальчик пытался <лезть> на высокое дерево во дворе.",
                            "target_lang_translations": {
                                "en": "The little boy tried to <climb> the tall tree in the yard."
                            }
                        },
                        {
                            "text": "Туристам пришлось долго <лезть> в гору по узкой тропинке.",
                            "target_lang_translations": {
                                "en": "The tourists had to <climb> up the mountain for a long time along a narrow path."
                            }
                        }
                    ],
                    "synonyms": [
                        "карабкаться",
                        "проникать",
                        "проникать"
                    ],
                    "common_phrases": [
                        "лезть на стену",
                        "лезть в гору",
                        "лезть через забор",
                        "лезть на крышу"
                    ],
                    "target_lang_definitions": {
                        "en": "To move upwards or forwards, climbing with hands and feet, or to make one's way through obstacles with difficulty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "climb",
                                "target_lang_sense_clarification": "to move upwards using hands and feet"
                            },
                            {
                                "target_lang_word": "clamber",
                                "target_lang_sense_clarification": "to climb or move with difficulty"
                            },
                            {
                                "target_lang_word": "scale",
                                "target_lang_sense_clarification": "to climb up a steep surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96dac19a-3eda-30fa-a026-593e8dc1f2eb",
                    "sense_definition": "Проникать внутрь чего-либо, заходить куда-либо; засовывать руку в карман, сумку и т.п.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит <лезть> в холодную воду, если ты простужен.",
                            "target_lang_translations": {
                                "en": "You shouldn't <get> into cold water if you have a cold."
                            }
                        },
                        {
                            "text": "Он полез в карман за ключами, но их там не оказалось.",
                            "target_lang_translations": {
                                "en": "He <reached> into his pocket for the keys, but they weren't there."
                            }
                        }
                    ],
                    "synonyms": [
                        "проникать",
                        "проникать"
                    ],
                    "common_phrases": [
                        "лезть в воду",
                        "лезть в карман",
                        "лезть в окно",
                        "лезть в драку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в разговорной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To penetrate inside something, enter somewhere; to put one's hand into a pocket, bag, etc."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get (into)",
                                "target_lang_sense_clarification": "to enter or penetrate a space"
                            },
                            {
                                "target_lang_word": "reach (into)",
                                "target_lang_sense_clarification": "to put one's hand inside something"
                            },
                            {
                                "target_lang_word": "climb (into)",
                                "target_lang_sense_clarification": "to enter a space with effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62ba7fc4-15f9-392c-bff3-f1940a38a654",
                    "sense_definition": "Назойливо вмешиваться в чужие дела, разговоры или отношения; приставать к кому-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я попросил его не <лезть> в мою личную жизнь.",
                            "target_lang_translations": {
                                "en": "I asked him not to <interfere> in my private life."
                            }
                        },
                        {
                            "text": "Соседка любит <лезть> с советами, когда её совсем не просят.",
                            "target_lang_translations": {
                                "en": "The neighbor loves to <butt in> with advice when she isn't asked at all."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лезть не в своё дело",
                        "лезть с советами",
                        "лезть в душу",
                        "лезть на рожон"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "неодобрительный оттенок"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To intrusively interfere in other people's affairs, conversations, or relationships; to pester someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interfere",
                                "target_lang_sense_clarification": "to meddle in others' affairs"
                            },
                            {
                                "target_lang_word": "meddle",
                                "target_lang_sense_clarification": "to busy oneself with things that are not one's concern"
                            },
                            {
                                "target_lang_word": "pester",
                                "target_lang_sense_clarification": "to annoy with frequent interruptions"
                            },
                            {
                                "target_lang_word": "bother",
                                "target_lang_sense_clarification": "to annoy or disturb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd5a7d78-59cb-3227-8677-8803f10d2b17",
                    "sense_definition": "Помещаться внутри чего-либо, быть впору (обычно об одежде или обуви, часто с отрицанием).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_fit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти старые ботинки мне уже не <лезут>.",
                            "target_lang_translations": {
                                "en": "These old boots don't <fit> me anymore."
                            }
                        },
                        {
                            "text": "В переполненный чемодан больше ничего не <лезет>.",
                            "target_lang_translations": {
                                "en": "Nothing else <fits> into the overfilled suitcase."
                            }
                        }
                    ],
                    "synonyms": [
                        "налезать"
                    ],
                    "common_phrases": [
                        "не лезть",
                        "еле лезть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fit inside something, to be the right size (usually about clothes or shoes, often with negation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "to be of the right size or shape"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "502bc3a5-c1d0-3878-ac0b-a7fe5161f87c",
                    "sense_definition": "Выпадать, отделяться от кожи (о волосах, шерсти).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_deterioration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У нашей кошки весной сильно <лезет> шерсть.",
                            "target_lang_translations": {
                                "en": "Our cat's fur <sheds> heavily in the spring."
                            }
                        },
                        {
                            "text": "После сильного стресса у него начали <лезть> волосы.",
                            "target_lang_translations": {
                                "en": "After severe stress, his hair started to <fall out>."
                            }
                        }
                    ],
                    "synonyms": [
                        "линять"
                    ],
                    "common_phrases": [
                        "волосы лезут",
                        "шерсть лезет"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fall out or separate from the skin (referring to hair, fur)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall out",
                                "target_lang_sense_clarification": "to detach from the skin (hair)"
                            },
                            {
                                "target_lang_word": "shed",
                                "target_lang_sense_clarification": "to lose hair or fur naturally"
                            },
                            {
                                "target_lang_word": "come out",
                                "target_lang_sense_clarification": "to become detached"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3cefb45-373f-314e-914a-5d9362cd1ea9",
                    "sense_definition": "Рваться, расползаться на волокна от ветхости или плохой выделки (о ткани, коже).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "state_deterioration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дешёвая ткань начала <лезть> сразу после первой стирки.",
                            "target_lang_translations": {
                                "en": "The cheap fabric started to <fray> immediately after the first wash."
                            }
                        },
                        {
                            "text": "Старая куртка совсем износилась и <лезет> по швам.",
                            "target_lang_translations": {
                                "en": "The old jacket is completely worn out and is <coming apart> at the seams."
                            }
                        }
                    ],
                    "synonyms": [
                        "дырявиться"
                    ],
                    "common_phrases": [
                        "ткань лезет",
                        "лезть по швам"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To tear, come apart at the fibers due to age or poor quality (referring to fabric, leather)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fray",
                                "target_lang_sense_clarification": "to unravel or become worn at the edge"
                            },
                            {
                                "target_lang_word": "come apart",
                                "target_lang_sense_clarification": "to separate into pieces"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
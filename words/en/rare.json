{
    "word_family": [
        "rarity",
        "rarely",
        "rarefy",
        "rarefaction"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e734b651-8a84-389c-b72b-aab4344d31c5",
                    "sense_definition": "Existing only in small numbers or happening infrequently; not common.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sg_uncommon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>rare</w> to see snow in this part of the country during October.",
                            "target_lang_translations": {
                                "ru": "В этой части страны в октябре <w>редко</w> можно увидеть снег."
                            }
                        },
                        {
                            "text": "The museum displays a <w>rare</w> collection of ancient coins.",
                            "target_lang_translations": {
                                "ru": "В музее выставлена <w>редкая</w> коллекция древних монет."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncommon",
                        "scarce",
                        "infrequent",
                        "few and far between"
                    ],
                    "antonyms": [
                        "common",
                        "frequent"
                    ],
                    "common_phrases": [
                        "rare occasion",
                        "rare bird",
                        "rare disease",
                        "rare species"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий только в небольшом количестве или случающийся нечасто; необычный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ре́дкий",
                                "target_lang_sense_clarification": "нечастый, необычный"
                            },
                            {
                                "target_lang_word": "нечастый",
                                "target_lang_sense_clarification": "случающийся редко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "324df4ee-31c0-386a-a3c7-86372f7d1558",
                    "sense_definition": "Of meat, especially beef: cooked for a short time so that the inside is still red.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sg_cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would like my steak cooked <w>rare</w>, please.",
                            "target_lang_translations": {
                                "ru": "Я бы хотел стейк <w>с кровью</w>, пожалуйста."
                            }
                        },
                        {
                            "text": "He prefers his burger <w>rare</w> rather than well done.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает бургер <w>с кровью</w>, а не хорошо прожаренный."
                            }
                        }
                    ],
                    "antonyms": [
                        "well done"
                    ],
                    "common_phrases": [
                        "medium rare",
                        "rare steak"
                    ],
                    "target_lang_definitions": {
                        "ru": "О мясе (особенно говядине): приготовленное в течение короткого времени, так что внутри оно остается красным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с кро́вью",
                                "target_lang_sense_clarification": "кулинарный термин"
                            },
                            {
                                "target_lang_word": "непрожа́ренный",
                                "target_lang_sense_clarification": "слабой прожарки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "958da60d-1b91-3bf1-942a-204dd114faa4",
                    "sense_definition": "Having low density; thin (usually describing air or atmosphere).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sg_density",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Climbers often struggle to breathe in the <w>rare</w> air at high altitudes.",
                            "target_lang_translations": {
                                "ru": "Альпинистам часто трудно дышать <w>разрежённым</w> воздухом на больших высотах."
                            }
                        },
                        {
                            "text": "The balloon ascended rapidly into the <w>rare</w> atmosphere.",
                            "target_lang_translations": {
                                "ru": "Воздушный шар быстро поднялся в <w>разрежённую</w> атмосферу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rare air",
                        "rare atmosphere"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "often literary or technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий низкую плотность; тонкий (обычно описывает воздух или атмосферу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрежённый",
                                "target_lang_sense_clarification": "о газе или воздухе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97b41e32-4167-31f5-b556-5d64fb8d6e7e",
                    "sense_definition": "Unusually good, fine, or excellent.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sg_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The garden was a place of <w>rare</w> beauty and tranquility.",
                            "target_lang_translations": {
                                "ru": "Сад был местом <w>редкой</w> красоты и спокойствия."
                            }
                        },
                        {
                            "text": "He is a man of <w>rare</w> talent and intelligence.",
                            "target_lang_translations": {
                                "ru": "Он человек <w>редкого</w> таланта и ума."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rare beauty",
                        "rare talent"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычайно хороший, прекрасный или превосходный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ре́дкий",
                                "target_lang_sense_clarification": "исключительный"
                            },
                            {
                                "target_lang_word": "редкостный",
                                "target_lang_sense_clarification": "удивительный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "60dc9efe-1f01-3537-9391-98c7a584fb07",
                    "sense_definition": "A scarce or uncommon item, especially in collecting or gaming.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sg_uncommon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He keeps his most valuable <w>rares</w> in a protective case.",
                            "target_lang_translations": {
                                "ru": "Он хранит свои самые ценные <w>раритеты</w> в защитном футляре."
                            }
                        },
                        {
                            "text": "Players often trade common cards for <w>rares</w> to complete their decks.",
                            "target_lang_translations": {
                                "ru": "Игроки часто обменивают обычные карты на <w>редкие</w>, чтобы собрать колоду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "gaming and collecting context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дефицитный или необычный предмет, особенно в коллекционировании или играх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раритет",
                                "target_lang_sense_clarification": "редкая вещь"
                            },
                            {
                                "target_lang_word": "редкость",
                                "target_lang_sense_clarification": "ценный предмет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c13d75e3-f75d-3e9f-b36d-cb331201a751",
                    "sense_definition": "To rise up on the hind legs; to rear.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sg_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The frightened horse <w>rared</w> up and threw its rider.",
                            "target_lang_translations": {
                                "ru": "Испуганная лошадь <w>встала на дыбы</w> и сбросила всадника."
                            }
                        },
                        {
                            "text": "The team is <w>raring</w> to go and start the championship.",
                            "target_lang_translations": {
                                "ru": "Команда <w>рвется</w> в бой и готова начать чемпионат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "raring to go"

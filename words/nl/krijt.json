    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f7d47b2-ebe1-36fb-923a-e687a163b82d",
                    "sense_definition": "Een staafje gemaakt van kalk of gips dat wordt gebruikt om mee te schrijven of te tekenen, vooral op een schoolbord.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "writing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar schrijft de som met <w>krijt</w> op het bord.",
                            "target_lang_translations": {
                                "en": "The teacher writes the sum on the board with <w>chalk</w>.",
                                "ru": "Учитель пишет задачу <w>мелом</w> на доске."
                            }
                        },
                        {
                            "text": "Mijn handen zijn wit van het stof door het <w>krijt</w>.",
                            "target_lang_translations": {
                                "en": "My hands are white with dust from the <w>chalk</w>.",
                                "ru": "Мои руки белые от пыли из-за <w>мела</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "perk"
                    ],
                    "common_phrases": [
                        "met krijt schrijven",
                        "een stukje krijt"
                    ],
                    "target_lang_definitions": {
                        "en": "A stick made of lime or gypsum used for writing or drawing, especially on a blackboard.",
                        "ru": "Палочка из извести или гипса, используемая для письма или рисования, особенно на школьной доске."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chalk",
                                "target_lang_sense_clarification": "writing tool"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мел",
                                "target_lang_sense_clarification": "для письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f031072f-a4b9-3650-b1b1-ac54075da8c2",
                    "sense_definition": "Een zacht, wit en poreus gesteente dat voornamelijk uit calciumcarbonaat bestaat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De beroemde witte kliffen van Dover bestaan bijna volledig uit <w>krijt</w>.",

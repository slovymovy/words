    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9aa64b79-1980-343e-92d7-9ec428d85e12",
                    "sense_definition": "Not swollen, inflated, or expanded.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baker was disappointed to see the <w>unpuffed</w> pastry dough when he opened the oven.",
                            "target_lang_translations": {
                                "ru": "Пекарь был разочарован, увидев <w>неподнявшееся</w> тесто, когда открыл духовку."
                            }
                        },
                        {
                            "text": "She preferred the dense texture of the <w>unpuffed</w> grain compared to the lighter version.",
                            "target_lang_translations": {
                                "ru": "Она предпочитала плотную текстуру <w>невзорванного</w> зерна более легкой версии."
                            }
                        }
                    ],
                    "synonyms": [
                        "flat",
                        "uninflated"
                    ],
                    "antonyms": [
                        "puffed",
                        "swollen",
                        "inflated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не вздутый, не раздутый или не увеличенный в объеме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неподнявшийся",
                                "target_lang_sense_clarification": "о тесте или выпечке"
                            },
                            {
                                "target_lang_word": "невзорванный",
                                "target_lang_sense_clarification": "о зерне или попкорне"
                            },
                            {
                                "target_lang_word": "нераздутый",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e3a5b68-0e78-3db6-9453-d1c410cee908",
                    "sense_definition": "Simple past and past participle of <w>unpuff</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bird slowly <w>unpuffed</w> its feathers once the threat had passed.",
                            "target_lang_translations": {
                                "ru": "Птица медленно <w>пригладила</w> перья, как только угроза миновала."
                            }
                        },
                        {
                            "text": "He <w>unpuffed</w> his cheeks and exhaled deeply after holding his breath.",
                            "target_lang_translations": {
                                "ru": "Он <w>сдул</w> щеки и глубоко выдохнул после задержки дыхания."
                            }
                        }
                    ],
                    "synonyms": [
                        "deflated"
                    ],
                    "antonyms": [
                        "puffed",
                        "inflated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола unpuff (сдувать, приглаживать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдул",
                                "target_lang_sense_clarification": "выпустил воздух (например, из щек)"
                            },
                            {
                                "target_lang_word": "пригладил",
                                "target_lang_sense_clarification": "о перьях или шерсти"
                            }
                        ]
                    }
                }
            ]
        }
    ]

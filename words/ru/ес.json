                    "sense_id": "31c0a6d7-7d67-3079-b400-6a62557ef2c6",
                    "sense_definition": "Сокращение от <w>Европейский Союз</w>; экономическое и политическое объединение ряда европейских стран.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "eu_org",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Многие государства стремятся вступить в <w>ЕС</w> для улучшения своей экономики.",
                            "target_lang_translations": {
                                "en": "Many states strive to join the <w>EU</w> to improve their economy."
                            }
                        },
                        {
                            "text": "Граждане стран <w>ЕС</w> имеют право свободно перемещаться через границы внутри союза.",
                            "target_lang_translations": {
                                "en": "Citizens of <w>EU</w> countries have the right to move freely across borders within the union."
                            }
                        }
                    ],
                    "synonyms": [
                        "Европейский Союз",
                        "Евросоюз"
                    ],
                    "common_phrases": [
                        "страны ЕС",
                        "вступление в ЕС",
                        "выход из ЕС",
                        "саммит ЕС"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "аббревиатура"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for European Union; an economic and political union of a number of European countries."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "EU",
                                "target_lang_sense_clarification": "abbreviation"
                            },
                            {
                                "target_lang_word": "European Union",
                                "target_lang_sense_clarification": "full name"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [

                                "target_lang_word": "ежекварта́льное изда́ние",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "ежекварта́льный журна́л",
                                "target_lang_sense_clarification": "тип издания"
                            },
                            {
                                "target_lang_word": "трёхме́сячник",

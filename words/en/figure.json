                {
                    "sense_id": "73f145ac-fcd9-3bed-a583-2b40e38f2dc8",
                    "sense_definition": "A number representing an amount, especially in official statistics or finance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "numbers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unemployment <w>figure</w> has dropped significantly this year.",
                            "target_lang_translations": {
                                "ru": "<w>Показатель</w> безработицы в этом году значительно снизился."
                            }
                        },
                        {
                            "text": "They sold the house for a <w>figure</w> close to one million dollars.",
                            "target_lang_translations": {
                                "ru": "Они продали дом за <w>сумму</w>, близкую к миллиону долларов."
                            }
                        }
                    ],
                    "synonyms": [
                        "amount",
                        "number",
                        "value",
                        "sum",
                        "statistic"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "828a1d58-2438-39a5-b533-b4b4957548c6",
                    "sense_definition": "Тайно брать чужое имущество без разрешения; присваивать то, что тебе не принадлежит.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "theft_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто-то повадился <w>воровать</w> яблоки из нашего сада по ночам.",
                            "target_lang_translations": {
                                "en": "Someone got into the habit of <w>stealing</w> apples from our garden at night."
                            }
                        },
                        {
                            "text": "Нельзя <w>воровать</w> деньги, даже если их никто не охраняет.",
                            "target_lang_translations": {
                                "en": "You must not <w>steal</w> money, even if no one is guarding it."
                            }
                        }
                    ],
                    "synonyms": [
                        "красть",
                        "похищать",
                        "тырить"
                    ],
                    "common_phrases": [
                        "воровать деньги",
                        "воровать идеи",
                        "воровать продукты"
                    ],
                    "target_lang_definitions": {
                        "en": "To secretly take someone else's property without permission; to appropriate what does not belong to you."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steal",
                                "target_lang_sense_clarification": "the most common, general term"
                            },
                            {
                                "target_lang_word": "pilfer",
                                "target_lang_sense_clarification": "to steal small items of little value"
                            },
                            {
                                "target_lang_word": "pinch",
                                "target_lang_sense_clarification": "informal, slang"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45aed2c2-1ad0-3045-ba4f-e9c54eda020a",
                    "sense_definition": "Заниматься кражами как постоянным делом или профессией; вести образ жизни преступника.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "theft_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не хотел работать честно и пошёл <w>воровать</w>.",
                            "target_lang_translations": {
                                "en": "He didn't want to work honestly and started <w>stealing</w>."
                            }
                        },
                        {
                            "text": "В этой банде заставляли детей <w>воровать</w> на рынках.",
                            "target_lang_translations": {
                                "en": "In this gang, they forced children to <w>steal</w> in the markets."
                            }
                        }
                    ],
                    "synonyms": [
                        "красть"
                    ],
                    "common_phrases": [
                        "бросить воровать",
                        "начать воровать",
                        "перестать воровать"
                    ],
                    "target_lang_definitions": {
                        "en": "To engage in theft as a regular activity or profession; to lead a criminal lifestyle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steal",
                                "target_lang_sense_clarification": "to commit theft habitually"
                            },
                            {
                                "target_lang_word": "thieve",
                                "target_lang_sense_clarification": "to practice theft as a profession or habit"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5abd6a61-9977-3a16-b109-d6bdb7d97b77",
                    "sense_definition": "Toestaan dat iets gebeurt of dat iemand iets doet; niet verhinderen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn ouders <w>laten</w> mij vanavond laat uitgaan.",
                            "target_lang_translations": {
                                "en": "My parents <w>let</w> me go out late tonight.",
                                "ru": "Мои родители <w>разрешают</w> мне сегодня поздно гулять."
                            }
                        },
                        {
                            "text": "<w>Laat</w> de kinderen lekker buiten spelen.",
                            "target_lang_translations": {
                                "en": "<w>Let</w> the children play outside happily.",
                                "ru": "<w>Позволь</w> детям спокойно поиграть на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "toestaan",
                        "toelaten",
                        "dulden"
                    ],
                    "antonyms": [
                        "verbieden",
                        "beletten",
                        "tegenhouden"
                    ],
                    "common_phrases": [
                        "laten gaan",
                        "laten vallen",
                        "laten zitten"
                    ],
                    "target_lang_definitions": {
                        "en": "To allow something to happen or someone to do something; not to prevent.",
                        "ru": "Позволять чему-то случиться или кому-то что-то сделать; не препятствовать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let",
                                "target_lang_sense_clarification": "allow to happen"
                            },
                            {
                                "target_lang_word": "allow",
                                "target_lang_sense_clarification": "give permission"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "позволять",
                                "target_lang_sense_clarification": "давать разрешение"
                            },
                            {
                                "target_lang_word": "разрешать",
                                "target_lang_sense_clarification": "давать согласие"
                            },
                            {
                                "target_lang_word": "давать",
                                "target_lang_sense_clarification": "разговорное; не мешать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7e32e12-578e-3955-a037-409b26303895",
                    "sense_definition": "Ervoor zorgen dat iets gebeurt, vaak door opdracht te geven aan iemand anders (causatief).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>laat</w> mijn auto morgen repareren bij de garage.",
                            "target_lang_translations": {
                                "en": "I am <w>having</w> my car repaired at the garage tomorrow.",
                                "ru": "Я завтра <w>отдам</w> машину в ремонт в автосервис."
                            }
                        },
                        {
                            "text": "Hij <w>liet</w> ons vol trots zijn nieuwe huis zien.",
                            "target_lang_translations": {
                                "en": "He proudly <w>let</w> us see his new house.",
                                "ru": "Он с гордостью <w>показал</w> нам свой новый дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "doen"
                    ],
                    "common_phrases": [
                        "laten zien",
                        "laten weten",
                        "laten horen",
                        "laten maken"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something to happen, often by instructing someone else to do it (causative).",
                        "ru": "Обеспечивать выполнение действия, часто поручая его кому-то другому (каузатив)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "cause to be done"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "arrange to be done"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заставлять",
                                "target_lang_sense_clarification": "принуждать к действию"
                            },
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "приказывать"
                            },
                            {
                                "target_lang_word": "поручать",
                                "target_lang_sense_clarification": "доверять выполнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f76175e-435b-3cf2-ac5b-fa951aae26bb",
                    "sense_definition": "Hulpwerkwoord om een voorstel, aansporing of wens uit te drukken (vaak in de vorm 'laten we').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hortative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Laten</w> we dit weekend naar het strand gaan.",
                            "target_lang_translations": {
                                "en": "<w>Let</w>'s go to the beach this weekend.",
                                "ru": "<w>Давайте</w> поедем на пляж в эти выходные."
                            }
                        },
                        {
                            "text": "<w>Laten</w> we hopen dat het morgen droog blijft.",
                            "target_lang_translations": {
                                "en": "<w>Let</w>'s hope it stays dry tomorrow.",
                                "ru": "<w>Будем</w> надеяться, что завтра будет сухо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "laten we"
                    ],
                    "target_lang_definitions": {
                        "en": "Auxiliary verb used to express a proposal, encouragement, or wish (usually 'let us').",
                        "ru": "Вспомогательный глагол для выражения предложения, призыва или желания (часто в форме 'laten we')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let",
                                "target_lang_sense_clarification": "in 'let us'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "побуждение (ед.ч. / неформ.)"
                            },
                            {
                                "target_lang_word": "давайте",
                                "target_lang_sense_clarification": "побуждение (мн.ч. / форм.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82e88fbc-762a-327c-91f7-583ac5c236a9",
                    "sense_definition": "Iets of iemand ergens achterlaten of in een bepaalde toestand houden; niet meenemen of veranderen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "leave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb mijn paraplu per ongeluk in de trein <w>gelaten</w>.",
                            "target_lang_translations": {
                                "en": "I accidentally <w>left</w> my umbrella on the train.",
                                "ru": "Я случайно <w>оставил</w> свой зонт в поезде."
                            }
                        },
                        {
                            "text": "<w>Laat</w> de deur maar open staan voor frisse lucht.",
                            "target_lang_translations": {
                                "en": "Just <w>leave</w> the door open for fresh air.",
                                "ru": "<w>Оставь</w> дверь открытой для свежего воздуха."
                            }
                        }
                    ],
                    "synonyms": [
                        "achterlaten",
                        "laten liggen"
                    ],
                    "antonyms": [
                        "meenemen"
                    ],
                    "common_phrases": [
                        "met rust laten",
                        "links laten liggen",
                        "alleen laten"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave something or someone behind or keep in a certain state; not to take or change.",
                        "ru": "Оставлять что-то или кого-то где-то или в определенном состоянии; не брать с собой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "allow to remain"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "не брать с собой или сохранять состояние"
                            },
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "случайно оставлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecd11eeb-963f-34bd-ba0a-9450684857cb",
                    "sense_definition": "Iets niet doen; stoppen met een handeling.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "refrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt dat vervelende commentaar beter <w>laten</w>.",
                            "target_lang_translations": {
                                "en": "You had better <w>stop</w> those annoying comments.",
                                "ru": "Тебе лучше <w>оставить</w> эти неприятные комментарии."
                            }
                        },
                        {
                            "text": "Hij kon het niet <w>laten</w> om even te lachen.",
                            "target_lang_translations": {
                                "en": "He could not <w>refrain from</w> laughing for a moment.",
                                "ru": "Он не мог <w>удержаться</w> от смеха."
                            }
                        }
                    ],
                    "synonyms": [
                        "nalaten",
                        "stoppen"
                    ],
                    "antonyms": [
                        "doen"
                    ],
                    "common_phrases": [
                        "doen en laten",
                        "dat kun je wel laten"
                    ],
                    "target_lang_definitions": {
                        "en": "To not do something; to stop an action or refrain from it.",
                        "ru": "Не делать чего-то; прекращать действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "refrain from",
                                "target_lang_sense_clarification": "stop doing"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "omit or stop"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переставать",
                                "target_lang_sense_clarification": "прекращать делать"
                            },
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "бросать привычку или действие"
                            },
                            {
                                "target_lang_word": "воздерживаться",
                                "target_lang_sense_clarification": "сдерживать себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18e46282-f83c-3a1f-8487-3b3d79d4d9b9",
                    "sense_definition": "Iets laten ontsnappen of naar buiten laten komen (zoals gassen of vocht).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "release",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De baby <w>liet</w> een hard boertje na het drinken.",
                            "target_lang_translations": {
                                "en": "The baby <w>let out</w> a loud burp after drinking.",
                                "ru": "Ребенок громко <w>рыгнул</w> после питья."
                            }
                        },
                        {
                            "text": "Zij moest een traan <w>laten</w> bij het droevige nieuws.",
                            "target_lang_translations": {
                                "en": "She had to <w>shed</w> a tear at the sad news.",
                                "ru": "Она <w>проронила</w> слезу, услышав печальные новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "loslaten"
                    ],
                    "antonyms": [
                        "inhouden"
                    ],
                    "common_phrases": [
                        "een scheet laten",
                        "een traan laten",
                        "een wind laten"
                    ],
                    "target_lang_definitions": {
                        "en": "To let something escape or come out (such as gases, liquid, or sound).",
                        "ru": "Выпускать наружу или давать выход (газам, жидкостям, эмоциям)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let out",
                                "target_lang_sense_clarification": "release"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "emit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "испускать",
                                "target_lang_sense_clarification": "выделять (газы, звуки)"
                            },
                            {
                                "target_lang_word": "пускать",
                                "target_lang_sense_clarification": "давать выйти"
                            },
                            {
                                "target_lang_word": "проронить",
                                "target_lang_sense_clarification": "о слезах или словах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

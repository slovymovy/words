{
    "word_family": [
        "activation",
        "activator",
        "active",
        "activity",
        "deactivate",
        "reactivate",
        "inactivate"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e6dbbf51-8221-3ca6-88ce-9f079c365b16",
                    "sense_definition": "To make a piece of equipment, mechanism, or system start working or become active.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "make_operational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to enter a code to <w>activate</w> the security system.",
                            "target_lang_translations": {
                                "ru": "Вам нужно ввести код, чтобы <w>активировать</w> систему безопасности."
                            }
                        },
                        {
                            "text": "The smoke detector will <w>activate</w> automatically if it senses a fire.",
                            "target_lang_translations": {
                                "ru": "Детектор дыма <w>сработает</w> автоматически, если почувствует огонь."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn on",
                        "start",
                        "trigger",
                        "set off",
                        "enable"
                    ],
                    "antonyms": [
                        "deactivate"
                    ],
                    "common_phrases": [
                        "activate the alarm",
                        "activate the system",
                        "voice-activated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить оборудование, механизм или систему начать работать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включить",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "активировать",
                                "target_lang_sense_clarification": "систему"
                            },
                            {
                                "target_lang_word": "привести в действие",
                                "target_lang_sense_clarification": "механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e43b394-91c5-3cdc-bedb-ec6819d8eb53",
                    "sense_definition": "To make a software program, account, or service operational, valid, or ready for use.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "make_operational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please click the link in the email to <w>activate</w> your account.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, перейдите по ссылке в письме, чтобы <w>активировать</w> вашу учетную запись."
                            }
                        },
                        {
                            "text": "I need to call the bank to <w>activate</w> my new credit card.",
                            "target_lang_translations": {
                                "ru": "Мне нужно позвонить в банк, чтобы <w>активировать</w> мою новую кредитную карту."
                            }
                        }
                    ],
                    "synonyms": [
                        "enable",
                        "turn on",
                        "start"
                    ],
                    "antonyms": [
                        "deactivate"
                    ],
                    "common_phrases": [
                        "activate an account",
                        "activate a card",
                        "activate a license"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать программное обеспечение, учетную запись или услугу готовыми к использованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активировать",
                                "target_lang_sense_clarification": "учетную запись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94e330f5-a9f0-3e77-a679-96061f62e1ae",
                    "sense_definition": "To encourage development, induce increased activity, or make something more dynamic.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "stimulate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government introduced new policies to <w>activate</w> the local economy.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело новые меры, чтобы <w>активизировать</w> местную экономику."
                            }
                        },
                        {
                            "text": "Teachers use interactive games to <w>activate</w> student interest in the subject.",
                            "target_lang_translations": {
                                "ru": "Учителя используют интерактивные игры, чтобы <w>активизировать</w> интерес учащихся к предмету."
                            }
                        }
                    ],
                    "synonyms": [
                        "stimulate",
                        "spur"
                    ],
                    "common_phrases": [
                        "activate the economy",
                        "activate interest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стимулировать развитие или повышать активность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активизировать",
                                "target_lang_sense_clarification": "деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2afa879e-06b4-3394-9660-1aa4481aae7b",
                    "sense_definition": "To cause a chemical reaction or make a substance chemically or biologically active.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scientific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Specific enzymes are required to <w>activate</w> the process within the cells.",
                            "target_lang_translations": {
                                "ru": "Для того чтобы <w>активировать</w> процесс внутри клеток, требуются особые ферменты."
                            }
                        },
                        {
                            "text": "Heat is often used to <w>activate</w> the chemical reaction in the mixture.",
                            "target_lang_translations": {
                                "ru": "Тепло часто используется, чтобы <w>активировать</w> химическую реакцию в смеси."
                            }
                        }
                    ],
                    "synonyms": [
                        "trigger",
                        "stimulate"
                    ],
                    "antonyms": [
                        "inactivate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать химическую реакцию или сделать вещество активным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активировать",
                                "target_lang_sense_clarification": "вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0bbdc55-ae5d-394a-9ffa-5b7ed8a7d6dd",
                    "sense_definition": "To officially organize and prepare a military unit for active service.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general decided to <w>activate</w> the reserve troops due to the emergency.",
                            "target_lang_translations": {
                                "ru": "Генерал решил <w>мобилизовать</w> резервные войска в связи с чрезвычайной ситуацией."
                            }
                        },
                        {
                            "text": "A new squadron was <w>activated</w> to support the mission overseas.",
                            "target_lang_translations": {
                                "ru": "Для поддержки миссии за рубежом была <w>сформирована</w> новая эскадрилья."
                            }
                        }
                    ],
                    "antonyms": [
                        "deactivate"
                    ],
                    "common_phrases": [
                        "activate the reserves",
                        "activate a unit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально организовать и подготовить воинскую часть к службе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мобилизовать",
                                "target_lang_sense_clarification": "войска"
                            },
                            {
                                "target_lang_word": "привести в действие",
                                "target_lang_sense_clarification": "силы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    }
                },
                {
                    "sense_id": "8446c4de-c81f-36fb-85a1-767f8ec19a44",
                    "sense_definition": "Completely or fully; used as an intensifier.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was <w>wide</w> awake long before the alarm went off.",
                            "target_lang_translations": {
                                "ru": "Я <w>совершенно</w> проснулся задолго до звонка будильника."
                            }

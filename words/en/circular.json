    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a3902cba-fdd7-32b3-8017-13c8684bf708",
                    "sense_definition": "In the shape of a circle, or moving in a path that forms a circle.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shape_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family sat around a large <w>circular</w> table for dinner.",
                            "target_lang_translations": {
                                "ru": "Семья села обедать за большой <w>круглый</w> стол."
                            }
                        },
                        {
                            "text": "The satellite moves in a <w>circular</w> orbit around the planet.",
                            "target_lang_translations": {
                                "ru": "Спутник движется по <w>круговой</w> орбите вокруг планеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "round",
                        "ring-shaped",
                        "annular",
                        "ringlike"
                    ],
                    "antonyms": [
                        "square"
                    ],
                    "common_phrases": [
                        "circular motion",
                        "circular shape",
                        "circular saw",
                        "circular route"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму круга или движущийся по траектории, образующей круг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круглый",
                                "target_lang_sense_clarification": "имеющий форму круга"
                            },
                            {
                                "target_lang_word": "круговой",
                                "target_lang_sense_clarification": "относящийся к движению по кругу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f431af7-eb9f-3ee0-8b83-5d309a7be7fd",
                    "sense_definition": "Referring back to itself in a way that prevents a logical conclusion or answer; using an idea to prove itself.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The essay was criticized for its <w>circular</w> reasoning, as it repeated the same point without proving it.",
                            "target_lang_translations": {
                                "ru": "Эссе критиковали за <w>круговую</w> аргументацию, так как в нем повторялась одна и та же мысль без доказательств."
                            }
                        },
                        {
                            "text": "Defining a word using the word itself is a <w>circular</w> definition.",
                            "target_lang_translations": {
                                "ru": "Определение слова с использованием самого этого слова — это <w>круговое</w> определение."
                            }
                        }
                    ],
                    "synonyms": [
                        "cyclic"
                    ],
                    "common_phrases": [
                        "circular reasoning",
                        "circular argument",
                        "circular definition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ссылающийся сам на себя таким образом, что это препятствует логическому выводу; использующий идею для доказательства самой себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круговой",
                                "target_lang_sense_clarification": "в логике: содержащий порочный круг"
                            },
                            {
                                "target_lang_word": "циклический",
                                "target_lang_sense_clarification": "повторяющийся, замкнутый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5dc748a4-f498-39bf-b00d-98c1dee9cb87",
                    "sense_definition": "A printed advertisement, directive, or notice intended to be sent to a large number of people.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We received a <w>circular</w> in the mail announcing the supermarket's weekly sales.",
                            "target_lang_translations": {
                                "ru": "Мы получили по почте <w>рекламную листовку</w> с анонсом еженедельных распродаж в супермаркете."
                            }
                        },
                        {
                            "text": "The manager sent a <w>circular</w> to all staff members regarding the new office policy.",
                            "target_lang_translations": {
                                "ru": "Менеджер разослал всем сотрудникам <w>циркуляр</w> касательно новой политики офиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "flyer"
                    ],
                    "common_phrases": [
                        "advertising circular",
                        "information circular"
                    ],
                    "target_lang_definitions": {
                        "ru": "Печатное объявление, распоряжение или уведомление, предназначенное для рассылки большому количеству людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "циркуляр",
                                "target_lang_sense_clarification": "официальное письмо или распоряжение"
                            },
                            {
                                "target_lang_word": "рекламная листовка",
                                "target_lang_sense_clarification": "рекламное объявление для рассылки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e315e333-064d-37f3-87dd-06551cf5ca28",
                    "sense_definition": "To distribute advertising circulars to people or places.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "distribution_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The marketing team <w>circulared</w> the entire neighborhood with brochures for the new gym.",
                            "target_lang_translations": {
                                "ru": "Маркетинговая команда <w>разослала</w> брошюры нового спортзала по всему району."
                            }
                        },
                        {
                            "text": "Residents complained about being <w>circulared</w> with junk mail every day.",
                            "target_lang_translations": {
                                "ru": "Жители жаловались на то, что их каждый день <w>заваливают</w> рекламной макулатурой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in business or marketing contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Раздавать или рассылать рекламные проспекты людям или по определенным местам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассылать",
                                "target_lang_sense_clarification": "распространять материалы"
                            },
                            {
                                "target_lang_word": "распространять",
                                "target_lang_sense_clarification": "раздавать многим"
                            }
                        ]
                    }
                }
            ]
        }
    ]

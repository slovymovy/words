        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cc07e8fb-ede5-3aec-9244-050c1c8038d7",
                    "sense_definition": "Zelfverzekerd, krachtig en onbevreesd overkomend, soms met de bedoeling om indruk te maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die <stoere> kerels hebben eigenlijk maar een klein hartje.",
                            "target_lang_translations": {
                                "en": "Those <tough> guys actually have a gentle heart.",
                                "ru": "У этих <крутых> парней на самом деле мягкое сердце."
                            }
                        },
                        {
                            "text": "De minister sloeg <stoere> taal uit tijdens de persconferentie.",
                            "target_lang_translations": {
                                "en": "The minister used <tough> language during the press conference.",
                                "ru": "Министр произносил <смелые> речи во время пресс-конференции."
                            }
                        }
                    ],
                    "synonyms": [
                        "dapper",
                        "flink",
                        "onbevreesd",
                        "krachtig"
                    ],
                    "antonyms": [
                        "laf",
                        "bang",
                        "onzeker"
                    ],
                    "common_phrases": [
                        "stoer doen",
                        "een stoere vent",
                        "stoere taal"
                    ],
                    "target_lang_definitions": {
                        "en": "Appearing confident, powerful, and fearless, sometimes with the intention to impress.",
                        "ru": "Выглядящий уверенным в себе, сильным и бесстрашным, иногда с целью произвести впечатление."
                    },
                    "translations": {

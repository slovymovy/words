{
    "word_family": [
        "secondary",
        "secondly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c93f78d4-217f-3b54-a6fc-79080cc8759a",
                    "sense_definition": "Coming directly after the first one in a series or sequence.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the <w>second</w> time I have visited this museum.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>второй</w> раз, когда я посещаю этот музей."
                            }
                        },
                        {
                            "text": "They live on the <w>second</w> floor of the apartment building.",
                            "target_lang_translations": {
                                "ru": "Они живут на <w>втором</w> этаже многоквартирного дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "second place",
                        "second chance",
                        "second thought"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий непосредственно за первым в серии или последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второ́й",
                                "target_lang_sense_clarification": "порядковое числительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce8217ad-323c-3a2c-afef-1f67633f24c3",
                    "sense_definition": "Being an additional one of the same kind as the first.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He asked for a <w>second</w> opinion from another doctor.",
                            "target_lang_translations": {
                                "ru": "Он попросил <w>второе</w> мнение у другого врача."
                            }
                        },
                        {
                            "text": "The team got a <w>second</w> wind and played with renewed energy.",
                            "target_lang_translations": {
                                "ru": "У команды открылось <w>второе</w> дыхание, и они заиграли с новой энергией."
                            }
                        }
                    ],
                    "synonyms": [
                        "other",
                        "detail#Verb"
                    ],
                    "common_phrases": [
                        "second opinion",
                        "second wind",
                        "second nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся добавочным, таким же, как первый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второ́й",
                                "target_lang_sense_clarification": "дополнительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b86822a-c064-322a-8409-b762fc1745a9",
                    "sense_definition": "Lower in rank, quality, or importance than the first.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "For this company, profit is <w>second</w> to customer satisfaction.",
                            "target_lang_translations": {
                                "ru": "Для этой компании прибыль стоит на <w>втором</w> месте после удовлетворенности клиентов."
                            }
                        },
                        {
                            "text": "He is an excellent player, <w>second</w> to none in the league.",
                            "target_lang_translations": {
                                "ru": "Он отличный игрок, <w>не имеющий равных</w> в лиге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "second to none"
                    ],
                    "target_lang_definitions": {
                        "ru": "Более низкий по рангу, качеству или важности, чем первый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второстепе́нный",
                                "target_lang_sense_clarification": "менее важный"
                            },
                            {
                                "target_lang_word": "второ́й",
                                "target_lang_sense_clarification": "позиция в рейтинге"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "75586ff8-67f4-334d-92dd-02ef0edcfaa8",
                    "sense_definition": "A unit of time equal to one-sixtieth of a minute.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are sixty <w>seconds</w> in one minute.",
                            "target_lang_translations": {
                                "ru": "В одной минуте шестьдесят <w>секунд</w>."
                            }
                        },
                        {
                            "text": "The winner finished the race just one <w>second</w> ahead.",
                            "target_lang_translations": {
                                "ru": "Победитель закончил гонку всего на одну <w>секунду</w> раньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "sec"
                    ],
                    "common_phrases": [
                        "per second",
                        "split second"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица времени, равная одной шестидесятой минуты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секу́нда",
                                "target_lang_sense_clarification": "единица времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4428ba28-db98-38fe-a2fd-63c8e51df5dc",
                    "sense_definition": "A very short, indeterminate amount of time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please wait a <w>second</w> while I find my keys.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подождите <w>секунду</w>, пока я найду ключи."
                            }
                        },
                        {
                            "text": "I will be there in a <w>second</w>.",
                            "target_lang_translations": {
                                "ru": "Я буду там через <w>секунду</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "instant",
                        "jiffy",
                        "sec"
                    ],
                    "common_phrases": [
                        "just a second",
                        "wait a second",
                        "in a second"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень короткий, неопределенный промежуток времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секу́нда",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "моме́нт",
                                "target_lang_sense_clarification": "миг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "723abb28-cd3c-31bc-a9bb-f2abede044d5",
                    "sense_definition": "An extra serving of food taken after the first one.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pie was so good that everyone wanted <w>seconds</w>.",
                            "target_lang_translations": {
                                "ru": "Пирог был так хорош, что все захотели <w>добавки</w>."
                            }
                        },
                        {
                            "text": "There is plenty of pasta if you would like <w>seconds</w>.",
                            "target_lang_translations": {
                                "ru": "Пасты много, если хочешь <w>добавки</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительная порция еды, взятая после первой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доба́вка",
                                "target_lang_sense_clarification": "еда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1a4ec1e-7052-314a-8d20-e9b451e74033",
                    "sense_definition": "Manufactured items that are sold at a reduced price because they are slightly imperfect.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This store sells factory <w>seconds</w> at a big discount.",
                            "target_lang_translations": {
                                "ru": "Этот магазин продает заводской <w>брак</w> с большой скидкой."
                            }
                        },
                        {
                            "text": "These towels are <w>seconds</w>, so they might have small flaws.",
                            "target_lang_translations": {
                                "ru": "Эти полотенца — <w>второй</w> сорт, поэтому у них могут быть небольшие дефекты."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Промышленные товары, продаваемые по сниженной цене из-за небольших дефектов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "това́р второ́го со́рта",
                                "target_lang_sense_clarification": "качество"
                            },
                            {
                                "target_lang_word": "брак",
                                "target_lang_sense_clarification": "дефект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff11b923-b313-38fc-a734-02b012ef6b74",
                    "sense_definition": "A person who assists a combatant in a duel or boxing match.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer's <w>second</w> stepped into the ring to give him water.",
                            "target_lang_translations": {
                                "ru": "<w>Секундант</w> боксера вышел на ринг, чтобы дать ему воды."
                            }
                        },
                        {
                            "text": "In the past, a duelist would bring a <w>second</w> to ensure fair play.",
                            "target_lang_translations": {
                                "ru": "В прошлом дуэлянт приводил с собой <w>секунданта</w>, чтобы обеспечить честную игру."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, помогающий участнику дуэли или боксерского поединка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секунда́нт",
                                "target_lang_sense_clarification": "помощник"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fe5198d4-6ea8-318b-aadf-6d0ed27b47bc",
                    "sense_definition": "In the second place, rank, or position.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She finished <w>second</w> in the swimming competition.",
                            "target_lang_translations": {
                                "ru": "Она финишировала <w>второй</w> в соревнованиях по плаванию."
                            }
                        },
                        {
                            "text": "First we mix the ingredients, and <w>second</w> we bake the cake.",
                            "target_lang_translations": {
                                "ru": "Во-первых, мы смешиваем ингредиенты, а <w>во-вторых</w>, печем торт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "finish second"
                    ],
                    "target_lang_definitions": {
                        "ru": "На втором месте, ранге или позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второ́й",
                                "target_lang_sense_clarification": "позиция"
                            },
                            {
                                "target_lang_word": "во-вторы́х",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1065385d-9733-3035-aa28-91aee59a7ccd",
                    "sense_definition": "To formally express support for a motion or proposal in a meeting.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>second</w> the motion to adjourn the meeting.",
                            "target_lang_translations": {
                                "ru": "Я <w>поддерживаю</w> предложение закрыть собрание."
                            }
                        },
                        {
                            "text": "The proposal was <w>seconded</w> by the committee chair.",
                            "target_lang_translations": {
                                "ru": "Предложение было <w>поддержано</w> председателем комитета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "second the motion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально выразить поддержку предложению на собрании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подде́рживать",
                                "target_lang_sense_clarification": "предложение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "140529ce-8990-38f3-ae6c-1cd86dcc5254",
                    "sense_definition": "To transfer an employee temporarily to another position or organization.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>seconded</w> to the London office for six months.",
                            "target_lang_translations": {
                                "ru": "Он был <w>командирован</w> в лондонский офис на шесть месяцев."
                            }
                        },
                        {
                            "text": "The manager was <w>seconded</w> from the parent company to help us.",
                            "target_lang_translations": {
                                "ru": "Менеджер был <w>откомандирован</w> из материнской компании, чтобы помочь нам."
                            }
                        }
                    ],
                    "synonyms": [
                        "detail#Verb"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "business"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно перевести сотрудника на другую должность или в другую организацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командирова́ть",
                                "target_lang_sense_clarification": "направлять"
                            },
                            {
                                "target_lang_word": "откомандирова́ть",
                                "target_lang_sense_clarification": "переводить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
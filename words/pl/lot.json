            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a10b2996-8931-312f-8ac2-0d1c6ec584da",
                    "sense_definition": "Podróż odbywana środkiem transportu powietrznego, np. samolotem",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "aviation_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>lot</w> do Londynu został niestety odwołany z powodu pogody.",
                            "target_lang_translations": {
                                "ru": "Мой <w>рейс</w> в Лондон, к сожалению, был отменён из-за погоды."
                            }
                        },
                        {
                            "text": "Musimy szybko zarezerwować <w>lot</w>, zanim wzrosną ceny biletów.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро забронировать <w>перелёт</w>, пока не выросли цены на билеты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lot samolotem",
                        "odwołany lot",
                        "lot krajowy",
                        "lot międzynarodowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие, совершаемое на воздушном судне, например, на самолёте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рейс",
                                "target_lang_sense_clarification": "регулярный полёт по расписанию"
                            },
                            {
                                "target_lang_word": "перелёт",
                                "target_lang_sense_clarification": "перемещение из одного пункта в другой по воздуху"
                            },
                            {
                                "target_lang_word": "полёт",
                                "target_lang_sense_clarification": "путешествие по воздуху"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72fd1de5-1e39-32e1-83cd-51575920406a",
                    "sense_definition": "Czynność przemieszczania się w powietrzu lub w przestrzeni kosmicznej",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "aviation_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uwielbiam obserwować spokojny <w>lot</w> dzikich ptaków nad jeziorem.",
                            "target_lang_translations": {
                                "ru": "Я люблю наблюдать за спокойным <w>полётом</w> диких птиц над озером."
                            }
                        },
                        {
                            "text": "Pierwszy <w>lot</w> człowieka w kosmos był wielkim wydarzeniem historycznym.",
                            "target_lang_translations": {
                                "ru": "Первый <w>полёт</w> человека в космос стал великим историческим событием."
                            }
                        }
                    ],
                    "synonyms": [
                        "latanie",
                        "fruwanie"
                    ],
                    "common_phrases": [
                        "lot ptaka",
                        "lot kosmiczny",
                        "w locie",
                        "wysoki lot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие перемещения по воздуху или в космическом пространстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полёт",
                                "target_lang_sense_clarification": "процесс движения в воздухе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0268be98-03d7-3fd7-a51b-60dbbcaab61b",
                    "sense_definition": "Bardzo szybki ruch, pęd lub gwałtowne przemieszczanie się",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "aviation_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobra wiadomość rozeszła się po całej firmie <w>lotem</w> błyskawicy.",
                            "target_lang_translations": {
                                "ru": "Хорошая новость разлетелась по всей фирме <w>со скоростью</w> молнии."
                            }
                        },
                        {
                            "text": "Bramkarz wykazał się refleksem i chwycił piłkę w <w>locie</w>.",
                            "target_lang_translations": {
                                "ru": "Вратарь проявил реакцию и поймал мяч на <w>лету</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lotem błyskawicy",
                        "chwytać w lot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень быстрое движение, устремление или траектория перемещения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёт",
                                "target_lang_sense_clarification": "в выражении «на лету»"
                            },
                            {
                                "target_lang_word": "полёт",
                                "target_lang_sense_clarification": "траектория или стремительное движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0aa662aa-ff1b-3ff8-bd08-850f9a37db41",
                    "sense_definition": "Standardowa jednostka transakcyjna w handlu giełdowym określająca ilość towaru lub waluty",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inwestor zdecydował się kupić jeden <w>lot</w> złota na giełdzie towarowej.",
                            "target_lang_translations": {
                                "ru": "Инвестор решил купить один <w>лот</w> золота на товарной бирже."
                            }
                        },
                        {
                            "text": "Minimalna wielkość transakcji u tego brokera to jedna dziesiąta <w>lota</w>.",
                            "target_lang_translations": {
                                "ru": "Минимальный размер сделки у этого брокера составляет одну десятую <w>лота</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wielkość lota",
                        "mikro lot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартная единица сделки в биржевой торговле, определяющая количество товара или валюты"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лот",
                                "target_lang_sense_clarification": "биржевая единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08e38ffa-d05d-3a1c-9723-ffc56f602069",
                    "sense_definition": "Ciężarek zawieszony na lince służący do ręcznego pomiaru głębokości wody",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony marynarz rzucił <w>lot</w>, aby sprawdzić głębokość przy brzegu.",
                            "target_lang_translations": {
                                "ru": "Опытный моряк бросил <w>лот</w>, чтобы проверить глубину у берега."
                            }
                        },
                        {
                            "text": "W skrzyni z narzędziami nawigacyjnymi leżał stary, ołowiany <w>lot</w>.",
                            "target_lang_translations": {
                                "ru": "В ящике с навигационными инструментами лежал старый свинцовый <w>лот</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sonda"
                    ],
                    "common_phrases": [
                        "rzucać lot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Груз на лине (веревке) для измерения глубины воды"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лот",
                                "target_lang_sense_clarification": "морской прибор для измерения глубины"
                            }
                        ]
                    }
                }
            ]
        }
    ]

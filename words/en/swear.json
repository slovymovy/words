{
    "word_family": [
        "swearer",
        "swearing",
        "forswear",
        "outswear",
        "unswear"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "775f37ed-9c08-3aef-ba42-d9d146265c7d",
                    "sense_definition": "To use offensive, profane, or obscene language, often to express anger or frustration.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bad_language",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>swear</w> in front of the children.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>ругайся</w> при детях."
                            }
                        },
                        {
                            "text": "He started to <w>swear</w> loudly when he hit his thumb with the hammer.",
                            "target_lang_translations": {
                                "ru": "Он начал громко <w>ругаться</w>, когда ударил молотком по пальцу."
                            }
                        }
                    ],
                    "synonyms": [
                        "curse",
                        "cuss",
                        "blaspheme",
                        "turn the air blue"
                    ],
                    "antonyms": [
                        "mind one's language"
                    ],
                    "common_phrases": [
                        "swear at",
                        "swear like a trooper",
                        "swear like a sailor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать оскорбительную, ненормативную лексику, часто для выражения гнева или разочарования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ругаться",
                                "target_lang_sense_clarification": "произносить грубые слова, браниться"
                            },
                            {
                                "target_lang_word": "материться",
                                "target_lang_sense_clarification": "использовать нецензурную брань (мат)"
                            },
                            {
                                "target_lang_word": "сквернословить",
                                "target_lang_sense_clarification": "употреблять непристойные выражения (формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "479b4bc9-0201-35e9-84bc-ca0cbaa73510",
                    "sense_definition": "To state or promise intensely and sincerely that something is true.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assertion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>swear</w> that I did not eat the last piece of cake.",
                            "target_lang_translations": {
                                "ru": "Я <w>клянусь</w>, что не ел последний кусок торта."
                            }
                        },
                        {
                            "text": "She <w>swears</w> she has never met him before in her life.",
                            "target_lang_translations": {
                                "ru": "Она <w>клянется</w>, что никогда в жизни его не встречала."
                            }
                        }
                    ],
                    "synonyms": [
                        "insist",
                        "assert",
                        "affirm",
                        "avow"
                    ],
                    "common_phrases": [
                        "I swear",
                        "swear blind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво и искренне утверждать или обещать, что что-то является правдой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клясться",
                                "target_lang_sense_clarification": "давать честное слово, уверять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3b21ef4-18b1-397b-9035-4cb350d1004c",
                    "sense_definition": "To make a solemn formal promise or take an oath, often with legal or binding effect.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "oath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new president will <w>swear</w> to uphold the constitution.",
                            "target_lang_translations": {
                                "ru": "Новый президент <w>поклянется</w> соблюдать конституцию."
                            }
                        },
                        {
                            "text": "The witness <w>swore</w> to tell the whole truth in court.",
                            "target_lang_translations": {
                                "ru": "Свидетель <w>поклялся</w> говорить в суде всю правду."
                            }
                        }
                    ],
                    "synonyms": [
                        "vow",
                        "pledge",
                        "promise"
                    ],
                    "common_phrases": [
                        "swear an oath",
                        "swear allegiance",
                        "swear to God",
                        "swear on one's life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать торжественное официальное обещание или приносить присягу, часто имеющую юридическую силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присягать",
                                "target_lang_sense_clarification": "давать официальную присягу (в суде, армии)"
                            },
                            {
                                "target_lang_word": "клясться",
                                "target_lang_sense_clarification": "торжественно обещать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebf4945d-3e12-3e94-a6d2-018352eb83f9",
                    "sense_definition": "To administer an oath to a person, usually so they can take up an official position or give evidence.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "oath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mayor was <w>sworn</w> in during a ceremony at city hall.",
                            "target_lang_translations": {
                                "ru": "Мэр был <w>приведен к присяге</w> во время церемонии в мэрии."
                            }
                        },
                        {
                            "text": "Before testifying, the witness must be <w>sworn</w> by the clerk.",
                            "target_lang_translations": {
                                "ru": "Перед дачей показаний свидетель должен быть <w>приведен к присяге</w> секретарем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "swear in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить кого-либо к присяге, обычно для вступления в должность или дачи показаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводить к присяге",
                                "target_lang_sense_clarification": "официальная процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cea9c71f-2a92-3e09-8c3a-9bde6166589e",
                    "sense_definition": "To be lazy or rest for a short while during working hours.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "laziness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers decided to <w>swear</w> for a bit under the shade of the tree.",
                            "target_lang_translations": {
                                "ru": "Рабочие решили немного <w>побездельничать</w> в тени дерева."
                            }
                        },
                        {
                            "text": "Don't just <w>swear</w> there while there is so much work to be done.",
                            "target_lang_translations": {
                                "ru": "Не стой тут и не <w>ленись</w>, когда так много работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "laze about",
                        "loaf",
                        "take it easy"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "chiefly Scots"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лениться или отдыхать короткое время в рабочие часы (диалектное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лениться",
                                "target_lang_sense_clarification": "уклоняться от работы"
                            },
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "проводить время в праздности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bbbd7d3a-f85b-39dc-9938-09524a415973",
                    "sense_definition": "A rude, offensive, or obscene word.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bad_language",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He let out a loud <w>swear</w> when he dropped the heavy box on his foot.",
                            "target_lang_translations": {
                                "ru": "Он выкрикнул громкое <w>ругательство</w>, когда уронил тяжелую коробку на ногу."
                            }
                        },
                        {
                            "text": "The teacher told the student that using a <w>swear</w> in class was not acceptable.",
                            "target_lang_translations": {
                                "ru": "Учитель сказал ученику, что использовать <w>ругательство</w> в классе недопустимо."
                            }
                        }
                    ],
                    "synonyms": [
                        "curse",
                        "expletive",
                        "profanity",
                        "cussword",
                        "bad word"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубое, оскорбительное или непристойное слово."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ругательство",
                                "target_lang_sense_clarification": "бранное слово"
                            },
                            {
                                "target_lang_word": "брань",
                                "target_lang_sense_clarification": "совокупность бранных слов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e077ef3b-ade4-374e-98ba-421dfba72cdb",
                    "sense_definition": "A short period of rest or laziness during working hours.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "laziness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We took a quick <w>swear</w> in the afternoon to escape the heat.",
                            "target_lang_translations": {
                                "ru": "Мы устроили себе небольшую <w>передышку</w> днем, чтобы спастись от жары."
                            }
                        },
                        {
                            "text": "The farmer enjoyed a brief <w>swear</w> before returning to the fields.",
                            "target_lang_translations": {
                                "ru": "Фермер насладился коротким <w>отдыхом</w> перед возвращением в поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "nap",
                        "rest",
                        "break"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "chiefly Scots"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий период отдыха или лени в рабочее время (диалектное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передышка",
                                "target_lang_sense_clarification": "короткий отдых"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4d1efe03-83da-31ac-b8c7-dcaf2a2decf5",
                    "sense_definition": "Reluctant or unwilling to do something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "laziness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>sweer</w> to leave the warm house on such a cold morning.",
                            "target_lang_translations": {
                                "ru": "Ему было <w>неохота</w> выходить из теплого дома в такое холодное утро."
                            }
                        },
                        {
                            "text": "The old man was <w>sweer</w> to part with his money.",
                            "target_lang_translations": {
                                "ru": "Старик <w>не желал</w> расставаться со своими деньгами."
                            }
                        }
                    ],
                    "synonyms": [
                        "reluctant",
                        "loath",
                        "disinclined",
                        "unwilling"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "chiefly Scots; often spelled 'sweer'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неохотный или не желающий что-то делать (диалектное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неохотный",
                                "target_lang_sense_clarification": "делающий что-то без желания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b54c5253-5166-37c2-83a3-16f8b4a6fbdb",
                    "sense_definition": "Lazy, slow, or inactive.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "laziness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Get up and work, do not be so <w>sweer</w>.",
                            "target_lang_translations": {
                                "ru": "Вставай и работай, не будь таким <w>ленивым</w>."
                            }
                        },
                        {
                            "text": "The <w>sweer</w> horse refused to pull the cart any further.",
                            "target_lang_translations": {
                                "ru": "<w>Ленивая</w> лошадь отказалась тащить телегу дальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "lazy",
                        "slothful",
                        "sluggish",
                        "idle"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "chiefly Scots; often spelled 'sweer'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ленивый, медлительный или неактивный (диалектное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ленивый",
                                "target_lang_sense_clarification": "склонный к лени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2c2646cf-8078-3321-8f00-117794d4addc",
                    "sense_definition": "A standard quantity used to measure or express physical attributes such as length, weight, or time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kilogram is a standard <w>unit</w> of mass used in most countries.",
                            "target_lang_translations": {
                                "ru": "Килограмм — это стандартная <w>единица</w> массы, используемая в большинстве стран."
                            }
                        },
                        {
                            "text": "You must use the correct <w>unit</w> of measurement to get an accurate result.",
                            "target_lang_translations": {
                                "ru": "Вы должны использовать правильную <w>единицу</w> измерения, чтобы получить точный результат."
                            }
                        }
                    ],
                    "synonyms": [
                        "unit of measure",
                        "unit of measurement"
                    ],
                    "common_phrases": [
                        "unit of measurement",
                        "standard unit",
                        "metric unit",
                        "basic unit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартная величина, используемая для измерения или выражения физических свойств, таких как длина, вес или время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "единица",
                                "target_lang_sense_clarification": "мера измерения"
                            },
                            {
                                "target_lang_word": "единица измерения",
                                "target_lang_sense_clarification": "устойчивое словосочетание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ad22331-70d7-3af3-b093-a5b5392deec0",
                    "sense_definition": "A distinct group of people or a department working together within a larger organization, such as in a hospital, police force, or military.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient was transferred to the intensive care <w>unit</w> for constant monitoring.",
                            "target_lang_translations": {
                                "ru": "Пациента перевели в <w>отделение</w> реанимации для постоянного наблюдения."
                            }
                        },
                        {
                            "text": "A special police <w>unit</w> was called in to handle the emergency situation.",
                            "target_lang_translations": {
                                "ru": "Для разрешения чрезвычайной ситуации было вызвано специальное полицейское <w>подразделение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "intensive care unit",
                        "military unit",
                        "police unit",
                        "special unit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная группа людей или отдел, работающие вместе в рамках более крупной организации, например, в больнице, полиции или армии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подразделение",
                                "target_lang_sense_clarification": "организационная единица"
                            },
                            {
                                "target_lang_word": "отделение",
                                "target_lang_sense_clarification": "в больнице или учреждении"
                            },
                            {
                                "target_lang_word": "отряд",
                                "target_lang_sense_clarification": "в полиции или спецслужбах"
                            },
                            {
                                "target_lang_word": "часть",
                                "target_lang_sense_clarification": "военная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdc0c9e3-e79d-32bf-a557-2d3d76ad97b7",
                    "sense_definition": "A single piece of equipment, machine, or device, especially one that is part of a larger system.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to repair the air conditioning <w>unit</w> before the summer heat begins.",
                            "target_lang_translations": {
                                "ru": "Нам нужно отремонтировать <w>блок</w> кондиционирования воздуха до начала летней жары."
                            }
                        },
                        {
                            "text": "The central processing <w>unit</w> is the most important part of this computer.",
                            "target_lang_translations": {
                                "ru": "Центральное процессорное <w>устройство</w> — самая важная часть этого компьютера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "air conditioning unit",
                        "control unit",
                        "display unit",
                        "processing unit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный предмет оборудования, механизм или устройство, особенно являющееся частью более крупной системы."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "af5bf910-6491-3834-a4f0-b32f7d6a39d6",
                    "sense_definition": "Без вины, не совершив проступка или преступления; также простодушно, наивно, не имея злого умысла.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка <w>невинно</w> хлопала ресницами, делая вид, что не понимает намёка.",
                            "target_lang_translations": {
                                "en": "The girl <w>innocently</w> batted her eyelashes, pretending not to understand the hint."
                            }
                        },
                        {
                            "text": "Многие люди пострадали <w>невинно</w> в годы суровых политических репрессий.",
                            "target_lang_translations": {
                                "en": "Many people suffered <w>innocently</w> during the years of harsh political repressions."
                            }
                        }
                    ],
                    "synonyms": [
                        "невиновно",
                        "безвинно",
                        "наивно",
                        "простодушно",
                        "безвредно"
                    ],
                    "antonyms": [
                        "виновно"
                    ],
                    "common_phrases": [
                        "невинно пострадать",
                        "невинно улыбаться",
                        "выглядеть невинно",
                        "невинно осуждённый"
                    ],
                    "target_lang_definitions": {
                        "en": "Without guilt, having committed no crime; also simply, naively, without evil intent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "innocently",
                                "target_lang_sense_clarification": "without guilt or malicious intent"
                            },
                            {
                                "target_lang_word": "naively",
                                "target_lang_sense_clarification": "in a simple-minded or artless manner"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ca8f5d5c-dd56-3143-bb96-e1fad9a8f95c",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>невинный</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это детское развлечение вполне <w>невинно</w> и никому не причинит вреда.",
                            "target_lang_translations": {
                                "en": "This children's entertainment is quite <w>innocent</w> and will cause no harm to anyone."
                            }
                        },
                        {
                            "text": "Выражение его лица было <w>невинно</w>, словно он никогда не совершал ошибок.",
                            "target_lang_translations": {
                                "en": "The expression on his face was <w>innocent</w>, as if he had never made mistakes."
                            }
                        }
                    ],
                    "synonyms": [
                        "чисто",
                        "безгрешно",
                        "непорочно"
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "302d9532-7fd0-3dd2-b415-347d59e4c3f5",
                    "sense_definition": "A period of one hundred consecutive years.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are currently living in the twenty-first <w>century</w>.",
                            "target_lang_translations": {
                                "ru": "Мы сейчас живем в двадцать первом <w>веке</w>."
                            }
                        },
                        {
                            "text": "The old castle has stood on the hill for more than a <w>century</w>.",
                            "target_lang_translations": {
                                "ru": "Старый замок стоит на холме уже более <w>века</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "twenty-first century",
                        "turn of the century",
                        "quarter of a century",
                        "half a century"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени продолжительностью в сто лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "век",
                                "target_lang_sense_clarification": "календарный период"
                            },
                            {
                                "target_lang_word": "столетие",
                                "target_lang_sense_clarification": "промежуток времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "451b0668-d19d-3e14-af36-86f5d976f66e",
                    "sense_definition": "A score of one hundred runs or points in sports such as cricket or snooker.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_score",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The batsman raised his bat to the crowd after scoring a <w>century</w>.",
                            "target_lang_translations": {
                                "ru": "Отбивающий поднял биту к зрителям, набрав <w>сотню</w>."
                            }
                        },
                        {
                            "text": "He managed to make a <w>century</w> in the final frame of the tournament.",
                            "target_lang_translations": {
                                "ru": "Ему удалось сделать <w>сенчури</w> в финальном фрейме турнира."
                            }
                        }
                    ],
                    "common_phrases": [
                        "score a century",
                        "make a century"
                    ],
                    "target_lang_definitions": {
                        "ru": "Счёт в сто очков или пробежек в таких видах спорта, как крикет или снукер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сенчури",
                                "target_lang_sense_clarification": "термин в снукере и крикете"
                            },
                            {
                                "target_lang_word": "сотня",
                                "target_lang_sense_clarification": "разговорное, о счёте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffadd5ad-329b-3e6f-b380-2b63a1ebcbae",
                    "sense_definition": "A unit in the ancient Roman army, originally consisting of one hundred soldiers.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "roman_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The centurion was responsible for the discipline of his <w>century</w>.",
                            "target_lang_translations": {
                                "ru": "Центурион отвечал за дисциплину в своей <w>центурии</w>."
                            }
                        },
                        {
                            "text": "A Roman legion was typically divided into several cohorts and <w>centuries</w>.",
                            "target_lang_translations": {
                                "ru": "Римский легион обычно делился на несколько когорт и <w>центурий</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "centuria"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подразделение в древнеримской армии, первоначально состоявшее из ста солдат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "центурия",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55d28aca-c5bc-3179-9db6-e4fafe2c15ff",
                    "sense_definition": "A bicycle race or ride covering a distance of one hundred miles.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cycling_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been training all summer to ride her first <w>century</w>.",
                            "target_lang_translations": {
                                "ru": "Она тренировалась всё лето, чтобы проехать свои первые <w>100 миль</w>."
                            }
                        },
                        {
                            "text": "Completing a <w>century</w> is a significant milestone for many amateur cyclists.",
                            "target_lang_translations": {
                                "ru": "Преодоление <w>дистанции в 100 миль</w> — важная веха для многих велосипедистов-любителей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Велосипедный заезд на дистанцию в сто миль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заезд на 100 миль",
                                "target_lang_sense_clarification": "велоспорт"
                            }
                        ]
                    }
                }
            ]
        },
        {

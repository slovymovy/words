    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "12af108c-df62-3009-bb66-9e2a637510d2",
                    "sense_definition": "Having or producing a shell, especially referring to bivalve mollusks.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biological_shell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The biologist carefully studied the <w>conchiferous</w> organism found in the tide pool.",
                            "target_lang_translations": {
                                "ru": "Биолог внимательно изучил <w>раковинный</w> организм, найденный в приливной луже."
                            }
                        },
                        {
                            "text": "Many <w>conchiferous</w> creatures rely on their hard outer layers for protection against predators.",
                            "target_lang_translations": {
                                "ru": "Многие <w>раковинные</w> существа полагаются на свои твердые внешние слои для защиты от хищников."
                            }
                        }
                    ],
                    "synonyms": [
                        "shell-bearing"
                    ],
                    "common_phrases": [
                        "conchiferous mollusks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий раковину или производящий её, особенно в отношении двустворчатых моллюсков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раковинный",
                                "target_lang_sense_clarification": "имеющий раковину"
                            },
                            {
                                "target_lang_word": "раковиносный",
                                "target_lang_sense_clarification": "несущий раковину (биол.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c33143f-590f-332e-ab03-446e1e057431",
                    "sense_definition": "Containing or consisting of shells.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geological_shell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The geologist identified a layer of <w>conchiferous</w> limestone in the cliff face.",
                            "target_lang_translations": {
                                "ru": "Геолог обнаружил слой <w>ракушечного</w> известняка в отвесной скале."
                            }
                        },
                        {
                            "text": "Excavators had to dig through dense <w>conchiferous</w> sand to reach the bedrock.",
                            "target_lang_translations": {
                                "ru": "Экскаваторам пришлось копать сквозь плотный <w>ракушечный</w> песок, чтобы добраться до коренной породы."
                            }
                        }
                    ],
                    "synonyms": [
                        "shelly"
                    ],
                    "common_phrases": [
                        "conchiferous limestone",
                        "conchiferous sand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий раковины или состоящий из них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ракушечный",
                                "target_lang_sense_clarification": "состоящий из ракушек (о породах)"
                            },
                            {
                                "target_lang_word": "раковинный",
                                "target_lang_sense_clarification": "содержащий остатки раковин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

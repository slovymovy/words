                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_entity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Chrześcijanie wierzą w jednego Boga w trzech osobach: <w>Ojca</w>, Syna i Ducha Świętego.",
                            "target_lang_translations": {
                                "ru": "Христиане верят в единого Бога в трёх лицах: <w>Отца</w>, Сына и Святого Духа."
                            }
                        },
                        {
                            "text": "Codzienna modlitwa jest rozmową z Bogiem <w>Ojcem</w>.",
                            "target_lang_translations": {
                                "ru": "Ежедневная молитва — это разговор с Богом <w>Отцом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Bóg",
                        "Stwórca",
                        "Pan"
                    ],
                    "common_phrases": [
                        "Bóg Ojciec",
                        "w imię Ojca",
                        "chwała Ojcu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "religious"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В христианстве: первая ипостась Святой Троицы; Бог."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Отец",
                                "target_lang_sense_clarification": "Бог"
                            }
                        ]
                    }
                }

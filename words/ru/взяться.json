{
    "word_family": [
        "взять",
        "браться",
        "взятие"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aeb74f1b-4b62-3b6d-b09e-c0c0f80281fb",
                    "sense_definition": "Ухватиться за что-либо рукой или соединить руки друг с другом",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grasp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Возьмёмся за́ руки, друзья́, чтоб не пропа́сть поодино́чке."
                        },
                        {
                            "text": "Чтобы не поскользнуться на льду, бабушка крепко <взялась> за мою руку."
                        },
                        {
                            "text": "Пассажирам следует <взяться> за поручни во время движения автобуса."
                        }
                    ],
                    "synonyms": [
                        "схватиться"
                    ],
                    "antonyms": [
                        "отпустить"
                    ],
                    "common_phrases": [
                        "взяться за руки",
                        "взяться за перила",
                        "взяться за ручку"
                    ]
                },
                {
                    "sense_id": "0f7ca031-6df5-3e5a-a370-24f9e54b889b",
                    "sense_definition": "Начать делать что-либо, приступить к выполнению какого-либо дела или обязанностей",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С понеде́льника я обязательно <возьму́сь> за изучение английского языка."
                        },
                        {
                            "text": "Мы решили дружно <взяться> за работу, чтобы закончить проект вовремя."
                        },
                        {
                            "text": "Пора тебе <взяться> за ум и начать серьёзно относиться к учёбе."
                        }
                    ],
                    "synonyms": [
                        "приступить",
                        "заняться"
                    ],
                    "common_phrases": [
                        "взяться за дело",
                        "взяться за ум",
                        "взяться за работу",
                        "взяться за учёбу"
                    ]
                },
                {
                    "sense_id": "23e87d98-15a0-3ba0-be65-0919606a53cf",
                    "sense_definition": "Появиться, возникнуть (часто неожиданно или неизвестно откуда)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "appear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Е́сли не хвата́ет де́нег сего́дня, отку́да они́ <возьму́тся> за́втра?"
                        },
                        {
                            "text": "Вдруг откуда ни <возьмись> налетел сильный ветер и сорвал шляпу."
                        },
                        {
                            "text": "У него <взялись> силы, чтобы добежать до финиша первым."
                        }
                    ],
                    "synonyms": [
                        "появиться",
                        "возникнуть"
                    ],
                    "common_phrases": [
                        "откуда ни возьмись",
                        "откуда что взялось"
                    ]
                },
                {
                    "sense_id": "b3c1d453-9620-369b-af21-60008537009c",
                    "sense_definition": "Покрыться слоем чего-либо (льдом, ржавчиной, влагой)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cover_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За ночь лужи на дорогах <взялись> тонким прозрачным ледком."
                        },
                        {
                            "text": "Старая медная монета от сырости <взялась> зеленью."
                        }
                    ],
                    "common_phrases": [
                        "взяться льдом",
                        "взяться коркой",
                        "взяться ржавчиной"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "2dd3c038-30a2-3b63-a50d-5dc597fafc25",
                    "sense_definition": "Затвердеть, скрепиться (о клее, цементе, растворе)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "harden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нужно подождать, пока клей хорошо <возьмётся>, прежде чем использовать деталь."
                        },
                        {
                            "text": "Цементный раствор ещё не <взялся>, поэтому ходить по полу нельзя."
                        }
                    ],
                    "synonyms": [
                        "схватиться"
                    ]
                }
            ]
        }
    ]
}
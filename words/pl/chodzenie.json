                {
                    "sense_id": "f9eb0f23-f326-3b97-bd02-4489f56d0330",
                    "sense_definition": "pozostawanie w bliskiej zażyłości emocjonalnej z drugą osobą, bycie parą",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W piątek świętujemy rocznicę naszego <w>chodzenia</w>.",

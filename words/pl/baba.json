                },
                {
                    "sense_id": "6a2cb485-8309-30a6-a663-e2d897f9dd99",
                    "sense_definition": "Postać z baśni i legend, zazwyczaj stara, brzydka czarownica posiadająca magiczne moce.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "folklore",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "W chatce na kurzej stopce mieszkała zła <baba>, która porywała dzieci.",
                            "target_lang_translations": {
                                "ru": "В избушке на курьих ножках жила злая <баба> (Баба-яга), которая похищала детей."
                            }
                        },
                        {
                            "text": "Dzieci bały się opowieści o starej <babie>, która rzucała uroki na wędrowców.",
                            "target_lang_translations": {
                                "ru": "Дети боялись рассказов о старой <бабе> (ведьме), которая накладывала проклятия на путников."
                            }
                        }
                    ],

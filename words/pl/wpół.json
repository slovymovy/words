{
    "word_family": [
        "pół",
        "połowa",
        "połówka"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "27c9cec4-71bd-3dc1-9477-e7ac9a405012",
                    "sense_definition": "Używane przy podawaniu czasu, oznaczając 30 minut przed wymienioną pełną godziną.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spotykamy się jutro rano o <w>wpół</w> do ósmej.",
                            "target_lang_translations": {
                                "ru": "Мы встречаемся завтра утром в <w>пол</w>восьмого."
                            }
                        },
                        {
                            "text": "Jest już <w>wpół</w> do szóstej, więc musimy się pospieszyć.",
                            "target_lang_translations": {
                                "ru": "Уже <w>пол</w>шестого, так что нам нужно поторопиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wpół do drugiej",
                        "wpół do dziesiątej",
                        "o wpół do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при указании времени, обозначая 30 минут до указанного часа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пол-",
                                "target_lang_sense_clarification": "в составе слов, обозначающих время (напр. полвторого)"
                            },
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "30 минут"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45c7c717-ccd2-3e13-ba9e-70c953dcd4a2",
                    "sense_definition": "W pasie, w talii (używane przy opisie obejmowania, chwytania lub trzymania kogoś).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_middle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopak objął dziewczynę <w>wpół</w> i przytulił ją do siebie.",
                            "target_lang_translations": {
                                "ru": "Парень обнял девушку <w>за талию</w> и прижал её к себе."
                            }
                        },
                        {
                            "text": "Obrońca złapał napastnika <w>wpół</w>, aby zatrzymać jego bieg.",
                            "target_lang_translations": {
                                "ru": "Защитник схватил нападающего <w>за талию</w>, чтобы остановить его бег."
                            }
                        }
                    ],
                    "synonyms": [
                        "w pasie"
                    ],
                    "common_phrases": [
                        "objąć wpół",
                        "złapać wpół",
                        "chwycić wpół"
                    ],
                    "target_lang_definitions": {
                        "ru": "В поясе, за талию (используется при описании объятий, захвата или удержания кого-либо)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за талию",
                                "target_lang_sense_clarification": "обхватить, обнять"
                            },
                            {
                                "target_lang_word": "поперёк",
                                "target_lang_sense_clarification": "схватить поперёк туловища"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cb6afbd-2c7b-3502-b014-3245692a78f6",
                    "sense_definition": "Na dwie części lub zginając się w połowie długości ciała.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_middle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna zgiął się <w>wpół</w> z powodu nagłego bólu brzucha.",
                            "target_lang_translations": {
                                "ru": "Мужчина согнулся <w>пополам</w> от внезапной боли в животе."
                            }
                        },
                        {
                            "text": "Podczas wichury stare drzewo złamało się <w>wpół</w>.",
                            "target_lang_translations": {
                                "ru": "Во время бури старое дерево сломалось <w>пополам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "na pół"
                    ],
                    "common_phrases": [
                        "zgiąć się wpół",
                        "złamać się wpół",
                        "złożyć wpół"
                    ],
                    "target_lang_definitions": {
                        "ru": "На две части или сгибаясь пополам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пополам",
                                "target_lang_sense_clarification": "надвое, сгибаясь посередине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7049f64-f4e2-3fa3-b728-3798c8609b26",
                    "sense_definition": "Częściowo, niecałkowicie, w pewnym stopniu (często występuje w wyrażeniu „na wpół”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ranny żołnierz leżał na ziemi na <w>wpół</w> przytomny.",
                            "target_lang_translations": {
                                "ru": "Раненый солдат лежал на земле <w>наполовину</w> в сознании."
                            }
                        },
                        {
                            "text": "Odpowiedziała mu na <w>wpół</w> żartem, a na <w>wpół</w> serio.",
                            "target_lang_translations": {
                                "ru": "Она ответила ему <w>наполовину</w> в шутку, а <w>наполовину</w> всерьез."
                            }
                        }
                    ],
                    "synonyms": [
                        "częściowo",
                        "niecałkowicie",
                        "na poły",
                        "niezupełnie",
                        "na wpół"
                    ],
                    "common_phrases": [
                        "na wpół",
                        "na wpół żywy",
                        "na wpół drwiąco"
                    ],
                    "target_lang_definitions": {
                        "ru": "Частично, не полностью, в некоторой степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наполовину",
                                "target_lang_sense_clarification": "частично"
                            },
                            {
                                "target_lang_word": "полу-",
                                "target_lang_sense_clarification": "приставка (напр. полуживой)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        {
                            "text": "I don't want any <w>verbal</w> from you, just do as you are told.",
                            "target_lang_translations": {
                                "ru": "Я не хочу слышать от тебя никаких <w>пререканий</w>, просто делай, что тебе говорят."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give it the verbal"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Агрессивная речь, ругань или оскорбления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ру́гань",
                                "target_lang_sense_clarification": "abuse"
                            },
                            {
                                "target_lang_word": "пререка́ния",
                                "target_lang_sense_clarification": "backchat, argument"
                            }
                        ]
                    }
                }
            ]
        },
        {

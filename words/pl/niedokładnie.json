            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7612d9db-9830-39b8-8357-d6e32d94af6b",
                    "sense_definition": "W sposób pozbawiony precyzji, nieścisły lub nieco odbiegający od rzeczywistości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "precision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń <w>niedokładnie</w> zmierzył kąty w trójkącie i otrzymał błędny wynik.",
                            "target_lang_translations": {
                                "ru": "Ученик <w>неточно</w> измерил углы в треугольнике и получил ошибочный результат."
                            }
                        },
                        {
                            "text": "Świadek opisał wygląd sprawcy dość <w>niedokładnie</w>, co utrudniło śledztwo.",
                            "target_lang_translations": {
                                "ru": "Свидетель описал внешность преступника довольно <w>приблизительно</w>, что затруднило расследование."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprecyzyjnie",
                        "nieściśle"
                    ],
                    "antonyms": [
                        "precyzyjnie",
                        "dokładnie"
                    ],
                    "common_phrases": [
                        "niedokładnie zmierzyć",
                        "niedokładnie opisać",
                        "niedokładnie określić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неточно, с погрешностями или отклонениями от истины; приблизительно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неточно",
                                "target_lang_sense_clarification": "об измерениях, данных"
                            },
                            {
                                "target_lang_word": "приблизительно",
                                "target_lang_sense_clarification": "примерно, не детально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "180c35b6-b034-3525-b6ce-b30424bac428",
                    "sense_definition": "W sposób niestaranny, powierzchowny, bez dbałości o szczegóły podczas wykonywania czynności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli będziesz <w>niedokładnie</w> myć naczynia, zostaną na nich tłuste plamy.",
                            "target_lang_translations": {
                                "ru": "Если ты будешь <w>небрежно</w> мыть посуду, на ней останутся жирные пятна."
                            }
                        },
                        {
                            "text": "Pracownik wykonał swoje zadanie <w>niedokładnie</w> i musiał je poprawiać.",
                            "target_lang_translations": {
                                "ru": "Сотрудник выполнил своё задание <w>неаккуратно</w> и был вынужден его переделывать."
                            }
                        }
                    ],
                    "synonyms": [
                        "niestarannie",
                        "niedbale",
                        "niechlujnie"
                    ],
                    "antonyms": [
                        "dokładnie"
                    ],
                    "common_phrases": [
                        "niedokładnie posprzątać",
                        "niedokładnie umyć",
                        "niedokładnie wykonać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небрежно, без должного внимания к деталям или старания при выполнении действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "небрежно",
                                "target_lang_sense_clarification": "без старания"
                            },
                            {
                                "target_lang_word": "неаккуратно",
                                "target_lang_sense_clarification": "не тщательно, грязно"
                            },
                            {
                                "target_lang_word": "поверхностно",
                                "target_lang_sense_clarification": "не углубляясь в детали"
                            }
                        ]
                    }
                }
            ]
        }
    ]

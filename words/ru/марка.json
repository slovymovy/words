    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7f0946b4-73f5-3b7e-a3a8-c3ec015e4f39",
                    "sense_definition": "Знак оплаты почтовых сборов, обычно в виде маленького бумажного прямоугольника с зубчиками и рисунком, который наклеивается на конверт.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sign_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я купил конверт и почтовую <w>марку</w>.",
                            "target_lang_translations": {
                                "en": "I bought an envelope and a postage <w>stamp</w>."
                            }
                        },
                        {
                            "text": "Мой брат коллекционирует редкие <w>марки</w>.",
                            "target_lang_translations": {
                                "en": "My brother collects rare <w>stamps</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "почтовая марка",
                        "наклеить марку",
                        "коллекция марок",
                        "гашеная марка"
                    ],
                    "target_lang_definitions": {
                        "en": "A sign of payment of postage fees, usually in the form of a small paper rectangle with perforations and a picture, which is glued to an envelope."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stamp",
                                "target_lang_sense_clarification": "postage"
                            },
                            {
                                "target_lang_word": "postage stamp",
                                "target_lang_sense_clarification": "full term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a830124-fbb4-36bf-96c8-d4f8670ad0f7",
                    "sense_definition": "Фирменный знак или наименование, идентифицирующее производителя товара; бренд.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "commercial_id",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта торговая <w>марка</w> известна во всем мире.",
                            "target_lang_translations": {
                                "en": "This trade<w>mark</w> is known all over the world."
                            }
                        },
                        {
                            "text": "Мы доверяем качеству этой <w>марки</w> одежды.",
                            "target_lang_translations": {
                                "en": "We trust the quality of this clothing <w>brand</w>."
                            }

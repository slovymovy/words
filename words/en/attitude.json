                },
                {
                    "sense_id": "4001ab00-02b9-3159-a1a6-82d174bad772",
                    "sense_definition": "Unpleasant behavior.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I asked the waiter for a clean fork and all I got was <w>attitude</w>.",

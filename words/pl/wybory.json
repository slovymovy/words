                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "принятие решения; в русском языке в этом значении обычно используется в единственном числе"
                            },
                            {
                                "target_lang_word": "решения",
                                "target_lang_sense_clarification": "сделанный выбор, варианты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfaa81ff-d6bb-37f5-ab48-4d060656947a",
                    "sense_definition": "proces, w którym obywatele wybierają spośród zgłoszonych kandydatów swoich przedstawicieli do organów władzy",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym roku odbędą się <w>wybory</w> prezydenckie.",
                            "target_lang_translations": {
                                "ru": "В следующем году пройдут президентские <w>выборы</w>."
                            }
                        },
                        {
                            "text": "Wielu Polaków bierze udział w <w>wyborach</w> samorządowych.",
                            "target_lang_translations": {
                                "ru": "Многие поляки участвуют в <w>выборах</w> в органы местного самоуправления."
                            }
                        }
                    ],
                    "synonyms": [
                        "głosowanie",
                        "elekcja"
                    ],
                    "common_phrases": [
                        "wybory prezydenckie",
                        "wybory parlamentarne",
                        "wygrać wybory",
                        "przegrać wybory"
                    ],
                    "target_lang_definitions": {
                        "ru": "процесс избрания представителей власти или должностных лиц путем голосования"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выборы",
                                "target_lang_sense_clarification": "политическая процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fb39cd7-220c-3fbe-852f-911ff2a273a0",
                    "sense_definition": "rodzaj konkursu, w którym wybiera się osoby kandydujące do jakiegoś tytułu, np. najpiękniejszej kobiety",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "contest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W telewizji transmitowano finałowe <w>wybory</w> miss.",
                            "target_lang_translations": {
                                "ru": "По телевидению транслировали финал <w>конкурса</w> красоты."
                            }
                        },
                        {
                            "text": "Jury miało trudne zadanie podczas ostatnich <w>wyborów</w>.",
                            "target_lang_translations": {
                                "ru": "У жюри была трудная задача во время последнего <w>конкурса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wybory miss"
                    ],
                    "target_lang_definitions": {
                        "ru": "конкурс, в котором выбирают победителя на определенный титул (например, королеву красоты)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкурс",
                                "target_lang_sense_clarification": "соревнование, отбор"

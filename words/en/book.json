    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "76694d0e-312c-3600-8d76-d7a313d6fe66",
                    "sense_definition": "A collection of sheets of paper bound together to hinge at one edge, containing printed or written material, pictures, etc.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She opened the <w>book</w> to page 37 and began to read aloud.",
                            "target_lang_translations": {
                                "ru": "Она открыла <w>книгу</w> на 37-й странице и начала читать вслух."
                            }
                        },
                        {
                            "text": "He put the <w>book</w> back on the shelf after he finished reading it.",
                            "target_lang_translations": {
                                "ru": "Он поставил <w>книгу</w> обратно на полку, когда закончил читать."
                            }
                        }
                    ],
                    "synonyms": [
                        "tome",
                        "volume"
                    ],
                    "common_phrases": [
                        "read a book",
                        "open a book",
                        "close a book"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сброшюрованные листы бумаги с текстом или изображениями, обычно в переплете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "книга",
                                "target_lang_sense_clarification": "печатное издание"
                            },
                            {
                                "target_lang_word": "том",
                                "target_lang_sense_clarification": "отдельная книга из серии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7d23ded-1953-3d2e-8a66-227e4d639996",
                    "sense_definition": "A long written work fit for publication, such as a novel or textbook.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have three copies of his first <w>book</w> in my collection.",
                            "target_lang_translations": {
                                "ru": "У меня в коллекции есть три экземпляра его первой <w>книги</w>."
                            }
                        },
                        {
                            "text": "It took her five years to write a <w>book</w> about her travels.",
                            "target_lang_translations": {
                                "ru": "У неё ушло пять лет на то, чтобы написать <w>книгу</w> о своих путешествиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "volume"
                    ],
                    "common_phrases": [
                        "write a book",
                        "publish a book"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное литературное или научное произведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "книга",
                                "target_lang_sense_clarification": "литературное произведение"
                            },
                            {
                                "target_lang_word": "произведение",
                                "target_lang_sense_clarification": "сочинение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b884d532-c9d0-32d7-b7bc-418e3b3eb7f8",
                    "sense_definition": "Records of the financial accounts of a business.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "record_keeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant checked the <w>books</w> to find where the money went.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер проверил <w>бухгалтерские книги</w>, чтобы выяснить, куда ушли деньги."
                            }
                        },
                        {
                            "text": "He was hired to keep the <w>books</w> for the small company.",
                            "target_lang_translations": {
                                "ru": "Его наняли вести <w>бухгалтерские книги</w> для небольшой компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "account",
                        "record"
                    ],
                    "common_phrases": [
                        "balance the books",
                        "cook the books",
                        "keep the books"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовые отчеты или бухгалтерские записи компании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бухгалтерские книги",
                                "target_lang_sense_clarification": "финансовая отчетность"
                            },
                            {
                                "target_lang_word": "счета",
                                "target_lang_sense_clarification": "учетные записи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ae728dd-ac6e-38df-aef0-398e76e50b1a",
                    "sense_definition": "A convenient collection of small paper items bound together for individual use.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She went to the post office to buy a <w>book</w> of stamps.",
                            "target_lang_translations": {
                                "ru": "Она пошла на почту, чтобы купить <w>набор</w> марок."
                            }
                        },
                        {
                            "text": "He handed me a <w>book</w> of matches to light the candle.",
                            "target_lang_translations": {
                                "ru": "Он протянул мне <w>книжечку</w> спичек, чтобы зажечь свечу."
                            }
                        }
                    ],
                    "synonyms": [
                        "booklet"
                    ],
                    "common_phrases": [
                        "book of stamps",
                        "book of matches",
                        "book of tickets"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой набор бумажных предметов, скрепленных вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "книжечка",
                                "target_lang_sense_clarification": "маленькая книга (напр. спичек)"
                            },
                            {
                                "target_lang_word": "набор",
                                "target_lang_sense_clarification": "комплект (напр. марок)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97c7d092-0b34-3180-ac93-bd7e9ae2727a",
                    "sense_definition": "A major division of a long written work.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Genesis is the first <w>book</w> of the Bible.",
                            "target_lang_translations": {
                                "ru": "Бытие — это первая <w>книга</w> Библии."
                            }
                        },
                        {
                            "text": "The students are currently studying the second <w>book</w> of the epic poem.",
                            "target_lang_translations": {
                                "ru": "Студенты сейчас изучают вторую <w>книгу</w> эпической поэмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "volume"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный раздел литературного произведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "книга",
                                "target_lang_sense_clarification": "раздел произведения"
                            },
                            {
                                "target_lang_word": "том",
                                "target_lang_sense_clarification": "часть издания"
                            },
                            {
                                "target_lang_word": "часть",
                                "target_lang_sense_clarification": "раздел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b6c70ac-3d5b-3d44-a42b-e6cc0e2eaf93",
                    "sense_definition": "A list of players who have received a warning in a game.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "record_keeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain went into the <w>book</w> for arguing with the referee.",
                            "target_lang_translations": {
                                "ru": "Капитан попал в <w>протокол</w> за споры с судьей."
                            }
                        },
                        {
                            "text": "After the foul, the referee took his name and put him in the <w>book</w>.",
                            "target_lang_translations": {
                                "ru": "После фола судья записал его имя и занес его в <w>протокол</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "record"
                    ],
                    "common_phrases": [
                        "in the book"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список игроков, получивших предупреждение во время игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протокол",
                                "target_lang_sense_clarification": "судейский список"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46572b5c-a64e-3b9b-979b-d4b5b41aeb08",
                    "sense_definition": "The script of a musical or opera.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The music was beautiful, but the <w>book</w> was rather weak.",
                            "target_lang_translations": {
                                "ru": "Музыка была прекрасной, но <w>либретто</w> было довольно слабым."
                            }
                        },
                        {
                            "text": "He wrote the <w>book</w> for the show, while his partner wrote the lyrics.",
                            "target_lang_translations": {
                                "ru": "Он написал <w>либретто</w> для шоу, а его партнер написал тексты песен."
                            }
                        }
                    ],
                    "synonyms": [
                        "libretto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сценарий или текст мюзикла или оперы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "либретто",
                                "target_lang_sense_clarification": "текст оперы/мюзикла"
                            },
                            {
                                "target_lang_word": "сценарий",
                                "target_lang_sense_clarification": "сюжетная основа"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "06bac635-10d2-340b-96cc-ba0e828ab360",
                    "sense_definition": "To reserve something, such as a room or ticket, for future use.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reservation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I want to <w>book</w> a hotel room for tomorrow night.",
                            "target_lang_translations": {
                                "ru": "Я хочу <w>забронировать</w> номер в отеле на завтрашнюю ночь."
                            }
                        },
                        {
                            "text": "You should <w>book</w> your flight early to get a good price.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>забронировать</w> рейс заранее, чтобы получить хорошую цену."
                            }
                        }
                    ],
                    "synonyms": [
                        "reserve",
                        "schedule"
                    ],
                    "common_phrases": [
                        "book a flight",
                        "book a table",
                        "book a room",
                        "fully booked"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зарезервировать что-либо (например, номер или билет) для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забронировать",
                                "target_lang_sense_clarification": "зарезервировать"
                            },
                            {
                                "target_lang_word": "заказать",
                                "target_lang_sense_clarification": "сделать заказ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85bbe427-9645-3e92-aa47-d4d91af2ffa9",
                    "sense_definition": "To record the name and details of a suspected offender and the offence for legal action.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "record_keeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer decided to <w>book</w> him for driving too fast.",
                            "target_lang_translations": {
                                "ru": "Полицейский решил <w>оформить</w> его за превышение скорости."
                            }
                        },
                        {
                            "text": "She was taken to the station and <w>booked</w> for theft.",
                            "target_lang_translations": {
                                "ru": "Ее доставили в участок и <w>оформили</w> за кражу."
                            }
                        }
                    ],
                    "synonyms": [
                        "record",
                        "register"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зарегистрировать правонарушение и данные подозреваемого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оформить",
                                "target_lang_sense_clarification": "составить протокол"
                            },
                            {
                                "target_lang_word": "зарегистрировать",
                                "target_lang_sense_clarification": "внести в базу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71397fc7-a38b-344d-a166-8130660a3307",
                    "sense_definition": "To issue a formal warning to a player during a game.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "record_keeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee <w>booked</w> the defender for a bad tackle.",
                            "target_lang_translations": {
                                "ru": "Судья <w>показал желтую карточку</w> защитнику за грубый подкат."
                            }
                        },
                        {
                            "text": "He has been <w>booked</w> twice this season for arguing.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне он уже дважды <w>получал предупреждение</w> за споры."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вынести официальное предупреждение игроку во время игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показать желтую карточку",
                                "target_lang_sense_clarification": "предупредить игрока"
                            },
                            {
                                "target_lang_word": "вынести предупреждение",
                                "target_lang_sense_clarification": "официально предупредить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73e92dc7-97e2-3ced-9ebb-6037c074fba6",
                    "sense_definition": "To travel very fast.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was really <w>booking</w> down the highway until he saw the police.",
                            "target_lang_translations": {
                                "ru": "Он реально <w>мчался</w> по шоссе, пока не увидел полицию."
                            }
                        },
                        {
                            "text": "We need to <w>book</w> it if we want to catch the train.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>поторопиться</w>, если мы хотим успеть на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "speed",
                        "hurtle",
                        "rocket",
                        "whiz"
                    ],
                    "common_phrases": [
                        "book it"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used to describe fast movement"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться очень быстро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "быстро ехать/бежать"
                            },
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "быстро двигаться"
                            }
                        ]
                    }
                }
            ]
        },

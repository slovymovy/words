            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "210fe940-db79-38e8-85e5-5cf9862ef412",
                    "sense_definition": "W sposób zaangażowany, biorąc w czymś bezpośredni udział.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy studenci <w>czynnie</w> uczestniczyli w dyskusji akademickiej.",
                            "target_lang_translations": {
                                "ru": "Все студенты <w>активно</w> участвовали в академической дискуссии."
                            }
                        },
                        {
                            "text": "Stowarzyszenie <w>czynnie</w> wspiera lokalne inicjatywy społeczne.",
                            "target_lang_translations": {
                                "ru": "Ассоциация <w>активно</w> поддерживает местные общественные инициативы."
                            }
                        }
                    ],
                    "synonyms": [
                        "aktywnie"
                    ],
                    "antonyms": [
                        "biernie",
                        "pasywnie",
                        "bezczynnie",
                        "z założonymi rękami"
                    ],
                    "common_phrases": [
                        "czynnie uczestniczyć",
                        "czynnie wspierać",
                        "czynnie działać"
                    ],
                    "target_lang_definitions": {
                        "ru": "С энтузиазмом, принимая непосредственное участие в чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активно",
                                "target_lang_sense_clarification": "об участии"
                            },
                            {
                                "target_lang_word": "деятельно",
                                "target_lang_sense_clarification": "энергично, с инициативой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ba50497-87d4-31c4-b019-8b2c7f8eeb00",
                    "sense_definition": "Regularnie uprawiając sport lub inną aktywność fizyczną.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek mimo wieku nadal <w>czynnie</w> uprawia sport.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка, несмотря на возраст, всё ещё <w>активно</w> занимается спортом."
                            }
                        },
                        {
                            "text": "Lekarze zalecają, aby <w>czynnie</w> spędzać wolny czas na świeżym powietrzu.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендуют <w>активно</w> проводить свободное время на свежем воздухе."
                            }
                        }
                    ],
                    "synonyms": [
                        "aktywnie"
                    ],
                    "antonyms": [
                        "biernie"
                    ],
                    "common_phrases": [
                        "czynnie uprawiać sport",
                        "czynnie wypoczywać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярно занимаясь спортом или физической активностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активно",
                                "target_lang_sense_clarification": "о занятиях спортом"

{
    "word_family": [
        "bezigheid",
        "bezigen"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "569cf6cd-5e7d-3cbf-b5f7-cb61a5352f37",
                    "sense_definition": "Aan het werk of iets aan het doen; niet vrij.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "occupied",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kan nu niet praten aan de telefoon, want hij is <w>bezig</w>.",
                            "target_lang_translations": {
                                "en": "He cannot talk on the phone right now, because he is <w>busy</w>."
                            }
                        },
                        {
                            "text": "Wij zijn momenteel erg <w>bezig</w> met de voorbereidingen voor het feest.",
                            "target_lang_translations": {
                                "en": "We are currently very <w>busy</w> with the preparations for the party."
                            }
                        }
                    ],
                    "synonyms": [
                        "druk",
                        "bezet"
                    ],
                    "antonyms": [
                        "vrij",
                        "beschikbaar"
                    ],
                    "common_phrases": [
                        "bezig zijn met",
                        "druk bezig",
                        "lekker bezig"
                    ],
                    "target_lang_definitions": {
                        "en": "Working or engaged in an activity; not free."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "busy",
                                "target_lang_sense_clarification": "engaged in activity"
                            },
                            {
                                "target_lang_word": "occupied",
                                "target_lang_sense_clarification": "not free"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac9c6348-a02b-3ed1-a8f0-053197efa4c4",
                    "sense_definition": "Op dit moment aan de gang; nog niet afgelopen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "in_progress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kom snel naar binnen, want de film is al <w>bezig</w>!",
                            "target_lang_translations": {
                                "en": "Come inside quickly, because the movie has already <w>started</w>!"
                            }
                        },
                        {
                            "text": "Het onderzoek naar de oorzaak van de brand is nog steeds <w>bezig</w>.",
                            "target_lang_translations": {
                                "en": "The investigation into the cause of the fire is still <w>ongoing</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaande",
                        "aan de gang"
                    ],
                    "antonyms": [
                        "afgelopen",
                        "klaar"
                    ],
                    "common_phrases": [
                        "al bezig",
                        "nog bezig"
                    ],
                    "target_lang_definitions": {
                        "en": "Currently happening or in progress; not yet finished."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in progress",
                                "target_lang_sense_clarification": "happening now"
                            },
                            {
                                "target_lang_word": "ongoing",
                                "target_lang_sense_clarification": "continuing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85a90eb4-0185-3db4-8cda-ba9c6f939d81",
                    "sense_definition": "Vlijtig en actief van karakter; iemand die graag werkt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "industrious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn tante is een <w>bezig</w> persoon die nooit lang stil kan zitten.",
                            "target_lang_translations": {
                                "en": "My aunt is a <w>busy</w> person who can never sit still for long."
                            }
                        },
                        {
                            "text": "Zij is altijd al een <w>bezig</w> bijtje geweest, zowel thuis als op het werk.",
                            "target_lang_translations": {
                                "en": "She has always been a <w>busy</w> bee, both at home and at work."
                            }
                        }
                    ],
                    "synonyms": [
                        "vlijtig",
                        "bedrijvig",
                        "actief"
                    ],
                    "antonyms": [
                        "lui",
                        "passief"
                    ],
                    "common_phrases": [
                        "een bezig bijtje"
                    ],
                    "target_lang_definitions": {
                        "en": "Industrious and active in character; someone who likes to work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "busy",
                                "target_lang_sense_clarification": "active/industrious"
                            },
                            {
                                "target_lang_word": "active",
                                "target_lang_sense_clarification": "energetic"
                            }
                        ]
                    }
                }
            ]
        },
        {

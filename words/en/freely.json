{
    "word_family": [
        "free",
        "freedom",
        "freeness"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "088c57d3-9a72-3868-a378-4d0d6fdbff48",
                    "sense_definition": "Without interference, restriction, or obstruction; in a manner that is not limited or controlled.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are among friends here, so please feel comfortable to speak <w>freely</w>.",
                            "target_lang_translations": {
                                "ru": "Вы здесь среди друзей, поэтому, пожалуйста, не стесняйтесь говорить <w>свободно</w>."
                            }
                        },
                        {
                            "text": "The river flows <w>freely</w> all the way to the ocean without any dams.",
                            "target_lang_translations": {
                                "ru": "Река течет <w>свободно</w> до самого океана, не встречая никаких плотин."
                            }
                        }
                    ],
                    "synonyms": [
                        "openly",
                        "unrestrictedly",
                        "unimpededly",
                        "loosely"
                    ],
                    "antonyms": [
                        "restrictedly"
                    ],
                    "common_phrases": [
                        "speak freely",
                        "move freely",
                        "roam freely",
                        "available freely"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без вмешательства, ограничений или препятствий; свободно, без контроля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободно",
                                "target_lang_sense_clarification": "без ограничений или препятствий"
                            },
                            {
                                "target_lang_word": "вольно",
                                "target_lang_sense_clarification": "непринужденно, раскованно"
                            },
                            {
                                "target_lang_word": "беспрепятственно",
                                "target_lang_sense_clarification": "не встречая преград"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b6b8144-43d2-3290-85bd-37184f411dde",
                    "sense_definition": "Of one's own free will; voluntarily and without being forced.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>freely</w> admitted that he had made a mistake in the calculation.",
                            "target_lang_translations": {
                                "ru": "Он <w>открыто</w> признал, что допустил ошибку в расчетах."
                            }
                        },
                        {
                            "text": "She gave her time <w>freely</w> to help the organization achieve its goals.",
                            "target_lang_translations": {
                                "ru": "Она <w>охотно</w> тратила свое время, чтобы помочь организации достичь своих целей."
                            }
                        }
                    ],
                    "synonyms": [
                        "voluntarily",
                        "willingly",
                        "readily",
                        "spontaneously"
                    ],
                    "antonyms": [
                        "reluctantly",
                        "unwillingly",
                        "grudgingly",
                        "forcedly"
                    ],
                    "common_phrases": [
                        "give freely",
                        "admit freely",
                        "offer freely"
                    ],
                    "target_lang_definitions": {
                        "ru": "По собственной воле; добровольно и без принуждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добровольно",
                                "target_lang_sense_clarification": "по собственному желанию"
                            },
                            {
                                "target_lang_word": "охотно",
                                "target_lang_sense_clarification": "с готовностью"
                            },
                            {
                                "target_lang_word": "открыто",
                                "target_lang_sense_clarification": "прямо, не скрывая (о признаниях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f725432f-d65d-3803-8aae-36c1c5c48248",
                    "sense_definition": "In large quantities or amounts; abundantly and profusely.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During the celebration, the wine was flowing <w>freely</w> for all the guests.",
                            "target_lang_translations": {
                                "ru": "Во время празднования вино для всех гостей лилось <w>рекой</w>."
                            }
                        },
                        {
                            "text": "He spends money <w>freely</w> on expensive clothes and luxury items.",
                            "target_lang_translations": {
                                "ru": "Он <w>щедро</w> тратит деньги на дорогую одежду и предметы роскоши."
                            }
                        }
                    ],
                    "synonyms": [
                        "abundantly",
                        "profusely",
                        "copiously",
                        "lavishly"
                    ],
                    "antonyms": [
                        "sparingly"
                    ],
                    "common_phrases": [
                        "flow freely",
                        "spend freely",
                        "bleed freely"
                    ],
                    "target_lang_definitions": {
                        "ru": "В больших количествах; обильно и щедро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щедро",
                                "target_lang_sense_clarification": "не скупясь"
                            },
                            {
                                "target_lang_word": "обильно",
                                "target_lang_sense_clarification": "в большом количестве"
                            },
                            {
                                "target_lang_word": "рекой",
                                "target_lang_sense_clarification": "в изобилии (обычно о напитках)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
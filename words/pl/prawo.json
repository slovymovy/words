                },
                {
                    "sense_id": "1b2789bf-1299-3422-ba13-46690fcf36b5",
                    "sense_definition": "Stała, udowodniona naukowo zasada opisująca powtarzalne zjawiska w przyrodzie lub nauce.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scientific_rule_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji fizyki nauczyciel wytłumaczył nam <w>prawo</w> ciążenia.",
                            "target_lang_translations": {
                                "ru": "На уроке физики учитель объяснил нам <w>закон</w> тяготения."
                            }
                        },
                        {
                            "text": "To zjawisko potwierdza znane <w>prawo</w> Archimedesa.",
                            "target_lang_translations": {
                                "ru": "Это явление подтверждает известный <w>закон</w> Архимеда."
                            }
                        }
                    ],
                    "synonyms": [
                        "zasada",
                        "sposób"
                    ],
                    "common_phrases": [
                        "prawo natury",
                        "prawa fizyki",
                        "prawo dżungli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянный, научно доказанный принцип, описывающий повторяющиеся явления в природе или науке."

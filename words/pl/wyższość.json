            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2b124459-d093-320f-84ce-bc90e64de69f",
                    "sense_definition": "Stan, w którym ktoś lub coś przewyższa innych pod względem wartości, znaczenia, siły lub umiejętności.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "objective_superiority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo młodego wieku zawodnik udowodnił swoją <w>wyższość</w> nad mistrzem.",
                            "target_lang_translations": {
                                "ru": "Несмотря на молодой возраст, спортсмен доказал свое <w>превосходство</w> над чемпионом."
                            }
                        },
                        {
                            "text": "Musieliśmy uznać <w>wyższość</w> przeciwnika i pogodzić się z porażką.",
                            "target_lang_translations": {
                                "ru": "Мы были вынуждены признать <w>превосходство</w> противника и смириться с поражением."
                            }
                        }
                    ],
                    "synonyms": [
                        "supremacja",
                        "pycha"
                    ],
                    "antonyms": [
                        "niższość"
                    ],
                    "common_phrases": [
                        "uznać wyższość",
                        "wyższość liczebna",
                        "wyższość technologiczna",
                        "wyższość moralna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором кто-то или что-то превосходит других по ценности, значимости, силе или навыкам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превосходство",
                                "target_lang_sense_clarification": "dominacja, bycie lepszym"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d1533f3-8394-3eac-bf32-34d03a9efdd7",
                    "sense_definition": "Sposób zachowania świadczący o przekonaniu o byciu lepszym od innych, często połączony z lekceważeniem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "subjective_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor traktował pracowników z nieukrywaną <w>wyższością</w>, co psuło atmosferę w biurze.",
                            "target_lang_translations": {
                                "ru": "Директор относился к сотрудникам с нескрываемым <w>высокомерием</w>, что портило атмосферу в офисе."
                            }
                        },
                        {
                            "text": "Jego spojrzenie pełne <w>wyższości</w> sprawiło, że poczułem się nieswojo.",
                            "target_lang_translations": {
                                "ru": "Его взгляд, полный <w>высокомерия</w>, заставил меня почувствовать себя неловко."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyniosłość",
                        "pycha",
                        "duma"
                    ],
                    "antonyms": [
                        "pokora",
                        "skromność"
                    ],
                    "common_phrases": [
                        "poczucie wyższości",
                        "patrzeć z wyższością",
                        "kompleks wyższości",
                        "okazywać wyższość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Манера поведения, свидетельствующая об убежденности в том, что человек лучше других, часто сопровождаемая пренебрежением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокомерие",
                                "target_lang_sense_clarification": "cecha charakteru, wyniosłość"
                            },
                            {
                                "target_lang_word": "надменность",
                                "target_lang_sense_clarification": "okazywanie wyższości"
                            }
                        ]
                    }
                }
            ]
        }
    ]

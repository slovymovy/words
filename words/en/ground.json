                            {
                                "target_lang_word": "отстранять от полетов",
                                "target_lang_sense_clarification": "о пилотах"
                            },
                            {
                                "target_lang_word": "оставлять на земле",
                                "target_lang_sense_clarification": "не давать взлететь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93ffde62-cead-39f4-a4a4-5f58b97d056d",
                    "sense_definition": "To base an idea, belief, or argument on a specific principle or fact.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "base_on",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>ground</w> your arguments in solid facts.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>обосновывать</w> свои аргументы твердыми фактами."
                            }
                        },
                        {
                            "text": "Her philosophy is <w>grounded</w> in the belief that everyone deserves respect.",
                            "target_lang_translations": {
                                "ru": "Ее философия <w>основана</w> на убеждении, что каждый заслуживает уважения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ground in fact",
                        "grounded in reality"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основывать идею, убеждение или аргумент на определенном принципе или факте."
                    },
                    "translations": {
                        "ru": [

                {
                    "sense_id": "4ad5f235-aa60-3deb-8134-df7b33794f61",
                    "sense_definition": "Wetgeving die speciaal is ingevoerd om te handelen tijdens een noodsituatie of crisis.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering kondigde een nieuwe <w>noodwet</w> aan om de crisis snel te kunnen bestrijden.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новом <w>чрезвычайном законе</w>, чтобы быстро справиться с кризисом."
                            }
                        },
                        {
                            "text": "Door de <w>noodwet</w> kreeg de politie tijdelijk meer bevoegdheden op straat.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>чрезвычайному закону</w> полиция временно получила больше полномочий на улицах."
                            }
                        }
                    ],
                    "synonyms": [
                        "noodverordening"
                    ],
                    "common_phrases": [
                        "een noodwet aannemen",
                        "de noodwet invoeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Законодательство, специально введенное для действий во время чрезвычайной ситуации или кризиса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чрезвычайный закон",
                                "target_lang_sense_clarification": "закон для кризисных ситуаций"
                            },
                            {
                                "target_lang_word": "закон о чрезвычайном положении",
                                "target_lang_sense_clarification": "правовой режим ЧП"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99e10395-541d-3568-ba73-139ccc35bf32",
                    "sense_definition": "Een tijdelijke wet die van kracht is in afwachting van een definitieve regeling.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat de onderhandelingen vastliepen, was er een <w>noodwet</w> nodig om de overheid te blijven financieren.",
                            "target_lang_translations": {
                                "ru": "Поскольку переговоры зашли в тупик, потребовался <w>экстренный закон</w> для продолжения финансирования правительства."
                            }
                        },
                        {
                            "text": "Deze <w>noodwet</w> vervalt zodra het parlement de definitieve wetgeving heeft goedgekeurd.",
                            "target_lang_translations": {
                                "ru": "Этот <w>временный закон</w> утратит силу, как только парламент одобрит окончательное законодательство."
                            }
                        }
                    ],
                    "synonyms": [
                        "overgangswet"
                    ],
                    "common_phrases": [
                        "een noodwet opstellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временный закон, действующий в ожидании окончательного урегулирования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экстренный закон",
                                "target_lang_sense_clarification": "срочная мера"
                            },
                            {
                                "target_lang_word": "временный закон",
                                "target_lang_sense_clarification": "переходный период"
                            }
                        ]
                    }
                }
            ]
        }

                    ],
                    "common_phrases": [
                        "umówić wizytę",
                        "umówić spotkanie",
                        "umówić pacjenta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записать кого-либо на прием или организовать встречу для другого человека."
                    },
                    "translations": {

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ea78255a-0782-3e24-82f3-66b55b062093",
                    "sense_definition": "Повернуть предмет другой стороной, с одной стороны на другую или снизу вверх.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_turn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <переверни> страницу и читай следующий текст.",
                            "target_lang_translations": {
                                "en": "Please <turn over> the page and read the next text."
                            }
                        },
                        {
                            "text": "Повар ловко <перевернул> мясо на горячей сковороде.",
                            "target_lang_translations": {
                                "en": "The cook deftly <flipped> the meat on the hot frying pan."
                            }
                        }
                    ],
                    "synonyms": [
                        "повернуть"
                    ],
                    "common_phrases": [
                        "перевернуть страницу",
                        "перевернуть на спину",
                        "перевернуть лист",
                        "перевернуть блин"
                    ],
                    "target_lang_definitions": {
                        "en": "To turn an object so that the other side is facing up or forward."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn over",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "flip",
                                "target_lang_sense_clarification": "quick motion, e.g., for pages or food"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8badde7-340a-3942-a5c1-df5fc6739216",
                    "sense_definition": "Заставить упасть на бок или вверх дном; опрокинуть.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_turn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильная волна чуть не <перевернула> нашу маленькую лодку.",
                            "target_lang_translations": {
                                "en": "A strong wave nearly <capsized> our small boat."
                            }
                        },
                        {
                            "text": "Кот прыгнул на стол и случайно <перевернул> вазу с цветами.",
                            "target_lang_translations": {
                                "en": "The cat jumped on the table and accidentally <knocked over> the vase of flowers."
                            }
                        }
                    ],
                    "synonyms": [
                        "опрокинуть",
                        "повалить"
                    ],
                    "antonyms": [
                        "поднять",
                        "поставить"
                    ],
                    "common_phrases": [
                        "перевернуть стул",
                        "перевернуть лодку",
                        "перевернуть вверх дном"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something to fall onto its side or turn upside down; to knock over."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overturn",
                                "target_lang_sense_clarification": "general, often for vehicles or large objects"
                            },
                            {
                                "target_lang_word": "knock over",
                                "target_lang_sense_clarification": "accidental, for standing objects"
                            },
                            {
                                "target_lang_word": "capsize",
                                "target_lang_sense_clarification": "specifically for boats"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65f2a811-985e-3fec-981a-47144c3da91f",
                    "sense_definition": "Коренным образом изменить что-либо (жизнь, мнение, порядок вещей).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это неожиданное событие полностью <перевернуло> мою жизнь.",
                            "target_lang_translations": {
                                "en": "This unexpected event completely <transformed> my life."
                            }
                        },
                        {
                            "text": "Новая технология может <перевернуть> весь мир.",
                            "target_lang_translations": {
                                "en": "The new technology could <revolutionize> the whole world."
                            }
                        }
                    ],
                    "synonyms": [
                        "изменить",
                        "преобразить"
                    ],
                    "antonyms": [
                        "сохранить"
                    ],
                    "common_phrases": [
                        "перевернуть мир",
                        "перевернуть жизнь",
                        "перевернуть сознание",
                        "перевернуть душу"
                    ],
                    "target_lang_definitions": {
                        "en": "To radically change a situation, life, or the existing order of things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transform",
                                "target_lang_sense_clarification": "to change completely"
                            },
                            {
                                "target_lang_word": "turn upside down",
                                "target_lang_sense_clarification": "idiomatic, to change drastically"
                            },
                            {
                                "target_lang_word": "revolutionize",
                                "target_lang_sense_clarification": "to change fundamentally, e.g., the world or industry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0363679-d830-39a6-98f0-74d453ea0553",
                    "sense_definition": "Привести всё в беспорядок, перекладывая вещи с места на место (обычно при поиске).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <перевернул> всю комнату, но так и не нашёл свои ключи.",
                            "target_lang_translations": {
                                "en": "I <turned over> the whole room, but never found my keys."
                            }
                        },
                        {
                            "text": "Грабители <перевернули> в квартире всё вверх дном в поисках денег.",
                            "target_lang_translations": {
                                "en": "The robbers <ransacked> the apartment in search of money."
                            }
                        }
                    ],
                    "synonyms": [
                        "перерыть"
                    ],
                    "common_phrases": [
                        "перевернуть весь дом",
                        "перевернуть вверх дном"
                    ],
                    "target_lang_definitions": {
                        "en": "To create disorder by moving things around, usually while searching for something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn upside down",
                                "target_lang_sense_clarification": "idiomatic, to make a mess while searching"
                            },
                            {
                                "target_lang_word": "turn over",
                                "target_lang_sense_clarification": "to search a place thoroughly"
                            },
                            {
                                "target_lang_word": "ransack",
                                "target_lang_sense_clarification": "to search aggressively or violently"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
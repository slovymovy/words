    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3098a5bb-c103-3c9b-ae3f-fd080d1efcc1",
                    "sense_definition": "Uzyskać prawo do używania lokalu, pojazdu lub innego przedmiotu przez określony czas w zamian za opłatę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "renting_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <wynająć> większe mieszkanie, ponieważ nasza rodzina wkrótce się powiększy.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <снять> квартиру побольше, так как наша семья скоро увеличится."
                            }
                        },
                        {
                            "text": "Podczas wakacji we Włoszech planujemy <wynająć> samochód, aby zwiedzić okoliczne miasteczka.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска в Италии мы планируем <арендовать> машину, чтобы осмотреть окрестные городки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypożyczyć"
                    ],
                    "antonyms": [
                        "kupić"
                    ],
                    "common_phrases": [
                        "wynająć mieszkanie",
                        "wynająć pokój",
                        "wynająć samochód",
                        "tanio wynająć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить право пользования помещением, транспортным средством или другим предметом на определенное время за плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снять",
                                "target_lang_sense_clarification": "квартиру, комнату"
                            },
                            {
                                "target_lang_word": "арендовать",
                                "target_lang_sense_clarification": "помещение, автомобиль"
                            },
                            {
                                "target_lang_word": "взять напрокат",
                                "target_lang_sense_clarification": "автомобиль, оборудование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b46b69cd-9d8c-321c-8f36-96924a0f7985",
                    "sense_definition": "Udostępnić komuś swoją własność (np. mieszkanie lub lokal) do używania przez pewien czas w zamian za pieniądze.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "renting_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciel kamienicy zdecydował się <wynająć> lokal na parterze pod nową kawiarnię.",
                            "target_lang_translations": {
                                "ru": "Владелец дома решил <сдать> помещение на первом этаже под новое кафе."
                            }
                        },
                        {
                            "text": "Chcemy <wynająć> turystom nasz domek letniskowy, który stoi pusty przez większość roku.",
                            "target_lang_translations": {
                                "ru": "Мы хотим <сдать> туристам наш летний домик, который пустует большую часть года."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydzierżawić",
                        "podnająć"
                    ],
                    "antonyms": [
                        "sprzedać"
                    ],
                    "common_phrases": [
                        "wynająć komuś mieszkanie",
                        "wynająć lokal",
                        "wynająć powierzchnię biurową"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставить кому-либо свою собственность (например, квартиру или помещение) в пользование на определенное время за деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "квартиру, комнату"
                            },
                            {
                                "target_lang_word": "сдать в аренду",
                                "target_lang_sense_clarification": "недвижимость, имущество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "051987f9-35e9-30ae-be0c-ed4b8cb7f32c",
                    "sense_definition": "Zatrudnić kogoś do wykonania określonej, zazwyczaj krótkoterminowej usługi lub pracy za wynagrodzeniem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "hiring_services",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma musiała <wynająć> prawnika, aby pomógł im wygrać trudną sprawę w sądzie.",
                            "target_lang_translations": {
                                "ru": "Фирме пришлось <нанять> юриста, чтобы он помог им выиграть сложное дело в суде."
                            }
                        },
                        {
                            "text": "Postanowiliśmy <wynająć> ekipę remontową, żeby szybko odnowić naszą łazienkę.",
                            "target_lang_translations": {
                                "ru": "Мы решили <нанять> ремонтную бригаду, чтобы быстро обновить нашу ванную."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrudnić",
                        "zaangażować"
                    ],
                    "antonyms": [
                        "zwolnić"
                    ],
                    "common_phrases": [
                        "wynająć detektywa",
                        "wynająć adwokata",
                        "wynająć ochronę",
                        "wynająć ekipę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлечь кого-либо к выполнению определенной, обычно краткосрочной услуги или работы за вознаграждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанять",
                                "target_lang_sense_clarification": "специалиста, работника"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
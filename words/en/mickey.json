                    "frequency": "High",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Mickey</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Микки</w> приезжает к нам в эти выходные."
                            }

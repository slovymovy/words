        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9572bd25-2879-32d2-994e-f4eae65ed6d8",
                    "sense_definition": "An unpleasant physical sensation caused by illness, injury, or bodily disorder.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_suffering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt a sharp <w>pain</w> in her leg after falling down.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала острую <w>боль</w> в ноге после падения."
                            }
                        },
                        {
                            "text": "The doctor prescribed medicine to help relieve the <w>pain</w>.",
                            "target_lang_translations": {
                                "ru": "Врач выписал лекарство, чтобы облегчить <w>боль</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ache",
                        "agony",
                        "hurt",
                        "suffering",
                        "smart",
                        "torture",
                        "torment"
                    ],
                    "antonyms": [
                        "pleasure"
                    ],
                    "common_phrases": [
                        "in pain",
                        "chest pain",
                        "back pain",
                        "feel pain",
                        "relieve pain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятное физическое ощущение, вызванное болезнью, травмой или расстройством организма."

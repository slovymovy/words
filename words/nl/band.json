        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd343c63-b7a6-3a15-953a-127b94ff76c6",
                    "sense_definition": "Een met lucht gevulde rubberen ring om het wiel van een voertuig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die auto heeft een lekke <w>band</w>.",
                            "target_lang_translations": {
                                "ru": "У этой машины спущена <w>шина</w>.",
                                "en": "That car has a flat <w>tire</w>."
                            }
                        },
                        {
                            "text": "Je moet de <w>banden</w> van je fiets regelmatig oppompen.",
                            "target_lang_translations": {
                                "ru": "Нужно регулярно накачивать <w>шины</w> своего велосипеда.",
                                "en": "You need to pump up your bike <w>tires</w> regularly."
                            }
                        }
                    ],
                    "synonyms": [
                        "autoband",
                        "fietsband",
                        "buitenband"
                    ],
                    "common_phrases": [
                        "lekke band",
                        "banden wisselen",
                        "zachte band"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполненное воздухом резиновое кольцо вокруг колеса транспортного средства.",
                        "en": "An air-filled rubber ring around the wheel of a vehicle."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шина",
                                "target_lang_sense_clarification": "резиновая оболочка колеса"
                            },
                            {
                                "target_lang_word": "покрышка",
                                "target_lang_sense_clarification": "внешняя часть шины"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tire",
                                "target_lang_sense_clarification": "rubber covering of a wheel (US)"
                            },
                            {
                                "target_lang_word": "tyre",
                                "target_lang_sense_clarification": "rubber covering of a wheel (UK)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92cd1a06-5312-3908-8d98-cd19210e2d27",
                    "sense_definition": "Een sterke connectie of gevoelsmatige relatie tussen personen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb geen goede <w>band</w> met mijn buren.",
                            "target_lang_translations": {
                                "ru": "У меня нет хорошей <w>связи</w> с соседями.",
                                "en": "I don't have a good <w>bond</w> with my neighbors."
                            }
                        },
                        {
                            "text": "De zussen hebben een heel sterke <w>band</w> met elkaar.",
                            "target_lang_translations": {
                                "ru": "У сестер очень сильная <w>связь</w> друг с другом.",
                                "en": "The sisters have a very strong <w>bond</w> with each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "relatie",
                        "connectie",
                        "verhouding"
                    ],
                    "common_phrases": [
                        "een goede band",
                        "een hechte band",
                        "de banden aanhalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная связь или эмоциональные отношения между людьми.",
                        "en": "A strong connection or emotional relationship between people."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "отношения, контакт"
                            },
                            {
                                "target_lang_word": "узы",
                                "target_lang_sense_clarification": "тесная связь (поэтич.)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bond",
                                "target_lang_sense_clarification": "emotional connection"
                            },
                            {
                                "target_lang_word": "connection",
                                "target_lang_sense_clarification": "relationship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fbe2e7b-8e20-3225-a3f9-e14bd1db5149",

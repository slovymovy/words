    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "59f19044-a952-3d6c-adc2-611fc44debae",
                    "sense_definition": "Ustalać tożsamość kogoś lub czegoś; rozpoznawać, kim ktoś jest lub czym coś jest.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "identification_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja stara się <identyfikować> sprawcę napadu na podstawie nagrań z kamer.",
                            "target_lang_translations": {
                                "ru": "Полиция пытается <w>опознать</w> преступника на основе записей с камер."
                            }
                        },
                        {
                            "text": "Lekarz musi szybko <identyfikować> objawy, aby wdrożyć odpowiednie leczenie.",
                            "target_lang_translations": {
                                "ru": "Врач должен быстро <w>распознать</w> симптомы, чтобы назначить соответствующее лечение."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpoznawać"
                    ],
                    "common_phrases": [
                        "identyfikować problem",
                        "identyfikować zwłoki",
                        "identyfikować zagrożenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать личность кого-либо или сущность чего-либо; распознавать, кем является кто-то или чем является что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идентифицировать",
                                "target_lang_sense_clarification": "устанавливать тождество"
                            },
                            {
                                "target_lang_word": "опознавать",
                                "target_lang_sense_clarification": "узнавать личность (обычно в криминалистике)"
                            },
                            {
                                "target_lang_word": "выявлять",
                                "target_lang_sense_clarification": "обнаруживать, определять (проблему, угрозу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1a9d883-78e5-38e4-8557-c81ddf2e0ce5",
                    "sense_definition": "Odczuwać silną więź z kimś lub czymś, podzielać czyjeś poglądy lub czuć przynależność do jakiejś grupy (używane z zaimkiem \"się\").",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu nastolatków zaczyna <identyfikować> się z postaciami z ulubionych seriali.",
                            "target_lang_translations": {
                                "ru": "Многие подростки начинают <w>отождествлять</w> себя с персонажами любимых сериалов."
                            }
                        },
                        {
                            "text": "Nie potrafię <identyfikować> się z wartościami, które promuje ta organizacja.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>отождествлять</w> себя с ценностями, которые продвигает эта организация."
                            }
                        }
                    ],
                    "common_phrases": [
                        "identyfikować się z grupą",
                        "identyfikować się z bohaterem",
                        "identyfikować się z poglądami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать сильную связь с кем-либо или чем-либо, разделять чьи-либо взгляды или ощущать принадлежность к какой-либо группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отождествлять себя",
                                "target_lang_sense_clarification": "чувствовать сходство или единство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f394b7c4-71cb-3ea8-afe9-88a1eb0e1127",
                    "sense_definition": "Uznawać dwie różne osoby, rzeczy lub zjawiska za tożsame; stawiać znak równości między czymś a czymś.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "equating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie należy <identyfikować> posiadania pieniędzy z prawdziwym szczęściem.",
                            "target_lang_translations": {
                                "ru": "Не следует <w>отождествлять</w> обладание деньгами с истинным счастьем."
                            }
                        },
                        {
                            "text": "Ludzie często błędnie potrafią <identyfikować> aktora z graną przez niego rolą.",
                            "target_lang_translations": {
                                "ru": "Люди часто могут ошибочно <w>отождествлять</w> актера с сыгранной им ролью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "identyfikować kogoś z czymś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Признавать двух разных людей, вещи или явления тождественными; ставить знак равенства между чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отождествлять",
                                "target_lang_sense_clarification": "приравнивать, считать одинаковым"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
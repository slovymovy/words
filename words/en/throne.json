    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad0c3d94-6a55-3ba3-ad17-19d3eb7475d7",
                    "sense_definition": "An impressive seat used by a monarch, often placed on a raised platform for formal occasions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "seat_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king sat on his golden <w>throne</w> to address the court.",
                            "target_lang_translations": {
                                "ru": "Король сидел на своем золотом <w>троне</w>, обращаясь к придворным."
                            }
                        },
                        {
                            "text": "Visitors must bow respectfully before approaching the royal <w>throne</w>.",
                            "target_lang_translations": {
                                "ru": "Посетители должны почтительно кланяться, прежде чем приблизиться к королевскому <w>трону</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sit on the throne",
                        "royal throne",
                        "golden throne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Впечатляющее сиденье, используемое монархом, часто установленное на возвышении для торжественных случаев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трон",
                                "target_lang_sense_clarification": "предмет мебели"
                            },
                            {
                                "target_lang_word": "престол",
                                "target_lang_sense_clarification": "торжественно/устар."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "571a7839-9cca-378a-91e9-b4125fb5cc4f",
                    "sense_definition": "The position of being a king or queen; the power, rank, or authority of a monarch.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "seat_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prince is currently second in line to the <w>throne</w>.",
                            "target_lang_translations": {
                                "ru": "Принц в настоящее время второй в очереди на <w>престол</w>."
                            }
                        },
                        {
                            "text": "She ascended the <w>throne</w> immediately after her father passed away.",
                            "target_lang_translations": {
                                "ru": "Она взошла на <w>престол</w> сразу после смерти отца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heir to the throne",
                        "ascend the throne",
                        "claim the throne",
                        "succeed to the throne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение короля или королевы; власть, ранг или полномочия монарха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "престол",
                                "target_lang_sense_clarification": "символ власти"
                            },
                            {
                                "target_lang_word": "трон",
                                "target_lang_sense_clarification": "власть"
                            }
                        ]
                    }
                },
                {

            "senses": [
                {
                    "sense_id": "30c0548e-4185-332c-beff-4152f240a382",
                    "sense_definition": "A useful or valuable quality, skill, or person.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "value_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her ability to speak three languages is a major <w>asset</w> to the company.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "327d5e5a-492a-3629-90c5-f815efc3b4f4",
                    "sense_definition": "Any coniferous evergreen tree of the genus Pinus, typically having needle-shaped leaves and producing cones.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "botany_pine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The northern slopes of the mountain were covered mainly in <w>pine</w>.",
                            "target_lang_translations": {
                                "ru": "Северные склоны горы были покрыты в основном <w>сосной</w>."
                            }
                        },
                        {
                            "text": "We sat under a tall <w>pine</w> tree to escape the hot afternoon sun.",
                            "target_lang_translations": {
                                "ru": "Мы сидели под высокой <w>сосной</w>, спасаясь от жаркого полуденного солнца."
                            }
                        }
                    ],
                    "synonyms": [
                        "pine tree"
                    ],
                    "common_phrases": [
                        "pine tree",
                        "pine forest",
                        "pine cone",
                        "pine needles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Род вечнозеленых хвойных деревьев, обычно имеющих игловидные листья и шишки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосна",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cac603b1-a2c7-3e16-bca5-7ed150b1fce8",
                    "sense_definition": "The soft, light-colored wood obtained from a pine tree, often used for making furniture.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "botany_pine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a simple kitchen table made of knotty <w>pine</w>.",
                            "target_lang_translations": {
                                "ru": "Она купила простой кухонный стол из сучковатой <w>сосны</w>."
                            }
                        },
                        {
                            "text": "The floorboards were made of <w>pine</w> and creaked whenever we walked on them.",
                            "target_lang_translations": {
                                "ru": "Половицы были сделаны из <w>сосны</w> и скрипели всякий раз, когда мы по ним ходили."
                            }
                        }
                    ],
                    "synonyms": [
                        "pinewood"
                    ],
                    "common_phrases": [
                        "pine furniture",
                        "knotty pine",
                        "pine table"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкая светлая древесина сосны, часто используемая для изготовления мебели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосна",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3fd4cb3-36d7-3861-a24c-ebb07da6a7b1",
                    "sense_definition": "The bench where sports players sit when they are not playing in the game.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The injured player had to ride the <w>pine</w> for the rest of the season.",
                            "target_lang_translations": {
                                "ru": "Травмированному игроку пришлось полировать <w>скамейку запасных</w> до конца сезона."
                            }
                        },
                        {
                            "text": "If you don't practice hard, you will end up sitting on the <w>pine</w> during the finals.",
                            "target_lang_translations": {
                                "ru": "Если не будешь усердно тренироваться, в финале будешь сидеть на <w>скамейке запасных</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ride the pine",
                        "hit the pine"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скамейка, на которой сидят игроки, когда не участвуют в игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скамейка запасных",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0fe5d5c-9750-3f0d-9c2c-b5c7805f82dc",
                    "sense_definition": "A pineapple.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany_pine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In older literature, a pineapple was often simply referred to as a <w>pine</w>.",
                            "target_lang_translations": {
                                "ru": "В старой литературе ананас часто называли просто <w>ананасом</w> (pine)."
                            }
                        },
                        {
                            "text": "She placed the exotic <w>pine</w> on the table as a centerpiece for the feast.",
                            "target_lang_translations": {
                                "ru": "Она поставила экзотический <w>ананас</w> на стол в качестве главного украшения пира."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "short for pineapple"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ананас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ананас",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f03c95d-b49a-31c9-8d41-92c08c96eee8",
                    "sense_definition": "A bar counter or bartop.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bartender stood behind the <w>pine</w> serving drinks to the customers.",
                            "target_lang_translations": {
                                "ru": "Бармен стоял за <w>стойкой</w>, разливая напитки клиентам."
                            }
                        },
                        {
                            "text": "I'll be working behind the <w>pine</w> all night, so come say hello.",
                            "target_lang_translations": {
                                "ru": "Я буду работать за <w>стойкой</w> всю ночь, так что заходи поздороваться."
                            }
                        }
                    ],
                    "common_phrases": [

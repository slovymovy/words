    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0f31dc08-b288-3a9c-9542-fce3f5055e86",
                    "sense_definition": "In a precise and correct manner, without any approximation or error.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please measure the ingredients <w>exactly</w> to ensure the cake tastes good.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отмерьте ингредиенты <w>точно</w>, чтобы торт получился вкусным."
                            }
                        },
                        {
                            "text": "The two sisters look <w>exactly</w> the same.",
                            "target_lang_translations": {
                                "ru": "Две сестры выглядят <w>в точности</w> одинаково."
                            }
                        }
                    ],
                    "synonyms": [
                        "accurately",
                        "correctly",
                        "precisely",
                        "perfectly"
                    ],
                    "antonyms": [
                        "approximately",
                        "imprecisely",
                        "inaccurately",
                        "inexactly"
                    ],
                    "common_phrases": [
                        "exactly the same",
                        "exactly right",
                        "know exactly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точным образом, без каких-либо приближений или ошибок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "безошибочно, аккуратно"
                            },
                            {
                                "target_lang_word": "в точности",
                                "target_lang_sense_clarification": "совершенно, один в один"
                            },
                            {
                                "target_lang_word": "ровно",
                                "target_lang_sense_clarification": "о времени или количестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bd494a2-fc2f-3c83-997d-4eafa08e43d7",
                    "sense_definition": "Used to emphasize the accuracy of a statement, reason, or description.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That is <w>exactly</w> why I told you to be careful.",
                            "target_lang_translations": {
                                "ru": "Вот <w>именно</w> поэтому я говорил тебе быть осторожным."
                            }
                        },
                        {
                            "text": "He is not <w>exactly</w> happy about the change in plans.",
                            "target_lang_translations": {
                                "ru": "Он не <w>совсем</w> рад изменению планов."
                            }
                        }
                    ],
                    "synonyms": [
                        "precisely",
                        "just"
                    ],
                    "common_phrases": [
                        "not exactly",
                        "exactly why",
                        "exactly what"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания точности утверждения, причины или описания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "именно",
                                "target_lang_sense_clarification": "усиление утверждения"
                            },
                            {
                                "target_lang_word": "как раз",
                                "target_lang_sense_clarification": "уточнение совпадения"
                            },
                            {
                                "target_lang_word": "совсем",
                                "target_lang_sense_clarification": "в отрицаниях (не совсем)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e72e46a5-9266-3dbb-8770-3088be7bba45",
                    "sense_definition": "Used to express strong agreement with what has just been said.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you mean we need to start over? — Yes, <w>exactly</w>!",
                            "target_lang_translations": {
                                "ru": "Ты имеешь в виду, что нам нужно начать сначала? — Да, <w>точно</w>!"
                            }
                        },
                        {
                            "text": "<w>Exactly</w>! That is the point I was trying to make.",
                            "target_lang_translations": {
                                "ru": "<w>Вот именно</w>! Это как раз то, что я пытался сказать."
                            }
                        }
                    ],

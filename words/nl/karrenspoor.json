{
    "word_family": [
        "kar",
        "spoor",
        "karren",
        "sporen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "506af4b3-849e-31dd-8ff8-f66eb25d3b97",
                    "sense_definition": "Een eenvoudige, onverharde weg die vaak uit twee uitgesleten sporen bestaat, oorspronkelijk gemaakt door de wielen van karren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "literal_track",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer reed langzaam met zijn tractor over het hobbelige <w>karrenspoor</w>.",
                            "target_lang_translations": {
                                "en": "The farmer drove slowly with his tractor over the bumpy <w>cart track</w>."
                            }
                        },
                        {
                            "text": "We wandelden via een oud <w>karrenspoor</w> naar het afgelegen huisje.",
                            "target_lang_translations": {
                                "en": "We walked along an old <w>cart track</w> to the remote cottage."
                            }
                        }
                    ],
                    "synonyms": [
                        "zandpad",
                        "veldweg"
                    ],
                    "antonyms": [
                        "snelweg",
                        "asfaltweg"
                    ],
                    "common_phrases": [
                        "een modderig karrenspoor",
                        "het oude karrenspoor"
                    ],
                    "target_lang_definitions": {
                        "en": "A simple, unpaved road often consisting of two worn tracks, originally made by cart wheels."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cart track",
                                "target_lang_sense_clarification": "unpaved road formed by wheels"
                            },
                            {
                                "target_lang_word": "rutted track",
                                "target_lang_sense_clarification": "rough path with deep wheel marks"
                            },
                            {
                                "target_lang_word": "dirt track",
                                "target_lang_sense_clarification": "general unpaved path"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13ef99b9-ff16-3203-87bc-1e72c083b250",
                    "sense_definition": "Een weg die als te smal, slecht onderhouden of ouderwets wordt beschouwd voor de hoeveelheid verkeer die er gebruik van maakt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_road",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bewoners klagen dat de drukke hoofdweg eigenlijk maar een veredeld <w>karrenspoor</w> is.",
                            "target_lang_translations": {
                                "en": "The residents complain that the busy main road is actually just a glorified <w>cart track</w>."
                            }
                        },
                        {
                            "text": "De gemeente wil dit gevaarlijke <w>karrenspoor</w> verbreden tot een veilige weg.",
                            "target_lang_translations": {
                                "en": "The municipality wants to widen this dangerous <w>dirt track</w> into a safe road."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een veredeld karrenspoor"
                    ],
                    "target_lang_definitions": {
                        "en": "A road considered too narrow, poorly maintained, or outdated for the volume of traffic it carries."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cart track",
                                "target_lang_sense_clarification": "pejorative for a bad road"
                            },
                            {
                                "target_lang_word": "goat track",
                                "target_lang_sense_clarification": "idiomatic for a narrow, rough road"
                            },
                            {
                                "target_lang_word": "dirt track",
                                "target_lang_sense_clarification": "implying poor quality"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
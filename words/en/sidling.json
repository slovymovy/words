                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бочком",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное, осторожно боком"
                            },
                            {
                                "target_lang_word": "боком",

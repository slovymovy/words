    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c1352cc-2094-3517-b3cd-1f3745da38df",
                    "sense_definition": "Головной убор из драгоценных металлов, часто украшенный камнями, являющийся символом власти монарха (короля, царя, императора).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "power_symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее мы увидели золотую <корону> императора.",
                            "target_lang_translations": {
                                "en": "In the museum, we saw the emperor's golden <w>crown</w>."
                            }
                        },
                        {
                            "text": "Король надел <корону> на голову во время церемонии.",
                            "target_lang_translations": {
                                "en": "The king put the <w>crown</w> on his head during the ceremony."
                            }
                        }
                    ],
                    "common_phrases": [
                        "золотая корона",
                        "надеть корону",
                        "снять корону"
                    ],
                    "target_lang_definitions": {
                        "en": "A headdress made of precious metals, often decorated with stones, serving as a symbol of a monarch's power (king, tsar, emperor)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crown",
                                "target_lang_sense_clarification": "royal headdress"
                            },
                            {
                                "target_lang_word": "diadem",
                                "target_lang_sense_clarification": "ornamental headband"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "764bdaf9-cad7-3627-b83f-bf5773229333",
                    "sense_definition": "Разговорное название коронавируса SARS-CoV-2 или вызываемой им болезни COVID-19.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "virus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие люди боялись заразиться <короной>.",
                            "target_lang_translations": {
                                "en": "Many people were afraid of catching <w>corona</w>."
                            }
                        },
                        {
                            "text": "Из-за <короны> границы стран были закрыты.",
                            "target_lang_translations": {
                                "en": "Because of <w>corona</w>, national borders were closed."
                            }
                        }
                    ],
                    "synonyms": [
                        "коронавирус",
                        "ковид"
                    ],
                    "common_phrases": [
                        "заболеть короной",
                        "тест на корону",
                        "прививка от короны"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "widely used in spoken language"
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A colloquial name for the SARS-CoV-2 coronavirus or the disease COVID-19 caused by it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "corona",
                                "target_lang_sense_clarification": "colloquial for coronavirus"
                            },
                            {
                                "target_lang_word": "coronavirus",
                                "target_lang_sense_clarification": "virus name"
                            },
                            {
                                "target_lang_word": "COVID",
                                "target_lang_sense_clarification": "disease name"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ff73dc6-816f-3d3b-94ee-d88d7e674b3a",
                    "sense_definition": "Монархическая власть, государство или правительство, возглавляемое монархом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "power_symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти земли официально принадлежат британской <короне>.",
                            "target_lang_translations": {
                                "en": "These lands officially belong to the British <w>Crown</w>."
                            }
                        },
                        {
                            "text": "Он всю жизнь верно служил <короне>.",
                            "target_lang_translations": {
                                "en": "He served the <w>Crown</w> faithfully all his life."
                            }

    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "ebcd2cf3-3a7a-3bc8-a0c9-547d8ff53b7b",
                    "sense_definition": "The majority of; more than half of.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "majority_det",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Most</w> bakers and dairy farmers have to get up early.",
                            "target_lang_translations": {
                                "ru": "<w>Большинство</w> пекарей и фермеров должны вставать рано."
                            }
                        },
                        {
                            "text": "Winning was not important for <w>most</w> participants.",
                            "target_lang_translations": {
                                "ru": "Победа была не важна для <w>большинства</w> участников."
                            }
                        }
                    ],
                    "synonyms": [
                        "the majority of",
                        "almost all",
                        "more than half of"
                    ],
                    "common_phrases": [
                        "most people",
                        "most cases",
                        "most part",
                        "for the most part"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая часть; более половины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "большинство",
                                "target_lang_sense_clarification": "существительное, обозначающее большую часть группы"
                            },
                            {
                                "target_lang_word": "большая часть",
                                "target_lang_sense_clarification": "словосочетание, применимое к исчисляемым и неисчисляемым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36fca9bb-6f6c-3fd0-b304-4d13a8867c8d",
                    "sense_definition": "The greatest amount or number of.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "superlative_det",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team with the <w>most</w> points wins the championship.",
                            "target_lang_translations": {
                                "ru": "Команда, набравшая <w>больше всего</w> очков, выигрывает чемпионат."
                            }
                        },
                        {
                            "text": "I spent the <w>most</w> time in Rome because of the floods.",
                            "target_lang_translations": {
                                "ru": "Я провел <w>больше всего</w> времени в Риме из-за наводнений."
                            }
                        }
                    ],
                    "antonyms": [
                        "least"
                    ],
                    "common_phrases": [
                        "the most money",
                        "the most time",
                        "the most points"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наибольшее количество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больше всего",
                                "target_lang_sense_clarification": "наречное выражение, обозначающее максимальное количество"
                            },
                            {
                                "target_lang_word": "наибольший",
                                "target_lang_sense_clarification": "прилагательное в превосходной степени"
                            }
                        ]
                    }
                }
            ]
        },
        {

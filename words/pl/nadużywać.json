    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0fb3157-5cb3-33de-9c50-d24852009378",
                    "sense_definition": "Korzystać z czegoś w nadmiarze, zbyt często lub w sposób szkodliwy (np. dla zdrowia).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "excess_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz ostrzegł pacjenta, aby nie <nadużywał> leków przeciwbólowych.",
                            "target_lang_translations": {
                                "ru": "Врач предупредил пациента, чтобы тот не <злоупотреблял> обезболивающими."
                            }
                        },
                        {
                            "text": "Staram się nie <nadużywać> gościnności moich przyjaciół i rzadko u nich nocuję.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь не <злоупотреблять> гостеприимством моих друзей и редко у них ночую."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesadzać"
                    ],
                    "common_phrases": [
                        "nadużywać alkoholu",
                        "nadużywać cierpliwości",
                        "nadużywać gościnności",
                        "nadużywać leków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пользоваться чем-либо чрезмерно, слишком часто или во вред (например, здоровью)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злоупотреблять",
                                "target_lang_sense_clarification": "пользоваться сверх меры (алкоголем, гостеприимством)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7886a2b9-a90a-3f27-9638-ef9110505700",
                    "sense_definition": "Wykorzystywać coś (np. władzę, stanowisko, zaufanie) w sposób niewłaściwy, nieuczciwy lub niezgodny z przeznaczeniem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "improper_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urzędnik został zwolniony, ponieważ <nadużywał> swojej władzy.",
                            "target_lang_translations": {
                                "ru": "Чиновник был уволен, так как <злоупотреблял> своей властью."
                            }
                        },
                        {
                            "text": "Nie wolno <nadużywać> zaufania, którym obdarzyli nas wyborcy.",
                            "target_lang_translations": {
                                "ru": "Нельзя <злоупотреблять> доверием, которое оказали нам избиратели."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykorzystywać"
                    ],
                    "antonyms": [
                        "szanować"
                    ],
                    "common_phrases": [
                        "nadużywać władzy",
                        "nadużywać zaufania",
                        "nadużywać stanowiska",
                        "nadużywać pozycji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать что-либо (например, власть, должность, доверие) неправильно, нечестно или не по назначению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "злоупотреблять",
                                "target_lang_sense_clarification": "использовать во вред или незаконно (властью, доверием)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
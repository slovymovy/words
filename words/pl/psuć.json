{
    "word_family": [
        "zepsuć",
        "popsuć",
        "zepsucie",
        "psuja",
        "zepsuty"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3114b683-f1d6-37fa-8c3b-61cfe7140400",
                    "sense_definition": "Sprawiać, że przedmiot przestaje działać, traci swoje właściwości lub ulega fizycznemu uszkodzeniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "uszkodzenie_fizyczne",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje dziecko ciągle <psuje> swoje nowe zabawki.",
                            "target_lang_translations": {
                                "ru": "Мой ребенок постоянно <ломает> свои новые игрушки."
                            }
                        },
                        {
                            "text": "Wilgoć <psuje> drewniane meble ogrodowe.",
                            "target_lang_translations": {
                                "ru": "Влага <портит> деревянную садовую мебель."
                            }
                        }
                    ],
                    "synonyms": [
                        "niszczyć",
                        "uszkadzać"
                    ],
                    "antonyms": [
                        "naprawiać"
                    ],
                    "common_phrases": [
                        "psuć zabawki",
                        "psuć wzrok",
                        "psuć zęby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить предмет в негодность, повреждать или ломать его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "повреждать, делать хуже"
                            },
                            {
                                "target_lang_word": "ломать",
                                "target_lang_sense_clarification": "разрушать физически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1deb634a-2a4e-3c56-82b6-ddbb9ee8eaf2",
                    "sense_definition": "Przestawać działać poprawnie lub ulegać awarii (o maszynach, urządzeniach).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "uszkodzenie_fizyczne",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary samochód często się <psuje>.",
                            "target_lang_translations": {
                                "ru": "Моя старая машина часто <ломается>."
                            }
                        },
                        {
                            "text": "Winda w naszym bloku znowu się <psuje>.",
                            "target_lang_translations": {
                                "ru": "Лифт в нашем доме снова <ломается>."
                            }
                        }
                    ],
                    "antonyms": [
                        "naprawiać"
                    ],
                    "common_phrases": [
                        "samochód się psuje",
                        "komputer się psuje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать работать исправно, выходить из строя (о механизмах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ломаться",
                                "target_lang_sense_clarification": "выходить из строя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcd713c4-1fb8-33dc-902b-71d318fe37fd",
                    "sense_definition": "Wpływać negatywnie na przebieg spraw, atmosferę, nastrój lub czyjąś opinię.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pogarszanie_stanu",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę ci <psuć> niespodzianki, więc nic nie powiem.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <портить> тебе сюрприз, поэтому ничего не скажу."
                            }
                        },
                        {
                            "text": "Nagły deszcz <psuje> nam plany na weekendowy wyjazd.",
                            "target_lang_translations": {
                                "ru": "Внезапный дождь <портит> нам планы на поездку в выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "rujnować"
                    ],
                    "antonyms": [
                        "poprawiać"
                    ],
                    "common_phrases": [
                        "psuć krew",
                        "psuć humor",
                        "psuć plany",
                        "psuć opinię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Негативно влиять на ситуацию, настроение или чью-то репутацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "ухудшать (настроение, планы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f9efdea-f8d2-357f-a335-43bd51439679",
                    "sense_definition": "Zmieniać się na gorsze, ulegać negatywnej zmianie (o zjawiskach, stanach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pogarszanie_stanu",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pogoda w górach <psuje> się bardzo gwałtownie.",
                            "target_lang_translations": {
                                "ru": "Погода в горах <портится> очень резко."
                            }
                        },
                        {
                            "text": "Relacje między sąsiadami <psują> się od dłuższego czasu.",
                            "target_lang_translations": {
                                "ru": "Отношения между соседями <портятся> уже долгое время."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymiotować"
                    ],
                    "antonyms": [
                        "poprawiać"
                    ],
                    "common_phrases": [
                        "pogoda się psuje",
                        "wzrok się psuje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Меняться в худшую сторону (о погоде, отношениях, здоровье)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портиться",
                                "target_lang_sense_clarification": "становиться хуже"
                            },
                            {
                                "target_lang_word": "ухудшаться",
                                "target_lang_sense_clarification": "о состоянии или качестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47848d87-a5e7-390a-965f-34319e29b6f7",
                    "sense_definition": "Tracić świeżość i stawać się niezdatnym do spożycia w wyniku procesów gnicia lub fermentacji (o żywności).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "gnicie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalne dni jedzenie pozostawione na stole szybko się <psuje>.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни еда, оставленная на столе, быстро <портится>."
                            }
                        },
                        {
                            "text": "To mleko chyba się <psuje>, bo dziwnie pachnie.",
                            "target_lang_translations": {
                                "ru": "Это молоко, кажется, <портится>, потому что странно пахнет."
                            }
                        }
                    ],
                    "synonyms": [
                        "niszczyć"
                    ],
                    "common_phrases": [
                        "mięso się psuje",
                        "ryba się psuje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гнить или терять свежесть, становясь непригодным для еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портиться",
                                "target_lang_sense_clarification": "о продуктах"
                            },
                            {
                                "target_lang_word": "пропадать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a959275-2630-305d-812f-d305b5ef710e",
                    "sense_definition": "Źle wychowywać dziecko poprzez nadmierne pobłażanie, brak dyscypliny i spełnianie wszystkich zachcianek.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "wychowanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia nieświadomie <psuje> wnuki, dając im ciągle słodycze.",
                            "target_lang_translations": {
                                "ru": "Бабушка неосознанно <портит> внуков, постоянно давая им сладости."
                            }
                        },
                        {
                            "text": "Zbyt duża ilość pieniędzy czasem <psuje> młodych ludzi.",
                            "target_lang_translations": {
                                "ru": "Слишком большие деньги иногда <портят> молодых людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "urzekać"
                    ],
                    "common_phrases": [
                        "psuć dziecko",
                        "psuć charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохо влиять на характер (обычно ребенка) излишней мягкостью или потаканием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "нравственно"
                            },
                            {
                                "target_lang_word": "баловать",
                                "target_lang_sense_clarification": "излишне потакать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
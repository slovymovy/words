                    "target_lang_definitions": {
                        "ru": "Поставщик ликвидности; участник рынка, обеспечивающий возможность торговли активами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поставщик ликвидности",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }

{
    "word_family": [],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "3a529d01-13c9-3fe6-bad0-c0a4e3f29806",
                    "sense_definition": "Not one nor the other of two people or things.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "binary_negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the end, <w>neither</w> side won the game.",
                            "target_lang_translations": {
                                "ru": "В конце концов, <w>ни одна</w> из сторон не выиграла игру."
                            }
                        },
                        {
                            "text": "<w>Neither</w> answer seems to be correct.",
                            "target_lang_translations": {
                                "ru": "<w>Ни один</w> ответ не кажется правильным."
                            }
                        }
                    ],
                    "antonyms": [
                        "both"
                    ],
                    "common_phrases": [
                        "neither one",
                        "neither side",
                        "neither way"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни один и ни другой из двух людей или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни оди́н",
                                "target_lang_sense_clarification": "ни один из двух"
                            },
                            {
                                "target_lang_word": "ни тот, ни друго́й",
                                "target_lang_sense_clarification": "отрицание обоих вариантов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "70472506-787f-3a80-991c-8e68d43d2b95",
                    "sense_definition": "Not one nor the other of two people or things.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "binary_negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have two brothers, but <w>neither</w> lives nearby.",
                            "target_lang_translations": {
                                "ru": "У меня два брата, но <w>ни один</w> не живёт поблизости."
                            }
                        },
                        {
                            "text": "<w>Neither</w> of us wants to go out tonight.",
                            "target_lang_translations": {
                                "ru": "<w>Никто</w> из нас не хочет идти гулять сегодня вечером."
                            }
                        }
                    ],
                    "antonyms": [
                        "both"
                    ],
                    "common_phrases": [
                        "neither of them",
                        "neither of us",
                        "neither of which"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни один и ни другой из двух людей или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни оди́н",
                                "target_lang_sense_clarification": "ни один из группы"
                            },
                            {
                                "target_lang_word": "никто́",
                                "target_lang_sense_clarification": "ни один человек"
                            },
                            {
                                "target_lang_word": "ничто́",
                                "target_lang_sense_clarification": "ни один предмет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c9ae4ab8-906a-35c9-9840-91fc27afb7bc",
                    "sense_definition": "Similarly not; used to indicate that a negative statement also applies to another person or thing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have never been to Paris, and <w>neither</w> has she.",
                            "target_lang_translations": {
                                "ru": "Я никогда не был в Париже, и она <w>тоже не</w> была."
                            }
                        },
                        {
                            "text": "\"I don't like spinach.\" — \"<w>Neither</w> do I.\"",
                            "target_lang_translations": {
                                "ru": "«Я не люблю шпинат». — «Я <w>тоже не</w> люблю»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "me neither",
                        "neither do I",
                        "neither can I"
                    ],
                    "target_lang_definitions": {
                        "ru": "Также не; используется для указания, что отрицательное утверждение относится и к другому лицу или предмету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то́же не",
                                "target_lang_sense_clarification": "согласие с отрицанием"
                            },
                            {
                                "target_lang_word": "та́кже не",
                                "target_lang_sense_clarification": "формальное согласие с отрицанием"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "7c8f5ea3-dbd2-3368-bbfc-ba482e78b690",
                    "sense_definition": "Used before the first of two (or more) alternatives to indicate that none of them is true (used with 'nor').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Neither</w> the blue dress nor the red one fits me.",
                            "target_lang_translations": {
                                "ru": "<w>Ни</w> синее платье, ни красное мне не подходят."
                            }
                        },
                        {
                            "text": "He speaks <w>neither</w> French nor German.",
                            "target_lang_translations": {
                                "ru": "Он не говорит <w>ни</w> по-французски, ни по-немецки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "neither ... nor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед первым из двух (или более) вариантов для указания, что ни один из них не подходит (используется с 'nor')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни",
                                "target_lang_sense_clarification": "первая часть союза 'ни... ни...'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "646a8ec3-e2d5-309d-89bf-2ffd234c5983",
                    "sense_definition": "Nor; and not.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spoke not, <w>neither</w> did he move.",
                            "target_lang_translations": {
                                "ru": "Он не говорил, <w>также не</w> двигался."
                            }
                        },
                        {
                            "text": "They toil not, <w>neither</w> do they spin.",
                            "target_lang_translations": {
                                "ru": "Они не трудятся, <w>также не</w> прядут."
                            }
                        }
                    ],
                    "synonyms": [
                        "nor"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "И не; также не."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "та́кже не",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "и не",
                                "target_lang_sense_clarification": "соединительный союз"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
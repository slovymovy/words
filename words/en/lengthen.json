    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "23ef7af9-1a10-3579-aa26-2fc268536eca",
                    "sense_definition": "To make something longer in size, distance, or duration.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "make_longer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>lengthen</w> the story if you want to have it published.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>удлинить</w> рассказ, если ты хочешь его опубликовать."
                            }
                        },
                        {
                            "text": "The tailor promised to <w>lengthen</w> the sleeves of my jacket.",
                            "target_lang_translations": {
                                "ru": "Портной пообещал <w>удлинить</w> рукава моего пиджака."
                            }
                        }
                    ],
                    "synonyms": [
                        "extend",
                        "elongate",
                        "prolong",
                        "stretch"
                    ],
                    "antonyms": [
                        "shorten",
                        "abbreviate",
                        "abridge"
                    ],
                    "common_phrases": [
                        "lengthen the life",
                        "lengthen the stride",
                        "lengthen the process"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-либо более длинным по размеру, расстоянию или продолжительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удлинять",
                                "target_lang_sense_clarification": "делать более длинным физически"
                            },
                            {
                                "target_lang_word": "продлевать",
                                "target_lang_sense_clarification": "увеличивать продолжительность по времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a532ec92-8636-389d-b72c-6a0236630e0c",
                    "sense_definition": "To become longer in size, distance, or duration.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "become_longer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shadows on the lawn <w>lengthened</w> as the sun began to set.",
                            "target_lang_translations": {
                                "ru": "Тени на лужайке <w>удлинялись</w>, когда солнце начало садиться."
                            }
                        },
                        {
                            "text": "In the spring, the days start to <w>lengthen</w> noticeably.",
                            "target_lang_translations": {
                                "ru": "Весной дни начинают заметно <w>удлиняться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "extend",
                        "stretch"
                    ],
                    "antonyms": [
                        "shorten"
                    ],
                    "common_phrases": [
                        "days lengthen",
                        "shadows lengthen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться более длинным по размеру, расстоянию или продолжительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удлиняться",
                                "target_lang_sense_clarification": "становиться длиннее"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "60c838de-6a53-313d-8997-2ad72534a058",
                    "sense_definition": "Mondeling of schriftelijk informatie proberen te verkrijgen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ask_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik u misschien iets <w>vragen</w>?",
                            "target_lang_translations": {
                                "en": "May I <w>ask</w> you something?",
                                "ru": "Могу я вас кое о чем <w>спросить</w>?"
                            }
                        },
                        {
                            "text": "Hij <w>vroeg</w> aan de agent waar het station was.",
                            "target_lang_translations": {
                                "en": "He <w>asked</w> the police officer where the station was.",
                                "ru": "Он <w>спросил</w> у полицейского, где находится вокзал."
                            }
                        }
                    ],
                    "antonyms": [
                        "antwoorden"
                    ],
                    "common_phrases": [
                        "iemand iets vragen",
                        "de weg vragen",
                        "om raad vragen"
                    ],
                    "target_lang_definitions": {
                        "en": "To try to obtain information orally or in writing.",
                        "ru": "Пытаться получить информацию устно или письменно; задавать вопрос."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to put a question"
                            },
                            {
                                "target_lang_word": "inquire",
                                "target_lang_sense_clarification": "to seek information"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спрашивать",
                                "target_lang_sense_clarification": "несовершенный вид; процесс задавания вопросов"
                            },
                            {
                                "target_lang_word": "спросить",
                                "target_lang_sense_clarification": "совершенный вид; однократное действие"
                            },
                            {
                                "target_lang_word": "задавать вопрос",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bd57fe7-996a-35a4-9482-4b7f4c4b9a47",
                    "sense_definition": "Iemand verzoeken om een bepaalde handeling uit te voeren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vraag</w> je om het raam even dicht te doen.",
                            "target_lang_translations": {
                                "en": "I <w>ask</w> you to close the window for a moment.",
                                "ru": "Я <w>прошу</w> тебя закрыть окно."
                            }
                        },
                        {
                            "text": "Zij <w>vroeg</w> of iemand haar kon helpen met de afwas.",
                            "target_lang_translations": {
                                "en": "She <w>asked</w> if anyone could help her with the dishes.",
                                "ru": "Она <w>попросила</w>, чтобы кто-нибудь помог ей с посудой."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlangen",
                        "eisen"
                    ],
                    "common_phrases": [
                        "vragen om hulp",
                        "vragen om stilte"
                    ],
                    "target_lang_definitions": {
                        "en": "To request someone to perform a certain action.",
                        "ru": "Обращаться к кому-либо с просьбой выполнить определенное действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to request an action"
                            },
                            {
                                "target_lang_word": "request",
                                "target_lang_sense_clarification": "to ask formally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "просить",
                                "target_lang_sense_clarification": "несовершенный вид; выражать просьбу"
                            },
                            {
                                "target_lang_word": "попросить",
                                "target_lang_sense_clarification": "совершенный вид; конкретная просьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95be7d26-39bf-38e6-9563-7c299969d635",
                    "sense_definition": "Iets nodig hebben, eisen of als prijs vaststellen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "require_demand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit moeilijke werk <w>vraagt</w> veel geduld en precisie.",
                            "target_lang_translations": {
                                "en": "This difficult work <w>requires</w> a lot of patience and precision.",
                                "ru": "Эта трудная работа <w>требует</w> большого терпения и точности."
                            }
                        },
                        {
                            "text": "De verkoper <w>vraagt</w> een redelijke prijs voor de auto.",
                            "target_lang_translations": {
                                "en": "The seller <w>asks</w> a reasonable price for the car.",
                                "ru": "Продавец <w>просит</w> разумную цену за машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "eisen",
                        "verlangen",
                        "vergen",
                        "behoeven"
                    ],
                    "common_phrases": [
                        "aandacht vragen",
                        "veel tijd vragen",
                        "een prijs vragen"
                    ],
                    "target_lang_definitions": {
                        "en": "To need something, demand it, or set as a price.",
                        "ru": "Нуждаться в чем-либо, требовать чего-либо или назначать цену."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "require",
                                "target_lang_sense_clarification": "to need or make necessary"
                            },
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "to ask for urgently"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to set a price"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "требовать",
                                "target_lang_sense_clarification": "быть необходимым (о работе, усилиях)"
                            },
                            {
                                "target_lang_word": "запрашивать",
                                "target_lang_sense_clarification": "назначать цену или делать официальный запрос"
                            },
                            {
                                "target_lang_word": "просить",
                                "target_lang_sense_clarification": "назначать цену (разговорное)"
                            }
                        ]
                    }
                }
            ]

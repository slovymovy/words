                {
                    "sense_id": "9526801d-ee37-3320-9b24-4d9e8c361cd0",
                    "sense_definition": "Część sklepu lub marketu, w której sprzedawane są produkty do jedzenia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przenieśli <w>spożywkę</w> na parter, żeby klientom było wygodniej robić zakupy.",
                            "target_lang_translations": {
                                "ru": "<w>Продуктовый отдел</w> перенесли на первый этаж, чтобы клиентам было удобнее делать покупки."
                            }
                        },
                        {
                            "text": "Ty idź na <w>spożywkę</w>, a ja poszukam proszku do prania na dziale chemicznym.",
                            "target_lang_translations": {
                                "ru": "Ты иди в <w>продуктовый отдел</w>, а я поищу стиральный порошок в отделе бытовой химии."

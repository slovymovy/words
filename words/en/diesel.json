    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "065f14d4-877a-31e8-9d8d-bbb0844983ce",
                    "sense_definition": "A type of heavy fuel derived from petroleum, used in engines that burn fuel using hot compressed air rather than spark plugs.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fuel_engine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The truck stopped at the station to fill up with <w>diesel</w>.",
                            "target_lang_translations": {
                                "ru": "Грузовик остановился на заправке, чтобы заправиться <w>соляркой</w>."
                            }
                        },
                        {
                            "text": "Prices for <w>diesel</w> have risen significantly compared to regular gasoline.",
                            "target_lang_translations": {
                                "ru": "Цены на <w>дизельное топливо</w> значительно выросли по сравнению с обычным бензином."
                            }
                        }
                    ],
                    "synonyms": [
                        "diesel fuel",
                        "DFO"
                    ],
                    "common_phrases": [
                        "diesel engine",
                        "diesel fuel",
                        "turbo diesel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид тяжелого топлива, получаемого из нефти, используемого в двигателях, где воспламенение происходит от сжатия горячего воздуха, а не от искры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дизельное топливо",
                                "target_lang_sense_clarification": "официальное название"
                            },
                            {
                                "target_lang_word": "солярка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "дизель",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de2a566c-d756-3061-be2c-ba0890c0c16a",
                    "sense_definition": "A vehicle, such as a car, truck, or train, that is powered by a diesel engine.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fuel_engine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His next truck will be a <w>diesel</w> because he needs the extra towing power.",
                            "target_lang_translations": {
                                "ru": "Его следующим грузовиком будет <w>дизель</w>, так как ему нужна дополнительная тяговая мощность."
                            }
                        },
                        {
                            "text": "Many drivers in Europe prefer to drive a <w>diesel</w> for its fuel efficiency.",
                            "target_lang_translations": {
                                "ru": "Многие водители в Европе предпочитают водить <w>дизель</w> из-за его топливной экономичности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drive a diesel",
                        "clean diesel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство (автомобиль, грузовик или поезд), работающее на дизельном двигателе."
                    },
                    "translations": {

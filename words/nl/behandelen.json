    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a7842cee-0678-3d71-bd46-f3af724a9759",
                    "sense_definition": "Op een bepaalde manier met een persoon of dier omgaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De chef <w>behandelt</w> zijn personeel heel goed en met respect.",
                            "target_lang_translations": {
                                "en": "The boss <w>treats</w> his staff very well and with respect.",
                                "ru": "Начальник очень хорошо и с уважением <w>обращается</w> со своим персоналом."
                            }
                        },
                        {
                            "text": "Je moet een huisdier altijd met liefde en geduld <w>behandelen</w>.",
                            "target_lang_translations": {
                                "en": "You must always <w>treat</w> a pet with love and patience.",
                                "ru": "С домашним животным всегда нужно <w>обращаться</w> с любовью и терпением."
                            }
                        }
                    ],
                    "synonyms": [
                        "omgaan met"
                    ],
                    "common_phrases": [
                        "iemand goed behandelen",
                        "met respect behandelen",
                        "iemand slecht behandelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To behave towards someone or deal with something in a particular way.",
                        "ru": "Вести себя определённым образом по отношению к человеку или животному; обходиться с кем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "treat",
                                "target_lang_sense_clarification": "behave towards"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "deal with"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "вести себя с кем-то"
                            },
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "проявлять отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53320657-1451-3f64-b608-e4d82d95f3c8",
                    "sense_definition": "Medische zorg geven aan een patiënt of een verwonding verzorgen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts <w>behandelt</w> de patiënt voor een ernstige infectie.",
                            "target_lang_translations": {
                                "en": "The doctor is <w>treating</w> the patient for a serious infection.",
                                "ru": "Врач <w>лечит</w> пациента от серьезной инфекции."
                            }
                        },
                        {
                            "text": "De wond werd direct na het ongeluk in het ziekenhuis <w>behandeld</w>.",
                            "target_lang_translations": {
                                "en": "The wound was <w>treated</w> in the hospital immediately after the accident.",
                                "ru": "Рана была <w>обработана</w> в больнице сразу после аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzorgen",
                        "genezen"
                    ],
                    "common_phrases": [
                        "een patiënt behandelen",
                        "een wond behandelen",
                        "medisch behandelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To give medical care to a patient or attend to an injury.",
                        "ru": "Оказывать медицинскую помощь пациенту или обрабатывать рану."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "treat",
                                "target_lang_sense_clarification": "medical care"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лечить",
                                "target_lang_sense_clarification": "пациента или болезнь"
                            },
                            {
                                "target_lang_word": "обрабатывать",
                                "target_lang_sense_clarification": "рану или травму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7e489df-7bb9-393a-be6c-4d35773f139b",
                    "sense_definition": "Een aanvraag, klacht of zaak in ontvangst nemen en verwerken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een paar weken wachten werd mijn aanvraag eindelijk <w>behandeld</w>.",
                            "target_lang_translations": {
                                "en": "After waiting a few weeks, my application was finally <w>processed</w>.",
                                "ru": "После нескольких недель ожидания мою заявку наконец <w>рассмотрели</w>."
                            }
                        },
                        {
                            "text": "De gemeente <w>behandelt</w> uw klacht binnen tien werkdagen.",
                            "target_lang_translations": {
                                "en": "The municipality will <w>handle</w> your complaint within ten working days.",
                                "ru": "Муниципалитет <w>рассмотрит</w> вашу жалобу в течение десяти рабочих дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwerken",
                        "afhandelen"
                    ],
                    "common_phrases": [
                        "een aanvraag behandelen",
                        "een klacht behandelen",
                        "een zaak behandelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To take care of, process, or deal with an application, complaint, or matter.",
                        "ru": "Принимать к рассмотрению и работать с заявлением, жалобой или делом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "manage or deal with"
                            },
                            {
                                "target_lang_word": "process",
                                "target_lang_sense_clarification": "administrative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рассматривать",
                                "target_lang_sense_clarification": "жалобу или дело"
                            },
                            {
                                "target_lang_word": "обрабатывать",
                                "target_lang_sense_clarification": "заявку или данные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d7aa5f9-78b9-3047-ae85-4cca931db014",
                    "sense_definition": "Een onderwerp bespreken, erover spreken of schrijven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het voorstel moest in de eerstvolgende vergadering uitgebreid worden <w>behandeld</w>.",
                            "target_lang_translations": {
                                "en": "The proposal had to be <w>discussed</w> extensively in the next meeting.",
                                "ru": "Предложение должно было быть подробно <w>рассмотрено</w> на следующем собрании."
                            }
                        },
                        {
                            "text": "In dit hoofdstuk <w>behandelen</w> we de geschiedenis van de Nederlandse taal.",
                            "target_lang_translations": {
                                "en": "In this chapter, we <w>cover</w> the history of the Dutch language.",
                                "ru": "В этой главе мы <w>рассматриваем</w> историю нидерландского языка."
                            }
                        }
                    ],
                    "synonyms": [
                        "bespreken"
                    ],
                    "common_phrases": [
                        "een onderwerp behandelen",
                        "een vraagstuk behandelen",
                        "een hoofdstuk behandelen"
                    ],
                    "target_lang_definitions": {

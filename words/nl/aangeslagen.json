{
    "word_family": [
        "aanslaan",
        "aanslag"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "79d9e328-9c2e-3728-8e50-c3fd6eb4356d",
                    "sense_definition": "Emotioneel geraakt, geschokt of van streek door een ingrijpende of droevige gebeurtenis",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De buurt is zwaar <w>aangeslagen</w> door het drama.",
                            "target_lang_translations": {
                                "en": "The neighborhood is deeply <w>shaken</w> by the tragedy.",
                                "ru": "Район глубоко <w>потрясён</w> этой трагедией."
                            }
                        },
                        {
                            "text": "Hij reageerde erg <w>aangeslagen</w> toen hij het slechte nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "He reacted very <w>upset</w> when he heard the bad news.",
                                "ru": "Он был очень <w>расстроен</w>, когда услышал плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontdaan",
                        "ontsteld",
                        "ontzet"
                    ],
                    "common_phrases": [
                        "zwaar aangeslagen",
                        "diep aangeslagen",
                        "aangeslagen reageren"
                    ],
                    "target_lang_definitions": {
                        "en": "Emotionally affected, shocked, or upset by a drastic or sad event",
                        "ru": "Эмоционально затронутый, потрясённый или расстроенный серьёзным или печальным событием"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shaken",
                                "target_lang_sense_clarification": "deeply affected emotionally"
                            },
                            {
                                "target_lang_word": "upset",
                                "target_lang_sense_clarification": "distressed or agitated"
                            },
                            {
                                "target_lang_word": "shocked",
                                "target_lang_sense_clarification": "surprised and upset"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "потрясённый",
                                "target_lang_sense_clarification": "глубоко взволнованный или шокированный"
                            },
                            {
                                "target_lang_word": "расстроенный",
                                "target_lang_sense_clarification": "выбитый из колеи, огорчённый"
                            },
                            {
                                "target_lang_word": "подавленный",
                                "target_lang_sense_clarification": "удручённый, в угнетённом состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55705d98-8c30-3af9-bd27-55eb1935c098",
                    "sense_definition": "Bedekt met een dun laagje vocht of damp, waardoor het oppervlak minder doorzichtig is",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door zijn <w>aangeslagen</w> brillenglazen kon hij de weg niet meer goed zien.",
                            "target_lang_translations": {
                                "en": "Because of his <w>fogged up</w> glasses, he could no longer see the road clearly.",
                                "ru": "Из-за <w>запотевших</w> очков он больше не мог хорошо видеть дорогу."
                            }
                        },
                        {
                            "text": "Ze tekende een hartje op de <w>aangeslagen</w> ruit van de bus.",
                            "target_lang_translations": {
                                "en": "She drew a little heart on the <w>steamed up</w> window of the bus.",
                                "ru": "Она нарисовала сердечко на <w>запотевшем</w> стекле автобуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedampt"
                    ],
                    "common_phrases": [
                        "aangeslagen ruiten",
                        "aangeslagen spiegel",
                        "aangeslagen bril"
                    ],
                    "target_lang_definitions": {
                        "en": "Covered with a thin layer of moisture or vapor, making the surface less transparent",
                        "ru": "Покрытый тонким слоем влаги или пара, из-за чего поверхность становится менее прозрачной"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fogged up",
                                "target_lang_sense_clarification": "covered in condensation"
                            },
                            {
                                "target_lang_word": "steamed up",
                                "target_lang_sense_clarification": "covered in steam"
                            },
                            {
                                "target_lang_word": "misty",
                                "target_lang_sense_clarification": "obscured by moisture"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "запотевший",
                                "target_lang_sense_clarification": "покрытый конденсатом (о стекле, зеркале)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57317945-46f3-3283-828d-3a05088ad53c",
                    "sense_definition": "Verkerend in een toestand met een hoger energieniveau dan de grondtoestand (in de natuurkunde)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als het atoom niet in de grondtoestand is, dan spreken we van een <w>aangeslagen</w> toestand.",
                            "target_lang_translations": {
                                "en": "If the atom is not in the ground state, we speak of an <w>excited</w> state.",
                                "ru": "Если атом не находится в основном состоянии, мы говорим о <w>возбуждённом</w> состоянии."
                            }
                        },
                        {
                            "text": "Het elektron viel terug vanuit de <w>aangeslagen</w> toestand en zond daarbij licht uit.",
                            "target_lang_translations": {
                                "en": "The electron fell back from the <w>excited</w> state and emitted light in the process.",
                                "ru": "Электрон вернулся из <w>возбуждённого</w> состояния и испустил при этом свет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aangeslagen toestand",
                        "aangeslagen atoom"
                    ],
                    "target_lang_definitions": {
                        "en": "Existing in a state with a higher energy level than the ground state (in physics)",
                        "ru": "Находящийся в состоянии с более высоким уровнем энергии, чем основное состояние (в физике)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "excited",
                                "target_lang_sense_clarification": "physics: higher energy state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "возбуждённый",
                                "target_lang_sense_clarification": "физика: имеющий энергию выше минимальной"
                            }
                        ]
                    }
                }

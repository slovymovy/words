{
    "word_family": [
        "causal",
        "causation",
        "causality",
        "causative",
        "causeless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e881c0a1-9fa0-3478-92b1-ebddac115f32",
                    "sense_definition": "The source or reason that produces a specific result, event, or condition.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causality_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police are still investigating the <w>cause</w> of the accident.",
                            "target_lang_translations": {
                                "ru": "Полиция все еще расследует <w>причину</w> аварии."
                            }
                        },
                        {
                            "text": "Scientists have identified the root <w>cause</w> of the disease.",
                            "target_lang_translations": {
                                "ru": "Ученые определили коренную <w>причину</w> заболевания."
                            }
                        },
                        {
                            "text": "Smoking is a leading <w>cause</w> of lung cancer.",
                            "target_lang_translations": {
                                "ru": "Курение является одной из основных <w>причин</w> рака легких."
                            }
                        }
                    ],
                    "synonyms": [
                        "reason",
                        "source",
                        "origin",
                        "root",
                        "basis"
                    ],
                    "antonyms": [
                        "result",
                        "effect",
                        "consequence"
                    ],
                    "common_phrases": [
                        "root cause",
                        "cause of death",
                        "cause and effect",
                        "common cause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Источник или причина, приводящая к определенному результату, событию или состоянию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причина",
                                "target_lang_sense_clarification": "явление, порождающее другое явление"
                            },
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "то, откуда исходит что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c73eb58a-7b8f-3b2a-8864-550e36f7f22a",
                    "sense_definition": "A sufficient reason or justification for feeling or acting in a certain way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "justification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no <w>cause</w> for alarm regarding the weather forecast.",
                            "target_lang_translations": {
                                "ru": "Нет <w>повода</w> для беспокойства из-за прогноза погоды."
                            }
                        },
                        {
                            "text": "The team's recent success is a <w>cause</w> for celebration.",
                            "target_lang_translations": {
                                "ru": "Недавний успех команды — это <w>повод</w> для празднования."
                            }
                        },
                        {
                            "text": "You have no <w>cause</w> to complain about the service.",
                            "target_lang_translations": {
                                "ru": "У вас нет <w>оснований</w> жаловаться на обслуживание."
                            }
                        }
                    ],
                    "synonyms": [
                        "grounds",
                        "justification",
                        "reason"
                    ],
                    "common_phrases": [
                        "cause for concern",
                        "cause for alarm",
                        "cause for celebration",
                        "cause for complaint",
                        "show cause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаточное основание или оправдание для определенных чувств или действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повод",
                                "target_lang_sense_clarification": "обстоятельство, дающее основание для действия"
                            },
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "обоснование, причина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d835954-6e89-3155-81f2-510890bdc01e",
                    "sense_definition": "A principle, aim, or movement that people support, defend, or fight for.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_goal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She dedicated her entire life to the <w>cause</w> of human rights.",
                            "target_lang_translations": {
                                "ru": "Она посвятила всю свою жизнь <w>делу</w> защиты прав человека."
                            }
                        },
                        {
                            "text": "They are collecting donations for a very worthy <w>cause</w>.",
                            "target_lang_translations": {
                                "ru": "Они собирают пожертвования на очень благое <w>дело</w>."
                            }
                        },
                        {
                            "text": "He felt that fighting for the <w>cause</w> was his duty.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что борьба за правое <w>дело</w> была его долгом."
                            }
                        }
                    ],
                    "synonyms": [
                        "goal",
                        "aim",
                        "objective",
                        "purpose",
                        "principle",
                        "target"
                    ],
                    "common_phrases": [
                        "lost cause",
                        "worthy cause",
                        "good cause",
                        "fight for a cause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принцип, цель или общественное движение, которое люди поддерживают и защищают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дело",
                                "target_lang_sense_clarification": "общее начинание, цель или работа"
                            },
                            {
                                "target_lang_word": "идея",
                                "target_lang_sense_clarification": "замысел или принцип"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42f07553-092f-3eee-bc8d-97392079b01b",
                    "sense_definition": "To make something happen or exist; to bring about a result.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causality_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain <w>caused</w> a flood in the lower valley.",
                            "target_lang_translations": {
                                "ru": "Проливной дождь <w>вызвал</w> наводнение в низине."
                            }
                        },
                        {
                            "text": "Driving too fast can <w>cause</w> serious accidents on the highway.",
                            "target_lang_translations": {
                                "ru": "Слишком быстрая езда может <w>вызвать</w> серьезные аварии на шоссе."
                            }
                        },
                        {
                            "text": "I hope I didn't <w>cause</w> you any trouble with my request.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, я не <w>причинил</w> вам беспокойства своей просьбой."
                            }
                        }
                    ],
                    "synonyms": [
                        "basis",
                        "reason",
                        "source",
                        "origin",
                        "root",
                        "basis"
                    ],
                    "antonyms": [
                        "preventer",
                        "prohibitor",
                        "hindrance"
                    ],
                    "common_phrases": [
                        "cause trouble",
                        "cause damage",
                        "cause problems",
                        "cause confusion",
                        "cause pain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной чего-либо; заставлять что-либо происходить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "порождать явление или действие"
                            },
                            {
                                "target_lang_word": "причинять",
                                "target_lang_sense_clarification": "доставлять (обычно о неприятном: боль, вред)"
                            },
                            {
                                "target_lang_word": "быть причиной",
                                "target_lang_sense_clarification": "являться источником события"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "e2d90abe-2a78-35f6-9cb3-0328ab5b7407",
                    "sense_definition": "Shortened form of because.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conjunction_reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I didn't go to the party <w>cause</w> I was too tired.",
                            "target_lang_translations": {
                                "ru": "Я не пошел на вечеринку, <w>потому что</w> слишком устал."
                            }
                        },
                        {
                            "text": "Hurry up <w>cause</w> we are going to be late for the movie.",
                            "target_lang_translations": {
                                "ru": "Поторапливайся, <w>потому что</w> мы опаздываем в кино."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Used in informal speech and writing."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорная сокращенная форма союза \"because\" (потому что)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потому что",
                                "target_lang_sense_clarification": "союз причины"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "03a67597-e46e-3265-a7e8-2b7207a1809f",
                    "sense_definition": "Een gevoel van onzekerheid over wat men moet doen of geloven",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er bestaat geen <w>twijfel</w> over dat hij de waarheid spreekt.",
                            "target_lang_translations": {
                                "en": "There is no <w>doubt</w> that he is speaking the truth.",
                                "ru": "Нет никаких <w>сомнений</w> в том, что он говорит правду."
                            }
                        },
                        {
                            "text": "Zijn <w>twijfel</w> verdween direct toen hij het duidelijke bewijs zag.",
                            "target_lang_translations": {
                                "en": "His <w>doubt</w> disappeared immediately when he saw the clear evidence.",
                                "ru": "Его <w>сомнение</w> исчезло сразу же, как только он увидел явные доказательства."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedenking"
                    ],
                    "antonyms": [
                        "zekerheid"
                    ],
                    "common_phrases": [
                        "zonder twijfel",
                        "in twijfel trekken",
                        "buiten twijfel"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of uncertainty about what one should do or believe",
                        "ru": "Чувство неуверенности в том, что следует делать или во что верить"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "doubt",
                                "target_lang_sense_clarification": "a feeling of uncertainty or lack of conviction"
                            },
                            {
                                "target_lang_word": "uncertainty",
                                "target_lang_sense_clarification": "the state of being uncertain"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сомнение",
                                "target_lang_sense_clarification": "чувство неуверенности"
                            },
                            {
                                "target_lang_word": "неуверенность",
                                "target_lang_sense_clarification": "отсутствие уверенности"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f08c2e6-ca18-3900-a58a-3434363b18c7",
                    "sense_definition": "Недостаток, нехватка чего-либо в нужном количестве",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У современного человека часто наблюдается хронический <w>дефицит</w> сна.",
                            "target_lang_translations": {
                                "en": "Modern people often suffer from a chronic sleep <w>deficit</w>."
                            }
                        },
                        {
                            "text": "Врач назначил витамины, чтобы восполнить <w>дефицит</w> железа в организме.",
                            "target_lang_translations": {
                                "en": "The doctor prescribed vitamins to replenish the iron <w>deficiency</w> in the body."
                            }
                        }
                    ],
                    "antonyms": [
                        "избыток"
                    ],
                    "common_phrases": [
                        "дефицит времени",
                        "дефицит внимания",
                        "острый дефицит",
                        "восполнить дефицит"
                    ],
                    "target_lang_definitions": {
                        "en": "A lack or shortage of something in the required amount"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deficiency",
                                "target_lang_sense_clarification": "used for health or nutritional lack (e.g., vitamins, iron)"
                            },
                            {
                                "target_lang_word": "shortage",
                                "target_lang_sense_clarification": "general lack of resources or supplies"
                            },
                            {
                                "target_lang_word": "deficit",
                                "target_lang_sense_clarification": "used for abstract concepts like time or attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2b7ee57-3136-3e08-8395-bc5130bd0d70",
                    "sense_definition": "Превышение расходов над доходами в бюджете или балансе",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство ищет способы сократить <w>дефицит</w> государственного бюджета.",
                            "target_lang_translations": {
                                "en": "The government is looking for ways to reduce the state budget <w>deficit</w>."
                            }
                        },
                        {
                            "text": "Компания закончила год с большим финансовым <w>дефицитом</w>.",
                            "target_lang_translations": {
                                "en": "The company ended the year with a large financial <w>deficit</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "убыток"
                    ],
                    "antonyms": [
                        "профицит"
                    ],
                    "common_phrases": [
                        "дефицит бюджета",
                        "торговый дефицит",
                        "покрыть дефицит"
                    ],
                    "target_lang_definitions": {
                        "en": "An excess of expenses over income in a budget or balance sheet"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deficit",
                                "target_lang_sense_clarification": "financial term for spending exceeding revenue"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "554a3c6b-8f2b-309d-9cfd-2a82342192aa",
                    "sense_definition": "Товар, который трудно купить из-за его отсутствия в свободной продаже",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В советское время качественная обувь была страшным <w>дефицитом</w>.",
                            "target_lang_translations": {
                                "en": "In Soviet times, quality shoes were in terribly <w>short supply</w>."
                            }
                        },
                        {
                            "text": "Сейчас гречка не является <w>дефицитом</w>, её можно купить в любом магазине.",
                            "target_lang_translations": {
                                "en": "Now buckwheat is not a <w>scarce commodity</w>, it can be bought in any shop."
                            }
                        }
                    ],
                    "common_phrases": [
                        "достать дефицит",
                        "страшный дефицит",
                        "выбросили дефицит"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A product that is difficult to buy due to its absence from free sale"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scarce commodity",
                                "target_lang_sense_clarification": "goods that are in short supply"
                            },
                            {
                                "target_lang_word": "rarity",
                                "target_lang_sense_clarification": "something that is uncommon or hard to find"
                            },
                            {
                                "target_lang_word": "hard-to-get item",
                                "target_lang_sense_clarification": "colloquial"
                            }
                        ]
                    }
                }
            ]
        }
    ]

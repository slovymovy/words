{
    "word_family": [
        "перевозка",
        "перевозчик",
        "перевоз",
        "перевозной",
        "возить",
        "везти"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3cbe8455-4ef8-320b-97dd-803fa45d3d1a",
                    "sense_definition": "Доставлять кого-либо или что-либо из одного места в другое, используя транспортное средство.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поезда могут <перевозить> тяжелые грузы на огромные расстояния.",
                            "target_lang_translations": {
                                "en": "Trains can <transport> heavy loads over huge distances."
                            }
                        },
                        {
                            "text": "Эта компания помогает <перевозить> мебель и вещи при переезде в новый дом.",
                            "target_lang_translations": {
                                "en": "This company helps <move> furniture and belongings when moving to a new house."
                            }
                        }
                    ],
                    "synonyms": [
                        "транспортировать",
                        "доставлять",
                        "везти"
                    ],
                    "common_phrases": [
                        "перевозить пассажиров",
                        "перевозить грузы",
                        "перевозить багаж",
                        "перевозить мебель"
                    ],
                    "target_lang_definitions": {
                        "en": "To take people or goods from one place to another using a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "general term for moving goods or people"
                            },
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to hold and transport something"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to change the location of belongings"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ed38576-c531-3686-9d79-4ac088b4d475",
                    "sense_definition": "Переправлять кого-либо или что-либо через какое-либо препятствие (реку, море, границу) на другую сторону.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_across",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Паром будет <перевозить> туристов на остров каждые полчаса.",
                            "target_lang_translations": {
                                "en": "The ferry will <transport> tourists to the island every half hour."
                            }
                        },
                        {
                            "text": "Лодочник согласился <перевозить> нас через бурную реку за небольшую плату.",
                            "target_lang_translations": {
                                "en": "The boatman agreed to <ferry> us across the stormy river for a small fee."
                            }
                        }
                    ],
                    "synonyms": [
                        "переправлять"
                    ],
                    "common_phrases": [
                        "перевозить через реку",
                        "перевозить через границу",
                        "перевозить на другой берег"
                    ],
                    "target_lang_definitions": {
                        "en": "To take someone or something across a barrier, such as a river or border."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ferry",
                                "target_lang_sense_clarification": "to transport across water"
                            },
                            {
                                "target_lang_word": "take across",
                                "target_lang_sense_clarification": "general movement to the other side"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7664a14f-089f-3df2-84c7-1c5d45c93bcc",
                    "sense_definition": "Постепенно перевезти большое количество людей или предметов (обычно в несколько приемов).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "transport_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За неделю мы <перевозили> на дачу кучу старых вещей из квартиры.",
                            "target_lang_translations": {
                                "en": "Over the week, we <moved> a bunch of old things from the apartment to the dacha."
                            }
                        },
                        {
                            "text": "Водитель такси успел <перевозить> всех гостей на праздник до начала дождя.",
                            "target_lang_translations": {
                                "en": "The taxi driver managed to <shuttle> all the guests to the party before the rain started."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To transport a large quantity of people or items over time, usually in multiple trips."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to transport things in batches"
                            },
                            {
                                "target_lang_word": "shuttle",
                                "target_lang_sense_clarification": "to transport back and forth repeatedly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "264fad26-3b3d-3aaf-8aa6-a5e646ef38f7",
                    "sense_definition": "Należący do mężczyzny, do którego zwracamy się per „pan”, lub go dotyczący (oficjalna forma zaimka dzierżawczego „twój” używana w stosunku do mężczyzn).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_polite_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy to jest <w>pański</w> płaszcz, czy należy do kogoś innego?",
                            "target_lang_translations": {
                                "ru": "Это <w>ваш</w> плащ или он принадлежит кому-то другому?"
                            }
                        },
                        {
                            "text": "Bardzo szanuję <w>pański</w> wkład w rozwój naszej firmy.",
                            "target_lang_translations": {
                                "ru": "Я очень уважаю <w>ваш</w> вклад в развитие нашей фирмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pański bilet",
                        "pański podpis",
                        "pański interes",
                        "pański problem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий мужчине, к которому обращаются на «вы» (официальная форма притяжательного местоимения «твой»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ваш",
                                "target_lang_sense_clarification": "вежливая форма при обращении к мужчине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44178ff3-429e-3377-be92-395de9aa470b",
                    "sense_definition": "Odnoszący się do Boga lub Jezusa Chrystusa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy wierni wspólnie odmówili głośno Modlitwę <w>Pańską</w>.",
                            "target_lang_translations": {
                                "ru": "Все верующие вместе громко прочитали Молитву <w>Господню</w>."
                            }
                        },
                        {
                            "text": "To ważne wydarzenie miało miejsce w Roku <w>Pańskim</w> dwa tysiące dziesiątym.",
                            "target_lang_translations": {
                                "ru": "Это важное событие произошло в лете <w>Господнем</w> две тысячи десятом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Modlitwa Pańska",
                        "Rok Pański",
                        "Grób Pański",
                        "Męka Pańska"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zazwyczaj pisane wielką literą"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Богу или Иисусу Христу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Господень",
                                "target_lang_sense_clarification": "религиозное"
                            },
                            {
                                "target_lang_word": "Божий",
                                "target_lang_sense_clarification": "относящийся к Богу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22d2b470-95c3-3441-b0a0-968aa42163d1",
                    "sense_definition": "Odnoszący się do pana jako właściciela majątku, szlachcica lub arystokraty; cechujący się bogactwem, wystawnością lub hojnością.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_noble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miał prawdziwie <w>pański</w> gest i zapłacił rachunek za wszystkich gości w restauracji.",
                            "target_lang_translations": {
                                "ru": "У него был поистине <w>барский</w> жест, и он оплатил счет за всех гостей в ресторане."
                            }
                        },
                        {
                            "text": "Stare polskie przysłowie mówi, że <w>pańskie</w> oko konia tuczy.",
                            "target_lang_translations": {
                                "ru": "Старая польская пословица гласит, что <w>хозяйский</w> глаз коня полнит."
                            }
                        }
                    ],
                    "synonyms": [
                        "dworski"
                    ],
                    "common_phrases": [
                        "pański gest",
                        "pańskie oko",
                        "pański stół",
                        "pański dwór"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "w odniesieniu do warstw społecznych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к барину, помещику или аристократу; отличающийся богатством, роскошью или щедростью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барский",
                                "target_lang_sense_clarification": "свойственный барину, роскошный"
                            },
                            {
                                "target_lang_word": "господский",
                                "target_lang_sense_clarification": "принадлежащий господину"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
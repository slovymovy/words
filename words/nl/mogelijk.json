{
    "word_family": [
        "mogelijkheid",
        "onmogelijk",
        "mogelijkerwijs"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "02f9c899-b955-3554-97aa-7b35b1e028cd",
                    "sense_definition": "Dat wat gedaan kan worden, kan gebeuren of kan bestaan; uitvoerbaar.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "feasible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is niet <w>mogelijk</w> om hier met een creditcard te betalen.",
                            "target_lang_translations": {
                                "en": "It is not <w>possible</w> to pay with a credit card here."
                            }
                        },
                        {
                            "text": "Is het voor jou <w>mogelijk</w> om morgen iets eerder te komen?",
                            "target_lang_translations": {
                                "en": "Is it <w>possible</w> for you to come a little earlier tomorrow?"
                            }
                        }
                    ],
                    "synonyms": [
                        "uitvoerbaar",
                        "doenlijk"
                    ],
                    "antonyms": [
                        "onmogelijk"
                    ],
                    "common_phrases": [
                        "alles is mogelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "That which can be done, happen, or exist; feasible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possible",
                                "target_lang_sense_clarification": "capable of being done or achieved"
                            },
                            {
                                "target_lang_word": "feasible",
                                "target_lang_sense_clarification": "capable of being carried out"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "787be36a-ee8c-30be-91d6-bf33ead24554",
                    "sense_definition": "In de hoogste mate die bereikbaar of uitvoerbaar is (vaak in combinatie met 'zo').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "extent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag zo snel <w>mogelijk</w> een antwoord op mijn vraag.",
                            "target_lang_translations": {
                                "en": "I would like an answer to my question as soon as <w>possible</w>."
                            }
                        },
                        {
                            "text": "Probeer de opdracht zo goed <w>mogelijk</w> uit te voeren.",
                            "target_lang_translations": {
                                "en": "Try to carry out the assignment as well as <w>possible</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zo snel mogelijk",
                        "zo veel mogelijk",
                        "zo goed mogelijk",
                        "zo spoedig mogelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "To the highest degree achievable or feasible (often used with 'as')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possible",
                                "target_lang_sense_clarification": "used in 'as ... as possible' constructions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc550860-c13e-3752-a559-37d7a9c7dad8",
                    "sense_definition": "Wat misschien het geval is of kan worden; potentieel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "potential",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is een <w>mogelijk</w> risico voor het slagen van het project.",
                            "target_lang_translations": {
                                "en": "This is a <w>possible</w> risk to the success of the project."
                            }
                        },
                        {
                            "text": "We hebben verschillende <w>mogelijke</w> oplossingen voor het probleem besproken.",
                            "target_lang_translations": {
                                "en": "We discussed several <w>possible</w> solutions to the problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "eventueel",
                        "potentieel"
                    ],
                    "target_lang_definitions": {
                        "en": "That which might be the case or might become so; potential."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possible",
                                "target_lang_sense_clarification": "that might exist or happen"
                            },
                            {
                                "target_lang_word": "potential",
                                "target_lang_sense_clarification": "existing in possibility"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c828d46f-95b4-38be-a0e6-35b1d0242858",
                    "sense_definition": "Om aan te geven dat iets kan gebeuren of waar kan zijn, maar niet zeker is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "maybe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mogelijk</w> komt hij later nog naar het feest als hij klaar is met werken.",
                            "target_lang_translations": {
                                "en": "<w>Possibly</w> he will come to the party later when he is finished working."
                            }
                        },
                        {
                            "text": "Het gaat morgen <w>mogelijk</w> regenen in het zuiden van het land.",
                            "target_lang_translations": {
                                "en": "It will <w>possibly</w> rain tomorrow in the south of the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "misschien",
                        "wellicht",
                        "eventueel"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate that something can happen or be true, but is not certain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possibly",
                                "target_lang_sense_clarification": "perhaps; maybe"
                            },
                            {
                                "target_lang_word": "maybe",
                                "target_lang_sense_clarification": "used to express uncertainty"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
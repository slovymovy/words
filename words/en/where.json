                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the house <w>where</w> I was born.",
                            "target_lang_translations": {
                                "ru": "Это дом, <w>где</w> я родился."
                            }
                        },
                        {
                            "text": "Stay <w>where</w> you are.",
                            "target_lang_translations": {
                                "ru": "Оставайся там, <w>где</w> ты есть."
                            }

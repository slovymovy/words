{
    "word_family": [
        "a",
        "one"
    ],
    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "c9fa3f84-2274-395f-b30c-d75a43ff169e",
                    "sense_definition": "The form of the indefinite article 'a' used before a word beginning with a vowel sound.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will be there in half <w>an</w> hour.",
                            "target_lang_translations": {
                                "ru": "Я буду там через полчаса."
                            }
                        },
                        {
                            "text": "She is <w>an</w> English teacher at the local school.",
                            "target_lang_translations": {
                                "ru": "Она — учитель английского в местной школе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "an hour",
                        "an apple",
                        "an idea",
                        "an old friend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма неопределенного артикля 'a', которая используется перед словами, начинающимися с гласного звука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "(не переводится)",
                                "target_lang_sense_clarification": "в русском языке артикли отсутствуют"
                            },
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "иногда используется для усиления значения единичности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "213a319d-0128-33b8-b003-d659df0cfb11",
                    "sense_definition": "In each; to or for each; per.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car was traveling at sixty miles <w>an</w> hour.",
                            "target_lang_translations": {
                                "ru": "Машина ехала со скоростью шестьдесят миль <w>в</w> час."
                            }
                        },
                        {
                            "text": "These specialized herbs cost five dollars <w>an</w> ounce.",
                            "target_lang_translations": {
                                "ru": "Эти особые травы стоят пять долларов <w>за</w> унцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "per"
                    ],
                    "common_phrases": [
                        "an hour",
                        "an ounce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий на распределение (в значении 'в', 'за', 'на')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "при указании скорости или частоты (в час, в год)"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "при указании цены за единицу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "3251fa16-6232-3e0b-90b3-7dfe53e4b3f0",
                    "sense_definition": "If.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>An</w> it please you, I shall depart immediately.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> вам будет угодно, я немедленно удалюсь."
                            }
                        },
                        {
                            "text": "<w>An</w> it harm none, do what you will.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> это никому не вредит, делай что хочешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "an it please",
                        "an it harm none"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Found in older literature."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Если (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "современный эквивалент"
                            },
                            {
                                "target_lang_word": "коли",
                                "target_lang_sense_clarification": "устаревший или просторечный вариант"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
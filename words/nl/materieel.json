                {
                    "sense_id": "6016720a-367a-32e3-ad22-6b73fdf2ccaa",
                    "sense_definition": "Betrekking hebbend op tastbare zaken, geld of goederen; stoffelijk.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adj_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brand veroorzaakte veel <w>materiële</w> schade aan het gebouw.",
                            "target_lang_translations": {
                                "ru": "Пожар нанес зданию значительный <w>материальный</w> ущерб."
                            }
                        },
                        {
                            "text": "Hij hecht weinig waarde aan <w>materieel</w> bezit en leeft liever eenvoudig.",
                            "target_lang_translations": {
                                "ru": "Он придает мало значения <w>материальному</w> имуществу и предпочитает жить просто."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoffelijk",
                        "fysiek",
                        "tastbaar"
                    ],
                    "antonyms": [
                        "immaterieel",
                        "geestelijk"
                    ],
                    "common_phrases": [
                        "materiële schade",
                        "materiële welvaart",
                        "materieel gewin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к осязаемым вещам, деньгам или товарам; вещественный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материальный",

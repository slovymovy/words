                },
                {
                    "sense_id": "e9e72d42-5af1-3915-beef-caa01cc65acc",
                    "sense_definition": "Zeer hard van materiaal of structuur; niet in te drukken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_hardness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Diamant is een <w>keiharde</w> edelsteen.",

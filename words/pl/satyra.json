                    "sense_id": "7c186f95-3ce0-3909-8890-9968b4979856",
                    "sense_definition": "Gatunek literacki lub artystyczny, który ośmiesza i piętnuje ukazywane zjawiska, obyczaje, politykę lub osoby.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "satire_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich <w>satyra</w> ukazuje społeczeństwo za pomocą czarnego humoru.",
                            "target_lang_translations": {
                                "ru": "Их <w>сатира</w> изображает общество с помощью черного юмора."
                            }
                        },
                        {
                            "text": "W gazecie ukazała się ostra <w>satyra</w> na temat nowych przepisów podatkowych.",
                            "target_lang_translations": {
                                "ru": "В газете появилась острая <w>сатира</w> на тему новых налоговых правил."
                            }
                        }
                    ],
                    "common_phrases": [
                        "satyra polityczna",
                        "satyra społeczna",
                        "ostra satyra",
                        "satyra na rządy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Литературный или художественный жанр, высмеивающий и обличающий явления, нравы, политику или людей."
                    },
                    "translations": {
                        "ru": [
                            {

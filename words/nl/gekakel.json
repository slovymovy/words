{
    "word_family": [
        "kakelen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "656a9722-fede-36d8-ada4-491cb447abb9",
                    "sense_definition": "Het aanhoudende, harde geluid dat kippen maken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door het luide <w>gekakel</w> wisten we dat de vos in de buurt was.",
                            "target_lang_translations": {
                                "en": "Because of the loud <w>cackling</w>, we knew the fox was nearby.",
                                "ru": "Из-за громкого <w>кудахтанья</w> мы поняли, что лиса где-то поблизости."
                            }
                        },
                        {
                            "text": "De boer werd elke ochtend wakker van het <w>gekakel</w> in de ren.",
                            "target_lang_translations": {
                                "en": "The farmer woke up every morning from the <w>cackling</w> in the run.",
                                "ru": "Каждое утро фермер просыпался от <w>кудахтанья</w> в курятнике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "luid gekakel",
                        "gekakel van kippen"
                    ],
                    "target_lang_definitions": {
                        "en": "The continuous, loud sound made by chickens.",
                        "ru": "Непрерывный, громкий звук, который издают куры."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cackling",
                                "target_lang_sense_clarification": "continuous sound of hens"
                            },
                            {
                                "target_lang_word": "cackle",
                                "target_lang_sense_clarification": "sound of a hen"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кудахтанье",
                                "target_lang_sense_clarification": "звук кур"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5be9b907-eddc-30f2-b364-7a88a6303542",
                    "sense_definition": "Druk en luidruchtig gepraat door mensen dat als zinloos of irritant wordt ervaren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "human_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de vergadering ergerde de voorzitter zich aan het <w>gekakel</w> achterin de zaal.",
                            "target_lang_translations": {
                                "en": "During the meeting, the chairman was annoyed by the <w>chatter</w> at the back of the room.",
                                "ru": "Во время собрания председателя раздражала <w>болтовня</w> в задней части зала."
                            }
                        },
                        {
                            "text": "Stop met dat zinloos <w>gekakel</w> en ga aan het werk.",
                            "target_lang_translations": {
                                "en": "Stop that senseless <w>chatter</w> and get to work.",
                                "ru": "Прекратите эту бессмысленную <w>болтовню</w> и идите работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "geklets",
                        "gekwebbel",
                        "gesnap",
                        "gesnater"
                    ],
                    "common_phrases": [
                        "zinloos gekakel",
                        "irritant gekakel"
                    ],
                    "target_lang_definitions": {
                        "en": "Busy and noisy talk by people perceived as senseless or annoying.",
                        "ru": "Шумные и бессмысленные разговоры, которые воспринимаются как раздражающие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chatter",
                                "target_lang_sense_clarification": "noisy, annoying talk"
                            },
                            {
                                "target_lang_word": "cackle",
                                "target_lang_sense_clarification": "futile or noisy talk"
                            },
                            {
                                "target_lang_word": "gab",
                                "target_lang_sense_clarification": "idle talk"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "болтовня",
                                "target_lang_sense_clarification": "разговоры"
                            },
                            {
                                "target_lang_word": "трескотня",
                                "target_lang_sense_clarification": "шумная речь"
                            },
                            {
                                "target_lang_word": "галдёж",
                                "target_lang_sense_clarification": "шум"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
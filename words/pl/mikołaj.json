    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a49b5d53-6925-3fbe-a13b-40324d56cf64",
                    "sense_definition": "Męskie imię własne, popularne w Polsce.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mój brat <w>Mikołaj</w> studiuje architekturę w Warszawie.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Николай</w> изучает архитектуру в Варшаве."
                            }
                        },
                        {
                            "text": "<w>Mikołaj</w> Kopernik był wybitnym polskim astronomem.",
                            "target_lang_translations": {
                                "ru": "<w>Николай</w> Коперник был выдающимся польским астрономом."
                            }
                        }
                    ],
                    "synonyms": [
                        "Mikołajek",
                        "Mikuś",
                        "Kola"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя, популярное в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Николай",
                                "target_lang_sense_clarification": "имя"
                            },
                            {
                                "target_lang_word": "Миколай",
                                "target_lang_sense_clarification": "транслитерация польского имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "785f4a20-6bea-3faf-9ae9-41a49c9bc739",
                    "sense_definition": "Postać świętego biskupa lub legendarna postać rozdająca prezenty w okresie świątecznym.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "santa_claus",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Dzieci z niecierpliwością czekają na wizytę świętego <w>Mikołaja</w>.",
                            "target_lang_translations": {
                                "ru": "Дети с нетерпением ждут визита святого <w>Николая</w>."
                            }
                        },
                        {
                            "text": "W tym roku napisałem długi list do świętego <w>Mikołaja</w>.",
                            "target_lang_translations": {
                                "ru": "В этом году я написал длинное письмо святому <w>Николаю</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "święty Mikołaj",
                        "list do Mikołaja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Святой епископ или легендарный персонаж, дарящий подарки в праздничный период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Святой Николай",
                                "target_lang_sense_clarification": "святой"
                            },
                            {
                                "target_lang_word": "Дед Мороз",
                                "target_lang_sense_clarification": "сказочный персонаж"
                            },
                            {
                                "target_lang_word": "Санта-Клаус",
                                "target_lang_sense_clarification": "западный аналог"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07361935-9bb5-3aee-b66c-e6106e538a38",
                    "sense_definition": "Osoba przebrana w strój świętego Mikołaja, często wynajmowana do rozdawania prezentów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "impersonator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wujek Janek co roku przebiera się za <w>mikołaja</w> dla swoich siostrzeńców.",
                            "target_lang_translations": {
                                "ru": "Дядя Янек каждый год переодевается в <w>Деда Мороза</w> для своих племянников."
                            }
                        },
                        {
                            "text": "W centrum handlowym można zrobić sobie zdjęcie z <w>mikołajem</w>.",
                            "target_lang_translations": {
                                "ru": "В торговом центре можно сфотографироваться с <w>Дедом Морозом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przebrać się za mikołaja",
                        "strój mikołaja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, переодетый в костюм Святого Николая или Деда Мороза, часто нанимаемый для раздачи подарков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дед Мороз",
                                "target_lang_sense_clarification": "ряженый / аниматор"
                            },
                            {
                                "target_lang_word": "Санта",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4cfaa17-393d-3f32-bb26-9a720bcf5ee0",
                    "sense_definition": "Figurka z czekolady lub innej słodkiej masy w kształcie świętego Mikołaja.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "confectionery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostałem w prezencie dużego, czekoladowego <w>mikołaja</w>.",
                            "target_lang_translations": {
                                "ru": "Я получил в подарок большого шоколадного <w>Деда Мороза</w>."
                            }
                        },
                        {
                            "text": "Sklepowe półki uginają się od świątecznych <w>mikołajów</w>.",
                            "target_lang_translations": {
                                "ru": "Полки магазинов ломятся от праздничных <w>Дедов Морозов</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czekoladowy mikołaj"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фигурка из шоколада или другой сладкой массы в форме Святого Николая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколадный Дед Мороз",
                                "target_lang_sense_clarification": "кондитерское изделие"
                            },
                            {
                                "target_lang_word": "шоколадный Санта",
                                "target_lang_sense_clarification": "кондитерское изделие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

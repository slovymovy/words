    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51d09360-3f2e-36c8-a5a0-e793a966d634",
                    "sense_definition": "Dorosła kobieta, która nie zawarła związku małżeńskiego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "marital_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja ciocia jest <panną> i mieszka sama w dużym domu.",
                            "target_lang_translations": {
                                "ru": "Моя тетя — <незамужняя> и живет одна в большом доме."
                            }
                        },
                        {
                            "text": "Jako <panna> młoda wyglądała przepięknie w białej sukni.",
                            "target_lang_translations": {
                                "ru": "Как <невеста>, она выглядела прекрасно в белом платье."
                            }
                        }
                    ],
                    "synonyms": [
                        "wolna"
                    ],
                    "antonyms": [
                        "mężatka",
                        "zamężna"
                    ],
                    "common_phrases": [
                        "panna młoda",
                        "stara panna",
                        "panna z dzieckiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослая женщина, не состоящая в браке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незамужняя женщина",
                                "target_lang_sense_clarification": "семейное положение"
                            },
                            {
                                "target_lang_word": "девица",
                                "target_lang_sense_clarification": "устаревшее или ироничное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "266ed341-0f64-3f69-897f-a9d24b8c50f5",
                    "sense_definition": "Partnerka życiowa lub sympatia, z którą jest się w związku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek przyszedł na imprezę ze swoją nową <panną>.",
                            "target_lang_translations": {
                                "ru": "Марек пришел на вечеринку со своей новой <девушкой>."
                            }
                        },
                        {
                            "text": "Jego <panna> nie mogła dzisiaj z nami wyjść do kina.",
                            "target_lang_translations": {
                                "ru": "Его <девушка> не смогла сегодня пойти с нами в кино."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziewczyna",
                        "partnerka"
                    ],
                    "common_phrases": [
                        "jego panna",
                        "moja panna"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спутница жизни или возлюбленная, с которой состоят в отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девушка",
                                "target_lang_sense_clarification": "партнерша в отношениях"
                            },
                            {
                                "target_lang_word": "подруга",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0d36d0a-9d09-3373-8dea-25b8b32af531",
                    "sense_definition": "Młoda kobieta lub dziewczyna; dawniej także tytuł grzecznościowy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "young_woman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na balu pojawiła się piękna <panna> z bogatej rodziny.",
                            "target_lang_translations": {
                                "ru": "На балу появилась прекрасная <барышня> из богатой семьи."
                            }
                        },
                        {
                            "text": "Szanowna <panno>, czy mogę prosić do tańca?",
                            "target_lang_translations": {
                                "ru": "Уважаемая <барышня>, могу я пригласить вас на танец?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dziewczyna",
                        "dziewoja"
                    ],
                    "common_phrases": [
                        "panna sklepowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодая женщина или девушка; ранее также вежливое обращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барышня",
                                "target_lang_sense_clarification": "вежливое или устаревшее"
                            },
                            {
                                "target_lang_word": "девушка",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d263b495-c653-3a3c-ac37-45842f67da24",
                    "sense_definition": "Osoba urodzona w okresie, któremu patronuje znak zodiaku Panny.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "zodiac_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest zodiakalną <Panną>, więc bardzo lubi porządek.",
                            "target_lang_translations": {
                                "ru": "Мой брат — <Дева> по зодиаку, поэтому очень любит порядок."
                            }
                        },
                        {
                            "text": "Ona zachowuje się jak typowa <Panna>, dbając o każdy szczegół.",
                            "target_lang_translations": {
                                "ru": "Она ведет себя как типичная <Дева>, заботясь о каждой детали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zodiakalna Panna",
                        "typowa Panna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, родившийся в период, которому покровительствует знак зодиака Дева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дева",
                                "target_lang_sense_clarification": "человек этого знака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d22d344-4e2e-3796-80af-cc51cff8da1f",
                    "sense_definition": "Kobieta, która nie odbyła jeszcze stosunku seksualnego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "virginity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych kulturach wymagano, aby narzeczona była <panną>.",
                            "target_lang_translations": {
                                "ru": "В древних культурах требовалось, чтобы невеста была <девственницей>."
                            }
                        },
                        {
                            "text": "Legenda głosi, że tylko niewinna <panna> może oswoić jednorożca.",
                            "target_lang_translations": {
                                "ru": "Легенда гласит, что только невинная <дева> может приручить единорога."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziewczyna"
                    ],
                    "common_phrases": [
                        "cnotliwa panna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, не имевшая половых сношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девственница",
                                "target_lang_sense_clarification": "физиологическое состояние"
                            },
                            {
                                "target_lang_word": "дева",
                                "target_lang_sense_clarification": "поэтическое/книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76201f19-bd58-3046-bf4e-6f8b33dd3211",
                    "sense_definition": "Poetyckie lub przestarzałe określenie źrenicy oka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spojrzał głęboko w ciemne <panny> jej oczu.",
                            "target_lang_translations": {
                                "ru": "Он глубоко заглянул в темные <зеницы> ее глаз."
                            }
                        },
                        {
                            "text": "Jego <panny> rozszerzyły się gwałtownie ze strachu.",
                            "target_lang_translations": {
                                "ru": "Его <зрачки> резко расширились от страха."
                            }
                        }
                    ],
                    "synonyms": [
                        "źrenica"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поэтическое или устаревшее название зрачка глаза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зеница",
                                "target_lang_sense_clarification": "устаревшее/поэтическое"
                            },
                            {
                                "target_lang_word": "зрачок",
                                "target_lang_sense_clarification": "анатомическое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e6a11e2b-7467-395e-9618-8b48f3abe53e",
                    "sense_definition": "Szósty znak zodiaku, przypisany osobom urodzonym od końca sierpnia do września.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "zodiac_sign",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Słońce wchodzi w znak <Panny> pod koniec wakacji.",
                            "target_lang_translations": {
                                "ru": "Солнце входит в знак <Девы> в конце каникул."
                            }
                        },
                        {
                            "text": "Osoby spod znaku <Panny> są często uważane za pracowite.",
                            "target_lang_translations": {
                                "ru": "Люди, рожденные под знаком <Девы>, часто считаются трудолюбивыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "♍"
                    ],
                    "common_phrases": [
                        "znak Panny",
                        "spod znaku Panny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шестой знак зодиака, относящийся к людям, родившимся с конца августа по сентябрь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дева",
                                "target_lang_sense_clarification": "знак зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc4fefcc-1e56-3bf3-a812-07a44173d3f6",
                    "sense_definition": "Tytuł odnoszący się do Marii z Nazaretu, matki Jezusa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_figure",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Wierni modlili się do Najświętszej Maryi <Panny>.",
                            "target_lang_translations": {
                                "ru": "Верующие молились Пресвятой <Деве> Марии."
                            }
                        },
                        {
                            "text": "W kościele znajduje się cudowny obraz <Panny> Maryi.",
                            "target_lang_translations": {
                                "ru": "В церкви находится чудотворный образ <Девы> Марии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Najświętsza Maryja Panna",
                        "Panna Maryja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, относящийся к Марии из Назарета, матери Иисуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дева",
                                "target_lang_sense_clarification": "религиозный титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5163c30d-5137-328a-be4b-4089e33cc50e",
                    "sense_definition": "Gwiazdozbiór nieba równikowego, utożsamiany z postacią kobiecą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "constellation",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Wiosną na niebie dobrze widoczny jest gwiazdozbiór <Panny>.",
                            "target_lang_translations": {
                                "ru": "Весной на небе хорошо видно созвездие <Девы>."
                            }
                        },
                        {
                            "text": "Najjaśniejszą gwiazdą w <Pannie> jest Spika.",
                            "target_lang_translations": {
                                "ru": "Самая яркая звезда в <Деве> — Спика."
                            }
                        }
                    ],
                    "synonyms": [
                        "Vir"
                    ],
                    "common_phrases": [
                        "gwiazdozbiór Panny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созвездие экваториального неба, отождествляемое с женской фигурой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дева",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
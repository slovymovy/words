{
    "word_family": [
        "suppression",
        "suppressor",
        "suppressive",
        "suppressant"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2763463-b43c-37e9-b9a4-58da02dd877a",
                    "sense_definition": "To hold back or restrain a physical reaction or emotion.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "control_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bit her lip to <w>suppress</w> a smile during the serious meeting."
                        },
                        {
                            "text": "He could barely <w>suppress</w> his anger when he heard the unfair decision."
                        }
                    ],
                    "synonyms": [
                        "stifle",
                        "restrain",
                        "hold back",
                        "check",
                        "repress"
                    ],
                    "antonyms": [
                        "express",
                        "release",
                        "vent"
                    ],
                    "common_phrases": [
                        "suppress a smile",
                        "suppress a laugh",
                        "suppress anger",
                        "suppress a yawn"
                    ]
                },
                {
                    "sense_id": "4d1fab11-4adf-33c6-91c1-b30211243330",
                    "sense_definition": "To put an end to something forcibly or by authority.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "control_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government sent the army to <w>suppress</w> the rebellion in the north."
                        },
                        {
                            "text": "Police were called to <w>suppress</w> the violent riots that broke out downtown."
                        }
                    ],
                    "synonyms": [
                        "crush",
                        "subdue",
                        "quell",
                        "clamp down",
                        "put down"
                    ],
                    "antonyms": [
                        "incite",
                        "instigate",
                        "provoke"
                    ],
                    "common_phrases": [
                        "suppress a revolt",
                        "suppress dissent",
                        "suppress an uprising",
                        "suppress a riot"
                    ]
                },
                {
                    "sense_id": "7fcf9524-e57e-3f83-8abc-ef91d864614f",
                    "sense_definition": "To prevent information from being published, known, or circulated.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "control_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dictator tried to <w>suppress</w> all news regarding the economic crisis."
                        },
                        {
                            "text": "Journalists accused the agency of trying to <w>suppress</w> the truth about the scandal."
                        }
                    ],
                    "synonyms": [
                        "censor",
                        "conceal",
                        "withhold",
                        "cover up",
                        "bury"
                    ],
                    "antonyms": [
                        "reveal",
                        "disclose",
                        "publish",
                        "publicize"
                    ],
                    "common_phrases": [
                        "suppress evidence",
                        "suppress the truth",
                        "suppress news",
                        "suppress a report"
                    ]
                },
                {
                    "sense_id": "8450b422-f12a-3a2e-8a8a-348a0551343d",
                    "sense_definition": "To consciously or unconsciously push unwanted thoughts or memories out of one's mind.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "control_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>suppress</w> the painful memories of the accident for years."
                        },
                        {
                            "text": "It is often difficult to <w>suppress</w> feelings of jealousy completely."
                        }
                    ],
                    "synonyms": [
                        "repress",
                        "block out",
                        "bury"
                    ],
                    "common_phrases": [
                        "suppress memories",
                        "suppress feelings",
                        "suppress thoughts"
                    ]
                },
                {
                    "sense_id": "341003ea-4e36-37de-834a-0bef005bce52",
                    "sense_definition": "To stop, reduce, or arrest the flow or activity of something, often biological or physical.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "control_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This medication is designed to <w>suppress</w> the body's immune response after a transplant."
                        },
                        {
                            "text": "Drinking warm water helped to <w>suppress</w> his coughing fit."
                        }
                    ],
                    "synonyms": [
                        "check",
                        "stem",
                        "stop",
                        "inhibit"
                    ],
                    "antonyms": [
                        "stimulate"
                    ],
                    "common_phrases": [
                        "suppress a cough",
                        "suppress appetite",
                        "suppress the immune system"
                    ]
                },
                {
                    "sense_id": "eccc7d16-e18d-3c58-b38c-806143d093dc",
                    "sense_definition": "To forbid the use of specific evidence at a legal trial.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "control_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defense attorney filed a motion to <w>suppress</w> the illegally obtained recording."
                        },
                        {
                            "text": "The judge agreed to <w>suppress</w> the evidence due to a procedural error."
                        }
                    ],
                    "synonyms": [
                        "exclude"
                    ],
                    "antonyms": [
                        "admit"
                    ],
                    "common_phrases": [
                        "suppress evidence",
                        "motion to suppress"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ]
                }
            ]
        }
    ]

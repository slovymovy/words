    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f2dbcda1-7c4a-3e11-bbde-841726cc7766",
                    "sense_definition": "Forma fleksyjna (dopełniacz lub biernik liczby pojedynczej) rzeczownika <w>kat</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia wezwał <kata>, aby wykonał zasądzony wyrok.",
                            "target_lang_translations": {
                                "ru": "Судья вызвал <палача>, чтобы тот привел приговор в исполнение."
                            }
                        },
                        {
                            "text": "W dawnych czasach ludzie unikali <kata> i bali się jego dotyku.",
                            "target_lang_translations": {
                                "ru": "В прежние времена люди избегали <палача> и боялись его прикосновения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Падежная форма (родительный или винительный падеж единственного числа) существительного *kat* (палач)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палача",
                                "target_lang_sense_clarification": "форма слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4afce9d6-a9c9-3f3e-94ca-54d5098667ad",
                    "sense_definition": "Wysoce sformalizowany rodzaj ćwiczeń stosowanych w wielu tradycyjnych sztukach i sportach walki (np. w karate), polegający na wykonywaniu ustalonych sekwencji ruchów symulujących walkę z wyimaginowanym przeciwnikiem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "martial_arts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na arenie zakończył się pokaz <kata> w wykonaniu mistrzów.",
                            "target_lang_translations": {
                                "ru": "На арене завершилось показательное выступление <ката> в исполнении мастеров."
                            }
                        },
                        {
                            "text": "Młodzi adepci karate muszą opanować podstawowe <kata>, zanim przystąpią do walki.",
                            "target_lang_translations": {
                                "ru": "Юные каратисты должны освоить базовые <ката>, прежде чем приступать к спаррингам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ćwiczyć kata",
                        "pokaz kata",
                        "wykonać kata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формализованная последовательность движений в боевых искусствах (например, в карате), имитирующая бой с воображаемым противником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ката",
                                "target_lang_sense_clarification": "боевые искусства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48db4565-e784-399d-a050-ceb08d0ad5c0",
                    "sense_definition": "Czuwaliczka jadalna; roślina (krzew lub drzewo) występująca w Afryce i na Półwyspie Arabskim, której liście zawierają substancje stymulujące.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W niektórych regionach Jemenu żucie liści <kata> jest codziennym rytuałem.",
                            "target_lang_translations": {
                                "ru": "В некоторых регионах Йемена жевание листьев <ката> является ежедневным ритуалом."
                            }
                        },
                        {
                            "text": "Uprawa <kata> wymaga specyficznych warunków klimatycznych i dużej ilości wody.",
                            "target_lang_translations": {
                                "ru": "Выращивание <ката> требует особых климатических условий и большого количества воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "czuwaliczka"
                    ],
                    "common_phrases": [
                        "liście kata",
                        "uprawa kata",
                        "żucie kata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кат съедобный; растение (кустарник или дерево), листья которого содержат стимулирующие вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кат",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1ea2ea8f-11df-3a28-897a-29ccdc4e47a2",
                    "sense_definition": "Dawne lub gwarowe wykrzyknienie wyrażające zdziwienie, oburzenie, zniecierpliwienie lub pogardę; eufemizm od słowa kat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <kata>, co ty tutaj robisz o tak późnej porze?",
                            "target_lang_translations": {
                                "ru": "А <чёрт>, что ты здесь делаешь в столь поздний час?"
                            }
                        },
                        {
                            "text": "Do <kata>, nie mogę znaleźć moich kluczy!",
                            "target_lang_translations": {
                                "ru": "<Чёрт возьми>, не могу найти свои ключи!"
                            }
                        }
                    ],
                    "synonyms": [
                        "boga",
                        "bogać",
                        "loboga",
                        "katać"
                    ],
                    "common_phrases": [
                        "do kata"
                    ],
                    "traits": [
                        {

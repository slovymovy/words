    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "36933737-2ba0-31d6-ba20-d6a2a2ac4a0f",
                    "sense_definition": "Having a vertical extent greater than the average; used to describe people or animals.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "height",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is very <w>tall</w> for his age.",
                            "target_lang_translations": {
                                "ru": "Мой брат очень <w>высокий</w> для своего возраста."
                            }
                        },
                        {
                            "text": "She is the <w>tallest</w> player on the basketball team.",
                            "target_lang_translations": {
                                "ru": "Она самый <w>высокий</w> игрок в баскетбольной команде."
                            }
                        }
                    ],
                    "antonyms": [
                        "short"
                    ],
                    "common_phrases": [
                        "stand tall",
                        "walk tall",
                        "tall for one's age"
                    ],
                    "target_lang_definitions": {

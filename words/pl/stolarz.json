        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d74932dd-e2bf-3c19-9744-91c4ba77fe14",
                    "sense_definition": "Rzemieślnik zajmujący się obróbką drewna oraz wykonywaniem, naprawą i konserwacją przedmiotów drewnianych, takich jak meble, okna czy drzwi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "professions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek był <w>stolarzem</w> i robił piękne krzesła dla całej rodziny.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был <w>столяром</w> и делал красивые стулья для всей семьи."
                            }
                        },
                        {
                            "text": "Musimy wezwać <w>stolarza</w>, żeby naprawił te skrzypiące drzwi.",
                            "target_lang_translations": {
                                "ru": "Нам нужно вызвать <w>столяра</w>, чтобы он починил эту скрипучую дверь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobry stolarz",
                        "warsztat stolarza",
                        "narzędzia stolarza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремесленник, занимающийся обработкой дерева, а также изготовлением, ремонтом и реставрацией деревянных предметов, таких как мебель, окна или двери."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столяр",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "0ac585ce-a16b-3118-bb56-d295473d05fc",
                    "sense_definition": "zaimek dzierżawczy oznaczający, że coś należy do osoby lub istoty rodzaju żeńskiego, o której była mowa wcześniej",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest <jej> najlepsza przyjaciółka ze szkoły.",
                            "target_lang_translations": {
                                "ru": "Это <её> лучшая школьная подруга."
                            }
                        },
                        {
                            "text": "Bardzo podoba mi się <jej> nowy samochód.",
                            "target_lang_translations": {
                                "ru": "Мне очень нравится <её> новая машина."
                            }
                        },
                        {
                            "text": "Możesz mi dać <jej> numer telefonu?",
                            "target_lang_translations": {
                                "ru": "Ты можешь дать мне <её> номер телефона?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "jej dom",
                        "jej sprawa",
                        "jej imię",
                        "z jej strony"
                    ],
                    "target_lang_definitions": {
                        "ru": "притяжательное местоимение, означающее, что что-то принадлежит лицу женского пола"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "её",
                                "target_lang_sense_clarification": "притяжательное местоимение (чья?)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63259678-8ed3-3757-8b8a-42c83f11766d",
                    "sense_definition": "odmieniona forma zaimka osobowego <w>ona</w> (w dopełniaczu lub celowniku), wskazująca na kobietę jako obiekt działania lub odbiorcę",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "case_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Jej> nie było dziś w pracy.",
                            "target_lang_translations": {
                                "ru": "<Её> сегодня не было на работе."
                            }
                        },
                        {
                            "text": "Kupiłem kwiaty i dałem <jej> je w prezencie.",
                            "target_lang_translations": {
                                "ru": "Я купил цветы и подарил <ей> их."
                            }
                        },
                        {
                            "text": "Nie ma <jej> teraz w domu.",
                            "target_lang_translations": {
                                "ru": "<Её> сейчас нет дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. ji"
                    ],
                    "common_phrases": [
                        "nie ma jej",
                        "daj jej",
                        "powiedz jej",
                        "szkoda jej"
                    ],
                    "target_lang_definitions": {
                        "ru": "форма личного местоимения «она» (в родительном или дательном падеже), указывающая на женщину как объект действия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "её",
                                "target_lang_sense_clarification": "родительный падеж (нет кого?)"
                            },
                            {
                                "target_lang_word": "ей",
                                "target_lang_sense_clarification": "дательный падеж (кому?)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c9f6174b-5c47-3693-9ced-a361a56f0365",
                    "sense_definition": "słowo używane do wyrażenia nagłych emocji, takich jak zdziwienie, zachwyt, zniecierpliwienie, przestrach lub współczucie",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Jej>, zapomniałam wyłączyć żelazko przed wyjściem!",
                            "target_lang_translations": {
                                "ru": "<Ой>, я забыла выключить утюг перед выходом!"
                            }
                        },
                        {
                            "text": "<Jej>, jak tu pięknie wygląda o zachodzie słońca.",
                            "target_lang_translations": {
                                "ru": "<Ой>, как здесь красиво на закате."
                            }
                        }
                    ],
                    "synonyms": [
                        "jejku"
                    ],
                    "common_phrases": [
                        "o jej",
                        "jej, ale numer"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "междометие, выражающее внезапные эмоции: удивление, восхищение, испуг или сочувствие"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ой",
                                "target_lang_sense_clarification": "выражение удивления или испуга"
                            },
                            {
                                "target_lang_word": "ай",
                                "target_lang_sense_clarification": "выражение боли или сильных эмоций"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
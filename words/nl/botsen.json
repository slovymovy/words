    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc9c8afb-f98f-3fd4-8e08-b83bcea34469",
                    "sense_definition": "Met snelheid of kracht tegen iets of iemand aan komen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twee auto's <w>botsten</w> hard tegen elkaar op de kruising.",
                            "target_lang_translations": {
                                "en": "The two cars <w>collided</w> hard with each other at the intersection.",
                                "ru": "Две машины сильно <w>столкнулись</w> друг с другом на перекрестке."
                            }
                        },
                        {
                            "text": "Ik lette niet op en <w>botste</w> per ongeluk tegen een voorbijganger.",
                            "target_lang_translations": {
                                "en": "I was not paying attention and accidentally <w>bumped</w> into a passerby.",
                                "ru": "Я не обратил внимания и случайно <w>налетел</w> на прохожего."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanrijden",
                        "stoten"
                    ],
                    "antonyms": [
                        "ontwijken"
                    ],
                    "common_phrases": [
                        "tegen elkaar botsen",
                        "op iemand botsen"
                    ],
                    "target_lang_definitions": {
                        "en": "To hit something or someone with speed or force.",
                        "ru": "Удариться обо что-то или кого-то на скорости или с силой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collide",
                                "target_lang_sense_clarification": "general term for striking together"
                            },
                            {
                                "target_lang_word": "crash",
                                "target_lang_sense_clarification": "violent impact, often involving vehicles"
                            },
                            {
                                "target_lang_word": "bump",
                                "target_lang_sense_clarification": "lighter impact, often involving people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сталкиваться",
                                "target_lang_sense_clarification": "общее значение, ударяться друг о друга"
                            },
                            {
                                "target_lang_word": "врезаться",
                                "target_lang_sense_clarification": "сильный удар, часто об авариях"
                            },
                            {
                                "target_lang_word": "налетать",
                                "target_lang_sense_clarification": "случайно задеть кого-то при движении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac7d968f-aed7-3d92-8800-99268bb23a2d",
                    "sense_definition": "Een conflict hebben of niet goed samengaan door verschillen in mening of karakter.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hun karakters <w>botsen</w> te veel om goed te kunnen samenwerken.",
                            "target_lang_translations": {
                                "en": "Their personalities <w>clash</w> too much to work well together.",
                                "ru": "Их характеры слишком сильно <w>конфликтуют</w>, чтобы они могли хорошо работать вместе."
                            }
                        },
                        {
                            "text": "De nieuwe regels <w>botsen</w> met de bestaande afspraken.",
                            "target_lang_translations": {
                                "en": "The new rules <w>conflict</w> with existing agreements.",
                                "ru": "Новые правила <w>противоречат</w> существующим договоренностям."
                            }
                        }
                    ],
                    "synonyms": [
                        "conflicteren",
                        "wringen"
                    ],
                    "antonyms": [
                        "overeenkomen",
                        "harmoniëren"
                    ],
                    "common_phrases": [
                        "botsende belangen",
                        "botsende karakters"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a conflict or be incompatible due to differences.",
                        "ru": "Иметь конфликт или не сочетаться из-за различий во мнениях или характере."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clash",
                                "target_lang_sense_clarification": "regarding personalities, opinions, or colors"
                            },
                            {
                                "target_lang_word": "conflict",
                                "target_lang_sense_clarification": "regarding rules, interests, or ideas"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "конфликтовать",
                                "target_lang_sense_clarification": "о людях, интересах или мнениях"
                            },
                            {
                                "target_lang_word": "противоречить",
                                "target_lang_sense_clarification": "о правилах, фактах или идеях"
                            },
                            {
                                "target_lang_word": "не сходиться",
                                "target_lang_sense_clarification": "о характерах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

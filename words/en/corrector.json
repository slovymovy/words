{
    "word_family": [
        "correct",
        "correction",
        "corrective",
        "correctness",
        "correctly",
        "correctional"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c486fcd7-353b-326e-bbee-6d00a79a5739",
                    "sense_definition": "A person or thing that identifies and fixes errors, faults, or defects.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I installed a spelling <w>corrector</w> on my browser to catch typos.",
                            "target_lang_translations": {
                                "ru": "Я установил <w>корректор</w> правописания в свой браузер, чтобы отлавливать опечатки."
                            }
                        },
                        {
                            "text": "The teacher acts as a strict <w>corrector</w> of our written assignments.",
                            "target_lang_translations": {
                                "ru": "Учитель выступает в роли строгого <w>корректора</w> наших письменных работ."
                            }
                        }
                    ],
                    "synonyms": [
                        "rectifier"
                    ],
                    "common_phrases": [
                        "spelling corrector",
                        "grammar corrector",
                        "posture corrector",
                        "color corrector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или устройство, которое выявляет и исправляет ошибки, неисправности или дефекты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корректор",
                                "target_lang_sense_clarification": "средство для исправления (канцелярское, косметическое, программное)"
                            },
                            {
                                "target_lang_word": "исправитель",
                                "target_lang_sense_clarification": "тот, кто исправляет что-либо (общее значение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad156629-e9be-3919-96a3-5a55b2090da3",
                    "sense_definition": "A person employed to read and correct text proofs before they are published.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>corrector</w> found several mistakes in the manuscript before it went to print.",
                            "target_lang_translations": {
                                "ru": "<w>Корректор</w> обнаружил несколько ошибок в рукописи перед ее отправкой в печать."
                            }
                        },
                        {
                            "text": "She worked as a press <w>corrector</w> for a major newspaper in London.",
                            "target_lang_translations": {
                                "ru": "Она работала <w>корректором</w> в крупной лондонской газете."
                            }
                        }
                    ],
                    "synonyms": [
                        "proofreader"
                    ],
                    "common_phrases": [
                        "press corrector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник, нанятый для вычитки и исправления текстов перед их публикацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корректор",
                                "target_lang_sense_clarification": "профессия в издательском деле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b96fe39-da57-3422-b994-a7b254aae218",
                    "sense_definition": "A director or governor, typically in a historical context.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The emperor appointed a new <w>corrector</w> to manage the province.",
                            "target_lang_translations": {
                                "ru": "Император назначил нового <w>корректора</w> для управления провинцией."
                            }
                        },
                        {
                            "text": "He served as a <w>corrector</w> of public morals during the ancient regime.",
                            "target_lang_translations": {
                                "ru": "Он служил <w>исправителем</w> общественной морали во времена старого режима."
                            }
                        }
                    ],
                    "synonyms": [
                        "governor",
                        "director"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Правитель или управляющий, обычно в историческом контексте (например, в Римской империи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корректор",
                                "target_lang_sense_clarification": "историческая административная должность"
                            },
                            {
                                "target_lang_word": "исправитель",
                                "target_lang_sense_clarification": "устаревшее, книжное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
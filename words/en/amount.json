{
    "word_family": [
        "mount",
        "dismount",
        "surmount",
        "paramount",
        "remount",
        "mountain"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "346520b2-db20-3814-a971-a4bcfdc23659",
                    "sense_definition": "A quantity or volume of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please pour a small <w>amount</w> of water into the bowl.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, налей небольшое <w>количество</w> воды в миску."
                            }
                        },
                        {
                            "text": "The recipe requires equal <w>amounts</w> of sugar and flour.",
                            "target_lang_translations": {
                                "ru": "Рецепт требует равного <w>количества</w> сахара и муки."
                            }
                        }
                    ],
                    "synonyms": [
                        "quantity",
                        "volume",
                        "measure"
                    ],
                    "common_phrases": [
                        "small amount",
                        "large amount",
                        "certain amount",
                        "huge amount",
                        "fair amount"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество или объем чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коли́чество",
                                "target_lang_sense_clarification": "объем или число"
                            },
                            {
                                "target_lang_word": "объём",
                                "target_lang_sense_clarification": "пространственная величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3acbdde2-add8-3561-9155-25d61cfaaabb",
                    "sense_definition": "The total sum, aggregate, or result of something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The full <w>amount</w> must be paid by the end of the month.",
                            "target_lang_translations": {
                                "ru": "Полная <w>сумма</w> должна быть выплачена к концу месяца."
                            }
                        },
                        {
                            "text": "They were surprised by the total <w>amount</w> of the bill.",
                            "target_lang_translations": {
                                "ru": "Они были удивлены общей <w>суммой</w> счета."
                            }
                        }
                    ],
                    "synonyms": [
                        "total",
                        "sum",
                        "aggregate",
                        "whole"
                    ],
                    "common_phrases": [
                        "total amount",
                        "full amount",
                        "entire amount"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая сумма, итог или результат чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "су́мма",
                                "target_lang_sense_clarification": "денежная или итоговая"
                            },
                            {
                                "target_lang_word": "ито́г",
                                "target_lang_sense_clarification": "результат подсчета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0776ab34-46ea-3f11-b923-8d11b931a873",
                    "sense_definition": "The number of elements or items in a set or group.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A significant <w>amount</w> of students signed up for the course.",
                            "target_lang_translations": {
                                "ru": "Значительное <w>количество</w> студентов записалось на курс."
                            }
                        },
                        {
                            "text": "We need to reduce the <w>amount</w> of cars on the road.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сократить <w>количество</w> машин на дорогах."
                            }
                        }
                    ],
                    "synonyms": [
                        "number",
                        "count"
                    ],
                    "common_phrases": [
                        "amount of people",
                        "amount of time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Число элементов или предметов в наборе или группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коли́чество",
                                "target_lang_sense_clarification": "численность"
                            },
                            {
                                "target_lang_word": "число́",
                                "target_lang_sense_clarification": "счетных предметов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "22cdd3b8-e1e3-30af-8703-514cda8b20ee",
                    "sense_definition": "To add up to a specific total or number.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_total",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The repair bill <w>amounted</w> to more than we expected.",
                            "target_lang_translations": {
                                "ru": "Счет за ремонт <w>составил</w> больше, чем мы ожидали."
                            }
                        },
                        {
                            "text": "His debts <w>amount</w> to a very large sum of money.",
                            "target_lang_translations": {
                                "ru": "Его долги <w>достигают</w> очень большой суммы денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "total",
                        "add up to",
                        "come to"
                    ],
                    "common_phrases": [
                        "amount to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать определенной суммы или количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составля́ть",
                                "target_lang_sense_clarification": "в сумме"
                            },
                            {
                                "target_lang_word": "достига́ть",
                                "target_lang_sense_clarification": "определенного уровня"
                            },
                            {
                                "target_lang_word": "равня́ться",
                                "target_lang_sense_clarification": "быть равным сумме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "207c13b9-ff67-3e56-b556-2d3c19fd88cb",
                    "sense_definition": "To be equal to or have the same effect as something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_equal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her silence on the matter <w>amounted</w> to a confession.",
                            "target_lang_translations": {
                                "ru": "Ее молчание по этому вопросу <w>было равносильно</w> признанию."
                            }
                        },
                        {
                            "text": "What he said <w>amounts</w> to a refusal to help us.",
                            "target_lang_translations": {
                                "ru": "То, что он сказал, <w>сводится</w> к отказу помочь нам."
                            }
                        }
                    ],
                    "synonyms": [
                        "constitute",
                        "equal",
                        "represent"
                    ],
                    "common_phrases": [
                        "amount to nothing",
                        "amount to much",
                        "amount to the same thing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть равносильным или иметь тот же эффект, что и что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть равноси́льным",
                                "target_lang_sense_clarification": "по значению или эффекту"
                            },
                            {
                                "target_lang_word": "своди́ться к",
                                "target_lang_sense_clarification": "иметь тот же результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5336aba-6544-3076-a79a-879ba39f1982",
                    "sense_definition": "To go up or ascend.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_ascend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thick smoke <w>amounted</w> straight into the air.",
                            "target_lang_translations": {
                                "ru": "Густой дым <w>поднимался</w> прямо в воздух."
                            }
                        },
                        {
                            "text": "He <w>amounted</w> the stairs with great effort.",
                            "target_lang_translations": {
                                "ru": "Он <w>поднялся</w> по лестнице с большим усилием."
                            }
                        }
                    ],
                    "synonyms": [
                        "ascend",
                        "rise",
                        "climb"
                    ],
                    "antonyms": [
                        "descend"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подниматься или восходить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднима́ться",
                                "target_lang_sense_clarification": "двигаться вверх"
                            },
                            {
                                "target_lang_word": "возноси́ться",
                                "target_lang_sense_clarification": "устремляться ввысь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
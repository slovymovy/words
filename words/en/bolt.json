                    "target_lang_definitions": {
                        "ru": "Прочно соединять или скреплять предметы, часто с помощью болтов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скреплять болтами",
                                "target_lang_sense_clarification": "соединять крепежом"
                            },
                            {
                                "target_lang_word": "привинчивать",
                                "target_lang_sense_clarification": "крепить винтами"

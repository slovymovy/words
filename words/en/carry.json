{
    "word_family": [
        "carrier",
        "carriage",
        "miscarry",
        "miscarriage"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc08c9dc-3cef-3127-997b-f7e9bdb56936",
                    "sense_definition": "To lift something and take it to another place; to transport something by lifting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you help me <w>carry</w> these heavy boxes to the car?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне <w>донести</w> эти тяжелые коробки до машины?"
                            }
                        },
                        {
                            "text": "She <w>carried</w> her sleeping child up the stairs to bed.",
                            "target_lang_translations": {
                                "ru": "Она <w>несла</w> своего спящего ребенка вверх по лестнице в кровать."
                            }
                        }
                    ],
                    "synonyms": [
                        "transport",
                        "move",
                        "convey",
                        "bear",
                        "lift",
                        "haul",
                        "cart",
                        "lug",
                        "tote",
                        "schlep"
                    ],
                    "common_phrases": [
                        "carry a bag",
                        "carry a box",
                        "carry groceries"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднимать что-либо и перемещать в другое место; транспортировать что-либо на весу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "перемещать в руках или на себе (однонаправленное действие)"
                            },
                            {
                                "target_lang_word": "носить",
                                "target_lang_sense_clarification": "перемещать в руках или на себе (многократное действие)"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "перемещать из одного места в другое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad6534d4-963d-3106-a384-955c5ee62504",
                    "sense_definition": "To have something on one's person; to hold or keep something with oneself.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "possession_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not like to <w>carry</w> a lot of cash in my wallet.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <w>носить</w> много наличных в кошельке."
                            }
                        },
                        {
                            "text": "Police officers must <w>carry</w> their identification at all times.",
                            "target_lang_translations": {
                                "ru": "Полицейские должны всегда <w>иметь при себе</w> удостоверение."
                            }
                        }
                    ],
                    "synonyms": [
                        "have",
                        "keep",
                        "bear"
                    ],
                    "common_phrases": [
                        "carry cash",
                        "carry a weapon",
                        "carry ID"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь что-либо при себе; держать или хранить что-либо у себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носить (с собой)",
                                "target_lang_sense_clarification": "иметь при себе постоянно или регулярно"
                            },
                            {
                                "target_lang_word": "иметь при себе",
                                "target_lang_sense_clarification": "обладать чем-то в данный момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfc0b106-cf8b-33fb-9cab-c7a6291700fe",
                    "sense_definition": "To stock or supply specific goods for sale; to have in store.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "commerce_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Does this supermarket <w>carry</w> fresh organic vegetables?",
                            "target_lang_translations": {
                                "ru": "В этом супермаркете <w>продают</w> свежие органические овощи?"
                            }
                        },
                        {
                            "text": "We are sorry, but we no longer <w>carry</w> that brand of shoes.",
                            "target_lang_translations": {
                                "ru": "Нам жаль, но мы больше не <w>продаем</w> обувь этой марки."
                            }
                        }
                    ],
                    "synonyms": [
                        "stock",
                        "supply",
                        "keep",
                        "have"
                    ],
                    "common_phrases": [
                        "carry a brand",
                        "carry products",
                        "carry stock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь в наличии определенные товары для продажи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продавать",
                                "target_lang_sense_clarification": "торговать определенным товаром"
                            },
                            {
                                "target_lang_word": "иметь в продаже",
                                "target_lang_sense_clarification": "иметь в наличии в магазине"
                            },
                            {
                                "target_lang_word": "торговать",
                                "target_lang_sense_clarification": "заниматься продажей чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16764e33-42d9-3499-b8ed-f53f792451fb",
                    "sense_definition": "In an addition operation, to transfer a digit to the next column of higher value.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "math_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When you add eight and five, write down three and <w>carry</w> the one.",
                            "target_lang_translations": {
                                "ru": "Когда складываешь восемь и пять, пиши три, а единицу <w>переноси</w>."
                            }
                        },
                        {
                            "text": "Don't forget to <w>carry</w> the numbers over to the next column.",
                            "target_lang_translations": {
                                "ru": "Не забудь <w>перенести</w> цифры в следующий столбец."
                            }
                        }
                    ],
                    "antonyms": [
                        "borrow"
                    ],
                    "common_phrases": [
                        "carry the one",
                        "carry over"
                    ],
                    "target_lang_definitions": {
                        "ru": "При сложении: переносить цифру в следующий разряд старшего значения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "в математических вычислениях"
                            },
                            {
                                "target_lang_word": "держать в уме",
                                "target_lang_sense_clarification": "запоминать цифру для следующего разряда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7856b994-e41b-369a-b5ae-179bf11fa896",
                    "sense_definition": "To support the weight of something; to hold up or sustain a burden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "support_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pillars were designed to <w>carry</w> the immense weight of the roof.",
                            "target_lang_translations": {
                                "ru": "Колонны были спроектированы так, чтобы <w>выдерживать</w> огромный вес крыши."
                            }
                        },
                        {
                            "text": "He felt that he had to <w>carry</w> the burden of the family's problems alone.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что должен <w>нести</w> бремя семейных проблем в одиночку."
                            }
                        }
                    ],
                    "synonyms": [
                        "bear",
                        "maintain",
                        "shoulder"
                    ],
                    "common_phrases": [
                        "carry the weight",
                        "carry the burden",
                        "carry the load"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдерживать вес чего-либо; поддерживать или нести бремя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдерживать",
                                "target_lang_sense_clarification": "справляться с весом или нагрузкой"
                            },
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "нести бремя, ответственность или груз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62840c92-e83b-3c68-aafd-0eb55209a3b6",
                    "sense_definition": "To be pregnant with a child or young.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pregnancy_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>carrying</w> twins and is due in October.",
                            "target_lang_translations": {
                                "ru": "Она <w>вынашивает</w> двойню, и срок у нее в октябре."
                            }
                        },
                        {
                            "text": "The surrogate mother <w>carried</w> the baby for the couple.",
                            "target_lang_translations": {
                                "ru": "Суррогатная мать <w>выносила</w> ребенка для этой пары."
                            }
                        }
                    ],
                    "synonyms": [
                        "bear"
                    ],
                    "common_phrases": [
                        "carry a child",
                        "carry to term"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть беременной ребенком или детенышем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынашивать",
                                "target_lang_sense_clarification": "быть беременной"
                            },
                            {
                                "target_lang_word": "носить",
                                "target_lang_sense_clarification": "разговорный вариант для беременности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc7c9db0-9df1-3aec-a2ad-05003f1a34b9",
                    "sense_definition": "To travel or be transmitted over a distance, such as sound or smell.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "travel_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sound of the church bells <w>carried</w> across the valley.",
                            "target_lang_translations": {
                                "ru": "Звон церковных колоколов <w>разносился</w> по долине."
                            }
                        },
                        {
                            "text": "His deep voice <w>carries</w> well in the large auditorium.",
                            "target_lang_translations": {
                                "ru": "Его глубокий голос хорошо <w>слышен</w> в большой аудитории."
                            }
                        }
                    ],
                    "synonyms": [
                        "travel"
                    ],
                    "common_phrases": [
                        "sound carries",
                        "voice carries"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространяться или передаваться на расстояние (о звуке или запахе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разноситься",
                                "target_lang_sense_clarification": "распространяться в пространстве"
                            },
                            {
                                "target_lang_word": "доноситься",
                                "target_lang_sense_clarification": "быть слышным на расстоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fe37975-d5e5-3d21-97db-d11934e08a85",
                    "sense_definition": "To bear oneself in a particular way; to behave or conduct oneself.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>carries</w> herself with great dignity and confidence.",
                            "target_lang_translations": {
                                "ru": "Она <w>держит</w> себя с большим достоинством и уверенностью."
                            }
                        },
                        {
                            "text": "Even in difficult times, he <w>carried</w> himself like a true professional.",
                            "target_lang_translations": {
                                "ru": "Даже в трудные времена он <w>вел</w> себя как настоящий профессионал."
                            }
                        }
                    ],
                    "synonyms": [
                        "conduct",
                        "bear"
                    ],
                    "common_phrases": [
                        "carry oneself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать себя определенным образом; вести себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держать себя",
                                "target_lang_sense_clarification": "о манере поведения и осанке"
                            },
                            {
                                "target_lang_word": "вести себя",
                                "target_lang_sense_clarification": "о поведении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca0f9205-1cf7-3753-9018-36e013dee6ae",
                    "sense_definition": "To be disproportionately responsible for a team's success; to do most of the work.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He really <w>carried</w> the team during the championship game.",
                            "target_lang_translations": {
                                "ru": "Он реально <w>тащил</w> команду во время чемпионата."
                            }
                        },
                        {
                            "text": "I am tired of <w>carrying</w> this project while everyone else relaxes.",
                            "target_lang_translations": {
                                "ru": "Я устал <w>тянуть</w> этот проект, пока все остальные отдыхают."
                            }
                        }
                    ],
                    "synonyms": [
                        "shoulder"
                    ],
                    "common_phrases": [
                        "carry the team"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in gaming and sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нести основную ответственность за успех команды; делать большую часть работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "сленг: обеспечивать победу команды в одиночку"
                            },
                            {
                                "target_lang_word": "вытягивать",
                                "target_lang_sense_clarification": "спасать ситуацию, делать всю работу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "972f024e-3c03-3aad-a2b6-cc6becea179d",
                    "sense_definition": "To succeed in winning support, an election, or a motion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "success_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate hopes to <w>carry</w> the swing states in the upcoming election.",
                            "target_lang_translations": {
                                "ru": "Кандидат надеется <w>победить</w> в колеблющихся штатах на предстоящих выборах."
                            }
                        },
                        {
                            "text": "The motion was <w>carried</w> by a large majority of the committee.",
                            "target_lang_translations": {
                                "ru": "Предложение было <w>принято</w> подавляющим большинством членов комитета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "carry the election",
                        "carry the motion",
                        "carry the day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добиться успеха в получении поддержки, победы на выборах или принятии предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побеждать",
                                "target_lang_sense_clarification": "выигрывать выборы или голосование"
                            },
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "добиваться принятия (резолюции, закона)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b87702b-325b-32e2-8a31-0f23839600a9",
                    "sense_definition": "The digit that is transferred to the next column in an addition operation.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "math_noun_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Write down the sum and do not forget to mark the <w>carry</w>.",
                            "target_lang_translations": {
                                "ru": "Запиши сумму и не забудь отметить <w>перенос</w>."
                            }
                        },
                        {
                            "text": "The computer handles the <w>carry</w> automatically during calculation.",
                            "target_lang_translations": {
                                "ru": "Компьютер обрабатывает <w>перенос</w> автоматически во время вычислений."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифра, которая переносится в следующий разряд при сложении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перенос",
                                "target_lang_sense_clarification": "математическое действие или цифра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e5e4f82-7fef-3803-a79c-2c0ca39fee3c",
                    "sense_definition": "A manner of holding or transporting something; the position in which something is held.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manner_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The firefighter used a special <w>carry</w> to lift the injured man.",
                            "target_lang_translations": {
                                "ru": "Пожарный использовал специальный <w>хват</w>, чтобы поднять раненого."
                            }
                        },
                        {
                            "text": "You should adjust your <w>carry</w> so you do not drop the equipment.",
                            "target_lang_translations": {
                                "ru": "Тебе следует поправить <w>хват</w>, чтобы не уронить оборудование."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fireman's carry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ удержания или транспортировки чего-либо; положение, в котором что-то держится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хват",
                                "target_lang_sense_clarification": "способ удержания"
                            },
                            {
                                "target_lang_word": "переноска",
                                "target_lang_sense_clarification": "процесс или способ несения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c16a9c07-b2ca-335b-aa11-59abb6400f26",
                    "sense_definition": "A tract of land where boats or goods must be transported between two bodies of water.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geo_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had to cross a long <w>carry</w> to get our canoe to the next lake.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось пересечь длинный <w>волок</w>, чтобы перенести каноэ к следующему озеру."
                            }
                        },
                        {
                            "text": "The explorers rested at the end of the <w>carry</w> before launching their boats.",
                            "target_lang_translations": {
                                "ru": "Исследователи отдыхали в конце <w>волока</w> перед спуском лодок на воду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок суши, по которому лодки или грузы переносятся между двумя водоемами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волок",
                                "target_lang_sense_clarification": "сухопутный путь между водоемами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4e38a740-e336-3265-9d98-9a199168ec7e",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_1",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Carry</w> signed the documents yesterday.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Кэрри</w> подписал документы вчера."
                            }
                        },
                        {
                            "text": "I went to school with a girl named Sarah <w>Carry</w>.",
                            "target_lang_translations": {
                                "ru": "Я учился в школе с девочкой по имени Сара <w>Кэрри</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "A surname."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кэрри",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
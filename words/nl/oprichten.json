{
    "word_family": [
        "oprichter",
        "oprichting",
        "heroprichten"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e397c62-61cd-3f97-b87b-baffc682d710",
                    "sense_definition": "Een organisatie, bedrijf of vereniging officieel starten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vereniging werd vorig jaar <w>opgericht</w>.",
                            "target_lang_translations": {
                                "en": "The association was <w>founded</w> last year.",
                                "ru": "Ассоциация была <w>основана</w> в прошлом году."
                            }
                        },
                        {
                            "text": "Mijn grootvader heeft dit succesvolle bedrijf vijftig jaar geleden <w>opgericht</w>.",
                            "target_lang_translations": {
                                "en": "My grandfather <w>founded</w> this successful company fifty years ago.",
                                "ru": "Мой дедушка <w>основал</w> эту успешную компанию пятьдесят лет назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "stichten",
                        "starten",
                        "vestigen"
                    ],
                    "antonyms": [
                        "opheffen",
                        "ontbinden"
                    ],
                    "common_phrases": [
                        "een bedrijf oprichten",
                        "een vereniging oprichten",
                        "een stichting oprichten"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially start an organization, company, or association.",
                        "ru": "Официально создать организацию, компанию или объединение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "found",
                                "target_lang_sense_clarification": "a company or organization"
                            },
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "an institution or business"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "a business or entity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "основать",
                                "target_lang_sense_clarification": "положить начало существованию (города, предприятия)"
                            },
                            {
                                "target_lang_word": "учредить",
                                "target_lang_sense_clarification": "официально образовать (организацию, фонд)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b814e268-fdaa-333e-b8c8-9c0b8e54328c",
                    "sense_definition": "Iets recht overeind zetten of bouwen, vaak een monument of standbeeld; ook: zichzelf rechtzetten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente wil een standbeeld <w>oprichten</w> ter ere van de slachtoffers.",
                            "target_lang_translations": {
                                "en": "The municipality wants to <w>erect</w> a statue in honor of the victims.",
                                "ru": "Муниципалитет хочет <w>воздвигнуть</w> памятник в честь жертв."
                            }
                        },
                        {
                            "text": "Hij <w>richtte</w> zich langzaam <w>op</w> uit zijn luie stoel.",
                            "target_lang_translations": {
                                "en": "He slowly <w>straightened</w> himself <w>up</w> from his easy chair.",
                                "ru": "Он медленно <w>приподнялся</w> со своего кресла."
                            }
                        }
                    ],
                    "synonyms": [
                        "rechtzetten",
                        "overeind zetten"
                    ],
                    "antonyms": [
                        "neerhalen",
                        "omverwerpen"
                    ],
                    "common_phrases": [
                        "een monument oprichten",
                        "zich oprichten"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something upright or build, often a monument or statue; also: to straighten oneself up.",
                        "ru": "Поставить что-либо вертикально или построить (часто памятник); также: выпрямиться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "erect",
                                "target_lang_sense_clarification": "a monument or structure"
                            },
                            {
                                "target_lang_word": "straighten up",
                                "target_lang_sense_clarification": "posture or body position"
                            },
                            {
                                "target_lang_word": "raise",
                                "target_lang_sense_clarification": "a memorial or structure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "воздвигнуть",
                                "target_lang_sense_clarification": "соорудить что-то значительное (памятник, здание)"
                            },
                            {
                                "target_lang_word": "приподняться",
                                "target_lang_sense_clarification": "выпрямиться, встать (возвратная форма)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
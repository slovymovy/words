    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a14342d3-4d8a-36d5-83b5-24393ee44a35",
                    "sense_definition": "De werking van een apparaat, machine of systeem stoppen; zorgen dat iets niet meer actief is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "turn_off",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeet niet om het licht <w>uit</w> te <w>schakelen</w> als je de kamer verlaat.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>switch off</w> the light when you leave the room.",
                                "ru": "Не забудь <w>выключить</w> свет, когда будешь выходить из комнаты."
                            }
                        },
                        {
                            "text": "Hij <w>schakelde</w> zijn telefoon <w>uit</w> voordat de film begon.",
                            "target_lang_translations": {
                                "en": "He <w>turned off</w> his phone before the movie started.",
                                "ru": "Он <w>выключил</w> свой телефон перед началом фильма."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitzetten",
                        "afschakelen"
                    ],
                    "antonyms": [
                        "inschakelen",
                        "aanschakelen"
                    ],
                    "common_phrases": [
                        "het licht uitschakelen",
                        "de stroom uitschakelen",
                        "een apparaat uitschakelen",
                        "de motor uitschakelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop the operation of a device, machine, or system; to ensure something is no longer active.",
                        "ru": "Остановить работу устройства, машины или системы; сделать так, чтобы что-то перестало быть активным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn off",
                                "target_lang_sense_clarification": "most common general term"
                            },
                            {
                                "target_lang_word": "switch off",
                                "target_lang_sense_clarification": "using a switch or button"
                            },
                            {
                                "target_lang_word": "disable",
                                "target_lang_sense_clarification": "to deactivate a function or feature"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выключать",
                                "target_lang_sense_clarification": "свет, приборы"
                            },
                            {
                                "target_lang_word": "отключать",
                                "target_lang_sense_clarification": "питание, функцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04109868-93b9-3ab9-ba3b-038e113944af",
                    "sense_definition": "Een tegenstander of team verslaan zodat deze niet meer verder kan spelen in een toernooi of competitie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "eliminate_contest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze ploeg werd helaas al in de eerste ronde <w>uitgeschakeld</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, our team was <w>eliminated</w> already in the first round.",
                                "ru": "К сожалению, наша команда <w>выбыла</w> уже в первом раунде."
                            }
                        },
                        {
                            "text": "De tennisser wist de favoriet van het toernooi verrassend <w>uit</w> te <w>schakelen</w>.",
                            "target_lang_translations": {
                                "en": "The tennis player surprisingly managed to <w>knock out</w> the tournament favorite.",
                                "ru": "Теннисист сумел неожиданно <w>выбить</w> фаворита турнира."
                            }
                        }
                    ],
                    "synonyms": [
                        "elimineren"
                    ],
                    "common_phrases": [
                        "de tegenstander uitschakelen",
                        "in de finale uitschakelen",
                        "iemand uitschakelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To defeat an opponent or team so they can no longer continue in a tournament or competition.",
                        "ru": "Победить противника или команду так, чтобы они не могли продолжать участие в турнире или соревновании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eliminate",
                                "target_lang_sense_clarification": "formal context"
                            },
                            {
                                "target_lang_word": "knock out",
                                "target_lang_sense_clarification": "common in sports contexts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выбивать",
                                "target_lang_sense_clarification": "из турнира"
                            },
                            {
                                "target_lang_word": "исключать",
                                "target_lang_sense_clarification": "из соревнований"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a557f986-f878-3400-b1e2-a5354dd08add",
                    "sense_definition": "Iets of iemand onschadelijk maken, neutraliseren of een gevaar wegnemen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "neutralize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie wist de gevaarlijke overvaller snel <w>uit</w> te <w>schakelen</w>.",
                            "target_lang_translations": {
                                "en": "The police managed to quickly <w>neutralize</w> the dangerous robber.",
                                "ru": "Полиции удалось быстро <w>обезвредить</w> опасного грабителя."
                            }
                        },
                        {
                            "text": "Dit nieuwe medicijn kan schadelijke bacteriën effectief <w>uitschakelen</w>.",
                            "target_lang_translations": {
                                "en": "This new medicine can effectively <w>eliminate</w> harmful bacteria.",
                                "ru": "Это новое лекарство может эффективно <w>нейтрализовать</w> вредные бактерии."
                            }
                        }
                    ],
                    "synonyms": [
                        "elimineren"
                    ],
                    "common_phrases": [
                        "een vijand uitschakelen",
                        "een gevaar uitschakelen",
                        "het alarm uitschakelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To render something or someone harmless, neutralize them, or remove a danger.",
                        "ru": "Обезвредить кого-то или что-то, нейтрализовать или устранить опасность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neutralize",
                                "target_lang_sense_clarification": "to make harmless"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "informal, often implies force"
                            },
                            {
                                "target_lang_word": "disable",
                                "target_lang_sense_clarification": "to incapacitate"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обезвреживать",
                                "target_lang_sense_clarification": "преступника, бомбу"
                            },
                            {
                                "target_lang_word": "нейтрализовать",
                                "target_lang_sense_clarification": "угрозу, врага"
                            }
                        ]
                    }
                }
            ]
        }

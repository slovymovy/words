                {
                    "sense_id": "091b6aac-178d-3b3d-82b0-f19c45a08d1d",
                    "sense_definition": "An ethnic Vietnamese person, specifically referring to those living in China.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ethnicity",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "The <w>Gin</w> people are one of the recognized ethnic minority groups in China.",
                            "target_lang_translations": {
                                "ru": "Народ <w>цзин</w> является одним из признанных этнических меньшинств в Китае."
                            }
                        },
                        {
                            "text": "Cultural traditions of the <w>Gin</w> community are celebrated annually.",
                            "target_lang_translations": {
                                "ru": "Культурные традиции общины <w>цзин</w> празднуются ежегодно."
                            }
                        }
                    ],
                    "synonyms": [
                        "Jing",
                        "Kinh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель этнической группы вьетнамского происхождения, проживающей в Китае."

            "senses": [
                {
                    "sense_id": "d1aac84a-3e59-3493-82e7-2d95cd3ab36f",
                    "sense_definition": "Een speelgoedpop die een volwassen vrouw voorstelt, vaak met lang haar en verwisselbare modieuze kleding.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn kleine zusje speelt elke middag met haar favoriete <w>barbiepop</w>.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра каждый день играет со своей любимой <w>куклой Барби</w>."
                            }
                        },
                        {
                            "text": "Voor haar verjaardag kreeg ze een nieuwe <w>barbiepop</w> met veel accessoires.",
                            "target_lang_translations": {
                                "ru": "На день рождения она получила новую <w>куклу Барби</w> с множеством аксессуаров."
                            }
                        }
                    ],
                    "synonyms": [
                        "Barbie"
                    ],
                    "common_phrases": [
                        "een nieuwe barbiepop",
                        "met een barbiepop spelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрушечная кукла, изображающая взрослую женщину, часто с длинными волосами и сменной модной одеждой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кукла Барби",
                                "target_lang_sense_clarification": "игрушка"
                            },
                            {
                                "target_lang_word": "Барби",
                                "target_lang_sense_clarification": "разговорное название куклы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9e16730-909d-3006-a8a3-c6d76361db1d",
                    "sense_definition": "Een vrouw die er door haar uiterlijk, make-up en kleding onnatuurlijk perfect of gemaakt uitziet, vergelijkbaar met een speelgoedpop.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door al die make-up en haar geblondeerde haar lijkt ze net een <w>barbiepop</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за всего этого макияжа и обесцвеченных волос она выглядит как <w>кукла Барби</w>."
                            }
                        },
                        {
                            "text": "Ze wil eruitzien als een levende <w>barbiepop</w> en besteedt uren aan haar uiterlijk.",
                            "target_lang_translations": {
                                "ru": "Она хочет выглядеть как живая <w>кукла Барби</w> и тратит часы на свою внешность."
                            }
                        }
                    ],
                    "synonyms": [
                        "modepopje"
                    ],
                    "common_phrases": [
                        "levende barbiepop",
                        "eruitzien als een barbiepop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которая из-за своей внешности, макияжа и одежды выглядит неестественно идеально или искусственно, подобно игрушечной кукле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кукла Барби",
                                "target_lang_sense_clarification": "о внешности девушки"
                            },
                            {
                                "target_lang_word": "Барби",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]

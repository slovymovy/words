                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worst",
                                "target_lang_sense_clarification": "superlative form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "худшее",
                                "target_lang_sense_clarification": "самое плохое"
                            },
                            {
                                "target_lang_word": "самое страшное",
                                "target_lang_sense_clarification": "наихудший вариант"

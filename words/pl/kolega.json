            "senses": [
                {
                    "sense_id": "f3ee48cc-c293-364f-bd53-406415da24d1",
                    "sense_definition": "Osoba, którą się zna i lubi, z którą spędza się czas wolny; bliski znajomy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój najlepszy <kolega> mieszka w bloku obok.",
                            "target_lang_translations": {
                                "ru": "Мой лучший <друг> живёт в соседнем доме."
                            }
                        },
                        {
                            "text": "Wczoraj wieczorem wyszedłem z <kolegą> do kina.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером я пошёл с <приятелем> в кино."
                            }
                        }
                    ],
                    "synonyms": [
                        "kumpel",
                        "znajomy",
                        "ziomek"
                    ],
                    "common_phrases": [
                        "dobry kolega",
                        "najlepszy kolega",
                        "stary kolega"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого знаешь и любишь, с которым проводишь свободное время; близкий знакомый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "друг",
                                "target_lang_sense_clarification": "человек, связанный с кем-то близкими отношениями, доверием и преданностью"
                            },
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "хороший знакомый, с которым поддерживаешь дружеские отношения, но не такие близкие, как с другом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c73b5074-5ccd-31e4-bdeb-bd7c280a302b",
                    "sense_definition": "Osoba, z którą się pracuje w jednej firmie lub uczy w jednej szkole.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz nowy <kolega> z pracy jest bardzo pomocny.",
                            "target_lang_translations": {
                                "ru": "Наш новый <коллега> по работе очень отзывчивый."
                            }
                        },
                        {
                            "text": "Spotkałem dawnego <kolegę> ze studiów na konferencji.",
                            "target_lang_translations": {
                                "ru": "Я встретил давнего <товарища> по учёбе на конференции."
                            }
                        }
                    ],
                    "synonyms": [
                        "znajomy"
                    ],
                    "common_phrases": [
                        "kolega z pracy",
                        "kolega ze szkoły",
                        "kolega po fachu",
                        "starszy kolega"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, с которым работаешь в одной компании или учишься в одной школе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллега",
                                "target_lang_sense_clarification": "товарищ по работе, профессии или роду занятий"
                            },
                            {
                                "target_lang_word": "товарищ",
                                "target_lang_sense_clarification": "человек, связанный с кем-то общим делом, условиями жизни или учёбой"
                            }
                        ]
                    }
                }
            ]
        }

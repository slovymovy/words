                },
                {
                    "sense_id": "70487cfa-e5c8-3ed6-bc37-788a2e5277a2",
                    "sense_definition": "A challenge; a defiance; bravado.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "challenge_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He ignored the <w>braves</w> of his enemies and stood his ground.",
                            "target_lang_translations": {
                                "ru": "Он проигнорировал <w>вызовы</w> своих врагов и стоял на своем."
                            }
                        },
                        {
                            "text": "She dismissed his <w>braves</w> and continued her work.",
                            "target_lang_translations": {
                                "ru": "Она отмахнулась от его <w>бравады</w> и продолжила работу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызов, бравада (устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызов",
                                "target_lang_sense_clarification": "приглашение к борьбе или спору"
                            },
                            {
                                "target_lang_word": "бравада",
                                "target_lang_sense_clarification": "показная смелость"
                            }
                        ]
                    }
                }
            ]
        },

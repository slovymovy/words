    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8b49d0f-66dd-3b73-a5e0-8ad09860cec3",
                    "sense_definition": "Сторона света, где восходит солнце; направление, противоположное западу.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро солнце встаёт на <w>востоке</w>.",
                            "target_lang_translations": {
                                "en": "Every morning the sun rises in the <w>east</w>."
                            }
                        },
                        {
                            "text": "Окна нашей новой квартиры выходят на <w>восток</w>.",
                            "target_lang_translations": {
                                "en": "The windows of our new apartment face <w>east</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "восход"
                    ],
                    "antonyms": [
                        "запад"
                    ],
                    "common_phrases": [
                        "на востоке",
                        "ехать на восток",
                        "Дальний Восток",
                        "Ближний Восток"
                    ],
                    "target_lang_definitions": {
                        "en": "The cardinal direction where the sun rises; the direction opposite to the west."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "east",
                                "target_lang_sense_clarification": "cardinal direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5a66664-95a1-3ff7-bbc3-985d5d7daa5d",
                    "sense_definition": "Часть какой-либо территории (страны, города), расположенная в восточном направлении.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Самые густые леса находятся на <w>востоке</w> области.",
                            "target_lang_translations": {
                                "en": "The densest forests are located in the <w>east</w> of the region."
                            }
                        },
                        {
                            "text": "Этот город расположен к <w>востоку</w> от столицы.",
                            "target_lang_translations": {
                                "en": "This city is located to the <w>east</w> of the capital."
                            }
                        }
                    ],
                    "antonyms": [
                        "запад"
                    ],
                    "common_phrases": [
                        "восток страны",
                        "к востоку от"
                    ],
                    "target_lang_definitions": {
                        "en": "A part of a territory (country, city) located in the eastern direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "east",
                                "target_lang_sense_clarification": "region or area"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2dfacbc1-9aff-3426-ac7c-aa24bc71584f",
                    "sense_definition": "Страны Азии и Северной Африки, а также их культура и традиции (обычно пишется с большой буквы).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geopolitical",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Он много лет изучал историю и культуру <w>Востока</w>.",
                            "target_lang_translations": {
                                "en": "He studied the history and culture of the <w>East</w> for many years."
                            }
                        },
                        {
                            "text": "Путешественники привезли с <w>Востока</w> редкие специи и ткани.",
                            "target_lang_translations": {
                                "en": "Travelers brought rare spices and fabrics from the <w>East</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "Запад"
                    ],
                    "common_phrases": [
                        "Древний Восток",
                        "дело тонкое"
                    ],
                    "target_lang_definitions": {
                        "en": "The countries of Asia and North Africa, as well as their culture and traditions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "East",
                                "target_lang_sense_clarification": "geopolitical region"
                            },
                            {
                                "target_lang_word": "Orient",
                                "target_lang_sense_clarification": "cultural/historical context"
                            }
                        ]
                    }
                }
            ]
        }
    ]

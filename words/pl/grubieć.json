    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a4248f6e-c3a9-3587-9d03-376fd426efae",
                    "sense_definition": "stawać się grubszym, zwiększać masę ciała",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kot zaczął <grubieć> na zimę, jedząc więcej niż zwykle.",
                            "target_lang_translations": {
                                "ru": "Мой кот начал <толстеть> к зиме, поедая больше обычного."
                            }
                        },
                        {
                            "text": "Od siedzącego trybu życia niestety szybko <grubieję>.",
                            "target_lang_translations": {
                                "ru": "От сидячего образа жизни я, к сожалению, быстро <толстею>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tyć",
                        "przybierać"
                    ],
                    "antonyms": [
                        "chudnąć"
                    ],
                    "common_phrases": [
                        "grubieć w oczach",
                        "zacząć grubieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "становиться толще, набирать вес (о живых существах)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толстеть",
                                "target_lang_sense_clarification": "становиться толстым"
                            },
                            {
                                "target_lang_word": "полнеть",
                                "target_lang_sense_clarification": "набирать вес (более мягкое выражение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bc37ccb-92e5-38ec-aa9d-67b5ca8ae888",
                    "sense_definition": "zwiększać swoją objętość lub szerokość w przekroju (o przedmiotach lub warstwach)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lód na jeziorze zaczął <grubieć> wraz z nadejściem silnych mrozów.",
                            "target_lang_translations": {
                                "ru": "Лед на озере начал <утолщаться> с приходом сильных морозов."
                            }
                        },
                        {
                            "text": "Warstwa śniegu na dachu zaczęła niebezpiecznie <grubieć>.",
                            "target_lang_translations": {
                                "ru": "Слой снега на крыше начал опасно <утолщаться>."
                            }
                        }
                    ],
                    "antonyms": [
                        "cienieć"
                    ],
                    "common_phrases": [
                        "grubieć z dnia na dzień"
                    ],
                    "target_lang_definitions": {
                        "ru": "увеличиваться в объеме или толщине (о предметах или слоях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утолщаться",
                                "target_lang_sense_clarification": "становиться более толстым (о слое, предмете)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56e0dcf4-6580-3f73-a03b-4fdb4909a7bc",
                    "sense_definition": "stawać się niższym, brzmieć bardziej basowo (o głosie)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W okresie dojrzewania głos chłopców zaczyna <grubieć> i się łamać.",
                            "target_lang_translations": {
                                "ru": "В период полового созревания голос у мальчиков начинает <грубеть> и ломаться."
                            }
                        },
                        {
                            "text": "Jej głos zaczął <grubieć> pod wpływem silnych emocji.",
                            "target_lang_translations": {
                                "ru": "Ее голос начал <грубеть> под влиянием сильных эмоций."
                            }
                        }
                    ],
                    "synonyms": [
                        "obniżać się"
                    ],
                    "common_phrases": [
                        "głos zaczyna grubieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "становиться ниже, звучать более басовито (о голосе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грубеть",
                                "target_lang_sense_clarification": "становиться более низким и резким (о голосе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b2bc9b2-d7e3-3478-927c-0a1a93400fae",
                    "sense_definition": "stawać się szorstkim, twardym lub mniej delikatnym w dotyku",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skóra na jego dłoniach zaczęła <grubieć> od ciężkiej pracy fizycznej.",
                            "target_lang_translations": {
                                "ru": "Кожа на его ладонях начала <грубеть> от тяжелой физической работы."
                            }
                        },
                        {
                            "text": "Naskórek na piętach może <grubieć>, jeśli nie jest odpowiednio pielęgnowany.",
                            "target_lang_translations": {
                                "ru": "Кожа на пятках может <огрубевать>, если за ней не ухаживать должным образом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skóra zaczyna grubieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "становиться шершавым, жестким или менее нежным на ощупь"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грубеть",
                                "target_lang_sense_clarification": "становиться жестким (о коже)"
                            },
                            {
                                "target_lang_word": "огрубевать",
                                "target_lang_sense_clarification": "покрываться жесткой кожей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f71e4d-239c-3b6e-a71d-50ad02a068e8",
                    "sense_definition": "tracić dobre maniery, stawać się ordynarnym lub wulgarnym",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zauważyłem, że jego żarty zaczęły <grubieć> w nowym towarzystwie.",
                            "target_lang_translations": {
                                "ru": "Я заметил, что его шутки начали <пошлеть> в новой компании."
                            }
                        },
                        {
                            "text": "Atmosfera spotkania zaczęła <grubieć>, gdy padły pierwsze wyzwiska.",
                            "target_lang_translations": {
                                "ru": "Атмосфера встречи начала <становиться грубой>, когда прозвучали первые оскорбления."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "терять хорошие манеры, становиться вульгарным или грубым"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пошлеть",
                                "target_lang_sense_clarification": "становиться непристойным (о шутках)"
                            },
                            {
                                "target_lang_word": "грубеть",
                                "target_lang_sense_clarification": "становиться грубым в поведении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
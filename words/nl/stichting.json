{
    "word_family": [
        "stichten",
        "stichter"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4fb1967e-31f8-33ac-a437-d3c53050ccb0",
                    "sense_definition": "Een organisatie met een maatschappelijk of sociaal doel die geen winst probeert te maken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze organisatie is een <w>stichting</w>, dus we hebben geen winstoogmerk.",
                            "target_lang_translations": {
                                "ru": "Наша организация — это <w>фонд</w>, поэтому мы не преследуем коммерческих целей."
                            }
                        },
                        {
                            "text": "Zij doneert elk jaar geld aan een goede <w>stichting</w> voor dierenwelzijn.",
                            "target_lang_translations": {
                                "ru": "Каждый год она жертвует деньги в хороший <w>фонд</w> защиты животных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een stichting oprichten",
                        "stichting zonder winstoogmerk",
                        "bestuur van de stichting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация с общественной или социальной целью, не стремящаяся к получению прибыли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонд",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "благотворительный фонд",
                                "target_lang_sense_clarification": "некоммерческая организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ba3c2cf-387f-3205-9a7e-cf65246c5bb0",
                    "sense_definition": "Het moment of de handeling van het oprichten van een instelling, gebouw of stad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "act_of_founding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>stichting</w> van de stad Rome vond duizenden jaren geleden plaats.",
                            "target_lang_translations": {
                                "ru": "<w>Основание</w> города Рима произошло тысячи лет назад."
                            }
                        },
                        {
                            "text": "Na de <w>stichting</w> van het bedrijf groeide het al snel uit tot een succes.",
                            "target_lang_translations": {
                                "ru": "После <w>основания</w> компания быстро выросла и стала успешной."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprichting"
                    ],
                    "common_phrases": [
                        "na de stichting",
                        "de stichting van Rome"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент или действие по созданию учреждения, здания или города."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "учреждение",
                                "target_lang_sense_clarification": "создание организации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
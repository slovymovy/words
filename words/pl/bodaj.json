{
    "word_family": [
        "bodajże"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b21bd4c4-89ba-3d43-ab8b-a3a850c97e88",
                    "sense_definition": "Słowo wyrażające przypuszczenie lub niepewność mówiącego co do prawdziwości wypowiedzi; pełni funkcję zbliżoną do 'chyba'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "uncertainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To było <bodaj> w zeszłym roku, ale nie jestem pewien.",
                            "target_lang_translations": {
                                "ru": "Это было, <пожалуй>, в прошлом году, но я не уверен."
                            }
                        },
                        {
                            "text": "Mieszka tam <bodaj> od urodzenia, więc zna wszystkich sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Он живет там, <пожалуй>, с рождения, поэтому знает всех соседей."
                            }
                        }
                    ],
                    "synonyms": [
                        "chyba",
                        "prawdopodobnie",
                        "zapewne",
                        "podobno",
                        "bodajże"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает предположение или неуверенность говорящего; близко по значению к «наверное» или «пожалуй»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожалуй",
                                "target_lang_sense_clarification": "вводное слово, выражающее допущение или неуверенность"
                            },
                            {
                                "target_lang_word": "наверное",
                                "target_lang_sense_clarification": "вводное слово, выражающее вероятность"
                            },
                            {
                                "target_lang_word": "кажется",
                                "target_lang_sense_clarification": "выражает неуверенное предположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ba16496-957f-3b0a-a982-f4725059ffa0",
                    "sense_definition": "Słowo wskazujące na minimalny, wystarczający zakres czegoś lub ograniczające żądanie do jednej, wymienionej rzeczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zadzwoń do mnie <bodaj> raz w tygodniu, żebym wiedziała, że żyjesz.",
                            "target_lang_translations": {
                                "ru": "Позвони мне <хотя бы> раз в неделю, чтобы я знала, что ты жив."
                            }
                        },
                        {
                            "text": "Pożycz mi <bodaj> stówę do pierwszego, bo nie mam za co żyć.",
                            "target_lang_translations": {
                                "ru": "Одолжи мне <хоть> сотню до первого числа, а то мне не на что жить."
                            }
                        }
                    ],
                    "synonyms": [
                        "przynajmniej",
                        "chociaż",
                        "choć",
                        "byle"
                    ],
                    "common_phrases": [
                        "bodaj trochę",
                        "bodaj jeden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на минимальный, достаточный предел чего-либо или ограничивает просьбу; соответствует «по крайней мере»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотя бы",
                                "target_lang_sense_clarification": "частица, выражающая ограничение или минимальное требование"
                            },
                            {
                                "target_lang_word": "хоть",
                                "target_lang_sense_clarification": "разговорный вариант, выражающий уступку или минимальный предел"
                            },
                            {
                                "target_lang_word": "по крайней мере",
                                "target_lang_sense_clarification": "устойчивое выражение для указания минимума"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c8dbd42-8f30-38e8-ae72-b50a59c499d4",
                    "sense_definition": "Partykuła nadająca wypowiedzi charakter silnego życzenia, pragnienia lub złorzeczenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Bodaj> cię licho wzięło za takie zachowanie!",
                            "target_lang_translations": {
                                "ru": "<Чтоб> тебя черт побрал за такое поведение!"
                            }
                        },
                        {
                            "text": "<Bodaj> to się nigdy nie wydarzyło, bo przyniosło same nieszczęścia.",
                            "target_lang_translations": {
                                "ru": "<Хоть бы> это никогда не случилось, потому что это принесло одни несчастья."
                            }
                        }
                    ],
                    "synonyms": [
                        "oby",
                        "niech",
                        "niechaj",
                        "żeby",
                        "bodajby"
                    ],
                    "common_phrases": [
                        "bodaj cię",
                        "bodaj to"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "używane głównie w stylizowanych wypowiedziach lub przekleństwach"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, придающая высказыванию характер сильного желания, мольбы или проклятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтоб",
                                "target_lang_sense_clarification": "используется в эмоциональных пожеланиях или проклятиях"
                            },
                            {
                                "target_lang_word": "хоть бы",
                                "target_lang_sense_clarification": "выражает сильное желание или сожаление"
                            },
                            {
                                "target_lang_word": "пусть",
                                "target_lang_sense_clarification": "частица для выражения пожелания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46865de9-955e-3212-819c-e196ddd129a4",
                    "sense_definition": "Słowo służące do podania dowolnego przykładu, sugerując obojętność wyboru lub wskazując na jedną z wielu możliwości.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "example",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możemy pójść gdziekolwiek, <bodaj> do parku na krótki spacer.",
                            "target_lang_translations": {
                                "ru": "Мы можем пойти куда угодно, <хотя бы> в парк на короткую прогулку."
                            }
                        },
                        {
                            "text": "Zjedz cokolwiek, <bodaj> jabłko, żebyś nie był głodny.",
                            "target_lang_translations": {
                                "ru": "Съешь что-нибудь, <хоть> яблоко, чтобы не быть голодным."
                            }
                        }
                    ],
                    "synonyms": [
                        "choćby",
                        "chociażby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служит для приведения произвольного примера, указывая на безразличие выбора или одну из возможностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотя бы",
                                "target_lang_sense_clarification": "частица для выделения примера"
                            },
                            {
                                "target_lang_word": "хоть",
                                "target_lang_sense_clarification": "частица для указания на один из вариантов"
                            },
                            {
                                "target_lang_word": "например",
                                "target_lang_sense_clarification": "вводное слово для приведения примера"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
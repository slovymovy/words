            "pos": "noun",
            "senses": [
                {
                    "sense_id": "38a0f17e-21f8-3d24-999c-a4c22b033fb4",
                    "sense_definition": "Een samenstel van geïsoleerde draden, beschermd door een mantel, bedoeld om elektriciteit of signalen over te brengen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "electrical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kabel</w> van mijn oplader is gisteren kapotgegaan.",
                            "target_lang_translations": {
                                "en": "My charger <w>cable</w> broke yesterday.",
                                "ru": "<w>Кабель</w> моего зарядного устройства вчера сломался."
                            }
                        },
                        {
                            "text": "Je moet de <w>kabel</w> goed aansluiten om beeld te krijgen.",
                            "target_lang_translations": {
                                "en": "You must connect the <w>cable</w> properly to get a picture.",
                                "ru": "Ты должен правильно подключить <w>кабель</w>, чтобы появилось изображение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een elektrische kabel",
                        "de kabel aansluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "An assembly of insulated wires, protected by a casing, intended to transmit electricity or signals.",
                        "ru": "Изолированный провод или пучок проводов в защитной оболочке, предназначенный для передачи электричества или сигналов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cable",
                                "target_lang_sense_clarification": "insulated wire assembly"
                            },
                            {
                                "target_lang_word": "cord",
                                "target_lang_sense_clarification": "flexible power line"
                            },
                            {
                                "target_lang_word": "wire",
                                "target_lang_sense_clarification": "conductor"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кабель",
                                "target_lang_sense_clarification": "электрический или информационный"
                            },
                            {
                                "target_lang_word": "провод",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "906b29f0-4158-3ff2-80a0-f137b20fa177",
                    "sense_definition": "Het systeem voor de ontvangst van televisie, radio en internet via een vast netwerk van kabels.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "service_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij kijken thuis televisie via de <w>kabel</w>.",
                            "target_lang_translations": {
                                "en": "We watch television via <w>cable</w> at home.",
                                "ru": "Мы смотрим телевизор дома через <w>кабельное</w>."
                            }
                        },
                        {
                            "text": "Door een storing in de <w>kabel</w> hadden we de hele avond geen internet.",
                            "target_lang_translations": {
                                "en": "Due to a fault in the <w>cable</w>, we had no internet all evening.",
                                "ru": "Из-за сбоя в <w>кабельной сети</w> у нас весь вечер не было интернета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "via de kabel",
                        "op de kabel"
                    ],
                    "target_lang_definitions": {
                        "en": "A system for receiving television, radio, and internet via a fixed network of cables.",
                        "ru": "Система приема телевидения, радио и интернета через стационарную сеть кабелей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cable",
                                "target_lang_sense_clarification": "service system"
                            },
                            {
                                "target_lang_word": "cable TV",
                                "target_lang_sense_clarification": "television service"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кабельное телевидение",
                                "target_lang_sense_clarification": "система вещания"
                            },
                            {
                                "target_lang_word": "кабельное",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc48908b-f7e2-38b1-9644-b5d27417840d",
                    "sense_definition": "Een dik en sterk touw, vaak gemaakt van staaldraad of hennep, dat wordt gebruikt voor zware lasten of in de scheepvaart.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "heavy_rope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sleepboot trok het grote schip voort met een stalen <w>kabel</w>.",
                            "target_lang_translations": {
                                "en": "The tugboat pulled the large ship along with a steel <w>cable</w>.",
                                "ru": "Буксир тянул большой корабль с помощью стального <w>троса</w>."
                            }
                        },
                        {
                            "text": "Hij controleerde of de <w>kabel</w> van de lift nog stevig genoeg was.",
                            "target_lang_translations": {
                                "en": "He checked if the elevator <w>cable</w> was still strong enough.",
                                "ru": "Он проверил, достаточно ли крепок <w>трос</w> лифта."
                            }
                        }
                    ],
                    "synonyms": [
                        "tros"
                    ],
                    "common_phrases": [
                        "een stalen kabel",
                        "de kabel vieren"
                    ],
                    "target_lang_definitions": {
                        "en": "A thick and strong rope, often made of steel wire or hemp, used for heavy loads or in shipping.",
                        "ru": "Толстый и прочный канат, часто изготовленный из стальной проволоки или пеньки, используемый для тяжелых грузов или в судоходстве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cable",
                                "target_lang_sense_clarification": "strong rope"
                            },
                            {
                                "target_lang_word": "hawser",
                                "target_lang_sense_clarification": "nautical rope"
                            },
                            {
                                "target_lang_word": "wire",
                                "target_lang_sense_clarification": "metal rope"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "трос",
                                "target_lang_sense_clarification": "стальной или для больших нагрузок"
                            },
                            {
                                "target_lang_word": "канат",

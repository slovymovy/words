    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6929241a-caad-3be7-8cf1-130c8261d542",
                    "sense_definition": "Знак препинания, который ставится в конце повествовательного предложения или при сокращении слов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mark_symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В конце этого предложения стоит <w>точка</w>.",
                            "target_lang_translations": {
                                "en": "There is a <w>period</w> at the end of this sentence."
                            }
                        },
                        {
                            "text": "Ученик забыл поставить <w>точку</w> и получил замечание.",
                            "target_lang_translations": {
                                "en": "The student forgot to put a <w>full stop</w> and received a reprimand."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поставить точку",
                        "точка с запятой",
                        "двоеточие"
                    ],
                    "target_lang_definitions": {
                        "en": "A punctuation mark used at the end of a sentence or to mark an abbreviation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "period",
                                "target_lang_sense_clarification": "punctuation mark (US English)"
                            },
                            {
                                "target_lang_word": "full stop",
                                "target_lang_sense_clarification": "punctuation mark (UK English)"
                            },
                            {
                                "target_lang_word": "dot",
                                "target_lang_sense_clarification": "in email addresses or URLs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f033610a-efa6-33d5-8eb9-fc52d23407c5",
                    "sense_definition": "Маленькое круглое пятно, крапинка или след от прикосновения чем-то острым.",

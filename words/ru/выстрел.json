    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a15573b2-8bb1-3e58-87d9-3ac20875121f",
                    "sense_definition": "Действие по глаголу «стрелять»; выбрасывание пули или снаряда из огнестрельного оружия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотник прицелился и сделал один меткий <w>выстрел</w>.",
                            "target_lang_translations": {
                                "en": "The hunter took aim and fired one accurate <w>shot</w>."
                            }
                        },
                        {
                            "text": "Каждый <w>выстрел</w> в тире приближал его к победе в соревновании.",
                            "target_lang_translations": {
                                "en": "Every <w>shot</w> at the shooting range brought him closer to victory in the competition."
                            }
                        }
                    ],
                    "common_phrases": [
                        "произвести выстрел",
                        "меткий выстрел",
                        "случайный выстрел",
                        "предупредительный выстрел"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of firing a gun or discharging a projectile from a weapon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shot",
                                "target_lang_sense_clarification": "act of firing"
                            },
                            {
                                "target_lang_word": "gunshot",
                                "target_lang_sense_clarification": "discharge of a firearm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db0778b4-1164-338f-931f-8c79513859fa",
                    "sense_definition": "Резкий и громкий звук, возникающий при стрельбе из оружия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ночной тишине неожиданно раздался громкий <w>выстрел</w>.",
                            "target_lang_translations": {
                                "en": "A loud <w>shot</w> suddenly rang out in the silence of the night."
                            }
                        },
                        {
                            "text": "Мы услышали <w>выстрел</w> и сразу побежали в укрытие.",
                            "target_lang_translations": {
                                "en": "We heard a <w>shot</w> and immediately ran for cover."
                            }
                        }
                    ],
                    "synonyms": [
                        "хлопок"
                    ],
                    "common_phrases": [
                        "звук выстрела",
                        "услышать выстрел",
                        "одиночный выстрел"
                    ],
                    "target_lang_definitions": {
                        "en": "A sharp, loud noise produced when a weapon is fired."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shot",
                                "target_lang_sense_clarification": "sound of firing"
                            },
                            {
                                "target_lang_word": "report",
                                "target_lang_sense_clarification": "loud noise of an explosion or gun"
                            },
                            {
                                "target_lang_word": "gunshot",
                                "target_lang_sense_clarification": "sound of a gun firing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "304dafbe-2e57-348e-871c-2b1f8295b281",
                    "sense_definition": "Расстояние, которое может пролететь пуля или снаряд; дальность стрельбы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осторожный зверь не подпускал охотников на верный <w>выстрел</w>.",
                            "target_lang_translations": {
                                "en": "The cautious beast did not let the hunters within sure <w>range</w>."
                            }
                        },
                        {
                            "text": "К этому старому замку никто не подходил даже на пушечный <w>выстрел</w>.",
                            "target_lang_translations": {
                                "en": "No one approached this old castle even within cannon <w>shot</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "дальность"
                    ],
                    "common_phrases": [
                        "на пушечный выстрел",
                        "подойти на выстрел"
                    ],
                    "target_lang_definitions": {
                        "en": "The distance a projectile can travel; the effective range of a weapon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "range",
                                "target_lang_sense_clarification": "distance of fire"
                            },
                            {
                                "target_lang_word": "shot",
                                "target_lang_sense_clarification": "distance (in phrases like 'within shot')"
                            },
                            {
                                "target_lang_word": "gunshot",
                                "target_lang_sense_clarification": "range of a gun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e995f10-b792-351d-a5c5-8793424c604a",
                    "sense_definition": "Воен. Комплект боеприпасов (снаряд, заряд, гильза) для стрельбы из артиллерийских орудий.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты аккуратно укладывали артиллерийские <w>выстрелы</w> в ящики.",
                            "target_lang_translations": {
                                "en": "The soldiers carefully packed the artillery <w>rounds</w> into crates."
                            }
                        },
                        {
                            "text": "Для этого танка требуются специальные <w>выстрелы</w> повышенной мощности.",
                            "target_lang_translations": {
                                "en": "This tank requires special high-power <w>rounds</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "боеприпас",
                        "снаряд"
                    ],
                    "common_phrases": [
                        "артиллерийский выстрел",
                        "комплект выстрелов"
                    ],
                    "target_lang_definitions": {
                        "en": "A complete unit of ammunition (projectile, charge, and casing) for artillery."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "round",
                                "target_lang_sense_clarification": "unit of ammunition"
                            },
                            {
                                "target_lang_word": "shell",
                                "target_lang_sense_clarification": "explosive projectile"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7555c2d-378e-3296-83a4-75859aea7f69",
                    "sense_definition": "Морск. Длинная балка на судне, выступающая за борт, для крепления шлюпок или парусов.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Матросы получили приказ закрепить шлюпку под <w>выстрел</w>.",
                            "target_lang_translations": {
                                "en": "The sailors received an order to secure the boat under the <w>boom</w>."
                            }
                        },
                        {
                            "text": "Во время шторма <w>выстрел</w> был поврежден ударом волны.",
                            "target_lang_translations": {
                                "en": "During the storm, the <w>boom</w> was damaged by the impact of a wave."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A long spar or pole projecting from a ship's side, used to secure boats or sails."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boom",
                                "target_lang_sense_clarification": "nautical spar"
                            },
                            {
                                "target_lang_word": "spar",
                                "target_lang_sense_clarification": "pole on a ship"
                            }
                        ]
                    }
                }
            ]
        }

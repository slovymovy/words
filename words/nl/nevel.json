            "senses": [
                {
                    "sense_id": "c4b9ddff-d8f6-39e3-8cd0-c1c1fab0bbe6",
                    "sense_definition": "Wolken dicht bij de grond die het zicht beperken, maar minder dicht dan mist.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather_fog",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de dichte <w>nevel</w> konden we de overkant van de straat nauwelijks zien.",
                            "target_lang_translations": {
                                "ru": "Из-за густого <w>тумана</w> мы с трудом видели противоположную сторону улицы.",
                                "en": "Due to the dense <w>mist</w>, we could hardly see the other side of the street."
                            }
                        },
                        {
                            "text": "In de vroege ochtend hing er een lichte <w>nevel</w> over de weilanden.",
                            "target_lang_translations": {
                                "ru": "Ранним утром над лугами висела легкая <w>дымка</w>.",
                                "en": "In the early morning, a light <w>mist</w> hung over the meadows."
                            }
                        }
                    ],
                    "synonyms": [
                        "mist"
                    ],
                    "antonyms": [
                        "zicht"
                    ],
                    "common_phrases": [
                        "dichte nevel",
                        "in nevelen gehuld",
                        "optrekkende nevel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Облака у поверхности земли, ограничивающие видимость, но менее густые, чем туман.",
                        "en": "Clouds close to the ground that limit visibility, but are less dense than fog."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2ae518a1-ca1a-344e-af82-9be4f61a1abb",
                    "sense_definition": "Вероятно, по всей видимости; используется для выражения предположения или неуверенности.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы, <w>наверное</w>, поедем на море этим летом, если у нас будет отпуск.",
                            "target_lang_translations": {
                                "en": "We will <w>probably</w> go to the sea this summer if we have a vacation."
                            }
                        },
                        {
                            "text": "Ты, <w>наверное</w>, очень устал после долгого рабочего дня.",
                            "target_lang_translations": {
                                "en": "You are <w>probably</w> very tired after a long workday."
                            }
                        }
                    ],
                    "synonyms": [
                        "вероятно",
                        "пожалуй",
                        "очевидно",
                        "наверно"
                    ],
                    "antonyms": [
                        "наверняка",
                        "обязательно"
                    ],
                    "common_phrases": [
                        "ты наверное знаешь",
                        "это наверное правда",
                        "он наверное придёт"
                    ],
                    "target_lang_definitions": {
                        "en": "Likely, apparently; used to express a supposition or uncertainty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "probably",
                                "target_lang_sense_clarification": "used to say that something is likely to happen or to be true"
                            },
                            {
                                "target_lang_word": "most likely",
                                "target_lang_sense_clarification": "indicating a high probability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9439ae42-a608-3f40-9ef1-c53f615d0fb5",
                    "sense_definition": "Несомненно, точно, с полной уверенностью.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу узнать это <w>наверное</w>, чтобы больше не сомневаться в фактах.",
                            "target_lang_translations": {
                                "en": "I want to know this <w>for sure</w> so as not to doubt the facts anymore."
                            }
                        },
                        {
                            "text": "Теперь я <w>наверное</w> знаю, что он сказал мне чистую правду.",
                            "target_lang_translations": {
                                "en": "Now I know <w>for certain</w> that he told me the pure truth."
                            }
                        }
                    ],
                    "synonyms": [
                        "несомненно",
                        "наверняка",
                        "безусловно",
                        "конечно",
                        "естественно",
                        "непременно"
                    ],
                    "antonyms": [
                        "вероятно"
                    ],
                    "common_phrases": [
                        "знать наверное"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "В современном языке в этом значении используется слово «наверняка»."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Undoubtedly, accurately, with full certainty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for sure",
                                "target_lang_sense_clarification": "without any doubt"
                            },
                            {
                                "target_lang_word": "for certain",
                                "target_lang_sense_clarification": "established as a fact; definitely"
                            },
                            {
                                "target_lang_word": "certainly",
                                "target_lang_sense_clarification": "in a manner that is certain"
                            }
                        ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5cb363f0-341b-3812-91ce-4367d40c025b",
                    "sense_definition": "A person who has overall creative authority and management responsibility for a television program or film.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The studio hired a famous <w>helmer</w> to direct the upcoming blockbuster movie.",
                            "target_lang_translations": {
                                "ru": "Студия наняла известного <w>режиссёра</w> для съёмок предстоящего блокбастера."
                            }
                        },
                        {
                            "text": "As the series <w>helmer</w>, she made the final decision on all casting choices.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>постановщиком</w> сериала, она принимала окончательные решения по всем вопросам кастинга."
                            }
                        }
                    ],
                    "synonyms": [
                        "showrunner"
                    ],
                    "common_phrases": [
                        "veteran helmer",
                        "film helmer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, несущий общую творческую и управленческую ответственность за телепрограмму или фильм; часто используется как синоним слова «режиссёр» в индустрии развлечений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "режиссёр",
                                "target_lang_sense_clarification": "творческий руководитель фильма или спектакля"
                            },
                            {
                                "target_lang_word": "постановщик",
                                "target_lang_sense_clarification": "лицо, осуществляющее постановку фильма или шоу"
                            }
                        ]
                    }
                }
            ]
        },
        {

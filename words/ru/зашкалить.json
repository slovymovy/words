            "pos": "verb",
            "senses": [
                {
                    "sense_id": "67018aab-6bd8-3bb8-b7d2-489229533e47",
                    "sense_definition": "Превзойти допустимый уровень, стать чрезмерно большим или сильным (об эмоциях, ценах, температуре и т. п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его радость <w>зашкалила</w>, когда он узнал о победе своей команды.",
                            "target_lang_translations": {
                                "en": "His joy <w>went off the charts</w> when he found out about his team's victory."
                            }
                        },
                        {
                            "text": "В этом месяце цены на бензин снова <w>зашкалили</w>.",
                            "target_lang_translations": {
                                "en": "This month, gas prices <w>went through the roof</w> again."
                            }
                        }
                    ],
                    "common_phrases": [
                        "эмоции зашкалили",
                        "цены зашкалили",
                        "адреналин зашкалил"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To exceed a permissible level, become excessively high or intense (about emotions, prices, temperature, etc.)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go off the charts",
                                "target_lang_sense_clarification": "idiomatic, regarding statistics, popularity, or intensity"
                            },
                            {
                                "target_lang_word": "go through the roof",
                                "target_lang_sense_clarification": "idiomatic, often regarding prices or temper"
                            },
                            {
                                "target_lang_word": "run high",
                                "target_lang_sense_clarification": "regarding feelings or emotions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b7ad250-ab9e-31ba-9711-4858a7620d5a",
                    "sense_definition": "Выйти за пределы шкалы измерительного прибора (о стрелке или показаниях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стрелка спидометра <w>зашкалила</w> на крутом повороте.",
                            "target_lang_translations": {
                                "en": "The speedometer needle <w>went off the scale</w> on a sharp turn."
                            }
                        },
                        {
                            "text": "Дозиметр <w>зашкалил</w>, показывая опасный уровень радиации.",
                            "target_lang_translations": {
                                "en": "The dosimeter <w>went off the scale</w>, showing a dangerous level of radiation."
                            }
                        }
                    ],
                    "antonyms": [

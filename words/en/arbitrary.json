    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "17f87867-84c2-3aae-9e93-4bc1ed17e007",
                    "sense_definition": "Based on random choice or personal whim, rather than any reason or system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "randomness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The selection of the winner seemed completely <w>arbitrary</w> to the audience.",
                            "target_lang_translations": {
                                "ru": "Выбор победителя показался зрителям совершенно <w>произвольным</w>."
                            }
                        },
                        {
                            "text": "He made an <w>arbitrary</w> decision to leave work early without telling anyone.",
                            "target_lang_translations": {
                                "ru": "Он принял <w>произвольное</w> решение уйти с работы пораньше, никому не сказав."
                            }
                        }
                    ],
                    "synonyms": [
                        "random",
                        "capricious",
                        "whimsical",
                        "discretionary",
                        "erratic"
                    ],
                    "antonyms": [
                        "rational",
                        "reasoned",
                        "systematic",
                        "logical",
                        "objective"
                    ],
                    "common_phrases": [
                        "arbitrary decision",
                        "arbitrary rule",
                        "arbitrary choice",
                        "arbitrary nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на случайном выборе или личной прихоти, а не на какой-либо причине или системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произвольный",
                                "target_lang_sense_clarification": "основанный на личной прихоти или случайности"
                            },
                            {
                                "target_lang_word": "необоснованный",
                                "target_lang_sense_clarification": "не имеющий веских причин"
                            },
                            {
                                "target_lang_word": "случайный",
                                "target_lang_sense_clarification": "выбранный наугад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ef8f901-19bd-3cd1-a9b0-9af13285ecdc",
                    "sense_definition": "Relating to the unrestrained and often harsh use of authority or power.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens lived in fear of the dictator's <w>arbitrary</w> power.",
                            "target_lang_translations": {
                                "ru": "Граждане жили в страхе перед <w>произвольной</w> властью диктатора."
                            }
                        },
                        {
                            "text": "International law protects people against <w>arbitrary</w> arrest and detention.",
                            "target_lang_translations": {
                                "ru": "Международное право защищает людей от <w>произвольного</w> ареста и задержания."
                            }
                        }
                    ],
                    "synonyms": [
                        "despotic",
                        "tyrannical",
                        "autocratic",
                        "dictatorial"
                    ],
                    "antonyms": [
                        "democratic",
                        "accountable",
                        "lawful"
                    ],
                    "common_phrases": [
                        "arbitrary power",
                        "arbitrary arrest",
                        "arbitrary detention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с неограниченным и часто жестким использованием власти или силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произвольный",
                                "target_lang_sense_clarification": "осуществляемый по произволу, беззаконный"
                            },
                            {
                                "target_lang_word": "самоуправный",
                                "target_lang_sense_clarification": "действующий по своему усмотрению вопреки закону"
                            },
                            {
                                "target_lang_word": "деспотичный",
                                "target_lang_sense_clarification": "склонный к подавлению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "785f8aa8-3a9b-33c3-b0d6-d6e1d26fa311",
                    "sense_definition": "Chosen to represent any possible value or case, especially in mathematics or logic.",

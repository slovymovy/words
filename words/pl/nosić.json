                },
                {
                    "sense_id": "50fb61c4-2251-34cd-8521-8f2ce97e0e1c",
                    "sense_definition": "Posiadać określone imię, nazwisko lub tytuł jako wyróżnik",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "identity",
                    "name_type": "no",
                    "examples": [
                        {

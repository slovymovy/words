{
    "word_family": [
        "lace",
        "shoelace",
        "necklace",
        "unlace",
        "lacework",
        "interlace"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "28efd098-8f4b-3188-b38d-81fa8da716f5",
                    "sense_definition": "The present participle and gerund of <w>lace</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is currently <lacing> her boots up for the hike.",
                            "target_lang_translations": {
                                "ru": "Она сейчас <зашнуровывает> свои ботинки для похода."
                            }
                        },
                        {
                            "text": "He spent the morning <lacing> the new cords through the eyelets.",
                            "target_lang_translations": {
                                "ru": "Он провел утро, <продевая> новые шнуры через отверстия."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола lace (шнуровать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шнурование",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "зашнуровывание",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46872051-a88d-3301-bd10-84ffa9845928",
                    "sense_definition": "The arrangement of cords or strings used to fasten a shoe, garment, or other item, often forming a netlike pattern.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fastening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tight <lacing> on the corset made it difficult for her to breathe.",
                            "target_lang_translations": {
                                "ru": "Тугая <шнуровка> на корсете мешала ей дышать."
                            }
                        },
                        {
                            "text": "He adjusted the <lacing> on his sneakers before the race began.",
                            "target_lang_translations": {
                                "ru": "Он поправил <шнуровку> на своих кроссовках перед началом забега."
                            }
                        }
                    ],
                    "synonyms": [
                        "laces",
                        "binding",
                        "cords"
                    ],
                    "common_phrases": [
                        "tight lacing",
                        "shoe lacing",
                        "intricate lacing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система шнуров или веревок, используемая для застегивания обуви, одежды или других предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шнуровка",
                                "target_lang_sense_clarification": "способ завязывания или сами шнурки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01df501d-849e-30e7-b0de-d6ca32b65a77",
                    "sense_definition": "A small amount of a strong substance, such as alcohol, added to food or drink.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mixture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef added a generous <lacing> of brandy to the dessert sauce.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар добавил щедрую <добавку> бренди в десертный соус."
                            }
                        },
                        {
                            "text": "There was a subtle <lacing> of spice in the hot chocolate.",
                            "target_lang_translations": {
                                "ru": "В горячем шоколаде чувствовалась тонкая <примесь> специй."
                            }
                        }
                    ],
                    "synonyms": [
                        "dash",
                        "splash",
                        "touch"
                    ],
                    "common_phrases": [
                        "lacing of rum",
                        "lacing of brandy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое количество крепкого вещества (например, алкоголя), добавленное в еду или напиток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примесь",
                                "target_lang_sense_clarification": "добавление чего-либо"
                            },
                            {
                                "target_lang_word": "добавка",
                                "target_lang_sense_clarification": "включение в состав"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4c41bc6-b1fc-3691-9a39-249d9585ba66",
                    "sense_definition": "A severe physical beating given as a punishment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "punishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boy was afraid he would get a <lacing> if he came home late.",
                            "target_lang_translations": {
                                "ru": "Мальчик боялся, что ему зададут <порку>, если он придет домой поздно."
                            }
                        },
                        {
                            "text": "He received a sound <lacing> for breaking the neighbor's window.",
                            "target_lang_translations": {
                                "ru": "Он получил хорошую <взбучку> за то, что разбил соседское окно."
                            }
                        }
                    ],
                    "synonyms": [
                        "beating",
                        "thrashing",
                        "hiding"
                    ],
                    "common_phrases": [
                        "give someone a lacing",
                        "get a lacing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суровое физическое наказание, порка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порка",
                                "target_lang_sense_clarification": "телесное наказание"
                            },
                            {
                                "target_lang_word": "взбучка",
                                "target_lang_sense_clarification": "нагоняй (разговорное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2506b5d8-8e44-3f9b-a32e-700441986ae4",
                    "sense_definition": "Będący na miejscu oznaczonym liczbą 7 w uporządkowanym szeregu; następujący po szóstym.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jerzy był <w>siódmy</w> na mecie podczas wczorajszego wyścigu.",
                            "target_lang_translations": {
                                "ru": "Ежи пришел <w>седьмым</w> к финишу во вчерашней гонке."
                            }
                        },
                        {
                            "text": "To już <w>siódme</w> pytanie, na które nie znasz odpowiedzi.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>седьмой</w> вопрос, на который ты не знаешь ответа."
                            }
                        }
                    ],
                    "synonyms": [
                        "7."
                    ],
                    "common_phrases": [
                        "siódme niebo",
                        "siódme poty",
                        "siódma woda po kisielu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на месте, обозначенном числом 7; следующий за шестым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "седьмой",
                                "target_lang_sense_clarification": "порядковое числительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eed07e7e-3328-355b-9760-643c26f68e73",
                    "sense_definition": "Godzina siódma rano lub wieczorem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_hour",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jest już <w>siódma</w>, więc musimy wstawać do pracy.",
                            "target_lang_translations": {
                                "ru": "Уже <w>семь</w> часов, поэтому нам нужно вставать на работу."
                            }
                        },
                        {
                            "text": "Umówiliśmy się na kolację o <w>siódmej</w> wieczorem.",
                            "target_lang_translations": {
                                "ru": "Мы договорились об ужине в <w>семь</w> вечера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wpół do siódmej",
                        "za piętnaście siódma"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane w formie żeńskiej (siódma) w domyśle 'godzina'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Семь часов утра или вечера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семь (часов)",
                                "target_lang_sense_clarification": "время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61ccd190-90cd-35db-a9cd-9fab583dc38d",
                    "sense_definition": "Siódmy dzień danego miesiąca.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_date",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypłatę otrzymujemy zazwyczaj <w>siódmego</w> każdego miesiąca.",
                            "target_lang_translations": {
                                "ru": "Зарплату мы обычно получаем <w>седьмого</w> числа каждого месяца."
                            }
                        },
                        {
                            "text": "<w>Siódmego</w> maja wyjeżdżamy na długie wakacje.",
                            "target_lang_translations": {
                                "ru": "<w>Седьмого</w> мая мы уезжаем в долгий отпуск."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do siódmego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Седьмой день данного месяца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "седьмое (число)",
                                "target_lang_sense_clarification": "дата"
                            }
                        ]
                    }
                }
            ]
        }
    ]

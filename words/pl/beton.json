    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0e98897-d2c3-39bd-aad2-b187d231aff6",
                    "sense_definition": "Twardy materiał budowlany powstający z połączenia cementu, kruszywa (np. piasku, żwiru) i wody, wykorzystywany powszechnie w konstrukcjach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy wylali <beton> pod fundamenty nowego domu.",
                            "target_lang_translations": {
                                "ru": "Рабочие залили <w>бетон</w> под фундамент нового дома."
                            }
                        },
                        {
                            "text": "Ten nowoczesny budynek jest wykonany głównie ze szkła i <betonu>.",
                            "target_lang_translations": {
                                "ru": "Это современное здание построено в основном из стекла и <w>бетона</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wylewać beton",
                        "beton zbrojony",
                        "beton komórkowy",
                        "warstwa betonu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый строительный материал, получаемый из смеси цемента, заполнителей (например, песка, гравия) и воды, широко используемый в строительстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бетон",
                                "target_lang_sense_clarification": "строительный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9835567c-9f5e-34ed-a89f-bc7af477f03e",
                    "sense_definition": "Potocznie o grupie ludzi lub osobie o skrajnie konserwatywnych poglądach, niechętnych jakimkolwiek zmianom i reformom.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Partyjny <beton> skutecznie blokuje wszelkie próby modernizacji urzędu.",
                            "target_lang_translations": {
                                "ru": "Партийные <w>твердолобые</w> эффективно блокируют любые попытки модернизации ведомства."
                            }
                        },
                        {
                            "text": "Trudno dyskutować z kimś, kto jest takim <betonem> i nie przyjmuje żadnych argumentów.",
                            "target_lang_translations": {
                                "ru": "Трудно спорить с тем, кто настолько <w>твердолоб</w> и не принимает никаких аргументов."
                            }
                        }
                    ],
                    "synonyms": [
                        "konserwa",
                        "twardogłowi",
                        "wstecznicy"
                    ],
                    "common_phrases": [
                        "partyjny beton",
                        "kruszyć beton"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное обозначение группы людей или человека с крайне консервативными взглядами, противящихся любым изменениям и реформам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твердолобые",
                                "target_lang_sense_clarification": "о людях (разг.)"
                            },
                            {
                                "target_lang_word": "консерваторы",
                                "target_lang_sense_clarification": "политическое"
                            },
                            {
                                "target_lang_word": "ретрограды",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2fb6aba-d817-32c9-98e5-8c0004499d6e",
                    "sense_definition": "Obraźliwie o osobie ograniczonej umysłowo, która wolno myśli lub trudno przyswaja wiedzę.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie tłumacz mu tego po raz setny, to straszny <beton>.",
                            "target_lang_translations": {
                                "ru": "Не объясняй ему это в сотый раз, он страшный <w>тупица</w>."
                            }
                        },
                        {
                            "text": "Zachowujesz się jak <beton>, nic do ciebie nie dociera.",
                            "target_lang_translations": {
                                "ru": "Ты ведешь себя как <w>дуб</w>, до тебя ничего не доходит."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оскорбительное название умственно ограниченного человека, который медленно соображает или с трудом усваивает знания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупица",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "дуб",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "дерево",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "752987b4-f61a-3730-93b8-acfb78ced99f",
                    "sense_definition": "Rodzaj drinka alkoholowego, którego głównymi składnikami są likier ziołowy Becherovka oraz tonik.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Barman zaserwował nam orzeźwiający <beton> z plasterkiem cytryny.",
                            "target_lang_translations": {
                                "ru": "Бармен подал нам освежающий <w>Бетон</w> с ломтиком лимона."
                            }
                        },
                        {
                            "text": "Na imprezie najchętniej piliśmy <beton>, bo jest prosty do zrobienia.",
                            "target_lang_translations": {
                                "ru": "На вечеринке мы охотнее всего пили <w>Бетон</w>, так как его просто готовить."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Алкогольный коктейль, основными ингредиентами которого являются травяной ликер «Бехеровка» и тоник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бетон",
                                "target_lang_sense_clarification": "коктейль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ae0d73c-1de4-3ebb-b11a-8cfa47ca3858",
                    "sense_definition": "W gwarze więziennej określenie na chleb.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "prison_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więzień poprosił strażnika o dodatkowy kawałek <betonu>.",
                            "target_lang_translations": {
                                "ru": "Заключенный попросил у охранника дополнительный кусок <w>хлеба</w>."
                            }
                        },
                        {
                            "text": "Ten czerstwy <beton> ledwo dało się ugryźć.",
                            "target_lang_translations": {
                                "ru": "Этот черствый <w>хлеб</w> едва можно было укусить."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "więzienny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На тюремном жаргоне — название хлеба."
                    },
                    "translations": {

                {
                    "sense_id": "2c2dfd2e-fb10-3c00-a470-fc7936cac836",
                    "sense_definition": "Een reden die men aanvoert om zich tegen iets te verzetten of om aan te geven dat men het er niet mee eens is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "objection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb geen enkel <w>bezwaar</w> tegen jouw voorstel.",
                            "target_lang_translations": {
                                "ru": "У меня нет никаких <w>возражений</w> против твоего предложения."
                            }
                        },
                        {
                            "text": "Je kunt schriftelijk <w>bezwaar</w> maken als je het niet eens bent met de boete.",
                            "target_lang_translations": {
                                "ru": "Вы можете подать письменное <w>возражение</w>, если не согласны со штрафом."
                            }
                        }
                    ],
                    "synonyms": [

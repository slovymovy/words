            "pos": "noun",
            "senses": [
                {
                    "sense_id": "42cf4a6e-242a-305e-8fac-1c5a586bed43",
                    "sense_definition": "De seksuele voorkeur van een persoon, oftewel tot welk geslacht men zich aangetrokken voelt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "identiteit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Nederland mag je niet gediscrimineerd worden vanwege je <w>geaardheid</w>.",
                            "target_lang_translations": {
                                "ru": "В Нидерландах запрещена дискриминация из-за <w>ориентации</w>."
                            }
                        },
                        {
                            "text": "Het duurde lang voordat hij open durfde te zijn over zijn seksuele <w>geaardheid</w>.",
                            "target_lang_translations": {
                                "ru": "Прошло много времени, прежде чем он осмелился открыто заговорить о своей сексуальной <w>ориентации</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "seksuele geaardheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сексуальная ориентация человека, то есть то, к какому полу он испытывает влечение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентация",
                                "target_lang_sense_clarification": "сексуальные предпочтения"
                            },
                            {
                                "target_lang_word": "сексуальная ориентация",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5a7df8b-f2f5-364c-a30f-eeebd00d90ff",
                    "sense_definition": "De manier waarop iemand van nature is gevormd, het innerlijke karakter.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "karakter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is van <w>geaardheid</w> een heel rustig en geduldig persoon.",
                            "target_lang_translations": {
                                "ru": "По своей <w>натуре</w> она очень спокойный и терпеливый человек."
                            }
                        },
                        {
                            "text": "Door zijn optimistische <w>geaardheid</w> ziet hij altijd de zonnige kant van het leven.",
                            "target_lang_translations": {
                                "ru": "Благодаря своему оптимистичному <w>складу характера</w> он всегда видит светлую сторону жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "karakter",
                        "aard",
                        "persoonlijkheid"
                    ],
                    "common_phrases": [
                        "van geaardheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренний характер, натура, то, каким человек является от природы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "натура",
                                "target_lang_sense_clarification": "природные свойства, характер"
                            },
                            {
                                "target_lang_word": "склад характера",
                                "target_lang_sense_clarification": "душевный склад"
                            },
                            {
                                "target_lang_word": "нрав",
                                "target_lang_sense_clarification": "темперамент"
                            }
                        ]
                    }
                }
            ]

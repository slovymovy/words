                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокий",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "рослый",
                                "target_lang_sense_clarification": "крупный, высокого роста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc0f225e-1d26-327b-9550-0bb324b33f63",
                    "sense_definition": "Having its top a long way up; having a great vertical extent (used for objects, plants, and buildings).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "height",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could see a <w>tall</w> building in the distance.",
                            "target_lang_translations": {
                                "ru": "Вдали мы видели <w>высокое</w> здание."
                            }
                        },
                        {
                            "text": "The <w>tall</w> trees provided plenty of shade in the park.",
                            "target_lang_translations": {
                                "ru": "<w>Высокие</w> деревья давали много тени в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "high",
                        "lofty"
                    ],
                    "antonyms": [
                        "short",
                        "low"
                    ],
                    "common_phrases": [
                        "tall building",
                        "tall tree",
                        "tall ship"
                    ],
                    "target_lang_definitions": {

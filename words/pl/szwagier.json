    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "381eab93-4e9f-302b-a4a1-fa8088bfb956",
                    "sense_definition": "Brat żony, brat męża lub mąż siostry.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "family_relatives",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <szwagier> pomógł mi wczoraj naprawić samochód.",
                            "target_lang_translations": {
                                "ru": "Мой <w>шурин</w> помог мне вчера починить машину."
                            }
                        },
                        {
                            "text": "W weekend jedziemy z wizytą do siostry i jej męża, czyli mojego <szwagra>.",
                            "target_lang_translations": {
                                "ru": "В выходные мы едем в гости к сестре и её мужу, то есть моему <w>зятю</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mój szwagier",
                        "przyszły szwagier",
                        "ulubiony szwagier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брат жены, брат мужа или муж сестры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шурин",
                                "target_lang_sense_clarification": "брат жены"
                            },
                            {
                                "target_lang_word": "деверь",
                                "target_lang_sense_clarification": "брат мужа"
                            },
                            {
                                "target_lang_word": "зять",
                                "target_lang_sense_clarification": "муж сестры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a417c2b-d964-3d50-9b4d-e4f79a19dcfa",
                    "sense_definition": "Mąż szwagierki (mąż siostry współmałżonka).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "family_relatives",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mąż siostry mojej żony to formalnie również mój <szwagier>.",
                            "target_lang_translations": {
                                "ru": "Муж сестры моей жены — это формально тоже мой <w>свояк</w>."
                            }
                        },
                        {
                            "text": "Na rodzinnym obiedzie spotkałem się ze <szwagrem>, który jest mężem siostry mojej żony.",
                            "target_lang_translations": {
                                "ru": "На семейном обеде я встретился со <w>свояком</w>, который является мужем сестры моей жены."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodzina szwagra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Муж свояченицы (муж сестры жены)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свояк",
                                "target_lang_sense_clarification": "муж сестры жены"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94db2174-be53-3093-bd2e-bda25356ca5d",
                    "sense_definition": "Mężczyzna, który utrzymuje lub utrzymywał kontakty seksualne z tą samą kobietą, co inny mężczyzna.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koledzy żartowali, że zostali <szwagrami>, ponieważ spotykali się z tą samą dziewczyną.",
                            "target_lang_translations": {
                                "ru": "Друзья шутили, что стали <w>молочными братьями</w>, так как встречались с одной и той же девушкой."
                            }
                        },
                        {
                            "text": "Nie wiedziałem o jej przeszłości i przypadkiem stałem się <szwagrem> mojego wroga.",
                            "target_lang_translations": {
                                "ru": "Я не знал о её прошлом и случайно стал <w>молочным братом</w> моего врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "współkochanek",
                        "suchy szwagier"
                    ],
                    "common_phrases": [
                        "zostać szwagrem"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "humorous or vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который имеет или имел сексуальные отношения с той же женщиной, что и другой мужчина."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]

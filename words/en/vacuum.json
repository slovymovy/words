    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4bc46acd-6e84-314a-afe7-23f0e0c6549b",
                    "sense_definition": "A machine that cleans floors and other surfaces by sucking up dust and dirt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appliance_cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please get the <w>vacuum</w> out of the closet so I can clean the rug.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, достань <w>пылесос</w> из шкафа, чтобы я мог почистить ковёр."
                            }
                        },
                        {
                            "text": "The noise of the <w>vacuum</w> always scares the cat away.",
                            "target_lang_translations": {
                                "ru": "Шум <w>пылесоса</w> всегда отпугивает кошку."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoover"
                    ],
                    "common_phrases": [
                        "run the vacuum",
                        "robot vacuum",
                        "vacuum bag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, которое чистит полы и другие поверхности, всасывая пыль и грязь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пылесос",
                                "target_lang_sense_clarification": "бытовой прибор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b1e9de1-d0fa-3f15-b90a-0b9326682ed6",
                    "sense_definition": "A space that is completely empty of all matter, including air.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physics_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sound waves cannot travel through a <w>vacuum</w> because there is no air.",
                            "target_lang_translations": {
                                "ru": "Звуковые волны не могут распространяться в <w>вакууме</w>, потому что там нет воздуха."
                            }
                        },
                        {
                            "text": "Astronauts wear special suits to survive the cold <w>vacuum</w> of space.",
                            "target_lang_translations": {
                                "ru": "Астронавты носят специальные костюмы, чтобы выжить в холодном <w>вакууме</w> космоса."
                            }
                        }
                    ],
                    "synonyms": [
                        "void"
                    ],
                    "antonyms": [
                        "plenum"
                    ],
                    "common_phrases": [
                        "in a vacuum",
                        "vacuum of space"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространство, полностью свободное от вещества, включая воздух."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вакуум",
                                "target_lang_sense_clarification": "физическое явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b85a1e0b-0584-3cae-b8a0-30f3ca656036",
                    "sense_definition": "A feeling of emptiness or a lack of something important, often caused by someone or something leaving.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_emptiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden resignation of the director left a power <w>vacuum</w> in the organization.",
                            "target_lang_translations": {
                                "ru": "Внезапная отставка директора оставила <w>вакуум</w> власти в организации."
                            }
                        },
                        {
                            "text": "Her departure created a painful <w>vacuum</w> in his daily life.",
                            "target_lang_translations": {
                                "ru": "Её отъезд создал болезненную <w>пустоту</w> в его повседневной жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "void"
                    ],
                    "common_phrases": [
                        "power vacuum",
                        "political vacuum",
                        "leave a vacuum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство пустоты или отсутствия чего-то важного, часто возникающее после чьего-то ухода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вакуум",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "пустота",
                                "target_lang_sense_clarification": "чувство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5853fad3-eaa4-316d-a388-f9ab0d453758",
                    "sense_definition": "A state of reduced air pressure in a sealed container.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physics_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist used a pump to create a <w>vacuum</w> inside the glass jar.",
                            "target_lang_translations": {
                                "ru": "Учёный использовал насос, чтобы создать <w>вакуум</w> внутри стеклянной банки."
                            }
                        },
                        {
                            "text": "These vegetables are sealed in a <w>vacuum</w> to keep them fresh for longer.",
                            "target_lang_translations": {
                                "ru": "Эти овощи упакованы в <w>вакуум</w>, чтобы сохранить их свежими дольше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vacuum pump",
                        "vacuum packed",
                        "vacuum seal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние пониженного давления воздуха в герметичном контейнере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вакуум",
                                "target_lang_sense_clarification": "техническое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea0c8fe3-1582-3688-8d7e-fc512ac1aee1",
                    "sense_definition": "A physical exercise where the stomach muscles are pulled in tightly towards the spine.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "exercise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He practiced the stomach <w>vacuum</w> every morning to slim his waist.",
                            "target_lang_translations": {
                                "ru": "Он делал <w>вакуум</w> живота каждое утро, чтобы уменьшить талию."
                            }
                        },
                        {
                            "text": "Bodybuilders often perform a <w>vacuum</w> pose to show off their physique.",
                            "target_lang_translations": {
                                "ru": "Бодибилдеры часто выполняют позу <w>вакуум</w>, чтобы продемонстрировать своё телосложение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stomach vacuum"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "bodybuilding"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое упражнение, при котором мышцы живота сильно втягиваются к позвоночнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вакуум",
                                "target_lang_sense_clarification": "упражнение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d232206-3a2b-33d0-be81-fac62c9cd8dc",
                    "sense_definition": "To clean a floor or other surface using a vacuum cleaner.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appliance_cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>vacuum</w> the living room before the guests arrive.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>пропылесосить</w> гостиную до прихода гостей."
                            }
                        },
                        {
                            "text": "She <w>vacuums</w> the carpets every Saturday morning to keep them clean.",
                            "target_lang_translations": {
                                "ru": "Она <w>пылесосит</w> ковры каждое субботнее утро, чтобы содержать их в чистоте."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoover"
                    ],
                    "common_phrases": [
                        "vacuum the floor",
                        "vacuum the carpet",
                        "vacuum up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чистить пол или другую поверхность с помощью пылесоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пылесосить",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "пропылесосить",
                                "target_lang_sense_clarification": "завершенное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4826056-11b7-3bf1-8a0a-a0f1d25512ca",
                    "sense_definition": "To optimize a database by removing unnecessary data to free up space.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system administrator wrote a script that automatically <w>vacuums</w> the database every night.",
                            "target_lang_translations": {
                                "ru": "Системный администратор написал скрипт, который автоматически <w>чистит</w> базу данных каждую ночь."
                            }
                        },
                        {
                            "text": "If you do not <w>vacuum</w> the table regularly, performance may degrade over time.",
                            "target_lang_translations": {
                                "ru": "Если вы не будете регулярно <w>чистить</w> таблицу, производительность может со временем снизиться."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оптимизировать базу данных путем удаления ненужных данных для освобождения места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистить",
                                "target_lang_sense_clarification": "базу данных"
                            },
                            {
                                "target_lang_word": "оптимизировать",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]

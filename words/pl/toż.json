{
    "word_family": [
        "tenże",
        "taż"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "64fa9c8b-21f1-3c3c-b829-6eafafcae078",
                    "sense_definition": "Partykuła wzmacniająca wypowiedź, nadająca jej ton polemiczny, wyrażająca zdziwienie, zniecierpliwienie lub wskazująca na oczywistość faktu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphatic_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Toż> to jest zupełnie niemożliwe!",
                            "target_lang_translations": {
                                "ru": "<w>Да ведь</w> это совершенно невозможно!"
                            }
                        },
                        {
                            "text": "Nie denerwuj się tak, <toż> tylko żartowałem.",
                            "target_lang_translations": {
                                "ru": "Не нервничай так, <w>ведь</w> я только шутил."
                            }
                        }
                    ],
                    "synonyms": [
                        "przecież"
                    ],
                    "common_phrases": [
                        "toż to",
                        "toż przecież"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, усиливающая высказывание, придающая ему полемический тон, выражающая удивление, нетерпение или указывающая на очевидность факта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведь",
                                "target_lang_sense_clarification": "указывает на очевидность или причину"
                            },
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "да ведь",
                                "target_lang_sense_clarification": "эмфатическое усиление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "1358fdb6-07fd-33c4-82a1-e5c313fd342e",
                    "sense_definition": "Forma zaimka wskazującego w rodzaju nijakim, oznaczająca „to samo”; często występuje w połączeniu ze słowem „samo”.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pronoun_neuter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powiedział dokładnie <toż> samo, co jego poprzednik.",
                            "target_lang_translations": {
                                "ru": "Он сказал в точности <w>то же</w> самое, что и его предшественник."
                            }
                        },
                        {
                            "text": "Czy to jest <toż> miejsce, w którym byliśmy wczoraj?",
                            "target_lang_translations": {
                                "ru": "Это <w>то же</w> место, где мы были вчера?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "toż samo"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма указательного местоимения среднего рода, означающая «то же самое»; часто встречается в сочетании со словом «samo»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то же",
                                "target_lang_sense_clarification": "местоимение"
                            },
                            {
                                "target_lang_word": "то же самое",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "b56fcbc2-ea57-3daa-96ba-95ad95ecd8a3",
                    "sense_definition": "Spójnik łączący zdania, oznaczający wynik lub konsekwencję; to samo co „więc” lub „zatem”.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "conjunction_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobiło się ciemno, <toż> musieliśmy zapalić świece.",
                            "target_lang_translations": {
                                "ru": "Стемнело, <w>поэтому</w> нам пришлось зажечь свечи."
                            }
                        },
                        {
                            "text": "Nie miał pieniędzy, <toż> nie mógł kupić chleba.",
                            "target_lang_translations": {
                                "ru": "У него не было денег, <w>так что</w> он не мог купить хлеба."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or regional"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий предложения, обозначающий результат или следствие; то же, что «więc» или «zatem»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поэтому",
                                "target_lang_sense_clarification": "следствие"
                            },
                            {
                                "target_lang_word": "так что",
                                "target_lang_sense_clarification": "результат"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "752a9baf-5b97-35be-9cac-3d0c20097f2a",
                    "sense_definition": "Słowo oznaczające podobieństwo działania; tak samo, również.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "adverb_similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król wstał z miejsca, <toż> uczynili wszyscy dworzanie.",
                            "target_lang_translations": {
                                "ru": "Король встал с места, <w>то же</w> сделали все придворные."
                            }
                        },
                        {
                            "text": "Ona się roześmiała, <toż> zrobiła jej siostra.",
                            "target_lang_translations": {
                                "ru": "Она рассмеялась, <w>так же</w> поступила её сестра."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, обозначающее сходство действия; так же, также."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то же",
                                "target_lang_sense_clarification": "о действии"
                            },
                            {
                                "target_lang_word": "так же",
                                "target_lang_sense_clarification": "о способе действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
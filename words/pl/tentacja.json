            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d912304f-7a29-3d52-ab3d-6052d7b4fbeb",
                    "sense_definition": "Przestarzałe określenie na silną pokusę, wabienie lub zachętę do zrobienia czegoś, co jest często zakazane lub niewłaściwe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młodzieniec czuł wielką <w>tentację</w>, by porzucić swoje codzienne obowiązki.",
                            "target_lang_translations": {
                                "ru": "Юноша чувствовал великое <w>искушение</w> бросить свои повседневные обязанности."
                            }
                        },
                        {
                            "text": "Nie mogła oprzeć się tej nagłej <w>tentacji</w> i sięgnęła po zakazany owoc.",
                            "target_lang_translations": {
                                "ru": "Она не могла противиться этому внезапному <w>соблазну</w> и потянулась за запретным плодом."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokusa",
                        "kuszenie"
                    ],
                    "common_phrases": [
                        "ulec tentacji",
                        "walczyć z tentacją"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo rzadko używane we współczesnej polszczyźnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее слово, обозначающее сильное искушение, соблазн или побуждение к совершению чего-либо, часто запретного или предосудительного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искушение",
                                "target_lang_sense_clarification": "сильное желание сделать что-то запретное"
                            },
                            {
                                "target_lang_word": "соблазн",
                                "target_lang_sense_clarification": "привлекательность чего-то запретного или недоступного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "226fb8f9-57e9-3bf6-9bff-c53503029bfd",
                    "sense_definition": "Przestarzałe określenie na trudną próbę, doświadczenie życiowe lub testowanie czyjejś wytrwałości i charakteru.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Los zesłał na niego kolejną trudną <w>tentację</w>, którą musiał przetrwać.",
                            "target_lang_translations": {
                                "ru": "Судьба послала ему очередное трудное <w>испытание</w>, которое он должен был пережить."
                            }
                        },
                        {
                            "text": "To wydarzenie było dla całej rodziny bolesną <w>tentacją</w> ich wiary.",
                            "target_lang_translations": {
                                "ru": "Это событие стало для всей семьи болезненным <w>испытанием</w> их веры."
                            }
                        }
                    ],
                    "synonyms": [
                        "próba",
                        "sprawdzian",
                        "doświadczenie"
                    ],
                    "common_phrases": [
                        "ciężka tentacja"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo rzadko używane we współczesnej polszczyźnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее слово, обозначающее трудное испытание, жизненный опыт или проверку чьей-либо стойкости и характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытание",
                                "target_lang_sense_clarification": "трудная ситуация, проверяющая качества человека"
                            },
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "тестирование на прочность или верность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
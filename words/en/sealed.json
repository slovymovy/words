    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2ca2af1-3ddd-3d9c-a5a3-74de73ede4fd",
                    "sense_definition": "Simple past and past participle of <w>seal</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>sealed</w> the envelope and put a stamp on it.",
                            "target_lang_translations": {
                                "ru": "Он <w>запечатал</w> конверт и наклеил на него марку."
                            }
                        },
                        {
                            "text": "The authorities have <w>sealed</w> the area for safety reasons.",
                            "target_lang_translations": {
                                "ru": "Власти <w>оцепили</w> район в целях безопасности."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола seal (запечатывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запечатал",
                                "target_lang_sense_clarification": "конверт, письмо"
                            },
                            {
                                "target_lang_word": "опечатал",
                                "target_lang_sense_clarification": "помещение, территорию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6c7bd9ee-2d81-3fb7-824b-ae3c7fd5f856",
                    "sense_definition": "Closed securely so that it cannot be opened without breaking the fastening.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She handed him a <w>sealed</w> letter containing the instructions.",
                            "target_lang_translations": {
                                "ru": "Она передала ему <w>запечатанное</w> письмо с инструкциями."
                            }
                        },
                        {
                            "text": "Store the cookies in a <w>sealed</w> container to keep them fresh.",
                            "target_lang_translations": {
                                "ru": "Храните печенье в <w>герметичном</w> контейнере, чтобы оно оставалось свежим."
                            }
                        }
                    ],
                    "antonyms": [
                        "open",
                        "unsealed"
                    ],
                    "common_phrases": [
                        "sealed envelope",
                        "sealed container",
                        "signed and sealed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надежно закрытый, так что его нельзя открыть, не нарушив целостность упаковки или крепления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запечатанный",
                                "target_lang_sense_clarification": "о письме или упаковке"
                            },
                            {
                                "target_lang_word": "опечатанный",
                                "target_lang_sense_clarification": "о двери или контейнере с пломбой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb953750-067d-34e7-91d7-b384a5e5859e",
                    "sense_definition": "Made tight against the leakage or passage of air, water, or other substances.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The samples were kept in a hermetically <w>sealed</w> jar.",
                            "target_lang_translations": {
                                "ru": "Образцы хранились в герметично <w>закрытой</w> банке."
                            }
                        },
                        {
                            "text": "Ensure the unit is <w>sealed</w> to prevent moisture from entering.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что блок <w>герметичен</w>, чтобы предотвратить попадание влаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "impermeable"
                    ],
                    "common_phrases": [
                        "hermetically sealed",
                        "vacuum sealed",
                        "sealed shut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непроницаемый для воздуха, воды или других веществ; герметичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герметичный",
                                "target_lang_sense_clarification": "непроницаемый"
                            },
                            {
                                "target_lang_word": "плотно закрытый",
                                "target_lang_sense_clarification": "общая фраза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96f24936-0fcb-33d6-97c4-490a715a819d",
                    "sense_definition": "Of a road: having a hard, paved surface like asphalt or concrete.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We drove for miles on a <w>sealed</w> road before hitting the dirt track.",
                            "target_lang_translations": {
                                "ru": "Мы проехали много миль по дороге <w>с твердым покрытием</w>, прежде чем выехали на грунтовку."
                            }
                        },
                        {
                            "text": "The <w>sealed</w> section of the highway ends in five kilometers.",
                            "target_lang_translations": {
                                "ru": "Участок шоссе <w>с твердым покрытием</w> заканчивается через пять километров."
                            }
                        }
                    ],
                    "antonyms": [
                        "unsealed"
                    ],
                    "common_phrases": [
                        "sealed road"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia, New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О дороге: имеющая твердое покрытие, например, асфальт или бетон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с твердым покрытием",
                                "target_lang_sense_clarification": "о дороге"
                            },
                            {
                                "target_lang_word": "асфальтированный",
                                "target_lang_sense_clarification": "покрытый асфальтом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7ca8097-f135-35b2-bf02-59869526f0d5",
                    "sense_definition": "Declared in programming code so that it cannot be subclassed or inherited from.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot extend a <w>sealed</w> class in this programming language.",
                            "target_lang_translations": {
                                "ru": "В этом языке программирования нельзя расширить <w>запечатанный</w> класс."
                            }
                        },
                        {
                            "text": "The developer marked the method as <w>sealed</w> to prevent further modification.",
                            "target_lang_translations": {
                                "ru": "Разработчик пометил метод как <w>запечатанный</w>, чтобы предотвратить дальнейшие изменения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sealed class"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объявленный в программном коде так, что от него нельзя наследовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запечатанный",
                                "target_lang_sense_clarification": "термин в C#"
                            },
                            {
                                "target_lang_word": "изолированный",
                                "target_lang_sense_clarification": "термин в Kotlin/Java"

{
    "word_family": [
        "domper",
        "domping",
        "onderdompelen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "454b61ed-fb36-394d-a22e-939f974bb247",
                    "sense_definition": "Een lichtbron of vuur doven, of het licht minder fel maken.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "extinguish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <dompte> de kaars voordat hij de kamer verliet.",
                            "target_lang_translations": {
                                "ru": "Он <погасил> свечу, прежде чем выйти из комнаты."
                            }
                        },
                        {
                            "text": "De dikke mist <dompte> het schijnsel van de straatlantaarns.",
                            "target_lang_translations": {
                                "ru": "Густой туман <приглушил> свет уличных фонарей."
                            }
                        }
                    ],
                    "synonyms": [
                        "doven",
                        "dempen",
                        "uitmaken"
                    ],
                    "antonyms": [
                        "aansteken",
                        "ontsteken"
                    ],
                    "common_phrases": [
                        "een kaars dompen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гасить источник света или огня, либо делать свет менее ярким."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гасить",
                                "target_lang_sense_clarification": "огонь или свечу"
                            },
                            {
                                "target_lang_word": "тушить",
                                "target_lang_sense_clarification": "прекращать горение"
                            },
                            {
                                "target_lang_word": "приглушать",
                                "target_lang_sense_clarification": "делать свет менее интенсивным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21a0a63c-7786-3656-a294-33aa59ff7b43",
                    "sense_definition": "Iets kortstondig in een vloeistof steken en er weer uithalen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "submerge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De smid <dompte> het hete ijzer in de bak met water.",
                            "target_lang_translations": {
                                "ru": "Кузнец <окунул> раскаленное железо в бак с водой."
                            }
                        },
                        {
                            "text": "Zij <dompten> hun brood in de soep tijdens de maaltijd.",
                            "target_lang_translations": {
                                "ru": "Они <макали> хлеб в суп во время еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopen",
                        "onderdompelen",
                        "dippen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "tegenwoordig is 'dopen' of 'onderdompelen' gebruikelijker"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ненадолго опускать что-либо в жидкость и вынимать обратно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окунать",
                                "target_lang_sense_clarification": "погружать в жидкость"
                            },
                            {
                                "target_lang_word": "макать",
                                "target_lang_sense_clarification": "погружать (обычно еду)"
                            },
                            {
                                "target_lang_word": "погружать",
                                "target_lang_sense_clarification": "опускать внутрь чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e9acd6d-e607-3694-a5aa-86c75707b524",
                    "sense_definition": "(van een schip) Met de boeg diep in de golven duiken door de deining van de zee.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schip <dompte> zwaar in de stormachtige zee.",
                            "target_lang_translations": {
                                "ru": "Корабль тяжело <зарывался носом> в штормовое море."
                            }
                        },
                        {
                            "text": "Door de hoge golven begon de boot hevig te <dompen> en te stampen.",
                            "target_lang_translations": {
                                "ru": "Из-за высоких волн лодка начала сильно <нырять> и испытывать качку."
                            }
                        }
                    ],
                    "synonyms": [
                        "stampen"
                    ],
                    "common_phrases": [
                        "stampen en dompen"
                    ],
                    "target_lang_definitions": {
                        "ru": "(о корабле) Глубоко погружаться носом в волны из-за морской качки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарываться носом",
                                "target_lang_sense_clarification": "о судне на волнах"
                            },
                            {
                                "target_lang_word": "нырять",
                                "target_lang_sense_clarification": "резко опускаться вниз"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "35a85202-52cc-3083-8f70-79c9ffbfef08",
                    "sense_definition": "Van hoge leeftijd; niet jong meer (vooral gebruikt voor mensen en dieren).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn opa is een erg <w>oude</w> man.",
                            "target_lang_translations": {
                                "en": "My grandpa is a very <w>old</w> man.",
                                "ru": "Мой дедушка — очень <w>старый</w> человек."
                            }
                        },
                        {
                            "text": "Zij is de <w>oudste</w> leerling van de klas.",
                            "target_lang_translations": {
                                "en": "She is the <w>oldest</w> pupil in the class.",
                                "ru": "Она — самая <w>старшая</w> ученица в классе."
                            }
                        }
                    ],
                    "synonyms": [
                        "bejaard"
                    ],
                    "antonyms": [
                        "jong"
                    ],
                    "common_phrases": [
                        "jong en oud",
                        "oude dag",
                        "van oudsher"
                    ],
                    "target_lang_definitions": {
                        "en": "Of advanced age; no longer young (especially used for people and animals).",
                        "ru": "В преклонном возрасте; больше не молодой (обычно о людях и животных)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "old",
                                "target_lang_sense_clarification": "having lived for a long time"
                            },
                            {
                                "target_lang_word": "elderly",
                                "target_lang_sense_clarification": "polite term for old people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "старый",
                                "target_lang_sense_clarification": "общий термин, противоположность молодому"
                            },
                            {
                                "target_lang_word": "пожилой",
                                "target_lang_sense_clarification": "вежливая форма, о людях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0a02efe-faf6-33bd-95ff-77435d58ec94",
                    "sense_definition": "Al lange tijd bestaand; uit een vroegere tijd afkomstig en niet nieuw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die stoel is al heel <w>oud</w>, hij was nog van mijn grootvader.",
                            "target_lang_translations": {
                                "en": "That chair is already very <w>old</w>; it used to belong to my grandfather.",
                                "ru": "Этот стул уже очень <w>старый</w>, он был ещё у моего дедушки."
                            }
                        },
                        {
                            "text": "We wonen in een mooi <w>oud</w> huis in het centrum.",
                            "target_lang_translations": {
                                "en": "We live in a beautiful <w>old</w> house in the center.",
                                "ru": "Мы живём в красивом <w>старом</w> доме в центре."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieuw",
                        "modern"
                    ],
                    "common_phrases": [
                        "oud en versleten",
                        "oude stad",
                        "hoe oud"
                    ],
                    "target_lang_definitions": {
                        "en": "Existing for a long time; coming from an earlier time and not new.",
                        "ru": "Существующий долгое время; не новый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "old",
                                "target_lang_sense_clarification": "not new"
                            },
                            {
                                "target_lang_word": "ancient",
                                "target_lang_sense_clarification": "very old"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "старый",
                                "target_lang_sense_clarification": "общий термин, противоположность новому"
                            },
                            {
                                "target_lang_word": "старинный",
                                "target_lang_sense_clarification": "древний, антикварный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb17c08c-3843-36c0-9ae1-cbdc47741bdf",
                    "sense_definition": "Vorig of voormalig; wat iemand vroeger was of had, maar nu niet meer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>oude</w> fiets heb ik gisteren doorverkocht.",
                            "target_lang_translations": {
                                "en": "I resold my <w>old</w> bike yesterday.",
                                "ru": "Вчера я перепродал свой <w>старый</w> велосипед."
                            }
                        },
                        {
                            "text": "Hij ging terug naar zijn <w>oude</w> school voor een reünie.",
                            "target_lang_translations": {
                                "en": "He went back to his <w>old</w> school for a reunion.",
                                "ru": "Он вернулся в свою <w>старую</w> школу на встречу выпускников."
                            }
                        }
                    ],
                    "synonyms": [
                        "voormalig",
                        "ex-"
                    ],
                    "antonyms": [
                        "huidig",
                        "nieuw"
                    ],
                    "common_phrases": [
                        "oude liefde",
                        "oude gewoonte",
                        "bij het oude laten"
                    ],
                    "target_lang_definitions": {
                        "en": "Previous or former; what someone used to be or have, but is no longer.",
                        "ru": "Прежний или бывший; то, чем кто-то был или что имел раньше."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "old",
                                "target_lang_sense_clarification": "former or previous"
                            },
                            {
                                "target_lang_word": "former",
                                "target_lang_sense_clarification": "having previously filled a role"
                            },
                            {
                                "target_lang_word": "previous",
                                "target_lang_sense_clarification": "existing or occurring before"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "старый",
                                "target_lang_sense_clarification": "предыдущий, тот что был раньше"
                            },
                            {
                                "target_lang_word": "прежний",
                                "target_lang_sense_clarification": "бывший, предшествующий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d1deb39-7d24-3480-a2d4-82625b7cec58",
                    "sense_definition": "Niet vers meer (vooral gezegd van brood of gebak).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit brood is <w>oud</w> en hard geworden.",
                            "target_lang_translations": {
                                "en": "This bread has become <w>stale</w> and hard.",
                                "ru": "Этот хлеб стал <w>чёрствым</w> и твёрдым."
                            }
                        },
                        {
                            "text": "Je kunt dat <w>oude</w> gebak beter weggooien.",
                            "target_lang_translations": {
                                "en": "You had better throw away that <w>stale</w> pastry.",
                                "ru": "Тебе лучше выбросить эту <w>старую</w> выпечку."
                            }
                        }
                    ],
                    "antonyms": [
                        "vers"
                    ],
                    "common_phrases": [
                        "oud brood",
                        "oud bakken"
                    ],
                    "target_lang_definitions": {
                        "en": "No longer fresh (especially said of bread or pastry).",
                        "ru": "Несвежий (обычно о хлебе или выпечке)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stale",
                                "target_lang_sense_clarification": "no longer fresh (food)"
                            },
                            {
                                "target_lang_word": "old",
                                "target_lang_sense_clarification": "not fresh"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чёрствый",
                                "target_lang_sense_clarification": "засохший (о хлебе)"
                            },
                            {
                                "target_lang_word": "несвежий",
                                "target_lang_sense_clarification": "испортившийся"
                            }
                        ]
                    }
                }
            ]
        }
    ]

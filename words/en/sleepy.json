    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "83a500b8-458f-3551-ad01-5f86ba8918de",
                    "sense_definition": "Tired; feeling the need for sleep.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sleepy</w> child rubbed his eyes and asked for his teddy bear.",
                            "target_lang_translations": {
                                "ru": "<w>Со́нный</w> ребёнок потёр глаза и попросил своего плюшевого мишку."
                            }
                        },
                        {
                            "text": "I always feel <w>sleepy</w> after eating a large lunch on Sunday.",
                            "target_lang_translations": {
                                "ru": "Я всегда чувствую себя <w>со́нным</w> после плотного обеда в воскресенье."
                            }
                        }
                    ],
                    "synonyms": [
                        "tired",
                        "drowsy",
                        "weary",
                        "exhausted",
                        "somnolent",
                        "heavy"
                    ],
                    "antonyms": [
                        "nonsleepy",
                        "unsleepy",
                        "nonsleepy"
                    ],
                    "common_phrases": [
                        "feel sleepy",
                        "look sleepy",
                        "sleepy eyes",
                        "sleepy voice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уставший; испытывающий потребность во сне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со́нный",
                                "target_lang_sense_clarification": "испытывающий желание спать"
                            },
                            {
                                "target_lang_word": "сонли́вый",
                                "target_lang_sense_clarification": "склонный ко сну, вялый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "010b3d46-d727-39a3-8e1c-a72cf8ee3fc3",
                    "sense_definition": "Quiet; without bustle or activity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moved to a <w>sleepy</w> village where nothing exciting ever happens.",
                            "target_lang_translations": {
                                "ru": "Мы переехали в <w>со́нную</w> деревню, где никогда не происходит ничего захватывающего."
                            }
                        },
                        {
                            "text": "The shop is located in a <w>sleepy</w> corner of the busy city.",
                            "target_lang_translations": {
                                "ru": "Магазин расположен в <w>ти́хом</w> уголке шумного города."
                            }
                        }
                    ],
                    "synonyms": [
                        "quiet",
                        "slow",
                        "inactive"
                    ],
                    "common_phrases": [
                        "sleepy village",
                        "sleepy town",
                        "sleepy hollow",
                        "sleepy suburb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тихий; без суеты или активности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со́нный",
                                "target_lang_sense_clarification": "перен. тихий, замерший"
                            },
                            {
                                "target_lang_word": "ти́хий",
                                "target_lang_sense_clarification": "спокойный, безлюдный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c3972c9-7111-3edb-a247-145e559c155f",
                    "sense_definition": "Tending to induce sleep.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor prescribed a <w>sleepy</w> medicine to help the patient rest.",
                            "target_lang_translations": {
                                "ru": "Врач прописал <w>снотво́рное</w> лекарство, чтобы помочь пациенту отдохнуть."
                            }
                        },
                        {
                            "text": "The warm sun and gentle breeze had a <w>sleepy</w> effect on the picnic group.",
                            "target_lang_translations": {
                                "ru": "Тёплое солнце и лёгкий ветерок оказали <w>усыпля́ющий</w> эффект на группу на пикнике."
                            }
                        }
                    ],
                    "synonyms": [
                        "soporific",
                        "hypnotic",
                        "somniferous",
                        "somnific"
                    ],
                    "common_phrases": [
                        "sleepy potion",
                        "sleepy drink",
                        "sleepy effect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный вызывать сон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усыпля́ющий",
                                "target_lang_sense_clarification": "навевающий сон"
                            },
                            {
                                "target_lang_word": "снотво́рный",
                                "target_lang_sense_clarification": "о лекарствах или средствах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d05c477-b1ec-3845-88a1-c61d12c0c3f0",
                    "sense_definition": "Dull; lazy or inactive.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager complained about the <w>sleepy</w> pace of the construction work.",
                            "target_lang_translations": {
                                "ru": "Менеджер жаловался на <w>вя́лый</w> темп строительных работ."
                            }
                        },
                        {
                            "text": "Do not be <w>sleepy</w> in your duties if you want a promotion.",
                            "target_lang_translations": {
                                "ru": "Не будьте <w>вя́лыми</w> в своих обязанностях, если хотите повышения."
                            }
                        }
                    ],
                    "synonyms": [
                        "sluggish",
                        "inactive",
                        "dull"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скучный; ленивый или неактивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вя́лый",
                                "target_lang_sense_clarification": "медлительный, без энергии"
                            },
                            {
                                "target_lang_word": "медли́тельный",
                                "target_lang_sense_clarification": "неспешный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48940233-b0be-30c1-be35-fc52a4eff38a",
                    "sense_definition": "Dried mucus that accumulates in the corner of the eye during sleep.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The toddler woke up with <w>sleepies</w> in the corners of his eyes.",
                            "target_lang_translations": {
                                "ru": "Малыш проснулся с <w>кисля́чками</w> в уголках глаз."
                            }
                        },
                        {
                            "text": "You should wash your face to get rid of the <w>sleepies</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе следует умыться, чтобы убрать <w>кисля́чки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleep"
                    ],
                    "common_phrases": [
                        "rub the sleepies out",
                        "wipe the sleepies"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in plural form 'sleepies'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Засохшие выделения в уголках глаз после сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кисля́чки",
                                "target_lang_sense_clarification": "разг. засохшие выделения глаз"
                            },
                            {
                                "target_lang_word": "ко́рочки",
                                "target_lang_sense_clarification": "нейтральное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6bdcc63-48f9-36fb-8802-3b71e559b626",
                    "sense_definition": "A shingleback lizard.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We saw a <w>sleepy</w> basking on a rock in the Australian outback.",
                            "target_lang_translations": {
                                "ru": "Мы видели <w>короткохво́ста</w>, греющегося на камне в австралийской глуши."
                            }
                        },
                        {
                            "text": "Be careful not to step on the <w>sleepy</w> hiding in the grass.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не наступи на <w>короткохво́ста</w>, прячущегося в траве."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "common name for the lizard"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткохвостый сцинк (ящерица)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "короткохво́ст",
                                "target_lang_sense_clarification": "вид ящерицы"
                            }

                {
                    "sense_id": "d4964ece-99b9-38ff-97ee-1b58eeef45f0",
                    "sense_definition": "De plaats waar iets ligt of gesitueerd is ten opzichte van de omgeving.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>situatie</w> van het nieuwe kantoorgebouw is ideaal voor forenzen.",
                            "target_lang_translations": {
                                "en": "The <w>location</w> of the new office building is ideal for commuters.",
                                "ru": "<w>Расположение</w> нового офисного здания идеально подходит для тех, кто ездит на работу из пригорода."
                            }
                        },
                        {
                            "text": "Door de gunstige <w>situatie</w> ten opzichte van de zon is het huis erg licht.",
                            "target_lang_translations": {
                                "en": "Due to the favorable <w>position</w> relative to the sun, the house is very light.",
                                "ru": "Благодаря удачному <w>расположению</w> относительно солнца, в доме очень светло."
                            }
                        }
                    ],
                    "synonyms": [

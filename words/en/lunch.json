                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "regional usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок или ломоть хлеба, мяса или сыра"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусок",
                                "target_lang_sense_clarification": "часть продукта"
                            },
                            {
                                "target_lang_word": "ломоть",
                                "target_lang_sense_clarification": "большой кусок хлеба"
                            }
                        ]
                    }
                }
            ]
        },
        {

            "senses": [
                {
                    "sense_id": "b07f8c39-a44a-383b-902a-4237b1c32870",
                    "sense_definition": "Bardzo obraźliwe określenie człowieka podłego, nikczemnego lub godnego pogardy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę mieć z tym kłamliwym <w>ścierwem</w> nic wspólnego.",
                            "target_lang_translations": {
                                "ru": "Я не хочу иметь ничего общего с этой лживой <w>мразью</w>."
                            }
                        },
                        {
                            "text": "To <w>ścierwo</w> ukradło pieniądze własnej matce i uciekło.",
                            "target_lang_translations": {
                                "ru": "Эта <w>падаль</w> украла деньги у собственной матери и сбежала."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścierw"
                    ],
                    "common_phrases": [
                        "ty ścierwo",
                        "podłe ścierwo",
                        "ludzkie ścierwo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wulgarne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень оскорбительное слово для подлого, низкого человека; мразь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мразь",
                                "target_lang_sense_clarification": "подлый человек"
                            },
                            {
                                "target_lang_word": "падаль",
                                "target_lang_sense_clarification": "презренный человек"
                            },
                            {
                                "target_lang_word": "стерва",
                                "target_lang_sense_clarification": "негодяй, подлец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e0e1260-b436-3600-8f7b-86ddb31efc51",
                    "sense_definition": "Martwe, rozkładające się ciało zwierzęcia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dead_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sępy krążyły nad pustynią, szukając jakiegoś świeżego <w>ścierwa</w>.",
                            "target_lang_translations": {
                                "ru": "Стервятники кружили над пустыней в поисках свежей <w>падали</w>."
                            }
                        },
                        {
                            "text": "W lesie znaleziono rozszarpane przez wilki <w>ścierwo</w> sarny.",
                            "target_lang_translations": {
                                "ru": "В лесу нашли растерзанную волками <w>падаль</w> косули."
                            }
                        }

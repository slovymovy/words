{
    "word_family": [
        "cisza",
        "cichy",
        "cicho",
        "uciszać",
        "wyciszyć",
        "ściszyć",
        "uciszenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7ec09e76-7a80-30ec-a4cd-956f81d29d89",
                    "sense_definition": "Sprawić, że ktoś lub coś przestanie wydawać dźwięki, mówić lub hałasować.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Porucznik Florczak wybuchnął głośnym śmiechem, ale Jordan <uciszył> go natychmiast chłodnym spojrzeniem.",
                            "target_lang_translations": {
                                "ru": "Поручик Флорчак разразился громким смехом, но Джордан немедленно <утихомирил> его холодным взглядом."
                            }
                        },
                        {
                            "text": "Nauczycielka musiała kilkakrotnie <uciszyć> rozgadaną klasę podczas lekcji.",
                            "target_lang_translations": {
                                "ru": "Учительнице пришлось несколько раз <утихомиривать> разговорившийся класс во время урока."
                            }
                        }
                    ],
                    "synonyms": [
                        "uspokoić",
                        "uspokoić"
                    ],
                    "common_phrases": [
                        "uciszyć tłum",
                        "uciszyć psa",
                        "skutecznie uciszyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то или что-то перестать издавать звуки, говорить или шуметь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утихомирить",
                                "target_lang_sense_clarification": "успокоить кого-то шумного"
                            },
                            {
                                "target_lang_word": "заставить замолчать",
                                "target_lang_sense_clarification": "прервать чью-то речь"
                            },
                            {
                                "target_lang_word": "успокоить",
                                "target_lang_sense_clarification": "восстановить тишину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "433ecde1-ce93-3d1f-b68b-db7d1827868e",
                    "sense_definition": "Przestać mówić lub hałasować; stać się cichym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy uczniowie <uciszyli> się, gdy do sali wszedł dyrektor.",
                            "target_lang_translations": {
                                "ru": "Все ученики <притихли>, когда в зал вошел директор."
                            }
                        },
                        {
                            "text": "Po gwałtownej burzy wiatr w końcu <uciszył> się i wyszło słońce.",
                            "target_lang_translations": {
                                "ru": "После сильной бури ветер наконец <утих>, и выглянуло солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamilknąć",
                        "umilknąć"
                    ],
                    "common_phrases": [
                        "uciszyć się na chwilę",
                        "nagłe uciszenie się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать говорить или шуметь; стать тихим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замолчать",
                                "target_lang_sense_clarification": "перестать говорить"
                            },
                            {
                                "target_lang_word": "утихнуть",
                                "target_lang_sense_clarification": "о ветре, буре или шуме"
                            },
                            {
                                "target_lang_word": "притихнуть",
                                "target_lang_sense_clarification": "стать тише, успокоиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28b995ab-ca8b-3f54-a9ed-7187f3f84b3d",
                    "sense_definition": "Przywrócić spokój, opanować gwałtowne emocje, konflikt lub złagodzić ból.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "calm_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długo próbował <uciszyć> swoje sumienie, ale poczucie winy wciąż wracało.",
                            "target_lang_translations": {
                                "ru": "Он долго пытался <заглушить> свою совесть, но чувство вины всё возвращалось."
                            }
                        },
                        {
                            "text": "Podane przez lekarza leki pomogły szybko <uciszyć> nieznośny ból.",
                            "target_lang_translations": {
                                "ru": "Лекарства, данные врачом, помогли быстро <унять> невыносимую боль."
                            }
                        }
                    ],
                    "synonyms": [
                        "uspokoić"
                    ],
                    "common_phrases": [
                        "uciszyć sumienie",
                        "uciszyć ból",
                        "uciszyć emocje",
                        "uciszyć spór"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстановить спокойствие, подавить сильные эмоции или облегчить боль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглушить",
                                "target_lang_sense_clarification": "подавить (совесть, боль)"
                            },
                            {
                                "target_lang_word": "унять",
                                "target_lang_sense_clarification": "успокоить (боль, эмоции)"
                            },
                            {
                                "target_lang_word": "успокоить",
                                "target_lang_sense_clarification": "привести в состояние покоя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
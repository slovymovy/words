    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7e049c1-25eb-34b2-91b8-186a75969a76",
                    "sense_definition": "Człowiek, który unika pracy, wysiłku i lubi odpoczywać ponad potrzebę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lazy_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat to straszny <w>leń</w>, który nigdy nie sprząta swojego pokoju.",
                            "target_lang_translations": {
                                "ru": "Мой брат — ужасный <w>лентяй</w>, который никогда не убирает свою комнату."
                            }
                        },
                        {
                            "text": "Na tapczanie leży <w>leń</w>, nic nie robi cały dzień.",
                            "target_lang_translations": {
                                "ru": "На диване лежит <w>лентяй</w>, ничего не делает весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "leniuch",
                        "obibok",
                        "próżniak",
                        "nierób",
                        "wałkoń",
                        "leser",
                        "bumelant",
                        "nygus"
                    ],
                    "antonyms": [
                        "pracuś"
                    ],
                    "common_phrases": [
                        "śmierdzący leń",
                        "patentowany leń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который избегает работы, усилий и любит отдыхать больше, чем нужно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лентяй",
                                "target_lang_sense_clarification": "ленивый человек"
                            },
                            {
                                "target_lang_word": "лодырь",
                                "target_lang_sense_clarification": "разговорное; бездельник"
                            },
                            {
                                "target_lang_word": "бездельник",
                                "target_lang_sense_clarification": "тот, кто ничего не делает"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08b07578-4a9a-3f94-9cfc-d1cf6a4b44fd",
                    "sense_definition": "Owad z rodziny leniowatych, przypominający muchę, często spotykany wiosną.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ogrodzie na liściach siedział czarny <w>leń</w> marcowy.",
                            "target_lang_translations": {
                                "ru": "В саду на листьях сидела черная <w>толстоножка</w>."
                            }
                        },
                        {
                            "text": "Ten owad to <w>leń</w>, który jest zupełnie nieszkodliwy dla człowieka.",
                            "target_lang_translations": {
                                "ru": "Это насекомое — <w>толстоножка</w>, которая совершенно безвредна для человека."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Насекомое из семейства комаров-толстоножек, напоминающее муху, часто встречающееся весной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толстоножка",
                                "target_lang_sense_clarification": "насекомое (Bibio)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4df8e4d7-4b9f-3bb0-90fd-611ff58fc1ce",
                    "sense_definition": "Gatunek słodkowodnej ryby z rodziny karpiowatych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędkarz był zaskoczony, gdy na haczyk złapał się rzadki <w>leń</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбак был удивлен, когда на крючок попался редкий <w>линь</w>."
                            }
                        },
                        {
                            "text": "W tym jeziorze występuje ryba zwana potocznie jako <w>leń</w>.",
                            "target_lang_translations": {
                                "ru": "В этом озере водится рыба, которую в просторечии называют <w>линь</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид пресноводной рыбы из семейства карповых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "линь",
                                "target_lang_sense_clarification": "рыба (Tinca tinca)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad07d91f-d44d-30e1-af48-947df5c6dde1",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>lenić</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>leń</w> się, tylko wstawaj i pomóż mi w kuchni!",
                            "target_lang_translations": {
                                "ru": "Не <w>ленись</w>, вставай и помоги мне на кухне!"
                            }
                        },
                        {
                            "text": "Proszę, nie <w>leń</w> się dzisiaj, bo mamy dużo pracy do wykonania.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>ленись</w> сегодня, потому что у нас много работы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "tryb rozkazujący"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения глагола «лениться» (lenić się)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ленись",
                                "target_lang_sense_clarification": "форма глагола (обычно с отрицанием «не»)"
                            }
                        ]
                    }
                }
            ]
        },

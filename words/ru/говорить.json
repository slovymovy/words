{
    "word_family": [
        "разговор",
        "договор",
        "приговор",
        "уговор",
        "заговор",
        "поговорка",
        "говор",
        "разговорчивый",
        "разговорный",
        "договориться",
        "уговорить",
        "разговориться",
        "переговорить",
        "отговорить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4b768f67-b178-3920-a0d4-d519760be330",
                    "sense_definition": "Владеть языком, уметь пользоваться устной речью.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой новый друг отлично <говорит> по-русски.",
                            "target_lang_translations": {
                                "en": "My new friend <speaks> Russian perfectly."
                            }
                        },
                        {
                            "text": "Маленький ребёнок начал <говорить> очень рано.",
                            "target_lang_translations": {
                                "en": "The small child started to <talk> very early."
                            }
                        }
                    ],
                    "antonyms": [
                        "немотствовать"
                    ],
                    "common_phrases": [
                        "говорить по-русски",
                        "говорить на английском",
                        "научиться говорить",
                        "свободно говорить"
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to communicate in a language or use oral speech."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "to know and use a language"
                            },
                            {
                                "target_lang_word": "talk",
                                "target_lang_sense_clarification": "to have the ability of speech"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c7bfb52-f851-3432-b267-a2b04eddb78b",
                    "sense_definition": "Произносить слова, выражать мысли вслух, сообщать информацию.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я всегда прошу его <говорить> только правду.",
                            "target_lang_translations": {
                                "en": "I always ask him to <tell> only the truth."
                            }
                        },
                        {
                            "text": "Учитель <говорит> нам открыть учебники на пятой странице.",
                            "target_lang_translations": {
                                "en": "The teacher <tells> us to open our textbooks to page five."
                            }
                        }
                    ],
                    "synonyms": [
                        "сообщать",
                        "высказывать",
                        "высказываться"
                    ],
                    "antonyms": [
                        "молчать"
                    ],
                    "common_phrases": [
                        "говорить правду",
                        "честно говоря",
                        "говорить громко",
                        "говорить шёпотом"
                    ],
                    "target_lang_definitions": {
                        "en": "To pronounce words, express thoughts aloud, or convey information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "say",
                                "target_lang_sense_clarification": "to utter words or express thoughts"
                            },
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to communicate information or give instructions"
                            },
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "to utter words"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7282f9e0-4dcc-3ccd-973b-82884c5ae2b5",
                    "sense_definition": "Вести беседу, общаться с кем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conversation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы любим <говорить> с друзьями о музыке и кино.",
                            "target_lang_translations": {
                                "en": "We love to <talk> with friends about music and movies."
                            }
                        },
                        {
                            "text": "Она может часами <говорить> по телефону.",
                            "target_lang_translations": {
                                "en": "She can <talk> on the phone for hours."
                            }
                        }
                    ],
                    "synonyms": [
                        "разговаривать",
                        "беседовать"
                    ],
                    "antonyms": [
                        "молчать"
                    ],
                    "common_phrases": [
                        "говорить по телефону",
                        "говорить с другом",
                        "говорить о делах",
                        "говорить по душам"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies informal conversation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To engage in conversation or discuss something with someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "talk",
                                "target_lang_sense_clarification": "to converse informally"
                            },
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "to converse formally or generally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c68cf47-0088-3b25-9593-43e140497be9",
                    "sense_definition": "Распространять слухи или сообщать общеизвестную информацию (обычно в 3-м лице множественного числа).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rumors",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем городе <говорят>, что скоро построят новый парк.",
                            "target_lang_translations": {
                                "en": "In our city they <say> that a new park will be built soon."
                            }
                        },
                        {
                            "text": "Про этого актёра <говорят> много хорошего.",
                            "target_lang_translations": {
                                "en": "They <say> many good things about this actor."
                            }
                        }
                    ],
                    "synonyms": [
                        "поговаривать",
                        "сплетничать"
                    ],
                    "common_phrases": [
                        "говорят, что",
                        "как говорят",
                        "люди говорят"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually impersonal 'говорят'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spread rumors or report common knowledge (usually impersonal 'they say')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "say",
                                "target_lang_sense_clarification": "used in 'they say' for rumors or general belief"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6b8894e-3c20-3c9c-bfcd-b82af445384c",
                    "sense_definition": "Свидетельствовать о чём-либо, служить доказательством (в переносном смысле).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его смелые поступки <говорят> сами за себя.",
                            "target_lang_translations": {
                                "en": "His brave deeds <speak> for themselves."
                            }
                        },
                        {
                            "text": "Эти факты <говорят> о том, что экономика растёт.",
                            "target_lang_translations": {
                                "en": "These facts <indicate> that the economy is growing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "факты говорят",
                        "говорить в пользу",
                        "говорить сам за себя"
                    ],
                    "target_lang_definitions": {
                        "en": "To serve as evidence or proof of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speak",
                                "target_lang_sense_clarification": "to serve as evidence (e.g., speak for itself)"
                            },
                            {
                                "target_lang_word": "indicate",
                                "target_lang_sense_clarification": "to show or suggest"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39d76f9b-19e7-362b-8c3a-77bec5117309",
                    "sense_definition": "Подсказывать или предчувствовать (об интуиции или чувствах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моё сердце <говорит>, что нужно помочь этому человеку.",
                            "target_lang_translations": {
                                "en": "My heart <tells> me that I need to help this person."
                            }
                        },
                        {
                            "text": "Интуиция <говорила> мне не соглашаться на это предложение.",
                            "target_lang_translations": {
                                "en": "Intuition <told> me not to agree to this proposal."
                            }
                        }
                    ],
                    "synonyms": [
                        "подсказывать",
                        "предсказывать"
                    ],
                    "common_phrases": [
                        "внутренний голос говорит",
                        "интуиция говорит",
                        "сердце говорит"
                    ],
                    "target_lang_definitions": {
                        "en": "To suggest or warn (referring to intuition or feelings)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to give an intuitive feeling"
                            },
                            {
                                "target_lang_word": "say",
                                "target_lang_sense_clarification": "to suggest internally"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
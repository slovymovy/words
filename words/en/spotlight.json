{
    "word_family": [
        "spot",
        "light"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "87a55ba7-41d9-3139-a55f-3367b54d4951",
                    "sense_definition": "The center of public attention or interest.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been in the <w>spotlight</w> since her first movie was released.",
                            "target_lang_translations": {
                                "ru": "Она находится в <w>центре внимания</w> с момента выхода её первого фильма."
                            }
                        },
                        {
                            "text": "The recent scandal put the company under the <w>spotlight</w>.",
                            "target_lang_translations": {
                                "ru": "Недавний скандал поставил компанию в <w>центр внимания</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jacklight"
                    ],
                    "antonyms": [
                        "obscurity"
                    ],
                    "common_phrases": [
                        "in the spotlight",
                        "steal the spotlight",
                        "under the spotlight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центр общественного внимания или интереса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "центр внимания",
                                "target_lang_sense_clarification": "общественный интерес"
                            },
                            {
                                "target_lang_word": "свет рампы",
                                "target_lang_sense_clarification": "театральная метафора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "453c8d4c-39df-3c8c-af58-483baf9c7fa6",
                    "sense_definition": "A powerful light with a focused beam that can be directed at a specific place or person.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The technician pointed the <w>spotlight</w> at the lead singer.",
                            "target_lang_translations": {
                                "ru": "Техник направил <w>прожектор</w> на солиста."
                            }
                        },
                        {
                            "text": "A bright <w>spotlight</w> followed the dancer across the stage.",
                            "target_lang_translations": {
                                "ru": "Яркий <w>прожектор</w> следовал за танцором по сцене."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bright spotlight",
                        "stage spotlight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мощный осветительный прибор с направленным лучом света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прожектор",
                                "target_lang_sense_clarification": "осветительный прибор"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d70f7941-06b6-35cc-ae85-1841c2216762",
                    "sense_definition": "To draw public attention to a particular issue or person.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The documentary aims to <w>spotlight</w> the problems in the education system.",
                            "target_lang_translations": {
                                "ru": "Документальный фильм призван <w>осветить</w> проблемы в системе образования."
                            }
                        },
                        {
                            "text": "This event will <w>spotlight</w> the achievements of local athletes.",
                            "target_lang_translations": {
                                "ru": "Это событие <w>привлечет внимание</w> к достижениям местных спортсменов."
                            }
                        }
                    ],
                    "synonyms": [
                        "jacklight"
                    ],
                    "antonyms": [
                        "ignore",
                        "overlook"
                    ],
                    "common_phrases": [
                        "spotlight an issue",
                        "spotlight a problem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекать общественное внимание к определенной проблеме или человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освещать",
                                "target_lang_sense_clarification": "проблему или вопрос"
                            },
                            {
                                "target_lang_word": "привлекать внимание",
                                "target_lang_sense_clarification": "делать заметным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d57d2dc9-d536-3075-a69e-58ad18f36697",
                    "sense_definition": "To illuminate something with a focused beam of light.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They plan to <w>spotlight</w> the monument during the festival.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>осветить прожектором</w> памятник во время фестиваля."
                            }
                        },
                        {
                            "text": "The director decided to <w>spotlight</w> the protagonist in the final scene.",
                            "target_lang_translations": {
                                "ru": "Режиссер решил <w>выделить светом</w> главного героя в финальной сцене."
                            }
                        }
                    ],
                    "synonyms": [
                        "jacklight"
                    ],
                    "common_phrases": [
                        "spotlight the stage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освещать что-либо направленным лучом света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освещать прожектором",
                                "target_lang_sense_clarification": "направлять свет"
                            },
                            {
                                "target_lang_word": "высвечивать",
                                "target_lang_sense_clarification": "выделять светом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "511c368e-354b-3980-9275-1cdc492cfbbb",
                    "sense_definition": "To hunt animals at night using a bright light to freeze them.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is illegal to <w>spotlight</w> deer in many areas.",
                            "target_lang_translations": {
                                "ru": "Во многих районах незаконно <w>охотиться с прожектором</w> на оленей."
                            }
                        },
                        {
                            "text": "The poachers attempted to <w>spotlight</w> rabbits in the field.",
                            "target_lang_translations": {
                                "ru": "Браконьеры пытались <w>ослепить прожектором</w> кроликов в поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "jacklight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охотиться на животных ночью, используя яркий свет для их ослепления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охотиться с прожектором",
                                "target_lang_sense_clarification": "способ охоты"
                            },
                            {
                                "target_lang_word": "лучить",
                                "target_lang_sense_clarification": "охотничий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "cienkość"
                    ],
                    "common_phrases": [
                        "grubość głosu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеристика голоса или звука, отличающаяся низким, глубоким звучанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "густота",

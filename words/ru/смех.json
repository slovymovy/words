    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "986b953a-12aa-3c8a-9eef-5ff180b17460",
                    "sense_definition": "Прерывистые горловые звуки, которые человек издаёт при веселье, радости или удовольствии.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В соседней комнате послышался громкий и весёлый <w>смех</w>.",
                            "target_lang_translations": {
                                "en": "Loud and cheerful <w>laughter</w> was heard in the next room."
                            }
                        },
                        {
                            "text": "Её звонкий <w>смех</w> всегда поднимает мне настроение.",
                            "target_lang_translations": {
                                "en": "Her ringing <w>laugh</w> always cheers me up."
                            }
                        }
                    ],
                    "synonyms": [
                        "хохот"
                    ],
                    "antonyms": [
                        "плач"
                    ],
                    "common_phrases": [
                        "громкий смех",
                        "взрыв смеха",
                        "сдерживать смех",
                        "заразительный смех",
                        "умирать со смеху"
                    ],
                    "target_lang_definitions": {
                        "en": "Intermittent guttural sounds that a person makes during fun, joy, or pleasure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "laughter",
                                "target_lang_sense_clarification": "the action or sound of laughing"
                            },
                            {
                                "target_lang_word": "laugh",
                                "target_lang_sense_clarification": "a specific instance of the sound of laughing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "924812ac-6dce-3534-99a1-40b3d275dd2b",
                    "sense_definition": "Веселье, забавная ситуация, шутки или насмешка над кем-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твои попытки починить этот старый прибор — просто <w>смех</w>.",
                            "target_lang_translations": {
                                "en": "Your attempts to fix this old device are just a <w>joke</w>."
                            }
                        },
                        {
                            "text": "Его подняли на <w>смех</w> после того, как он рассказал эту небылицу.",
                            "target_lang_translations": {
                                "en": "He was held up to <w>ridicule</w> after he told that tall tale."
                            }
                        }
                    ],
                    "antonyms": [
                        "грусть"
                    ],
                    "common_phrases": [
                        "курам на смех",
                        "поднять на смех",
                        "и смех и грех",
                        "смех смехом"
                    ],
                    "target_lang_definitions": {
                        "en": "Fun, a funny situation, jokes, or mockery of someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "something ridiculous or not serious"
                            },
                            {
                                "target_lang_word": "mockery",
                                "target_lang_sense_clarification": "ridicule or derision"
                            },
                            {
                                "target_lang_word": "laugh",
                                "target_lang_sense_clarification": "something that provokes mirth or scorn"
                            }
                        ]
                    }
                }
            ]
        }
    ]

        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "15742ac3-23aa-3d2c-8869-54e56781696e",
                    "sense_definition": "Jedna z dwóch równych części, na które podzielono jakąś całość.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_exact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do ciasta musisz dodać <w>pół</w> szklanki cukru.",
                            "target_lang_translations": {
                                "ru": "В тесто нужно добавить <w>пол</w>стакана сахара."
                            }
                        },
                        {
                            "text": "Nasza rozmowa trwała ponad <w>pół</w> godziny.",
                            "target_lang_translations": {
                                "ru": "Наш разговор длился более <w>полу</w>часа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pół godziny",
                        "pół roku",
                        "pół na pół",
                        "o wpół do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух равных частей, на которые разделено целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пол-",
                                "target_lang_sense_clarification": "первая часть сложных слов, обозначающая половину (напр. полчаса, полкило)"
                            },
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "существительное, обозначающее 1/2 часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5017452-453e-32d7-b43f-b0eada42aa45",
                    "sense_definition": "Znaczna część czegoś, niekoniecznie dokładnie 50%, często używane w wyolbrzymieniach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_approx",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zjadł <w>pół</w> tortu i teraz boli go brzuch.",
                            "target_lang_translations": {
                                "ru": "Он съел <w>пол</w>торта, и теперь у него болит живот."
                            }
                        },
                        {
                            "text": "Przeszukałem <w>pół</w> miasta, żeby znaleźć ten prezent.",
                            "target_lang_translations": {
                                "ru": "Я обыскал <w>пол</w>города, чтобы найти этот подарок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pół świata",
                        "pół miasta",
                        "pół biedy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej dla podkreślenia ilości"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительная часть чего-либо, не обязательно ровно 50%, часто используется для преувеличения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пол-",
                                "target_lang_sense_clarification": "в разговорной речи для обозначения большого количества"
                            },
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "общая часть чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {

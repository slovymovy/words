                },
                {
                    "sense_id": "f0ac7acf-4f59-3558-8311-f1e06f032bd9",
                    "sense_definition": "Drukt berusting uit over een situatie die men niet kan veranderen of waar men ambivalent tegenover staat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een oude auto vraagt nu eenmaal veel onderhoud. — <w>Tja</w>, dat hou je toch."
                        },
                        {
                            "text": "Het regent al de hele dag, maar <w>tja</w>, het is nu eenmaal herfst."
                        }
                    ],
                    "synonyms": [
                        "ach",
                        "helaas"
                    ],
                    "common_phrases": [
                        "tja, het is niet anders",
                        "tja, wat doe je eraan"

                {
                    "sense_id": "4082483d-7c98-3361-b38a-e7453d713634",
                    "sense_definition": "toponim, nazwa kilku miast i wsi w Albanii, Bułgarii, Serbii, Chorwacji, Macedonii i na Ukrainie",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "other_places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Bułgarski <w>Sopot</w> słynie z produkcji maszynowej.",
                            "target_lang_translations": {
                                "ru": "Болгарский <w>Сопот</w> славится машиностроением."
                            }
                        },
                        {
                            "text": "Na mapie Bałkanów można znaleźć niejeden <w>Sopot</w>.",
                            "target_lang_translations": {
                                "ru": "На карте Балкан можно найти не один <w>Сопот</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "топоним, название нескольких городов и сел в Албании, Болгарии, Сербии, Хорватии, Македонии и на Украине"
                    },

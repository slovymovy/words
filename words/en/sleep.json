    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6266f864-d1a0-34d4-9da5-592d7795ed5b",
                    "sense_definition": "To rest in a state of reduced consciousness.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually <w>sleep</w> for about eight hours every night.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>сплю</w> около восьми часов каждую ночь."
                            }
                        },
                        {
                            "text": "The baby finally managed to <w>sleep</w> after crying for hours.",
                            "target_lang_translations": {
                                "ru": "Ребенок наконец-то смог <w>поспать</w> после того, как плакал несколько часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "slumber",
                        "snooze",
                        "doze",
                        "rest",
                        "crash",
                        "repose"
                    ],
                    "antonyms": [
                        "wake",
                        "wake up"
                    ],
                    "common_phrases": [
                        "go to sleep",
                        "sleep tight",
                        "sleep well",
                        "fast asleep",
                        "sleep in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в состоянии сна, отдыхать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спать",
                                "target_lang_sense_clarification": "находиться в состоянии сна"
                            },
                            {
                                "target_lang_word": "поспать",
                                "target_lang_sense_clarification": "провести некоторое время во сне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ec9e937-0183-303b-a633-fcb3fa46210d",
                    "sense_definition": "To have sexual intercourse.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>sleep</w> together after dating for a month.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>переспать</w> после месяца свиданий."
                            }
                        },
                        {
                            "text": "He did not want to <w>sleep</w> with a stranger.",
                            "target_lang_translations": {
                                "ru": "Он не хотел <w>спать</w> с незнакомкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sleep with",
                        "sleep together"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь половую связь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спать",
                                "target_lang_sense_clarification": "иметь интимные отношения"
                            },
                            {
                                "target_lang_word": "переспать",
                                "target_lang_sense_clarification": "совершить половой акт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cd48cdc-f810-34e9-aa93-4230cadb168b",
                    "sense_definition": "To provide beds or sleeping space for a certain number of people.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This small tent <w>sleeps</w> two people very comfortably.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая палатка с комфортом <w>вмещает</w> двух человек."
                            }
                        },
                        {
                            "text": "The holiday cottage <w>sleeps</w> up to six guests.",
                            "target_lang_translations": {
                                "ru": "В этом коттедже могут <w>разместиться</w> до шести гостей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять спальные места для определенного количества людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вмещать",
                                "target_lang_sense_clarification": "о помещении или палатке"
                            },
                            {
                                "target_lang_word": "размещать",
                                "target_lang_sense_clarification": "предоставлять место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1865cb29-e319-33df-9db8-b61845f8bcf6",
                    "sense_definition": "To enter a power-saving state or wait for a period of time (of a computer or program).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The computer will <w>sleep</w> after ten minutes of inactivity.",
                            "target_lang_translations": {
                                "ru": "Компьютер <w>перейдет в спящий режим</w> через десять минут бездействия."
                            }
                        },
                        {
                            "text": "The script <w>sleeps</w> for five seconds before retrying.",
                            "target_lang_translations": {
                                "ru": "Скрипт <w>ждет</w> пять секунд перед повторной попыткой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sleep mode"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходить в энергосберегающий режим или ожидать (о компьютере или программе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переходить в спящий режим",
                                "target_lang_sense_clarification": "о технике"
                            },
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "о программном коде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4b23c3c-9ec2-3758-a023-7c8a9c066eeb",
                    "sense_definition": "To be dead or lie in a grave.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many soldiers <w>sleep</w> in this field forever.",
                            "target_lang_translations": {
                                "ru": "Многие солдаты вечно <w>спят</w> на этом поле."
                            }
                        },
                        {
                            "text": "She went to <w>sleep</w> peacefully at the age of ninety.",
                            "target_lang_translations": {
                                "ru": "Она мирно <w>уснула</w> вечным сном в возрасте девяноста лет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "euphemistic"
                        },
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть мертвым или лежать в могиле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покоиться",
                                "target_lang_sense_clarification": "лежать в могиле"
                            },
                            {
                                "target_lang_word": "спать",
                                "target_lang_sense_clarification": "поэтическое/эвфемизм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9609d49-51bc-3ada-b7f7-f802ff287d4b",
                    "sense_definition": "The natural state of rest during which the eyes are closed and consciousness is reduced.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to get some <w>sleep</w> before the big exam.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немного <w>поспать</w> перед важным экзаменом."
                            }
                        },
                        {
                            "text": "The doctor recommended a good night's <w>sleep</w> for recovery.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал хороший ночной <w>сон</w> для восстановления."
                            }
                        }
                    ],
                    "synonyms": [
                        "slumber",
                        "rest",
                        "repose"
                    ],
                    "antonyms": [
                        "wakefulness"
                    ],
                    "common_phrases": [
                        "deep sleep",
                        "get some sleep",
                        "lack of sleep",
                        "go to sleep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественное состояние отдыха, при котором глаза закрыты, а сознание ослаблено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сон",
                                "target_lang_sense_clarification": "физиологическое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8acf2a11-46a2-3079-bef2-f6ed2684338d",
                    "sense_definition": "A night's rest, often used to count time until an event.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are only three more <w>sleeps</w> until Christmas morning.",
                            "target_lang_translations": {
                                "ru": "До рождественского утра осталось всего три <w>ночи</w>."
                            }
                        },
                        {
                            "text": "We have two <w>sleeps</w> left before we go on vacation.",
                            "target_lang_translations": {
                                "ru": "Нам осталось две <w>ночи</w> до отпуска."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "childish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ночной отдых; часто используется для отсчета времени до события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ночь",
                                "target_lang_sense_clarification": "единица времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af5e5ba0-98ca-3e88-8d71-d32fa310c8f7",
                    "sense_definition": "A crusty or gummy discharge found in the corner of the eyes after waking.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wiped the <w>sleep</w> from his eyes when he woke up.",
                            "target_lang_translations": {
                                "ru": "Проснувшись, он протер <w>сон</w> с глаз."
                            }
                        },
                        {
                            "text": "There was a bit of <w>sleep</w> in the corner of her eye.",
                            "target_lang_translations": {
                                "ru": "В уголке её глаза было немного <w>засохших выделений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eye gunk",
                        "sleepies",
                        "eye crust",
                        "eye sand"
                    ],
                    "common_phrases": [
                        "sleep in one's eyes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Засохшие выделения в уголках глаз после сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сон",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "кисляк",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4e59766d-60aa-3abb-96ed-d683d8b87880",
                    "sense_definition": "Zonder medelijden of gevoel; hardvochtig en gemeen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dictator was een <w>wrede</w> man die zijn volk onderdrukte.",
                            "target_lang_translations": {
                                "en": "The dictator was a <w>cruel</w> man who oppressed his people.",
                                "ru": "Диктатор был <w>жестоким</w> человеком, который угнетал свой народ."
                            }
                        },
                        {
                            "text": "Het is <w>wreed</w> om dieren pijn te doen voor je eigen plezier.",
                            "target_lang_translations": {
                                "en": "It is <w>cruel</w> to hurt animals for your own pleasure.",
                                "ru": "Это <w>жестоко</w> — причинять боль животным ради собственного удовольствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "harteloos"
                    ],
                    "common_phrases": [
                        "een wreed lot",
                        "wrede grap",
                        "wrede werkelijkheid"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking pity or compassion; harsh and mean.",
                        "ru": "Без жалости или сострадания; бессердечный и злой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cruel",
                                "target_lang_sense_clarification": "causing pain or suffering"
                            },
                            {
                                "target_lang_word": "ruthless",
                                "target_lang_sense_clarification": "without pity or mercy"
                            },
                            {
                                "target_lang_word": "heartless",
                                "target_lang_sense_clarification": "lacking feeling"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "жестокий",
                                "target_lang_sense_clarification": "безжалостный"
                            },
                            {
                                "target_lang_word": "бессердечный",
                                "target_lang_sense_clarification": "не имеющий сочувствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b2672fc-1520-31d4-be23-18c49ca81563",
                    "sense_definition": "Heel erg goed, gaaf of indrukwekkend.",

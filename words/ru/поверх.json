    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "ec7788ee-3160-367e-86c2-4901789e77e9",
                    "sense_definition": "Указывает на предмет или одежду, на которые сверху надевается, кладётся или на которых находится что-то другое.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "layering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он надел тёплую куртку <поверх> свитера.",
                            "target_lang_translations": {
                                "en": "He put on a warm jacket <over> his sweater."
                            }
                        },
                        {
                            "text": "Она повязала красивый шарф <поверх> пальто.",
                            "target_lang_translations": {
                                "en": "She tied a beautiful scarf <over> her coat."
                            }
                        }
                    ],
                    "synonyms": [
                        "сверху"
                    ],
                    "antonyms": [
                        "под"
                    ],
                    "common_phrases": [
                        "поверх одежды",
                        "надеть поверх",
                        "поверх всего"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an object or clothing over which something else is worn, placed, or located."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "on top of or covering"
                            },
                            {
                                "target_lang_word": "on top of",
                                "target_lang_sense_clarification": "placed upon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2016bc7c-dced-3f70-aafe-5a0d4395a1c4",
                    "sense_definition": "Указывает на пространство выше предмета, через которое направлено действие, взгляд или движение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель строго посмотрел на ученика <поверх> очков.",
                            "target_lang_translations": {
                                "en": "The teacher looked sternly at the student <over> his glasses."
                            }
                        },
                        {
                            "text": "Мы увидели яркое солнце <поверх> крыш домов.",
                            "target_lang_translations": {
                                "en": "We saw the bright sun <over> the roofs of the houses."
                            }
                        }
                    ],
                    "synonyms": [
                        "над"
                    ],
                    "antonyms": [
                        "под"
                    ],
                    "common_phrases": [
                        "смотреть поверх очков",
                        "глядеть поверх голов",
                        "поверх барьеров"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the space above an object across which an action, gaze, or movement is directed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "across the top of"
                            },
                            {
                                "target_lang_word": "above",
                                "target_lang_sense_clarification": "higher than"
                            }
                        ]
                    }
                }
            ]
        }
    ]

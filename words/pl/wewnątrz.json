        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fd916350-b105-37d0-a2eb-ad61dd967f90",
                    "sense_definition": "W środku, w jakiejś zamkniętej przestrzeni.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tutaj tłum zachowywał się spokojnie, lecz <w>wewnątrz</w> działy się straszne rzeczy!",
                            "target_lang_translations": {
                                "ru": "Здесь толпа вела себя спокойно, но <w>внутри</w> творились страшные вещи!"
                            }
                        },
                        {
                            "text": "Drzwi były zablokowane od <w>wewnątrz</w>, więc nie mogliśmy wejść.",
                            "target_lang_translations": {
                                "ru": "Дверь была заблокирована <w>изнутри</w>, поэтому мы не могли войти."
                            }
                        }
                    ],
                    "synonyms": [
                        "w środku"
                    ],
                    "antonyms": [
                        "na zewnątrz"
                    ],
                    "common_phrases": [
                        "od wewnątrz",
                        "do wewnątrz",
                        "siedzieć wewnątrz"
                    ],
                    "target_lang_definitions": {
                        "ru": "В середине, в каком-либо замкнутом пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутри",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "в помещении",
                                "target_lang_sense_clarification": "в здании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1cd6343-7c44-31a3-bfd2-8450568a0fe9",
                    "sense_definition": "W sferze psychicznej, w myślach lub uczuciach; w sposób niewidoczny dla innych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Co prawda jest cyniczny, ale <w>wewnątrz</w> jest bardzo wrażliwą osobą.",
                            "target_lang_translations": {
                                "ru": "Правда, он циничен, но <w>внутри</w> он очень чувствительный человек."
                            }
                        },
                        {
                            "text": "Mimo że się uśmiechała, <w>wewnątrz</w> czuła ogromny smutek.",
                            "target_lang_translations": {
                                "ru": "Хотя она улыбалась, <w>внутри</w> она чувствовала огромную грусть."
                            }
                        }
                    ],
                    "synonyms": [
                        "w głębi",
                        "prywatnie"
                    ],
                    "common_phrases": [
                        "czuć wewnątrz",
                        "zmienić się wewnątrz"
                    ],
                    "target_lang_definitions": {
                        "ru": "В психической сфере, в мыслях или чувствах; невидимо для других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутри",
                                "target_lang_sense_clarification": "о чувствах"
                            },
                            {
                                "target_lang_word": "в душе",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]

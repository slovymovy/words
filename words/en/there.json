{
    "word_family": [
        "thereabout",
        "thereabouts",
        "thereafter",
        "thereat",
        "thereby",
        "therefore",
        "therefrom",
        "therein",
        "thereinto",
        "thereof",
        "thereon",
        "thereto",
        "thereunder",
        "thereupon",

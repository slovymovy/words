    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ffc3512c-401f-3824-aa57-5f6053ecbdf3",
                    "sense_definition": "Wypukła kość po obu stronach nogi, łącząca stopę z łydką.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas biegania źle stanąłem i boli mnie <w>kostka</w>.",
                            "target_lang_translations": {
                                "ru": "Во время бега я неудачно наступил на ногу, и у меня болит <w>щиколотка</w>."
                            }
                        },
                        {
                            "text": "Lekarz powiedział, że to tylko skręcona <w>kostka</w>, a nie złamanie.",
                            "target_lang_translations": {
                                "ru": "Врач сказал, что это только подвернутая <w>лодыжка</w>, а не перелом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skręcić kostkę",
                        "zwichnięta kostka",
                        "opuchnięta kostka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая кость с обеих сторон ноги, соединяющая стопу с голенью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щиколотка",
                                "target_lang_sense_clarification": "разговорное, общеупотребительное"
                            },
                            {
                                "target_lang_word": "лодыжка",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bbdad70-689f-3c99-83e2-93e51a0e0385",
                    "sense_definition": "Przedmiot w kształcie sześcianu lub prostopadłościanu, często będący porcją jakiejś substancji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poproszę herbatę i jedną <w>kostkę</w> cukru.",
                            "target_lang_translations": {
                                "ru": "Дайте, пожалуйста, чай и один <w>кубик</w> сахара."
                            }
                        },
                        {
                            "text": "Do ciasta potrzebna będzie cała <w>kostka</w> masła.",
                            "target_lang_translations": {
                                "ru": "Для теста понадобится целая <w>пачка</w> масла."
                            }

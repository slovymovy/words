    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d6703ceb-f51d-364a-96ac-2696cd967e92",
                    "sense_definition": "W sposób zajmujący dużo miejsca w poziomie lub na dużą odległość w poprzek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_width",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko uśmiechnęło się <szeroko> na widok wymarzonego prezentu.",
                            "target_lang_translations": {
                                "ru": "Ребёнок <широко> улыбнулся при виде подарка своей мечты."
                            }
                        },
                        {
                            "text": "Gospodarz otworzył <szeroko> drzwi, zapraszając wszystkich gości do środka.",
                            "target_lang_translations": {
                                "ru": "Хозяин <широко> открыл дверь, приглашая всех гостей внутрь."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozlegle"
                    ],
                    "antonyms": [
                        "wąsko"
                    ],
                    "common_phrases": [
                        "szeroko otwarte oczy",
                        "szeroko się uśmiechać",
                        "szeroko otworzyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимая много места в пространстве или открываясь на большое расстояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "о физическом пространстве, открытии глаз, дверей, улыбке"
                            },
                            {
                                "target_lang_word": "настежь",
                                "target_lang_sense_clarification": "полностью, до конца (обычно о дверях или окнах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ec85018-fc7d-3055-b049-6f6f4ad55773",
                    "sense_definition": "W sposób obejmujący duży zakres spraw, zjawisk lub osób; powszechnie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten problem jest obecnie <szeroko> dyskutowany w mediach społecznościowych.",
                            "target_lang_translations": {
                                "ru": "Эта проблема сейчас <широко> обсуждается в социальных сетях."
                            }
                        },
                        {
                            "text": "Jest to pisarz <szeroko> znany i ceniony na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Это писатель, <широко> известный и ценимый во всём мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "powszechnie",
                        "obszernie"
                    ],
                    "antonyms": [
                        "wąsko"
                    ],
                    "common_phrases": [
                        "szeroko znany",
                        "szeroko pojęty",
                        "szeroko zakrojony",
                        "szeroko dyskutowany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охватывая большой круг вопросов, явлений или людей; повсеместно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "в переносном смысле: об известности, обсуждении, применении"
                            },
                            {
                                "target_lang_word": "повсеместно",
                                "target_lang_sense_clarification": "везде, во многих местах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d09eec92-0c25-38a5-a1eb-e53a86b9b92a",
                    "sense_definition": "Na określoną miarę w poprzek.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_width",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzeka w tym miejscu rozlewa się <szeroko> na kilkaset metrów.",
                            "target_lang_translations": {
                                "ru": "Река в этом месте разливается <широко> на несколько сотен метров."
                            }
                        },
                        {
                            "text": "Zawodnik rozstawił nogi <szeroko>, aby utrzymać lepszą równowagę.",
                            "target_lang_translations": {
                                "ru": "Спортсмен расставил ноги <широко>, чтобы лучше удерживать равновесие."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На значительное расстояние в стороны; в поперечнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "о расстановке ног, рук или предметов"
                            },
                            {
                                "target_lang_word": "вширь",
                                "target_lang_sense_clarification": "в ширину, раздаваясь в стороны"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
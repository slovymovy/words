    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1ec7bd4f-84eb-3e5d-b78b-32fdf081e6b8",
                    "sense_definition": "In or at no place; not anywhere.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "no_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The missing keys were <w>nowhere</w> to be found in the house.",
                            "target_lang_translations": {
                                "ru": "Потерянные ключи <w>нигде</w> в доме не удалось найти."
                            }
                        },
                        {
                            "text": "I looked for my friend, but he was <w>nowhere</w> in the room.",
                            "target_lang_translations": {
                                "ru": "Я искал своего друга, но его <w>нигде</w> не было в комнате."
                            }
                        }
                    ],
                    "antonyms": [
                        "everywhere"
                    ],
                    "common_phrases": [
                        "nowhere to be found",
                        "nowhere near",
                        "nowhere else"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни в каком месте; отсутствие местонахождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нигде",
                                "target_lang_sense_clarification": "in no place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a519134-cb7e-3a25-afcf-056cc09ebef8",
                    "sense_definition": "To no place; without a result or destination.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "no_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We drove around for hours, but the road seemed to go <w>nowhere</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ездили часами, но дорога, казалось, вела <w>никуда</w>."
                            }
                        },
                        {
                            "text": "This discussion is leading <w>nowhere</w>, so we should stop.",
                            "target_lang_translations": {
                                "ru": "Это обсуждение ведет <w>никуда</w>, поэтому нам следует остановиться."
                            }
                        }
                    ],
                    "antonyms": [
                        "everywhere"
                    ],
                    "common_phrases": [
                        "go nowhere",
                        "lead nowhere",
                        "get nowhere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ни в какое место; без результата или пункта назначения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никуда",
                                "target_lang_sense_clarification": "to no place"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ba8aa3d-95ab-3b9f-bf90-1ceda0031fe5",
                    "sense_definition": "A place that is remote, unknown, or non-existent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They live on a small farm in the middle of <w>nowhere</w>.",
                            "target_lang_translations": {
                                "ru": "Они живут на маленькой ферме в полной <w>глуши</w>."
                            }
                        },
                        {
                            "text": "The car suddenly appeared out of <w>nowhere</w> and almost hit us.",
                            "target_lang_translations": {
                                "ru": "Машина внезапно появилась из <w>ниоткуда</w> и чуть не сбила нас."
                            }
                        }
                    ],
                    "common_phrases": [
                        "middle of nowhere",
                        "out of nowhere",
                        "road to nowhere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, которое удалено, неизвестно или не существует."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниоткуда",
                                "target_lang_sense_clarification": "from no place (source)"
                            },
                            {
                                "target_lang_word": "глушь",
                                "target_lang_sense_clarification": "remote place"
                            },
                            {
                                "target_lang_word": "неизвестность",
                                "target_lang_sense_clarification": "unknown place"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b880067e-53b5-34a8-a6aa-0989662c9747",
                    "sense_definition": "Having no value, importance, or prospects.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt stuck in a <w>nowhere</w> job with no chance of promotion.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что застрял на <w>бесперспективной</w> работе без шансов на повышение."
                            }
                        },
                        {
                            "text": "She wanted to leave that <w>nowhere</w> town and see the world.",
                            "target_lang_translations": {
                                "ru": "Она хотела уехать из этого <w>захолустного</w> городка и повидать мир."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nowhere man",
                        "nowhere job"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий ценности, важности или перспектив."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесперспективный",
                                "target_lang_sense_clarification": "without prospects"
                            },
                            {
                                "target_lang_word": "захолустный",
                                "target_lang_sense_clarification": "remote/insignificant (of a place)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1421c71-ca9c-33e7-a8c1-77be13c01aa9",
                    "sense_definition": "Укрепление здоровья и повышение сопротивляемости организма неблагоприятным условиям, особенно холоду, путём систематических процедур.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Регулярная <w>закалка</w> холодной водой помогла мне укрепить иммунитет и перестать болеть.",
                            "target_lang_translations": {
                                "en": "Regular <w>hardening</w> with cold water helped me strengthen my immunity and stop getting sick."
                            }
                        },
                        {
                            "text": "В детском саду проводится <w>закалка</w> организма, чтобы дети росли здоровыми и крепкими.",
                            "target_lang_translations": {
                                "en": "Body <w>hardening</w> is practiced in the kindergarten so that children grow up healthy and strong."
                            }
                        }
                    ],
                    "synonyms": [
                        "закаливание"
                    ],
                    "common_phrases": [
                        "закалка организма",
                        "холодная закалка",
                        "спортивная закалка"
                    ],
                    "target_lang_definitions": {
                        "en": "Strengthening health and increasing the body's resistance to adverse conditions, especially cold, through systematic procedures."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hardening",
                                "target_lang_sense_clarification": "of the body/health"
                            },
                            {
                                "target_lang_word": "toughening",
                                "target_lang_sense_clarification": "making the body stronger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc5f8b20-75f9-39a8-adad-af3cf36ee9e3",
                    "sense_definition": "Технологический процесс нагревания и быстрого охлаждения материалов (металла, стекла) для придания им повышенной твёрдости и прочности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правильная <w>закалка</w> стали делает нож острым и долговечным.",
                            "target_lang_translations": {
                                "en": "Proper <w>hardening</w> of steel makes a knife sharp and durable."
                            }
                        },
                        {
                            "text": "На заводе установлена новая печь для <w>закалки</w> деталей двигателя.",
                            "target_lang_translations": {
                                "en": "A new furnace for the <w>hardening</w> of engine parts has been installed at the factory."
                            }
                        }
                    ],
                    "synonyms": [
                        "закаливание"
                    ],
                    "common_phrases": [
                        "закалка стали",
                        "печь для закалки",
                        "двойная закалка"
                    ],
                    "target_lang_definitions": {
                        "en": "A technological process of heating and rapidly cooling materials (metal, glass) to give them increased hardness and strength."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hardening",
                                "target_lang_sense_clarification": "of materials"
                            },
                            {
                                "target_lang_word": "tempering",
                                "target_lang_sense_clarification": "heat treatment process"
                            },
                            {
                                "target_lang_word": "quenching",
                                "target_lang_sense_clarification": "rapid cooling process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc7a69f0-0975-384b-8251-f59d5c1874e4",
                    "sense_definition": "Нравственная стойкость, выносливость и сила характера, приобретённые жизненным опытом или в борьбе с трудностями.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка — человек старой <w>закалки</w>, он привык к строгому порядку и дисциплине.",
                            "target_lang_translations": {
                                "en": "My grandfather is a man of the old <w>school</w>; he is used to strict order and discipline."
                            }
                        },
                        {
                            "text": "Суровая жизненная <w>закалка</w> научила её не сдаваться даже в самых сложных ситуациях.",
                            "target_lang_translations": {
                                "en": "Harsh life <w>tempering</w> taught her not to give up even in the most difficult situations."
                            }
                        }
                    ],
                    "synonyms": [
                        "закалённость"
                    ],
                    "common_phrases": [
                        "человек старой закалки",
                        "боевая закалка",
                        "жизненная закалка"
                    ],
                    "target_lang_definitions": {
                        "en": "Moral stamina, endurance, and strength of character acquired through life experience or in the struggle with difficulties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tempering",
                                "target_lang_sense_clarification": "of character"
                            },
                            {
                                "target_lang_word": "school",
                                "target_lang_sense_clarification": "idiomatic: old school"
                            },
                            {
                                "target_lang_word": "mould",
                                "target_lang_sense_clarification": "type of character"
                            }
                        ]
                    }
                }
            ]
        }
    ]

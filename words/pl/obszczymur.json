                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To nie był drobny pijaczek, ale wielki i groźny <w>obszczymur</w>.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b28c071-f30e-3c46-ab13-7d46f08cbe39",
                    "sense_definition": "Een symbool in de vorm van een V-tje (✓) om aan te geven dat iets gecontroleerd, correct of afgehandeld is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "checkmark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet een <w>vink</w> achter je naam als je aanwezig bent.",
                            "target_lang_translations": {
                                "en": "Put a <w>check mark</w> behind your name if you are present.",
                                "ru": "Поставьте <w>галочку</w> напротив своего имени, если вы присутствуете."
                            }
                        },
                        {
                            "text": "Als je de taak hebt afgerond, mag je een <w>vink</w> plaatsen.",
                            "target_lang_translations": {
                                "en": "When you have completed the task, you may place a <w>tick</w>.",
                                "ru": "Когда закончишь задание, можешь поставить <w>галочку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een vink zetten",
                        "groene vink"
                    ],
                    "target_lang_definitions": {
                        "en": "A symbol in the shape of a V (✓) to indicate that something has been checked, correct, or handled.",
                        "ru": "Символ в форме V (✓), указывающий, что что-то проверено, верно или выполнено."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "check mark",
                                "target_lang_sense_clarification": "symbol indicating correctness or completion"
                            },
                            {
                                "target_lang_word": "tick",
                                "target_lang_sense_clarification": "symbol (✓)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "галочка",
                                "target_lang_sense_clarification": "знак проверки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1c85a44-b634-39f0-acf3-b3442c51ab4b",
                    "sense_definition": "Een veelvoorkomende zangvogel (*Fringilla coelebs*) uit de familie van de vinkachtigen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>vink</w> zong een vrolijk liedje in de hoge boom.",
                            "target_lang_translations": {
                                "en": "The <w>chaffinch</w> sang a cheerful song in the high tree.",
                                "ru": "<w>Зяблик</w> пел веселую песенку на высоком дереве."
                            }
                        },
                        {
                            "text": "In onze tuin zit vaak een <w>vink</w> op het voederhuisje.",
                            "target_lang_translations": {
                                "en": "There is often a <w>finch</w> on the bird feeder in our garden.",
                                "ru": "В нашем саду на кормушке часто сидит <w>зяблик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "boekvink",
                        "botvink"
                    ],
                    "common_phrases": [
                        "bonte vink",
                        "blinde vink"
                    ],
                    "target_lang_definitions": {
                        "en": "A common songbird (Fringilla coelebs) from the finch family.",
                        "ru": "Распространенная певчая птица (*Fringilla coelebs*) из семейства вьюрковых."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chaffinch",
                                "target_lang_sense_clarification": "specific bird species (Fringilla coelebs)"

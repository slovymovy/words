{
    "word_family": [
        "druh",
        "druhna",
        "drużynowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc67b2e9-cdab-325d-8b72-2d375ecd1bba",
                    "sense_definition": "Grupa sportowców grających wspólnie w jednej dyscyplinie lub zespół ludzi wykonujących razem określone zadanie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_sport_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza <w>drużyna</w> wygrała wczorajszy mecz piłki nożnej.",
                            "target_lang_translations": {
                                "ru": "Наша <w>команда</w> выиграла вчерашний футбольный матч."
                            }
                        },
                        {
                            "text": "Mediolańska <w>drużyna</w> pokonała piłkarzy z Neapolu dopiero w dogrywce.",
                            "target_lang_translations": {
                                "ru": "Миланская <w>команда</w> победила футболистов из Неаполя только в дополнительное время."
                            }
                        }
                    ],
                    "synonyms": [
                        "zespół",
                        "ekipa",
                        "grupa"
                    ],
                    "antonyms": [
                        "osoba"
                    ],
                    "common_phrases": [
                        "drużyna piłkarska",
                        "kapitan drużyny",
                        "drużyna narodowa",
                        "duch drużyny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа спортсменов, выступающих вместе в одной дисциплине, или коллектив людей, выполняющих общую задачу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "спортивная или рабочая группа"
                            },
                            {
                                "target_lang_word": "коллектив",
                                "target_lang_sense_clarification": "группа людей, работающих вместе"
                            },
                            {
                                "target_lang_word": "сборная",
                                "target_lang_sense_clarification": "национальная спортивная команда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a265ca32-aa87-37c5-82f8-f9146d2be340",
                    "sense_definition": "Podstawowa jednostka organizacyjna w harcerstwie, składająca się z kilku zastępów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_scouting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wstąpiłem do <w>drużyny</w> w pierwszej klasie gimnazjum.",
                            "target_lang_translations": {
                                "ru": "Я вступил в <w>отряд</w> в первом классе гимназии."
                            }
                        },
                        {
                            "text": "Cała <w>drużyna</w> harcerska spotkała się na zbiórce w lesie.",
                            "target_lang_translations": {
                                "ru": "Весь скаутский <w>отряд</w> встретился на сборе в лесу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drużyna harcerska",
                        "zbiórka drużyny",
                        "prowadzić drużynę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная организационная единица в скаутском движении, состоящая из нескольких звеньев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отряд",
                                "target_lang_sense_clarification": "скаутское подразделение"
                            },
                            {
                                "target_lang_word": "дружина",
                                "target_lang_sense_clarification": "в пионерской или специфической скаутской терминологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de4557de-854d-3e09-8000-385840823fdf",
                    "sense_definition": "Grupa przyjaciół lub towarzyszy połączona wspólną ideą, celem lub więzami przyjaźni.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wesoła <w>drużyna</w> wędrowała razem przez góry.",
                            "target_lang_translations": {
                                "ru": "Веселая <w>компания</w> вместе бродила по горам."
                            }
                        },
                        {
                            "text": "To była zgrana <w>drużyna</w>, na której zawsze można było polegać.",
                            "target_lang_translations": {
                                "ru": "Это была сплоченная <w>команда</w>, на которую всегда можно было положиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "kompania",
                        "towarzystwo",
                        "gromada"
                    ],
                    "common_phrases": [
                        "zgrana drużyna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа друзей или товарищей, объединенных общей идеей, целью или узами дружбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "группа друзей"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "сплоченная группа людей (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb805ceb-a975-3b36-832b-47c187d33418",
                    "sense_definition": "Najmniejszy pododdział taktyczny w wojsku, wchodzący zazwyczaj w skład plutonu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Drużyna</w> bojowa wzorowo wypełniła swoje zadanie na poligonie.",
                            "target_lang_translations": {
                                "ru": "Боевое <w>отделение</w> образцово выполнило свою задачу на полигоне."
                            }
                        },
                        {
                            "text": "Sierżant wydał rozkaz swojej <w>drużynie</w> piechoty.",
                            "target_lang_translations": {
                                "ru": "Сержант отдал приказ своему пехотному <w>отделению</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dowódca drużyny",
                        "drużyna piechoty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наименьшее тактическое подразделение в армии, обычно входящее в состав взвода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделение",
                                "target_lang_sense_clarification": "воинское подразделение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "742fcf18-97d7-30e7-8f83-ca7516c7549e",
                    "sense_definition": "Przyboczna gwardia władcy w średniowiecznej Słowiańszczyźnie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Książę wyruszył na wojnę wraz ze swoją wierną <w>drużyną</w>.",
                            "target_lang_translations": {
                                "ru": "Князь отправился на войну вместе со своей верной <w>дружиной</w>."
                            }
                        },
                        {
                            "text": "Wojowie z <w>drużyny</w> przysięgali wierność swojemu władcy.",
                            "target_lang_translations": {
                                "ru": "Воины из <w>дружины</w> присягали на верность своему правителю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drużyna książęca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личная гвардия правителя в средневековых славянских землях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дружина",
                                "target_lang_sense_clarification": "историческое войско князя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
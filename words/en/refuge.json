                },
                {
                    "sense_id": "014d961d-3a11-3ee4-ae41-5772afdc5696",
                    "sense_definition": "To return to a central place or shelter, typically used of animals in biology.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The biologists observed the monkeys <w>refuging</w> near the river every evening.",
                            "target_lang_translations": {
                                "ru": "Биологи наблюдали, как обезьяны каждый вечер <w>укрывались</w> у реки."
                            }
                        },
                        {
                            "text": "Certain species are known for <w>refuging</w> in caves during the heat of the day.",
                            "target_lang_translations": {
                                "ru": "Известно, что некоторые виды <w>укрываются</w> в пещерах в разгар дневной жары."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical usage in ethology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвращаться в центральное место или укрытие, обычно используется в биологии животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрываться",
                                "target_lang_sense_clarification": "прятаться в безопасном месте"
                            },
                            {
                                "target_lang_word": "находить убежище",
                                "target_lang_sense_clarification": "возвращаться в укрытие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

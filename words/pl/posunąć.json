{
    "word_family": [
        "posuwać",
        "posunięcie",
        "posuw",
        "posuwisty"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c39e137b-abb4-3c58-864d-9a5ca0fe4146",
                    "sense_definition": "Zmienić położenie przedmiotu lub osoby, przesuwając je w określonym kierunku (zwykle do przodu lub w bok).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <posunąć> ten ciężki stół, żebyśmy mogli umyć podłogę.",
                            "target_lang_translations": {
                                "ru": "Ты должен <подвинуть> этот тяжелый стол, чтобы мы могли помыть пол."
                            }
                        },
                        {
                            "text": "Czy mógłbyś <posunąć> krzesło bliżej biurka?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <подвинуть> стул поближе к письменному столу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "przesunąć",
                        "pchnąć"
                    ],
                    "antonyms": [
                        "cofnąć"
                    ],
                    "common_phrases": [
                        "posunąć stół",
                        "posunąć krzesło",
                        "posunąć szafę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить положение предмета или человека, перемещая их в определенном направлении (обычно вперед или в сторону)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвинуть",
                                "target_lang_sense_clarification": "переместить на небольшое расстояние"
                            },
                            {
                                "target_lang_word": "передвинуть",
                                "target_lang_sense_clarification": "сменить местоположение предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6651212-c5a5-3d06-85dc-90a299de683b",
                    "sense_definition": "Zrobić komuś miejsce, przesuwając własne ciało w bok (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę, <posuń> się trochę, żebym też mógł usiąść na ławce.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <подвинься> немного, чтобы я тоже мог сесть на скамейку."
                            }
                        },
                        {
                            "text": "Kiedy pasażer się <posunął>, zrobiło się miejsce dla starszej pani.",
                            "target_lang_translations": {
                                "ru": "Когда пассажир <подвинулся>, освободилось место для пожилой женщины."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesunąć się"
                    ],
                    "common_phrases": [
                        "posuń się",
                        "posunąć się na ławce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освободить место для кого-то, переместив собственное тело в сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвинуться",
                                "target_lang_sense_clarification": "освободить место, переместившись"
                            },
                            {
                                "target_lang_word": "потесниться",
                                "target_lang_sense_clarification": "сдвинуться, чтобы дать место другому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95d6f086-566b-3cb9-b887-0edc82b146fb",
                    "sense_definition": "Sprawić, że jakaś sprawa, praca lub proces rozwinie się, przyspieszy lub przejdzie do kolejnego etapu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "progress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To odkrycie pozwoli nam znacznie <posunąć> badania naukowe do przodu.",
                            "target_lang_translations": {
                                "ru": "Это открытие позволит нам значительно <продвинуть> научные исследования вперед."
                            }
                        },
                        {
                            "text": "Mamy nadzieję <posunąć> negocjacje i podpisać umowę w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся <продвинуть> переговоры и подписать соглашение на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyspieszyć",
                        "pchnąć"
                    ],
                    "antonyms": [
                        "opóźnić",
                        "wstrzymać"
                    ],
                    "common_phrases": [
                        "posunąć sprawy naprzód",
                        "posunąć prace",
                        "posunąć śledztwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить дело, работу или процесс развиваться, ускоряться или переходить на следующий этап."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвинуть",
                                "target_lang_sense_clarification": "способствовать развитию дела или процесса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eed74529-883e-3145-a0fa-8f08957e30c3",
                    "sense_definition": "Zrobić coś skrajnego, ryzykownego lub nieetycznego, przekraczając zwykłe granice zachowania (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior_limits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie sądziłem, że on może <posunąć> się do kłamstwa, by zdobyć te pieniądze.",
                            "target_lang_translations": {
                                "ru": "Я не думал, что он может <дойти> до лжи, чтобы получить эти деньги."
                            }
                        },
                        {
                            "text": "W swojej desperacji był gotów <posunąć> się do wszystkiego.",
                            "target_lang_translations": {
                                "ru": "В своем отчаянии он был готов <пойти> на всё."
                            }
                        }
                    ],
                    "common_phrases": [
                        "posunąć się za daleko",
                        "posunąć się do ostateczności",
                        "posunąć się do kłamstwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить что-то крайнее, рискованное или неэтичное, переходя обычные границы поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пойти (на)",
                                "target_lang_sense_clarification": "решиться на рискованный шаг"
                            },
                            {
                                "target_lang_word": "дойти (до)",
                                "target_lang_sense_clarification": "достичь крайности в поведении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d29c13f2-7808-31ed-97cd-f8f6c4ae0a18",
                    "sense_definition": "Odbyć stosunek seksualny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wulgarnie chwalił się kolegom, że ją wczoraj <posunął>.",
                            "target_lang_translations": {
                                "ru": "Он вульгарно хвастался коллегам, что вчера её <трахнул>."
                            }
                        },
                        {
                            "text": "To słowo oznacza, że ktoś kogoś <posunął>, ale jest bardzo niegrzeczne.",
                            "target_lang_translations": {
                                "ru": "Это слово означает, что кто-то кого-то <трахнул>, но оно очень грубое."
                            }
                        }
                    ],
                    "synonyms": [
                        "bzyknąć",
                        "rżnąć",
                        "dymać",
                        "zaliczyć"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить половой акт (вульгарно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трахнуть",
                                "target_lang_sense_clarification": "вульгарное обозначение полового акта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
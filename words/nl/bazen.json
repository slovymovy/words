    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "078f4b8e-5293-327d-9616-1ddfb90ecae1",
                    "sense_definition": "Meervoud van <w>baas</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "baas_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bazen</w> van het bedrijf hebben gisteren lang vergaderd.",
                            "target_lang_translations": {
                                "en": "The <w>bosses</w> of the company had a long meeting yesterday.",
                                "ru": "<w>Начальники</w> компании вчера долго совещались."
                            }
                        },
                        {
                            "text": "Niet alle <w>bazen</w> luisteren goed naar hun werknemers.",
                            "target_lang_translations": {
                                "en": "Not all <w>bosses</w> listen well to their employees.",
                                "ru": "Не все <w>боссы</w> хорошо слушают своих сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "chefs",
                        "leidinggevenden",
                        "werkgevers"
                    ],
                    "antonyms": [
                        "ondergeschikten",
                        "werknemers",
                        "knechten"
                    ],
                    "common_phrases": [
                        "grote bazen",
                        "hoge bazen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of 'baas' (boss).",
                        "ru": "Множественное число от слова \"baas\" (начальник, руководитель)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bosses",
                                "target_lang_sense_clarification": "plural of boss"
                            },
                            {
                                "target_lang_word": "chiefs",
                                "target_lang_sense_clarification": "leaders or heads of a group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "начальники",
                                "target_lang_sense_clarification": "руководители"
                            },
                            {
                                "target_lang_word": "боссы",
                                "target_lang_sense_clarification": "разг. начальники"
                            },
                            {
                                "target_lang_word": "шефы",
                                "target_lang_sense_clarification": "разг. руководители"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "79af76bf-1263-3756-ac86-d1abe899bf4d",
                    "sense_definition": "Zich dominant gedragen en anderen commanderen alsof men de leiding heeft.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "baas_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet niet zo tegen mij <w>bazen</w>, want ik bepaal zelf wat ik doe.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>boss</w> me <w>around</w> like that, because I decide for myself what I do.",
                                "ru": "Не надо мной так <w>командовать</w>, я сам решаю, что мне делать."
                            }
                        },
                        {
                            "text": "Hij loopt de hele dag te <w>bazen</w> over zijn jongere broertje.",
                            "target_lang_translations": {
                                "en": "He walks around <w>bossing</w> his younger brother <w>around</w> all day.",
                                "ru": "Он весь день ходит и <w>распоряжается</w> своим младшим братом."
                            }
                        }
                    ],
                    "synonyms": [
                        "commanderen",
                        "de baas spelen"
                    ],
                    "antonyms": [
                        "gehoorzamen",
                        "volgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To behave dominantly and command others as if in charge.",
                        "ru": "Вести себя властно, командовать другими, изображать из себя начальника."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boss around",
                                "target_lang_sense_clarification": "to tell people what to do"
                            },
                            {
                                "target_lang_word": "domineer",
                                "target_lang_sense_clarification": "to exercise control arrogantly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "командовать",
                                "target_lang_sense_clarification": "раздавать приказы"
                            },
                            {
                                "target_lang_word": "распоряжаться",
                                "target_lang_sense_clarification": "указывать, что делать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b688d478-3057-3e49-bc88-1373f419e2e8",
                    "sense_definition": "Wartaal uitslaan of onzin praten, vaak als gevolg van koorts of verwarring.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "babble_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zieke patiënt lag te <w>bazen</w> in zijn slaap.",
                            "target_lang_translations": {
                                "en": "The sick patient lay <w>raving</w> in his sleep.",
                                "ru": "Больной пациент лежал и <w>бредил</w> во сне."
                            }
                        },
                        {
                            "text": "Luister niet naar hem, hij staat maar wat te <w>bazen</w>.",
                            "target_lang_translations": {
                                "en": "Don't listen to him, he's just standing there <w>babbling</w>.",
                                "ru": "Не слушай его, он просто <w>несет чушь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ijlen",
                        "raaskallen",
                        "bazelen",
                        "wauwelen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often considered archaic or dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To speak nonsense or talk incoherently, often due to fever or confusion.",
                        "ru": "Говорить бессмыслицу или бредить (устаревшее или диалектное значение, схожее с \"bazelen\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rave",
                                "target_lang_sense_clarification": "to talk wildly, as in delirium"
                            },
                            {
                                "target_lang_word": "babble",
                                "target_lang_sense_clarification": "to talk foolishly or inarticulately"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бредить",
                                "target_lang_sense_clarification": "говорить бессвязно (в болезни)"
                            },
                            {
                                "target_lang_word": "нести чушь",
                                "target_lang_sense_clarification": "говорить глупости"
                            },
                            {
                                "target_lang_word": "пустословить",
                                "target_lang_sense_clarification": "болтать попусту"
                            }
                        ]
                    }
                }
            ]
        }
    ]

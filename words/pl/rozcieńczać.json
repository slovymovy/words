    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3091921-b432-37a5-a4d2-d8e3ec8b98b8",
                    "sense_definition": "Zmniejszać stężenie cieczy lub roztworu poprzez dodanie do niego innej substancji, najczęściej wody lub rozpuszczalnika.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "zmiana_stezenia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>rozcieńczać</w> ten sok wodą, ponieważ jest zbyt słodki.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>разбавлять</w> этот сок водой, так как он слишком сладкий."
                            }
                        },
                        {
                            "text": "Malarz zaczął <w>rozcieńczać</w> gęstą farbę, aby łatwiej rozprowadzić ją na ścianie.",
                            "target_lang_translations": {
                                "ru": "Маляр начал <w>разводить</w> густую краску, чтобы легче распределить её по стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozwadniać",
                        "rozrabiać"
                    ],
                    "antonyms": [
                        "zagęszczać",
                        "koncentrować"
                    ],
                    "common_phrases": [
                        "rozcieńczać wodą",
                        "rozcieńczać kwas",
                        "rozcieńczać farbę",
                        "rozcieńczać alkohol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшать концентрацию жидкости или раствора путем добавления другого вещества, чаще всего воды или растворителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбавлять",
                                "target_lang_sense_clarification": "делать жидкость менее концентрированной"
                            },
                            {
                                "target_lang_word": "разводить",
                                "target_lang_sense_clarification": "смешивать с водой или растворителем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50c26c91-5721-3c5c-9bfe-98da8c74968b",
                    "sense_definition": "Stawać się mniej stężonym, tracić gęstość lub intensywność (zazwyczaj o cieczach mieszających się z inną substancją).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "zmiana_stezenia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wlana do rzeki substancja zaczęła szybko <w>rozcieńczać się</w> w nurcie wody.",
                            "target_lang_translations": {
                                "ru": "Вылитое в реку вещество начало быстро <w>растворяться</w> в потоке воды."
                            }
                        },
                        {
                            "text": "Zapach perfum powoli <w>rozcieńczał się</w> w powietrzu, stając się mniej wyczuwalnym.",
                            "target_lang_translations": {
                                "ru": "Запах духов медленно <w>рассеивался</w> в воздухе, становясь менее ощутимым."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzednąć"
                    ],
                    "antonyms": [
                        "gęstnieć"
                    ],
                    "common_phrases": [
                        "rozcieńczać się w wodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться менее концентрированным, терять густоту или интенсивность (обычно о жидкостях, смешивающихся с другим веществом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбавляться",
                                "target_lang_sense_clarification": "становиться менее насыщенным"
                            },
                            {
                                "target_lang_word": "растворяться",
                                "target_lang_sense_clarification": "смешиваться с жидкостью до исчезновения видимых границ"
                            },
                            {
                                "target_lang_word": "рассеиваться",
                                "target_lang_sense_clarification": "о запахах или газах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "045df24b-ac54-350c-b096-23e732a724db",
                    "sense_definition": "Temat dyskusji lub możliwość zrobienia czegoś (występuje głównie w stałych zwrotach zaprzeczających lub pytających).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "topic_discussion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie ma <w>mowy</w>, żebym wyszedł z domu w taką burzę.",
                            "target_lang_translations": {
                                "ru": "Не может быть и <w>речи</w>, чтобы я вышел из дома в такую грозу."
                            }
                        },
                        {
                            "text": "O czym była <w>mowa</w> na wczorajszym zebraniu?",
                            "target_lang_translations": {
                                "ru": "О чём шла <w>речь</w> на вчерашнем собрании?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie ma mowy",
                        "o czym mowa",
                        "być mowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тема обсуждения или возможность сделать что-то (встречается в устойчивых выражениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "речь",
                                "target_lang_sense_clarification": "в выражениях «о чём речь», «не может быть и речи»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "701dfc71-cac5-3b3d-b19a-2b50c071b948",
                    "sense_definition": "Publiczne wystąpienie ustne na określony temat, skierowane do grupy ludzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "formal_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent wygłosił ważną <w>mowę</w> dotyczącą bezpieczeństwa kraju.",
                            "target_lang_translations": {
                                "ru": "Президент произнёс важную <w>речь</w>, касающуюся безопасности страны."
                            }
                        },
                        {
                            "text": "Jego <w>mowa</w> pożegnalna wzruszyła wszystkich gości do łez.",
                            "target_lang_translations": {
                                "ru": "Его прощальная <w>речь</w> тронула всех гостей до слёз."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemówienie",
                        "przemowa",
                        "wymowa"
                    ],
                    "common_phrases": [
                        "wygłosić mowę",
                        "mowa pożegnalna",
                        "krótka mowa",
                        "mowa powitalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное устное выступление на определённую тему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "речь",
                                "target_lang_sense_clarification": "публичное выступление"
                            },
                            {
                                "target_lang_word": "выступление",
                                "target_lang_sense_clarification": "доклад, обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbe89141-db21-33ac-872c-5a81019700aa",
                    "sense_definition": "Fizyczna zdolność człowieka do wydawania artykułowanych dźwięków i słów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wypadku pacjent na kilka dni stracił <w>mowę</w>.",
                            "target_lang_translations": {
                                "ru": "После аварии пациент на несколько дней потерял <w>речь</w>."
                            }
                        },
                        {
                            "text": "Ze zdziwienia odebrało mi <w>mowę</w>, gdy zobaczyłem ten prezent.",
                            "target_lang_translations": {
                                "ru": "От удивления у меня отняло <w>речь</w>, когда я увидел этот подарок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stracić mowę",
                        "odzyskać mowę",
                        "odebrać mowę",
                        "dar mowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая способность человека произносить звуки и слова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "речь",
                                "target_lang_sense_clarification": "способность говорить"
                            },
                            {
                                "target_lang_word": "дар речи",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "786dda3f-b186-339b-8725-f37ed6b9cdb2",
                    "sense_definition": "Sposób komunikacji bez użycia słów, wykorzystujący gesty, mimikę i postawę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "non_verbal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mowa</w> ciała często zdradza prawdziwe emocje człowieka.",
                            "target_lang_translations": {
                                "ru": "<w>Язык</w> тела часто выдаёт истинные эмоции человека."
                            }
                        },
                        {
                            "text": "Podczas rozmowy o pracę ważna jest pewna siebie <w>mowa</w> ciała.",
                            "target_lang_translations": {
                                "ru": "Во время собеседования важен уверенный <w>язык</w> тела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mowa ciała",
                        "mowa gestów",
                        "mowa oczu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ общения без слов, использующий жесты, мимику и позу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "в выражении «язык тела»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b5fbd6a-388a-3941-a013-17255445d297",
                    "sense_definition": "System porozumiewania się używany przez określoną grupę ludzi; język.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "language_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci emigrantów powinny uczyć się <w>mowy</w> swoich przodków.",
                            "target_lang_translations": {
                                "ru": "Дети эмигрантов должны учить <w>язык</w> своих предков."
                            }
                        },
                        {
                            "text": "W dawnych czasach ta <w>mowa</w> była używana na dworach królewskich.",
                            "target_lang_translations": {
                                "ru": "В давние времена этот <w>язык</w> использовался при королевских дворах."
                            }
                        }
                    ],
                    "synonyms": [
                        "język"
                    ],
                    "common_phrases": [
                        "mowa ojczysta",
                        "mowa potoczna",
                        "mowa wiązana",
                        "części mowy"

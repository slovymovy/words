{
    "word_family": [
        "rehearse",
        "rehearser"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97fb3995-384e-37df-99b3-ba84b7395460",
                    "sense_definition": "The act of practicing a play, concert, or other performance before presenting it to an audience.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "practice_preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actors gathered on stage for a final <w>rehearsal</w> before opening night.",
                            "target_lang_translations": {
                                "ru": "Актёры собрались на сцене для финальной <w>репетиции</w> перед премьерой."
                            }
                        },
                        {
                            "text": "We need a dress <w>rehearsal</w> to ensure the costumes and lighting are perfect.",
                            "target_lang_translations": {
                                "ru": "Нам нужна генеральная <w>репетиция</w>, чтобы убедиться, что костюмы и освещение идеальны."
                            }
                        }
                    ],
                    "synonyms": [
                        "practice",
                        "run-through",
                        "dry run"
                    ],
                    "antonyms": [
                        "performance",
                        "premiere"
                    ],
                    "common_phrases": [
                        "dress rehearsal",
                        "wedding rehearsal",
                        "in rehearsal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс подготовки пьесы, концерта или другого представления перед показом публике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репетиция",
                                "target_lang_sense_clarification": "спектакля, концерта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf9e8880-9d3e-312e-a1ac-46f4882ce0e8",
                    "sense_definition": "A preparatory activity or event that serves as practice for a future situation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "practice_preparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mock debate was a useful <w>rehearsal</w> for the actual negotiation next week.",
                            "target_lang_translations": {
                                "ru": "Пробные дебаты стали полезной <w>репетицией</w> перед настоящими переговорами на следующей неделе."
                            }
                        },
                        {
                            "text": "Many experts view these small conflicts as a <w>rehearsal</w> for a much larger war.",
                            "target_lang_translations": {
                                "ru": "Многие эксперты рассматривают эти небольшие конфликты как <w>репетицию</w> гораздо более крупной войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "trial",
                        "preparation",
                        "warm-up"
                    ],
                    "antonyms": [
                        "real thing"
                    ],
                    "common_phrases": [
                        "rehearsal for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготовительное мероприятие или событие, служащее практикой для будущей ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репетиция",
                                "target_lang_sense_clarification": "перен. подготовка, проба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca233adb-58fc-34e6-99cd-b9e8523038de",
                    "sense_definition": "The act of repeating, reciting, or recounting a story, list, or set of facts.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer's constant <w>rehearsal</w> of the evidence began to bore the jury.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>повторение</w> улик адвокатом начало утомлять присяжных."
                            }
                        },
                        {
                            "text": "Memory retention improves significantly with the mental <w>rehearsal</w> of new information.",
                            "target_lang_translations": {
                                "ru": "Запоминание значительно улучшается при мысленном <w>проговаривании</w> новой информации."
                            }
                        }
                    ],
                    "synonyms": [
                        "repetition",
                        "recital",
                        "recounting"
                    ],
                    "common_phrases": [
                        "rehearsal of facts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт повторения, пересказа или перечисления истории, списка или набора фактов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повторение",
                                "target_lang_sense_clarification": "информации, фактов"
                            },
                            {
                                "target_lang_word": "проговаривание",
                                "target_lang_sense_clarification": "мысленное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
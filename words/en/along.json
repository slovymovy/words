                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вме́сте",
                                "target_lang_sense_clarification": "сообща, совместно"
                            },
                            {
                                "target_lang_word": "с собо́й",
                                "target_lang_sense_clarification": "взять кого-то или что-то"

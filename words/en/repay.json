    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "35920641-90b8-39fe-ae12-dc4fb1674df8",
                    "sense_definition": "To give back money that has been borrowed or owed to someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took him several years to fully <w>repay</w> his student loans.",
                            "target_lang_translations": {
                                "ru": "Ему потребовалось несколько лет, чтобы полностью <w>выплатить</w> свои студенческие кредиты."
                            }
                        },
                        {
                            "text": "You must <w>repay</w> the debt to the bank by the end of the month.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>вернуть</w> долг банку до конца месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay back in all senses"
                    ],
                    "common_phrases": [
                        "repay a loan",
                        "repay a debt",
                        "repay in full",
                        "repay money"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть деньги, которые были взяты в долг или которые вы должны кому-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвращать",
                                "target_lang_sense_clarification": "общий термин для возврата долга"
                            },
                            {
                                "target_lang_word": "выплачивать",
                                "target_lang_sense_clarification": "обычно о кредитах или полной сумме"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84fdafdd-8294-35fe-ba43-494407878b7d",
                    "sense_definition": "To do something in return for a favor, kindness, or a bad action received from someone else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wanted to find a way to <w>repay</w> him for his generous hospitality.",
                            "target_lang_translations": {
                                "ru": "Она хотела найти способ <w>отблагодарить</w> его за щедрое гостеприимство."
                            }
                        },
                        {
                            "text": "He swore that he would <w>repay</w> the insult he suffered at the meeting.",
                            "target_lang_translations": {
                                "ru": "Он поклялся, что <w>отплатит</w> за оскорбление, которое он перенес на собрании."
                            }
                        }
                    ],
                    "synonyms": [
                        "pay back in all senses"
                    ],
                    "common_phrases": [
                        "repay a favor",
                        "repay kindness",
                        "repay hospitality",
                        "repay with interest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то в ответ на услугу, доброту или плохой поступок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отплатить",
                                "target_lang_sense_clarification": "совершить ответное действие (как хорошее, так и плохое)"
                            },
                            {
                                "target_lang_word": "отблагодарить",
                                "target_lang_sense_clarification": "ответить добром на услугу"
                            },
                            {
                                "target_lang_word": "вознаградить",
                                "target_lang_sense_clarification": "дать что-то ценное в ответ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "499f85cb-d795-34b2-82c6-cfb181e1117e",
                    "sense_definition": "To provide a result or benefit that makes an effort, action, or study worthwhile.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "worth_effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This complex novel will <w>repay</w> careful reading and close attention.",
                            "target_lang_translations": {
                                "ru": "Этот сложный роман <w>вознаградит</w> за внимательное чтение и пристальное внимание."
                            }
                        },
                        {
                            "text": "The beautiful view from the summit will <w>repay</w> the difficult climb.",
                            "target_lang_translations": {
                                "ru": "Прекрасный вид с вершины <w>окупит</w> трудный подъем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "repay the effort",
                        "repay investigation",
                        "repay study"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принести результат или пользу, которые делают усилия или действия оправданными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окупать",
                                "target_lang_sense_clarification": "делать усилия не напрасными"
                            },
                            {
                                "target_lang_word": "вознаграждать",
                                "target_lang_sense_clarification": "приносить удовлетворение или пользу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b58fb356-1e17-3e82-ac2f-6804e9355ea9",
                    "sense_definition": "To cover a seam or surface, especially on a ship, with tar or pitch again to make it waterproof.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailors had to <w>repay</w> the deck seams before the storm arrived.",
                            "target_lang_translations": {
                                "ru": "Морякам пришлось <w>заново просмолить</w> палубные швы до прихода шторма."
                            }
                        },
                        {
                            "text": "We need to <w>repay</w> the hull to ensure the boat remains watertight.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>переконопатить</w> корпус, чтобы лодка оставалась водонепроницаемой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторно покрыть шов или поверхность (особенно на корабле) смолой для водонепроницаемости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заново смолить",
                                "target_lang_sense_clarification": "покрывать смолой"
                            },
                            {
                                "target_lang_word": "переконопатить",
                                "target_lang_sense_clarification": "заделывать щели паклей или варом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
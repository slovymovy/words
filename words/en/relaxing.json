            "senses": [
                {
                    "sense_id": "bb021cdf-2560-35a2-82ec-9e9ee31decb1",
                    "sense_definition": "Conducive to relaxation; helping one to relax.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We spent a very <w>relaxing</w> afternoon sitting by the river.",
                            "target_lang_translations": {
                                "ru": "Мы провели очень <w>расслабляющий</w> день, сидя у реки."
                            }
                        },
                        {
                            "text": "Soft lighting creates a <w>relaxing</w> atmosphere in the living room.",
                            "target_lang_translations": {
                                "ru": "Мягкое освещение создает <w>расслабляющую</w> атмосферу в гостиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "calming",
                        "soothing",
                        "restful",
                        "peaceful"
                    ],
                    "antonyms": [
                        "stressful",
                        "tense"
                    ],
                    "common_phrases": [
                        "relaxing atmosphere",
                        "relaxing holiday",
                        "relaxing music",
                        "very relaxing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способствующий отдыху и снятию напряжения; помогающий расслабиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расслабляющий",
                                "target_lang_sense_clarification": "вызывающий состояние покоя и отдыха"
                            },
                            {
                                "target_lang_word": "успокаивающий",
                                "target_lang_sense_clarification": "снимающий волнение или стресс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d4d5975b-e62b-3fcb-a952-76033b11a4d4",
                    "sense_definition": "Present participle and gerund of <w>relax</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>relaxing</w> on the sofa with a good book.",
                            "target_lang_translations": {
                                "ru": "Она <w>расслабляется</w> на диване с хорошей книгой."
                            }
                        },
                        {
                            "text": "After a long week, I enjoy <w>relaxing</w> at home with my family.",
                            "target_lang_translations": {
                                "ru": "После долгой недели мне нравится <w>отдыхать</w> дома с семьей."
                            }
                        }
                    ],
                    "synonyms": [
                        "resting",
                        "unwinding",
                        "chilling out"
                    ],
                    "antonyms": [
                        "working",
                        "stressing"
                    ],
                    "common_phrases": [
                        "relaxing at home",
                        "just relaxing",
                        "relaxing after work"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола \"relax\" (отдыхать, расслабляться), обозначающая действие в процессе или само занятие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдыхать",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07254513-eb41-37db-a95a-0b73dd0eeac0",
                    "sense_definition": "Third-person singular simple present indicative form of <w>let</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "let_verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>lets</w> her children play in the park until sunset.",
                            "target_lang_translations": {
                                "ru": "Она <w>разрешает</w> своим детям играть в парке до заката."
                            }
                        },
                        {
                            "text": "He never <w>lets</w> anyone borrow his car because he is very careful.",
                            "target_lang_translations": {
                                "ru": "Он никогда никому не <w>позволяет</w> брать свою машину, потому что он очень осторожен."
                            }
                        }
                    ],
                    "synonyms": [
                        "allows",
                        "permits",
                        "authorizes"
                    ],
                    "antonyms": [
                        "forbids",
                        "prohibits",
                        "prevents"
                    ],
                    "common_phrases": [
                        "lets go",
                        "lets down",
                        "lets in",
                        "lets off"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "third-person singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица единственного числа глагола let (позволять, разрешать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволяет",
                                "target_lang_sense_clarification": "дает возможность что-то сделать"
                            },
                            {
                                "target_lang_word": "разрешает",
                                "target_lang_sense_clarification": "дает официальное или формальное согласие"
                            },
                            {
                                "target_lang_word": "пускает",
                                "target_lang_sense_clarification": "разговорное; позволяет войти или сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e2a43bb-7511-3759-840a-31ca09c8da77",
                    "sense_definition": "Misspelling of <w>let's</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "misspelling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Come on, <w>lets</w> go to the cinema before the movie starts.",
                            "target_lang_translations": {
                                "ru": "Пошли, <w>давай</w> сходим в кино, пока фильм не начался."
                            }
                        },
                        {
                            "text": "If everyone is ready, <w>lets</w> begin the meeting immediately.",
                            "target_lang_translations": {
                                "ru": "Если все готовы, <w>давайте</w> начнем собрание немедленно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "misspelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное ошибочное написание сокращения let's (давай, давайте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "побуждение к действию (обращение на «ты»)"
                            },
                            {
                                "target_lang_word": "давайте",
                                "target_lang_sense_clarification": "побуждение к действию (обращение на «вы» или к группе)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66bdb972-4c73-350f-a3d7-dd270c7a53d0",
                    "sense_definition": "Plural of <w>let</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "let_noun_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The real estate agency manages several holiday <w>lets</w> along the coast.",
                            "target_lang_translations": {
                                "ru": "Агентство недвижимости управляет несколькими <w>домами для сдачи в аренду</w> на побережье."
                            }
                        },
                        {
                            "text": "The referee called two <w>lets</w> during the first set of the tennis match.",
                            "target_lang_translations": {
                                "ru": "Судья объявил две <w>переигровки</w> во время первого сета теннисного матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "rentals",
                        "leases"
                    ],
                    "common_phrases": [
                        "holiday lets",
                        "short-term lets"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного let (сдача в аренду или переигровка в спорте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдача в аренду",
                                "target_lang_sense_clarification": "недвижимость, сдаваемая на короткий срок"
                            },
                            {
                                "target_lang_word": "переигровки",
                                "target_lang_sense_clarification": "термин в теннисе (незасчитанные подачи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c981cb4-b00c-35f6-a5f8-7859ae2b2966",
                    "sense_definition": "Initialism of local exchange trading system.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "acronym",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our community established a <w>LETS</w> to facilitate bartering services without money.",
                            "target_lang_translations": {
                                "ru": "Наше сообщество создало <w>систему местного обмена</w>, чтобы облегчить бартер услуг без денег."
                            }
                        },
                        {
                            "text": "Members of the <w>LETS</w> trade gardening skills for home repairs.",
                            "target_lang_translations": {
                                "ru": "Участники <w>системы местного обмена</w> обменивают навыки садоводства на ремонт дома."

                            "target_lang_translations": {
                                "en": "Our <firm> develops software for banks."
                            }
                        },
                        {
                            "text": "Мой отец работает в крупной строительной <фирме> уже десять лет.",
                            "target_lang_translations": {
                                "en": "My father has been working at a large construction <firm> for ten years."
                            }
                        }
                    ],
                    "synonyms": [
                        "компания",
                        "предприятие",
                        "организация"
                    ],
                    "common_phrases": [

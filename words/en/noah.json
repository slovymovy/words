    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cf8a79d9-9a78-3981-b399-5db2ae040f67",
                    "sense_definition": "A common male given name of Hebrew origin.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Noah</w> is starting school tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Ной</w> завтра идет в школу."
                            }
                        },
                        {
                            "text": "We decided to name the baby <w>Noah</w> because we liked the sound of it.",
                            "target_lang_translations": {
                                "ru": "Мы решили назвать ребенка <w>Ной</w>, потому что нам понравилось, как это звучит."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное мужское имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ной",
                                "target_lang_sense_clarification": "мужское имя (традиционное/библейское)"
                            },
                            {
                                "target_lang_word": "Ноа",
                                "target_lang_sense_clarification": "мужское имя (современная транслитерация)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f33643e-6fa1-334c-b012-8d85f7a77805",
                    "sense_definition": "A major figure in Abrahamic religions who is believed to have built a large ark to save his family and animals from a Great Flood.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The story of <w>Noah</w> and the flood is well known in many cultures.",
                            "target_lang_translations": {
                                "ru": "История о <w>Ное</w> и потопе хорошо известна во многих культурах."
                            }
                        },
                        {
                            "text": "According to the scripture, <w>Noah</w> gathered two of every animal onto the ship.",
                            "target_lang_translations": {
                                "ru": "Согласно писанию, <w>Ной</w> собрал на корабль по паре всех животных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Noah's Ark"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важная фигура в авраамических религиях, которая, согласно верованиям, построила большой ковчег, чтобы спасти свою семью и животных от Всемирного потопа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ной",
                                "target_lang_sense_clarification": "библейский персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eacb8f6c-7b3a-3c52-953f-6e4d6b81e4c4",
                    "sense_definition": "A surname.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Noah</w> has been working at the company for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Ноа</w> работает в компании уже более двадцати лет."
                            }
                        },
                        {
                            "text": "The <w>Noah</w> family reunion will be held in the park this summer.",
                            "target_lang_translations": {
                                "ru": "Семейная встреча <w>Ноа</w> пройдет в парке этим летом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."

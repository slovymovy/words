                },
                {
                    "sense_id": "691c4ca2-06ea-32cc-83db-8415e5e1b11d",
                    "sense_definition": "Совокупность связанных между собой произведений, лекций или научных дисциплин, составляющих единое целое.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Профессор прочитал студентам интересный <w>цикл</w> лекций по истории древнего мира.",
                            "target_lang_translations": {
                                "en": "The professor delivered an interesting <w>series</w> of lectures on the history of the ancient world to the students."
                            }
                        },
                        {
                            "text": "Известный поэт завершил свой новый <w>цикл</w> стихов о природе родного края.",
                            "target_lang_translations": {
                                "en": "The famous poet completed his new <w>cycle</w> of poems about the nature of his native land."
                            }
                        }
                    ],
                    "synonyms": [
                        "серия",
                        "сборник"
                    ],
                    "common_phrases": [
                        "цикл лекций",
                        "цикл передач",
                        "цикл рассказов",
                        "поэтический цикл"
                    ],
                    "target_lang_definitions": {
                        "en": "A collection of interconnected works, lectures, or scientific disciplines constituting a single whole."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "series",
                                "target_lang_sense_clarification": "sequence of lectures or works"
                            },
                            {
                                "target_lang_word": "cycle",
                                "target_lang_sense_clarification": "set of related artistic works"
                            }
                        ]
                    }
                },
                {

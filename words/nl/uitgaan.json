                    }
                },
                {
                    "sense_id": "0c6a521b-1be9-309f-901b-60695a8daed5",
                    "sense_definition": "Iets aannemen als waarheid of als basis gebruiken voor een redenering.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assumption_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag er niet zomaar van <uitgaan> dat iedereen het hiermee eens is.",
                            "target_lang_translations": {
                                "en": "You cannot just <assume> that everyone agrees with this.",
                                "ru": "Нельзя просто так <исходить> из того, что все с этим согласны."
                            }

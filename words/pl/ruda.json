    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0d7ffbd4-4cf5-3187-a53d-36b4a7b83912",
                    "sense_definition": "Kopalina (skała lub minerał) wydobywana z ziemi, zawierająca metale lub inne cenne pierwiastki chemiczne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej kopalni od lat wydobywana jest wysokiej jakości <ruda> miedzi.",
                            "target_lang_translations": {
                                "ru": "В этой шахте уже много лет добывается высококачественная медная <руда>."
                            }
                        },
                        {
                            "text": "Bogata <ruda> żelaza pozwoliła na szybki rozwój przemysłu w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Богатая железная <руда> позволила быстро развить промышленность в этом регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "kopalina"
                    ],
                    "common_phrases": [
                        "ruda żelaza",
                        "ruda miedzi",
                        "wydobycie rudy",
                        "złoża rudy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полезное ископаемое (горная порода или минерал), добываемое из земли и содержащее металлы или другие ценные химические элементы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руда",
                                "target_lang_sense_clarification": "ископаемое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87b73992-9fe2-3d3f-a310-c88cf6588652",
                    "sense_definition": "Kobieta lub dziewczyna mająca naturalnie rude włosy.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra to naturalna <ruda>, chociaż wszyscy inni w rodzinie są blondynami.",
                            "target_lang_translations": {
                                "ru": "Моя сестра — натуральная <рыжая>, хотя все остальные в семье — блондины."
                            }
                        },
                        {
                            "text": "Ta sympatyczna <ruda> ze sklepu zawsze się do mnie uśmiecha.",
                            "target_lang_translations": {
                                "ru": "Эта симпатичная <рыжая> из магазина всегда мне улыбается."
                            }
                        }
                    ],
                    "synonyms": [
                        "ryża"
                    ],
                    "common_phrases": [
                        "farbowana ruda",
                        "piegowata ruda"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девушка, имеющая от природы рыжие волосы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыжая",
                                "target_lang_sense_clarification": "о женщине"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c201164e-ddc1-35bc-bdb4-14e7446fe63a",
                    "sense_definition": "Forma rodzaju żeńskiego przymiotnika <w>rudy</w> (oznaczającego kolor pomarańczowoczerwony, np. włosów lub sierści).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po drzewie w parku biegała mała, <ruda> wiewiórka.",
                            "target_lang_translations": {
                                "ru": "По дереву в парке бегала маленькая <рыжая> белка."
                            }
                        },
                        {
                            "text": "Jej <ruda> czupryna była widoczna z daleka w tłumie ludzi.",
                            "target_lang_translations": {
                                "ru": "Её <рыжая> шевелюра была видна издалека в толпе людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ruda wiewiórka",
                        "ruda kotka",
                        "ruda broda"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rodzaj żeński"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма женского рода прилагательного «rudy» (обозначающего оранжево-красный цвет, например, волос или шерсти)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыжая",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "af3e348b-15e0-3828-82cb-264bbbf1a300",
                    "sense_definition": "Nazwa własna wielu miejscowości, wsi oraz dzielnic miast w Polsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "toponyms",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają w małej wsi o nazwie <Ruda>.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка живут в маленькой деревне под названием <Руда>."
                            }
                        },
                        {
                            "text": "Największym miastem o tej nazwie jest <Ruda> Śląska.",
                            "target_lang_translations": {
                                "ru": "Крупнейшим городом с таким названием является <Руда> Слёнска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ruda Śląska",
                        "Nowa Ruda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собственное название многих населенных пунктов, деревень и районов городов в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Руда",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "361f6811-63fa-3c1f-81d9-72158eda5c7d",
                    "sense_definition": "Nazwa własna kilku rzek, potoków i strug płynących przez Polskę.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hydronyms",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Przez naszą okolicę przepływa rzeka <Ruda>, która jest dopływem Odry.",
                            "target_lang_translations": {
                                "ru": "Через нашу местность протекает река <Руда>, являющаяся притоком Одры."
                            }
                        },
                        {
                            "text": "Latem często chodzimy na spacer nad rzekę, która nazywa się <Ruda>.",
                            "target_lang_translations": {
                                "ru": "Летом мы часто ходим на прогулку к реке, которая называется <Руда>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собственное название нескольких рек, ручьев и потоков, протекающих через Польшу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Руда",
                                "target_lang_sense_clarification": "гидроним"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "8e26c6dc-bd22-3b3d-9b09-1328e9240c01",
                    "sense_definition": "O niewielkich rozmiarach fizycznych; zajmujący mało miejsca w przestrzeni.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>mały</w> piesek szczeka głośniej niż duży owczarek sąsiada.",
                            "target_lang_translations": {
                                "ru": "Этот <w>маленький</w> песик лает громче, чем большая овчарка соседа."
                            }
                        },
                        {
                            "text": "Kupiłam sobie <w>mały</w> samochód, żeby łatwiej parkować w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Я купила себе <w>маленькую</w> машину, чтобы было легче парковаться в центре города."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieduży",
                        "niewielki",
                        "drobny",
                        "malutki"
                    ],
                    "antonyms": [
                        "duży",
                        "wielki",
                        "ogromny",
                        "olbrzymi"
                    ],
                    "common_phrases": [
                        "mały dom",
                        "mały pokój",
                        "mały palec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий небольшие физические размеры; занимающий мало места в пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маленький",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "небольшой",
                                "target_lang_sense_clarification": "не крупный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f91f09f-77e2-374a-9f43-7dfd8aee261a",
                    "sense_definition": "Będący w młodym wieku, niedorosły (często o dzieciach lub młodych zwierzętach).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy byłem <w>mały</w>, marzyłem o podróży na księżyc.",
                            "target_lang_translations": {
                                "ru": "Когда я был <w>маленьким</w>, я мечтал о путешествии на Луну."
                            }
                        },
                        {
                            "text": "Jej <w>mały</w> synek właśnie nauczył się chodzić.",
                            "target_lang_translations": {
                                "ru": "Ее <w>маленький</w> сынок только что научился ходить."
                            }
                        }
                    ],
                    "synonyms": [
                        "młody",
                        "niedorosły"
                    ],
                    "antonyms": [
                        "dorosły",
                        "szeroki"
                    ],
                    "common_phrases": [
                        "gdy byłem mały",
                        "małe dziecko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в юном возрасте, несовершеннолетний (часто о детях или детенышах животных)."

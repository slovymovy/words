            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "34c63018-a90d-3607-8ccf-cb5d42ea22a8",
                    "sense_definition": "Собирательное числительное, обозначающее группу из трёх человек (обычно мужчин или смешанную группу) или детёнышей животных.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_of_three",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моей старшей сестры <трое> детей.",
                            "target_lang_translations": {
                                "en": "My older sister has <three> children."
                            }
                        },
                        {
                            "text": "Нас было <трое> в этой маленькой комнате.",
                            "target_lang_translations": {
                                "en": "There were <three> of us in this small room."
                            }
                        },
                        {
                            "text": "Все <трое> друзей решили пойти в кино.",
                            "target_lang_translations": {
                                "en": "All <three> friends decided to go to the cinema."
                            }
                        }
                    ],
                    "synonyms": [
                        "три"
                    ],
                    "common_phrases": [
                        "трое детей",
                        "нас трое",
                        "все трое",
                        "трое сыновей"
                    ],
                    "target_lang_definitions": {
                        "en": "A collective numeral indicating a group of three people or animals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "three",
                                "target_lang_sense_clarification": "group of three"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "152a17b7-9d6b-3b72-ada2-fff14a3aea08",
                    "sense_definition": "Количество из трёх предметов (употребляется с существительными, имеющими только форму множественного числа, или обозначающими парные предметы).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_of_three",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы ехали на поезде <трое> суток.",
                            "target_lang_translations": {
                                "en": "We traveled by train for <three> days."
                            }
                        },
                        {
                            "text": "Мне нужно купить <трое> новых ножниц.",
                            "target_lang_translations": {
                                "en": "I need to buy <three pairs of> new scissors."
                            }
                        },
                        {
                            "text": "В шкафу висят <трое> брюк.",
                            "target_lang_translations": {
                                "en": "There are <three pairs of> trousers hanging in the wardrobe."
                            }
                        }
                    ],
                    "synonyms": [
                        "три"
                    ],
                    "common_phrases": [
                        "трое суток",
                        "трое часов",
                        "трое ножниц",
                        "трое брюк"
                    ],
                    "target_lang_definitions": {
                        "en": "Three items (used with nouns that are plural-only or denote paired objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "three",
                                "target_lang_sense_clarification": "with plural-only nouns"
                            },
                            {
                                "target_lang_word": "three pairs of",
                                "target_lang_sense_clarification": "for paired objects"
                            }
                        ]
                    }
                }
            ]

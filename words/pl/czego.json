{
    "word_family": [
        "co",
        "coś",
        "cokolwiek",
        "nieco"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "e9051fa3-0ace-3d9f-b90e-d934fbf174e4",
                    "sense_definition": "Forma dopełniacza (Genitiv) zaimka <w>co</w>, używana w pytaniach o przedmiot, przyczynę lub w przeczeniach.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, <czego> szukasz w mojej szufladzie.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, <чего> ты ищешь в моем ящике."
                            }
                        },
                        {
                            "text": "<Czego> potrzebujesz, żeby naprawić ten rower?",
                            "target_lang_translations": {
                                "ru": "<Чего> тебе нужно, чтобы починить этот велосипед?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "do czego",
                        "od czego",
                        "z czego",
                        "czego dusza zapragnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа местоимения «что», используемая в вопросах о предмете, причине или в отрицаниях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чего",
                                "target_lang_sense_clarification": "родительный падеж от «что»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b48bdb1-9e0d-3f75-bc58-975ddd9a5e0f",
                    "sense_definition": "Potoczne słowo używane w funkcji pytajnej oznaczające: dlaczego, z jakiego powodu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Czego> stoisz tak w deszczu bez parasola?",
                            "target_lang_translations": {
                                "ru": "<Чего> ты так стоишь под дождем без зонта?"
                            }
                        },
                        {
                            "text": "Nie rozumiem, <czego> się tak denerwujesz.",
                            "target_lang_translations": {
                                "ru": "Не понимаю, <чего> ты так нервничаешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "dlaczego",
                        "czemu",
                        "po co"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное слово, используемое в вопросительной функции, означающее: почему, по какой причине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чего",
                                "target_lang_sense_clarification": "в значении «почему»"
                            },
                            {
                                "target_lang_word": "почему",
                                "target_lang_sense_clarification": "стандартный синоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "dfc32735-4a61-3161-9dad-8f544cb16ec2",
                    "sense_definition": "Niegrzeczne lub bardzo potoczne pytanie o to, co ktoś powiedział lub czego chce; używane zamiast słowa „słucham”.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Marek, chodź tu na chwilę! – <Czego>?",
                            "target_lang_translations": {
                                "ru": "– Марек, иди сюда на минутку! – <Чего>?"
                            }
                        },
                        {
                            "text": "<Czego> znowu ode mnie chcesz?",
                            "target_lang_translations": {
                                "ru": "<Чего> ты опять от меня хочешь?"
                            }
                        }
                    ],
                    "synonyms": [
                        "co"
                    ],
                    "antonyms": [
                        "słucham"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often perceived as rude"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубый или очень разговорный вопрос о том, что кто-то сказал или чего хочет; используется вместо вежливого «слушаю»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чего",
                                "target_lang_sense_clarification": "грубый ответ на обращение"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "резкий вопрос"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
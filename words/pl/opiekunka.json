    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e7bc882-2480-3dea-9a4d-7cade4b6f484",
                    "sense_definition": "Kobieta lub dziewczyna zajmująca się dziećmi pod nieobecność rodziców, często odpłatnie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "caregiver_child",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice wyszli do kina, więc dziećmi zajmuje się dzisiaj <w>opiekunka</w>.",
                            "target_lang_translations": {
                                "ru": "Родители ушли в кино, поэтому сегодня с детьми сидит <w>няня</w>."
                            }
                        },
                        {
                            "text": "Szukamy odpowiedzialnej <w>opiekunki</w> do naszego dwuletniego syna.",
                            "target_lang_translations": {
                                "ru": "Мы ищем ответственную <w>няню</w> для нашего двухлетнего сына."
                            }
                        }
                    ],
                    "synonyms": [
                        "niania",
                        "piastunka"
                    ],
                    "common_phrases": [
                        "opiekunka do dziecka",
                        "zatrudnić opiekunkę",
                        "doświadczona opiekunka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девушка, которая присматривает за детьми в отсутствие родителей, часто за плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "няня",
                                "target_lang_sense_clarification": "для ребенка"
                            },
                            {
                                "target_lang_word": "бебиситтер",
                                "target_lang_sense_clarification": "приходящая няня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f8470d7-c303-3340-be98-6bff86a14b89",
                    "sense_definition": "Kobieta zajmująca się zawodowo lub społecznie osobami starszymi, chorymi lub niepełnosprawnymi.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "caregiver_medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia ma wspaniałą <w>opiekunkę</w>, która pomaga jej w codziennych czynnościach.",
                            "target_lang_translations": {
                                "ru": "У моей бабушки замечательная <w>сиделка</w>, которая помогает ей в повседневных делах."
                            }
                        },
                        {
                            "text": "Praca jako <w>opiekunka</w> osób starszych wymaga dużo cierpliwości i empatii.",
                            "target_lang_translations": {
                                "ru": "Работа <w>сиделкой</w> для пожилых людей требует много терпения и эмпатии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opiekunka osób starszych",
                        "opiekunka środowiskowa",
                        "opiekunka medyczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которая профессионально ухаживает за пожилыми, больными людьми или людьми с ограниченными возможностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сиделка",
                                "target_lang_sense_clarification": "для больных или пожилых"
                            },
                            {
                                "target_lang_word": "помощница по уходу",
                                "target_lang_sense_clarification": "социальный работник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee92ff3f-a477-345b-85c8-a73f1985684f",
                    "sense_definition": "Żeńska forma słowa <w>opiekun</w> (kobieta sprawująca prawną pieczę nad małoletnim lub osobą ubezwłasnowolnioną).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_guardian",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd wyznaczył ciotkę na <w>opiekunkę</w> prawną osieroconych dzieci.",
                            "target_lang_translations": {
                                "ru": "Суд назначил тетю законным <w>опекуном</w> осиротевших детей."
                            }
                        },
                        {
                            "text": "Jako <w>opiekunka</w>, jest ona odpowiedzialna za zarządzanie majątkiem swojego podopiecznego.",
                            "target_lang_translations": {
                                "ru": "Как <w>опекун</w>, она несет ответственность за управление имуществом своего подопечного."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opiekunka prawna",
                        "ustanowić opiekunkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, осуществляющая юридическую опеку над несовершеннолетним или недееспособным лицом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опекун",
                                "target_lang_sense_clarification": "юридический статус"
                            },
                            {
                                "target_lang_word": "попечительница",
                                "target_lang_sense_clarification": "законный представитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18b4051b-ba7d-3ca0-a54f-1f7a61524657",
                    "sense_definition": "Żeńska forma słowa <w>opiekun</w> (pracownica ogrodu zoologicznego zajmująca się zwierzętami).",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_keeper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Opiekunka</w> karmi lwy każdego ranka o ustalonej porze.",
                            "target_lang_translations": {
                                "ru": "<w>Смотрительница</w> кормит львов каждое утро в установленное время."
                            }
                        },
                        {
                            "text": "Doświadczona <w>opiekunka</w> potrafi szybko zauważyć zmiany w zachowaniu zwierząt.",
                            "target_lang_translations": {
                                "ru": "Опытная <w>смотрительница</w> может быстро заметить изменения в поведении животных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opiekunka zwierząt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудница зоопарка, которая ухаживает за животными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотрительница",
                                "target_lang_sense_clarification": "за животными"
                            },
                            {
                                "target_lang_word": "служительница",
                                "target_lang_sense_clarification": "зоопарка"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bf05793-ea97-3b29-b63d-56e18b95b632",
                    "sense_definition": "An enclosure for livestock, such as cattle or sheep.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "enclosure_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer drove the herd into the <w>kraal</w> for the night.",
                            "target_lang_translations": {
                                "ru": "Фермер загнал стадо в <w>загон</w> на ночь."
                            }
                        },
                        {
                            "text": "They built a strong <w>kraal</w> to protect the sheep from predators.",
                            "target_lang_translations": {
                                "ru": "Они построили прочный <w>крааль</w>, чтобы защитить овец от хищников."
                            }
                        }
                    ],
                    "synonyms": [
                        "corral",
                        "pen"
                    ],
                    "common_phrases": [
                        "cattle kraal",
                        "sheep kraal"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Central and Southern Africa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Огороженное место для содержания скота, такого как крупный рогатый скот или овцы (особенно в Южной и Центральной Африке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загон",
                                "target_lang_sense_clarification": "место для скота"
                            },
                            {
                                "target_lang_word": "крааль",
                                "target_lang_sense_clarification": "специфический термин для Африки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "638ef762-53c6-3519-9c85-c3bb3b8cba46",
                    "sense_definition": "In Central and Southern Africa, a rural village of huts surrounded by a stockade.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "village_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tourists visited a traditional <w>kraal</w> to learn about the local culture.",
                            "target_lang_translations": {
                                "ru": "Туристы посетили традиционный <w>крааль</w>, чтобы узнать о местной культуре."
                            }
                        },
                        {
                            "text": "The chief gathered the elders in the center of the <w>kraal</w>.",
                            "target_lang_translations": {
                                "ru": "Вождь собрал старейшин в центре <w>крааля</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chief's kraal",
                        "visit the kraal"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Central and Southern Africa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В Центральной и Южной Африке: сельское поселение из хижин, окруженное частоколом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крааль",
                                "target_lang_sense_clarification": "африканское поселение"
                            },
                            {
                                "target_lang_word": "поселение",
                                "target_lang_sense_clarification": "укрепленная деревня"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4a43939a-e697-367f-a160-ea5a3da01d3b",
                    "sense_definition": "To enclose livestock within a kraal or stockade.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclosure_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is time to <w>kraal</w> the animals before the sun sets.",
                            "target_lang_translations": {
                                "ru": "Пора <w>загонять</w> животных, пока солнце не село."
                            }
                        },
                        {
                            "text": "The shepherd <w>kraaled</w> the goats to keep them safe at night.",
                            "target_lang_translations": {
                                "ru": "Пастух <w>загнал</w> коз в загон, чтобы обезопасить их на ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "corral"
                    ],
                    "common_phrases": [
                        "kraal the cattle",
                        "kraal the sheep"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Central and Southern Africa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Загонять скот в загон или крааль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загонять",
                                "target_lang_sense_clarification": "скот в укрытие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

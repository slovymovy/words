            "senses": [
                {
                    "sense_id": "c8a200dc-177b-3646-96c9-502ba3989279",
                    "sense_definition": "Postąpić w określony sposób w danej sytuacji; odnieść się do kogoś lub czegoś (wymaga użycia zaimka zwrotnego „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiedziałem, jak <zachować> się w towarzystwie tak ważnych osób.",
                            "target_lang_translations": {
                                "ru": "Я не знал, как <вести> себя в обществе столь важных персон."
                            }
                        },
                        {
                            "text": "Uczniowie obiecali <zachować> się cicho podczas wizyty w muzeum.",
                            "target_lang_translations": {
                                "ru": "Ученики обещали <вести> себя тихо во время посещения музея."
                            }
                        }
                    ],
                    "synonyms": [
                        "pogrzebać"
                    ],
                    "common_phrases": [
                        "zachować się poprawnie",
                        "zachować się przyzwoicie",
                        "zachować się jak trzeba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступить определённым образом в данной ситуации; отнестись к кому-либо или чему-либо (требует возвратного местоимения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повести себя",
                                "target_lang_sense_clarification": "поступить определённым образом в конкретной ситуации"
                            },
                            {
                                "target_lang_word": "вести себя",
                                "target_lang_sense_clarification": "о манере поведения в целом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27cf4d8e-20c2-3c52-8b18-39cfef1d132a",
                    "sense_definition": "Sprawić, że coś nadal istnieje, trwa lub pozostaje w czyimś posiadaniu; nie dopuścić do utraty, zniszczenia lub zmiany czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnej sytuacji należy przede wszystkim <zachować> spokój i nie panikować.",
                            "target_lang_translations": {
                                "ru": "В трудной ситуации нужно прежде всего <сохранять> спокойствие и не паниковать."
                            }
                        },
                        {
                            "text": "Chciałbym <zachować> to zdjęcie na pamiątkę naszych wspólnych wakacji.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <сохранить> это фото на память о нашем совместном отпуске."
                            }
                        }
                    ],
                    "synonyms": [
                        "pogrzebać",
                        "pogrzebać"
                    ],
                    "antonyms": [
                        "stracić",
                        "zgubić",
                        "zniszczyć",
                        "zaprzepaścić"
                    ],
                    "common_phrases": [
                        "zachować spokój",
                        "zachować ciszę",
                        "zachować twarz",
                        "zachować dystans",
                        "zachować w pamięci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы что-то продолжало существовать или оставалось у кого-то; не допустить потери, разрушения или изменения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохранить",
                                "target_lang_sense_clarification": "уберечь от исчезновения или порчи"
                            },
                            {
                                "target_lang_word": "сберечь",
                                "target_lang_sense_clarification": "не потратить, не потерять"
                            },
                            {
                                "target_lang_word": "соблюсти",
                                "target_lang_sense_clarification": "выполнить, не нарушить (правила, тишину)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa52d709-7250-3739-9a2f-a5bf96b58df5",
                    "sense_definition": "Złożyć ciało zmarłego w grobie; dokonać pochówku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciało poległego rycerza postanowiono uroczyście <zachować> w rodzinnej krypcie.",
                            "target_lang_translations": {
                                "ru": "Тело павшего рыцаря решили торжественно <похоронить> в семейном склепе."

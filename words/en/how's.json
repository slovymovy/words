    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7bbe70c5-2312-3682-b93f-8695b7714586",
                    "sense_definition": "Contraction of <w>how</w> <w>is</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "contraction_be",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>How's</w> your brother doing these days?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> поживает твой брат в последнее время?"
                            }
                        },
                        {
                            "text": "<w>How's</w> the weather looking for our trip tomorrow?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> там с погодой для нашей завтрашней поездки?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "how's it going",
                        "how's life",
                        "how's that",
                        "how's things"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"how is\" (как есть/является)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "942c4476-c674-31bf-9472-abe2f427951a",
                    "sense_definition": "Contraction of <w>how</w> <w>has</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "contraction_have",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>How's</w> she been since she moved to the new city?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> она поживает с тех пор, как переехала в новый город?"
                            }
                        },
                        {
                            "text": "<w>How's</w> the team managed to finish the project so quickly?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> команде удалось закончить проект так быстро?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"how has\" (используется для образования времен Perfect)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7156614c-6ede-3a66-b9f6-507b7ba3352e",
                    "sense_definition": "Contraction of <w>how</w> <w>does</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "contraction_do",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>How's</w> that work if you don't have the right tools?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> это работает, если у тебя нет нужных инструментов?"
                            }
                        },
                        {
                            "text": "<w>How's</w> it feel to finally be finished with school?",
                            "target_lang_translations": {
                                "ru": "<w>Каково</w> это — наконец закончить школу?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "how's that sound"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Used in informal speech."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"how does\" (разговорная форма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "вопросительное слово"
                            },
                            {
                                "target_lang_word": "каково",
                                "target_lang_sense_clarification": "наречие (о чувствах/ощущениях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "49cb3df7-ab57-37d2-bc1d-543c9cac469e",
                    "sense_definition": "Alternative spelling of <w>hows</w> (plural of <w>how</w>); the means, methods, or reasons for something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was never able to stop the search for the why's and <w>how's</w> of the situation.",
                            "target_lang_translations": {
                                "ru": "Я так и не смог перестать искать причины и <w>способы</w> разрешения этой ситуации."
                            }
                        },
                        {
                            "text": "The teacher explained the <w>how's</w> of the experiment before we started.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил <w>методику</w> эксперимента, прежде чем мы начали."
                            }
                        }
                    ],
                    "synonyms": [

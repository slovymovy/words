            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "41783ca8-a1ef-3985-abc7-e5f96b313ac5",
                    "sense_definition": "Вариант написания слова <w>крёстный</w> (духовный родитель по отношению к крестнику); обычно произносится через «ё».",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "godparent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <крестный> отец подарил ему на день рождения велосипед.",
                            "target_lang_translations": {
                                "en": "His <god>father gave him a bicycle for his birthday."
                            }
                        },
                        {
                            "text": "Моя <крестная> всегда помогает мне мудрым советом.",
                            "target_lang_translations": {
                                "en": "My <godmother> always helps me with wise advice."
                            }
                        }
                    ],
                    "common_phrases": [
                        "крестный отец",
                        "крестная мать",
                        "крестные родители"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative spelling of крёстный"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a godparent (spiritual parent); variant spelling of 'крёстный'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "god-",
                                "target_lang_sense_clarification": "prefix used in godfather, godmother"
                            },
                            {
                                "target_lang_word": "godparent",
                                "target_lang_sense_clarification": "attributive"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49db0c68-c13d-3250-8ecf-fd7280fb55e4",
                    "sense_definition": "Связанный с крестом как символом христианской веры или предметом культа.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_cross",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На Пасху вокруг храма совершается торжественный <крестный> ход.",
                            "target_lang_translations": {
                                "en": "At Easter, a solemn procession <of the cross> takes place around the church."
                            }
                        },
                        {
                            "text": "Старушка сотворила <крестное> знамение и тихо вошла в церковь.",
                            "target_lang_translations": {
                                "en": "The old woman made the sign <of the cross> and quietly entered the church."
                            }
                        }
                    ],
                    "common_phrases": [
                        "крестный ход",
                        "крестное знамение",
                        "крестный путь"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to the cross as a Christian symbol or object of worship."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "attributive, as in 'sign of the cross'"
                            },
                            {
                                "target_lang_word": "of the cross",
                                "target_lang_sense_clarification": "possessive meaning"
                            }
                        ]
                    }
                }
            ]
        }

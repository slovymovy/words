            "senses": [
                {
                    "sense_id": "748d6804-45b9-3c9a-a60c-4622f459b133",
                    "sense_definition": "The largest and most populous country of the United Kingdom, located in the southern part of the island of Great Britain.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "country_sense",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "London is the capital city of <w>England</w>.",
                            "target_lang_translations": {
                                "ru": "Лондон — столица <w>Англии</w>."
                            }
                        },
                        {
                            "text": "We are planning a trip to <w>England</w> next summer to visit historical sites.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Англию</w> следующим летом, чтобы посетить исторические места."
                            }
                        }
                    ],
                    "synonyms": [
                        "Albion",
                        "Blighty"
                    ],
                    "common_phrases": [
                        "Bank of England",
                        "Church of England",
                        "Merry England",
                        "north of England"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупнейшая и наиболее густонаселенная страна Соединенного Королевства, расположенная в южной части острова Великобритания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Англия",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6806fe63-d759-3373-bb82-150e1530eb5e",
                    "sense_definition": "The United Kingdom or Great Britain as a whole; often used loosely or historically to refer to the entire sovereign state rather than just the specific country.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "country_sense",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Foreign observers sometimes incorrectly refer to the entire United Kingdom as <w>England</w>.",
                            "target_lang_translations": {
                                "ru": "Иностранные наблюдатели иногда ошибочно называют всё Соединенное Королевство <w>Англией</w>."
                            }
                        },
                        {
                            "text": "The Prime Minister of <w>England</w> is often mentioned in older texts when referring to the British leader.",
                            "target_lang_translations": {
                                "ru": "Премьер-министр <w>Англии</w> часто упоминается в старых текстах, когда речь идет о британском лидере."
                            }
                        }
                    ],
                    "synonyms": [
                        "Great Britain or United Kingdom"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered incorrect in formal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединенное Королевство или Великобритания в целом; часто используется неточно или в историческом контексте для обозначения всего государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Англия",
                                "target_lang_sense_clarification": "Великобритания (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "377f0e18-5ed2-3a81-9e51-0538c6ec097e",
                    "sense_definition": "A surname of English origin.",

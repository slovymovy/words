                            {
                                "target_lang_word": "неформальный",
                                "target_lang_sense_clarification": "не по протоколу, свободный"
                            },
                            {
                                "target_lang_word": "неофициальный",
                                "target_lang_sense_clarification": "не служебный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1aa16e0-0d7f-3b4f-aa24-fc6568ca8013",

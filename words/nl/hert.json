                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "pronunciation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшая или диалектная форма слова \"hart\" (сердце)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердце",
                                "target_lang_sense_clarification": "устаревшее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {

                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "наблюдать за обновлениями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4e317b0-46f9-32b6-a6b0-760dbbe6b3cd",
                    "sense_definition": "Handelen volgens regels, advies, instructies of een bepaald pad.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adherence_1",
                    "name_type": "no",

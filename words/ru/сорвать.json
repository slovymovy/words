{
    "word_family": [
        "срывать",
        "срыв",
        "срывание"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "acdf5499-9f37-3c7e-b8d4-6a31b7d17097",
                    "sense_definition": "Отделить, убрать или снять что-либо резким движением (цветок, плод, объявление, одежду и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик подпрыгнул и <сорвал> с ветки спелое яблоко.",
                            "target_lang_translations": {
                                "en": "The boy jumped up and <picked> a ripe apple from the branch."
                            }
                        },
                        {
                            "text": "Сильный ветер <сорвал> шляпу с головы прохожего.",
                            "target_lang_translations": {
                                "en": "A strong wind <blew> the hat <off> the passerby's head."
                            }
                        }
                    ],
                    "synonyms": [
                        "сдёрнуть",
                        "снять"
                    ],
                    "common_phrases": [
                        "сорвать цветок",
                        "сорвать яблоко",
                        "сорвать маску",
                        "сорвать одежду"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate, remove, or take something off with a sharp movement (flower, fruit, notice, clothes, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick",
                                "target_lang_sense_clarification": "flowers, fruit"
                            },
                            {
                                "target_lang_word": "tear off",
                                "target_lang_sense_clarification": "paper, clothes, general force"
                            },
                            {
                                "target_lang_word": "rip off",
                                "target_lang_sense_clarification": "forceful removal"
                            },
                            {
                                "target_lang_word": "pluck",
                                "target_lang_sense_clarification": "flowers, strings"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cc8767a-2300-3ece-b371-919c86d0e35f",
                    "sense_definition": "Сделать невозможным проведение чего-либо, помешать осуществлению планов или договоренностей.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "disruption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Шумные ученики <сорвали> урок математики, и учитель ушёл из класса.",
                            "target_lang_translations": {
                                "en": "Noisy students <disrupted> the math lesson, and the teacher left the classroom."
                            }
                        },
                        {
                            "text": "Внезапная болезнь актёра <сорвала> премьеру спектакля.",
                            "target_lang_translations": {
                                "en": "The actor's sudden illness <ruined> the play's premiere."
                            }
                        }
                    ],
                    "synonyms": [
                        "провалить",
                        "расстроить"
                    ],
                    "common_phrases": [
                        "сорвать урок",
                        "сорвать планы",
                        "сорвать переговоры",
                        "сорвать сроки"
                    ],
                    "target_lang_definitions": {
                        "en": "To make the conduct of something impossible, to interfere with the implementation of plans or agreements."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disrupt",
                                "target_lang_sense_clarification": "events, lessons, meetings"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "plans, events"
                            },
                            {
                                "target_lang_word": "derail",
                                "target_lang_sense_clarification": "negotiations, plans"
                            },
                            {
                                "target_lang_word": "sabotage",
                                "target_lang_sense_clarification": "intentional disruption"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "836a9983-55d6-3095-a5b8-19a4f37ca9f4",
                    "sense_definition": "Выразить своё раздражение или гнев на ком-либо, часто незаслуженно.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не нужно <сорвать> на мне свою злость из-за проблем на работе.",
                            "target_lang_translations": {
                                "en": "You don't need to <take out> your anger on me because of problems at work."
                            }
                        },
                        {
                            "text": "Начальник был в ярости и <сорвал> гнев на своих подчинённых.",
                            "target_lang_translations": {
                                "en": "The boss was furious and <vented> his anger on his subordinates."
                            }
                        }
                    ],
                    "synonyms": [
                        "выместить"
                    ],
                    "common_phrases": [
                        "сорвать злость",
                        "сорвать гнев",
                        "сорвать раздражение"
                    ],
                    "target_lang_definitions": {
                        "en": "To express one's irritation or anger at someone, often undeservedly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take out on",
                                "target_lang_sense_clarification": "anger, frustration"
                            },
                            {
                                "target_lang_word": "vent on",
                                "target_lang_sense_clarification": "anger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54343ca9-fd02-33a8-baa7-0fb72302e162",
                    "sense_definition": "Повредить орган или часть тела чрезмерным напряжением (обычно голос, спину или желудок).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Певец так громко кричал на репетиции, что <сорвал> голос.",
                            "target_lang_translations": {
                                "en": "The singer shouted so loudly at the rehearsal that he <strained> his voice."
                            }
                        },
                        {
                            "text": "Рабочий поднял тяжёлый ящик без подготовки и <сорвал> спину.",
                            "target_lang_translations": {
                                "en": "The worker lifted a heavy box without preparation and <threw out> his back."
                            }
                        }
                    ],
                    "synonyms": [
                        "посадить"
                    ],
                    "common_phrases": [
                        "сорвать голос",
                        "сорвать спину",
                        "сорвать желудок"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in everyday speech regarding health"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To damage an organ or body part through excessive strain (usually voice, back, or stomach)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strain",
                                "target_lang_sense_clarification": "voice, back, muscle"
                            },
                            {
                                "target_lang_word": "damage",
                                "target_lang_sense_clarification": "stomach, health"
                            },
                            {
                                "target_lang_word": "throw out",
                                "target_lang_sense_clarification": "back"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb64db5c-10c2-3026-b62b-9ad82590f75e",
                    "sense_definition": "Получить или добиться чего-либо ценного или приятного (обычно денег или одобрения).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Талантливый артист вышел на сцену и сразу <сорвал> бурные аплодисменты.",
                            "target_lang_translations": {
                                "en": "The talented artist went on stage and immediately <won> stormy applause."
                            }
                        },
                        {
                            "text": "Игрок в казино рискнул всем и <сорвал> огромный куш.",
                            "target_lang_translations": {
                                "en": "The casino player risked everything and <hit> a huge jackpot."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сорвать куш",
                        "сорвать аплодисменты",
                        "сорвать банк"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To receive or achieve something valuable or pleasant (usually money or approval)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "the jackpot"
                            },
                            {
                                "target_lang_word": "win",
                                "target_lang_sense_clarification": "applause, money"
                            },
                            {
                                "target_lang_word": "score",
                                "target_lang_sense_clarification": "colloquial, success"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58679cf9-7bb0-3e4d-8047-61993e0625f4",
                    "sense_definition": "Испортить резьбу у винта, гайки или крана, слишком сильно закручивая.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если крутить гайку слишком сильно, можно случайно <сорвать> резьбу.",
                            "target_lang_translations": {
                                "en": "If you turn the nut too hard, you can accidentally <strip> the thread."
                            }
                        },
                        {
                            "text": "Сантехник чинил трубу и по неосторожности <сорвал> кран.",
                            "target_lang_translations": {
                                "en": "The plumber was fixing the pipe and carelessly <stripped> the tap."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сорвать резьбу",
                        "сорвать гайку",
                        "сорвать кран"
                    ],
                    "target_lang_definitions": {
                        "en": "To damage the thread of a screw, nut, or tap by tightening too much."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strip",
                                "target_lang_sense_clarification": "threads, screws"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
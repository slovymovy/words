                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать свое отношение или характер через позу, тон голоса; вести себя манерно или вызывающе."
                    },
                    "translations": {
                        "ru": [

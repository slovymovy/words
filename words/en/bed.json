                {
                    "sense_id": "ca3c7a00-ca7a-3260-b247-78cdbd526910",
                    "sense_definition": "The open platform at the back of a truck or trailer used for carrying goods.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We loaded all the camping gear into the truck <w>bed</w>.",
                            "target_lang_translations": {
                                "ru": "Мы погрузили все туристическое снаряжение в <w>кузов</w> грузовика."
                            }
                        },
                        {
                            "text": "The dog loves to ride in the <w>bed</w> of the pickup.",
                            "target_lang_translations": {
                                "ru": "Собака любит ездить в <w>кузове</w> пикапа."
                            }

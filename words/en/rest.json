{
    "word_family": [
        "restful",
        "restless",
        "restlessness",
        "restroom",
        "unrest"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8d828c89-894b-322e-b91a-455c22df78bf",
                    "sense_definition": "The part of something that is left over; the others that are not included.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "remainder_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She ate half of the apple and gave the <w>rest</w> to her brother.",
                            "target_lang_translations": {
                                "ru": "Она съела половину яблока, а <w>остальное</w> отдала брату."
                            }
                        },
                        {
                            "text": "The <w>rest</w> of the students are waiting in the classroom.",
                            "target_lang_translations": {
                                "ru": "<w>Остальные</w> студенты ждут в классе."
                            }
                        }
                    ],
                    "synonyms": [
                        "remainder",
                        "others",
                        "leftovers"
                    ],
                    "common_phrases": [
                        "the rest of",
                        "for the rest",
                        "and the rest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставшаяся часть чего-либо; другие люди или предметы, которые не включены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "часть, которая осталась"
                            },
                            {
                                "target_lang_word": "остальное",
                                "target_lang_sense_clarification": "всё, что осталось"
                            },
                            {
                                "target_lang_word": "остальные",
                                "target_lang_sense_clarification": "другие люди или предметы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90cfb73a-3354-30f7-a9c7-7bca0bbd6813",
                    "sense_definition": "A period of relaxing, sleeping, or stopping work to recover strength.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relaxation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You look tired, so you should go home and have a <w>rest</w>.",
                            "target_lang_translations": {
                                "ru": "Ты выглядишь усталым, тебе стоит пойти домой и <w>отдохнуть</w>."
                            }
                        },
                        {
                            "text": "We took a short <w>rest</w> at the top of the hill.",
                            "target_lang_translations": {
                                "ru": "Мы устроили короткий <w>отдых</w> на вершине холма."
                            }
                        }
                    ],
                    "synonyms": [
                        "break",
                        "nap",
                        "relaxation",
                        "repose",
                        "sleep"
                    ],
                    "antonyms": [
                        "activity",
                        "motion"
                    ],
                    "common_phrases": [
                        "have a rest",
                        "take a rest",
                        "get some rest",
                        "day of rest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период расслабления, сна или перерыв в работе для восстановления сил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдых",
                                "target_lang_sense_clarification": "восстановление сил"
                            },
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "состояние тишины и бездействия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08bf4f39-3ef2-3109-8eac-e357211aa169",
                    "sense_definition": "An object used to support something.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "support_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He placed his arms comfortably on the arm <w>rest</w>.",
                            "target_lang_translations": {
                                "ru": "Он удобно положил руки на <w>подлокотник</w>."
                            }
                        },
                        {
                            "text": "The rifle had a steady <w>rest</w> on the sandbag.",
                            "target_lang_translations": {
                                "ru": "Винтовка имела устойчивый <w>упор</w> на мешке с песком."
                            }
                        }
                    ],
                    "synonyms": [
                        "support",
                        "holder",
                        "stand"
                    ],
                    "common_phrases": [
                        "arm rest",
                        "head rest",
                        "chin rest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, используемый для поддержки чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подставка",
                                "target_lang_sense_clarification": "предмет, на который что-то ставят"
                            },
                            {
                                "target_lang_word": "упор",
                                "target_lang_sense_clarification": "место или предмет для опоры"
                            },
                            {
                                "target_lang_word": "подпорка",
                                "target_lang_sense_clarification": "приспособление для устойчивости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2000f56-90f1-320b-ac5e-090e829c2e37",
                    "sense_definition": "A state of not moving; absence of motion.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "motionless_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ball finally came to <w>rest</w> near the fence.",
                            "target_lang_translations": {
                                "ru": "Мяч наконец <w>остановился</w> у забора."
                            }
                        },
                        {
                            "text": "The heavy machine was at <w>rest</w> when we entered the factory.",
                            "target_lang_translations": {
                                "ru": "Тяжелая машина была в состоянии <w>покоя</w>, когда мы вошли на фабрику."
                            }
                        }
                    ],
                    "synonyms": [
                        "standstill",
                        "stop"
                    ],
                    "antonyms": [
                        "motion"
                    ],
                    "common_phrases": [
                        "at rest",
                        "come to rest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние неподвижности; отсутствие движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "физическое состояние неподвижности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09810208-3322-388d-a7be-be0e4e968b36",
                    "sense_definition": "A pause in a piece of music, or the symbol representing it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure you count the <w>rest</w> correctly before playing the next note.",
                            "target_lang_translations": {
                                "ru": "Убедись, что ты правильно отсчитываешь <w>паузу</w>, прежде чем играть следующую ноту."
                            }
                        },
                        {
                            "text": "The composer included a long <w>rest</w> for the violins in this section.",
                            "target_lang_translations": {
                                "ru": "Композитор включил длинную <w>паузу</w> для скрипок в этой секции."
                            }
                        }
                    ],
                    "synonyms": [
                        "pause"
                    ],
                    "common_phrases": [
                        "quarter rest",
                        "whole rest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пауза в музыкальном произведении или знак, её обозначающий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пауза",
                                "target_lang_sense_clarification": "перерыв в звучании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3ed96c3-63ad-3c26-ba58-05fddf4119a1",
                    "sense_definition": "To stop working or being active in order to relax or sleep.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relaxation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>rest</w> for a few minutes before we continue walking.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>отдохнуть</w> несколько минут, прежде чем мы продолжим идти."
                            }
                        },
                        {
                            "text": "The doctor told him to <w>rest</w> his leg for a week.",
                            "target_lang_translations": {
                                "ru": "Врач сказал ему <w>дать отдых</w> ноге в течение недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "relax",
                        "sleep",
                        "pause"
                    ],
                    "common_phrases": [
                        "rest up",
                        "rest one's eyes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать или быть активным, чтобы расслабиться или поспать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдыхать",
                                "target_lang_sense_clarification": "проводить время без работы"
                            },
                            {
                                "target_lang_word": "отдохнуть",
                                "target_lang_sense_clarification": "восстановить силы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6df48e69-06e8-34d1-8d4b-7f80cb64b823",
                    "sense_definition": "To support an object or body part against something; to be supported by something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "support_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>rested</w> her head on the pillow and fell asleep.",
                            "target_lang_translations": {
                                "ru": "Она <w>положила</w> голову на подушку и заснула."
                            }
                        },
                        {
                            "text": "The ladder <w>rested</w> against the wall securely.",
                            "target_lang_translations": {
                                "ru": "Лестница надежно <w>опиралась</w> на стену."
                            }
                        }
                    ],
                    "synonyms": [
                        "lean",
                        "prop",
                        "lay"
                    ],
                    "common_phrases": [
                        "rest against",
                        "rest on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживать предмет или часть тела, опираясь на что-либо; быть поддерживаемым чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опираться",
                                "target_lang_sense_clarification": "использовать как опору"
                            },
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "помещать на опору (голову, руки)"
                            },
                            {
                                "target_lang_word": "прислонять",
                                "target_lang_sense_clarification": "ставить наклонно к опоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ad4dba5-a8b5-3f5f-abb9-9e5d86f67f6c",
                    "sense_definition": "To depend on or be based on something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "depend_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The success of our project <w>rests</w> on good communication.",
                            "target_lang_translations": {
                                "ru": "Успех нашего проекта <w>держится</w> на хорошей коммуникации."
                            }
                        },
                        {
                            "text": "Her entire argument <w>rests</w> on a single assumption.",
                            "target_lang_translations": {
                                "ru": "Весь её аргумент <w>основывается</w> на одном предположении."
                            }
                        }
                    ],
                    "synonyms": [
                        "depend",
                        "rely",
                        "hinge"
                    ],
                    "common_phrases": [
                        "rest on",
                        "rest upon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зависеть от чего-либо или основываться на чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основываться",
                                "target_lang_sense_clarification": "базироваться на фактах или аргументах"
                            },
                            {
                                "target_lang_word": "держаться",
                                "target_lang_sense_clarification": "зависеть от чего-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28ca6e8f-e7f7-36dd-9e3f-81aee74a01df",
                    "sense_definition": "To remain in a specific state (used primarily in fixed phrases).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>rest</w> assured that we will do everything to help.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>быть</w> уверены, что мы сделаем всё, чтобы помочь."
                            }
                        },
                        {
                            "text": "The matter cannot <w>rest</w> here; we must investigate further.",
                            "target_lang_translations": {
                                "ru": "Дело не может <w>оставаться</w> так; мы должны расследовать дальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "remain",
                        "stay"
                    ],
                    "common_phrases": [
                        "rest assured"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставаться в определенном состоянии (используется преимущественно в устойчивых фразах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "сохранять состояние"
                            },
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "в составе фраз (быть уверенным)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0016076-585f-3eb5-a444-7df8444578af",
                    "sense_definition": "To conclude the presentation of evidence in a legal case.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defense attorney decided to <w>rest</w> his case after the witness spoke.",
                            "target_lang_translations": {
                                "ru": "Адвокат защиты решил <w>завершить</w> представление доказательств после выступления свидетеля."
                            }
                        },
                        {
                            "text": "I <w>rest</w> my case.",
                            "target_lang_translations": {
                                "ru": "У меня <w>всё</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rest one's case"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Завершить представление доказательств в судебном деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завершать",
                                "target_lang_sense_clarification": "заканчивать прения сторон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "20b93f0b-c751-3293-b9d8-55391343790c",
                    "sense_definition": "Acronym for Representational State Transfer, a software architectural style.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tech_group",
                    "name_type": "technology_or_software_name",
                    "examples": [
                        {
                            "text": "Many modern web services are built using a <w>REST</w> API.",
                            "target_lang_translations": {
                                "ru": "Многие современные веб-сервисы созданы с использованием <w>REST</w> API."
                            }
                        }
                    ],
                    "common_phrases": [
                        "REST API",
                        "RESTful service"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акроним для Representational State Transfer, архитектурного стиля программного обеспечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "REST",
                                "target_lang_sense_clarification": "архитектурный стиль"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
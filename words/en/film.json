{
    "word_family": [
        "filmmaker",
        "filmography",
        "filmic"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cd2fcb4-cb25-32d4-87a8-1ec9c71ac66d",
                    "sense_definition": "A story shown in moving images on a screen, usually at a cinema or on television.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_picture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the cinema to watch a new <w>film</w>.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в кинотеатр, чтобы посмотреть новый <w>фильм</w>."
                            }
                        },
                        {
                            "text": "My favorite <w>film</w> is about a lost dog.",
                            "target_lang_translations": {
                                "ru": "Мой любимый <w>фильм</w> — о потерявшейся собаке."
                            }
                        }
                    ],
                    "synonyms": [
                        "movie"
                    ],
                    "common_phrases": [
                        "watch a film",
                        "short film",
                        "feature film",
                        "horror film"
                    ],
                    "target_lang_definitions": {
                        "ru": "История, показанная в движущихся изображениях на экране, обычно в кинотеатре или по телевидению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильм",
                                "target_lang_sense_clarification": "кинокартина"
                            },
                            {
                                "target_lang_word": "кино",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2a1fe2f-37ec-370d-ae26-3323fb6dc3a2",
                    "sense_definition": "The art, business, or study of making movies.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion_picture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wants to study <w>film</w> at the university.",
                            "target_lang_translations": {
                                "ru": "Она хочет изучать <w>кино</w> в университете."
                            }
                        },
                        {
                            "text": "Many people work in the <w>film</w> industry in this city.",
                            "target_lang_translations": {
                                "ru": "Многие люди в этом городе работают в индустрии <w>кино</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "film industry",
                        "film festival",
                        "film director",
                        "film school"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусство, бизнес или изучение создания кинофильмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кино",
                                "target_lang_sense_clarification": "сфера деятельности"
                            },
                            {
                                "target_lang_word": "кинематограф",
                                "target_lang_sense_clarification": "искусство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0759fc44-f7c1-3614-8882-b3b3af898aaf",
                    "sense_definition": "A strip of plastic material used in cameras to take photographs or record movies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "camera_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new roll of <w>film</w> for my old camera.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую катушку <w>плёнки</w> для моей старой камеры."
                            }
                        },
                        {
                            "text": "You must develop the <w>film</w> before you can see the pictures.",
                            "target_lang_translations": {
                                "ru": "Нужно проявить <w>плёнку</w>, прежде чем можно будет увидеть фотографии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roll of film",
                        "develop film",
                        "color film"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоска пластикового материала, используемая в камерах для фотографирования или записи фильмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плёнка",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "фотоплёнка",
                                "target_lang_sense_clarification": "для фотоаппарата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d504853f-36f2-31d8-9148-48c6edf6f70c",
                    "sense_definition": "A very thin layer of liquid, powder, or other substance on a surface.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "thin_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A thin <w>film</w> of dust covered the old books.",
                            "target_lang_translations": {
                                "ru": "Тонкий <w>слой</w> пыли покрывал старые книги."
                            }
                        },
                        {
                            "text": "Please wrap the sandwiches in plastic <w>film</w> to keep them fresh.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, заверни бутерброды в пищевую <w>плёнку</w>, чтобы они оставались свежими."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thin film",
                        "plastic film",
                        "film of oil",
                        "film of dust"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень тонкий слой жидкости, порошка или другого вещества на поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плёнка",
                                "target_lang_sense_clarification": "тонкое покрытие"
                            },
                            {
                                "target_lang_word": "налёт",
                                "target_lang_sense_clarification": "тонкий слой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a890b84f-50bf-33a4-bd8e-6eb91086ec2f",
                    "sense_definition": "To use a camera to record a story or real events.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion_picture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crew will <w>film</w> the final scene tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Съемочная группа будет <w>снимать</w> финальную сцену завтра утром."
                            }
                        },
                        {
                            "text": "They are <w>filming</w> a documentary about nature in the park.",
                            "target_lang_translations": {
                                "ru": "Они <w>снимают</w> документальный фильм о природе в парке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "film a movie",
                        "film a scene",
                        "film on location"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать камеру для записи истории или реальных событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "производить съемку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9db72dd9-2e0f-32cf-8940-8addc8f04bca",
                    "sense_definition": "To become covered with a thin layer of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "thin_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her eyes <w>filmed</w> over with tears as she heard the news.",
                            "target_lang_translations": {
                                "ru": "Её глаза <w>затуманились</w> слезами, когда она услышала новости."
                            }
                        },
                        {
                            "text": "The windows began to <w>film</w> up with steam from the cooking.",
                            "target_lang_translations": {
                                "ru": "Окна начали <w>запотевать</w> от пара при готовке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "film over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрываться тонким слоем чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затягиваться",
                                "target_lang_sense_clarification": "покрываться пеленой"
                            },
                            {
                                "target_lang_word": "запотевать",
                                "target_lang_sense_clarification": "о стекле"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
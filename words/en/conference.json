                {
                    "sense_id": "8cfb1322-c956-3860-9899-3a14d184900a",
                    "sense_definition": "A large formal meeting where people gather to discuss specific topics, such as business, politics, or academic research.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event_meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She flew to London to attend an international business <w>conference</w>.",
                            "target_lang_translations": {
                                "ru": "Она полетела в Лондон, чтобы посетить международную бизнес-<w>конференцию</w>."
                            }
                        },
                        {
                            "text": "The prime minister held a press <w>conference</w> to announce the new policy.",
                            "target_lang_translations": {
                                "ru": "Премьер-министр провел пресс-<w>конференцию</w>, чтобы объявить о новой политике."
                            }
                        }
                    ],
                    "synonyms": [
                        "convention",
                        "congress",
                        "symposium",
                        "seminar"
                    ],
                    "common_phrases": [
                        "press conference",
                        "conference call",
                        "video conference",
                        "annual conference",
                        "conference room"
                    ],
                    "target_lang_definitions": {

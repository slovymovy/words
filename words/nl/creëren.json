            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d67ae9b-fbf5-3142-aeda-a3eb30e0c0a7",
                    "sense_definition": "Iets nieuws maken of laten ontstaan dat er eerder niet was",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid wil dit jaar duizenden nieuwe banen <w>creëren</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет <w>создать</w> тысячи новых рабочих мест в этом году.",
                                "en": "The government wants to <w>create</w> thousands of new jobs this year."
                            }
                        },
                        {
                            "text": "Je moet eerst een account <w>creëren</w> om in te loggen op de website.",
                            "target_lang_translations": {
                                "ru": "Вам нужно сначала <w>создать</w> учетную запись, чтобы войти на сайт.",
                                "en": "You must first <w>create</w> an account to log in to the website."
                            }
                        }
                    ],
                    "synonyms": [
                        "maken",
                        "scheppen",
                        "ontwerpen"
                    ],
                    "antonyms": [
                        "vernietigen"
                    ],
                    "common_phrases": [
                        "kansen creëren",
                        "banen creëren",
                        "een account creëren",
                        "mogelijkheden creëren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать что-то новое или давать начало чему-то, чего раньше не существовало",
                        "en": "To make something new or bring something into existence that did not exist before"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "делать, конструировать"
                            },
                            {
                                "target_lang_word": "творить",
                                "target_lang_sense_clarification": "созидать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "create",
                                "target_lang_sense_clarification": "to bring into existence"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "to produce or construct"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0c43ab7-ea47-34ac-a39a-1da07d868d5d",
                    "sense_definition": "De oorzaak zijn van een bepaalde situatie, sfeer of gevoel",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "causation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn onduidelijke antwoord <w>creëerde</w> veel verwarring in de zaal.",
                            "target_lang_translations": {
                                "ru": "Его неясный ответ <w>вызвал</w> большое замешательство в зале.",
                                "en": "His unclear answer <w>created</w> a lot of confusion in the hall."
                            }
                        },
                        {
                            "text": "Met kaarsen en zachte muziek kun je een gezellige sfeer <w>creëren</w>.",
                            "target_lang_translations": {
                                "ru": "С помощью свечей и тихой музыки можно <w>создать</w> уютную атмосферу.",
                                "en": "With candles and soft music you can <w>create</w> a cozy atmosphere."
                            }
                        }
                    ],
                    "synonyms": [
                        "veroorzaken"
                    ],
                    "common_phrases": [
                        "verwarring creëren",
                        "een sfeer creëren",
                        "problemen creëren",
                        "afstand creëren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной определенной ситуации, атмосферы или чувства",
                        "en": "To cause a specific situation, atmosphere, or feeling"
                    },

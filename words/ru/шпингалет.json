    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9fbabe19-4a2c-33d8-8ed1-30a3798afce7",
                    "sense_definition": "Небольшая металлическая задвижка для запирания окон или дверей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hardware",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я плотно закрыл окно на <w>шпингалет</w>, чтобы в комнату не попадал холодный воздух.",
                            "target_lang_translations": {
                                "en": "I tightly closed the window using the <w>latch</w> to keep the cold air out of the room."
                            }
                        },
                        {
                            "text": "Старый <w>шпингалет</w> на дачной двери заржавел и с трудом двигался.",
                            "target_lang_translations": {
                                "en": "The old <w>sliding bolt</w> on the country house door was rusty and hard to move."
                            }
                        }
                    ],
                    "synonyms": [
                        "задвижка",
                        "щеколда",
                        "защёлка",
                        "засов"
                    ],
                    "common_phrases": [
                        "закрыть на шпингалет",
                        "оконный шпингалет",
                        "дверной шпингалет"
                    ],
                    "target_lang_definitions": {
                        "en": "A small metal sliding bolt used to lock windows or doors."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "latch",
                                "target_lang_sense_clarification": "general term for a fastening mechanism"
                            },
                            {
                                "target_lang_word": "sliding bolt",
                                "target_lang_sense_clarification": "specific mechanism describing the action"
                            },
                            {
                                "target_lang_word": "espagnolette",
                                "target_lang_sense_clarification": "technical term for a vertical window bolt"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "205524c8-2ab1-3713-9934-9a7a86faf9e8",
                    "sense_definition": "Мальчик, подросток или человек невысокого роста.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во дворе бегали шумные <w>шпингалеты</w> и играли в футбол.",
                            "target_lang_translations": {
                                "en": "Noisy <w>squirts</w> were running around the courtyard playing football."
                            }
                        },
                        {
                            "text": "Ты ещё <w>шпингалет</w>, чтобы указывать взрослым, как им поступать.",
                            "target_lang_translations": {
                                "en": "You're still too much of a <w>squirt</w> to tell adults what to do."
                            }
                        }
                    ],
                    "synonyms": [
                        "шкет",
                        "пацан",
                        "пацанёнок"
                    ],
                    "common_phrases": [
                        "мелкий шпингалет"
                    ],
                    "target_lang_definitions": {
                        "en": "A young boy, teenager, or a person of short stature (colloquial)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "squirt",
                                "target_lang_sense_clarification": "colloquial, referring to a small or young person"
                            },
                            {
                                "target_lang_word": "shrimp",
                                "target_lang_sense_clarification": "colloquial, emphasizing small size"
                            },
                            {
                                "target_lang_word": "half-pint",
                                "target_lang_sense_clarification": "slang for a short person or child"
                            }
                        ]
                    }
                }
            ]
        }

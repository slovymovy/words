            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b3396fa9-7c3f-3121-8985-898961209408",
                    "sense_definition": "Een klein, meestal gelatineus omhulsel dat een medicijn of voedingssupplement bevat om in te nemen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medicine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de <w>capsule</w> met een groot glas water innemen.",
                            "target_lang_translations": {
                                "en": "You must take the <w>capsule</w> with a large glass of water.",
                                "ru": "Нужно запить <w>капсулу</w> большим стаканом воды."
                            }
                        },
                        {
                            "text": "De arts heeft me nieuwe <w>capsules</w> voorgeschreven tegen de pijn.",
                            "target_lang_translations": {
                                "en": "The doctor prescribed me new <w>capsules</w> for the pain.",
                                "ru": "Врач прописал мне новые <w>капсулы</w> от боли."
                            }
                        }
                    ],
                    "synonyms": [
                        "pil"
                    ],
                    "common_phrases": [
                        "een capsule innemen",
                        "capsules slikken"
                    ],
                    "target_lang_definitions": {
                        "en": "A small, usually gelatinous shell containing a medicine or dietary supplement to be taken orally.",
                        "ru": "Небольшая, обычно желатиновая оболочка, содержащая лекарство или пищевую добавку для приема внутрь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "capsule",
                                "target_lang_sense_clarification": "medicine"
                            },
                            {
                                "target_lang_word": "pill",
                                "target_lang_sense_clarification": "general term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "капсула",
                                "target_lang_sense_clarification": "лекарственная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af4357bc-f578-30df-a4c8-9cc96ab46a4d",
                    "sense_definition": "Een klein ruimtevaartuig of een module van een raket waarin de bemanning of apparatuur wordt vervoerd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De astronauten keerden veilig terug naar de aarde in de <w>capsule</w>.",
                            "target_lang_translations": {
                                "en": "The astronauts returned safely to Earth in the <w>capsule</w>.",
                                "ru": "Астронавты благополучно вернулись на Землю в <w>капсуле</w>."
                            }
                        },
                        {
                            "text": "Na de missie landde de <w>capsule</w> in de oceaan.",
                            "target_lang_translations": {
                                "en": "After the mission, the <w>capsule</w> landed in the ocean.",
                                "ru": "После завершения миссии <w>капсула</w> приводнилась в океане."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruimtecapsule"
                    ],
                    "common_phrases": [
                        "in de capsule",
                        "de capsule landde"
                    ],
                    "target_lang_definitions": {
                        "en": "A small spacecraft or a module of a rocket in which the crew or equipment is transported.",
                        "ru": "Небольшой космический аппарат или модуль ракеты, в котором перевозится экипаж или оборудование."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "capsule",
                                "target_lang_sense_clarification": "spacecraft"
                            },
                            {
                                "target_lang_word": "space capsule",
                                "target_lang_sense_clarification": "spacecraft"
                            }
                        ],
                        "ru": [
                            {

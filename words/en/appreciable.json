                    ],
                    "antonyms": [
                        "negligible",
                        "insignificant"
                    ],
                    "common_phrases": [
                        "appreciable difference",
                        "appreciable amount",
                        "appreciable increase"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаточно большой, чтобы быть замеченным или считаться важным."
                    },
                    "translations": {

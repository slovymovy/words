    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "118bd1a3-281a-3bb5-bb7c-8526be0e5437",
                    "sense_definition": "Наносить косметическое средство (румяна) на лицо, чтобы сделать щёки розовыми или красными.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "makeup",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актриса села перед зеркалом и начала <w>румянить</w> щёки перед выходом на сцену.",
                            "target_lang_translations": {
                                "en": "The actress sat before the mirror and began to <w>rouge</w> her cheeks before going on stage."
                            }
                        },
                        {
                            "text": "В прошлом веке было модно сильно белить и <w>румянить</w> лицо.",
                            "target_lang_translations": {
                                "en": "In the last century, it was fashionable to heavily whiten and <w>rouge</w> the face."
                            }
                        }
                    ],
                    "synonyms": [
                        "подкрашивать"
                    ],
                    "antonyms": [
                        "белить"
                    ],
                    "common_phrases": [
                        "румянить щёки",
                        "румянить лицо"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply a cosmetic product (rouge) to the face to make the cheeks pink or red."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rouge",
                                "target_lang_sense_clarification": "to apply cosmetic rouge"
                            },
                            {
                                "target_lang_word": "apply blush to",
                                "target_lang_sense_clarification": "modern cosmetic term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcc37376-4665-3834-b361-ebefedfb6654",
                    "sense_definition": "Придавать розовый или красный цвет, делать что-либо ярким и румяным (обычно о воздействии холода, солнца или огня).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лёгкий зимний мороз <w>румянит</w> лица детей, играющих на улице.",
                            "target_lang_translations": {
                                "en": "A light winter frost <w>reddens</w> the faces of the children playing outside."
                            }
                        },
                        {
                            "text": "Утреннее солнце <w>румянит</w> облака на горизонте.",
                            "target_lang_translations": {
                                "en": "The morning sun <w>reddens</w> the clouds on the horizon."
                            }
                        }
                    ],
                    "synonyms": [
                        "окрашивать"
                    ],
                    "common_phrases": [
                        "мороз румянит",
                        "солнце румянит",
                        "заря румянит"
                    ],
                    "target_lang_definitions": {
                        "en": "To impart a pink or red color, to make something bright and ruddy (usually regarding the effect of cold, sun, or fire)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "redden",
                                "target_lang_sense_clarification": "to make red or pink"
                            },
                            {
                                "target_lang_word": "make ruddy",
                                "target_lang_sense_clarification": "to give a healthy red color"
                            },
                            {
                                "target_lang_word": "flush",
                                "target_lang_sense_clarification": "to cause to turn red"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce970598-380f-3241-81ec-850a51512c3b",
                    "sense_definition": "The state or quality of causing trouble, difficulty, or discomfort.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We apologize for the <w>inconvenience</w> caused by the train delay.",
                            "target_lang_translations": {
                                "ru": "Мы приносим извинения за <w>неудобства</w>, вызванные задержкой поезда."
                            }
                        },
                        {
                            "text": "The construction work caused considerable <w>inconvenience</w> to the local residents.",
                            "target_lang_translations": {
                                "ru": "Строительные работы причинили значительные <w>неудобства</w> местным жителям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "apologize for the inconvenience",
                        "cause inconvenience",
                        "sorry for the inconvenience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или качество, вызывающее проблемы, трудности или дискомфорт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудобство",
                                "target_lang_sense_clarification": "затруднение, дискомфорт"
                            },
                            {
                                "target_lang_word": "беспокойство",
                                "target_lang_sense_clarification": "хлопоты, нарушение покоя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8729b5a9-581b-39f2-9443-154508943127",
                    "sense_definition": "A specific situation, person, or thing that causes trouble or difficulty.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a major <w>inconvenience</w> to have the electricity cut off for the whole day.",
                            "target_lang_translations": {
                                "ru": "Отключение электричества на весь день стало большим <w>неудобством</w>."
                            }
                        },
                        {
                            "text": "Losing your keys is a terrible <w>inconvenience</w> when you are in a hurry.",
                            "target_lang_translations": {
                                "ru": "Потеря ключей — ужасное <w>неудобство</w>, когда вы спешите."
                            }
                        }
                    ],
                    "synonyms": [
                        "nuisance",
                        "annoyance",
                        "trouble"
                    ],
                    "common_phrases": [
                        "major inconvenience",
                        "minor inconvenience",
                        "unnecessary inconvenience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная ситуация или обстоятельство, создающее трудности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудобство",
                                "target_lang_sense_clarification": "неприятное обстоятельство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7c202815-51d5-3c3e-9b15-b631ec05ac5d",
                    "sense_definition": "To cause trouble, difficulty, or discomfort to someone.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am very sorry to <w>inconvenience</w> you with this unexpected request.",
                            "target_lang_translations": {
                                "ru": "Мне очень жаль <w>доставлять</w> вам <w>неудобства</w> этой неожиданной просьбой."
                            }
                        },
                        {
                            "text": "The new traffic regulations will <w>inconvenience</w> many drivers in the city.",
                            "target_lang_translations": {
                                "ru": "Новые правила дорожного движения <w>доставят неудобства</w> многим водителям в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "incommode",
                        "impose"
                    ],
                    "common_phrases": [
                        "sorry to inconvenience",
                        "inconvenience someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять кому-либо беспокойство, трудности или дискомфорт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доставлять неудобства",
                                "target_lang_sense_clarification": "создавать проблемы"
                            },
                            {
                                "target_lang_word": "беспокоить",
                                "target_lang_sense_clarification": "утруждать, нарушать покой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
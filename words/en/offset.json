                        {
                            "text": "Buying a carbon <w>offset</w> is a popular way to reduce a personal carbon footprint.",
                            "target_lang_translations": {
                                "ru": "Покупка углеродной <w>компенсации</w> — популярный способ уменьшить личный углеродный след."
                            }
                        }
                    ],
                    "synonyms": [
                        "counterbalance",
                        "equivalence"
                    ],
                    "common_phrases": [
                        "carbon offset",
                        "tax offset",
                        "act as an offset"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что действует как противовес или компенсирующий эквивалент чему-то другому."

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "47567b43-d6db-3946-bbf0-7bb86e2830b9",
                    "sense_definition": "To state or explain the precise meaning of a word, phrase, or concept.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked the student to <w>define</w> the difficult word for the class.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика <w>дать определение</w> сложному слову перед классом."
                            }
                        },
                        {
                            "text": "Dictionaries <w>define</w> words so that we can understand them clearly.",
                            "target_lang_translations": {
                                "ru": "Словари <w>определяют</w> значения слов, чтобы мы могли ясно их понимать."
                            }
                        }
                    ],
                    "synonyms": [
                        "explain",
                        "interpret",
                        "describe"
                    ],
                    "common_phrases": [
                        "define a word",
                        "define the term",
                        "clearly define"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать точное объяснение значения слова, фразы или понятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "раскрывать значение"
                            },
                            {
                                "target_lang_word": "давать определение",
                                "target_lang_sense_clarification": "формулировать значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1de6ca78-ddc1-37a6-97bd-b615c1bf32a2",
                    "sense_definition": "To show or describe the essential character or nature of someone or something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your past mistakes do not <w>define</w> who you are today.",
                            "target_lang_translations": {
                                "ru": "Твои прошлые ошибки не <w>определяют</w>, кто ты есть сегодня."
                            }
                        },
                        {
                            "text": "This single moment of bravery will <w>define</w> his career forever.",
                            "target_lang_translations": {
                                "ru": "Этот единственный момент храбрости навсегда <w>определит</w> его карьеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "characterize"
                    ],
                    "common_phrases": [
                        "define yourself",
                        "define a generation",
                        "define who you are"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрывать или описывать сущность или характер кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "обуславливать сущность"

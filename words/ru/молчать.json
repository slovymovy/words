{
    "word_family": [
        "молчание",
        "замолчать",
        "помолчать",
        "промолчать",
        "умолчать",
        "смолчать",
        "молчаливый",
        "молчун"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f24f4091-5afd-3f3d-ae26-09519ff44a87",
                    "sense_definition": "Ничего не произносить, не говорить, не издавать звуков.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_silence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель строго попросил учеников <молчать> во время урока.",
                            "target_lang_translations": {
                                "en": "The teacher strictly asked the students <to be silent> during the lesson."
                            }
                        },
                        {
                            "text": "Почему ты <молчишь> и ничего мне не отвечаешь?",
                            "target_lang_translations": {
                                "en": "Why are you <keeping silent> and not answering me?"
                            }
                        }
                    ],
                    "synonyms": [
                        "безмолвствовать",
                        "хранить молчание",
                        "помалкивать"
                    ],
                    "antonyms": [
                        "говорить",
                        "кричать"
                    ],
                    "common_phrases": [
                        "молча́ть как рыба",
                        "молча́ть в ответ"
                    ],
                    "target_lang_definitions": {
                        "en": "To say nothing, not to speak, or not to make sounds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be silent",
                                "target_lang_sense_clarification": "general state of silence"
                            },
                            {
                                "target_lang_word": "keep quiet",
                                "target_lang_sense_clarification": "refrain from making noise"
                            },
                            {
                                "target_lang_word": "say nothing",
                                "target_lang_sense_clarification": "act of not speaking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "115b90de-9957-3b4d-a502-f9733147da67",
                    "sense_definition": "Скрывать информацию, не высказывать своего мнения, хранить секрет.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "withholding_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свидетель решил <молчать> о том, что он видел вчера вечером.",
                            "target_lang_translations": {
                                "en": "The witness decided <to keep silent> about what he saw last night."
                            }
                        },
                        {
                            "text": "Мы договорились <молчать> об этом сюрпризе до дня рождения.",
                            "target_lang_translations": {
                                "en": "We agreed <to keep quiet> about this surprise until the birthday."
                            }
                        }
                    ],
                    "synonyms": [
                        "держа́ть язы́к за зуба́ми",
                        "держа́ть ро́т на замке́"
                    ],
                    "antonyms": [
                        "высказываться"
                    ],
                    "common_phrases": [
                        "молча́ть о главном",
                        "молча́ть до последнего"
                    ],
                    "target_lang_definitions": {
                        "en": "To conceal information, withhold an opinion, or keep a secret."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep silent",
                                "target_lang_sense_clarification": "about a fact or secret"
                            },
                            {
                                "target_lang_word": "keep quiet",
                                "target_lang_sense_clarification": "informal; about a secret"
                            },
                            {
                                "target_lang_word": "withhold",
                                "target_lang_sense_clarification": "information or opinion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6447da63-cd33-3b6b-8d76-b399a7187f11",
                    "sense_definition": "Не подавать о себе вестей (не писать, не звонить) или не работать (о технике).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "no_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг уехал в экспедицию и уже неделю <молчит>.",
                            "target_lang_translations": {
                                "en": "My friend went on an expedition and has been <silent> for a week already."
                            }
                        },
                        {
                            "text": "Радио <молчало> весь день из-за сильной грозы.",
                            "target_lang_translations": {
                                "en": "The radio <was silent> all day due to the severe thunderstorm."
                            }
                        }
                    ],
                    "common_phrases": [
                        "телефон молчи́т",
                        "пушки молча́т"
                    ],
                    "target_lang_definitions": {
                        "en": "To not send news (write, call) or not to function (regarding equipment)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be silent",
                                "target_lang_sense_clarification": "lack of communication or signal"
                            },
                            {
                                "target_lang_word": "stay silent",
                                "target_lang_sense_clarification": "continue not communicating"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
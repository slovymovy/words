    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b84a16f-f156-382c-a135-94e794be3ee6",
                    "sense_definition": "The process or result of joining two or more things together to form a single entity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The show was a successful <w>fusion</w> of dance and theater.",
                            "target_lang_translations": {
                                "ru": "Шоу представляло собой удачный <w>сплав</w> танца и театра."
                            }
                        },
                        {
                            "text": "The new company was formed by the <w>fusion</w> of two smaller firms.",
                            "target_lang_translations": {
                                "ru": "Новая компания была образована путем <w>слияния</w> двух более мелких фирм."
                            }
                        }
                    ],
                    "synonyms": [
                        "combination",
                        "mixture",
                        "blend",
                        "union",
                        "merger"
                    ],
                    "antonyms": [
                        "fission",
                        "fission"
                    ],
                    "common_phrases": [
                        "fusion of styles",
                        "complete fusion",
                        "cultural fusion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или результат объединения двух или более элементов в единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слияние",
                                "target_lang_sense_clarification": "объединение компаний, организаций или потоков"
                            },
                            {
                                "target_lang_word": "сплав",
                                "target_lang_sense_clarification": "переносное значение: гармоничное соединение разных качеств или стилей"
                            },
                            {
                                "target_lang_word": "синтез",
                                "target_lang_sense_clarification": "создание сложного целого из различных частей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65a73996-de25-32ae-ade2-2538accfddc6",
                    "sense_definition": "A style of cooking that combines ingredients and techniques from different countries or cultures.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to eat at a restaurant that serves Asian <w>fusion</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили поужинать в ресторане, где подают азиатский <w>фьюжн</w>."
                            }
                        },
                        {
                            "text": "The chef is famous for his innovative <w>fusion</w> cuisine.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар знаменит своей новаторской кухней <w>фьюжн</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fusion cuisine",
                        "Asian fusion",
                        "fusion food"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кулинарный стиль, сочетающий ингредиенты и техники разных стран или культур."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фьюжн",
                                "target_lang_sense_clarification": "кулинарный стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bc91b7c-ee1c-3f66-9f42-81be9ca0d027",
                    "sense_definition": "A nuclear reaction in which atomic nuclei combine to form heavier nuclei, releasing a large amount of energy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists are working to make nuclear <w>fusion</w> a viable energy source.",
                            "target_lang_translations": {
                                "ru": "Ученые работают над тем, чтобы сделать управляемый термоядерный <w>синтез</w> доступным источником энергии."
                            }
                        },
                        {
                            "text": "The sun generates heat and light through the process of <w>fusion</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце вырабатывает тепло и свет в процессе термоядерного <w>синтеза</w>."
                            }
                        }

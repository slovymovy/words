                {
                    "sense_id": "1317540c-cc82-324c-b133-dbfeb6ed6930",
                    "sense_definition": "Tijdens; op het moment dat iets gebeurt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tijd",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Bij</w> het ontbijt lees ik graag de krant.",
                            "target_lang_translations": {

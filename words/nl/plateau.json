    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d51c6a3-b64c-33b1-ab95-cbce5859268a",
                    "sense_definition": "Een vlak, hooggelegen gebied in een bergachtig landschap.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vegetatie hier op het <w>plateau</w> is veel kariger dan in het dal.",
                            "target_lang_translations": {
                                "ru": "Растительность здесь, на <w>плато</w>, гораздо скуднее, чем в долине."
                            }
                        },
                        {
                            "text": "Toen het vlakke <w>plateau</w> overging in een steile klim, moesten we hard werken.",
                            "target_lang_translations": {
                                "ru": "Когда ровное <w>плоскогорье</w> сменилось крутым подъемом, нам пришлось потрудиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoogvlakte"
                    ],
                    "common_phrases": [
                        "op het plateau",
                        "een uitgestrekt plateau"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский участок местности, расположенный на возвышенности или в горах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плато",
                                "target_lang_sense_clarification": "география"
                            },
                            {
                                "target_lang_word": "плоскогорье",
                                "target_lang_sense_clarification": "обширный участок горного рельефа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da48d4a0-62a5-39a2-9cf9-71773f841e2c",
                    "sense_definition": "Een plat voorwerp om glazen of gerechten op te dragen of te serveren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ober balanceerde een vol <w>plateau</w> met drankjes door de drukke zaal.",
                            "target_lang_translations": {
                                "ru": "Официант балансировал с полным <w>подносом</w> напитков, пробираясь через переполненный зал."
                            }
                        },
                        {
                            "text": "Verbouwereerd nam ze een glas witte wijn van het <w>plateau</w>.",
                            "target_lang_translations": {
                                "ru": "Ошеломленная, она взяла бокал белого вина с <w>подноса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een plateau met hapjes"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België en Zuid-Nederland"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский предмет, используемый для переноски напитков или подачи еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднос",
                                "target_lang_sense_clarification": "для переноски посуды"
                            },
                            {
                                "target_lang_word": "блюдо",
                                "target_lang_sense_clarification": "большая тарелка для сервировки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e9b1f51-40fc-3cde-80b2-b58177835037",
                    "sense_definition": "Een periode of toestand van stabiliteit waarin geen verandering, groei of daling plaatsvindt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na maanden van snelle groei hebben de verkoopcijfers nu een <w>plateau</w> bereikt.",
                            "target_lang_translations": {
                                "ru": "После месяцев быстрого роста показатели продаж вышли на <w>плато</w>."
                            }
                        },
                        {
                            "text": "Zijn sportprestaties zitten op een <w>plateau</w> en verbeteren niet meer.",
                            "target_lang_translations": {
                                "ru": "Его спортивные результаты застыли на <w>плато</w> и больше не улучшаются."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een plateau bereiken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние стабильности, при котором прекращается рост, развитие или спад."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плато",
                                "target_lang_sense_clarification": "период застоя или стабильности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
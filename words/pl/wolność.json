    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c0a4f66-9c79-3de5-8088-9f7d71952045",
                    "sense_definition": "Możliwość samodzielnego podejmowania decyzji i działania bez przymusu; brak ograniczeń.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_freedom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek powinien mieć zagwarantowaną osobistą <w>wolność</w> i prawo wyboru.",
                            "target_lang_translations": {
                                "ru": "Каждому человеку должны быть гарантированы личная <w>свобода</w> и право выбора."
                            }
                        },
                        {
                            "text": "W demokratycznym państwie <w>wolność</w> słowa jest jedną z najważniejszych wartości.",
                            "target_lang_translations": {
                                "ru": "В демократическом государстве <w>свобода</w> слова является одной из важнейших ценностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "swoboda",
                        "niezależność"
                    ],
                    "antonyms": [
                        "niewola",
                        "zniewolenie"
                    ],
                    "common_phrases": [
                        "wolność słowa",
                        "wolność wyboru",
                        "pełna wolność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность самостоятельно принимать решения и действовать без принуждения; отсутствие ограничений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свобода",
                                "target_lang_sense_clarification": "отсутствие ограничений"
                            },
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "свобода действий, отсутствие принуждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de6b3746-727c-35bf-8098-bca1c7583aed",
                    "sense_definition": "Niezależność państwa od innych krajów; suwerenność polityczna.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_freedom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze z wielkim poświęceniem walczyli o <w>wolność</w> i niepodległość swojego kraju.",
                            "target_lang_translations": {
                                "ru": "Солдаты с большим самопожертвованием сражались за <w>свободу</w> и независимость своей страны."
                            }
                        },
                        {
                            "text": "Po latach zaborów naród w końcu odzyskał upragnioną <w>wolność</w> polityczną.",
                            "target_lang_translations": {
                                "ru": "После долгих лет оккупации народ наконец обрел долгожданную политическую <w>свободу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niepodległość",
                        "niezawisłość",
                        "autonomia",
                        "niezależność"
                    ],
                    "antonyms": [
                        "niewola"
                    ],
                    "common_phrases": [
                        "walka o wolność",
                        "odzyskać wolność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Независимость государства от других стран; политический суверенитет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "независимость",
                                "target_lang_sense_clarification": "суверенитет государства"
                            },
                            {
                                "target_lang_word": "свобода",
                                "target_lang_sense_clarification": "политическая независимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf68b8c3-7325-3f8c-98e1-59493ceaaeda",
                    "sense_definition": "Stan przebywania poza więzieniem lub aresztem; brak fizycznego uwięzienia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_freedom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd zdecydował, że oskarżony może wyjść na <w>wolność</w> za kaucją.",
                            "target_lang_translations": {
                                "ru": "Суд постановил, что обвиняемый может выйти на <w>свободу</w> под залог."
                            }
                        },
                        {
                            "text": "Po odbyciu kary więzień cieszył się, że wreszcie jest na <w>wolności</w>.",
                            "target_lang_translations": {
                                "ru": "Отбыв наказание, заключенный радовался, что наконец находится на <w>свободе</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "uwięzienie"
                    ],
                    "common_phrases": [
                        "wyjść na wolność",
                        "być na wolności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние пребывания вне тюрьмы или под стражей; отсутствие физического заключения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свобода",
                                "target_lang_sense_clarification": "пребывание вне тюрьмы"
                            },
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "разг. жизнь вне заключения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61ef448c-cc1b-3a11-b0f3-fad6899db905",
                    "sense_definition": "Uprawnienie obywatela do określonych działań gwarantowane przez prawo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "civil_rights",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Konstytucja określa podstawowe prawa i <w>wolności</w>, które przysługują każdemu obywatelowi.",
                            "target_lang_translations": {
                                "ru": "Конституция определяет основные права и <w>свободы</w>, полагающиеся каждому гражданину."
                            }
                        },
                        {
                            "text": "W dawnych czasach szlachta posiadała liczne <w>wolności</w> i przywileje polityczne.",
                            "target_lang_translations": {
                                "ru": "В прежние времена шляхта обладала многочисленными <w>вольностями</w> и политическими привилегиями."
                            }
                        }
                    ],
                    "synonyms": [
                        "prawo",
                        "przywilej"
                    ],
                    "common_phrases": [
                        "wolności obywatelskie",
                        "prawa i wolności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право гражданина на определенные действия, гарантированное законом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свобода",
                                "target_lang_sense_clarification": "гражданское право"
                            },
                            {
                                "target_lang_word": "вольность",
                                "target_lang_sense_clarification": "привилегия, историческое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d59cab8-2124-342b-8af3-3db3b279fe7b",
                    "sense_definition": "Możliwość swobodnego dostępu lub wejścia gdzieś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnej polszczyźnie słowo to oznaczało, że ktoś ma <w>wolność</w> do dworu królewskiego.",
                            "target_lang_translations": {
                                "ru": "В старопольском языке это слово означало, что кто-то имеет <w>доступ</w> к королевскому двору."
                            }
                        },
                        {
                            "text": "Ten termin jest już rzadko używany w znaczeniu posiadania <w>wolności</w> wstępu.",
                            "target_lang_translations": {
                                "ru": "Этот термин уже редко используется в значении обладания <w>правом входа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przystęp"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Znaczenie historyczne, obecnie rzadko spotykane."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность свободного доступа или входа куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доступ",
                                "target_lang_sense_clarification": "право входа"
                            },
                            {
                                "target_lang_word": "свободный вход",
                                "target_lang_sense_clarification": "устар. значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

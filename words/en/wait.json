                        {
                            "text": "Thieves lay in <w>wait</w> for the travelers on the lonely road.",
                            "target_lang_translations": {
                                "ru": "Воры лежали в <w>засаде</w>, поджидая путников на пустынной дороге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lie in wait"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайно наблюдать, часто с враждебными намерениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поджидать",
                                "target_lang_sense_clarification": "в засаде"

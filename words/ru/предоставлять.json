                {
                    "sense_id": "61d5b07c-f533-33fb-b17c-75041c5c723b",
                    "sense_definition": "Давать что-либо в пользование, владение или распоряжение",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "give_grant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания <предоставляет> своим сотрудникам бесплатное медицинское страхование.",
                            "target_lang_translations": {
                                "en": "The company <provides> free health insurance to its employees."
                            }
                        },
                        {
                            "text": "Банк <предоставляет> кредиты на покупку жилья под низкий процент.",
                            "target_lang_translations": {
                                "en": "The bank <provides> loans for purchasing housing at a low interest rate."
                            }
                        }
                    ],
                    "synonyms": [
                        "давать",
                        "позволять",
                        "позволять"
                    ],
                    "antonyms": [
                        "отбирать",
                        "лишать"
                    ],
                    "common_phrases": [
                        "предоставлять услуги",
                        "предоставлять информацию",
                        "предоставлять помощь",
                        "предоставлять жильё",
                        "предоставлять кредит"
                    ],
                    "target_lang_definitions": {
                        "en": "To give something for use, possession, or disposal"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "provide",
                                "target_lang_sense_clarification": "services, information, help"

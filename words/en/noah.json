                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ноа",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e079b73-7d25-356d-8647-bcec50570eef",
                    "sense_definition": "A female given name.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Although usually a boy's name, she was named <w>Noah</w> after her grandmother.",
                            "target_lang_translations": {
                                "ru": "Хотя обычно это имя мальчика, её назвали <w>Ноа</w> в честь бабушки."
                            }
                        },
                        {
                            "text": "The actress <w>Noah</w> Cyrus is a popular singer.",
                            "target_lang_translations": {
                                "ru": "Актриса <w>Ноа</w> Сайрус — популярная певица."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {

                {
                    "sense_id": "530ba993-820a-3569-9b70-a2161b389398",
                    "sense_definition": "O zwierzęciu: taki, który ma rogi lub wyrostki przypominające rogi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_horns",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na łące pasł się duży, <w>rogaty</w> byk.",
                            "target_lang_translations": {
                                "ru": "На лугу пасся большой <w>рогатый</w> бык."
                            }
                        },
                        {
                            "text": "W lesie można czasem spotkać <w>rogatą</w> zwierzynę, taką jak jelenie czy łosie.",
                            "target_lang_translations": {
                                "ru": "В лесу можно иногда встретить <w>рогатых</w> зверей, таких как олени или лоси."
                            }
                        }
                    ],
                    "antonyms": [
                        "nierogaty"
                    ],
                    "common_phrases": [
                        "bydło rogate",
                        "rogaty diabeł",
                        "rogata zwierzyna"
                    ],
                    "target_lang_definitions": {
                        "ru": "О животном: имеющий рога или выросты, напоминающие рога."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рогатый",
                                "target_lang_sense_clarification": "о животных"

                },
                {
                    "sense_id": "509e25bd-deb6-37d8-9c6b-2b9bbc6c407a",
                    "sense_definition": "The authority or capacity to control, influence, or govern people, organizations, or events.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ability_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The political party has been in <w>power</w> for over a decade.",

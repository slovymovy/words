        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d77c2648-fe30-3551-9c4b-f61d82d1e1f0",
                    "sense_definition": "Having a consistency which is hard yet brittle, and easily broken with a sharp snap; often used to describe fresh food.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "texture_brittle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>crisp</w> snow crunched loudly under our boots.",
                            "target_lang_translations": {
                                "ru": "<w>Хрустящий</w> снег громко скрипел под нашими ботинками."
                            }
                        },
                        {
                            "text": "I enjoy eating a <w>crisp</w> apple for a snack.",
                            "target_lang_translations": {
                                "ru": "Мне нравится есть <w>хрустящее</w> яблоко на перекус."
                            }
                        }
                    ],
                    "synonyms": [
                        "crunch"
                    ],
                    "antonyms": [
                        "flabby"
                    ],
                    "common_phrases": [
                        "crisp apple",
                        "crisp bacon",
                        "crisp lettuce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий твердую, но ломкую консистенцию, легко ломающийся с резким хрустом; часто используется для описания свежих продуктов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хрустящий",
                                "target_lang_sense_clarification": "о еде или текстуре"
                            },
                            {
                                "target_lang_word": "свежий",
                                "target_lang_sense_clarification": "о продуктах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6cdb567-7b3f-3301-95c1-e878e02b78d8",

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9b6def9e-fee3-3bcb-aeb3-ab602186d30e",
                    "sense_definition": "Трудно, тяжело; оценка ситуации или действия как требующих больших усилий для понимания или выполнения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "difficulty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <сложно> говорить по-русски быстро.",
                            "target_lang_translations": {
                                "en": "It is <hard> for me to speak Russian fast."
                            }
                        },
                        {
                            "text": "Решить эту задачу было очень <сложно>.",
                            "target_lang_translations": {
                                "en": "It was very <difficult> to solve this problem."
                            }
                        },
                        {
                            "text": "В такой ситуации <сложно> принять правильное решение.",
                            "target_lang_translations": {
                                "en": "In such a situation, it is <difficult> to make the right decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "непросто",
                        "нелегко"
                    ],
                    "antonyms": [
                        "просто",
                        "легко"
                    ],
                    "common_phrases": [
                        "мне сложно",
                        "очень сложно",
                        "слишком сложно",
                        "сложно сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "Hard, difficult; an assessment of a situation or action as requiring great effort to understand or perform."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "difficult",
                                "target_lang_sense_clarification": "requiring effort or skill to do or understand"
                            },
                            {
                                "target_lang_word": "hard",
                                "target_lang_sense_clarification": "not easy; requiring physical or mental effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "340cc779-0afa-3be9-94a0-cc05ab8776dc",
                    "sense_definition": "Запутанно, витиевато; образом, имеющим сложную структуру или множество деталей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "complexity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот механизм <сложно> устроен внутри.",
                            "target_lang_translations": {
                                "en": "This mechanism is <intricately> designed inside."
                            }
                        },
                        {
                            "text": "Автор пишет слишком <сложно> для детей.",
                            "target_lang_translations": {
                                "en": "The author writes too <complexly> for children."
                            }
                        }
                    ],
                    "synonyms": [
                        "непросто"
                    ],
                    "antonyms": [
                        "просто"
                    ],
                    "common_phrases": [
                        "сложно устроенный",
                        "сложно объяснить"
                    ],
                    "target_lang_definitions": {
                        "en": "Intricately, in a complex manner; in a way that has a complex structure or many details."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complexly",
                                "target_lang_sense_clarification": "in a complex or intricate manner"
                            },
                            {
                                "target_lang_word": "intricately",
                                "target_lang_sense_clarification": "with many complicated details"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d42c92ee-d78c-34e9-b936-c9965da79ad7",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>сложный</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это предложение слишком <сложно> для перевода.",
                            "target_lang_translations": {
                                "en": "This sentence is too <difficult> to translate."
                            }
                        },
                        {
                            "text": "Всё это <сложно>, но очень интересно.",
                            "target_lang_translations": {
                                "en": "All this is <complicated>, but very interesting."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the neuter singular adjective 'complex' or 'difficult'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complicated",
                                "target_lang_sense_clarification": "consisting of many interconnecting parts or elements"
                            },
                            {
                                "target_lang_word": "difficult",
                                "target_lang_sense_clarification": "hard to deal with or understand"
                            },
                            {
                                "target_lang_word": "complex",
                                "target_lang_sense_clarification": "consisting of many different and connected parts"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
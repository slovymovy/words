    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d654003-f1ee-35cc-b6fb-b2ee7310ffa0",
                    "sense_definition": "Iets samen met anderen gebruiken, bezitten of ervaren.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sharing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>delen</w> een kamer in het studentenhuis.",
                            "target_lang_translations": {
                                "ru": "Мы <w>делим</w> комнату в студенческом общежитии."
                            }
                        },
                        {
                            "text": "De kinderen moeten leren om hun speelgoed te <w>delen</w>.",
                            "target_lang_translations": {
                                "ru": "Дети должны научиться <w>делиться</w> своими игрушками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "eerlijk delen",
                        "samen delen",
                        "kosten delen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать, владеть или испытывать что-либо совместно с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "использовать совместно"
                            },
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "совместно переживать или чувствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e70ea27d-6423-3110-808e-e8d986c303d4",
                    "sense_definition": "Informatie, gedachten of bestanden aan anderen geven of laten zien, vaak via internet of sociale media.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sharing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>deelde</w> zijn gevoelens alleen met zijn beste vriend.",
                            "target_lang_translations": {
                                "ru": "Он <w>делился</w> своими чувствами только с лучшим другом."
                            }
                        },
                        {
                            "text": "Je kunt dit bericht eenvoudig <w>delen</w> met je volgers.",
                            "target_lang_translations": {
                                "ru": "Вы можете легко <w>поделиться</w> этим сообщением со своими подписчиками."
                            }
                        }
                    ],
                    "synonyms": [
                        "verspreiden"
                    ],
                    "common_phrases": [
                        "een link delen",
                        "kennis delen",
                        "locatie delen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать информацию, мысли или файлы другим, часто через интернет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делиться",
                                "target_lang_sense_clarification": "рассказывать или передавать"
                            },
                            {
                                "target_lang_word": "публиковать",
                                "target_lang_sense_clarification": "размещать в соцсетях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efd3636a-0e34-358e-bbec-595448cf3c8b",
                    "sense_definition": "Een rekenkundige bewerking uitvoeren om te bepalen hoe vaak een getal in een ander getal past.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je twaalf <w>deelt</w> door drie, is de uitkomst vier.",
                            "target_lang_translations": {
                                "ru": "Если <w>разделить</w> двенадцать на три, получится четыре."
                            }
                        },
                        {
                            "text": "Op school leren kinderen optellen, aftrekken, vermenigvuldigen en <w>delen</w>.",
                            "target_lang_translations": {
                                "ru": "В школе дети учатся складывать, вычитать, умножать и <w>делить</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "vermenigvuldigen"
                    ],
                    "common_phrases": [
                        "delen door"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять математическую операцию, чтобы узнать, сколько раз одно число содержится в другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "математическое действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "998d2c8f-9300-394b-b6df-bd6122280403",
                    "sense_definition": "Iets in meerdere stukken splitsen of scheiden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dividing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De taart werd in acht gelijke stukken <w>gedeeld</w>.",
                            "target_lang_translations": {
                                "ru": "Торт <w>разделили</w> на восемь равных кусков."
                            }
                        },
                        {
                            "text": "De rivier <w>deelt</w> de stad in een noordelijk en een zuidelijk deel.",
                            "target_lang_translations": {
                                "ru": "Река <w>делит</w> город на северную и южную части."
                            }
                        }
                    ],
                    "synonyms": [
                        "splitsen",
                        "scheiden",
                        "verdelen"
                    ],
                    "common_phrases": [
                        "in tweeën delen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разбивать или разделять что-либо на несколько частей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "разбивать на части"
                            },
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "расчленять"
                            }
                        ]
                    }
                }
            ]
        },

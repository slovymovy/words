    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2da2e374-04d3-3dd0-932f-b89c95a19a44",
                    "sense_definition": "Present participle and gerund of <w>die</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The flowers in the garden are <w>dying</w> because of the heat.",
                            "target_lang_translations": {
                                "ru": "Цветы в саду <w>умирают</w> из-за жары."
                            }
                        },
                        {
                            "text": "Many people are <w>dying</w> from the new disease.",
                            "target_lang_translations": {
                                "ru": "Многие люди <w>умирают</w> от новой болезни."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола <w>die</w> (умирать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "переставать жить"
                            },
                            {
                                "target_lang_word": "погибать",
                                "target_lang_sense_clarification": "трагически умирать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44f891f9-2ba7-3e5e-82b7-972b521b5035",
                    "sense_definition": "Present participle and gerund of <w>dye</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>dying</w> her hair black for the party.",
                            "target_lang_translations": {
                                "ru": "Она <w>красит</w> волосы в черный цвет для вечеринки."
                            }
                        },
                        {
                            "text": "The factory is responsible for <w>dying</w> the fabrics.",
                            "target_lang_translations": {
                                "ru": "Фабрика отвечает за <w>окрашивание</w> тканей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола <w>dye</w> (красить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "наносить цвет"
                            },
                            {
                                "target_lang_word": "окрашивать",
                                "target_lang_sense_clarification": "придавать окраску"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "17a84eaf-f971-351b-b867-f64a9fa5c442",
                    "sense_definition": "Approaching death; about to die.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>dying</w> soldier asked for a glass of water.",
                            "target_lang_translations": {
                                "ru": "<w>Умирающий</w> солдат попросил стакан воды."
                            }
                        },
                        {
                            "text": "Doctors did everything they could to save the <w>dying</w> patient.",
                            "target_lang_translations": {
                                "ru": "Врачи делали все возможное, чтобы спасти <w>умирающего</w> пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "fading",
                        "moribund"
                    ],
                    "antonyms": [
                        "nascent"
                    ],
                    "common_phrases": [
                        "dying man",
                        "dying breed",
                        "dead and dying"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся при смерти; тот, кто скоро умрет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирающий",
                                "target_lang_sense_clarification": "находящийся при смерти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bca7c4f-ba0a-3ed3-8718-a4ef0aa85c8e",
                    "sense_definition": "Happening at or relating to the moments before death.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "related_to_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He whispered a secret to his son with his <w>dying</w> breath.",
                            "target_lang_translations": {
                                "ru": "Он прошептал секрет своему сыну на <w>последнем</w> дыхании."
                            }
                        },
                        {
                            "text": "Her <w>dying</w> wish was to see the ocean one last time.",
                            "target_lang_translations": {
                                "ru": "Её <w>предсмертным</w> желанием было увидеть океан в последний раз."
                            }
                        }
                    ],
                    "synonyms": [
                        "final",
                        "last"
                    ],
                    "common_phrases": [
                        "dying wish",
                        "dying words",
                        "dying breath",
                        "dying day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий в моменты перед смертью или относящийся к ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предсмертный",
                                "target_lang_sense_clarification": "случающийся перед смертью"
                            },
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "финальный, прощальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1065d510-6f98-361a-aaca-1cb39a955eba",
                    "sense_definition": "Declining, coming to an end, or losing strength.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We sat on the beach and watched the <w>dying</w> light of the sunset.",
                            "target_lang_translations": {
                                "ru": "Мы сидели на пляже и наблюдали за <w>угасающим</w> светом заката."
                            }
                        },
                        {
                            "text": "Writing handwritten letters seems to be a <w>dying</w> art these days.",
                            "target_lang_translations": {
                                "ru": "Написание писем от руки в наши дни кажется <w>вымирающим</w> искусством."
                            }
                        }
                    ],
                    "synonyms": [
                        "declining",
                        "fading",
                        "waning"
                    ],
                    "antonyms": [
                        "nascent"
                    ],
                    "common_phrases": [
                        "dying light",
                        "dying art",
                        "dying moments"
                    ],
                    "target_lang_definitions": {
                        "ru": "Угасающий, исчезающий или теряющий силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угасающий",
                                "target_lang_sense_clarification": "о свете или надежде"
                            },
                            {
                                "target_lang_word": "вымирающий",
                                "target_lang_sense_clarification": "об исчезающих видах или традициях"
                            },
                            {
                                "target_lang_word": "исчезающий",
                                "target_lang_sense_clarification": "перестающий существовать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b5b7b22-7a3a-3b7c-9ebc-c024fc501a02",
                    "sense_definition": "The process of approaching death or losing life.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had a deep fear of <w>dying</w> and refused to discuss it.",
                            "target_lang_translations": {
                                "ru": "У него был глубокий страх <w>умирания</w>, и он отказывался обсуждать это."
                            }
                        },
                        {
                            "text": "The book explores the <w>dying</w> of traditional cultures in the modern world.",
                            "target_lang_translations": {
                                "ru": "Книга исследует <w>вымирание</w> традиционных культур в современном мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "death",
                        "demise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс приближения смерти или потери жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умирание",
                                "target_lang_sense_clarification": "процесс ухода из жизни"
                            },
                            {
                                "target_lang_word": "вымирание",
                                "target_lang_sense_clarification": "исчезновение группы или явления"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
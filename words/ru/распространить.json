            "senses": [
                {
                    "sense_id": "9bc56514-3c09-375b-9ce2-e34883f34e34",
                    "sense_definition": "Сделать что-либо (информацию, знания, предметы) доступным или известным для многих людей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Журналисты успели <распространить> эту важную новость по всему миру за один час.",
                            "target_lang_translations": {
                                "en": "Journalists managed to <spread> this important news all over the world in one hour."
                            }
                        },
                        {
                            "text": "Нам нужно срочно <распространить> рекламные буклеты среди наших потенциальных клиентов.",
                            "target_lang_translations": {
                                "en": "We need to urgently <distribute> advertising brochures among our potential clients."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздать",
                        "разослать",
                        "обнародовать"
                    ],
                    "antonyms": [
                        "скрыть",
                        "утаить"
                    ],
                    "common_phrases": [
                        "распространить информацию",
                        "распространить слухи",
                        "распространить опыт",
                        "распространить заявление"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something (information, knowledge, items) available or known to many people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spread",
                                "target_lang_sense_clarification": "general term for information or news"
                            },
                            {
                                "target_lang_word": "distribute",
                                "target_lang_sense_clarification": "for physical items like leaflets or products"
                            },
                            {
                                "target_lang_word": "disseminate",
                                "target_lang_sense_clarification": "formal, for knowledge or information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e2e6f54-ecab-3578-ae4c-46b531d8c94e",
                    "sense_definition": "Расширить сферу действия или влияния чего-либо на новые объекты, области или людей.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство решило <распространить> действие нового закона на все регионы страны.",
                            "target_lang_translations": {
                                "en": "The government decided to <extend> the effect of the new law to all regions of the country."
                            }
                        },
                        {
                            "text": "Руководство планирует <распространить> успешную практику продаж на другие отделы.",
                            "target_lang_translations": {
                                "en": "Management plans to <extend> the successful sales practice to other departments."
                            }
                        }
                    ],
                    "synonyms": [
                        "расширить"
                    ],
                    "antonyms": [
                        "сузить",
                        "ограничить"
                    ],
                    "common_phrases": [
                        "распространить действие",
                        "распространить влияние",
                        "распространить власть",
                        "распространить правила"
                    ],
                    "target_lang_definitions": {
                        "en": "To widen the scope of action or influence of something to new objects, areas, or people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "to apply a law, rule, or influence to a wider area"
                            },
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "to make a rule or practice valid for others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34cdf5e8-1453-37c7-8112-270c5367951b",
                    "sense_definition": "Передать болезнь или инфекцию другим людям или организмам.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грязная питьевая вода может быстро <распространить> опасную инфекцию в деревне.",
                            "target_lang_translations": {
                                "en": "Dirty drinking water can quickly <spread> a dangerous infection in the village."
                            }
                        },
                        {
                            "text": "Один больной пассажир способен случайно <распространить> вирус в переполненном автобусе.",
                            "target_lang_translations": {
                                "en": "One sick passenger is capable of accidentally <spreading> the virus in a crowded bus."
                            }
                        }
                    ],
                    "synonyms": [
                        "разнести",
                        "передать"
                    ],
                    "antonyms": [
                        "локализовать"
                    ],
                    "common_phrases": [
                        "распространить инфекцию",
                        "распространить вирус",
                        "распространить болезнь",
                        "распространить заразу"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass a disease or infection to other people or organisms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spread",
                                "target_lang_sense_clarification": "most common for diseases"
                            },
                            {
                                "target_lang_word": "transmit",
                                "target_lang_sense_clarification": "medical or formal context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c263f7a5-20ed-36dc-b7c2-4e42a88aa796",
                    "sense_definition": "Наполнить окружающее пространство чем-либо (обычно запахом, светом или звуком).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sensory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лёгкий ветер помог <распространить> сладкий аромат цветущей сирени по всему саду.",
                            "target_lang_translations": {
                                "en": "A light breeze helped <spread> the sweet scent of blooming lilacs throughout the garden."
                            }
                        },
                        {
                            "text": "Открытая дверь позволила <распространить> тепло из кухни в холодный коридор.",
                            "target_lang_translations": {
                                "en": "The open door allowed the heat to <spread> from the kitchen into the cold hallway."
                            }

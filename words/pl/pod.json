                {
                    "sense_id": "e18a140c-5c2f-36bc-8fca-0b7e1ecbb201",
                    "sense_definition": "Wskazuje na położenie obiektu niżej od czegoś innego lub pod przykryciem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_under",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kot najchętniej śpi <w>pod</w> łóżkiem.",
                            "target_lang_translations": {
                                "ru": "Мой кот охотнее всего спит <w>под</w> кроватью."
                            }
                        },
                        {
                            "text": "Znalazłem stare zdjęcia ukryte <w>pod</w> stertą gazet.",
                            "target_lang_translations": {
                                "ru": "Я нашел старые фотографии, спрятанные <w>под</w> стопкой газет."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d99bfab9-9c36-3251-86e6-0d2e376d8e35",
                    "sense_definition": "Печатное произведение (книга, журнал, газета), имеющее собственное название.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В библиотеке можно найти любое периодическое <w>издание</w>.",
                            "target_lang_translations": {
                                "en": "You can find any periodical <w>publication</w> in the library."
                            }
                        },
                        {
                            "text": "Это подарочное <w>издание</w> с иллюстрациями стоит довольно дорого.",
                            "target_lang_translations": {
                                "en": "This gift <w>edition</w> with illustrations is quite expensive."
                            }
                        }
                    ],
                    "synonyms": [
                        "публикация"
                    ],
                    "common_phrases": [
                        "периодическое издание",
                        "печатное издание",
                        "редкое издание",
                        "электронное издание"
                    ],
                    "target_lang_definitions": {
                        "en": "A printed work (such as a book, magazine, or newspaper) that has its own title."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "publication",
                                "target_lang_sense_clarification": "general term for a printed work"
                            },
                            {
                                "target_lang_word": "edition",
                                "target_lang_sense_clarification": "a specific form of a book, e.g. a gift edition"
                            },
                            {
                                "target_lang_word": "issue",
                                "target_lang_sense_clarification": "a single instance of a periodical or newspaper"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc0fee13-35d1-3366-8576-712368663eb9",
                    "sense_definition": "Определённый выпуск книги или другого печатного произведения, отличающийся от других временем печати или содержанием.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Первое <w>издание</w> этого романа вышло десять лет назад.",
                            "target_lang_translations": {
                                "en": "The first <w>edition</w> of this novel came out ten years ago."
                            }
                        },
                        {
                            "text": "Студентам рекомендовали купить новое, исправленное <w>издание</w> учебника.",
                            "target_lang_translations": {
                                "en": "Students were recommended to buy the new, revised <w>edition</w> of the textbook."
                            }
                        }
                    ],
                    "synonyms": [
                        "выпуск",
                        "тираж"
                    ],
                    "common_phrases": [
                        "первое издание",
                        "второе издание",
                        "исправленное издание",
                        "посмертное издание"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific version of a book or other printed work, distinguished by its content or time of printing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "edition",
                                "target_lang_sense_clarification": "specific version, e.g. first edition, revised edition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "821b7d5c-9684-325d-b29b-9dda71ecb820",
                    "sense_definition": "Процесс подготовки и выпуска в свет книг, газет, журналов и другой печатной продукции.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На титульном листе книги обычно указан год её <w>издания</w>.",
                            "target_lang_translations": {
                                "en": "The year of its <w>publication</w> is usually indicated on the book's title page."
                            }
                        },
                        {
                            "text": "<w>Издание</w> научной литературы требует тщательной проверки фактов.",
                            "target_lang_translations": {
                                "en": "The <w>publication</w> of scientific literature requires careful fact-checking."
                            }
                        }
                    ],
                    "synonyms": [
                        "публикация",
                        "обнародование",
                        "выпуск"
                    ],
                    "common_phrases": [
                        "год издания",
                        "место издания",
                        "подготовка к изданию",
                        "расходы на издание"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of preparing and releasing books, newspapers, magazines, and other printed products to the public."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "publication",
                                "target_lang_sense_clarification": "the act of publishing"
                            },
                            {
                                "target_lang_word": "publishing",
                                "target_lang_sense_clarification": "the business or activity of producing books"
                            }
                        ]
                    }
                }
            ]
        }
    ]

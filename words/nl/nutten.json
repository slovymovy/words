                {
                    "sense_id": "7dce7b55-433a-3a21-b6a4-b641f7d86252",
                    "sense_definition": "Gebruiken of ergens voordeel uit halen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer wist het stuk land goed te <w>nutten</w> voor zijn gewassen.",
                            "target_lang_translations": {
                                "en": "The farmer knew how to <w>utilize</w> the piece of land well for his crops.",
                                "ru": "Фермер знал, как с толком <w>использовать</w> этот участок земли для своих посевов."
                            }
                        },
                        {
                            "text": "Zij <w>nutten</w> elke gelegenheid om hun kennis te vergroten.",
                            "target_lang_translations": {
                                "en": "They <w>make use of</w> every opportunity to increase their knowledge.",
                                "ru": "Они <w>используют</w> любую возможность, чтобы расширить свои знания."
                            }
                        }
                    ],
                    "synonyms": [
                        "benutten"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This verb form is largely obsolete in modern Dutch; 'benutten' is the standard term."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To use or derive benefit from something.",
                        "ru": "Использовать или извлекать выгоду из чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "utilize",
                                "target_lang_sense_clarification": "to make practical use of something"
                            },
                            {
                                "target_lang_word": "make use of",
                                "target_lang_sense_clarification": "to employ for a purpose"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "использовать",
                                "target_lang_sense_clarification": "применять что-либо для дела"
                            },
                            {
                                "target_lang_word": "извлекать пользу",
                                "target_lang_sense_clarification": "получать выгоду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "319e8d54-7c43-3a09-bbee-b2399ae99a97",
                    "sense_definition": "Eten of drinken; voedsel tot zich nemen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>nutten</w> de maaltijd in volledige stilte.",

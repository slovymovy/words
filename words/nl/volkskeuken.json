    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c6a898f-c485-3f69-97ba-8ca417c7902d",
                    "sense_definition": "Een instelling waar mensen met weinig geld goedkoop of gratis een maaltijd kunnen krijgen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Daklozen kunnen elke avond terecht in de <w>volkskeuken</w> voor een warme maaltijd.",
                            "target_lang_translations": {
                                "ru": "Бездомные могут каждый вечер приходить в <w>бесплатную столовую</w> за горячей едой."
                            }
                        },
                        {
                            "text": "Hij werkt als vrijwilliger in de <w>volkskeuken</w> om de hoek.",
                            "target_lang_translations": {
                                "ru": "Он работает волонтером в <w>благотворительной столовой</w> за углом."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaarkeuken"
                    ],
                    "common_phrases": [
                        "eten in de volkskeuken",
                        "lokale volkskeuken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение, где люди с низким доходом могут получить дешевую или бесплатную еду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесплатная столовая",
                                "target_lang_sense_clarification": "место раздачи еды нуждающимся"
                            },
                            {
                                "target_lang_word": "благотворительная столовая",
                                "target_lang_sense_clarification": "организация питания для бедных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "015099a0-3aa0-3d61-bc67-24df6f32cfd7",
                    "sense_definition": "Gerechten en kookstijlen die traditioneel gegeten worden door het gewone volk in een bepaalde streek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stamppot is een klassiek voorbeeld uit de Nederlandse <w>volkskeuken</w>.",
                            "target_lang_translations": {
                                "ru": "Стамппот — классический пример нидерландской <w>народной кухни</w>."
                            }
                        },
                        {
                            "text": "In dit restaurant serveren ze gerechten uit de eerlijke <w>volkskeuken</w>.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают блюда честной <w>народной кухни</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "traditionele volkskeuken",
                        "eenvoudige volkskeuken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционные блюда и кулинарные обычаи простого народа в определенном регионе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "народная кухня",
                                "target_lang_sense_clarification": "традиционная кулинария"
                            },
                            {
                                "target_lang_word": "простая кухня",
                                "target_lang_sense_clarification": "еда без изысков"
                            }
                        ]
                    }
                }
            ]
        }
    ]

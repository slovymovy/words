    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "a9d77706-a69d-3ccd-8487-645cd70f1d06",
                    "sense_definition": "Up to a specified time or event.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have to work <w>till</w> eight o'clock tonight.",
                            "target_lang_translations": {
                                "ru": "Я должен работать <w>до</w> восьми часов вечера."
                            }
                        },
                        {
                            "text": "Please wait here <w>till</w> the rain stops.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подожди здесь, <w>пока не</w> закончится дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "until",
                        "'til"
                    ],
                    "common_phrases": [
                        "till now",
                        "till then",
                        "till later",
                        "till tomorrow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вплоть до указанного времени или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "предлог времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f761b99-a1ad-3f29-9bc6-8153e53fa8eb",
                    "sense_definition": "Before a certain time; used when telling the time to indicate how many minutes remain before the hour.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_telling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a quarter <w>till</w> two.",
                            "target_lang_translations": {
                                "ru": "Сейчас <w>без</w> четверти два."
                            }
                        },
                        {
                            "text": "We have ten minutes <w>till</w> the show starts.",
                            "target_lang_translations": {
                                "ru": "У нас есть десять минут <w>до</w> начала шоу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "quarter till",
                        "ten till"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "До определенного времени; используется при указании времени, чтобы показать, сколько минут осталось до часа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указание на время, оставшееся до события"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "1cf0bf02-6b77-3a10-8bd4-af75c930574b",
                    "sense_definition": "Up to the point in time that; until.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You won't know the answer <w>till</w> you try.",
                            "target_lang_translations": {
                                "ru": "Ты не узнаешь ответа, <w>пока не</w> попробуешь."
                            }
                        },
                        {
                            "text": "Don't open your eyes <w>till</w> I say so.",
                            "target_lang_translations": {
                                "ru": "Не открывай глаза, <w>пока</w> я <w>не</w> скажу."
                            }
                        }
                    ],
                    "synonyms": [
                        "until",
                        "'til"
                    ],
                    "common_phrases": [
                        "wait till",
                        "till death"
                    ],
                    "target_lang_definitions": {
                        "ru": "До того момента, как; пока не."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока не",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "до тех пор пока",
                                "target_lang_sense_clarification": "союз, более формальный вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

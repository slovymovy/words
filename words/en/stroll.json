{
    "word_family": [
        "stroller"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a89d09d7-ecdf-3602-b674-dc09ab8b5473",
                    "sense_definition": "To walk in a slow, relaxed manner, especially for pleasure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "walking_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>stroll</w> along the beach to watch the sunset.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>прогуляться</w> по пляжу, чтобы посмотреть на закат."
                            }
                        },
                        {
                            "text": "The couple <w>strolled</w> through the park holding hands.",
                            "target_lang_translations": {
                                "ru": "Пара <w>прогуливалась</w> по парку, держась за руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wander",
                        "ramble",
                        "roam",
                        "meander",
                        "rove"
                    ],
                    "common_phrases": [
                        "stroll around",
                        "stroll along",
                        "stroll down",
                        "stroll through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходить медленно и расслабленно, особенно ради удовольствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прогуливаться",
                                "target_lang_sense_clarification": "совершать прогулку, ходить не спеша"
                            },
                            {
                                "target_lang_word": "гулять",
                                "target_lang_sense_clarification": "проводить время на свежем воздухе"
                            },
                            {
                                "target_lang_word": "прохаживаться",
                                "target_lang_sense_clarification": "ходить туда-сюда или неторопливо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c47308bb-21b0-3978-8edf-cf81a0d3a907",
                    "sense_definition": "To enter or move through a place in a casual, confident, or unhurried way.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "walking_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>strolled</w> into the meeting ten minutes late as if nothing were wrong.",
                            "target_lang_translations": {
                                "ru": "Он <w>заявился</w> на собрание с десятиминутным опозданием, как ни в чем не бывало."
                            }
                        },
                        {
                            "text": "You cannot just <w>stroll</w> in here without an appointment.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто так <w>зайти</w> сюда без записи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stroll in",
                        "stroll into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Входить или передвигаться небрежно, уверенно или не спеша."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошествовать",
                                "target_lang_sense_clarification": "пройти важно, торжественно или самоуверенно"
                            },
                            {
                                "target_lang_word": "заявиться",
                                "target_lang_sense_clarification": "разг. прийти (часто без приглашения или нагло)"
                            },
                            {
                                "target_lang_word": "зайти",
                                "target_lang_sense_clarification": "войти мимоходом или легко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf4d2106-37d1-3d1f-babb-e670e21dc391",
                    "sense_definition": "To achieve a goal or complete a task with very little effort.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "easy_achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The experienced team <w>strolled</w> to a comfortable victory.",
                            "target_lang_translations": {
                                "ru": "Опытная команда <w>легко пришла</w> к уверенной победе."
                            }
                        },
                        {
                            "text": "She studied hard and <w>strolled</w> through the final exam.",
                            "target_lang_translations": {
                                "ru": "Она усердно училась и <w>без труда сдала</w> выпускной экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "breeze",
                        "coast"
                    ],
                    "antonyms": [
                        "labor",
                        "scrape"
                    ],
                    "common_phrases": [
                        "stroll to victory",
                        "stroll through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достичь цели или выполнить задачу с очень небольшими усилиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легко добиться",
                                "target_lang_sense_clarification": "получить результат без усилий"
                            },
                            {
                                "target_lang_word": "справиться играючи",
                                "target_lang_sense_clarification": "выполнить задачу очень легко"
                            }

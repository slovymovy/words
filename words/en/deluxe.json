    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0bfaebf3-9093-3a89-a524-556c3671fa83",
                    "sense_definition": "Of very high quality, superior, or luxurious.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality_luxury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel offers a <w>deluxe</w> suite with a private balcony and ocean view.",
                            "target_lang_translations": {
                                "ru": "Отель предлагает номер <w>люкс</w> с собственным балконом и видом на океан."
                            }
                        },
                        {
                            "text": "I decided to buy the <w>deluxe</w> edition of the album because it includes extra tracks.",
                            "target_lang_translations": {
                                "ru": "Я решил купить издание альбома <w>люкс</w>, так как оно включает дополнительные треки."
                            }
                        }
                    ],
                    "synonyms": [
                        "luxurious",
                        "premium",
                        "superior",
                        "high-end",
                        "fancy"
                    ],
                    "antonyms": [
                        "basic",
                        "standard",
                        "economy",
                        "plain"
                    ],
                    "common_phrases": [
                        "deluxe edition",
                        "deluxe room",
                        "deluxe suite",
                        "super deluxe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень высокого качества, превосходный или роскошный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "люкс",
                                "target_lang_sense_clarification": "неизменяемое определение, указывающее на высший класс (напр. номер люкс)"
                            },
                            {
                                "target_lang_word": "роскошный",
                                "target_lang_sense_clarification": "общий термин для описания богатства и высокого качества"
                            },
                            {
                                "target_lang_word": "элитный",
                                "target_lang_sense_clarification": "предназначенный для узкого круга, высшего разряда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b3879bd7-daa3-3bcc-a825-8117a1ca44cc",
                    "sense_definition": "An item, version, or service of superior quality or luxury compared to the standard.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality_luxury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We ordered three regular burgers and one <w>deluxe</w> for the party.",
                            "target_lang_translations": {
                                "ru": "Мы заказали на вечеринку три обычных бургера и один <w>делюкс</w>."
                            }
                        },
                        {
                            "text": "The store sells standard models, but the <w>deluxes</w> are much more popular with customers.",
                            "target_lang_translations": {
                                "ru": "Магазин продает стандартные модели, но <w>люксы</w> пользуются у покупателей гораздо большей популярностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "premium"
                    ],
                    "antonyms": [
                        "standard"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as a shorthand for a deluxe version of a product"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Товар, версия или услуга высшего качества или повышенной комфортности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "люкс",
                                "target_lang_sense_clarification": "существительное, обычно обозначает номер в отеле или купе высшей категории"
                            },
                            {
                                "target_lang_word": "делюкс",
                                "target_lang_sense_clarification": "версия продукта с расширенными возможностями (часто в названиях товаров)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
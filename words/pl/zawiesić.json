{
    "word_family": [
        "zawieszenie",
        "zawieszka",
        "zawiesina",
        "odwiesić",
        "przewiesić",
        "zwiesić",
        "uwiesić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "333c8b7d-d610-331b-a070-1a4430c119de",
                    "sense_definition": "Umieścić przedmiot na pewnej wysokości, przymocowując go tak, aby swobodnie opadał i nie dotykał podłoża.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_hanging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <zawiesić> ten nowy obraz na ścianie w salonie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <повесить> эту новую картину на стену в гостиной."
                            }
                        },
                        {
                            "text": "Mama poprosiła mnie, abym pomógł jej <zawiesić> czyste firanki.",
                            "target_lang_translations": {
                                "ru": "Мама попросила меня помочь ей <повесить> чистые занавески."
                            }
                        }
                    ],
                    "synonyms": [
                        "powiesić"
                    ],
                    "antonyms": [
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "zawiesić obraz",
                        "zawiesić firanki",
                        "zawiesić na kołku",
                        "zawiesić na haku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить предмет на некоторой высоте, закрепив его так, чтобы он свободно свисал и не касался пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повесить",
                                "target_lang_sense_clarification": "прикрепить что-то на высоте (картину, одежду)"
                            },
                            {
                                "target_lang_word": "подвесить",
                                "target_lang_sense_clarification": "закрепить так, чтобы предмет свисал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "325d9d0b-1515-3497-b6e9-edf593a48647",
                    "sense_definition": "Wstrzymać tymczasowo realizację jakiegoś działania, procesu lub obowiązywanie prawa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_suspension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zarząd firmy zdecydował się <zawiesić> działalność gospodarczą na trzy miesiące.",
                            "target_lang_translations": {
                                "ru": "Правление компании решило <приостановить> хозяйственную деятельность на три месяца."
                            }
                        },
                        {
                            "text": "Związkowcy postanowili <zawiesić> strajk po rozpoczęciu negocjacji z rządem.",
                            "target_lang_translations": {
                                "ru": "Члены профсоюза решили <приостановить> забастовку после начала переговоров с правительством."
                            }
                        }
                    ],
                    "synonyms": [
                        "przerwać",
                        "wykluczyć"
                    ],
                    "antonyms": [
                        "wznowić"
                    ],
                    "common_phrases": [
                        "zawiesić działalność",
                        "zawiesić strajk",
                        "zawiesić wykonanie kary",
                        "zawiesić projekt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно остановить выполнение какого-либо действия, процесса или действие закона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приостановить",
                                "target_lang_sense_clarification": "временно прекратить деятельность или процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a2e637e-a068-3efc-ac43-ec73922d375b",
                    "sense_definition": "Sprawić, że program komputerowy lub urządzenie elektroniczne przestaje reagować na polecenia użytkownika.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_freeze",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uruchomienie zbyt wielu aplikacji naraz może <zawiesić> twój stary komputer.",
                            "target_lang_translations": {
                                "ru": "Запуск слишком большого количества приложений одновременно может <подвесить> твой старый компьютер."
                            }
                        },
                        {
                            "text": "Ten błąd w kodzie potrafi <zawiesić> cały system operacyjny.",
                            "target_lang_translations": {
                                "ru": "Эта ошибка в коде может <подвесить> всю операционную систему."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zawiesić komputer",
                        "zawiesić system",
                        "zawiesić aplikację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести к тому, что компьютерная программа или электронное устройство перестает реагировать на команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвесить",
                                "target_lang_sense_clarification": "компьютерный сленг: вызвать зависание системы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "686c5cff-9381-3f68-adf3-b1918d07c896",
                    "sense_definition": "Pozbawić kogoś tymczasowo prawa do pełnienia funkcji, wykonywania zawodu lub uczestnictwa w czymś, zazwyczaj jako kara.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_suspension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor szkoły musiał <zawiesić> ucznia w prawach za agresywne zachowanie.",
                            "target_lang_translations": {
                                "ru": "Директор школы был вынужден <отстранить> ученика от занятий за агрессивное поведение."
                            }
                        },
                        {
                            "text": "Komisja dyscyplinarna postanowiła <zawiesić> zawodnika na dwa mecze.",
                            "target_lang_translations": {
                                "ru": "Дисциплинарная комиссия постановила <дисквалифицировать> игрока на два матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykluczyć"
                    ],
                    "antonyms": [
                        "przywrócić"
                    ],
                    "common_phrases": [
                        "zawiesić w prawach",
                        "zawiesić w czynnościach",
                        "zawiesić ucznia",
                        "zawiesić pracownika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно лишить кого-либо права выполнять обязанности или участвовать в чем-либо, обычно в качестве наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстранить",
                                "target_lang_sense_clarification": "лишить возможности выполнять обязанности"
                            },
                            {
                                "target_lang_word": "дисквалифицировать",
                                "target_lang_sense_clarification": "в спорте: лишить права участия в соревнованиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd58bb90-a638-30ba-b3f7-f3997ea5eac0",
                    "sense_definition": "Wstrzymać chwilowo połączenie telefoniczne bez rozłączania się, aby rozmówca czekał na linii.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_suspension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sekretarka musiała na chwilę <zawiesić> rozmowę, aby sprawdzić kalendarz szefa.",
                            "target_lang_translations": {
                                "ru": "Секретарше пришлось на минуту <прервать> разговор, чтобы проверить календарь начальника."
                            }
                        },
                        {
                            "text": "Proszę nie rozłączać się, muszę <zawiesić> połączenie i przełączyć pana do innego działu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не вешайте трубку, я должен <поставить> звонок <на удержание> и переключить вас в другой отдел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zawiesić połączenie",
                        "zawiesić rozmowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно прервать телефонное соединение без разрыва связи, чтобы собеседник оставался на линии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поставить на удержание",
                                "target_lang_sense_clarification": "телефонный звонок"
                            },
                            {
                                "target_lang_word": "прервать",
                                "target_lang_sense_clarification": "временно остановить разговор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7444169f-f342-3008-bf19-0a21876a4eed",
                    "sense_definition": "Zewnętrzna warstwa lub strona przedmiotu, która ogranicza go od otoczenia i jest bezpośrednio widoczna lub dotykalna.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górna <w>powierzchnia</w> tego stołu jest bardzo gładka i lśniąca.",
                            "target_lang_translations": {
                                "ru": "Верхняя <w>поверхность</w> этого стола очень гладкая и блестящая."
                            }
                        },
                        {
                            "text": "Musimy dokładnie wyczyścić <w>powierzchnię</w> ściany przed malowaniem.",
                            "target_lang_translations": {
                                "ru": "Мы должны тщательно очистить <w>поверхность</w> стены перед покраской."
                            }
                        }
                    ],
                    "synonyms": [
                        "przestrzeń"
                    ],
                    "antonyms": [
                        "wnętrze",
                        "środek"
                    ],
                    "common_phrases": [
                        "gładka powierzchnia",
                        "duża powierzchnia",
                        "powierzchnia Ziemi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наружный слой или сторона предмета, отделяющая его от окружающей среды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поверхность",
                                "target_lang_sense_clarification": "наружная сторона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fdb9a4a-f459-3afe-8ef2-b8fd133fe014",
                    "sense_definition": "Rozmiar lub wielkość danego obszaru, terenu lub pomieszczenia wyrażona w jednostkach miary (np. w metrach kwadratowych).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Całkowita <w>powierzchnia</w> tego mieszkania wynosi pięćdziesiąt metrów kwadratowych.",
                            "target_lang_translations": {
                                "ru": "Общая <w>площадь</w> этой квартиры составляет пятьдесят квадратных метров."
                            }
                        },
                        {
                            "text": "Jaka jest <w>powierzchnia</w> Polski w kilometrach kwadratowych?",
                            "target_lang_translations": {
                                "ru": "Какова <w>площадь</w> Польши в квадратных километрах?"
                            }
                        }
                    ],
                    "synonyms": [
                        "pole powierzchni",
                        "rozległość",
                        "przestrzeń",
                        "powierzchowność"
                    ],
                    "common_phrases": [
                        "powierzchnia mieszkalna",
                        "powierzchnia biurowa",
                        "powierzchnia całkowita"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размер участка, территории или помещения, выраженный в единицах измерения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "площадь",
                                "target_lang_sense_clarification": "размер пространства"
                            },
                            {
                                "target_lang_word": "пространство",
                                "target_lang_sense_clarification": "место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "451dcffd-e793-379c-9167-76ced6c1f5c4",
                    "sense_definition": "Najwyższa warstwa cieczy lub zbiornika wodnego, która styka się z powietrzem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "liquid_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały korek unosi się swobodnie na <w>powierzchni</w> wody w wiadrze.",
                            "target_lang_translations": {
                                "ru": "Маленькая пробка свободно плавает на <w>поверхности</w> воды в ведре."
                            }
                        },
                        {
                            "text": "Wieloryb wypłynął na <w>powierzchnię</w>, aby zaczerpnąć świeżego powietrza.",
                            "target_lang_translations": {
                                "ru": "Кит всплыл на <w>поверхность</w>, чтобы глотнуть свежего воздуха."
                            }
                        }
                    ],
                    "antonyms": [
                        "dno",
                        "głębia"
                    ],
                    "common_phrases": [
                        "na powierzchni wody",
                        "wypłynąć na powierzchnię",
                        "pod powierzchnią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний слой жидкости или водоема, соприкасающийся с воздухом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поверхность",
                                "target_lang_sense_clarification": "воды или жидкости"
                            },
                            {
                                "target_lang_word": "гладь",
                                "target_lang_sense_clarification": "спокойная поверхность воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d6104c2-e21e-359a-a484-7b85259c74bb",
                    "sense_definition": "Zewnętrzny wygląd czegoś lub kogoś, często postrzegany jako mylący lub nieoddający istoty rzeczy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod spokojną <w>powierzchnią</w> jego zachowania kryły się bardzo silne emocje.",
                            "target_lang_translations": {
                                "ru": "Под спокойной <w>внешностью</w> скрывались очень сильные эмоции."
                            }
                        },
                        {
                            "text": "Nie powinniśmy oceniać ludzi tylko po ich <w>powierzchni</w>, lecz patrzeć głębiej.",
                            "target_lang_translations": {
                                "ru": "Мы не должны судить о людях только по <w>внешности</w>, а смотреть глубже."
                            }
                        }
                    ],
                    "synonyms": [
                        "powierzchowność"
                    ],
                    "antonyms": [
                        "wnętrze",
                        "istota"
                    ],
                    "common_phrases": [
                        "pod powierzchnią",
                        "na powierzchni"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w znaczeniu przenośnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний вид чего-либо или кого-либо, часто обманчивый и не отражающий сути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внешность",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "484784e9-d873-3212-9272-37886546c8c8",
                    "sense_definition": "Отсутствие свободного времени для какого-либо дела или занятия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "no_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Извини, мне сейчас совершенно <w>некогда</w> разговаривать.",
                            "target_lang_translations": {
                                "en": "Sorry, I absolutely <w>have no time</w> to talk right now."
                            }
                        },
                        {
                            "text": "Ему вечно <w>некогда</w>, потому что он очень много работает.",
                            "target_lang_translations": {
                                "en": "He never <w>has time</w> because he works a lot."
                            }
                        }
                    ],
                    "synonyms": [
                        "недосуг"
                    ],
                    "antonyms": [
                        "есть когда"
                    ],
                    "common_phrases": [
                        "мне некогда",
                        "сейчас некогда",
                        "вечно некогда"
                    ],
                    "target_lang_definitions": {
                        "en": "To lack free time to do something; to be too busy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have no time",
                                "target_lang_sense_clarification": "lack of free time"
                            },
                            {
                                "target_lang_word": "be too busy",
                                "target_lang_sense_clarification": "state of being occupied"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4a9d4671-a05d-3d93-bb1b-2ede915045c8",
                    "sense_definition": "В какой-то момент в прошлом; когда-то давно.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "formerly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <w>некогда</w> красивый парк теперь полностью заброшен.",
                            "target_lang_translations": {
                                "en": "This <w>once</w> beautiful park is now completely abandoned."
                            }
                        },
                        {
                            "text": "Здесь <w>некогда</w> стоял большой деревянный дом.",
                            "target_lang_translations": {
                                "en": "A large wooden house <w>once</w> stood here."
                            }
                        }
                    ],
                    "synonyms": [
                        "когда-то",
                        "в своё время"
                    ],
                    "common_phrases": [
                        "некогда известный",
                        "некогда популярный"
                    ],
                    "target_lang_definitions": {
                        "en": "At some time in the past; formerly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "once",
                                "target_lang_sense_clarification": "at some time in the past"
                            },
                            {
                                "target_lang_word": "formerly",
                                "target_lang_sense_clarification": "previously"
                            }
                        ]
                    }
                }
            ]
        }
    ]

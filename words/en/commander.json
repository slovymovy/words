{
    "word_family": [
        "command",
        "commandant",
        "commandment",
        "commandeer",
        "commandery"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f969cde4-abf0-3c28-ab13-68da11fdac52",
                    "sense_definition": "A person who exercises authority and control over a military unit or a group of people.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "authority_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>commander</w> ordered his troops to hold their position until reinforcements arrived.",
                            "target_lang_translations": {
                                "ru": "<w>Командир</w> приказал своим войскам удерживать позицию до прибытия подкрепления."
                            }
                        },
                        {
                            "text": "As the base <w>commander</w>, she is responsible for the safety and discipline of all personnel.",
                            "target_lang_translations": {
                                "ru": "Как <w>командир</w> базы, она отвечает за безопасность и дисциплину всего персонала."
                            }
                        }
                    ],
                    "synonyms": [
                        "leader",
                        "chief",
                        "head",
                        "officer"
                    ],
                    "antonyms": [
                        "subordinate",
                        "follower"
                    ],
                    "common_phrases": [
                        "commander in chief",
                        "military commander",
                        "field commander",
                        "base commander"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, осуществляющее власть и управление воинской частью или группой людей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "руководитель воинского подразделения"
                            },
                            {
                                "target_lang_word": "военачальник",
                                "target_lang_sense_clarification": "высший военный руководитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0347b20d-d444-3623-a353-85961c26fcfd",
                    "sense_definition": "A naval officer whose rank is above that of a lieutenant commander and below that of captain.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "authority_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>commander</w> stood on the bridge, scanning the horizon for any sign of the enemy fleet.",
                            "target_lang_translations": {
                                "ru": "<w>Коммандер</w> стоял на мостике, сканируя горизонт в поисках вражеского флота."
                            }
                        },
                        {
                            "text": "He was promoted to <w>commander</w> after twenty years of distinguished service in the navy.",
                            "target_lang_translations": {
                                "ru": "Он был повышен до <w>коммандера</w> после двадцати лет выдающейся службы на флоте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lieutenant commander",
                        "wing commander"
                    ],
                    "target_lang_definitions": {
                        "ru": "Офицер военно-морского флота, по званию выше лейтенант-коммандера и ниже капитана"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коммандер",
                                "target_lang_sense_clarification": "звание в ВМС США и Великобритании"
                            },
                            {
                                "target_lang_word": "капитан второго ранга",
                                "target_lang_sense_clarification": "российский аналог звания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a57c0d3-351f-33c1-9ee6-ec09ec6bd2d6",
                    "sense_definition": "A high-ranking officer in certain non-military organizations, such as police forces or space agencies.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "authority_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>commander</w> briefed the media on the details of the ongoing investigation.",
                            "target_lang_translations": {
                                "ru": "<w>Начальник</w> полиции проинформировал СМИ о деталях текущего расследования."
                            }
                        },
                        {
                            "text": "She served as the mission <w>commander</w> for the agency's latest expedition to the space station.",
                            "target_lang_translations": {
                                "ru": "Она служила <w>командиром</w> миссии в последней экспедиции агентства на космическую станцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "police commander",
                        "mission commander",
                        "incident commander"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокопоставленный офицер в некоторых невоенных организациях (полиция, космонавтика)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "руководитель в полиции или службе"
                            },
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "руководитель экипажа или миссии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67fce43a-cd7c-3532-bea1-28fd539f316b",
                    "sense_definition": "A specific rank within an honorary order or society.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "authority_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was appointed a <w>Commander</w> of the Order of the British Empire for his services to the arts.",
                            "target_lang_translations": {
                                "ru": "Он был назначен <w>командором</w> Ордена Британской империи за заслуги перед искусством."
                            }
                        },
                        {
                            "text": "The title of <w>Commander</w> is one of the highest honors the society can bestow upon a member.",
                            "target_lang_translations": {
                                "ru": "Титул <w>командора</w> — одна из высших почестей, которые общество может даровать члену."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Knight Commander"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особый ранг в почетном ордене или обществе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командор",
                                "target_lang_sense_clarification": "титул в рыцарских орденах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3720b0a-d0b4-3a3c-97a4-e269a7bb21b7",
                    "sense_definition": "A heavy wooden mallet used in technical trades like paving or sail making.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The worker used a heavy <w>commander</w> to settle the paving stones firmly into the sand.",
                            "target_lang_translations": {
                                "ru": "Рабочий использовал тяжелый <w>деревянный молот</w>, чтобы плотно уложить брусчатку в песок."
                            }
                        },
                        {
                            "text": "In the sail loft, the <w>commander</w> was an essential tool for working with heavy canvas.",
                            "target_lang_translations": {
                                "ru": "В парусной мастерской <w>киянка</w> была важным инструментом для работы с тяжелой парусиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "mallet",
                        "beetle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелый деревянный молоток, используемый в технических ремеслах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киянка",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "деревянный молот",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0ebf6cb-2cb3-3b61-88db-b460fb9b25e6",
                    "sense_definition": "Any of various nymphalid butterflies of the Asian genus Moduza.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The entomologist traveled to the tropical forest to photograph the rare <w>commander</w> in its natural habitat.",
                            "target_lang_translations": {
                                "ru": "Энтомолог отправился в тропический лес, чтобы сфотографировать редкого <w>командора</w> в его естественной среде обитания."
                            }
                        },
                        {
                            "text": "This species of <w>commander</w> is known for the striking pattern on its wings.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>командора</w> известен поразительным узором на крыльях."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность бабочек-нимфалид азиатского рода Moduza"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командор",
                                "target_lang_sense_clarification": "название бабочки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0ee3010b-74ba-3990-ad5d-9ee949f17a3d",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Commander</w> has lived in this neighborhood for over forty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Коммандер</w> живет в этом районе уже более сорока лет."
                            }
                        },
                        {
                            "text": "We are looking for a file belonging to a Jane <w>Commander</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем файл, принадлежащий Джейн <w>Коммандер</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "family name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Коммандер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
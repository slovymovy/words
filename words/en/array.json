    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce61b1e9-240f-37ee-9729-d80e55c4bf0b",
                    "sense_definition": "A large and impressive group or collection of things or people.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "collection_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store offers a dazzling <w>array</w> of fresh produce every morning.",
                            "target_lang_translations": {
                                "ru": "Каждое утро магазин предлагает впечатляющее <w>множество</w> свежих продуктов."
                            }
                        },
                        {
                            "text": "We faced a vast <w>array</w> of challenges when starting the new project.",
                            "target_lang_translations": {
                                "ru": "Мы столкнулись с огромным <w>множеством</w> проблем при запуске нового проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothe",
                        "decorate",
                        "dress"
                    ],
                    "common_phrases": [
                        "wide array",
                        "vast array",
                        "dazzling array",
                        "array of options"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внушительная группа или коллекция вещей или людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "множество",
                                "target_lang_sense_clarification": "большое количество"
                            },
                            {
                                "target_lang_word": "набор",
                                "target_lang_sense_clarification": "совокупность предметов"
                            },
                            {
                                "target_lang_word": "масса",
                                "target_lang_sense_clarification": "разг. большое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "facb1f4f-ebd2-3a9d-8fe1-0bac5765e0e2",
                    "sense_definition": "An orderly series, arrangement, or sequence of things.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "arrangement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The solar <w>array</w> in the desert generates enough power for the whole city.",
                            "target_lang_translations": {
                                "ru": "Солнечная <w>батарея</w> в пустыне вырабатывает достаточно энергии для всего города."
                            }
                        },
                        {
                            "text": "He looked at the confusing <w>array</w> of buttons on the control panel.",
                            "target_lang_translations": {
                                "ru": "Он смотрел на запутанный <w>ряд</w> кнопок на панели управления."
                            }
                        }
                    ],
                    "antonyms": [
                        "disarray"
                    ],
                    "common_phrases": [
                        "solar array",
                        "antenna array"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченная серия, расстановка или последовательность вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ряд",
                                "target_lang_sense_clarification": "последовательность"
                            },
                            {
                                "target_lang_word": "батарея",
                                "target_lang_sense_clarification": "техническое, группа однотипных устройств"
                            },
                            {
                                "target_lang_word": "строй",
                                "target_lang_sense_clarification": "порядок расположения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "519c0538-38a6-3eee-89c7-38db26fd3c31",
                    "sense_definition": "A data structure designed to hold multiple elements of the same type, usually accessed by numeric indices.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "arrangement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program stores the list of student names in a string <w>array</w>.",
                            "target_lang_translations": {
                                "ru": "Программа хранит список имен студентов в строковом <w>массиве</w>."
                            }
                        },
                        {
                            "text": "You can access any item in the <w>array</w> by referring to its index number.",
                            "target_lang_translations": {
                                "ru": "Вы можете получить доступ к любому элементу в <w>массиве</w>, обратившись к его индексу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "byte array",
                        "array index",
                        "multidimensional array"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Структура данных, предназначенная для хранения нескольких элементов одного типа, обычно доступных по числовым индексам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массив",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f95a60d-3af7-3449-8be1-82ab93620642",
                    "sense_definition": "Clothing and ornamentation, especially when rich or elaborate.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The queen appeared on the balcony in full royal <w>array</w>.",
                            "target_lang_translations": {
                                "ru": "Королева появилась на балконе в полном королевском <w>облачении</w>."
                            }
                        },
                        {
                            "text": "He was dressed in the simple <w>array</w> of a traveling merchant.",
                            "target_lang_translations": {
                                "ru": "Он был одет в простой <w>наряд</w> странствующего торговца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rich array",
                        "battle array"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда и украшения, особенно богатые или изысканные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наряд",
                                "target_lang_sense_clarification": "одежда, костюм"
                            },
                            {
                                "target_lang_word": "одеяние",
                                "target_lang_sense_clarification": "торжественная одежда"
                            },
                            {
                                "target_lang_word": "убор",
                                "target_lang_sense_clarification": "устар. одежда, наряд"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eab9edb8-e2a5-3c24-ada4-1e24acae677e",
                    "sense_definition": "To lay out things or people in an orderly arrangement or formation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "arrangement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general <w>arrayed</w> his troops along the ridge before the battle began.",
                            "target_lang_translations": {
                                "ru": "Генерал <w>выстроил</w> свои войска вдоль хребта перед началом битвы."
                            }
                        },
                        {
                            "text": "Various delicious desserts were <w>arrayed</w> on the table for the guests.",
                            "target_lang_translations": {
                                "ru": "Различные вкусные десерты были <w>расставлены</w> на столе для гостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arrayed against",
                        "arrayed for battle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагать вещи или людей в определенном порядке или строю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выстраивать",
                                "target_lang_sense_clarification": "ставить в ряд или строй"
                            },
                            {
                                "target_lang_word": "располагать",
                                "target_lang_sense_clarification": "размещать в определенном порядке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0e868aa-bea7-385e-bb45-dc4d0ac25556",
                    "sense_definition": "To clothe and ornament someone, often in a formal or impressive way.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bride was <w>arrayed</w> in a beautiful gown of white silk.",
                            "target_lang_translations": {
                                "ru": "Невеста была <w>облачена</w> в красивое платье из белого шелка."
                            }
                        },
                        {
                            "text": "The forest was <w>arrayed</w> in the vibrant colors of autumn.",
                            "target_lang_translations": {
                                "ru": "Лес был <w>одет</w> в яркие цвета осени."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothe",
                        "decorate",
                        "dress",
                        "don"
                    ],
                    "common_phrases": [
                        "arrayed in",
                        "arrayed with"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одевать и украшать кого-либо, часто торжественно или впечатляюще."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облачать",
                                "target_lang_sense_clarification": "одевать в торжественную одежду"
                            },
                            {
                                "target_lang_word": "наряжать",
                                "target_lang_sense_clarification": "красиво одевать"
                            },
                            {
                                "target_lang_word": "одевать",
                                "target_lang_sense_clarification": "покрывать одеждой"
                            }
                        ]
                    }
                }
            ]
        }
    ]

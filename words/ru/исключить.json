{
    "word_family": [
        "исключение",
        "исключительный",
        "исключительно"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bab17f4a-df6f-3f31-8d56-4c260cd6e7fe",
                    "sense_definition": "Устранить или удалить кого-либо или что-либо из состава, списка, организации или коллектива.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "remove_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор пригрозил <исключить> хулигана из школы за плохое поведение.",
                            "target_lang_translations": {
                                "en": "The principal threatened to <expel> the hooligan from school for bad behavior."
                            }
                        },
                        {
                            "text": "Мы решили <исключить> этот пункт из договора, так как он нам не подходит.",
                            "target_lang_translations": {
                                "en": "We decided to <remove> this clause from the contract, as it does not suit us."
                            }
                        }
                    ],
                    "synonyms": [
                        "удалить",
                        "вычеркнуть",
                        "отчислить"
                    ],
                    "antonyms": [
                        "включить"
                    ],
                    "common_phrases": [
                        "исключить из списка",
                        "исключить из школы",
                        "исключить из партии",
                        "исключить из состава"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove someone or something from a list, group, or organization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exclude",
                                "target_lang_sense_clarification": "to keep out or remove from a group or list"
                            },
                            {
                                "target_lang_word": "expel",
                                "target_lang_sense_clarification": "to officially force someone to leave a school or organization"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "to take something off a list or out of a document"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee06e8cf-6cc6-36d3-987b-e83589b61f07",
                    "sense_definition": "Сделать невозможным, не допустить появления или осуществления чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "prevent_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи провели тесты, чтобы <исключить> возможность инфекции.",
                            "target_lang_translations": {
                                "en": "The doctors conducted tests to <rule out> the possibility of infection."
                            }
                        },
                        {
                            "text": "Мы не можем полностью <исключить> вероятность ошибки в расчётах.",
                            "target_lang_translations": {
                                "en": "We cannot completely <exclude> the probability of an error in the calculations."
                            }
                        }
                    ],
                    "synonyms": [
                        "устранить",
                        "предотвратить",
                        "отбросить"
                    ],
                    "antonyms": [
                        "включить",
                        "включить"
                    ],
                    "common_phrases": [
                        "исключить возможность",
                        "полностью исключить",
                        "исключить ошибку",
                        "исключить риск"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something impossible or prevent it from happening."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rule out",
                                "target_lang_sense_clarification": "to decide that something is impossible or not the cause"
                            },
                            {
                                "target_lang_word": "exclude",
                                "target_lang_sense_clarification": "to eliminate a possibility or hypothesis"
                            },
                            {
                                "target_lang_word": "eliminate",
                                "target_lang_sense_clarification": "to completely remove a risk or possibility"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
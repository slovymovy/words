                            },
                            {
                                "target_lang_word": "оформление",
                                "target_lang_sense_clarification": "придание документу законной силы"
                            },
                            {
                                "target_lang_word": "подписание",
                                "target_lang_sense_clarification": "заключение договора"
                            }
                        ]
                    }
                }
            ]
        }
    ]

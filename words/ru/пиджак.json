    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4fc738c3-ef1e-30d9-89ad-ae36d546102e",
                    "sense_definition": "Верхняя часть мужского костюма с рукавами, открытым отложным воротником и застёгивающимися полами.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В офисе принято носить строгий костюм и <w>пиджак</w>.",
                            "target_lang_translations": {
                                "en": "It is customary to wear a formal suit and <w>jacket</w> in the office."
                            }
                        },
                        {
                            "text": "Он снял <w>пиджак</w> и повесил его на спинку стула.",
                            "target_lang_translations": {
                                "en": "He took off his <w>jacket</w> and hung it on the back of the chair."
                            }
                        }
                    ],
                    "common_phrases": [
                        "надеть пиджак",
                        "снять пиджак",
                        "карман пиджака",
                        "пуговица пиджака",
                        "новый пиджак"
                    ],
                    "target_lang_definitions": {
                        "en": "The upper part of a men's suit with sleeves, an open turn-down collar, and buttoning flaps."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jacket",
                                "target_lang_sense_clarification": "part of a suit"
                            },
                            {
                                "target_lang_word": "suit jacket",
                                "target_lang_sense_clarification": "formal wear"
                            },
                            {
                                "target_lang_word": "coat",
                                "target_lang_sense_clarification": "suit coat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10a29612-81a0-3ded-b4ad-8b89e0f8a696",
                    "sense_definition": "Короткая женская верхняя одежда, укороченный вариант пиджака.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ней был чёрный длинный <w>пиджак</w> и зелёное платье.",
                            "target_lang_translations": {
                                "en": "She was wearing a long black <w>jacket</w> and a green dress."
                            }
                        },
                        {
                            "text": "Мама купила себе красивый шерстяной <w>пиджак</w> для работы.",
                            "target_lang_translations": {
                                "en": "Mom bought herself a beautiful wool <w>blazer</w> for work."
                            }
                        }
                    ],
                    "synonyms": [
                        "жакет"
                    ],
                    "common_phrases": [
                        "женский пиджак",
                        "длинный пиджак"
                    ],
                    "target_lang_definitions": {
                        "en": "Short women's outerwear, a shortened version of a jacket."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jacket",
                                "target_lang_sense_clarification": "women's clothing"
                            },
                            {
                                "target_lang_word": "blazer",
                                "target_lang_sense_clarification": "smart casual or formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0a2547d-f37f-3adc-8798-d3770d172680",
                    "sense_definition": "Некадровый офицер — выпускник гражданского вуза с военной кафедрой.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кадровые военные часто с иронией называли его <w>пиджаком</w>.",
                            "target_lang_translations": {
                                "en": "Career officers often ironically called him a <w>civilian</w>."
                            }
                        },
                        {
                            "text": "Этот <w>пиджак</w> ничего не смыслит в настоящей строевой подготовке.",
                            "target_lang_translations": {
                                "en": "This <w>civilian</w> knows nothing about real drill training."
                            }
                        }
                    ],
                    "synonyms": [
                        "двухгодичник"
                    ],
                    "antonyms": [
                        "сапог"
                    ],
                    "common_phrases": [
                        "вечный пиджак"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "военный"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A non-career officer — a graduate of a civilian university with a military department."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "civilian",
                                "target_lang_sense_clarification": "military slang for non-career officer"
                            },
                            {
                                "target_lang_word": "paper officer",
                                "target_lang_sense_clarification": "derogatory slang"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7b0fcad-baac-301e-94e2-2b5b08f711a5",
                    "sense_definition": "Кавалер, ухажёр.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твой новый <w>пиджак</w> уже ждёт тебя у подъезда с цветами.",
                            "target_lang_translations": {
                                "en": "Your new <w>suitor</w> is already waiting for you at the entrance with flowers."
                            }
                        },
                        {
                            "text": "На танцы она пришла одна, но её <w>пиджак</w> скоро появился.",
                            "target_lang_translations": {
                                "en": "She came to the dance alone, but her <w>beau</w> soon appeared."
                            }
                        }
                    ],
                    "synonyms": [
                        "кавалер",
                        "ухажёр"
                    ],
                    "traits": [

                    ],
                    "common_phrases": [
                        "continuous function",
                        "continuous variable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывающий математическую функцию или кривую, которая не имеет резких изменений, разрывов или пробелов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непрерывный",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

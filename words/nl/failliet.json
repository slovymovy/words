{
    "word_family": [
        "faillissement",
        "faling"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2beb69b9-8d72-34d3-bc38-d214234afb22",
                    "sense_definition": "Niet meer in staat om schulden te betalen en daardoor in een toestand van bankroet verkerend.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>failliete</w> bedrijf moest al zijn werknemers ontslaan.",
                            "target_lang_translations": {
                                "ru": "<w>Обанкротившаяся</w> компания была вынуждена уволить всех своих сотрудников."
                            }
                        },
                        {
                            "text": "Door de economische crisis dreigen veel kleine winkels <w>failliet</w> te gaan.",
                            "target_lang_translations": {
                                "ru": "Из-за экономического кризиса многие мелкие магазины рискуют <w>обанкротиться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bankroet"
                    ],
                    "antonyms": [
                        "solvabel"
                    ],
                    "common_phrases": [
                        "failliet gaan",
                        "failliet verklaren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный платить долги и, следовательно, находящийся в состоянии банкротства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обанкротившийся",
                                "target_lang_sense_clarification": "прилагательное/причастие"
                            },
                            {
                                "target_lang_word": "несостоятельный",
                                "target_lang_sense_clarification": "финансово"
                            },
                            {
                                "target_lang_word": "разорившийся",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ed46284-f265-3912-8637-e7b665defbde",
                    "sense_definition": "De toestand waarin men niet meer aan betalingsverplichtingen kan voldoen, of figuurlijk de volledige mislukking van iets.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Critici beschouwen het nieuwe beleid als het <w>failliet</w> van de democratie.",
                            "target_lang_translations": {
                                "ru": "Критики рассматривают новую политику как <w>крах</w> демократии."
                            }
                        },
                        {
                            "text": "Na jaren van verlies was het <w>failliet</w> van de onderneming onvermijdelijk.",
                            "target_lang_translations": {
                                "ru": "После многих лет убытков <w>банкротство</w> предприятия было неизбежным."
                            }
                        }
                    ],
                    "synonyms": [
                        "faillissement",
                        "bankroet",
                        "faling"
                    ],
                    "common_phrases": [
                        "het failliet van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние неспособности выполнять платежные обязательства или, в переносном смысле, полный провал чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крах",
                                "target_lang_sense_clarification": "переносное значение, провал"
                            },
                            {
                                "target_lang_word": "банкротство",
                                "target_lang_sense_clarification": "финансовое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "429acbb6-e164-3aa4-89bd-0c8a11710880",
                    "sense_definition": "Een persoon of bedrijf die door de rechtbank failliet is verklaard.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De curator beheert tijdelijk het vermogen van de <w>failliet</w>.",
                            "target_lang_translations": {
                                "ru": "Конкурсный управляющий временно распоряжается имуществом <w>банкрота</w>."
                            }
                        },
                        {
                            "text": "Als <w>failliet</w> ben je niet langer bevoegd om over je eigen bezittingen te beschikken.",
                            "target_lang_translations": {
                                "ru": "Как <w>банкрот</w>, вы больше не имеете права распоряжаться собственным имуществом."
                            }
                        }
                    ],
                    "synonyms": [
                        "bankroetier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или компания, объявленные судом банкротом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банкрот",
                                "target_lang_sense_clarification": "юридический статус лица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
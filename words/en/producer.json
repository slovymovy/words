    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12a3bb22-16e2-3972-8b29-f23731e12bbe",
                    "sense_definition": "One who manages and oversees the creation of an artistic project, such as a film, album, play, or television show.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>producer</w> hired a famous director for the new movie.",
                            "target_lang_translations": {
                                "ru": "<w>Продюсер</w> нанял известного режиссёра для нового фильма."
                            }
                        },
                        {
                            "text": "She wants to become a music <w>producer</w> after she finishes college.",
                            "target_lang_translations": {
                                "ru": "Она хочет стать музыкальным <w>продюсером</w> после окончания колледжа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "executive producer",
                        "music producer",
                        "film producer",
                        "record producer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто организует и контролирует создание художественного проекта, например фильма, музыкального альбома, спектакля или телешоу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продюсер",
                                "target_lang_sense_clarification": "в кино, музыке, на ТВ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd6bfc49-f23c-3687-b2ec-d6009a62f5df",
                    "sense_definition": "An individual or company that makes, grows, or supplies goods and commodities for sale.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brazil is the world's largest <w>producer</w> of coffee.",
                            "target_lang_translations": {
                                "ru": "Бразилия — крупнейший в мире <w>производитель</w> кофе."
                            }
                        },
                        {
                            "text": "Local <w>producers</w> sell their fresh vegetables at the weekend market.",
                            "target_lang_translations": {
                                "ru": "Местные <w>производители</w> продают свои свежие овощи на рынке в выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "manufacturer",
                        "maker",
                        "creator",
                        "supplier"
                    ],
                    "antonyms": [
                        "consumer"
                    ],
                    "common_phrases": [
                        "major producer",
                        "food producer",
                        "oil producer",
                        "agricultural producer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или компания, которые производят, выращивают или поставляют товары на продажу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производитель",
                                "target_lang_sense_clarification": "товаров, услуг"
                            },
                            {
                                "target_lang_word": "изготовитель",
                                "target_lang_sense_clarification": "продукции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea6d6996-9629-39b1-aae3-3b0f2540b86c",
                    "sense_definition": "An organism, usually a plant or bacterium, that produces its own food from simple inorganic substances.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grass acts as a <w>producer</w> at the bottom of the food chain.",
                            "target_lang_translations": {
                                "ru": "Трава выступает в роли <w>продуцента</w> в основании пищевой цепи."
                            }
                        },
                        {
                            "text": "In this ecosystem, algae is the primary <w>producer</w> of energy.",
                            "target_lang_translations": {
                                "ru": "В этой экосистеме водоросли являются основным <w>продуцентом</w> энергии."
                            }
                        }
                    ],
                    "synonyms": [
                        "autotroph"
                    ],
                    "antonyms": [
                        "consumer",
                        "heterotroph"
                    ],
                    "common_phrases": [
                        "primary producer"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Организм (обычно растение или бактерия), производящий собственную пищу из простых неорганических веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продуцент",
                                "target_lang_sense_clarification": "биология"
                            },
                            {
                                "target_lang_word": "производитель",
                                "target_lang_sense_clarification": "в пищевой цепи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d01ab8b-af35-3bf5-8702-96d10f834021",
                    "sense_definition": "A type of furnace used to generate combustible gas for fuel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory installed a new <w>producer</w> to power the engines efficiently.",
                            "target_lang_translations": {
                                "ru": "На заводе установили новый <w>газогенератор</w> для эффективного питания двигателей."
                            }
                        },
                        {
                            "text": "Gas generated by the <w>producer</w> was used for heating the metal.",
                            "target_lang_translations": {
                                "ru": "Газ, вырабатываемый <w>газогенератором</w>, использовался для нагрева металла."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gas producer"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип печи, используемой для получения горючего газа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газогенератор",
                                "target_lang_sense_clarification": "техническое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22da3719-7e05-33c1-aa7e-dc329db6f05f",
                    "sense_definition": "A requirement issued by police during a traffic stop to present driving documents at a station within a set number of days.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The officer gave him a <w>producer</w> because he didn't have his license with him.",
                            "target_lang_translations": {
                                "ru": "Офицер выписал ему <w>предписание</w>, так как у него не было с собой прав."
                            }
                        },
                        {
                            "text": "I have to go to the police station today to sort out a <w>producer</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сегодня зайти в полицейский участок, чтобы разобраться с <w>предписанием</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK, Ireland"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "police jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Требование полиции предъявить водительские документы в участке в течение определенного срока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предписание",
                                "target_lang_sense_clarification": "предъявить документы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Starożytne cywilizacje rozwijały się w żyznym dorzeczu <w>Tygrysu</w>.",
                            "target_lang_translations": {
                                "ru": "Древние цивилизации развивались в плодородном бассейне <w>Тигра</w>."
                            }
                        },
                        {
                            "text": "Rzeka <w>Tygrys</w> jest kluczowym źródłem wody dla tego regionu.",
                            "target_lang_translations": {
                                "ru": "Река <w>Тигр</w> является ключевым источником воды для этого региона."
                            }
                        }
                    ],
                    "common_phrases": [

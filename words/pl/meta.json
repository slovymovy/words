                {
                    "sense_id": "e8fcd64a-7039-3e84-9620-17bd99fb0e94",
                    "sense_definition": "Linia lub punkt kończący wyścig, bieg lub inne zawody sportowe.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "end_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawodnik był tak wyczerpany, że ledwo dobiegł do <w>mety</w>.",
                            "target_lang_translations": {
                                "ru": "Спортсмен был так измотан, что едва добежал до <w>финиша</w>."
                            }
                        },
                        {
                            "text": "Wszyscy kibice zgromadzili się przy linii <w>mety</w>, aby zobaczyć zwycięzcę.",
                            "target_lang_translations": {
                                "ru": "Все болельщики собрались у линии <w>финиша</w>, чтобы увидеть победителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "miejscówka"
                    ],
                    "antonyms": [
                        "start"
                    ],
                    "common_phrases": [
                        "dobiec do mety",
                        "linia mety",
                        "na mecie",
                        "sędzia mety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Линия или точка, завершающая гонку, забег или другие спортивные соревнования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "финиш",
                                "target_lang_sense_clarification": "спорт"

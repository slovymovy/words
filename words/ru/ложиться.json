                    "sense_id": "2e14f082-b258-3bf8-960c-5b99028f80ab",
                    "sense_definition": "Принимать горизонтальное положение на какой-либо поверхности.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я устал стоять и хочу <ложиться> на диван.",
                            "target_lang_translations": {
                                "en": "I am tired of standing and want to <lie down> on the sofa."
                            }
                        },
                        {
                            "text": "Собака медленно <ложится> на коврик у двери.",
                            "target_lang_translations": {
                                "en": "The dog slowly <lies down> on the mat by the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "укладываться"
                    ],
                    "antonyms": [
                        "вставать"
                    ],
                    "common_phrases": [
                        "ложиться на пол",
                        "ложиться на спину",
                        "ложиться на живот"
                    ],
                    "target_lang_definitions": {
                        "en": "To assume a horizontal position on a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie down",
                                "target_lang_sense_clarification": "general physical action"
                            },
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "short form"
                            }
                        ]
                    }
                },
                {

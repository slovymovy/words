            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "56e05ce6-3d79-3737-b276-e386b01520b1",
                    "sense_definition": "Прямо, без участия третьих лиц, посредников или промежуточных звеньев.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили обратиться к директору <w>непосредственно</w>, минуя секретаря.",
                            "target_lang_translations": {
                                "en": "We decided to appeal to the director <w>directly</w>, bypassing the secretary."
                            }
                        },
                        {
                            "text": "Этот вопрос касается вас <w>непосредственно</w>, поэтому прошу внимания.",
                            "target_lang_translations": {
                                "en": "This issue concerns you <w>directly</w>, so I ask for your attention."
                            }
                        }
                    ],
                    "synonyms": [
                        "напрямую"
                    ],
                    "antonyms": [
                        "косвенно",
                        "опосредованно"
                    ],
                    "common_phrases": [
                        "непосредственно участвовать",
                        "непосредственно влиять",
                        "обращаться непосредственно",
                        "подчиняться непосредственно"
                    ],
                    "target_lang_definitions": {
                        "en": "Directly, without the participation of third parties, intermediaries, or intermediate links."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "directly",
                                "target_lang_sense_clarification": "without intermediaries"
                            },
                            {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6c2147e-abc8-38d8-857d-03e7396bbde8",
                    "sense_definition": "To move past someone or something; to go from one side to the other.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You will <w>pass</w> the post office on your right side.",
                            "target_lang_translations": {
                                "ru": "Вы <w>пройдете</w> почту справа."
                            }
                        },
                        {
                            "text": "We watched the long parade <w>pass</w> down the street.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели, как длинный парад <w>проходит</w> по улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "go by",
                        "move",
                        "go"
                    ],
                    "common_phrases": [
                        "pass by",
                        "pass through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить мимо кого-то или чего-то; перемещаться с одной стороны на другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "двигаться мимо"
                            },
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "оставлять позади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1aad98e5-bd4e-3022-9759-f225200a1954",
                    "sense_definition": "To give or transfer something to someone using one's hands.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you please <w>pass</w> the salt to your father?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <w>передать</w> соль отцу?"
                            }
                        },
                        {
                            "text": "The teacher <w>passed</w> the worksheets to the students in the front row.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>передал</w> рабочие листы ученикам в первом ряду."
                            }
                        }
                    ],
                    "synonyms": [
                        "give",
                        "hand",
                        "transfer",
                        "deliver"
                    ],
                    "common_phrases": [
                        "pass the salt",
                        "pass a note"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать или передавать что-то кому-то из рук в руки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "вручать"
                            },
                            {
                                "target_lang_word": "подавать",
                                "target_lang_sense_clarification": "давать в руки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edbc2276-4b13-3670-adce-5e1843df9fbd",
                    "sense_definition": "To be successful in an examination, test, or inspection.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "success_validation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She studied every night to <w>pass</w> her driving test.",
                            "target_lang_translations": {
                                "ru": "Она занималась каждую ночь, чтобы <w>сдать</w> экзамен по вождению."
                            }
                        },
                        {
                            "text": "If you do not <w>pass</w> the exam, you will have to take the course again.",
                            "target_lang_translations": {
                                "ru": "Если вы не <w>сдадите</w> экзамен, вам придется пройти курс заново."
                            }
                        }
                    ],
                    "synonyms": [
                        "succeed"
                    ],
                    "antonyms": [
                        "meet"
                    ],
                    "common_phrases": [
                        "pass a test",
                        "pass an exam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешно пройти экзамен, тест или проверку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "экзамен или тест"
                            },
                            {
                                "target_lang_word": "пройти",
                                "target_lang_sense_clarification": "проверку или испытание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04d8bcab-4d3e-3817-b10c-61b092f5f132",
                    "sense_definition": "To move through time; to go by (regarding time).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_elapse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten years have <w>passed</w> since we last saw each other.",
                            "target_lang_translations": {
                                "ru": "Десять лет <w>прошло</w> с тех пор, как мы виделись в последний раз."
                            }
                        },
                        {
                            "text": "Time <w>passes</w> very quickly when you are having fun.",
                            "target_lang_translations": {
                                "ru": "Время <w>проходит</w> очень быстро, когда тебе весело."
                            }
                        }
                    ],
                    "synonyms": [
                        "elapse",
                        "go by"
                    ],
                    "common_phrases": [
                        "time passes",
                        "days pass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить (о времени); протекать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "заканчиваться (о сроке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4cad327-8241-3245-8fb8-f58c4540ac88",
                    "sense_definition": "To spend a period of time doing something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_spend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We played card games to <w>pass</w> the time while waiting for the train.",
                            "target_lang_translations": {
                                "ru": "Мы играли в карты, чтобы <w>скоротать</w> время в ожидании поезда."
                            }
                        },
                        {
                            "text": "He reads books to <w>pass</w> the long winter evenings.",
                            "target_lang_translations": {
                                "ru": "Он читает книги, чтобы <w>коротать</w> долгие зимние вечера."
                            }
                        }
                    ],
                    "synonyms": [
                        "spend"
                    ],
                    "common_phrases": [
                        "pass the time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить время за каким-либо занятием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коротать",
                                "target_lang_sense_clarification": "время"
                            },
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e36b3812-b67d-339a-8201-07b8180c0a70",

{
    "word_family": [
        "hellish",
        "hellion"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59b1dbda-a4dd-3d23-ba79-bc7e844a1c97",
                    "sense_definition": "Used as an intensifier in phrases to express anger, surprise, or emphasis, often grammatically functioning as a noun.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What the <w>hell</w> are you doing in my room?",
                            "target_lang_translations": {
                                "ru": "Какого <w>чёрта</w> ты делаешь в моей комнате?"
                            }
                        },
                        {
                            "text": "We decided to drive to the beach just for the <w>hell</w> of it.",
                            "target_lang_translations": {
                                "ru": "Мы решили поехать на пляж просто так, от <w>нечего делать</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "what the hell",
                        "bloody hell",
                        "hell of a",
                        "for the hell of it",
                        "like hell"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used for emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления в фразах, выражающих гнев, удивление или для эмфазы; часто переводится как «чёрт» в составе устойчивых выражений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрт",
                                "target_lang_sense_clarification": "в составе восклицаний и идиом (напр. «какого чёрта»)"
                            },
                            {
                                "target_lang_word": "хрен",
                                "target_lang_sense_clarification": "грубое просторечное усиление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f672502-98b8-39ce-9445-3dbec4bcc469",
                    "sense_definition": "A place or situation of great suffering, misery, or chaos in life.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "suffering_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My new job is absolute <w>hell</w> because of the stress.",
                            "target_lang_translations": {
                                "ru": "Моя новая работа — сущий <w>ад</w> из-за стресса."
                            }
                        },
                        {
                            "text": "They went through <w>hell</w> to finish the project on time.",
                            "target_lang_translations": {
                                "ru": "Они прошли через <w>ад</w>, чтобы закончить проект вовремя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go through hell",
                        "living hell",
                        "hell on earth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или ситуация, причиняющая сильные страдания, мучения или хаос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ад",
                                "target_lang_sense_clarification": "невыносимая ситуация"
                            },
                            {
                                "target_lang_word": "пекло",
                                "target_lang_sense_clarification": "хаос, неразбериха, тяжелые условия"
                            },
                            {
                                "target_lang_word": "кошмар",
                                "target_lang_sense_clarification": "ужасная ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff13e834-521c-3af3-b42e-e419c9af40f1",
                    "sense_definition": "Something extremely painful or harmful to a specific part of the body or system.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "suffering_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Walking up those steep stairs is <w>hell</w> on my knees.",
                            "target_lang_translations": {
                                "ru": "Подниматься по этим крутым лестницам — сущий <w>ад</w> для моих коленей."
                            }
                        },
                        {
                            "text": "This rough terrain is <w>hell</w> on the tires of the truck.",
                            "target_lang_translations": {
                                "ru": "Эта пересеченная местность — <w>смерть</w> для шин грузовика."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то чрезвычайно болезненное или вредное для определенной части тела или системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ад",
                                "target_lang_sense_clarification": "сильная боль или мучение"
                            },
                            {
                                "target_lang_word": "мучение",
                                "target_lang_sense_clarification": "источник страданий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ebef7774-6686-30ca-b4d3-f58dbd3f0928",
                    "sense_definition": "A place of torment where sinners are believed to go after death.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "theological_place",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Many religions teach that evil people will go to <w>Hell</w>.",
                            "target_lang_translations": {
                                "ru": "Многие религии учат, что злые люди попадут в <w>Ад</w>."
                            }
                        },
                        {
                            "text": "He was afraid of burning in <w>Hell</w> for his sins.",
                            "target_lang_translations": {
                                "ru": "Он боялся гореть в <w>Аду</w> за свои грехи."
                            }
                        }
                    ],
                    "synonyms": [
                        "afterlife"
                    ],
                    "antonyms": [
                        "heaven"
                    ],
                    "common_phrases": [
                        "go to hell",
                        "rot in hell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место мучений, куда, по религиозным представлениям, попадают грешники после смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ад",
                                "target_lang_sense_clarification": "загробный мир для грешников"
                            },
                            {
                                "target_lang_word": "преисподняя",
                                "target_lang_sense_clarification": "нижний мир, бездна"
                            },
                            {
                                "target_lang_word": "пекло",
                                "target_lang_sense_clarification": "адский огонь, геенна"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e71f9629-6c64-3b39-8fa2-761520c74b21",
                    "sense_definition": "Used to express discontent, unhappiness, annoyance, or anger.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_anger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oh <w>hell</w>! I forgot to bring my wallet.",
                            "target_lang_translations": {
                                "ru": "<w>Чёрт</w>! Я забыл взять кошелек."
                            }
                        },
                        {
                            "text": "<w>Hell</w>, I didn't mean to break the vase.",
                            "target_lang_translations": {
                                "ru": "<w>Чёрт</w>, я не хотел разбить вазу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oh hell"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения недовольства, досады, раздражения или гнева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрт",
                                "target_lang_sense_clarification": "восклицание при досаде"
                            },
                            {
                                "target_lang_word": "проклятье",
                                "target_lang_sense_clarification": "выражение гнева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74324c5a-7004-3172-982f-8692e726655f",
                    "sense_definition": "Used to emphasize a statement or express strong agreement.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exclamation_emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hell</w> yeah! I would love to come to the party.",
                            "target_lang_translations": {
                                "ru": "<w>Чёрт возьми</w>, да! Я с удовольствием приду на вечеринку."
                            }
                        },
                        {
                            "text": "Will I ever do that again? <w>Hell</w> no!",
                            "target_lang_translations": {
                                "ru": "Сделаю ли я это снова? <w>Чёрта с два</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "hell yeah",
                        "hell no"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления утверждения или выражения решительного согласия (или несогласия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрт возьми",
                                "target_lang_sense_clarification": "эмоциональное усиление согласия"
                            },
                            {
                                "target_lang_word": "ещё как",
                                "target_lang_sense_clarification": "разговорное выражение сильного согласия"
                            },
                            {
                                "target_lang_word": "чёрта с два",
                                "target_lang_sense_clarification": "категорическое отрицание (hell no)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ab4459cc-f82d-32a4-909d-2d3cdfff0d0f",
                    "sense_definition": "Very; used to emphasize an adjective or adverb strongly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adverb_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That movie was <w>hell</w> funny.",
                            "target_lang_translations": {
                                "ru": "Этот фильм был <w>чертовски</w> смешным."
                            }
                        },
                        {
                            "text": "It is <w>hell</w> expensive to live in the city center.",
                            "target_lang_translations": {
                                "ru": "Жить в центре города <w>адски</w> дорого."
                            }
                        }
                    ],
                    "synonyms": [
                        "hella"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень; используется для сильного усиления значения прилагательного или наречия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чертовски",
                                "target_lang_sense_clarification": "очень, крайне"
                            },
                            {
                                "target_lang_word": "адски",
                                "target_lang_sense_clarification": "невероятно, ужасно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9b0dc027-ccd2-376e-be0d-56fc0ebc61f0",
                    "sense_definition": "To move quickly, loudly, or recklessly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went <w>helling</w> down the highway in his sports car.",
                            "target_lang_translations": {
                                "ru": "Он <w>мчался</w> по шоссе на своей спортивной машине."
                            }
                        },
                        {
                            "text": "Stop <w>helling</w> around and get some work done.",
                            "target_lang_translations": {
                                "ru": "Перестань <w>беситься</w> и займись делом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hell around"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться быстро, шумно или безрассудно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "быстро двигаться"
                            },
                            {
                                "target_lang_word": "беситься",
                                "target_lang_sense_clarification": "вести себя шумно и безрассудно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
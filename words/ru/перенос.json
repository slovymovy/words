    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ded1d27-78b7-3599-9f1a-4f2ee8bfac69",
                    "sense_definition": "Изменение времени или даты запланированного события на более поздний срок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за плохой погоды был объявлен <w>перенос</w> рейса на следующее утро.",
                            "target_lang_translations": {
                                "en": "Due to bad weather, the <w>rescheduling</w> of the flight to the next morning was announced."
                            }
                        },
                        {
                            "text": "Мы согласовали с клиентом <w>перенос</w> встречи на понедельник.",
                            "target_lang_translations": {
                                "en": "We agreed with the client on the <w>rescheduling</w> of the meeting to Monday."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перенос встречи",
                        "перенос сроков",
                        "перенос даты"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of changing the time or date of a planned event to a later time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rescheduling",
                                "target_lang_sense_clarification": "setting a new time for an event"
                            },
                            {
                                "target_lang_word": "postponement",
                                "target_lang_sense_clarification": "delaying an event to a later time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c323c4b7-9c61-3c6e-b826-edb870a51397",
                    "sense_definition": "Перемещение объекта, вещества или информации из одного места в другое.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для <w>переноса</w> тяжёлой мебели нам потребовалась помощь грузчиков.",
                            "target_lang_translations": {
                                "en": "We needed the help of movers for the <w>moving</w> of heavy furniture."
                            }
                        },
                        {
                            "text": "Сейчас выполняется автоматический <w>перенос</w> данных на новый сервер.",
                            "target_lang_translations": {
                                "en": "Automatic data <w>transfer</w> to the new server is currently being performed."
                            }
                        }
                    ],
                    "synonyms": [
                        "перемещение",
                        "трансфер",
                        "перекладывание",
                        "перевод"
                    ],
                    "common_phrases": [
                        "перенос данных",
                        "перенос столицы",
                        "устройство для переноса"
                    ],
                    "target_lang_definitions": {
                        "en": "The movement of an object, substance, or information from one place to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transfer",
                                "target_lang_sense_clarification": "general movement or data migration"
                            },
                            {
                                "target_lang_word": "moving",
                                "target_lang_sense_clarification": "physical relocation of objects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35a720cc-359c-3247-a25e-956fd6ecfe7e",
                    "sense_definition": "Членение слова для записи его части на следующей строке, а также графический знак, обозначающий это действие.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В начальной школе дети изучают правила <w>переноса</w> слов.",
                            "target_lang_translations": {
                                "en": "In elementary school, children study the rules of word <w>hyphenation</w>."
                            }
                        },
                        {
                            "text": "Компьютерная программа неправильно расставила знаки <w>переноса</w> в тексте.",
                            "target_lang_translations": {
                                "en": "The computer program incorrectly placed the <w>hyphenation</w> marks in the text."
                            }
                        }
                    ],
                    "synonyms": [
                        "слогоделение",
                        "разбиение"
                    ],
                    "common_phrases": [
                        "правила переноса",
                        "знак переноса",
                        "автоматический перенос"
                    ],
                    "target_lang_definitions": {
                        "en": "The division of a word at the end of a line, or the character indicating this."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hyphenation",
                                "target_lang_sense_clarification": "linguistic term for word division"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5926dd4a-4220-3716-92fa-2cebe78f95a0",
                    "sense_definition": "Бессознательное перемещение ранее пережитых чувств и отношений, проявлявшихся к одному лицу, на другое лицо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "psychology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Психоаналитик объяснил пациенту, что его реакция — это типичный <w>перенос</w>.",
                            "target_lang_translations": {
                                "en": "The psychoanalyst explained to the patient that his reaction is typical <w>transference</w>."
                            }
                        },
                        {
                            "text": "В процессе терапии часто возникает эмоциональный <w>перенос</w> на врача.",
                            "target_lang_translations": {
                                "en": "During therapy, emotional <w>transference</w> onto the doctor often occurs."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перенос и контрперенос",
                        "явление переноса"
                    ],
                    "target_lang_definitions": {
                        "en": "The unconscious redirection of feelings and attitudes from one person to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transference",
                                "target_lang_sense_clarification": "psychology term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f172fd12-c9bf-391a-9be6-191f240ab9fc",
                    "sense_definition": "Скопление снега, наметённое ветром поперёк дороги.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимняя дорога была трудной из-за частых снежных <w>переносов</w>.",
                            "target_lang_translations": {
                                "en": "The winter road was difficult due to frequent <w>snow drifts</w>."
                            }

{
    "word_family": [
        "overloop",
        "lopen",
        "loop"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9ee4349b-734c-3561-8f6a-599556d8970c",
                    "sense_definition": "Over de rand stromen omdat een vat of ruimte te vol is (van vloeistoffen).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "overflow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bad is <w>overgelopen</w> omdat ik de kraan was vergeten dicht te draaien.",
                            "target_lang_translations": {
                                "ru": "Ванна <w>перелилась через край</w>, потому что я забыл закрыть кран.",
                                "en": "The bath <w>overflowed</w> because I forgot to turn off the tap."
                            }
                        },
                        {
                            "text": "Door de hevige regenval <w>liepen</w> de sloten <w>over</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного ливня канавы <w>вышли из берегов</w>.",
                                "en": "Due to the heavy rainfall, the ditches <w>overflowed</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "overstromen"
                    ],
                    "common_phrases": [
                        "de emmer loopt over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переливаться через край из-за переполнения сосуда или пространства (о жидкостях).",
                        "en": "To flow over the edge because a vessel or space is too full (of liquids)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переливаться (через край)",
                                "target_lang_sense_clarification": "о жидкости в емкости"
                            },
                            {
                                "target_lang_word": "выходить из берегов",
                                "target_lang_sense_clarification": "о водоемах"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "overflow",
                                "target_lang_sense_clarification": "of liquids or containers"
                            },
                            {
                                "target_lang_word": "run over",
                                "target_lang_sense_clarification": "spill over the edge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97946109-3f9e-36fe-81b5-8631c8423841",
                    "sense_definition": "Iets stap voor stap bekijken, lezen of bespreken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "review",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we de agenda voor de vergadering even <w>overlopen</w>?",
                            "target_lang_translations": {
                                "ru": "Может, быстро <w>пробежимся</w> по повестке дня собрания?",
                                "en": "Shall we briefly <w>run through</w> the agenda for the meeting?"
                            }
                        },
                        {
                            "text": "De leraar <w>overliep</w> de belangrijkste punten van de les nog een keer.",
                            "target_lang_translations": {
                                "ru": "Учитель еще раз <w>прошелся</w> по главным пунктам урока.",
                                "en": "The teacher <w>went over</w> the most important points of the lesson one more time."
                            }
                        }
                    ],
                    "synonyms": [
                        "doornemen"
                    ],
                    "common_phrases": [
                        "een lijst overlopen",
                        "de agenda overlopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассматривать, читать или обсуждать что-либо шаг за шагом.",
                        "en": "To examine, read, or discuss something step by step."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просматривать",
                                "target_lang_sense_clarification": "изучать или проверять"
                            },
                            {
                                "target_lang_word": "пробегать",
                                "target_lang_sense_clarification": "быстро читать или обсуждать"
                            },
                            {
                                "target_lang_word": "проходиться",
                                "target_lang_sense_clarification": "разбирать по пунктам"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "go over",
                                "target_lang_sense_clarification": "examine details"
                            },
                            {
                                "target_lang_word": "run through",
                                "target_lang_sense_clarification": "review quickly"
                            },
                            {
                                "target_lang_word": "review",
                                "target_lang_sense_clarification": "inspect or evaluate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c185aa72-296d-3a5d-bf87-17b308c49f42",
                    "sense_definition": "Van de ene partij overgaan naar de vijandige andere partij.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat besloot om naar de vijand <w>over</w> te <w>lopen</w>.",
                            "target_lang_translations": {
                                "ru": "Солдат решил <w>перебежать</w> к врагу.",
                                "en": "The soldier decided to <w>defect</w> to the enemy."
                            }
                        },
                        {
                            "text": "Tijdens de oorlog zijn er veel spionnen <w>overgelopen</w>.",
                            "target_lang_translations": {
                                "ru": "Во время войны многие шпионы <w>перешли на сторону врага</w>.",
                                "en": "During the war, many spies <w>defected</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar de vijand overlopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходить от одной стороны к враждебной другой стороне.",
                        "en": "To switch from one party to the hostile opposing party."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перебегать",
                                "target_lang_sense_clarification": "к врагу"
                            },
                            {
                                "target_lang_word": "переходить",
                                "target_lang_sense_clarification": "менять сторону конфликта"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "defect",
                                "target_lang_sense_clarification": "political or military context"
                            },
                            {
                                "target_lang_word": "switch sides",
                                "target_lang_sense_clarification": "change allegiance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3395a3ba-d1fa-3c4c-9f8e-e814664ab8b7",
                    "sense_definition": "Geleidelijk in iets anders overgaan zonder duidelijke grens.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "blend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleuren van de regenboog <w>lopen</w> prachtig in elkaar <w>over</w>.",
                            "target_lang_translations": {
                                "ru": "Цвета радуги красиво <w>переходят</w> друг в друга.",
                                "en": "The colors of the rainbow <w>blend</w> beautifully into one another."
                            }
                        },
                        {
                            "text": "Waar de stad <w>overloopt</w> in het platteland, is het erg rustig.",
                            "target_lang_translations": {
                                "ru": "Там, где город <w>переходит</w> в сельскую местность, очень тихо.",
                                "en": "Where the city <w>merges</w> into the countryside, it is very quiet."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in elkaar overlopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постепенно переходить во что-то другое без четкой границы.",
                        "en": "To gradually change into something else without a clear boundary."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переходить",
                                "target_lang_sense_clarification": "плавно сменяться"
                            },
                            {
                                "target_lang_word": "сливаться",
                                "target_lang_sense_clarification": "соединяться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "blend",
                                "target_lang_sense_clarification": "mix together (e.g., colors)"
                            },
                            {
                                "target_lang_word": "merge",
                                "target_lang_sense_clarification": "combine or transition into"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50d2371f-d89b-353c-a774-607bdcd5b3e7",
                    "sense_definition": "Door een te grote menigte mensen bezocht of betreden worden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "overrun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het populaire museum werd in het weekend volledig <w>overlopen</w> door toeristen.",
                            "target_lang_translations": {
                                "ru": "В выходные популярный музей был полностью <w>наводнен</w> туристами.",
                                "en": "The popular museum was completely <w>overrun</w> by tourists during the weekend."
                            }
                        },
                        {
                            "text": "We werden <w>overlopen</w> met vragen na de presentatie.",
                            "target_lang_translations": {
                                "ru": "После презентации нас <w>завалили</w> вопросами.",
                                "en": "We were <w>overwhelmed</w> with questions after the presentation."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in passive constructions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть посещаемым или заполненным слишком большой толпой людей.",
                        "en": "To be visited or entered by an excessively large crowd of people."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наводнять",
                                "target_lang_sense_clarification": "заполнять людьми"
                            },
                            {
                                "target_lang_word": "заваливать",
                                "target_lang_sense_clarification": "перегружать (вопросами, работой)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "overrun",
                                "target_lang_sense_clarification": "occupy in large numbers"
                            },
                            {
                                "target_lang_word": "overwhelm",
                                "target_lang_sense_clarification": "burden with too much (figurative)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7be2196a-dee3-3c71-9b8d-5ee2c5095f46",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>overloop</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>overlopen</w> in dit gebouw zijn breed en licht.",
                            "target_lang_translations": {
                                "ru": "<w>Лестничные площадки</w> в этом здании широкие и светлые.",
                                "en": "The <w>landings</w> in this building are wide and light."
                            }
                        },
                        {
                            "text": "Op de <w>overlopen</w> stonden grote kasten met beddengoed.",
                            "target_lang_translations": {
                                "ru": "На <w>лестничных площадках</w> стояли большие шкафы с постельным бельем.",
                                "en": "Large cupboards with bedding stood on the <w>landings</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного 'overloop' (лестничная площадка).",
                        "en": "Plural of the noun 'overloop' (landing)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лестничные площадки",
                                "target_lang_sense_clarification": "пространство перед лестницей"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "landings",
                                "target_lang_sense_clarification": "area at the top of stairs"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "d7499a0e-9499-3854-9446-0df8a3f11608",
                    "sense_definition": "The number occurring after nine and before eleven, represented as 10.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She counted to <w>ten</w> before opening her eyes.",
                            "target_lang_translations": {
                                "ru": "Она сосчитала до <w>десяти</w>, прежде чем открыть глаза."
                            }
                        },
                        {
                            "text": "The meeting is scheduled for <w>ten</w> o'clock.",
                            "target_lang_translations": {
                                "ru": "Встреча запланирована на <w>десять</w> часов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "top ten",
                        "ten o'clock",
                        "count to ten",
                        "ten times"
                    ],
                    "target_lang_definitions": {
                        "ru": "Число, следующее за девяткой и перед одиннадцатью, обозначаемое как 10."

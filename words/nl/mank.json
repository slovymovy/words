            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e35acc02-db5f-3026-b80a-b2b682143157",
                    "sense_definition": "Niet goed kunnen lopen door een verwonding, pijn of een afwijking aan een been of voet.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_disability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sinds zijn ongeluk loopt hij een beetje <w>mank</w>.",
                            "target_lang_translations": {
                                "en": "Since his accident, he has been walking with a slight <w>limp</w>.",
                                "ru": "После аварии он немного <w>хромает</w>."
                            }
                        },
                        {
                            "text": "Het oude paard is <w>mank</w> en kan niet meer werken.",
                            "target_lang_translations": {
                                "en": "The old horse is <w>lame</w> and can no longer work.",
                                "ru": "Старая лошадь <w>хромая</w> и больше не может работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "kreupel"
                    ],
                    "common_phrases": [
                        "mank lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "Unable to walk properly due to an injury, pain, or a defect in a leg or foot.",
                        "ru": "Неспособность нормально ходить из-за травмы, боли или дефекта ноги или ступни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lame",
                                "target_lang_sense_clarification": "physical condition of leg or foot"
                            },
                            {
                                "target_lang_word": "limping",
                                "target_lang_sense_clarification": "state of walking unevenly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хромой",
                                "target_lang_sense_clarification": "физический недостаток"
                            },
                            {
                                "target_lang_word": "увечный",
                                "target_lang_sense_clarification": "имеющий физический дефект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea7e884f-e77a-361a-8154-743dc75041dc",
                    "sense_definition": "Niet kloppend, onvolkomen of gebrekkig (vaak gebruikt bij vergelijkingen).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is een <w>manke</w> vergelijking die eigenlijk nergens op slaat.",
                            "target_lang_translations": {
                                "en": "That is a <w>flawed</w> comparison that actually makes no sense.",
                                "ru": "Это <w>хромое</w> сравнение, которое на самом деле никуда не годится."
                            }
                        },
                        {
                            "text": "Het bewijs voor zijn theorie is helaas nogal <w>mank</w>.",
                            "target_lang_translations": {
                                "en": "The evidence for his theory is unfortunately rather <w>shaky</w>.",
                                "ru": "Доказательство его теории, к сожалению, довольно <w>слабое</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "manke vergelijking"
                    ],
                    "target_lang_definitions": {
                        "en": "Incorrect, imperfect, or defective (often used in comparisons).",
                        "ru": "Неверный, несовершенный или дефектный (часто используется в сравнениях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flawed",
                                "target_lang_sense_clarification": "regarding logic or comparisons"
                            },
                            {
                                "target_lang_word": "shaky",
                                "target_lang_sense_clarification": "regarding evidence or arguments"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хромой",
                                "target_lang_sense_clarification": "переносное значение: неудачный, неубедительный"
                            },
                            {
                                "target_lang_word": "неудачный",
                                "target_lang_sense_clarification": "о сравнении или аргументе"
                            }
                        ]
                    }
                }
            ]
        },

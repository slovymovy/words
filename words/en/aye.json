{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "88ce3ab1-9ca3-3991-a676-5df854f9f69a",
                    "sense_definition": "Yes; a word expressing agreement, assent, or an affirmative answer.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"<w>Aye</w>, I will help you with the work tomorrow.\"",
                            "target_lang_translations": {
                                "ru": "\"<w>Да</w>, я помогу тебе с работой завтра.\""
                            }
                        },
                        {
                            "text": "\"<w>Aye</w>, captain, we are ready to set sail immediately.\"",
                            "target_lang_translations": {
                                "ru": "\"<w>Есть</w>, капитан, мы готовы отплыть немедленно.\""
                            }
                        }
                    ],
                    "synonyms": [
                        "yes",
                        "yea"
                    ],
                    "antonyms": [
                        "no",
                        "nay"
                    ],
                    "common_phrases": [
                        "aye aye"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly Scottish, Northern English, or nautical"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "in standard non-dialectal English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Да; слово, выражающее согласие, подтверждение или утвердительный ответ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "общее значение, часто диалектное или устаревшее"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "морской или военный ответ, подтверждение приказа"
                            },
                            {
                                "target_lang_word": "так точно",
                                "target_lang_sense_clarification": "военный утвердительный ответ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c41023e-037c-3a23-a7b7-5c803ac259a4",
                    "sense_definition": "An affirmative vote, or a person who votes in favor of a motion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>ayes</w> have it, and the new law is passed.",
                            "target_lang_translations": {
                                "ru": "Большинство <w>голосов за</w>, и новый закон принят."
                            }
                        },
                        {
                            "text": "We counted twenty <w>ayes</w> and only five nays during the meeting.",
                            "target_lang_translations": {
                                "ru": "Мы насчитали двадцать <w>голосов за</w> и только пять против во время собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "yes"
                    ],
                    "antonyms": [
                        "nay"
                    ],
                    "common_phrases": [
                        "the ayes have it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Голос «за» или человек, голосующий в поддержку предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голос за",
                                "target_lang_sense_clarification": "при голосовании"
                            },
                            {
                                "target_lang_word": "сторонник",
                                "target_lang_sense_clarification": "тот, кто голосует за"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d953a53b-7211-3412-a4ad-075e5eb9bd2b",
                    "sense_definition": "Ever or always; used to express eternity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He promised to remain faithful to his country for <w>aye</w>.",
                            "target_lang_translations": {
                                "ru": "Он обещал хранить верность своей стране <w>вечно</w>."
                            }
                        },
                        {
                            "text": "Their names shall be remembered for ever and for <w>aye</w>.",
                            "target_lang_translations": {
                                "ru": "Их имена запомнят во веки веков и <w>навсегда</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for ever and for aye"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary or poetic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечно или всегда; используется для выражения вечности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "поэтическое или устаревшее"
                            },
                            {
                                "target_lang_word": "навсегда",
                                "target_lang_sense_clarification": "о времени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc4a08a6-278b-38bb-ba9c-b9adf2c90f76",
                    "sense_definition": "To respond with the word \"aye\" to show agreement.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crew members <w>ayed</w> in unison when asked if they were ready.",
                            "target_lang_translations": {
                                "ru": "Члены экипажа хором <w>выразили согласие</w>, когда их спросили, готовы ли они."
                            }
                        },
                        {
                            "text": "He <w>ayed</w> his consent to the proposal without hesitation.",
                            "target_lang_translations": {
                                "ru": "Он без колебаний <w>подтвердил</w> своё согласие на предложение."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвечать утвердительно в знак согласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выражать согласие",
                                "target_lang_sense_clarification": "отвечать утвердительно"
                            },
                            {
                                "target_lang_word": "голосовать за",
                                "target_lang_sense_clarification": "поддерживать предложение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",

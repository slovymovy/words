    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cae1158e-7db5-3592-9cf6-6de0978ab2ae",
                    "sense_definition": "Według określonych zasad, przepisów lub ustaleń; stosownie do czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystko poszło <zgodnie> z naszym wcześniejszym planem.",
                            "target_lang_translations": {
                                "ru": "Всё прошло <согласно> нашему первоначальному плану."
                            }
                        },
                        {
                            "text": "Należy postępować <zgodnie> z instrukcją obsługi urządzenia.",
                            "target_lang_translations": {
                                "ru": "Следует действовать <согласно> инструкции по эксплуатации устройства."
                            }
                        }
                    ],
                    "antonyms": [
                        "niezgodnie"
                    ],
                    "common_phrases": [
                        "zgodnie z prawem",
                        "zgodnie z planem",
                        "zgodnie z instrukcją",
                        "zgodnie z umową"
                    ],
                    "target_lang_definitions": {
                        "ru": "В соответствии с определёнными правилами, предписаниями или договорённостями; сообразно чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласно",
                                "target_lang_sense_clarification": "в соответствии с чем-либо"
                            },
                            {
                                "target_lang_word": "в соответствии",
                                "target_lang_sense_clarification": "следуя правилам или плану"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cca07627-8989-3d9f-aba6-c11cd37e267a",
                    "sense_definition": "W sposób harmonijny, przyjazny, bez kłótni i konfliktów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_harmony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci bawiły się <zgodnie> w piaskownicy przez całe popołudnie.",
                            "target_lang_translations": {
                                "ru": "Дети <дружно> играли в песочнице весь день."
                            }
                        },
                        {
                            "text": "Sąsiedzi żyli ze sobą <zgodnie> i często sobie pomagali.",
                            "target_lang_translations": {
                                "ru": "Соседи жили <мирно> и часто помогали друг другу."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezkonfliktowo"
                    ],
                    "antonyms": [
                        "konfliktowo",
                        "niezgodnie"
                    ],
                    "common_phrases": [
                        "żyć zgodnie",
                        "bawić się zgodnie",
                        "współpracować zgodnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гармонично, дружелюбно, без ссор и конфликтов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дружно",
                                "target_lang_sense_clarification": "мирно, сообща"
                            },
                            {
                                "target_lang_word": "мирно",
                                "target_lang_sense_clarification": "без конфликтов"
                            },
                            {
                                "target_lang_word": "согласно",
                                "target_lang_sense_clarification": "в гармонии (устар./книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01b209c8-8423-338a-a8d8-5b260445493a",
                    "sense_definition": "W sposób wyrażający pełną jedność poglądów lub decyzji; jednomyślnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_harmony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krytycy <zgodnie> uznali ten film za arcydzieło roku.",
                            "target_lang_translations": {
                                "ru": "Критики <единодушно> признали этот фильм шедевром года."
                            }
                        },
                        {
                            "text": "Wszyscy zebrani <zgodnie> poparli nowy projekt ustawy.",
                            "target_lang_translations": {
                                "ru": "Все собравшиеся <единогласно> поддержали новый законопроект."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozbieżnie"
                    ],
                    "common_phrases": [
                        "zgodnie twierdzić",
                        "zgodnie uznać",
                        "zgodnie zdecydować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражая полное единство взглядов или решений; единодушно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "единодушно",
                                "target_lang_sense_clarification": "с полным согласием во мнениях"
                            },
                            {
                                "target_lang_word": "единогласно",
                                "target_lang_sense_clarification": "общим решением (при голосовании)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

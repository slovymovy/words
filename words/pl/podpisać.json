            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b2a9cd60-3408-3cd9-8acf-b17c0496afe8",
                    "sense_definition": "Złożyć własnoręczny podpis na dokumencie, liście lub innym piśmie w celu jego zatwierdzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <podpisać> tę umowę o pracę jeszcze dzisiaj.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <подписать> этот трудовой договор сегодня."
                            }
                        },
                        {
                            "text": "Dyrektor odmówił i nie chciał <podpisać> dokumentu.",
                            "target_lang_translations": {
                                "ru": "Директор отказался и не захотел <подписывать> документ."
                            }
                        }
                    ],
                    "synonyms": [
                        "sygnować",
                        "parafować"
                    ],
                    "common_phrases": [
                        "podpisać umowę",
                        "podpisać dokument",
                        "podpisać list",
                        "podpisać petycję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить собственноручную подпись на документе, письме или другой бумаге для его утверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписать",
                                "target_lang_sense_clarification": "документ, договор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf31d6cc-d912-38f0-84d2-d8b46a392f04",
                    "sense_definition": "Napisać swoje imię i nazwisko (złożyć podpis) na czymś; używane z zaimkiem zwrotnym \"się\".",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <podpisać> się czytelnie na dole strony.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <распишитесь> разборчиво внизу страницы."
                            }
                        },
                        {
                            "text": "Kurier poprosił mnie, abym <podpisał> się na tablecie.",
                            "target_lang_translations": {
                                "ru": "Курьер попросил меня <расписаться> на планшете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "podpisać się czytelnie",
                        "podpisać się imieniem",
                        "podpisać się pod czymś"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Написать своё имя и фамилию (поставить подпись) на чём-либо; часто используется в значении подтверждения ознакомления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расписаться",
                                "target_lang_sense_clarification": "поставить росчерк"
                            },
                            {
                                "target_lang_word": "подписаться",
                                "target_lang_sense_clarification": "поставить подпись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95c3576f-9207-3d5e-bc5e-3cc942f24374",
                    "sense_definition": "Dodać napis, tytuł lub informację do ilustracji, zdjęcia lub przedmiotu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel kazał nam <podpisać> wszystkie zeszyty i podręczniki.",
                            "target_lang_translations": {
                                "ru": "Учитель велел нам <подписать> все тетради и учебники."
                            }
                        },
                        {
                            "text": "Zapomniałem <podpisać> zdjęcie z wakacji datą jego wykonania.",
                            "target_lang_translations": {
                                "ru": "Я забыл <подписать> фотографию из отпуска датой съёмки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatytułować",
                        "oznaczyć",
                        "opisać"
                    ],
                    "common_phrases": [
                        "podpisać zdjęcie",
                        "podpisać zeszyt",
                        "podpisać pracę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавить надпись, заголовок или информацию на предмет, иллюстрацию или фотографию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписать",
                                "target_lang_sense_clarification": "сделать надпись, озаглавить"
                            }
                        ]
                    }
                }
            ]
        }
    ]

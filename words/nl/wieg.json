            "senses": [
                {
                    "sense_id": "fa9096aa-510d-32b0-a2db-1c71b7e88af5",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>wiegen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wieg</w> de baby zachtjes in mijn armen.",
                            "target_lang_translations": {

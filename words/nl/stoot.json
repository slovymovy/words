    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ce9c180-f5dd-3979-9132-8a9fc858d87a",
                    "sense_definition": "Een korte, krachtige duw of slag tegen iets of iemand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bokser gaf zijn tegenstander een harde <w>stoot</w>.",
                            "target_lang_translations": {
                                "ru": "Боксер нанес противнику сильный <w>удар</w>."
                            }
                        },
                        {
                            "text": "Hij gaf de deur een <w>stoot</w> met zijn schouder om hem te openen.",
                            "target_lang_translations": {
                                "ru": "Он открыл дверь сильным <w>толчком</w> плеча."
                            }
                        }
                    ],
                    "synonyms": [
                        "duw",
                        "klap",
                        "slag",
                        "stomp"
                    ],
                    "common_phrases": [
                        "een stoot geven",
                        "tegen een stootje kunnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий, сильный толчок или удар по чему-либо или кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "резкое физическое воздействие"
                            },
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "резкое движение от себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "272bd050-bbcf-3410-b2eb-81d1b5b1b673",
                    "sense_definition": "Een zeer aantrekkelijk persoon, meestal een jonge vrouw.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen keek om toen die blonde <w>stoot</w> het café binnenliep.",
                            "target_lang_translations": {
                                "ru": "Все обернулись, когда эта белокурая <w>красотка</w> вошла в кафе."
                            }
                        },
                        {
                            "text": "Hij vindt zijn nieuwe vriendin echt een <w>stoot</w>.",
                            "target_lang_translations": {
                                "ru": "Он считает свою новую подружку настоящей <w>красоткой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stuk"
                    ],
                    "common_phrases": [
                        "wat een stoot"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень привлекательный человек, обычно молодая женщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красотка",
                                "target_lang_sense_clarification": "разговорное, о красивой девушке"
                            },
                            {
                                "target_lang_word": "штучка",
                                "target_lang_sense_clarification": "сленг, привлекательная девушка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a5f7756-2a97-383f-b8a8-20b6b0709f82",
                    "sense_definition": "Een plotselinge, kortdurende hoeveelheid of toename van iets.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrik zorgde voor een <w>stoot</w> adrenaline in zijn lichaam.",
                            "target_lang_translations": {
                                "ru": "Испуг вызвал <w>выброс</w> адреналина в его организме."
                            }
                        },
                        {
                            "text": "Met een extra <w>stoot</w> energie maakte hij de klus eindelijk af.",
                            "target_lang_translations": {
                                "ru": "С дополнительным <w>приливом</w> энергии он наконец закончил работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een stoot adrenaline",
                        "een stoot energie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное, кратковременное появление или увеличение чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прилив",
                                "target_lang_sense_clarification": "энергии или сил"
                            },
                            {
                                "target_lang_word": "выброс",
                                "target_lang_sense_clarification": "адреналина или эмоций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8add74b2-0b02-3528-9a6c-6a5870bf5222",
                    "sense_definition": "Een opmerkelijke daad, streek of sterk verhaal.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is weer zo'n typische <w>stoot</w> die alleen hij kan uithalen.",
                            "target_lang_translations": {
                                "ru": "Это типичный <w>номер</w>, который может выкинуть только он."
                            }
                        },
                        {
                            "text": "Hij vertelde een sterke <w>stoot</w> over zijn avonturen in het buitenland.",
                            "target_lang_translations": {
                                "ru": "Он рассказал крутую <w>байку</w> о своих приключениях за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "streek",
                        "poets"
                    ],
                    "common_phrases": [
                        "een stoot uithalen",
                        "een sterke stoot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Примечательный поступок, выходка или небылица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "номер",
                                "target_lang_sense_clarification": "выходка или трюк"
                            },
                            {
                                "target_lang_word": "байка",
                                "target_lang_sense_clarification": "выдуманная история"
                            },
                            {
                                "target_lang_word": "выходка",
                                "target_lang_sense_clarification": "неожиданный поступок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b322efe3-1624-3daf-a58d-67e5bbf25bd8",
                    "sense_definition": "Vervoeging van <w>stoten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd, en gebiedende wijs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>stoot</w> per ongeluk mijn glas water om.",
                            "target_lang_translations": {
                                "ru": "Я случайно <w>опрокидываю</w> стакан воды."
                            }
                        },
                        {
                            "text": "<w>Stoot</w> je hoofd niet als je door die lage deur gaat.",
                            "target_lang_translations": {
                                "ru": "Не <w>ударься</w> головой, когда будешь проходить через эту низкую дверь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {

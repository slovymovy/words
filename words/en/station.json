                {
                    "sense_id": "7abeff63-8c49-3451-b464-542593f586ee",
                    "sense_definition": "A regular stopping place for trains, buses, or other ground transportation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train arrived at the <w>station</w> on time.",
                            "target_lang_translations": {
                                "ru": "Поезд прибыл на <w>станцию</w> вовремя."
                            }
                        },
                        {
                            "text": "I will meet you outside the bus <w>station</w>.",
                            "target_lang_translations": {
                                "ru": "Я встречу тебя у авто<w>вокзала</w>."
                            }
                        },
                        {
                            "text": "The next <w>station</w> is Esperanza.",
                            "target_lang_translations": {
                                "ru": "Следующая <w>станция</w> — Эсперанса."
                            }

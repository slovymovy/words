            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f07939e7-beeb-38e3-a3a8-af074cf7296b",
                    "sense_definition": "Een meisje of jonge vrouw.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>meiden</w> uit mijn klas gaan vanavond samen naar de bioscoop.",
                            "target_lang_translations": {
                                "en": "The <w>girls</w> from my class are going to the cinema together tonight.",
                                "ru": "<w>Девчонки</w> из моего класса сегодня вечером идут вместе в кино."
                            }
                        },
                        {
                            "text": "Wat ben jij toch een grote <w>meid</w> geworden!",
                            "target_lang_translations": {
                                "en": "What a big <w>girl</w> you have become!",
                                "ru": "Какая же ты стала большая <w>девочка</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "grote meid",
                        "leuke meid",
                        "stoere meid"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "vaak informeler dan 'meisje'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A girl or young woman.",
                        "ru": "Девочка или молодая женщина (часто используется в разговорной речи)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "girl",
                                "target_lang_sense_clarification": "young female"
                            },
                            {
                                "target_lang_word": "lass",
                                "target_lang_sense_clarification": "girl or young woman (informal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "девчонка",
                                "target_lang_sense_clarification": "разговорное, девочка-подросток или молодая женщина"
                            },
                            {
                                "target_lang_word": "девочка",
                                "target_lang_sense_clarification": "ребенок женского пола"
                            },
                            {
                                "target_lang_word": "девушка",
                                "target_lang_sense_clarification": "молодая женщина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66e84e07-56b3-3ee9-b94c-376c84f80a97",
                    "sense_definition": "Een aanspreekvorm voor een vrouwelijk huisdier, meestal een hond.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kom hier, brave <w>meid</w>, ik heb iets lekkers voor je.",
                            "target_lang_translations": {
                                "en": "Come here, good <w>girl</w>, I have a treat for you.",
                                "ru": "Иди сюда, хорошая <w>девочка</w>, у меня есть для тебя кое-что вкусненькое."
                            }
                        },
                        {
                            "text": "De hond kwispelde vrolijk toen haar baasje 'goed zo <w>meid</w>' riep.",
                            "target_lang_translations": {
                                "en": "The dog wagged her tail happily when her owner called out 'good <w>girl</w>'.",
                                "ru": "Собака радостно виляла хвостом, когда хозяин крикнул «молодец, <w>девочка</w>»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brave meid"
                    ],
                    "target_lang_definitions": {
                        "en": "A form of address for a female pet, usually a dog.",
                        "ru": "Ласковое обращение к самке домашнего животного, обычно к собаке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "girl",
                                "target_lang_sense_clarification": "address to a female animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "девочка",
                                "target_lang_sense_clarification": "обращение к животному женского пола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bab274db-b567-35fa-9f8b-8123cefabe12",
                    "sense_definition": "Een vrouw die als beroep huishoudelijk werk doet in een particulier huishouden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vroeger hadden rijke families vaak een <w>meid</w> in dienst voor het huishouden.",
                            "target_lang_translations": {
                                "en": "In the past, wealthy families often employed a <w>maid</w> for the household.",
                                "ru": "Раньше богатые семьи часто нанимали <w>служанку</w> для ведения хозяйства."
                            }
                        },
                        {
                            "text": "De <w>meid</w> zorgde ervoor dat alle kamers elke dag netjes werden schoongemaakt.",
                            "target_lang_translations": {
                                "en": "The <w>maid</w> ensured that all rooms were cleaned neatly every day.",
                                "ru": "<w>Служанка</w> следила за тем, чтобы все комнаты убирались каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "dienstbode",
                        "dienstmaagd",
                        "dienstmeid"
                    ],
                    "common_phrases": [
                        "de meid van"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "tegenwoordig spreekt men eerder van een huishoudelijke hulp"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A woman employed to do domestic work in a private household.",
                        "ru": "Женщина, работающая прислугой в частном доме (устаревшее или пренебрежительное)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maid",
                                "target_lang_sense_clarification": "female domestic worker"
                            },
                            {
                                "target_lang_word": "maidservant",
                                "target_lang_sense_clarification": "female servant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "служанка",
                                "target_lang_sense_clarification": "женщина-прислуга"
                            },
                            {
                                "target_lang_word": "домработница",
                                "target_lang_sense_clarification": "помощница по хозяйству"
                            }
                        ]
                    }
                }
            ]
        }

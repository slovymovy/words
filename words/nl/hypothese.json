{
    "word_family": [
        "hypothetisch"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ee7be80-43c0-3684-952d-ed894a2c0598",
                    "sense_definition": "Een beredeneerde verklaring voor waarnemingen die nog door onderzoek bevestigd of weerlegd moet worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De onderzoekers wilden hun <w>hypothese</w> toetsen met een uitgebreid experiment.",
                            "target_lang_translations": {
                                "ru": "Исследователи хотели проверить свою <w>гипотезу</w> с помощью масштабного эксперимента."
                            }
                        },
                        {
                            "text": "Volgens de wetenschappelijke <w>hypothese</w> wordt dit gedrag veroorzaakt door omgevingsfactoren.",
                            "target_lang_translations": {
                                "ru": "Согласно научной <w>гипотезе</w>, это поведение вызвано факторами окружающей среды."
                            }
                        }
                    ],
                    "synonyms": [
                        "stelling"
                    ],
                    "common_phrases": [
                        "een hypothese toetsen",
                        "een hypothese verwerpen",
                        "wetenschappelijke hypothese"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обоснованное объяснение наблюдений, которое ещё предстоит подтвердить или опровергнуть в ходе исследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гипотеза",
                                "target_lang_sense_clarification": "научное предположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5999379-4dec-3267-b6c4-845ac9eb5305",
                    "sense_definition": "Een veronderstelling die men als uitgangspunt neemt zonder zekerheid over de juistheid ervan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is slechts een <w>hypothese</w> dat hij de dader is, want er is geen bewijs.",
                            "target_lang_translations": {
                                "ru": "То, что он преступник — это лишь <w>гипотеза</w>, так как доказательств нет."
                            }
                        },
                        {
                            "text": "Zijn hele plan is gebaseerd op een <w>hypothese</w> die waarschijnlijk niet klopt.",
                            "target_lang_translations": {
                                "ru": "Весь его план основан на <w>гипотезе</w>, которая, вероятно, неверна."
                            }
                        }
                    ],
                    "synonyms": [
                        "veronderstelling"
                    ],
                    "common_phrases": [
                        "gebaseerd op een hypothese",
                        "een hypothese aannemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предположение, принимаемое за отправную точку без уверенности в его правильности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гипотеза",
                                "target_lang_sense_clarification": "допущение"
                            },
                            {
                                "target_lang_word": "предположение",
                                "target_lang_sense_clarification": "догадка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
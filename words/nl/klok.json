                            {
                                "target_lang_word": "клош",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "272527f3-e4d2-3ae3-8a08-eb6f3a3d2b42",

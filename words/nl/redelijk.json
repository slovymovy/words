            "senses": [
                {
                    "sense_id": "b56f0f33-db6c-3c01-a4b1-e273cd1adf8e",
                    "sense_definition": "Acceptabel van kwaliteit, hoeveelheid of prijs; niet te slecht en niet te duur.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "acceptability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit restaurant kun je lekker eten tegen <w>redelijke</w> prijzen.",

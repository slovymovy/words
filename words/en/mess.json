                            },
                            {
                                "target_lang_word": "растяпа",
                                "target_lang_sense_clarification": "неловкий, рассеянный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "664f8288-dfa9-3989-8858-dd97b57fd71a",

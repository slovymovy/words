    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c664d8e0-ca15-390c-81aa-dab53095e85f",
                    "sense_definition": "Выпуск в свет нового продукта (фильма, книги, музыкального альбома, компьютерной программы), а также сам этот продукт.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый <w>релиз</w> популярной компьютерной игры ожидается в конце этого года.",
                            "target_lang_translations": {
                                "en": "A new <w>release</w> of the popular computer game is expected at the end of this year."
                            }
                        },
                        {
                            "text": "В каталоге музыкального лейбла насчитывается уже более десятка успешных <w>релизов</w>.",
                            "target_lang_translations": {
                                "en": "The music label's catalog already counts more than a dozen successful <w>releases</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дата релиза",
                        "официальный релиз",
                        "график релизов",
                        "новый релиз"
                    ],
                    "target_lang_definitions": {
                        "en": "The release of a new product (film, book, music album, computer program), as well as the product itself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "product launch"
                            },
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "act of releasing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "672e34de-4292-367e-b682-f63c9a664b3b",
                    "sense_definition": "Информационное сообщение для прессы; то же, что пресс-релиз.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания разослала официальный <w>релиз</w> о назначении нового генерального директора.",
                            "target_lang_translations": {
                                "en": "The company sent out an official <w>press release</w> about the appointment of a new CEO."
                            }
                        },
                        {
                            "text": "Я узнала из утреннего <w>релиза</w>, что премьера спектакля состоится летом.",
                            "target_lang_translations": {
                                "en": "I learned from the morning <w>press release</w> that the play's premiere will take place in the summer."
                            }
                        }
                    ],
                    "synonyms": [
                        "пресс-релиз"
                    ],
                    "common_phrases": [
                        "подготовить релиз",
                        "читать в релизе",
                        "разослать релиз"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An informational message for the press; same as a press release."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "press release",
                                "target_lang_sense_clarification": "media statement"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "statement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2932813-16ab-3427-bb68-b3398802c798",
                    "sense_definition": "Комиссионное вознаграждение, выплачиваемое биржевым маклером посреднику за поиск покупателя ценных бумаг.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маклер выплатил посреднику оговоренный <w>релиз</w> после успешного завершения сделки.",
                            "target_lang_translations": {
                                "en": "The broker paid the intermediary the agreed <w>commission</w> after the successful completion of the deal."
                            }
                        },
                        {
                            "text": "Размер <w>релиза</w> зависит от объёма проданных ценных бумаг и условий договора.",
                            "target_lang_translations": {
                                "en": "The size of the <w>commission</w> depends on the volume of securities sold and the terms of the contract."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A commission fee paid by a stockbroker to an intermediary for finding a buyer for securities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commission",
                                "target_lang_sense_clarification": "brokerage fee"
                            },
                            {
                                "target_lang_word": "finder's fee",
                                "target_lang_sense_clarification": "payment for intermediary"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
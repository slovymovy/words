    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d19c417a-b573-3be5-9dab-ca68b92023f6",
                    "sense_definition": "Taki, który nie cierpi na żadną chorobę, ma się dobrze i jest w dobrej kondycji fizycznej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "stan_zdrowia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek ma dziewięćdziesiąt lat, ale wciąż jest silny i <w>zdrowy</w>.",
                            "target_lang_translations": {
                                "ru": "Моему дедушке девяносто лет, но он всё ещё силён и <w>здоров</w>."
                            }
                        },
                        {
                            "text": "Po tygodniu leczenia pacjent jest już całkowicie <w>zdrowy</w> i może wrócić do domu.",
                            "target_lang_translations": {
                                "ru": "После недели лечения пациент уже полностью <w>здоров</w> и может вернуться домой."
                            }
                        },
                        {
                            "text": "Musisz brać witaminy, żeby być <w>zdrowym</w> przez całą zimę.",
                            "target_lang_translations": {
                                "ru": "Ты должен принимать витамины, чтобы быть <w>здоровым</w> всю зиму."
                            }
                        }
                    ],
                    "antonyms": [
                        "chory",
                        "niezdrowy"
                    ],
                    "common_phrases": [
                        "zdrowy jak ryba",
                        "cały i zdrowy",
                        "bądź zdrów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не страдает от болезни, чувствует себя хорошо и находится в хорошей физической форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "не больной, обладающий хорошим здоровьем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65697af1-8b52-3e70-9f66-94e94cfcda2a",
                    "sense_definition": "Dobry dla zdrowia, służący organizmowi, korzystnie wpływający na kondycję.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wplyw_na_zdrowie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Warzywa i owoce są bardzo <w>zdrowe</w>, dlatego powinieneś jeść ich dużo.",
                            "target_lang_translations": {
                                "ru": "Овощи и фрукты очень <w>полезны</w>, поэтому тебе следует есть их много."
                            }
                        },
                        {
                            "text": "Prowadzę <w>zdrowy</w> tryb życia, biegając codziennie rano w parku.",
                            "target_lang_translations": {
                                "ru": "Я веду <w>здоровый</w> образ жизни, бегая каждое утро в парке."
                            }
                        },
                        {
                            "text": "To nie jest <w>zdrowe</w> jedzenie, ponieważ zawiera dużo cukru i tłuszczu.",
                            "target_lang_translations": {
                                "ru": "Это не <w>здоровая</w> еда, так как она содержит много сахара и жира."
                            }
                        }
                    ],
                    "antonyms": [
                        "niezdrowy",
                        "szkodliwy"
                    ],
                    "common_phrases": [
                        "zdrowy tryb życia",
                        "zdrowe odżywianie",
                        "zdrowa żywność",
                        "zdrowa dieta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полезный для здоровья, благотворно влияющий на организм и самочувствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полезный",
                                "target_lang_sense_clarification": "благотворный для здоровья (о еде, привычках)"
                            },
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "способствующий здоровью (напр. здоровый образ жизни)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1d289c5-7daf-3946-b81f-30ac05653710",
                    "sense_definition": "Oparty na rozsądku, logiczny i racjonalny (zwykle w odniesieniu do myślenia lub zasad).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rozsadek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej trudnej sytuacji uratował nas tylko jego <w>zdrowy</w> rozsądek.",
                            "target_lang_translations": {
                                "ru": "В этой трудной ситуации нас спас только его <w>здравый</w> смысл."
                            }
                        },
                        {
                            "text": "To są <w>zdrowe</w> zasady współpracy, które powinniśmy zaakceptować.",
                            "target_lang_translations": {
                                "ru": "Это <w>здравые</w> принципы сотрудничества, которые мы должны принять."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zdrowy rozsądek",
                        "zdrowe podejście",
                        "zdrowa konkurencja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на разуме, логичный и рациональный (обычно о мышлении или принципах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здравый",
                                "target_lang_sense_clarification": "рассудительный (обычно в выражении \"здравый смысл\")"
                            },
                            {
                                "target_lang_word": "разумный",
                                "target_lang_sense_clarification": "логичный, обоснованный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "8f9f4d48-96e4-3c2c-a5b5-bcb96785e2c8",
                    "sense_definition": "Kolej o rozstawie szyn mniejszym niż standardowy, obecnie często wykorzystywana jako atrakcja turystyczna lub w przemyśle.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W niedzielę całą rodziną wybraliśmy się na przejażdżkę zabytkową <w>wąskotorówką</w>.",
                            "target_lang_translations": {
                                "ru": "В воскресенье мы всей семьёй отправились покататься на старинной <w>узкоколейке</w>."
                            }
                        },
                        {
                            "text": "Lokalna <w>wąskotorówka</w> jest jedną z największych atrakcji turystycznych w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Местная <w>узкоколейка</w> является одной из главных туристических достопримечательностей в этом регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolejka wąskotorowa",
                        "kolej wąskotorowa"
                    ],
                    "antonyms": [
                        "kolej normalnotorowa",
                        "kolej szerokotorowa"
                    ],
                    "common_phrases": [
                        "zabytkowa wąskotorówka",
                        "jechać wąskotorówką",
                        "stacja wąskotorówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Железная дорога с шириной колеи меньше стандартной, в настоящее время часто используемая как туристический аттракцион или в промышленности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узкоколейка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "узкоколейная железная дорога",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b523c43-9b87-35b7-8506-00c54bc5b083",
                    "sense_definition": "W gwarze uczniowskiej: ocena dostateczna z dwoma minusami (3=).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grades",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z ostatniego sprawdzianu z matematyki dostałem niestety <w>wąskotorówkę</w>.",
                            "target_lang_translations": {
                                "ru": "За последнюю контрольную по математике я, к сожалению, получил <w>тройку с двумя минусами</w>."
                            }
                        },
                        {
                            "text": "Nauczyciel postawił mi <w>wąskotorówkę</w>, więc ledwo zaliczyłem ten przedmiot.",
                            "target_lang_translations": {
                                "ru": "Учитель поставил мне <w>тройку с двумя минусами</w>, поэтому я едва сдал этот предмет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dostać wąskotorówkę",
                        "mieć wąskotorówkę"
                    ],
                    "traits": [

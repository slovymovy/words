    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "404b6544-08d5-30d0-a8a5-4190ff95df4d",
                    "sense_definition": "Форма родительного падежа единственного числа местоимений <w>тот</w> (мужской род) и <w>то</w> (средний род).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar_genitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У <w>того</w> дома растёт высокое дерево.",
                            "target_lang_translations": {
                                "en": "A tall tree grows by <w>that</w> house."
                            }
                        },
                        {
                            "text": "После <w>того</w> случая мы стали друзьями.",
                            "target_lang_translations": {
                                "en": "After <w>that</w> incident we became friends."
                            }
                        }
                    ],
                    "common_phrases": [
                        "кроме <w>того</w>",
                        "для <w>того</w> чтобы",
                        "после <w>того</w>",
                        "более <w>того</w>",
                        "до <w>того</w> как"
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive singular form of the pronouns \"тот\" (that, masculine) and \"то\" (that, neuter)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative pronoun"
                            },
                            {
                                "target_lang_word": "of that",
                                "target_lang_sense_clarification": "genitive case indicator"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc8603f2-8996-3b33-a301-2e7d8e394c9e",
                    "sense_definition": "Форма винительного падежа единственного числа местоимения <w>тот</w> (для одушевлённых существительных мужского рода).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar_accusative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хорошо знаю <w>того</w> человека.",
                            "target_lang_translations": {
                                "en": "I know <w>that</w> person well."
                            }
                        },
                        {
                            "text": "Мы выбрали <w>того</w> кандидата, который нам понравился.",
                            "target_lang_translations": {
                                "en": "We chose <w>that</w> candidate who we liked."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вижу <w>того</w>",
                        "встретил <w>того</w>"
                    ],
                    "target_lang_definitions": {
                        "en": "Accusative singular form of the pronoun \"тот\" (for animate masculine nouns)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "demonstrative pronoun referring to a person"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ccc01510-ade1-323f-9743-31ae6a6d44b1",
                    "sense_definition": "О человеке, который ведёт себя странно, глупо, неадекватно или кажется сумасшедшим.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idiom_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Соседи говорят, что он немного <w>того</w>.",
                            "target_lang_translations": {
                                "en": "The neighbors say that he is a bit <w>nuts</w>."
                            }
                        },
                        {
                            "text": "Ты что, совсем <w>того</w>, раз пошёл в лес ночью?",
                            "target_lang_translations": {
                                "en": "Are you completely <w>crazy</w> to go into the forest at night?"
                            }
                        }
                    ],
                    "synonyms": [
                        "не в себе",
                        "сойти с ума"
                    ],
                    "common_phrases": [
                        "немного <w>того</w>",
                        "совсем <w>того</w>",
                        "он <w>того</w>"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "used as a predicate"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to a person who behaves strangely, stupidly, inappropriately, or seems crazy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nuts",
                                "target_lang_sense_clarification": "colloquial for crazy"
                            },
                            {
                                "target_lang_word": "crazy",
                                "target_lang_sense_clarification": "insane or strange"
                            },
                            {
                                "target_lang_word": "not all there",
                                "target_lang_sense_clarification": "idiom for mentally lacking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50d5a272-30ef-38b4-8839-d7252e6cf700",
                    "sense_definition": "Эвфемизм, обозначающий, что кто-то убит, устранён или что предмет сломан, испорчен.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiom_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бандиты его быстро <w>того</w>, и никто ничего не узнал.",
                            "target_lang_translations": {
                                "en": "The bandits quickly <w>did him in</w>, and no one found out anything."
                            }
                        },
                        {
                            "text": "Мой телефон упал и теперь экран совсем <w>того</w>.",
                            "target_lang_translations": {
                                "en": "My phone fell and now the screen is completely <w>bust</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "убить"
                    ],
                    "common_phrases": [
                        "его уже <w>того</w>"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "slang",
                            "comment": "euphemism"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A euphemism indicating that someone has been killed/removed or that an object is broken/ruined."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "done for",
                                "target_lang_sense_clarification": "ruined or killed"
                            },
                            {

{
    "word_family": [
        "warm",
        "warmte",
        "verwarmen",
        "opwarming"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "529aeff2-9817-37a8-864a-4b4db0bbd835",
                    "sense_definition": "Iets warmer maken; warmte toevoegen aan iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kan jij de kliekjes van gisteren even <w>opwarmen</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>разогреть</w> вчерашние остатки?"
                            }
                        },
                        {
                            "text": "Hij <w>warmde</w> zijn koude koffie <w>op</w> in de magnetron.",
                            "target_lang_translations": {
                                "ru": "Он <w>подогрел</w> свой остывший кофе в микроволновке."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwarmen"
                    ],
                    "antonyms": [
                        "afkoelen"
                    ],
                    "common_phrases": [
                        "eten opwarmen",
                        "de oven opwarmen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то более теплым; нагревать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разогревать",
                                "target_lang_sense_clarification": "еду, готовые блюда"
                            },
                            {
                                "target_lang_word": "подогревать",
                                "target_lang_sense_clarification": "напитки, суп"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "904ec2c2-4edd-3bc5-b3be-2052dd51b939",
                    "sense_definition": "Warmer worden; in temperatuur stijgen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde <w>warmt</w> langzaam <w>op</w> door klimaatverandering.",
                            "target_lang_translations": {
                                "ru": "Земля медленно <w>нагревается</w> из-за изменения климата."
                            }
                        },
                        {
                            "text": "Het water in het kleine zwembad <w>warmt</w> snel <w>op</w> als de zon schijnt.",
                            "target_lang_translations": {
                                "ru": "Вода в маленьком бассейне быстро <w>нагревается</w>, когда светит солнце."
                            }
                        }
                    ],
                    "antonyms": [
                        "afkoelen"
                    ],
                    "common_phrases": [
                        "snel opwarmen",
                        "de aarde warmt op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться теплее; повышаться в температуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагреваться",
                                "target_lang_sense_clarification": "о предметах, воде, воздухе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
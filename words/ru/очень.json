    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d3490207-de0d-31a4-9816-edb94b182b04",
                    "sense_definition": "В сильной, высокой, значительной степени; используется для усиления значения прилагательных, наречий и глаголов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сегодня на улице <очень> холодно, поэтому лучше одеться потеплее.",
                            "target_lang_translations": {
                                "en": "It is <very> cold outside today, so it is better to dress warmly."
                            }
                        },
                        {
                            "text": "Мне <очень> жаль, что я не смог прийти на твой день рождения.",
                            "target_lang_translations": {
                                "en": "I am <very> sorry that I could not come to your birthday."
                            }
                        },
                        {
                            "text": "Этот студент <очень> хорошо говорит по-русски и понимает сложные тексты.",
                            "target_lang_translations": {
                                "en": "This student speaks Russian <very> well and understands complex texts."
                            }
                        }
                    ],
                    "synonyms": [
                        "сильно",
                        "весьма",
                        "крайне",
                        "чрезвычайно",
                        "изрядно",
                        "необычайно",
                        "в высшей степени"
                    ],
                    "antonyms": [
                        "слегка"
                    ],
                    "common_phrases": [
                        "очень приятно",
                        "очень жаль",
                        "очень много",
                        "очень рад",
                        "очень хорошо"
                    ],
                    "target_lang_definitions": {
                        "en": "To a high degree or extent; used to emphasize adjectives, adverbs, and verbs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "very",
                                "target_lang_sense_clarification": "general intensifier"
                            },
                            {
                                "target_lang_word": "really",
                                "target_lang_sense_clarification": "informal intensifier"
                            },
                            {
                                "target_lang_word": "extremely",
                                "target_lang_sense_clarification": "indicating a high degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b13a796a-e8a5-3b77-be1e-b6af79883c8e",
                    "sense_definition": "Больше, чем нужно или следует; чересчур, избыточно.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "degree_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот костюм мне <очень> велик, нужно найти размер поменьше.",
                            "target_lang_translations": {
                                "en": "This suit is <too> big for me, I need to find a smaller size."
                            }
                        },
                        {
                            "text": "Ты <очень> строг к нему, ведь он всего лишь ребёнок.",
                            "target_lang_translations": {
                                "en": "You are <too> strict with him, after all, he is just a child."
                            }
                        }
                    ],
                    "synonyms": [
                        "слишком",
                        "чересчур",
                        "чрезмерно",
                        "излишне",
                        "сверх меры",
                        "избыточно"
                    ],
                    "target_lang_definitions": {
                        "en": "More than is necessary or desirable; excessively."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "too",
                                "target_lang_sense_clarification": "indicating excess"
                            },
                            {
                                "target_lang_word": "excessively",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "way",
                                "target_lang_sense_clarification": "colloquial intensifier for excess"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "28d69f13-8594-3e28-b3ad-e3ba5f0a42b3",
                    "sense_definition": "Посредственный, неважный, плохого качества (обычно используется с отрицанием «не» в значении сказуемого).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality_evaluation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Настроение у меня сегодня не <очень>, поэтому я останусь дома.",
                            "target_lang_translations": {
                                "en": "My mood today is not <so good>, so I will stay home."
                            }
                        },
                        {
                            "text": "Фильм оказался не <очень>, я ожидал от него гораздо большего.",
                            "target_lang_translations": {
                                "en": "The movie turned out not <great>, I expected much more from it."
                            }
                        }
                    ],
                    "common_phrases": [
                        "не очень"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в разговорной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Mediocre or of poor quality (usually used with negation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so good",
                                "target_lang_sense_clarification": "used in negative contexts (not so good)"
                            },
                            {
                                "target_lang_word": "great",
                                "target_lang_sense_clarification": "used in negative contexts (not great)"
                            },
                            {
                                "target_lang_word": "up to much",
                                "target_lang_sense_clarification": "idiomatic (not up to much)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

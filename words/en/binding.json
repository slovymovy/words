    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2af13c77-fce2-3439-aa49-e668ae53d618",
                    "sense_definition": "Present participle and gerund of <w>bind</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>binding</w> the old newspapers into neat stacks.",

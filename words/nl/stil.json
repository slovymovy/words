                {
                    "sense_id": "77f0a81a-c344-3fbd-a11a-a1055977f6a8",
                    "sense_definition": "Als deel van een scheidbaar werkwoord om aan te geven dat iets stopt of niet beweegt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wind viel 's avonds plotseling <w>stil</w> na de storm.",
                            "target_lang_translations": {
                                "en": "The wind suddenly fell <w>still</w> in the evening after the storm.",
                                "ru": "Вечером после шторма ветер внезапно <w>стих</w>."
                            }
                        },
                        {
                            "text": "Waarom blijf je midden op de weg <w>stil</w> staan?",
                            "target_lang_translations": {
                                "en": "Why do you remain standing <w>still</w> in the middle of the road?",
                                "ru": "Почему ты стоишь <w>неподвижно</w> посреди дороги?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "stilvallen",
                        "stilstaan",
                        "stilzitten"
                    ],
                    "target_lang_definitions": {
                        "en": "As part of a separable verb to indicate that something stops or does not move.",
                        "ru": "Как часть отделяемой приставки глагола, указывающая на прекращение действия или движения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "still",
                                "target_lang_sense_clarification": "used in phrasal verbs like 'stand still' or 'fall still'"

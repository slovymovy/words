                {
                    "sense_id": "822e8d87-8d93-3a23-9b18-97fe20acc196",
                    "sense_definition": "The lower layer of the Earth's outer crust, composed mainly of rocks rich in silica and magnesium.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geology_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geologists analyze the composition of the <w>sima</w> found beneath the ocean floor.",
                            "target_lang_translations": {
                                "ru": "Геологи анализируют состав <w>симы</w>, находящейся под океанским дном."
                            }
                        },
                        {
                            "text": "The heavier <w>sima</w> layer supports the lighter continental crust known as sial.",
                            "target_lang_translations": {
                                "ru": "Более тяжелый слой <w>симы</w> поддерживает более легкую континентальную кору, известную как сиаль."
                            }
                        }
                    ],
                    "antonyms": [
                        "sial"
                    ],
                    "common_phrases": [
                        "sima layer",
                        "sial and sima"
                    ],
                    "target_lang_definitions": {

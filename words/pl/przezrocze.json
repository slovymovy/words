{
    "word_family": [
        "przezroczysty",
        "przezroczystość",
        "przezroczyście"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c6b8946-dfc9-3f0e-b7d0-9a6d9f71357f",
                    "sense_definition": "Obraz fotograficzny utrwalony na materiale przezroczystym, przeznaczony do wyświetlania na ekranie za pomocą rzutnika lub oglądania pod światło.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji geografii nauczycielka pokazywała nam <przezrocza> ze zdjęciami z Himalajów.",
                            "target_lang_translations": {
                                "ru": "На уроке географии учительница показывала нам <слайды> с фотографиями Гималаев."
                            }
                        },
                        {
                            "text": "Muszę uporządkować stare <przezrocza>, aby przygotować pokaz dla rodziny.",
                            "target_lang_translations": {
                                "ru": "Мне нужно привести в порядок старые <слайды>, чтобы подготовить показ для семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "slajd",
                        "diapozytyw"
                    ],
                    "common_phrases": [
                        "rzutnik na przezrocza",
                        "kolorowe przezrocze",
                        "oglądać przezrocza",
                        "zbiór przezroczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фотографическое изображение на прозрачном материале, предназначенное для проецирования на экран или просмотра на свет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слайд",
                                "target_lang_sense_clarification": "фотография для проектора"
                            },
                            {
                                "target_lang_word": "диапозитив",
                                "target_lang_sense_clarification": "фотографический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ece90f6-23c7-3468-aa3e-bc767dd18247",
                    "sense_definition": "Właściwość materiału lub substancji polegająca na przepuszczaniu światła, pozwalająca widzieć to, co znajduje się za nimi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niezwykłe <przezrocze> górskiego potoku pozwalało dostrzec każdy kamień na dnie.",
                            "target_lang_translations": {
                                "ru": "Необычайная <прозрачность> горного потока позволяла разглядеть каждый камень на дне."
                            }
                        },
                        {
                            "text": "Artysta próbował uchwycić na obrazie idealne <przezrocze> powietrza po burzy.",
                            "target_lang_translations": {
                                "ru": "Художник пытался запечатлеть на картине идеальную <прозрачность> воздуха после грозы."
                            }
                        }
                    ],
                    "synonyms": [
                        "przejrzystość"
                    ],
                    "common_phrases": [
                        "przezrocze wody",
                        "przezrocze powietrza",
                        "kryształowe przezrocze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство материала или вещества пропускать свет; прозрачность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "свойство вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5ac5731-4be3-34d2-b558-ab54adf06d24",
                    "sense_definition": "Nieoszklony otwór w ścianie budowli, często o ozdobnym kształcie, przepuszczający do wnętrza światło dzienne.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zabytkowe kościelne <przezrocza> zostały wypełnione cienkimi płytkami z alabastru.",
                            "target_lang_translations": {
                                "ru": "Старинные церковные <световые проёмы> были заполнены тонкими пластинками алебастра."
                            }
                        },
                        {
                            "text": "Przez wąskie <przezrocze> w murze obronnym wpadał do środka promień słońca.",
                            "target_lang_translations": {
                                "ru": "Сквозь узкий <просвет> в крепостной стене внутрь падал луч солнца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gotyckie przezrocze",
                        "kamienne przezrocze",
                        "przezrocze okienne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Незастекленный проем в стене здания, часто декоративной формы, пропускающий внутрь дневной свет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "световой проём",
                                "target_lang_sense_clarification": "архитектура"
                            },
                            {
                                "target_lang_word": "просвет",
                                "target_lang_sense_clarification": "отверстие в стене"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f4ff792-a08a-3f79-9261-913b62325b79",
                    "sense_definition": "Wolna, przejrzysta przestrzeń, przez którą można swobodnie patrzeć.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędrowcy wyszli z gęstego lasu na leśne <przezrocze> zalane słońcem.",
                            "target_lang_translations": {
                                "ru": "Путники вышли из густого леса на залитую солнцем лесную <прогалину>."
                            }
                        },
                        {
                            "text": "Ptak wzbił się wysoko w błękitne <przezrocze> nieba nad naszymi głowami.",
                            "target_lang_translations": {
                                "ru": "Птица взмыла высоко в голубую <прозрачность> неба над нашими головами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leśne przezrocze",
                        "błękitne przezrocze",
                        "świetliste przezrocze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободное, просматриваемое насквозь пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просвет",
                                "target_lang_sense_clarification": "свободное пространство"
                            },
                            {
                                "target_lang_word": "прогалина",
                                "target_lang_sense_clarification": "в лесу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
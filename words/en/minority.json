                },
                {
                    "sense_id": "8ee13dc2-900e-309a-a11f-026234847d7b",
                    "sense_definition": "Empowered by or representing less than half of the votes cast or legislative seats.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "smaller_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is often difficult for a <w>minority</w> government to pass new laws.",
                            "target_lang_translations": {
                                "ru": "Правительству <w>меньшинства</w> часто бывает трудно принимать новые законы."
                            }
                        },
                        {
                            "text": "The <w>minority</w> leader criticized the new policy.",
                            "target_lang_translations": {
                                "ru": "Лидер <w>меньшинства</w> раскритиковал новую политику."
                            }
                        }
                    ],
                    "common_phrases": [
                        "minority government",
                        "minority leader"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сформированный или представленный менее чем половиной голосов или мест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меньшинства",
                                "target_lang_sense_clarification": "политический термин"

{
    "word_family": [
        "indentation",
        "indention",
        "indenture",
        "unindent",
        "outdent"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "86e1abe7-52d4-317e-b74c-c82571b31592",
                    "sense_definition": "To start a line of text further from the margin than the main part of the text.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "typography_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>indent</w> the first line of every paragraph to make the text easier to read.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>делать отступ</w> в первой строке каждого абзаца, чтобы текст было легче читать."
                            }
                        },
                        {
                            "text": "The software will automatically <w>indent</w> the code when you press the tab key.",
                            "target_lang_translations": {
                                "ru": "Программа автоматически <w>сделает отступ</w> в коде, когда вы нажмете клавишу Tab."
                            }
                        }
                    ],
                    "antonyms": [
                        "unindent",
                        "outdent",
                        "dedent"
                    ],
                    "common_phrases": [
                        "indent the paragraph",
                        "indent the line",
                        "hanging indent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать строку текста с отступом от поля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать отступ",
                                "target_lang_sense_clarification": "текст"
                            },
                            {
                                "target_lang_word": "отступать",
                                "target_lang_sense_clarification": "от края"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "772ff99c-5283-3d18-950e-b02bdbd2ba3c",
                    "sense_definition": "To make a depression or mark into a surface by pressing or striking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy hail began to <w>indent</w> the metal roof of the car.",
                            "target_lang_translations": {
                                "ru": "Сильный град начал <w>оставлять вмятины</w> на металлической крыше автомобиля."
                            }
                        },
                        {
                            "text": "He managed to <w>indent</w> the soft wood with a hammer.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <w>сделать вмятину</w> в мягком дереве молотком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "indent the surface"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять углубление или след на поверхности нажатием или ударом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдавливать",
                                "target_lang_sense_clarification": "поверхность"
                            },
                            {
                                "target_lang_word": "делать вмятину",
                                "target_lang_sense_clarification": "повреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7ba9743-7e7a-315c-a4c8-699311910d8c",
                    "sense_definition": "To cut notches, points, or irregularities into an edge or border.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many small bays and inlets <w>indent</w> the coastline of the island.",
                            "target_lang_translations": {
                                "ru": "Множество небольших бухт и заливов <w>изрезают</w> береговую линию острова."
                            }
                        },
                        {
                            "text": "The craftsman used a tool to <w>indent</w> the edge of the coin.",
                            "target_lang_translations": {
                                "ru": "Ремесленник использовал инструмент, чтобы <w>сделать насечки</w> на краю монеты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heavily indented"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать вырезы, зубцы или неровности по краю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изрезать",
                                "target_lang_sense_clarification": "о береговой линии"
                            },
                            {
                                "target_lang_word": "зазубривать",
                                "target_lang_sense_clarification": "край"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af8aa8f9-1d9c-367f-b81e-13929870cb26",
                    "sense_definition": "To officially request or order supplies, especially in a military or administrative context.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ordering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The officer had to <w>indent</w> for fresh ammunition and rations.",
                            "target_lang_translations": {
                                "ru": "Офицеру пришлось <w>сделать заявку</w> на свежие боеприпасы и продовольствие."
                            }
                        },
                        {
                            "text": "We must <w>indent</w> upon the central store for new equipment.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>выписать</w> новое оборудование с центрального склада."
                            }
                        }
                    ],
                    "common_phrases": [
                        "indent for supplies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально запрашивать или заказывать припасы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать заявку",
                                "target_lang_sense_clarification": "на товары"
                            },
                            {
                                "target_lang_word": "выписывать",
                                "target_lang_sense_clarification": "со склада"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f3402f5-a61c-3f92-a61b-218b7e4d4b60",
                    "sense_definition": "To bind someone by a formal contract to work for a specific period.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "contract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the past, parents would often <w>indent</w> their children to a master craftsman.",
                            "target_lang_translations": {
                                "ru": "В прошлом родители часто <w>отдавали</w> своих детей <w>по контракту</w> мастеру-ремесленнику."
                            }
                        },
                        {
                            "text": "He agreed to <w>indent</w> himself as a servant for four years in exchange for passage.",
                            "target_lang_translations": {
                                "ru": "Он согласился <w>наняться по контракту</w> слугой на четыре года в обмен на оплату проезда."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Связывать кого-либо официальным контрактом для работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связывать договором",
                                "target_lang_sense_clarification": "юридически"
                            },
                            {
                                "target_lang_word": "нанимать по контракту",
                                "target_lang_sense_clarification": "на работу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a0d822e-c35f-357a-bc2f-3f85e7a653f9",
                    "sense_definition": "A cut, notch, or small hollow space in a surface or edge.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a small <w>indent</w> on the frame where the lock fits.",
                            "target_lang_translations": {
                                "ru": "На раме была небольшая <w>выемка</w>, куда входит замок."
                            }
                        },
                        {
                            "text": "Make a slight <w>indent</w> in the clay with your thumb before baking it.",
                            "target_lang_translations": {
                                "ru": "Сделайте небольшое <w>углубление</w> в глине большим пальцем перед обжигом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "small indent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вырез, насечка или небольшое углубление на поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "углубление",
                                "target_lang_sense_clarification": "ямочка"
                            },
                            {
                                "target_lang_word": "выемка",
                                "target_lang_sense_clarification": "паз"
                            },
                            {
                                "target_lang_word": "вмятина",
                                "target_lang_sense_clarification": "дефект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e053bd7-607c-3654-be52-205bd2de9e47",
                    "sense_definition": "An official requisition or order for supplies.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain signed an <w>indent</w> for the necessary medical supplies.",
                            "target_lang_translations": {
                                "ru": "Капитан подписал <w>заявку</w> на необходимые медикаменты."
                            }
                        },
                        {
                            "text": "Please submit your <w>indent</w> to the procurement department by Friday.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подайте вашу <w>заявку</w> в отдел закупок до пятницы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "submit an indent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное требование или заказ на поставку припасов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заявка",
                                "target_lang_sense_clarification": "документ"
                            },
                            {
                                "target_lang_word": "заказ-наряд",
                                "target_lang_sense_clarification": "на товары"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
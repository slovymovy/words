                            },
                            {
                                "target_lang_word": "car",
                                "target_lang_sense_clarification": "railway vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b74cc1f-9b8e-3995-bf9f-a9468ecbd80b",
                    "sense_definition": "Een spoorvoertuig dat specifiek bedoeld is voor het vervoer van goederen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

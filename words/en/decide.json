{
    "word_family": [
        "decision",
        "decisive",
        "decisively",
        "indecisive",
        "decider"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "64aa2669-b0f3-3a98-a772-8bcb90b5b01a",
                    "sense_definition": "To make a choice or judgment about something, especially after thinking about it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>decided</w> that it is healthier to walk to work.",
                            "target_lang_translations": {
                                "ru": "Я <w>решил</w>, что полезнее ходить на работу пешком."
                            }
                        },
                        {
                            "text": "You must <w>decide</w> between the red shirt and the blue one.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>выбрать</w> между красной и синей рубашкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "choose",
                        "pick",
                        "make up one's mind",
                        "elect"
                    ],
                    "common_phrases": [
                        "decide to do",
                        "decide against",
                        "decide between"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять решение или сделать выбор относительно чего-либо, особенно после обдумывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "принять решение, сделать вывод"
                            },
                            {
                                "target_lang_word": "принять решение",
                                "target_lang_sense_clarification": "фразовый глагол"
                            },
                            {
                                "target_lang_word": "выбрать",
                                "target_lang_sense_clarification": "сделать выбор из вариантов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c1f2810-8c69-3830-a772-efff0547f37f",
                    "sense_definition": "To settle a dispute, contest, or problem, or to determine the final result of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resolve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her last-minute goal <w>decided</w> the game.",
                            "target_lang_translations": {
                                "ru": "Её гол на последней минуте <w>решил</w> исход игры."
                            }
                        },
                        {
                            "text": "The upcoming election will be <w>decided</w> on foreign policies.",
                            "target_lang_translations": {
                                "ru": "Исход предстоящих выборов <w>решится</w> вопросами внешней политики."
                            }
                        }
                    ],
                    "synonyms": [
                        "determine"
                    ],
                    "common_phrases": [
                        "decide the outcome",
                        "decide the case",
                        "decide the fate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешить спор или проблему; определить окончательный результат или исход чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "определить исход события"
                            },
                            {
                                "target_lang_word": "определить",
                                "target_lang_sense_clarification": "обусловить результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ada22158-2dbe-3977-87d7-e331ad20f599",
                    "sense_definition": "To cause someone to make a particular choice or come to a conclusion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your strong warning <w>decided</w> me against that course of action.",
                            "target_lang_translations": {
                                "ru": "Твоё строгое предупреждение <w>убедило</w> меня отказаться от этого плана действий."
                            }
                        },
                        {
                            "text": "The sudden rain <w>decided</w> us to stay at home for the evening.",
                            "target_lang_translations": {
                                "ru": "Внезапный дождь <w>склонил</w> нас к решению остаться дома на вечер."
                            }
                        }
                    ],
                    "synonyms": [
                        "determine"
                    ],
                    "common_phrases": [
                        "decide someone against",
                        "decide someone to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побудить кого-либо сделать определенный выбор или прийти к заключению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склонить",
                                "target_lang_sense_clarification": "к решению или выбору"
                            },
                            {
                                "target_lang_word": "убедить",
                                "target_lang_sense_clarification": "заставить принять точку зрения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d794f9c9-e194-367f-85fe-db6483a48451",
                    "sense_definition": "To return a definitive correct answer for every possible input in a computational problem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "No Turing Machine can <w>decide</w> the halting problem.",
                            "target_lang_translations": {
                                "ru": "Ни одна машина Тьюринга не может <w>разрешить</w> проблему остановки."
                            }
                        },
                        {
                            "text": "We need an algorithm to <w>decide</w> if the formula is valid.",
                            "target_lang_translations": {
                                "ru": "Нам нужен алгоритм, чтобы <w>определить</w>, верна ли формула."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдать однозначный правильный ответ (да/нет) для любых входных данных в вычислительной задаче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрешить",
                                "target_lang_sense_clarification": "алгоритмически"
                            },
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "найти решение задачи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46ea6480-f039-37ef-96cd-d6a427f3ab25",
                    "sense_definition": "To cut off or separate one thing from another.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The deep sea <w>decides</w> us from the rest of the continent.",
                            "target_lang_translations": {
                                "ru": "Глубокое море <w>отделяет</w> нас от остального континента."
                            }
                        },
                        {
                            "text": "A high stone wall <w>decides</w> the garden from the street.",
                            "target_lang_translations": {
                                "ru": "Высокая каменная стена <w>отделяет</w> сад от улицы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрезать или отделить одно от другого (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделять",
                                "target_lang_sense_clarification": "физически разделять"
                            },
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "проводить границу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
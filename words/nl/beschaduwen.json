    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0494c38-eca6-3dad-a0e1-23a3c89f6d6f",
                    "sense_definition": "Ervoor zorgen dat er schaduw op iets valt, vaak om het tegen de zon te beschermen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shadow_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grote eikenbomen <w>beschaduwen</w> het terras tijdens de hete middaguren.",
                            "target_lang_translations": {
                                "en": "The large oak trees <w>shade</w> the terrace during the hot afternoon hours.",
                                "ru": "Большие дубы <w>затеняют</w> террасу в жаркие полуденные часы."
                            }
                        },
                        {
                            "text": "Wij moeten voorkomen dat nieuwe gebouwen de tuin van de buren volledig <w>beschaduwen</w>.",
                            "target_lang_translations": {
                                "en": "We must prevent new buildings from completely <w>shading</w> the neighbors' garden.",
                                "ru": "Мы должны не допустить, чтобы новые здания полностью <w>затеняли</w> сад соседей."
                            }
                        }
                    ],
                    "synonyms": [
                        "overschaduwen"
                    ],
                    "antonyms": [
                        "belichten",
                        "verlichten"
                    ],
                    "common_phrases": [
                        "de bodem beschaduwen",
                        "het gezicht beschaduwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a shadow to fall on something, often to protect it from the sun.",
                        "ru": "Создавать тень на чем-либо, часто для защиты от солнца."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shade",
                                "target_lang_sense_clarification": "to screen from direct light"
                            },
                            {
                                "target_lang_word": "cast a shadow",
                                "target_lang_sense_clarification": "to project a shadow upon"
                            },
                            {
                                "target_lang_word": "shadow",
                                "target_lang_sense_clarification": "to cast a shadow over"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "затенять",
                                "target_lang_sense_clarification": "создавать тень"
                            },
                            {
                                "target_lang_word": "бросать тень",
                                "target_lang_sense_clarification": "накрывать тенью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea6a41c5-8af7-3bbd-b454-5c8dc6e7e25a",
                    "sense_definition": "Oogschaduw of make-up aanbrengen rondom de ogen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "makeup",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor het feest had zij haar ogen donker <w>beschaduwd</w>.",
                            "target_lang_translations": {
                                "en": "For the party, she had <w>shaded</w> her eyes darkly.",
                                "ru": "Перед вечеринкой она темно <w>подвела</w> глаза."
                            }
                        },
                        {
                            "text": "De visagist <w>beschaduwde</w> de oogleden met een zachte, blauwe kleur.",
                            "target_lang_translations": {
                                "en": "The makeup artist <w>shaded</w> the eyelids with a soft, blue color.",
                                "ru": "Визажист <w>нанес тени</w> на веки мягкого голубого цвета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de ogen beschaduwen"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply eyeshadow or makeup around the eyes.",
                        "ru": "Наносить тени для век или макияж вокруг глаз."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shade",
                                "target_lang_sense_clarification": "to apply color or makeup"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наносить тени",
                                "target_lang_sense_clarification": "макияж"
                            },
                            {
                                "target_lang_word": "подводить",
                                "target_lang_sense_clarification": "глаза"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
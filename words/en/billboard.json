                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "Her debut single climbed to the top of <w>Billboard</w> within weeks.",
                            "target_lang_translations": {
                                "ru": "Ее дебютный сингл поднялся на вершину чарта <w>Billboard</w> за считанные недели."
                            }
                        },
                        {
                            "text": "The band was thrilled to finally chart on <w>Billboard</w>.",
                            "target_lang_translations": {
                                "ru": "Группа была в восторге от того, что наконец попала в чарт <w>Billboard</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on Billboard",
                        "Billboard charts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальные чарты, публикуемые журналом Billboard."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Billboard",
                                "target_lang_sense_clarification": "название журнала/чарта"
                            },
                            {
                                "target_lang_word": "Билборд",
                                "target_lang_sense_clarification": "транслитерация"

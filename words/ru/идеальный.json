            "senses": [
                {
                    "sense_id": "116a1f21-771d-3e88-8cf1-6f9d1eccd386",
                    "sense_definition": "Соответствующий идеалу; совершенный, не имеющий недостатков, лучший из возможных.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы нашли <идеальное> место для пикника на берегу реки.",
                            "target_lang_translations": {
                                "en": "We found a <perfect> place for a picnic on the river bank."
                            }
                        },
                        {
                            "text": "В комнате был наведён <идеальный> порядок перед приходом гостей.",
                            "target_lang_translations": {
                                "en": "The room was put in <perfect> order before the guests arrived."
                            }
                        }
                    ],
                    "synonyms": [
                        "безупречный",
                        "совершенный",
                        "образцовый"
                    ],
                    "antonyms": [
                        "действительный",
                        "реальный"
                    ],
                    "common_phrases": [
                        "идеальный порядок",
                        "идеальный вариант",
                        "идеальная пара",
                        "идеальный слух",
                        "идеальная чистота"
                    ],
                    "target_lang_definitions": {
                        "en": "Corresponding to an ideal; perfect, having no flaws, the best possible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perfect",
                                "target_lang_sense_clarification": "having no flaws or defects; excellent"
                            },
                            {
                                "target_lang_word": "ideal",
                                "target_lang_sense_clarification": "satisfying one's conception of what is perfect; most suitable"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5116771a-7703-3eca-919d-e6141fc531a2",
                    "sense_definition": "Существующий только в мыслях или воображении, а не в действительности; абстрактный.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В физике часто рассматривают <идеальный> газ для упрощения теоретических расчётов.",
                            "target_lang_translations": {
                                "en": "In physics, an <ideal> gas is often considered to simplify theoretical calculations."
                            }
                        },
                        {
                            "text": "Философ описывал <идеальный> мир, где царит полная гармония.",
                            "target_lang_translations": {
                                "en": "The philosopher described an <ideal> world where complete harmony reigns."
                            }
                        }
                    ],
                    "synonyms": [
                        "воображаемый",
                        "нереальный"
                    ],
                    "antonyms": [
                        "реальный",
                        "существующий",
                        "действительный"
                    ],
                    "common_phrases": [
                        "идеальный газ",
                        "идеальный мир"
                    ],
                    "target_lang_definitions": {
                        "en": "Existing only in thoughts or imagination, not in reality; abstract."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ideal",
                                "target_lang_sense_clarification": "existing only as an idea; conceptual or theoretical"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e2b53098-fef1-3d95-9eb8-2f89febbbe2c",
                    "sense_definition": "Een tafel waaraan men schrijft of werkt, vaak voorzien van laden en ruimte voor een computer.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met twee computers staat mijn <w>bureau</w> behoorlijk vol.",
                            "target_lang_translations": {
                                "ru": "С двумя компьютерами мой <w>рабочий стол</w> довольно загроможден."
                            }
                        },
                        {
                            "text": "Hij zat de hele ochtend achter zijn <w>bureau</w> te studeren.",
                            "target_lang_translations": {
                                "ru": "Он все утро сидел за своим <w>письменным столом</w> и занимался."
                            }
                        }
                    ],
                    "synonyms": [
                        "schrijftafel",
                        "schrijfbureau"
                    ],
                    "common_phrases": [
                        "achter het bureau",
                        "aan het bureau",
                        "een opgeruimd bureau"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стол, за которым пишут или работают, часто с ящиками и местом для компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "письменный стол",
                                "target_lang_sense_clarification": "мебель для письма"
                            },
                            {
                                "target_lang_word": "рабочий стол",
                                "target_lang_sense_clarification": "мебель для работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbb7a128-261f-3e53-a71d-1422699a736e",
                    "sense_definition": "Een gebouw of kantoor van de politie waar men terechtkan voor hulp of aangiften.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt aangifte van diefstal doen op het <w>bureau</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете подать заявление о краже в <w>отделении полиции</w>."
                            }
                        },
                        {
                            "text": "De agenten namen de verdachte voor verhoor mee naar het <w>bureau</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейские забрали подозреваемого на допрос в <w>участок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kantoor"
                    ],
                    "common_phrases": [
                        "op het bureau",
                        "naar het bureau"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или офис полиции, куда обращаются за помощью или для подачи заявлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделение полиции",
                                "target_lang_sense_clarification": "официальное учреждение"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "разговорное название полицейского участка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed2c0b19-d6b3-319a-98f6-fae51587bf8a",
                    "sense_definition": "Een bedrijf of organisatie die gespecialiseerde diensten levert of zaken regelt voor anderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>bureau</w> verzorgt de marketing en communicatie voor grote bedrijven.",
                            "target_lang_translations": {
                                "ru": "<w>Агентство</w> занимается маркетингом и коммуникациями для крупных компаний."
                            }
                        },
                        {
                            "text": "Zij werkt bij een <w>bureau</w> dat gespecialiseerd is in grafisch ontwerp.",
                            "target_lang_translations": {
                                "ru": "Она работает в <w>бюро</w>, специализирующемся на графическом дизайне."
                            }
                        }
                    ],
                    "synonyms": [
                        "kantoor",
                        "bureel"
                    ],
                    "common_phrases": [
                        "een commercieel bureau",
                        "het dagelijks bureau"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания или организация, предоставляющая специализированные услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агентство",
                                "target_lang_sense_clarification": "организация по оказанию услуг"
                            },
                            {
                                "target_lang_word": "бюро",
                                "target_lang_sense_clarification": "учреждение или отдел"
                            },
                            {
                                "target_lang_word": "контора",
                                "target_lang_sense_clarification": "разговорное, офис"
                            }
                        ]
                    }
                }
            ]
        }
    ]

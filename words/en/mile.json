                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миля",
                                "target_lang_sense_clarification": "бонус авиакомпании"
                            },
                            {
                                "target_lang_word": "балл",
                                "target_lang_sense_clarification": "единица счета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b892971-4258-37c5-8a6c-222830002ccf",
                    "sense_definition": "One mile per hour, used as a measure of speed.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was driving ten <w>miles</w> over the speed limit.",

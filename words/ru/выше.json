                    "sense_id": "34785a59-9b38-3c36-a714-cd354dec0112",
                    "sense_definition": "Сравнительная степень прилагательного <w>высокий</w>; имеющий бо́льшую высоту или рост.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comp_high",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой старший брат уже <w>выше</w> отца.",
                            "target_lang_translations": {
                                "en": "My older brother is already <w>taller</w> than father."
                            }
                        },
                        {
                            "text": "Это новое здание значительно <w>выше</w> соседних домов.",
                            "target_lang_translations": {
                                "en": "This new building is significantly <w>higher</w> than the neighboring houses."
                            }
                        }
                    ],
                    "antonyms": [
                        "ниже"
                    ],
                    "common_phrases": [
                        "стать выше",
                        "значительно выше",
                        "намного выше"
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative form of \"high\" or \"tall\"; having greater height or stature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "taller",
                                "target_lang_sense_clarification": "used for people, trees, and vertical objects"
                            },
                            {
                                "target_lang_word": "higher",
                                "target_lang_sense_clarification": "used for buildings, mountains, and general height"
                            }
                        ]
                    }
                }
            ]
        },

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4509726b-f632-37b2-a26e-06581c3a1ea5",
                    "sense_definition": "A person who is new to an activity, job, or organization and lacks experience.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "novice_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>rookie</w> asked his supervisor for help with the difficult task.",
                            "target_lang_translations": {
                                "ru": "<w>Новичок</w> попросил своего руководителя о помощи с трудным заданием."
                            }
                        },
                        {
                            "text": "I am a total <w>rookie</w> at chess, so please go easy on me.",
                            "target_lang_translations": {
                                "ru": "Я полный <w>чайник</w> в шахматах, так что, пожалуйста, не суди строго."
                            }
                        }
                    ],
                    "synonyms": [
                        "beginner",
                        "novice",
                        "newbie"
                    ],
                    "antonyms": [
                        "expert",
                        "veteran",
                        "professional"
                    ],
                    "common_phrases": [
                        "total rookie",
                        "green rookie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который является новичком в какой-либо деятельности, работе или организации и не имеет опыта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "новичок",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "неопытный работник",
                                "target_lang_sense_clarification": "о сотруднике"
                            },
                            {
                                "target_lang_word": "чайник",
                                "target_lang_sense_clarification": "разговорное, о неумелом человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4120578-01b2-3b8e-a2fb-68963e651ac9",
                    "sense_definition": "An athlete in their first year of playing for a professional team or in a specific league.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "novice_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He played so well during his <w>rookie</w> season that he won an award.",
                            "target_lang_translations": {
                                "ru": "Он играл так хорошо в свой <w>дебютный</w> сезон, что выиграл награду."
                            }
                        },
                        {
                            "text": "The coach decided to put the <w>rookie</w> in the starting lineup for the final game.",
                            "target_lang_translations": {
                                "ru": "Тренер решил поставить <w>новичка</w> в стартовый состав на финальную игру."
                            }
                        }
                    ],
                    "antonyms": [
                        "veteran"
                    ],
                    "common_phrases": [
                        "rookie season",
                        "rookie of the year"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sports"

{
    "word_family": [
        "sequential",
        "sequencer",
        "subsequent",
        "consequence"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8b7aa21-aab9-3297-927a-fdb3bf4b4104",
                    "sense_definition": "A set of things next to each other in a set order; a series.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The numbers follow a specific <w>sequence</w>.",
                            "target_lang_translations": {
                                "ru": "Числа следуют в определенной <w>последовательности</w>."
                            }
                        },
                        {
                            "text": "He memorized the <w>sequence</w> of events perfectly.",
                            "target_lang_translations": {
                                "ru": "Он идеально запомнил <w>последовательность</w> событий."
                            }
                        }
                    ],
                    "synonyms": [
                        "series",
                        "chain",
                        "string",
                        "succession"
                    ],
                    "antonyms": [
                        "hodgepodge"
                    ],
                    "common_phrases": [
                        "logical sequence",
                        "number sequence",
                        "sequence of events"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор вещей, следующих друг за другом в определенном порядке; серия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последовательность",
                                "target_lang_sense_clarification": "порядок следования чего-либо"
                            },
                            {
                                "target_lang_word": "ряд",
                                "target_lang_sense_clarification": "совокупность предметов, расположенных друг за другом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c0004b6-a145-31f5-9957-65edb562c390",
                    "sense_definition": "The state of being sequent or following; order of succession.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please complete the listed tasks in <w>sequence</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, выполняйте перечисленные задачи по <w>порядку</w>."
                            }
                        },
                        {
                            "text": "The pages of the book were out of <w>sequence</w>.",
                            "target_lang_translations": {
                                "ru": "Страницы книги были не по <w>порядку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "succession"
                    ],
                    "common_phrases": [
                        "in sequence",
                        "out of sequence",
                        "chronological sequence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние следования или нахождения позади; порядок преемственности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядок",
                                "target_lang_sense_clarification": "правильное расположение, последовательный ход"
                            },
                            {
                                "target_lang_word": "очерёдность",
                                "target_lang_sense_clarification": "последовательность в очереди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9265ed27-cb44-37b5-892e-431407931dc5",
                    "sense_definition": "A series of shots that depict a single action or style in a film or television show.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie features a thrilling car chase <w>sequence</w>.",
                            "target_lang_translations": {
                                "ru": "В фильме есть захватывающий <w>эпизод</w> с погоней на машинах."
                            }
                        },
                        {
                            "text": "The director filmed the dream <w>sequence</w> in black and white.",
                            "target_lang_translations": {
                                "ru": "Режиссер снял <w>сцену</w> сна в черно-белом цвете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dream sequence",
                        "action sequence",
                        "opening sequence"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Film"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия кадров, изображающих одно действие или стиль в фильме или телешоу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпизод",
                                "target_lang_sense_clarification": "часть фильма"
                            },
                            {
                                "target_lang_word": "сцена",
                                "target_lang_sense_clarification": "часть действия в фильме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82275777-853d-31d2-b578-1940fc5ede03",
                    "sense_definition": "An ordered list of objects, typically indexed with natural numbers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Fibonacci <w>sequence</w> appears often in nature.",
                            "target_lang_translations": {
                                "ru": "<w>Последовательность</w> Фибоначчи часто встречается в природе."
                            }
                        },
                        {
                            "text": "Calculate the next number in the <w>sequence</w>.",
                            "target_lang_translations": {
                                "ru": "Вычислите следующее число в <w>последовательности</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченный список объектов, обычно пронумерованных натуральными числами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последовательность",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f916f510-6fa4-36aa-b6c3-dc38fe6c7794",
                    "sense_definition": "A series of musical phrases where a theme or melody is repeated, with some change each time, such as in pitch or length.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The composer used a melodic <w>sequence</w> to build tension.",
                            "target_lang_translations": {
                                "ru": "Композитор использовал мелодическую <w>секвенцию</w> для нагнетания напряжения."
                            }
                        },
                        {
                            "text": "Beethoven's Fifth Symphony opens with a famous <w>sequence</w>.",
                            "target_lang_translations": {
                                "ru": "Пятая симфония Бетховена открывается знаменитой <w>секвенцией</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Music"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия музыкальных фраз, где тема или мелодия повторяется с некоторыми изменениями, например, по высоте или длительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секвенция",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "076df78a-da41-3906-85b7-c23f7a3fea5b",
                    "sense_definition": "A meld consisting of three or more cards of successive ranks in the same suit.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She won the game with a <w>sequence</w> of hearts.",
                            "target_lang_translations": {
                                "ru": "Она выиграла игру с <w>последовательностью</w> червей."
                            }
                        },
                        {
                            "text": "You need a <w>sequence</w> of at least three cards to score points.",
                            "target_lang_translations": {
                                "ru": "Чтобы набрать очки, нужна <w>последовательность</w> как минимум из трех карт."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Card games"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Комбинация, состоящая из трех или более карт последовательных рангов одной масти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последовательность",
                                "target_lang_sense_clarification": "карт"
                            },
                            {
                                "target_lang_word": "секвенция",
                                "target_lang_sense_clarification": "термин в карточных играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cef38ac4-4424-3f43-9190-9d7e0580b9cf",
                    "sense_definition": "A subsequent event; a consequence or result.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His anger was a natural <w>sequence</w> to the insult.",
                            "target_lang_translations": {
                                "ru": "Его гнев был естественным <w>следствием</w> оскорбления."
                            }
                        },
                        {
                            "text": "The punishment was a sad <w>sequence</w> of his poor choices.",
                            "target_lang_translations": {
                                "ru": "Наказание стало печальным <w>результатом</w> его неудачного выбора."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Последующее событие; последствие или результат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следствие",
                                "target_lang_sense_clarification": "результат чего-либо"
                            },
                            {
                                "target_lang_word": "результат",
                                "target_lang_sense_clarification": "итог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a43ac91e-4fb0-3ac3-968a-3ff249193110",
                    "sense_definition": "A musical composition used in some Catholic Masses between the readings.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir sang a beautiful <w>sequence</w> during the funeral service.",
                            "target_lang_translations": {
                                "ru": "Хор пел красивую <w>секвенцию</w> во время заупокойной службы."
                            }
                        },
                        {
                            "text": "Dies Irae is a well-known <w>sequence</w> from the Mass.",
                            "target_lang_translations": {
                                "ru": "Dies Irae — известная <w>секвенция</w> из мессы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Religion"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальная композиция, используемая в некоторых католических мессах между чтениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секвенция",
                                "target_lang_sense_clarification": "жанр песнопения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a0127dcf-e42d-3a91-a438-83e003f6e86a",
                    "sense_definition": "To arrange something in a specific order.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>sequence</w> the files by date.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>упорядочить</w> файлы по дате."
                            }
                        },
                        {
                            "text": "The historian tried to <w>sequence</w> the events of the war accurately.",
                            "target_lang_translations": {
                                "ru": "Историк пытался точно <w>выстроить в хронологическом порядке</w> события войны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sequence the data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагать что-либо в определенном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упорядочивать",
                                "target_lang_sense_clarification": "приводить в порядок"
                            },
                            {
                                "target_lang_word": "расставлять по порядку",
                                "target_lang_sense_clarification": "организовывать последовательность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87c390df-21dd-37b1-ae55-77e6a3cef2a3",
                    "sense_definition": "To determine the order of monomers in a biological polymer, such as amino acids in a protein or bases in a nucleic acid.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists managed to <w>sequence</w> the entire human genome.",
                            "target_lang_translations": {
                                "ru": "Ученым удалось <w>секвенировать</w> весь геном человека."
                            }
                        },
                        {
                            "text": "It is expensive to <w>sequence</w> DNA samples in a private lab.",
                            "target_lang_translations": {
                                "ru": "<w>Секвенировать</w> образцы ДНК в частной лаборатории дорого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sequence DNA",
                        "sequence the genome"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять порядок мономеров в биологическом полимере, например, аминокислот в белке или оснований в нуклеиновой кислоте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секвенировать",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81f96450-8d3c-34f6-be28-280f9aa55c76",
                    "sense_definition": "To produce music with a sequencer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He learned how to <w>sequence</w> drums on his computer.",
                            "target_lang_translations": {
                                "ru": "Он научился <w>программировать</w> ударные на своем компьютере."
                            }
                        },
                        {
                            "text": "The producer <w>sequenced</w> the track using vintage hardware.",
                            "target_lang_translations": {
                                "ru": "Продюсер <w>запрограммировал</w> трек, используя винтажное оборудование."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Music"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать музыку с помощью секвенсора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программировать",
                                "target_lang_sense_clarification": "создавать партию в секвенсоре"
                            },
                            {
                                "target_lang_word": "прописывать",
                                "target_lang_sense_clarification": "сленг музыкантов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd68e303-21ad-3381-8f79-2929907e1258",
                    "sense_definition": "To proceed through a sequence or series of things.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software will <w>sequence</w> through the images automatically.",
                            "target_lang_translations": {
                                "ru": "Программа будет автоматически <w>перебирать</w> изображения."
                            }
                        },
                        {
                            "text": "Users can <w>sequence</w> through different options on the menu.",
                            "target_lang_translations": {
                                "ru": "Пользователи могут <w>пролистывать</w> различные опции в меню."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить через последовательность или серию вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перебирать",
                                "target_lang_sense_clarification": "проходить по очереди"
                            },
                            {
                                "target_lang_word": "пролистывать",
                                "target_lang_sense_clarification": "просматривать по порядку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b08944ba-b64e-392f-aec2-ff1796c5d005",
                    "sense_definition": "Внимательно посмотреть на кого-либо или что-либо с разных сторон.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_examination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Покупатель решил внимательно <w>осмотреть</w> автомобиль перед тем, как платить деньги.",
                            "target_lang_translations": {
                                "en": "The buyer decided to carefully <w>inspect</w> the car before paying the money."
                            }
                        },
                        {
                            "text": "Она подошла к зеркалу, чтобы <w>осмотреть</w> своё новое платье со всех сторон.",
                            "target_lang_translations": {
                                "en": "She approached the mirror to <w>look over</w> her new dress from all sides."
                            }
                        }
                    ],
                    "synonyms": [
                        "оглядеть"
                    ],
                    "common_phrases": [
                        "внимательно осмотреть",
                        "осмотреть себя",
                        "осмотреть комнату",
                        "бегло осмотреть"
                    ],
                    "target_lang_definitions": {
                        "en": "To look at someone or something carefully from different sides."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "examine",
                                "target_lang_sense_clarification": "look closely at details"
                            },
                            {
                                "target_lang_word": "look over",
                                "target_lang_sense_clarification": "view thoroughly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25409a0a-ef61-312c-b512-c426ce26963f",
                    "sense_definition": "Провести медицинское обследование для определения состояния здоровья.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_exam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач должен <w>осмотреть</w> пациента и послушать его легкие перед постановкой диагноза.",
                            "target_lang_translations": {
                                "en": "The doctor must <w>examine</w> the patient and listen to his lungs before making a diagnosis."
                            }
                        },
                        {
                            "text": "Ветеринар согласился <w>осмотреть</w> собаку, которая повредила лапу на прогулке.",
                            "target_lang_translations": {
                                "en": "The veterinarian agreed to <w>examine</w> the dog that injured its paw on the walk."
                            }
                        }
                    ],
                    "synonyms": [
                        "обследовать"
                    ],
                    "common_phrases": [
                        "врач осмотрел",
                        "осмотреть больного",
                        "осмотреть рану",
                        "осмотреть пациента"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a medical check-up to determine health status."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "examine",
                                "target_lang_sense_clarification": "medical check-up"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06ab04d7-6084-305b-94db-0c2ccb399849",
                    "sense_definition": "Ознакомиться с чем-либо (городом, музеем, выставкой), обходя и рассматривая.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sightseeing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы успели <w>осмотреть</w> главные достопримечательности города всего за один день.",
                            "target_lang_translations": {
                                "en": "The tourists managed to <w>see</w> the main sights of the city in just one day."
                            }
                        },
                        {
                            "text": "Мы планируем <w>осмотреть</w> старинный замок, который находится на вершине горы.",
                            "target_lang_translations": {
                                "en": "We plan to <w>look around</w> the ancient castle located on the mountain top."
                            }
                        }
                    ],
                    "common_phrases": [
                        "осмотреть достопримечательности",
                        "осмотреть город",
                        "осмотреть музей",
                        "осмотреть выставку"
                    ],
                    "target_lang_definitions": {
                        "en": "To visit a place like a city or museum to see what is there."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look around",
                                "target_lang_sense_clarification": "explore a place"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "visit attractions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caa7d9b6-6099-3ba6-9c44-01456e28eebb",
                    "sense_definition": "Проверить или обследовать что-либо с официальной или специальной целью (для поиска чего-либо или проверки исправности).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official_inspection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция прибыла на место, чтобы <w>осмотреть</w> место происшествия и собрать улики.",
                            "target_lang_translations": {
                                "en": "The police arrived at the scene to <w>examine</w> the crime scene and collect evidence."
                            }
                        },
                        {
                            "text": "Механик должен <w>осмотреть</w> двигатель, чтобы понять причину странного шума.",
                            "target_lang_translations": {
                                "en": "The mechanic needs to <w>inspect</w> the engine to understand the cause of the strange noise."
                            }
                        }
                    ],
                    "synonyms": [
                        "обследовать",
                        "исследовать"
                    ],
                    "common_phrases": [
                        "осмотреть место происшествия",
                        "осмотреть багаж",
                        "осмотреть технику",
                        "осмотреть помещение"
                    ],
                    "target_lang_definitions": {
                        "en": "To check or investigate something for an official or technical purpose."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inspect",
                                "target_lang_sense_clarification": "check condition or compliance"
                            },
                            {
                                "target_lang_word": "examine",
                                "target_lang_sense_clarification": "investigate details"
                            }
                        ]
                    }
                }
            ]
        }
    ]

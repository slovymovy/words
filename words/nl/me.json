    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "a41dff88-380c-3067-be06-d6174bee4de3",
                    "sense_definition": "De onbeklemtoonde vorm van het persoonlijk voornaamwoord <w>mij</w> (eerste persoon enkelvoud, object).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je <w>me</w> even helpen met deze zware tas?",
                            "target_lang_translations": {
                                "en": "Can you help <w>me</w> for a moment with this heavy bag?",
                                "ru": "Ты можешь <w>мне</w> помочь с этой тяжелой сумкой?"
                            }
                        },
                        {
                            "text": "Hij heeft <w>me</w> gisteren nog gebeld over de afspraak.",
                            "target_lang_translations": {
                                "en": "He called <w>me</w> yesterday about the appointment.",
                                "ru": "Он звонил <w>мне</w> вчера по поводу встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "mij"
                    ],
                    "common_phrases": [
                        "volgens me",
                        "voor me",
                        "naar me toe"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "unstressed form of 'mij'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The unstressed form of the personal pronoun 'me' (first person singular, object).",
                        "ru": "Безударная форма личного местоимения \"mij\" (первое лицо, единственное число, объектный падеж)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "me",
                                "target_lang_sense_clarification": "object pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "меня",
                                "target_lang_sense_clarification": "винительный/родительный падеж"
                            },
                            {
                                "target_lang_word": "мне",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0d91d3b-b5fb-3f2a-b19f-a84046ee566b",
                    "sense_definition": "Wederkerend voornaamwoord eerste persoon enkelvoud (onbeklemtoond); verwijst terug naar het onderwerp 'ik'.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet <w>me</w> haasten om op tijd op mijn werk te komen.",
                            "target_lang_translations": {
                                "en": "I have to hurry to get to work on time.",
                                "ru": "Я должен <w>поторопиться</w>, чтобы вовремя попасть на работу."
                            }
                        },
                        {
                            "text": "Ik voel <w>me</w> vandaag veel beter dan gisteren.",
                            "target_lang_translations": {
                                "en": "I feel much better today than yesterday.",
                                "ru": "Я чувствую <w>себя</w> сегодня намного лучше, чем вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "mezelf",
                        "mijzelf"
                    ],
                    "common_phrases": [
                        "ik vergis me",
                        "ik herinner me",
                        "ik vraag me af"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "unstressed form of 'mezelf'/'mijzelf'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Reflexive pronoun first person singular (unstressed); refers back to the subject 'I'.",
                        "ru": "Возвратное местоимение первого лица единственного числа (безударное); отсылает к подлежащему \"ik\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "myself",
                                "target_lang_sense_clarification": "reflexive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "-ся",
                                "target_lang_sense_clarification": "возвратный суффикс глагола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6954afff-379c-3260-976b-32103b48496a",
                    "sense_definition": "Spreektaalvariant van het bezittelijk voornaamwoord <w>mijn</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_possessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben <w>me</w> sleutels vergeten mee te nemen.",
                            "target_lang_translations": {
                                "en": "I forgot to bring <w>my</w> keys.",
                                "ru": "Я забыл взять <w>свои</w> ключи."
                            }
                        },
                        {
                            "text": "Dat is <w>me</w> wat moois!",
                            "target_lang_translations": {
                                "en": "That is quite something!",
                                "ru": "Ну и дела!"
                            }
                        }
                    ],
                    "synonyms": [
                        "mijn",
                        "m'n"
                    ],
                    "common_phrases": [
                        "op me gemak",
                        "van me leven"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered incorrect in formal writing"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial variant of the possessive pronoun 'my'.",
                        "ru": "Разговорный вариант притяжательного местоимения \"mijn\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "my",
                                "target_lang_sense_clarification": "possessive (colloquial)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мой",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "принадлежность подлежащему"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22f8bdf5-d82c-331c-b549-e0498bcd7688",
                    "sense_definition": "Mobiele Eenheid; een speciale afdeling van de politie die wordt ingezet bij rellen of grote evenementen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_police",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "De <w>ME</w> moest ingrijpen toen de demonstratie uit de hand liep.",
                            "target_lang_translations": {
                                "en": "The <w>riot police</w> had to intervene when the demonstration got out of hand.",
                                "ru": "<w>ОМОН</w> был вынужден вмешаться, когда демонстрация вышла из-под контроля."
                            }
                        },
                        {
                            "text": "Er stond veel <w>ME</w> klaar rondom het stadion voor de wedstrijd.",
                            "target_lang_translations": {
                                "en": "There were a lot of <w>riot police</w> ready around the stadium before the match.",
                                "ru": "Вокруг стадиона перед матчем стояло много бойцов <w>ОМОНа</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Riot police; a special police unit deployed during riots or large events.",
                        "ru": "Мобильное подразделение; специальный отряд полиции для подавления беспорядков."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "riot police",
                                "target_lang_sense_clarification": "Mobile Unit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ОМОН",
                                "target_lang_sense_clarification": "культурный эквивалент"
                            },
                            {
                                "target_lang_word": "спецназ",
                                "target_lang_sense_clarification": "полиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b7cdb2c-b255-3983-9a7f-4e9bb15045b4",
                    "sense_definition": "Myalgische encefalomyelitis; een chronische ziekte die wordt gekenmerkt door ernstige vermoeidheid.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Patiënten met <w>ME</w> hebben vaak weinig energie voor dagelijkse activiteiten.",
                            "target_lang_translations": {
                                "en": "Patients with <w>ME</w> often have little energy for daily activities.",
                                "ru": "У пациентов с <w>МЭ</w> часто мало энергии для повседневных дел."
                            }
                        },
                        {
                            "text": "De diagnose <w>ME</w> is soms lastig te stellen door artsen.",
                            "target_lang_translations": {
                                "en": "The diagnosis of <w>ME</w> is sometimes difficult for doctors to make.",
                                "ru": "Врачам иногда сложно поставить диагноз <w>МЭ</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Myalgic encephalomyelitis; a chronic disease characterized by severe fatigue.",
                        "ru": "Миалгический энцефаломиелит; хроническое заболевание, характеризующееся сильной усталостью."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ME",
                                "target_lang_sense_clarification": "medical abbreviation"
                            },
                            {
                                "target_lang_word": "chronic fatigue syndrome",
                                "target_lang_sense_clarification": "condition name"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "МЭ",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "синдром хронической усталости",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        }
    ]

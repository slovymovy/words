                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "informeel gebruik"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вымогать деньги или товары у кого-либо под принуждением или угрозами.",
                        "en": "To force someone to hand over money or goods under duress or threat."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вымогать",
                                "target_lang_sense_clarification": "требовать силой или угрозами"
                            },
                            {
                                "target_lang_word": "вытрясать",
                                "target_lang_sense_clarification": "разговорное, добиваться отдачи денег"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "extort",
                                "target_lang_sense_clarification": "obtain by force or threats"
                            },

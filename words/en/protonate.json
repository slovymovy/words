    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6b8219ac-4ce3-31c0-8353-cfa12c652429",
                    "sense_definition": "To add a proton (a positively charged subatomic particle) to an atom, molecule, or ion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chemical_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strong acid will <w>protonate</w> the water molecules to form hydronium ions.",
                            "target_lang_translations": {
                                "ru": "Сильная кислота <w>протонирует</w> молекулы воды с образованием ионов гидроксония."
                            }
                        },
                        {
                            "text": "Chemists often <w>protonate</w> the carbonyl group to make it more reactive.",
                            "target_lang_translations": {
                                "ru": "Химики часто <w>протонируют</w> карбонильную группу, чтобы сделать её более реакционноспособной."
                            }
                        }
                    ],
                    "antonyms": [
                        "deprotonate"
                    ],
                    "common_phrases": [
                        "protonate the base",
                        "protonate the group",
                        "protonate the molecule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять протон (положительно заряженную субатомную частицу) к атому, молекуле или иону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протонировать",
                                "target_lang_sense_clarification": "добавлять протон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bb4da06-1176-3746-b90e-312052997e85",
                    "sense_definition": "To acquire or accept an additional proton.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chemical_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Under these specific conditions, the amine will <w>protonate</w> very rapidly.",
                            "target_lang_translations": {
                                "ru": "В этих специфических условиях амин <w>протонируется</w> очень быстро."
                            }
                        },
                        {
                            "text": "Some substances <w>protonate</w> easily when they are dissolved in an acidic solution.",
                            "target_lang_translations": {
                                "ru": "Некоторые вещества легко <w>протонируются</w> при растворении в кислом растворе."
                            }
                        }
                    ],
                    "antonyms": [
                        "deprotonate"
                    ],
                    "common_phrases": [
                        "protonate easily",
                        "readily protonate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приобретать или принимать дополнительный протон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протонироваться",
                                "target_lang_sense_clarification": "принимать протон"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historisch"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A member of a West Germanic people who founded a large empire in Europe in the early Middle Ages.",
                        "ru": "Представитель западногерманского народа, основавшего крупную империю в Европе в раннем Средневековье."
                    },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bbfea08-4444-3f37-8201-5fd842d74d4f",
                    "sense_definition": "Een gedomesticeerd zoogdier met een stompe snuit en een krulstaart, dat vaak gehouden wordt voor zijn vlees",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de kinderboerderij ligt het <w>varken</w> lekker in de modder te rollen.",
                            "target_lang_translations": {
                                "ru": "На детской ферме <w>свинья</w> с удовольствием валяется в грязи."
                            }
                        },
                        {
                            "text": "De boer heeft honderden <w>varkens</w> in zijn stallen staan.",
                            "target_lang_translations": {
                                "ru": "У фермера в хлеву сотни <w>свиней</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwijn"
                    ],
                    "common_phrases": [
                        "varkentje wassen",
                        "schreeuwen als een mager varken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Домашнее млекопитающее с тупым рылом и закрученным хвостом, которое часто разводят ради мяса"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинья",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f497b13c-e8f8-379e-9e82-64b36d9be19f",
                    "sense_definition": "Iemand die zich vies, ongemanierd of lomp gedraagt",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eet eens netjes en smak niet zo, je lijkt wel een <w>varken</w>.",
                            "target_lang_translations": {
                                "ru": "Ешь аккуратно и не чавкай, ты похож на <w>свинью</w>."
                            }
                        },
                        {
                            "text": "Hij gedroeg zich als een <w>varken</w> door al zijn afval op straat te gooien.",
                            "target_lang_translations": {
                                "ru": "Он вёл себя как <w>свинья</w>, разбрасывая мусор на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwijn"
                    ],
                    "common_phrases": [
                        "vies varken",
                        "lui varken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто ведёт себя грязно, невоспитанно или грубо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинья",
                                "target_lang_sense_clarification": "грязный или грубый человек"
                            },
                            {
                                "target_lang_word": "неряха",
                                "target_lang_sense_clarification": "неопрятный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7285944-c1c5-34b0-9297-fd99029d13cc",
                    "sense_definition": "Een zachte borstel om stof en vuil mee op te vegen, vaak gebruikt in combinatie met een blik",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pak even het <w>varken</w> en het blik om die scherven op te ruimen.",
                            "target_lang_translations": {
                                "ru": "Возьми <w>щётку</w> и совок, чтобы убрать эти осколки."
                            }
                        },
                        {
                            "text": "Zij veegde de vloer schoon met een <w>varken</w>.",
                            "target_lang_translations": {
                                "ru": "Она подмела пол <w>щёткой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "borstel",
                        "schuier"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België (Vlaanderen)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкая щётка для сметания пыли и грязи, часто используемая вместе с совком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щётка",
                                "target_lang_sense_clarification": "инструмент для уборки"
                            },
                            {
                                "target_lang_word": "смётка",
                                "target_lang_sense_clarification": "ручная щётка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0dd3f36-55ed-34ab-bda8-069df1385604",
                    "sense_definition": "Een student die geen lid is van een studentenvereniging",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "student_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Binnen het corps wordt soms neergekeken op een <w>varken</w>.",
                            "target_lang_translations": {
                                "ru": "Внутри студенческой корпорации иногда смотрят свысока на <w>независимого студента</w>."
                            }
                        },
                        {
                            "text": "Hij besloot geen lid te worden en bleef zijn hele studietijd een <w>varken</w>.",
                            "target_lang_translations": {
                                "ru": "Он решил не вступать в братство и всё время учёбы оставался <w>независимым студентом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "knor"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "studententaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Студент, не являющийся членом студенческой ассоциации (корпорации)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "независимый студент",
                                "target_lang_sense_clarification": "не состоящий в братстве"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
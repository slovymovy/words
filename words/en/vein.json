{
    "word_family": [
        "veiny",
        "venous",
        "venation",
        "intravenous",
        "venule"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "58144818-73e5-31ff-8d12-b9953f2b4e25",
                    "sense_definition": "A tube in the body that carries blood from the tissues back to the heart.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biology_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse struggled to find a <w>vein</w> in the patient's arm to draw blood.",
                            "target_lang_translations": {
                                "ru": "Медсестра с трудом нашла <w>вену</w> на руке пациента, чтобы взять кровь."
                            }
                        },
                        {
                            "text": "Blood flows through the <w>veins</w> to return to the heart for oxygenation.",
                            "target_lang_translations": {
                                "ru": "Кровь течет по <w>венам</w>, возвращаясь к сердцу для насыщения кислородом."
                            }
                        }
                    ],
                    "antonyms": [
                        "artery"
                    ],
                    "common_phrases": [
                        "varicose veins",
                        "jugular vein",
                        "spider veins"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосуд в теле, несущий кровь от тканей обратно к сердцу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вена",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "кровеносный сосуд",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "959a696b-8bde-3f7e-8984-deef69fdd4d5",
                    "sense_definition": "A distinctive style, manner, or train of thought.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author's new book continues in the same humorous <w>vein</w> as his previous works.",
                            "target_lang_translations": {
                                "ru": "Новая книга автора написана в том же юмористическом <w>ключе</w>, что и его предыдущие работы."
                            }
                        },
                        {
                            "text": "She spoke in a serious <w>vein</w> about the challenges facing the company.",
                            "target_lang_translations": {
                                "ru": "Она говорила в серьезном <w>тоне</w> о проблемах, стоящих перед компанией."
                            }
                        }
                    ],
                    "synonyms": [
                        "vena",
                        "vena"
                    ],
                    "common_phrases": [
                        "in the same vein",
                        "in a similar vein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерный стиль, манера или ход мыслей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дух",
                                "target_lang_sense_clarification": "настроение, характер (обычно в фразах 'в том же духе')"
                            },
                            {
                                "target_lang_word": "ключ",
                                "target_lang_sense_clarification": "переносное значение: манера, тон"
                            },
                            {
                                "target_lang_word": "манера",
                                "target_lang_sense_clarification": "стиль поведения или речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b936f01e-152a-3494-b6fb-159e1d0e93ee",
                    "sense_definition": "A layer of ore or minerals crystallized within a rock.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miners were excited to discover a rich <w>vein</w> of gold deep inside the mountain.",
                            "target_lang_translations": {
                                "ru": "Шахтеры были взволнованы, обнаружив богатую золотую <w>жилу</w> глубоко внутри горы."
                            }
                        },
                        {
                            "text": "They followed the coal <w>vein</w> for several miles underground.",
                            "target_lang_translations": {
                                "ru": "Они шли вдоль угольной <w>жилы</w> несколько миль под землей."
                            }
                        }
                    ],
                    "synonyms": [
                        "vena",
                        "vena"
                    ],
                    "common_phrases": [
                        "gold vein",
                        "rich vein",
                        "mineral vein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой руды или минералов, кристаллизовавшийся внутри горной породы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жила",
                                "target_lang_sense_clarification": "геологический термин"
                            },
                            {
                                "target_lang_word": "пласт",
                                "target_lang_sense_clarification": "слой породы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb594ab8-935d-3724-a194-27b93b63b1a4",
                    "sense_definition": "A thin line that forms the supporting framework of a leaf or an insect's wing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can clearly see the delicate <w>veins</w> running through the green leaf.",
                            "target_lang_translations": {
                                "ru": "Можно отчетливо видеть тонкие <w>жилки</w>, проходящие через зеленый лист."
                            }
                        },
                        {
                            "text": "The sunlight shone through the transparent <w>veins</w> of the dragonfly's wings.",
                            "target_lang_translations": {
                                "ru": "Солнечный свет сиял сквозь прозрачные <w>жилки</w> крыльев стрекозы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leaf vein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкая линия, образующая несущий каркас листа или крыла насекомого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жилка",
                                "target_lang_sense_clarification": "у растений и насекомых"
                            },
                            {
                                "target_lang_word": "прожилка",
                                "target_lang_sense_clarification": "тонкое разветвление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "583d527a-e38b-385d-b90e-6009daee38df",
                    "sense_definition": "A streak or stripe of a different color or texture in a material like wood, marble, or cheese.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pattern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kitchen counter was made of white marble with a dark grey <w>vein</w>.",
                            "target_lang_translations": {
                                "ru": "Кухонная столешница была сделана из белого мрамора с темно-серой <w>прожилкой</w>."
                            }
                        },
                        {
                            "text": "This type of cheese is known for the blue <w>vein</w> running through its center.",
                            "target_lang_translations": {
                                "ru": "Этот тип сыра известен голубой <w>прожилкой</w>, проходящей через его центр."
                            }
                        }
                    ],
                    "synonyms": [
                        "vena",
                        "vena"
                    ],
                    "common_phrases": [
                        "blue vein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоска или линия другого цвета или текстуры в материале, например, в дереве, мраморе или сыре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прожилка",
                                "target_lang_sense_clarification": "вкрапление в материале"
                            },
                            {
                                "target_lang_word": "жилка",
                                "target_lang_sense_clarification": "тонкая полоска"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4c62ed3a-bb83-3a8c-8c3f-d8f9239dbaa4",
                    "sense_definition": "To mark with a pattern of lines resembling veins.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Flashes of lightning <w>veined</w> the dark sky during the intense storm.",
                            "target_lang_translations": {
                                "ru": "Вспышки молнии <w>испещрили</w> темное небо во время сильной бури."
                            }
                        },
                        {
                            "text": "Age had <w>veined</w> his hands with prominent blue lines.",
                            "target_lang_translations": {
                                "ru": "Возраст <w>покрыл</w> его руки выступающими синими венами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать узором из линий, напоминающих вены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испещрять",
                                "target_lang_sense_clarification": "покрывать узором или линиями"
                            },
                            {
                                "target_lang_word": "пронизывать",
                                "target_lang_sense_clarification": "проходить сквозь (о линиях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
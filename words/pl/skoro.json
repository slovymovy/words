{
    "word_family": [
        "skory",
        "skorość"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "bab48580-06cb-3b86-be1a-30a506c2d299",
                    "sense_definition": "Spójnik wprowadzający zdanie podrzędne, które stanowi uzasadnienie, przyczynę lub punkt wyjścia dla treści zdania nadrzędnego; ponieważ, zważywszy że.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie będę cię namawiał, <w>skoro</w> podjąłeś już ostateczną decyzję.",
                            "target_lang_translations": {
                                "ru": "Я не буду тебя уговаривать, <w>раз</w> ты уже принял окончательное решение."
                            }
                        },
                        {
                            "text": "<w>Skoro</w> tak bardzo ci na tym zależy, mogę się zgodzić.",
                            "target_lang_translations": {
                                "ru": "<w>Поскольку</w> тебе это так важно, я могу согласиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "jeśli",
                        "jeżeli"
                    ],
                    "common_phrases": [
                        "skoro tak",
                        "skoro już",
                        "no skoro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий придаточное предложение, которое содержит обоснование, причину или предпосылку; соответствует значениям «раз», «поскольку»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раз",
                                "target_lang_sense_clarification": "разговорный союз, указывающий на очевидную причину"
                            },
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "нейтральный/официальный союз причины"
                            },
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "в значении условия, принятого за факт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6626853-bb24-382e-810a-91b2f14a50df",
                    "sense_definition": "Spójnik oznaczający moment, w którym coś się wydarzyło lub wydarzy, wskazujący na bezpośrednie następstwo czasowe; gdy tylko.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Skoro</w> tylko przestało padać, dzieci wybiegły na podwórko.",
                            "target_lang_translations": {
                                "ru": "<w>Как только</w> перестал идти дождь, дети выбежали во двор."
                            }
                        },
                        {
                            "text": "Wyślemy towar, <w>skoro</w> tylko otrzymamy potwierdzenie przelewu.",
                            "target_lang_translations": {
                                "ru": "Мы отправим товар, <w>как только</w> получим подтверждение перевода."
                            }
                        }
                    ],
                    "synonyms": [
                        "gdy",
                        "kiedy"
                    ],
                    "common_phrases": [
                        "skoro tylko",
                        "skoro świt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, указывающий на момент времени, непосредственно предшествующий действию в главном предложении; как только."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как только",
                                "target_lang_sense_clarification": "временной союз"
                            },
                            {
                                "target_lang_word": "лишь только",
                                "target_lang_sense_clarification": "литературный вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "abf52312-271a-3f35-8317-1f353b48c934",
                    "sense_definition": "W sposób szybki, pośpieszny; obecnie rzadko używane w tym znaczeniu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Posłaniec przybył <w>skoro</w>, niosąc ważne wieści z frontu.",
                            "target_lang_translations": {
                                "ru": "Гонец прибыл <w>поспешно</w>, неся важные вести с фронта."
                            }
                        },
                        {
                            "text": "Musisz wracać <w>skoro</w>, zanim zapadnie zmrok.",
                            "target_lang_translations": {
                                "ru": "Ты должен возвращаться <w>быстро</w>, пока не стемнело."
                            }
                        }
                    ],
                    "synonyms": [
                        "szybko",
                        "prędko",
                        "rychło"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie zastąpione przez 'szybko' lub 'prędko'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наречие, означающее быстрое, поспешное действие; в современном языке в этом значении встречается редко."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быстро",
                                "target_lang_sense_clarification": "современный эквивалент"
                            },
                            {
                                "target_lang_word": "поспешно",
                                "target_lang_sense_clarification": "наречие образа действия"
                            },
                            {
                                "target_lang_word": "скоро",
                                "target_lang_sense_clarification": "устаревшее значение скорости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "consider",
        "considerable",
        "considerably",
        "considerate",
        "considerately",
        "reconsider",
        "reconsideration"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "276e22ad-f02a-3372-9280-074ab7cfc5d3",
                    "sense_definition": "The act of thinking about something carefully, especially before making a choice or decision.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "thinking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After much <w>consideration</w>, I have decided to accept the job offer.",
                            "target_lang_translations": {
                                "ru": "После долгих <w>размышлений</w> я решил принять предложение о работе."
                            }
                        },
                        {
                            "text": "The proposal is currently under serious <w>consideration</w> by the committee.",
                            "target_lang_translations": {
                                "ru": "Это предложение в данный момент находится на серьезном <w>рассмотрении</w> комитета."
                            }
                        }
                    ],
                    "synonyms": [
                        "deliberation",
                        "thought",
                        "reflection",
                        "contemplation",
                        "pondering",
                        "cogitation",
                        "rumination"
                    ],
                    "common_phrases": [
                        "take into consideration",
                        "under consideration",
                        "careful consideration",
                        "after much consideration",
                        "serious consideration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс тщательного обдумывания чего-либо, особенно перед принятием выбора или решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассмотрение",
                                "target_lang_sense_clarification": "процесс анализа или изучения вопроса"
                            },
                            {
                                "target_lang_word": "размышление",
                                "target_lang_sense_clarification": "мыслительный процесс"
                            },
                            {
                                "target_lang_word": "обсуждение",
                                "target_lang_sense_clarification": "совместный разбор темы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "942ae5ad-f654-34d4-9eb0-98add7bc7c5f",
                    "sense_definition": "A specific fact, detail, or reason that must be thought about when making a decision.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "thinking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Safety is the most important <w>consideration</w> when choosing a new car.",
                            "target_lang_translations": {
                                "ru": "Безопасность — самый важный <w>фактор</w> при выборе новой машины."
                            }
                        },
                        {
                            "text": "Financial <w>considerations</w> forced the company to delay the project.",
                            "target_lang_translations": {
                                "ru": "Финансовые <w>соображения</w> вынудили компанию отложить проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "factor",
                        "reason",
                        "motive"
                    ],
                    "common_phrases": [
                        "key consideration",
                        "important consideration",
                        "practical consideration",
                        "main consideration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный факт, деталь или причина, которые необходимо учитывать при принятии решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соображение",
                                "target_lang_sense_clarification": "довод, мотив или мысль, принимаемая в расчет"
                            },
                            {
                                "target_lang_word": "фактор",
                                "target_lang_sense_clarification": "обстоятельство, влияющее на решение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "430138f2-d5bf-3a9d-9fb0-35ee3a639fb8",
                    "sense_definition": "The quality of being sensitive, helpful, and respectful toward the feelings or needs of others.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He showed no <w>consideration</w> for his neighbors when he played loud music at night.",
                            "target_lang_translations": {
                                "ru": "Он не проявил никакого <w>уважения</w> к соседям, когда громко включал музыку ночью."
                            }
                        },
                        {
                            "text": "We remained quiet out of <w>consideration</w> for the sleeping baby.",
                            "target_lang_translations": {
                                "ru": "Мы вели себя тихо из <w>уважения</w> к спящему ребенку."
                            }
                        }
                    ],
                    "synonyms": [
                        "thought",
                        "thought"
                    ],
                    "antonyms": [
                        "selfishness",
                        "disregard"
                    ],
                    "common_phrases": [
                        "show consideration",
                        "out of consideration",
                        "lack of consideration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, проявляющееся в чуткости, внимании и уважении к чувствам или потребностям других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уважение",
                                "target_lang_sense_clarification": "почтительное отношение к другим"
                            },
                            {
                                "target_lang_word": "предупредительность",
                                "target_lang_sense_clarification": "вежливая внимательность и готовность помочь"
                            },
                            {
                                "target_lang_word": "чуткость",
                                "target_lang_sense_clarification": "внимание к интересам других"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05fd9bc1-0c5e-3330-890c-67994998cce7",
                    "sense_definition": "A payment or reward given in return for a service or help.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He agreed to guide the tourists through the old city for a small <w>consideration</w>.",
                            "target_lang_translations": {
                                "ru": "Он согласился провести туристов по старому городу за небольшое <w>вознаграждение</w>."
                            }
                        },
                        {
                            "text": "The official was willing to expedite the process for a <w>consideration</w>.",
                            "target_lang_translations": {
                                "ru": "Чиновник был готов ускорить процесс за определенное <w>вознаграждение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "reconsideration",
                        "rethinking"
                    ],
                    "common_phrases": [
                        "for a consideration"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Плата или награда, предоставляемая в обмен на услугу или помощь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вознаграждение",
                                "target_lang_sense_clarification": "оплата за услугу"
                            },
                            {
                                "target_lang_word": "компенсация",
                                "target_lang_sense_clarification": "возмещение затрат или усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08e798fb-a1e2-3678-b8be-41033f476be0",
                    "sense_definition": "Something of value exchanged between parties that makes a contract binding.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract was declared void by the court due to a lack of <w>consideration</w>.",
                            "target_lang_translations": {
                                "ru": "Контракт был признан судом недействительным из-за отсутствия <w>встречного удовлетворения</w>."
                            }
                        },
                        {
                            "text": "In <w>consideration</w> of the payment, the seller agrees to transfer the property title.",
                            "target_lang_translations": {
                                "ru": "В качестве <w>встречного удовлетворения</w> за платеж продавец соглашается передать право собственности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "valuable consideration",
                        "in consideration of"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "law"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ценность, которой обмениваются стороны, делающая договор юридически обязательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречное удовлетворение",
                                "target_lang_sense_clarification": "юридический термин в договорном праве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "158cbc25-7946-3236-a33f-093be8c81a96",
                    "sense_definition": "High status, respect, or importance attached to a person or thing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "esteem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family was held in high <w>consideration</w> by the local community.",
                            "target_lang_translations": {
                                "ru": "Семья пользовалась большим <w>уважением</w> в местном сообществе."
                            }
                        },
                        {
                            "text": "He was a man of some <w>consideration</w> in the literary world.",
                            "target_lang_translations": {
                                "ru": "Он был человеком определенной <w>значимости</w> в литературном мире."
                            }
                        }
                    ],
                    "common_phrases": [
                        "held in high consideration"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий статус, уважение или важность, приписываемые человеку или вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уважение",
                                "target_lang_sense_clarification": "высокая оценка обществом"
                            },
                            {
                                "target_lang_word": "почтение",
                                "target_lang_sense_clarification": "глубокое уважение"
                            },
                            {
                                "target_lang_word": "значимость",
                                "target_lang_sense_clarification": "важность, вес в обществе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
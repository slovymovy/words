                    "synonyms": [
                        "разнимать",
                        "разъединять"
                    ],
                    "antonyms": [
                        "собирать",
                        "соединять",
                        "складывать"
                    ],
                    "common_phrases": [
                        "разбирать на части",
                        "разбирать механизм",
                        "разбирать конструктор",
                        "разбирать мотор"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate a complex object into its component parts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take apart",
                                "target_lang_sense_clarification": "general use"
                            },
                            {

                {
                    "sense_id": "211fa03e-c660-3234-b66a-19dd3f0463da",
                    "sense_definition": "Сделанный из молока, приготовленный на молоке или содержащий молоко.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "composition",
                    "name_type": "no",

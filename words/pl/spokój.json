    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a537f4b-0dd3-32b0-8949-6ed02c2e5e98",
                    "sense_definition": "Stan równowagi psychicznej, brak zdenerwowania, lęku lub gniewu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz zachować <w>spokój</w>, żeby podjąć dobrą decyzję.",
                            "target_lang_translations": {
                                "ru": "Ты должен сохранять <w>спокойствие</w>, чтобы принять правильное решение."
                            }
                        },
                        {
                            "text": "Daj mi wreszcie święty <w>spokój</w> i wyjdź z mojego pokoju.",
                            "target_lang_translations": {
                                "ru": "Оставь меня наконец в <w>покое</w> и выйди из моей комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "opanowanie",
                        "równowaga",
                        "zimna krew",
                        "zrównoważenie"
                    ],
                    "antonyms": [
                        "nerwowość",
                        "niepokój"
                    ],
                    "common_phrases": [
                        "święty spokój",
                        "zachować spokój",
                        "dać spokój",
                        "zostawić w spokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние душевного равновесия, отсутствие нервозности, страха или гнева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спокойствие",
                                "target_lang_sense_clarification": "отсутствие волнения"
                            },
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "душевный комфорт"
                            },
                            {
                                "target_lang_word": "равновесие",
                                "target_lang_sense_clarification": "психическая устойчивость"
                            },
                            {
                                "target_lang_word": "безмятежность",
                                "target_lang_sense_clarification": "полная невозмутимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09a697da-17a5-3953-b2c8-d8f35bab1da3",
                    "sense_definition": "Brak hałasu, cisza, stan otoczenia wolny od zakłóceń i ruchu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "external_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie panował błogi <w>spokój</w>, przerywany tylko śpiewem ptaków.",
                            "target_lang_translations": {
                                "ru": "В лесу царил блаженный <w>покой</w>, прерываемый только пением птиц."
                            }
                        },
                        {
                            "text": "Cenię sobie ciszę i <w>spokój</w> podczas czytania książek.",
                            "target_lang_translations": {
                                "ru": "Я ценю тишину и <w>покой</w> во время чтения книг."
                            }
                        }
                    ],
                    "synonyms": [
                        "harmonia"
                    ],
                    "antonyms": [
                        "wrzawa"
                    ],
                    "common_phrases": [
                        "cisza i spokój",
                        "zakłócać spokój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие шума, тишина, состояние окружающей среды без помех и суеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тишина",
                                "target_lang_sense_clarification": "отсутствие звуков"
                            },
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "неподвижность и безмолвие"
                            },
                            {
                                "target_lang_word": "спокойствие",
                                "target_lang_sense_clarification": "мирная обстановка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3038a1e-24fc-39a9-a7dc-09e3f86c3167",
                    "sense_definition": "Stan braku konfliktów, kłótni lub wojny; zgoda między ludźmi.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej kłótni w domu wreszcie zapanował <w>spokój</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой ссоры в доме наконец воцарилось <w>спокойствие</w>."
                            }
                        },
                        {
                            "text": "Wszyscy obywatele mają prawo do życia w bezpieczeństwie i <w>spokoju</w>.",
                            "target_lang_translations": {
                                "ru": "Все граждане имеют право на жизнь в безопасности и <w>спокойствии</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokój",
                        "harmonia",
                        "łagodność"
                    ],
                    "antonyms": [
                        "niepokój"
                    ],
                    "common_phrases": [
                        "spokój publiczny",
                        "burzyć spokój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие конфликтов, ссор или войны; согласие между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спокойствие",
                                "target_lang_sense_clarification": "общественный порядок"
                            },
                            {
                                "target_lang_word": "гармония",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "отсутствие вражды"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d8665fc6-05c3-39f3-b947-1e47ab58c0fd",
                    "sense_definition": "Wykrzyknienie służące do uciszania kogoś lub przywoływania do porządku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Spokój</w>! Nie chcę słyszeć ani słowa więcej na ten temat.",
                            "target_lang_translations": {
                                "ru": "<w>Тихо</w>! Я не хочу слышать больше ни слова на эту тему."
                            }
                        },
                        {
                            "text": "Dzieci, <w>spokój</w>, bo obudzicie sąsiadów!",
                            "target_lang_translations": {
                                "ru": "Дети, <w>тихо</w>, а то разбудите соседей!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "spokój ma być"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a command"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, служащий для успокоения кого-либо или призыва к порядку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тихо",
                                "target_lang_sense_clarification": "призыв к молчанию"
                            },
                            {
                                "target_lang_word": "тишина",
                                "target_lang_sense_clarification": "требование прекратить шум"
                            }
                        ]
                    }
                }
            ]
        }

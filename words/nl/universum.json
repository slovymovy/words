            "senses": [
                {
                    "sense_id": "23ed851d-8dc2-3c58-b168-e148d55c21a7",
                    "sense_definition": "Het geheel van tijd, ruimte, materie en energie; alles wat bestaat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wetenschappers proberen te begrijpen hoe het <w>universum</w> is ontstaan.",
                            "target_lang_translations": {
                                "en": "Scientists are trying to understand how the <w>universe</w> originated.",
                                "ru": "Ученые пытаются понять, как возникла <w>вселенная</w>."
                            }
                        },
                        {
                            "text": "Onze planeet is maar een heel klein onderdeel van het onmetelijke <w>universum</w>.",
                            "target_lang_translations": {
                                "en": "Our planet is just a very small part of the immense <w>universe</w>.",
                                "ru": "Наша планета — лишь очень малая часть необъятной <w>вселенной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "heelal",
                        "kosmos"
                    ],
                    "common_phrases": [
                        "het hele universum",
                        "in het universum",
                        "ontstaan van het universum"
                    ],
                    "target_lang_definitions": {
                        "en": "The entirety of time, space, matter, and energy; everything that exists.",
                        "ru": "Совокупность времени, пространства, материи и энергии; всё, что существует."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "universe",

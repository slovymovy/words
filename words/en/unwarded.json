    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "eef2f0d4-a66a-34cf-9381-0603b40dbde1",
                    "sense_definition": "Not protected, watched over, or kept safe by a guard.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The spies slipped unnoticed through the <w>unwarded</w> gate at midnight.",
                            "target_lang_translations": {
                                "ru": "В полночь шпионы незамеченными проскользнули через <w>неохраняемые</w> ворота."
                            }
                        },
                        {
                            "text": "He felt uneasy leaving the valuable supplies <w>unwarded</w> in the open camp.",
                            "target_lang_translations": {
                                "ru": "Ему было не по себе оставлять ценные припасы <w>без охраны</w> в открытом лагере."
                            }
                        }
                    ],
                    "synonyms": [
                        "unguarded",
                        "unprotected",
                        "undefended",
                        "unwatched"
                    ],
                    "antonyms": [
                        "guarded",
                        "protected",
                        "defended"
                    ],
                    "common_phrases": [
                        "unwarded gate",
                        "unwarded entrance",
                        "left unwarded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не защищённый, не находящийся под наблюдением или охраной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неохраняемый",
                                "target_lang_sense_clarification": "о месте или проходе"
                            },
                            {
                                "target_lang_word": "без охраны",
                                "target_lang_sense_clarification": "состояние объекта"
                            },
                            {
                                "target_lang_word": "незащищённый",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e30f4641-4726-3dbd-99e1-ae01cf838b13",
                    "sense_definition": "Describing a lock or key that is not fitted with internal ridges or projections known as wards.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "locksmithing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique chest had a simple <w>unwarded</w> lock that offered little security.",
                            "target_lang_translations": {
                                "ru": "На старинном сундуке был простой замок <w>без перегородок</w>, который почти не обеспечивал безопасности."
                            }
                        },
                        {
                            "text": "An <w>unwarded</w> key has a smooth bit without the complex notches found in modern keys.",
                            "target_lang_translations": {
                                "ru": "Ключ <w>без нарезок</w> имеет гладкую бородку без сложных вырезов, характерных для современных ключей."
                            }
                        }
                    ],
                    "synonyms": [
                        "wardless"
                    ],
                    "antonyms": [
                        "warded"
                    ],
                    "common_phrases": [
                        "unwarded lock",
                        "unwarded key"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает замок или ключ, не имеющий внутренних защитных выступов (перегородок) или нарезок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без перегородок",
                                "target_lang_sense_clarification": "о механизме замка"
                            },
                            {
                                "target_lang_word": "без нарезок",
                                "target_lang_sense_clarification": "о ключе"
                            },
                            {
                                "target_lang_word": "бессувальдный",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

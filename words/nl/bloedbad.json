            "pos": "noun",
            "senses": [
                {
                    "sense_id": "86811f75-63b8-32d1-b22a-a3037dac2abd",
                    "sense_definition": "Een gewelddadige gebeurtenis waarbij veel mensen worden gedood of gewond raken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten richtten een vreselijk <w>bloedbad</w> aan in de veroverde stad.",
                            "target_lang_translations": {
                                "en": "The soldiers caused a terrible <w>massacre</w> in the conquered city.",
                                "ru": "Солдаты устроили ужасную <w>резню</w> в захваченном городе."
                            }
                        },
                        {
                            "text": "De politie wist door snel in te grijpen een groter <w>bloedbad</w> te voorkomen.",
                            "target_lang_translations": {
                                "en": "By intervening quickly, the police managed to prevent a larger <w>bloodbath</w>.",
                                "ru": "Благодаря быстрому вмешательству полиции удалось предотвратить еще большую <w>бойню</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "slachting",
                        "massamoord",
                        "bloedvergieten"
                    ],
                    "common_phrases": [
                        "een bloedbad aanrichten",
                        "een bloedbad voorkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "A violent event in which many people are killed or injured.",
                        "ru": "Насильственное событие, в ходе которого погибает или получает ранения множество людей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bloodbath",
                                "target_lang_sense_clarification": "literal slaughter"
                            },
                            {
                                "target_lang_word": "massacre",
                                "target_lang_sense_clarification": "indiscriminate killing"
                            },
                            {
                                "target_lang_word": "carnage",
                                "target_lang_sense_clarification": "chaotic violence"
                            }
                        ],
                        "ru": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ec26536-dc7f-3396-9196-11731e05c75e",
                    "sense_definition": "The ability or capacity to do something or act in a particular way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ability_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has lost the <w>power</w> to speak clearly after the accident.",
                            "target_lang_translations": {
                                "ru": "После аварии он потерял <w>способность</w> чётко говорить."
                            }
                        },
                        {
                            "text": "It is not within my <w>power</w> to help you with this specific problem.",
                            "target_lang_translations": {
                                "ru": "Не в моей <w>власти</w> помочь вам с этой конкретной проблемой."
                            }

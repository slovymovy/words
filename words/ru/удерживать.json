    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e868c234-e4a4-351a-8281-3d7b14650288",
                    "sense_definition": "Держать кого-либо или что-либо силой, не давая упасть, уйти или вырваться.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_holding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мать крепко <w>удерживала</w> ребёнка за руку, чтобы он не выбежал на дорогу.",
                            "target_lang_translations": {
                                "en": "The mother was firmly <w>holding</w> the child by the hand so he wouldn't run out onto the road."
                            }
                        },
                        {
                            "text": "Он с трудом <w>удерживал</w> тяжёлую дверь, пока остальные входили в здание.",
                            "target_lang_translations": {
                                "en": "He was <w>holding</w> the heavy door with difficulty while the others entered the building."
                            }
                        }
                    ],
                    "synonyms": [
                        "придерживать"
                    ],
                    "antonyms": [
                        "отпускать",
                        "выпускать"
                    ],
                    "common_phrases": [
                        "удерживать за руку",
                        "удерживать силой",
                        "удерживать равновесие"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold someone or something firmly to prevent them from falling, leaving, or breaking free."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to grasp or grip with hands"
                            },
                            {
                                "target_lang_word": "hold onto",
                                "target_lang_sense_clarification": "to keep a firm grip on something"
                            },
                            {
                                "target_lang_word": "restrain",
                                "target_lang_sense_clarification": "to hold back by physical force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62d6c3c2-2041-3fb9-803e-9d985bc4b7bc",
                    "sense_definition": "Сохранять что-либо у себя, не отдавать, не терять или поддерживать на определённом уровне (власть, внимание, тепло, позицию).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "retention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хороший оратор умеет <w>удерживать</w> внимание публики на протяжении всей речи.",
                            "target_lang_translations": {
                                "en": "A good orator knows how to <w>hold</w> the audience's attention throughout the entire speech."
                            }
                        },
                        {
                            "text": "Команде удалось <w>удерживать</w> преимущество в счёте до самого конца матча.",
                            "target_lang_translations": {
                                "en": "The team managed to <w>hold</w> the lead until the very end of the match."
                            }
                        }
                    ],
                    "synonyms": [
                        "сохранять",
                        "сберегать"
                    ],
                    "antonyms": [
                        "терять",
                        "утрачивать",
                        "упускать"
                    ],
                    "common_phrases": [
                        "удерживать внимание",
                        "удерживать тепло",
                        "удерживать власть",
                        "удерживать позиции"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep something in one's possession or maintain a specific state, level, or position (such as attention, power, or a lead)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to maintain a position, lead, or attention"
                            },
                            {
                                "target_lang_word": "retain",
                                "target_lang_sense_clarification": "formal; to keep possession of or continue to have"
                            },
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "to maintain possession or condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2356ece5-5778-3cdb-85da-ef6c84bc4e97",
                    "sense_definition": "Мешать кому-либо совершить какое-либо действие, останавливать от поступка.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "prevention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Только страх наказания <w>удерживал</w> его от нарушения строгих правил.",
                            "target_lang_translations": {
                                "en": "Only fear of punishment <w>restrained</w> him from breaking the strict rules."
                            }
                        },
                        {
                            "text": "Друзья пытались <w>удерживать</w> его от необдуманного и рискованного шага.",
                            "target_lang_translations": {
                                "en": "Friends tried to <w>hold</w> him <w>back</w> from a rash and risky step."
                            }
                        }
                    ],
                    "synonyms": [
                        "сдерживать"
                    ],
                    "antonyms": [
                        "позволять",
                        "разрешать"
                    ],
                    "common_phrases": [
                        "удерживать от ошибки",
                        "удерживать от шага"
                    ],
                    "target_lang_definitions": {
                        "en": "To prevent someone from performing an action or stop them from doing something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "restrain",
                                "target_lang_sense_clarification": "to control or keep in check"
                            },
                            {
                                "target_lang_word": "hold back",
                                "target_lang_sense_clarification": "to stop someone from moving forward or acting"
                            },
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "to prevent (usually followed by 'from')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6dbbfc4-5b04-35ce-9136-088c70ec0282",
                    "sense_definition": "Усилием воли не давать проявиться чувствам, эмоциям или физиологическим реакциям (слёзы, смех, крик).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "suppression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она изо всех сил старалась <w>удерживать</w> слёзы, чтобы не расплакаться при всех.",
                            "target_lang_translations": {
                                "en": "She tried with all her might to <w>hold back</w> her tears so as not to cry in front of everyone."
                            }
                        },
                        {
                            "text": "Студенты не могли <w>удерживать</w> смех во время забавной ситуации на лекции.",
                            "target_lang_translations": {
                                "en": "The students could not <w>suppress</w> their laughter during the funny situation at the lecture."
                            }
                        }
                    ],
                    "synonyms": [
                        "сдерживать",
                        "подавлять"
                    ],
                    "common_phrases": [
                        "удерживать слёзы",
                        "удерживать смех",
                        "удерживать крик"
                    ],
                    "target_lang_definitions": {
                        "en": "To use willpower to prevent emotions or physiological reactions (like tears or laughter) from showing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold back",
                                "target_lang_sense_clarification": "to repress tears or emotions"
                            },
                            {
                                "target_lang_word": "suppress",
                                "target_lang_sense_clarification": "to forcibly put an end to a feeling or reaction"
                            },
                            {
                                "target_lang_word": "stifle",
                                "target_lang_sense_clarification": "to restrain a reaction like a laugh or yawn"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "beacbc62-c11e-35ad-b683-1fe2a4071283",
                    "sense_definition": "Вычитать часть денег из суммы, предназначенной к выплате.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бухгалтерия будет автоматически <w>удерживать</w> налог из вашей ежемесячной зарплаты.",
                            "target_lang_translations": {
                                "en": "Accounting will automatically <w>deduct</w> tax from your monthly salary."
                            }
                        },
                        {
                            "text": "Банк имеет право <w>удерживать</w> комиссию за обслуживание кредитного счёта.",
                            "target_lang_translations": {
                                "en": "The bank has the right to <w>deduct</w> a commission for maintaining the credit account."
                            }
                        }
                    ],
                    "synonyms": [
                        "вычитать"
                    ],
                    "antonyms": [
                        "выплачивать",
                        "выдавать"
                    ],
                    "common_phrases": [
                        "удерживать налог",
                        "удерживать из зарплаты",
                        "удерживать комиссию"
                    ],
                    "target_lang_definitions": {
                        "en": "To subtract a portion of money from a sum intended for payment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deduct",
                                "target_lang_sense_clarification": "to subtract from a total"
                            },
                            {
                                "target_lang_word": "withhold",
                                "target_lang_sense_clarification": "to refuse to give or keep back (often taxes)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
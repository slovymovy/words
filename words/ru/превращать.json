{
    "word_family": [
        "превращение",
        "превращаться"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e977d3f5-ab48-3ad4-a31c-6bf80482deb3",
                    "sense_definition": "Придавать кому-либо или чему-либо новый вид, переводить в иное состояние или качество.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный мороз быстро <превращал> воду в лёд.",
                            "target_lang_translations": {
                                "en": "The severe frost quickly <w>turned</w> the water into ice."
                            }
                        },
                        {
                            "text": "Талантливый повар <превращает> простые продукты в настоящий шедевр.",
                            "target_lang_translations": {
                                "en": "A talented chef <w>transforms</w> simple ingredients into a real masterpiece."
                            }
                        }
                    ],
                    "synonyms": [
                        "обращать",
                        "преобразовывать",
                        "трансформировать",
                        "изменять",
                        "переделывать"
                    ],
                    "antonyms": [
                        "сохранять",
                        "оставлять"
                    ],
                    "common_phrases": [
                        "превращать в шутку",
                        "превращать в руины",
                        "превращать мечты в реальность",
                        "превращать в пыль"
                    ],
                    "target_lang_definitions": {
                        "en": "To give someone or something a new appearance, or to bring into a different state or quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn into",
                                "target_lang_sense_clarification": "general change of state or form"
                            },
                            {
                                "target_lang_word": "transform",
                                "target_lang_sense_clarification": "thorough or dramatic change"
                            },
                            {
                                "target_lang_word": "convert",
                                "target_lang_sense_clarification": "change in function or nature"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56f7b58c-8f40-30af-ab65-599ce0021cc3",
                    "sense_definition": "С помощью волшебства изменять облик живого существа или предмета.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "change_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Злая колдунья <превращала> всех путников в камни.",
                            "target_lang_translations": {
                                "en": "The evil witch <w>turned</w> all travelers into stone."
                            }
                        },
                        {
                            "text": "В этой сказке добрая фея <превращает> тыкву в золотую карету.",
                            "target_lang_translations": {
                                "en": "In this fairy tale, the kind fairy <w>turns</w> a pumpkin into a golden carriage."
                            }
                        }
                    ],
                    "common_phrases": [
                        "превращать в лягушку",
                        "превращать в камень",
                        "превращать в золото"
                    ],
                    "target_lang_definitions": {
                        "en": "To change the appearance of a living being or object using magic."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn into",
                                "target_lang_sense_clarification": "magical change"
                            },
                            {
                                "target_lang_word": "transform",
                                "target_lang_sense_clarification": "magical alteration of shape"
                            }
                        ]
                    }
                }
            ]
        }
    ]

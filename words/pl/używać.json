    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1a11a8f2-e703-3c71-9d27-2e4b3ad22b21",
                    "sense_definition": "Korzystać z przedmiotu, narzędzia, środka lub metody w celu wykonania jakiejś czynności lub osiągnięcia rezultatu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "instrumental_use",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od tygodnia nie <w>używam</w> komputera i czuję się nieco zagubiony.",
                            "target_lang_translations": {
                                "ru": "Я уже неделю не <w>пользуюсь</w> компьютером и чувствую себя немного потерянным."
                            }
                        },
                        {
                            "text": "Do krojenia chleba powinieneś <w>używać</w> ostrego noża.",
                            "target_lang_translations": {
                                "ru": "Для нарезки хлеба тебе следует <w>использовать</w> острый нож."
                            }
                        }
                    ],
                    "synonyms": [
                        "korzystać",
                        "posługiwać się",
                        "wykorzystywać",
                        "brać"
                    ],
                    "common_phrases": [
                        "używać komputera",
                        "używać słownika",
                        "używać siły",
                        "używać narzędzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать предмет, инструмент или метод для выполнения действия или достижения результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пользоваться",
                                "target_lang_sense_clarification": "применять что-либо (обычно с творительным падежом)"
                            },
                            {
                                "target_lang_word": "использовать",
                                "target_lang_sense_clarification": "употреблять для дела (обычно с винительным падежом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d65ed9f9-2cd6-38fb-a3a6-ac8d79e52484",
                    "sense_definition": "Traktować kogoś instrumentalnie, czerpać korzyści z czyjejś pracy lub naiwności, nie dając nic w zamian.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "instrumental_use",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona czuła, że szef tylko jej <w>używa</w> do załatwiania prywatnych spraw.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала, что начальник просто её <w>использует</w> для решения личных дел."
                            }
                        },
                        {
                            "text": "Politycy często <w>używają</w> strachu, aby manipulować wyborcami.",
                            "target_lang_translations": {
                                "ru": "Политики часто <w>используют</w> страх, чтобы манипулировать избирателями."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykorzystywać"
                    ],
                    "common_phrases": [
                        "używać ludzi",
                        "używać wpływów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-то как к инструменту, извлекать выгоду из чьей-то работы или наивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "использовать",
                                "target_lang_sense_clarification": "манипулировать людьми в своих интересах"
                            },
                            {
                                "target_lang_word": "пользоваться",
                                "target_lang_sense_clarification": "злоупотреблять чьим-то доверием или положением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "311f18a1-e57e-3a23-bb81-c73bcba29d7a",
                    "sense_definition": "Korzystać z uroków życia, bawić się i dogadzać sobie (często występuje z zaimkiem „sobie”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "enjoyment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy moi sąsiedzi sobie <w>użyją</w>, to cała klatka schodowa nie może spać.",
                            "target_lang_translations": {
                                "ru": "Когда мои соседи <w>гуляют</w>, весь подъезд не может спать."
                            }
                        },
                        {
                            "text": "Na wakacjach zamierzamy <w>używać</w> życia i nie martwić się kosztami.",
                            "target_lang_translations": {
                                "ru": "В отпуске мы собираемся <w>наслаждаться</w> жизнью и не беспокоиться о расходах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "używać życia",
                        "używać sobie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used with 'sobie'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наслаждаться жизнью, развлекаться и угождать себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наслаждаться",
                                "target_lang_sense_clarification": "получать удовольствие от жизни"
                            },
                            {
                                "target_lang_word": "развлекаться",
                                "target_lang_sense_clarification": "весело проводить время, гулять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01b8ee20-f82b-3974-a8ea-fcb89068f508",
                    "sense_definition": "Ostro kogoś krytykować, wyśmiewać lub znęcać się nad kimś słownie (zwykle w konstrukcji „używać sobie na kimś”).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abuse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor <w>używał</w> sobie na pracowniku za najmniejszy błąd w raporcie.",
                            "target_lang_translations": {
                                "ru": "Директор <w>срывал злость</w> на сотруднике за малейшую ошибку в отчёте."
                            }
                        },
                        {
                            "text": "Nie pozwolę, żebyś <w>używał</w> sobie na mnie przy całej rodzinie.",
                            "target_lang_translations": {
                                "ru": "Я не позволю тебе <w>отыгрываться</w> на мне при всей семье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "używać sobie na kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко критиковать, высмеивать или срывать на ком-то злость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срывать злость",
                                "target_lang_sense_clarification": "вымещать гнев на ком-то"
                            },
                            {
                                "target_lang_word": "отыгрываться",
                                "target_lang_sense_clarification": "компенсировать свои неудачи за счет другого"
                            },
                            {
                                "target_lang_word": "издеваться",
                                "target_lang_sense_clarification": "зло насмехаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca6eee27-f14e-32c4-a631-42aa310051a8",
                    "sense_definition": "Odbywać stosunek seksualny (zwykle w kontekście przedmiotowego traktowania partnera).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciekawe, czy ten arystokrata naprawdę <w>używał</w> tej kobiety, czy to tylko plotki.",
                            "target_lang_translations": {
                                "ru": "Интересно, этот аристократ действительно <w>пользовал</w> эту женщину, или это только слухи."
                            }
                        },
                        {
                            "text": "W dawnych powieściach pisano, jak pan <w>używał</w> dziewek folwarcznych.",
                            "target_lang_translations": {
                                "ru": "В старых романах писали, как барин <w>пользовал</w> дворовых девок."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly archaic or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступать в половую связь (обычно в контексте потребительского отношения к партнеру)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пользовать",
                                "target_lang_sense_clarification": "иметь связь (устаревшее, грубое)"
                            },
                            {
                                "target_lang_word": "использовать",
                                "target_lang_sense_clarification": "сексуально эксплуатировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "0289f424-926d-3828-91c9-c7d5155e58de",
                    "sense_definition": "Zacząć odczuwać nagłą zmianę samopoczucia fizycznego lub psychicznego (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "feelings_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dusznym autobusie pasażerce nagle <zrobiło> się słabo.",
                            "target_lang_translations": {
                                "ru": "В душном автобусе пассажирке внезапно <стало> плохо."
                            }
                        },
                        {
                            "text": "Kiedy usłyszałem tę smutną historię, <zrobiło> mi się przykro.",
                            "target_lang_translations": {
                                "ru": "Когда я услышал эту грустную историю, мне <стало> жаль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zrobić się słabo",
                        "zrobić się niedobrze",
                        "zrobić się smutno",
                        "zrobić się głupio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почувствовать внезапное изменение физического или эмоционального состояния."
                    },
                    "translations": {
                        "ru": [

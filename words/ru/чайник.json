    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0d64d869-183a-3b66-93cb-1718444cfa57",
                    "sense_definition": "Сосуд с носиком, ручкой и крышкой, используемый для кипячения воды.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kitchenware",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я поставил <w>чайник</w> на плиту, чтобы выпить горячего чаю.",
                            "target_lang_translations": {
                                "en": "I put the <w>kettle</w> on the stove to drink some hot tea."
                            }
                        },
                        {
                            "text": "На кухне громко свистел <w>чайник</w>, приглашая всех к столу.",
                            "target_lang_translations": {
                                "en": "The <w>kettle</w> whistled loudly in the kitchen, inviting everyone to the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "электрочайник"
                    ],
                    "common_phrases": [
                        "поставить чайник",
                        "электрический чайник",
                        "чайник со свистком",
                        "кипящий чайник"
                    ],
                    "target_lang_definitions": {
                        "en": "A vessel with a spout, handle, and lid, used for boiling water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kettle",
                                "target_lang_sense_clarification": "vessel for boiling water"
                            },
                            {
                                "target_lang_word": "teakettle",
                                "target_lang_sense_clarification": "vessel for boiling water"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "354ba652-32ff-3eef-bf4d-3260eef39a52",
                    "sense_definition": "Сосуд для заваривания чая, обычно керамический, фарфоровый или стеклянный.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "kitchenware",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама достала из шкафа красивый фарфоровый <w>чайник</w> для гостей.",
                            "target_lang_translations": {
                                "en": "Mom took a beautiful porcelain <w>teapot</w> out of the cupboard for the guests."
                            }
                        },
                        {
                            "text": "В этот <w>чайник</w> нужно насыпать две ложки заварки и залить кипятком.",
                            "target_lang_translations": {
                                "en": "You need to put two spoons of tea leaves into this <w>teapot</w> and pour boiling water over them."
                            }
                        }
                    ],
                    "synonyms": [
                        "зава́рочный ча́йник",
                        "заварно́й ча́йник",
                        "зава́рник",
                        "заварник"
                    ],
                    "common_phrases": [
                        "заварочный чайник",
                        "фарфоровый чайник",
                        "носик чайника"
                    ],
                    "target_lang_definitions": {
                        "en": "A vessel for brewing tea, usually ceramic, porcelain, or glass."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teapot",
                                "target_lang_sense_clarification": "vessel for brewing tea"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cf34a44-866f-3538-954b-ec91f5d5439d",
                    "sense_definition": "Неопытный человек, новичок в каком-либо деле (часто о водителях или пользователях компьютера).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я в компьютерах полный <w>чайник</w>, поэтому прошу сына помочь мне с программой.",
                            "target_lang_translations": {
                                "en": "I am a complete <w>dummy</w> with computers, so I ask my son to help me with the program."
                            }
                        },
                        {
                            "text": "Опытные водители часто недовольны поведением <w>чайников</w> на дороге.",
                            "target_lang_translations": {
                                "en": "Experienced drivers are often unhappy with the behavior of <w>novices</w> on the road."
                            }
                        }
                    ],
                    "synonyms": [
                        "новичок",
                        "неспециалист",
                        "неумеха"
                    ],
                    "antonyms": [
                        "специалист",
                        "профессионал",
                        "эксперт",
                        "профи"
                    ],
                    "common_phrases": [
                        "полный чайник",
                        "чайник за рулем"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "шутливое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An inexperienced person, a beginner in a specific field (often referring to drivers or computer users)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "novice",
                                "target_lang_sense_clarification": "beginner"
                            },
                            {
                                "target_lang_word": "newbie",
                                "target_lang_sense_clarification": "slang for beginner"
                            },
                            {
                                "target_lang_word": "dummy",
                                "target_lang_sense_clarification": "colloquial, inexperienced person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f19ca141-cbf6-38aa-aec6-37b2748fad94",
                    "sense_definition": "Количество жидкости, которое вмещается в такой сосуд.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы выпили целый <w>чайник</w> чая, обсуждая последние новости.",
                            "target_lang_translations": {
                                "en": "We drank a whole <w>pot</w> of tea while discussing the latest news."
                            }
                        },
                        {
                            "text": "Для приготовления супа нам понадобится ещё один <w>чайник</w> воды.",
                            "target_lang_translations": {
                                "en": "To make the soup, we will need another <w>kettle</w> of water."
                            }
                        }
                    ],
                    "common_phrases": [
                        "целый чайник",
                        "чайник кипятка"
                    ],
                    "target_lang_definitions": {
                        "en": "The quantity of liquid that fits into such a vessel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kettleful",
                                "target_lang_sense_clarification": "quantity held by a kettle"
                            },
                            {
                                "target_lang_word": "pot",
                                "target_lang_sense_clarification": "quantity held by a teapot"
                            }
                        ]
                    }
                }
            ]
        }

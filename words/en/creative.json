                },
                {
                    "sense_id": "8c4e3c4c-d1da-3ec0-b42d-d9c870d6adbd",
                    "sense_definition": "Relating to a specific type of set of natural numbers in mathematical logic.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor explained the complex properties of a <w>creative</w> set.",
                            "target_lang_translations": {
                                "ru": "Профессор объяснил сложные свойства <w>креативного</w> множества."
                            }

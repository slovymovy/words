            "senses": [
                {
                    "sense_id": "aaf99510-71eb-329d-aa91-5301b40be9c6",
                    "sense_definition": "Pokryty pancerzem pajęczak z kleszczami i kolcem jadowym na ostatnim segmencie odwłoka, zamieszkujący głównie ciepłe strefy klimatyczne.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "arachnids",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wycieczki na pustynię musieliśmy uważać, aby nie nadepnąć na <w>skorpiona</w>."
                        },
                        {
                            "text": "Jad tego gatunku <w>skorpiona</w> może być bardzo niebezpieczny dla człowieka."
                        }
                    ],
                    "common_phrases": [
                        "jadowity skorpion",
                        "ukąszenie skorpiona",
                        "czarny skorpion"
                    ]
                },
                {
                    "sense_id": "ba9c5180-428d-3078-ab47-baf0fff58a3d",
                    "sense_definition": "Osoba urodzona pod znakiem zodiaku Skorpiona (między 23 października a 21 listopada).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "zodiac_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest zodiakalnym <w>Skorpionem</w>, więc bywa bardzo uparty i tajemniczy."
                        },
                        {
                            "text": "Ona zachowuje się jak typowy <w>Skorpion</w>, zawsze pamięta wyrządzone jej krzywdy."
                        }
                    ],
                    "common_phrases": [
                        "zodiakalny Skorpion",
                        "typowy Skorpion"
                    ]
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4d3763c7-4349-3f9b-91ed-5e8c9f4549bb",
                    "sense_definition": "Ósmy znak zodiaku, w którym Słońce znajduje się od 23 października do 21 listopada.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "zodiac_signs",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Pod koniec października Słońce wchodzi w znak <w>Skorpiona</w>."
                        },
                        {
                            "text": "Osoby spod znaku <w>Skorpiona</w> są często uważane za bardzo namiętne i emocjonalne."
                        }
                    ],
                    "synonyms": [
                        "♏"
                    ],
                    "common_phrases": [
                        "znak Skorpiona",
                        "spod znaku Skorpiona"
                    ]
                },
                {
                    "sense_id": "b4b05661-2eff-32bb-8af6-54faa85b0a62",
                    "sense_definition": "Zodiakalny gwiazdozbiór nieba południowego, w Polsce widoczny tylko częściowo latem, nisko nad horyzontem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "constellations",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Najjaśniejszą gwiazdą w gwiazdozbiorze <w>Skorpiona</w> jest czerwony nadolbrzym Antares."
                        },
                        {
                            "text": "W lipcowe noce obserwowałem fragment <w>Skorpiona</w> tuż nad południowym horyzontem."

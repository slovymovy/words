    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec747a26-acfc-3f08-902f-ab102a0427df",
                    "sense_definition": "Een van de vlakken die de begrenzing van een voorwerp vormen, of een specifiek aspect van een situatie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto staat aan de <w>kant</w> van de weg geparkeerd.",
                            "target_lang_translations": {
                                "en": "The car is parked on the <w>side</w> of the road.",
                                "ru": "Машина припаркована на <w>обочине</w> дороги."
                            }
                        },
                        {
                            "text": "Aan de ene <w>kant</w> wil ik gaan, maar aan de andere <w>kant</w> heb ik geen tijd.",
                            "target_lang_translations": {
                                "en": "On the one <w>hand</w> I want to go, but on the other <w>hand</w> I don't have time.",
                                "ru": "С одной <w>стороны</w> я хочу пойти, но с другой <w>стороны</w> у меня нет времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijde",
                        "zij"
                    ],
                    "common_phrases": [
                        "aan de kant",
                        "aan de andere kant",
                        "op zijn kant",
                        "aan de ene kant"
                    ],
                    "target_lang_definitions": {
                        "en": "One of the surfaces forming the boundary of an object, or a specific aspect of a situation.",
                        "ru": "Одна из поверхностей, образующих границу предмета, или определенный аспект ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "side",
                                "target_lang_sense_clarification": "surface, boundary, or aspect"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сторона",
                                "target_lang_sense_clarification": "поверхность или точка зрения"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "граница или кромка"
                            },
                            {
                                "target_lang_word": "бок",
                                "target_lang_sense_clarification": "боковая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69d4d9c7-0534-372e-be86-a18e131fefeb",
                    "sense_definition": "De lijn waarlangs iets of iemand zich beweegt of waarnaar iets wijst.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten deze <w>kant</w> op om bij het station te komen.",
                            "target_lang_translations": {
                                "en": "We have to go this <w>way</w> to get to the station.",
                                "ru": "Нам нужно в эту <w>сторону</w>, чтобы попасть на вокзал."
                            }
                        },
                        {
                            "text": "Hij keek de verkeerde <w>kant</w> uit toen hij overstak.",
                            "target_lang_translations": {
                                "en": "He looked the wrong <w>way</w> when he crossed.",
                                "ru": "Он посмотрел не в ту <w>сторону</w>, когда переходил дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "richting"
                    ],
                    "common_phrases": [
                        "welke kant op",
                        "die kant op",
                        "alle kanten op"
                    ],
                    "target_lang_definitions": {
                        "en": "The line along which something or someone moves or towards which something points.",
                        "ru": "Направление, в котором кто-то движется или куда что-то указывает."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "way",
                                "target_lang_sense_clarification": "direction of movement"
                            },
                            {
                                "target_lang_word": "direction",
                                "target_lang_sense_clarification": "course or path"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сторона",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "направление",
                                "target_lang_sense_clarification": "вектор движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d457a5e-77c3-3292-a24c-b2c9ab94107b",
                    "sense_definition": "Een fijn, opengewerkt weefsel van draden in patronen, vaak gebruikt als versiering aan kleding.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bruidsjurk was versierd met prachtig wit <w>kant</w>.",
                            "target_lang_translations": {
                                "en": "The wedding dress was decorated with beautiful white <w>lace</w>.",
                                "ru": "Свадебное платье было украшено красивым белым <w>кружевом</w>."
                            }
                        },
                        {
                            "text": "Vroeger werd <w>kant</w> vaak met de hand geklost in Vlaanderen.",
                            "target_lang_translations": {
                                "en": "In the past, <w>lace</w> was often made by hand in Flanders.",
                                "ru": "Раньше во Фландрии <w>кружево</w> часто плели вручную."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van kant",
                        "kanten kraag",
                        "stukje kant"
                    ],
                    "target_lang_definitions": {
                        "en": "A fine, open fabric of threads in patterns, often used as decoration on clothing.",
                        "ru": "Тонкая ажурная ткань с узорами, часто используемая для украшения одежды."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lace",
                                "target_lang_sense_clarification": "delicate fabric"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кружево",
                                "target_lang_sense_clarification": "ажурная ткань"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24c06844-1fd7-364f-9360-65c3cf43e166",
                    "sense_definition": "Vervoeging van <w>kanten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd, of gebiedende wijs.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>kant</w> zich fel tegen de nieuwe plannen van de gemeente.",
                            "target_lang_translations": {
                                "en": "He fiercely <w>opposes</w> the municipality's new plans.",
                                "ru": "Он яростно <w>противится</w> новым планам муниципалитета."
                            }
                        },
                        {
                            "text": "De zware steen <w>kant</w> langzaam naar voren als we duwen.",
                            "target_lang_translations": {
                                "en": "The heavy stone slowly <w>tilts</w> forward when we push.",
                                "ru": "Тяжелый камень медленно <w>наклоняется</w> вперед, когда мы толкаем."
                            }
                        }
                    ],

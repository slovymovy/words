            "senses": [
                {
                    "sense_id": "26f21aeb-fbfc-3254-95fe-4d386d46bdee",
                    "sense_definition": "Forma trybu rozkazującego (druga osoba liczby pojedynczej) czasownika <w>meczeć</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <mecz> tak głośno, bo spłoszysz inne zwierzęta.",
                            "target_lang_translations": {
                                "ru": "Не <w>блей</w> так громко, а то распугаешь других животных."
                            }
                        },
                        {
                            "text": "Proszę cię, nie <mecz> mi nad uchem, bo boli mnie głowa.",
                            "target_lang_translations": {
                                "ru": "Прошу тебя, не <w>ной</w> мне над ухом, потому что у меня болит голова."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "348003c7-ffea-3f50-ac10-3021a06c6ec1",
                    "sense_definition": "To think, suppose, or believe something is true or likely.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>reckon</w> it is going to rain later this afternoon."
                        },
                        {
                            "text": "Do you <w>reckon</w> we will arrive before the shops close?"
                        }
                    ],
                    "synonyms": [
                        "think",
                        "believe",
                        "suppose",
                        "guess"
                    ],
                    "common_phrases": [
                        "I reckon",
                        "reckon so",
                        "reckon on"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in spoken British, Australian, and Southern US English"
                        }
                    ]
                },
                {
                    "sense_id": "f552c4df-75a4-3d90-b5af-a23fba453c82",
                    "sense_definition": "To take something into consideration or deal with a difficult person or situation; usually used in the phrase 'reckon with'.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "consideration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You have to <w>reckon</w> with the fact that the project might take longer than planned."
                        },
                        {
                            "text": "She is a force to be <w>reckoned</w> with in the business world."
                        }
                    ],
                    "common_phrases": [
                        "reckon with",
                        "force to be reckoned with"
                    ]
                },
                {
                    "sense_id": "d7e0b7f5-8aa5-33c0-8f87-773eef55146c",
                    "sense_definition": "To calculate, compute, or count a number or amount.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "calculation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shopkeeper <w>reckoned</w> the total cost to be fifty dollars."
                        },
                        {
                            "text": "Astronomers <w>reckon</w> the distance to the star using complex formulas."
                        }
                    ],
                    "synonyms": [
                        "calculate",
                        "compute",
                        "number",
                        "enumerate"
                    ],
                    "common_phrases": [
                        "reckon up"
                    ]
                },
                {
                    "sense_id": "e6675e05-5e6e-3abb-9647-cb98c47f7c70",
                    "sense_definition": "To consider or regard someone or something as having a specific quality, status, or value.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "estimation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>reckoned</w> among the greatest writers of the twentieth century."
                        },
                        {
                            "text": "The loss was <w>reckoned</w> to be significant for the company."
                        }
                    ],
                    "synonyms": [
                        "esteem",
                        "rate",
                        "value"
                    ],
                    "common_phrases": [
                        "reckoned among",
                        "reckoned as"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "10b9998a-883c-3952-ad38-0f43b4ad5578",
                    "sense_definition": "Het loeiende geluid dat een koe maakt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sound_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koe in de wei zegt <w>boe</w> als hij honger heeft.",
                            "target_lang_translations": {
                                "en": "The cow in the meadow says <w>moo</w> when it is hungry."
                            }
                        },
                        {
                            "text": "Kun jij het geluid van een koe nadoen en <w>boe</w> zeggen?",
                            "target_lang_translations": {
                                "en": "Can you imitate the sound of a cow and say <w>moo</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "boe zeggen"
                    ],
                    "target_lang_definitions": {
                        "en": "The lowing sound that a cow makes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moo",
                                "target_lang_sense_clarification": "sound made by a cow"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15e314a9-8fba-31e1-9ea0-65bb55ffe2a8",
                    "sense_definition": "Een uitroep die bedoeld is om iemand plotseling te laten schrikken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_scare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sprong onverwachts achter de deur vandaan en riep hard <w>boe</w>!",
                            "target_lang_translations": {
                                "en": "He jumped unexpectedly from behind the door and yelled <w>boo</w> loudly!"
                            }
                        },
                        {
                            "text": "De kinderen vinden het leuk om <w>boe</w> te roepen in het donker.",
                            "target_lang_translations": {
                                "en": "The children like to shout <w>boo</w> in the dark."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boe roepen"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation intended to startle someone suddenly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boo",
                                "target_lang_sense_clarification": "exclamation to scare someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60276685-ee38-3ae1-b614-2256099c1196",
                    "sense_definition": "Een uitroep waarmee men afkeuring of ontevredenheid uitdrukt tegenover een spreker of artiest.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exclamation_disapproval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het ontevreden publiek riep luid <w>boe</w> na de slechte voorstelling.",
                            "target_lang_translations": {
                                "en": "The dissatisfied audience shouted <w>boo</w> loudly after the bad performance."
                            }
                        },
                        {
                            "text": "Als de scheidsrechter een fout maakt, hoor je vaak <w>boe</w> vanaf de tribune.",
                            "target_lang_translations": {
                                "en": "If the referee makes a mistake, you often hear <w>boos</w> from the stands."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boe roepen"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used to express disapproval or dissatisfaction towards a speaker or performer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boo",
                                "target_lang_sense_clarification": "exclamation of disapproval"
                            }
                        ]
                    }
                }
            ]
        },
        {

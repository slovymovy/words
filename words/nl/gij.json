    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "7b308533-bfe0-3a24-bac2-d93475563c1c",
                    "sense_definition": "Jij; persoonlijk voornaamwoord voor de tweede persoon enkelvoud (en soms meervoud), algemeen gebruikt in de spreektaal in Vlaanderen en delen van Zuid-Nederland.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "address_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komt <gij> vanavond ook naar het feest?",
                            "target_lang_translations": {
                                "en": "Are <you> coming to the party tonight as well?",
                                "ru": "<w>Ты</w> тоже идешь сегодня вечером на вечеринку?"
                            }
                        },
                        {
                            "text": "Weet <gij> toevallig waar het station is?",
                            "target_lang_translations": {
                                "en": "Do <you> happen to know where the station is?",
                                "ru": "<w>Ты</w> случайно не знаешь, где находится вокзал?"
                            }
                        },
                        {
                            "text": "<Gij> zijt hier altijd van harte welkom.",
                            "target_lang_translations": {
                                "en": "<You> are always very welcome here.",
                                "ru": "<w>Ты</w> здесь всегда желанный гость."
                            }
                        }
                    ],
                    "synonyms": [
                        "jij",
                        "u"
                    ],
                    "antonyms": [
                        "ik"
                    ],
                    "common_phrases": [
                        "gij en ik",
                        "hoort gij"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Vlaanderen en Zuid-Nederland"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "in de genoemde regio's"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "You; personal pronoun for the second person singular (and sometimes plural), commonly used in spoken language in Flanders and parts of the Southern Netherlands.",
                        "ru": "Ты; личное местоимение второго лица единственного числа (иногда множественного), широко используемое в разговорной речи во Фландрии и на юге Нидерландов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "standard English equivalent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "разговорное, региональное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cd3383a-a733-3c39-a381-3a57ec2754e1",
                    "sense_definition": "U; plechtige, bijbelse of verouderde aanspreekvorm voor de tweede persoon, vaak gebruikt in gebeden, religieuze teksten of historische toespraken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "address_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Gij> zult uw naaste liefhebben als uzelf.",
                            "target_lang_translations": {
                                "en": "<Thou> shalt love thy neighbor as thyself.",
                                "ru": "<w>Ты</w> должен возлюбить ближнего твоего, как самого себя."
                            }
                        },
                        {
                            "text": "Heer, <gij> bent onze toevlucht van geslacht tot geslacht.",
                            "target_lang_translations": {
                                "en": "Lord, <thou> art our refuge from generation to generation.",
                                "ru": "Господи, <w>Ты</w> — нам прибежище в род и род."
                            }
                        },
                        {
                            "text": "<Gij> zult niet doden.",
                            "target_lang_translations": {
                                "en": "<Thou> shalt not kill.",
                                "ru": "<w>Ты</w> не должен убивать."
                            }
                        }
                    ],
                    "synonyms": [
                        "u"
                    ],
                    "common_phrases": [
                        "gij zult",
                        "gij zijt"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "voornamelijk religieus of literair"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Thou; solemn, biblical, or archaic form of address for the second person, often used in prayers, religious texts, or historical speeches.",
                        "ru": "Торжественная, библейская или устаревшая форма обращения ко второму лицу, часто используемая в молитвах и религиозных текстах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thou",
                                "target_lang_sense_clarification": "archaic singular subject"
                            },
                            {
                                "target_lang_word": "ye",
                                "target_lang_sense_clarification": "archaic plural or formal subject"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "торжественное, религиозное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "zondaar",
        "zondigen",
        "zondig",
        "doodzonde",
        "erfzonde"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5424de1e-0799-3e3d-a1ad-3fa33f2a71cf",
                    "sense_definition": "Iets wat jammer is of als verspilling wordt ervaren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "regret_waste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is echt <w>zonde</w> dat we niet mee kunnen gaan.",
                            "target_lang_translations": {
                                "en": "It is really a <w>pity</w> that we cannot go along."
                            }
                        },
                        {
                            "text": "Dat is <w>zonde</w> van al het harde werk dat je erin hebt gestoken.",
                            "target_lang_translations": {
                                "en": "That is a <w>waste</w> of all the hard work you put into it."
                            }
                        }
                    ],
                    "synonyms": [
                        "jammer",
                        "spijtig"
                    ],
                    "common_phrases": [
                        "zonde van de tijd",
                        "zonde van het geld",
                        "eeuwig zonde"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is a pity or perceived as a waste."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waste",
                                "target_lang_sense_clarification": "useless expenditure of resources like time or money"
                            },
                            {
                                "target_lang_word": "pity",
                                "target_lang_sense_clarification": "something to be regretted"
                            },
                            {
                                "target_lang_word": "shame",
                                "target_lang_sense_clarification": "a regrettable situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5aaf133-413e-3f63-ad0c-8ff3ac13a822",
                    "sense_definition": "Een overtreding van een religieuze of morele wet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_transgression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de kerk is liegen een ernstige <w>zonde</w>.",
                            "target_lang_translations": {
                                "en": "According to the church, lying is a serious <w>sin</w>."
                            }
                        },
                        {
                            "text": "Hij wilde zijn <w>zonde</w> opbiechten aan de priester.",
                            "target_lang_translations": {
                                "en": "He wanted to confess his <w>sin</w> to the priest."
                            }
                        }
                    ],
                    "synonyms": [
                        "betreurenswaard",
                        "spijtig"
                    ],
                    "antonyms": [
                        "deugd"
                    ],
                    "common_phrases": [
                        "in zonde leven",
                        "een zonde begaan"
                    ],
                    "target_lang_definitions": {
                        "en": "A violation of a religious or moral law."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sin",
                                "target_lang_sense_clarification": "moral or religious transgression"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6b449e40-01b6-38cf-a66e-089e1f77573d",
                    "sense_definition": "Te betreuren; een gevoel van spijt of teleurstelling oproepend.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "regret_waste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vind het <w>zonde</w> dat het vandaag regent.",
                            "target_lang_translations": {
                                "en": "I think it's a <w>shame</w> that it is raining today."
                            }
                        },
                        {
                            "text": "Het is <w>zonde</w> van zo'n mooie dag om binnen te blijven.",
                            "target_lang_translations": {
                                "en": "It is a <w>waste</w> of such a beautiful day to stay inside."
                            }
                        }
                    ],
                    "synonyms": [
                        "jammer",
                        "spijtig",
                        "betreurenswaard"
                    ],
                    "target_lang_definitions": {
                        "en": "Regrettable; evoking a feeling of regret or disappointment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pity",
                                "target_lang_sense_clarification": "cause for regret"
                            },
                            {
                                "target_lang_word": "shame",
                                "target_lang_sense_clarification": "disappointing occurrence"
                            },
                            {
                                "target_lang_word": "waste",
                                "target_lang_sense_clarification": "poor use of something valuable"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9df159fe-ebb5-32a5-ae55-f85635d0d6cb",
                    "sense_definition": "Enkelvoud verleden tijd van <w>zonnen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>zonde</w> gisteren de hele middag in de achtertuin.",
                            "target_lang_translations": {
                                "en": "She <w>sunbathed</w> in the backyard all afternoon yesterday."
                            }
                        },
                        {
                            "text": "De toerist <w>zonde</w> aan het zwembad tot hij rood zag.",
                            "target_lang_translations": {
                                "en": "The tourist <w>sunbathed</w> by the pool until he turned red."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Singular past tense of 'to sunbathe'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sunbathed",
                                "target_lang_sense_clarification": "past action of sitting in the sun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb232e49-486f-3f5d-b4d0-592182b3ea6d",
                    "sense_definition": "Aanvoegende wijs verleden tijd van <w>zenden</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was alsof de hemel een teken <w>zonde</w> om ons te waarschuwen.",
                            "target_lang_translations": {
                                "en": "It was as if heaven <w>sent</w> a sign to warn us."
                            }
                        },
                        {
                            "text": "Hij bad dat de koning hem spoedig hulp <w>zonde</w>.",
                            "target_lang_translations": {
                                "en": "He prayed that the king <w>would send</w> him help soon."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Subjunctive form rarely used in modern Dutch."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Subjunctive past tense of 'to send'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sent",
                                "target_lang_sense_clarification": "archaic or hypothetical past action"
                            }
                        ]
                    }
                }
            ]
        }
    ]

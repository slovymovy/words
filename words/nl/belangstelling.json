                {
                    "sense_id": "8f62627f-343f-319e-9b01-0eb8605e533e",
                    "sense_definition": "De bereidheid en het verlangen om ergens aandacht aan te geven omdat je het interessant vindt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb veel <w>belangstelling</w> voor literatuur en geschiedenis.",
                            "target_lang_translations": {
                                "en": "I have a lot of <w>interest</w> in literature and history.",
                                "ru": "У меня большой <w>интерес</w> к литературе и истории."
                            }
                        },
                        {
                            "text": "De leerlingen toonden weinig <w>belangstelling</w> voor de uitleg van de leraar.",
                            "target_lang_translations": {
                                "en": "The students showed little <w>interest</w> in the teacher's explanation.",
                                "ru": "Ученики проявили мало <w>интереса</w> к объяснению учителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "interesse"
                    ],
                    "antonyms": [
                        "onverschilligheid"
                    ],
                    "common_phrases": [
                        "belangstelling hebben voor",
                        "belangstelling tonen",
                        "met belangstelling"
                    ],
                    "target_lang_definitions": {
                        "en": "The willingness and desire to pay attention to something because you find it interesting.",
                        "ru": "Готовность и желание уделять чему-либо внимание, так как это кажется интересным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interest",
                                "target_lang_sense_clarification": "intellectual curiosity or concern"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "интерес",
                                "target_lang_sense_clarification": "любопытство, увлеченность"
                            },
                            {
                                "target_lang_word": "заинтересованность",
                                "target_lang_sense_clarification": "состояние увлеченности чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae347496-7f7f-3041-940f-dc739eaddfbc",
                    "sense_definition": "De aandacht die iemand of iets krijgt van het publiek of andere mensen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jonge zanger staat momenteel erg in de <w>belangstelling</w>.",
                            "target_lang_translations": {
                                "en": "The young singer is currently attracting a lot of <w>interest</w>.",
                                "ru": "Молодой певец сейчас находится в центре <w>внимания</w>."
                            }
                        },
                        {
                            "text": "Er is veel <w>belangstelling</w> voor de nieuwe tentoonstelling in het museum.",
                            "target_lang_translations": {
                                "en": "There is a lot of <w>interest</w> in the new exhibition in the museum.",
                                "ru": "К новой выставке в музее проявляют большой <w>интерес</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aandacht"
                    ],
                    "common_phrases": [
                        "in de belangstelling staan"
                    ],
                    "target_lang_definitions": {
